  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk ben van boven <rhyme label="a" type="m">of</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gekomen inden <rhyme label="a" type="m" corresp="#s1:l1">Hof</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van die, die mijn <rhyme label="b" type="f">Gebeente</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Mijn Suster is, en <rhyme label="c" type="m">Bruyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die boven al uyt <rhyme label="c" type="m" corresp="#s1:l4">muyt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn bloeyende <rhyme label="b" type="f" corresp="#s1:l3">Gemeente</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Daer zijnde, was mijn <rhyme label="a" type="m">Oogh</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dan neder, dan om <rhyme label="a" type="m" corresp="#s2:l1">hoogh</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dan op de <rhyme label="b" type="f">Galeryen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dan hier, dan weder <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Tot dat ick wier <rhyme label="c" type="m" corresp="#s2:l4">gewaer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">De Soete <rhyme label="b" type="f" corresp="#s2:l3">Speceryen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Het is my wel <rhyme label="a" type="m">geluckt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ick heb' aldaer <rhyme label="a" type="m" corresp="#s3:l1">gepluckt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Mijn Myrrh', en <rhyme label="b" type="f">Honich-raten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">[Hier door ist dat ick <rhyme label="c" type="m">meen'</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Danck-off'ren, en <rhyme label="c" type="m" corresp="#s3:l4">Gebeen'</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van myne <rhyme label="b" type="f" corresp="#s3:l3">Ledematen</rhyme>.]</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick vond' daer Wijn en <rhyme label="a" type="m">Melck</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Oock heb ick uyt den <rhyme label="a" type="m" corresp="#s4:l1">Kelck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Daer van met smaeck <rhyme label="b" type="f">gedroncken</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Ick sagh waer dat ghy <rhyme label="c" type="m">waert</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dit heb ick u <rhyme label="c" type="m" corresp="#s4:l4">gespaert</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">'t Is u om niet <rhyme label="b" type="f" corresp="#s4:l3">geschoncken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick sliep, ick sliep, <rhyme label="a" type="m">helaes</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">(Den Voorspoet, en het <rhyme label="a" type="m" corresp="#s5:l1">aes</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Van weeld', had my doen <rhyme label="b" type="f">staken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">t' Gevoel van 's Hemels <rhyme label="c" type="m">win</rhyme>:)</l>
        <l id="s5:l5" met="3" type="-+">Maer des al niet te <rhyme label="c" type="m" corresp="#s5:l4">min</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Soo was myn Hert aen 't <rhyme label="b" type="f" corresp="#s5:l3">waken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">'k Vergat mijn Brugom <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Noch sijne gunst; want <rhyme label="a" type="m" corresp="#s6:l1">siet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Soo haest ick hoorde <rhyme label="b" type="f">Hemmen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En kloppen aende <rhyme label="c" type="m">deur</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Riep ick, wie is daer <rhyme label="c" type="m" corresp="#s6:l4">veur</rhyme>?</l>
        <l id="s6:l6" met="3" type="-+">En seyd': ô! 't is sijn <rhyme label="b" type="f" corresp="#s6:l3">Stemme</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn Suster, mijn <rhyme label="a" type="m">Vriendin'</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Mijn Duyve, laet my <rhyme label="a" type="m" corresp="#s7:l1">in</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Want siet; myn Hooft, en <rhyme label="b" type="f">wangen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Mijn Locken, en mijn <rhyme label="c" type="m">Haar</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Met drupp'len al te <rhyme label="c" type="m" corresp="#s7:l4">gaar</rhyme>;</l>
        <l id="s7:l6" met="3" type="-+">Sijn jammerlick <rhyme label="b" type="f" corresp="#s7:l3">om-hangen</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Met drupp'len van <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En Bloet, gelijck ghy <rhyme label="a" type="m" corresp="#s8:l1">siet</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">Dit heb ick al <rhyme label="b" type="f">geleden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Dit heb ick al <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En dus ist my <rhyme label="c" type="m" corresp="#s8:l4">vergaen</rhyme>;</l>
        <l id="s8:l6" met="3" type="-+">In uwe plaets en <rhyme label="b" type="f" corresp="#s8:l3">Stede</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick legh op myn <rhyme label="a" type="m">gemack</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Ick hebbe pack, en <rhyme label="a" type="m" corresp="#s9:l1">sack</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En Rocken <rhyme label="b" type="f">uytgetogen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">De Rust, en weelde, <rhyme label="c" type="m">siet</rhyme></l>
        <l id="s9:l5" met="3" type="-+">My hare gunste <rhyme label="c" type="m" corresp="#s9:l4">biet</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Waer wil ick meer na <rhyme label="b" type="f" corresp="#s9:l3">pogen</rhyme>?</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Mijn Voeten heb ick <rhyme label="a" type="m">oock</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Eer dat ick onder <rhyme label="a" type="m" corresp="#s10:l1">doock</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Met Wat'ren schoon <rhyme label="b" type="f">gewassen</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Dies kan ick, ('t is my <rhyme label="c" type="m">leet</rhyme>!)</l>
        <l id="s10:l5" met="3" type="-+">Dus trecken aen mijn <rhyme label="c" type="m" corresp="#s10:l4">kleet</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Soo schielijck, en soo <rhyme label="b" type="f" corresp="#s10:l3">rasse</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Mijn Voeten, die ick <rhyme label="a" type="m">heb</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Soo suyver als een <rhyme label="a" type="m" corresp="#s11:l1">Web'</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Sal ick die weer <rhyme label="b" type="f">besoet'len</rhyme>?</l>
        <l id="s11:l4" met="3" type="-+">Mijn Vleesch seyt, <rhyme label="c" type="m">ongeraen</rhyme>!</l>
        <l id="s11:l5" met="3" type="-+">Ten wilder oock niet <rhyme label="c" type="m" corresp="#s11:l4">aen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En roept, hoe dus te <rhyme label="b" type="f" corresp="#s11:l3">hoet'len</rhyme>!</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Mijn vrient, die trock de <rhyme label="a" type="m">Hant</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">(Wanneer hy my soo <rhyme label="a" type="m" corresp="#s12:l1">vant</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">In 't Weelde-Bed <rhyme label="b" type="f">gelegen</rhyme>)</l>
        <l id="s12:l4" met="3" type="-+">Van 't Slot: dat was <rhyme label="c" type="m">geseyt</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Ick ga; in <rhyme label="c" type="m" corresp="#s12:l4">eeuwicheyt</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Ghy missen sult myn <rhyme label="b" type="f" corresp="#s12:l3">Zegen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Soo haest ick dat <rhyme label="a" type="m">vernam</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Jck over eynde <rhyme label="a" type="m" corresp="#s13:l1">klam</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">En reyckte na mijn <rhyme label="b" type="f">kleeren</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">En sprongh ten Bedden <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">En riep vast <rhyme label="c" type="m" corresp="#s13:l4">overluyt</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Jck kom, ick kom, mijn <rhyme label="b" type="f" corresp="#s13:l3">Heere</rhyme>!</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Ick voelde sulcken <rhyme label="a" type="m">Brant</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">In 't hert en <rhyme label="a" type="m" corresp="#s14:l1">ingewant</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Ick voelde sulcke <rhyme label="b" type="f">nepen</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Wanneer hy riep, ick <rhyme label="c" type="m">gà</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Als of de <rhyme label="c" type="m" corresp="#s14:l4">Satana</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Mijn wegh hadt willen <rhyme label="b" type="f" corresp="#s14:l3">slepen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Ick dé de grendel <rhyme label="a" type="m">los</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">En kreegh een Roode <rhyme label="a" type="m" corresp="#s15:l1">blos</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Mijn vingers, en myn <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">(Hoort wat een wonder <rhyme label="c" type="m">dingh</rhyme>!)</l>
        <l id="s15:l5" met="3" type="-+">Die drupten, op den <rhyme label="c" type="m" corresp="#s15:l4">Ring</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Van Myrh', en op de <rhyme label="b" type="f" corresp="#s15:l3">Randen</rhyme>:</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Mijn suchten en <rhyme label="a" type="m">Gebeen'</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Mijn Tranen vlogen <rhyme label="a" type="m" corresp="#s16:l1">heen</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Als Sluysen uyt mijn <rhyme label="b" type="f">Oogen</rhyme>.</l>
        <l id="s16:l4" met="3" type="-+">Mits, open was de <rhyme label="c" type="m">deur</rhyme>;</l>
        <l id="s16:l5" met="3" type="-+">Hy isser dacht ick, <rhyme label="c" type="m" corresp="#s16:l4">veur</rhyme>;</l>
        <l id="s16:l6" met="3" type="-+">Dan, vond my heel <rhyme label="b" type="f" corresp="#s16:l3">bedrogen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Doen viel ick plat ter <rhyme label="a" type="m">neer</rhyme>.</l>
        <l id="s17:l2" met="3" type="-+">En riep, ô Godt! mijn <rhyme label="a" type="m" corresp="#s17:l1">Heer</rhyme>!</l>
        <l id="s17:l3" met="3" type="-+">Het hert wil my <rhyme label="b" type="f">beswijcken</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Mijn krachten henen <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Het is met my <rhyme label="c" type="m" corresp="#s17:l4">gedaen</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Ick word' gelijck de <rhyme label="b" type="f" corresp="#s17:l3">Lijcken</rhyme>!</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Jck sogt, doch vont hem <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Ick stommelden, ick <rhyme label="a" type="m" corresp="#s18:l1">stiet</rhyme>;</l>
        <l id="s18:l3" met="3" type="-+">Ick liep soo op de <rhyme label="b" type="f">Straten</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Als waer ick dul <rhyme label="c" type="m">geweest</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Of van een krancken <rhyme label="c" type="m" corresp="#s18:l4">Geest</rhyme>:</l>
        <l id="s18:l6" met="3" type="-+">De Wachters die daer <rhyme label="b" type="f" corresp="#s18:l3">saten</rhyme>,</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Die riepen tot my, <rhyme label="a" type="m">Stà</rhyme>!</l>
        <l id="s19:l2" met="3" type="-+">De Ronde, qui va <rhyme label="a" type="m" corresp="#s19:l1">là</rhyme>?</l>
        <l id="s19:l3" met="3" type="-+">Ick wiert een bed <rhyme label="b" type="f">geslagen</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Mijn Sleuyer raeckt' ick <rhyme label="c" type="m">quijt</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">En, dat my 't meeste <rhyme label="c" type="m" corresp="#s19:l4">spijt</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Voor Hoer sy my <rhyme label="b" type="f" corresp="#s19:l3">aensagen</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">'k Besweer u Dochters, <rhyme label="a" type="m">hoort</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">En daer mé loop ick <rhyme label="a" type="m" corresp="#s20:l1">voort</rhyme>;</l>
        <l id="s20:l3" met="3" type="-+">Soo ghy mijn Lief <rhyme label="b" type="f">ontmoetet</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">In Stadt, of op het <rhyme label="c" type="m">Velt</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">Segt hoe ick ben <rhyme label="c" type="m" corresp="#s20:l4">gestelt</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">En hem aldus <rhyme label="b" type="f" corresp="#s20:l3">begroetet</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Sy is van Liefde <rhyme label="a" type="m">kranck</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">Sy nut noch spijs noch <rhyme label="a" type="m" corresp="#s21:l1">dranck</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">Se seyt, ick kan niet <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Ten sy, mijn <rhyme label="c" type="m">Brudegom</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Mijn Ziele, <rhyme label="c" type="m" corresp="#s21:l4">wederom</rhyme></l>
        <l id="s21:l6" met="3" type="-+">Als vooren wert <rhyme label="b" type="f" corresp="#s21:l3">gegeven</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Ghy stoft dus van u <rhyme label="a" type="m">Heer</rhyme>;</l>
        <l id="s22:l2" met="3" type="-+">Maer segt ons, is hy <rhyme label="a" type="m" corresp="#s22:l1">meer</rhyme>?</l>
        <l id="s22:l3" met="3" type="-+">(ô Schoonste vande <rhyme label="b" type="f">Vrouwen</rhyme>!)</l>
        <l id="s22:l4" met="3" type="-+">Als, die door Godes <rhyme label="c" type="m">Geest</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Gedreven sijn <rhyme label="c" type="m" corresp="#s22:l4">geweest</rhyme></l>
        <l id="s22:l6" met="3" type="-+">Om Israël te <rhyme label="b" type="f" corresp="#s22:l3">bouwen</rhyme>?</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">Den grooten <rhyme label="a" type="m">Abraham</rhyme>,</l>
        <l id="s23:l2" met="3" type="-+">Of yemant vande <rhyme label="a" type="m" corresp="#s23:l1">Stam</rhyme></l>
        <l id="s23:l3" met="3" type="-+">En Choren der <rhyme label="b" type="f">Propheten</rhyme>?</l>
        <l id="s23:l4" met="3" type="-+">Die voor, of na de <rhyme label="c" type="m">Wet</rhyme>,</l>
        <l id="s23:l5" met="3" type="-+">Van Godt zijn <rhyme label="c" type="m" corresp="#s23:l4">ingeset</rhyme>?</l>
        <l id="s23:l6" met="3" type="-+">Dat wouden wy wel <rhyme label="b" type="f" corresp="#s23:l3">weten</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Ia, waerlick is hy <rhyme label="a" type="m">meer</rhyme>;</l>
        <l id="s24:l2" met="3" type="-+">Immanuel mijn <rhyme label="a" type="m" corresp="#s24:l1">Heer</rhyme>,</l>
        <l id="s24:l3" met="3" type="-+">Is Blanck, en sonder <rhyme label="b" type="f">sonden</rhyme>:</l>
        <l id="s24:l4" met="3" type="-+">Bespa en Root van <rhyme label="c" type="m">bloet</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">Dus hadmen hem <rhyme label="c" type="m" corresp="#s24:l4">begroet'</rhyme>,</l>
        <l id="s24:l6" met="3" type="-+">En jammerlijck <rhyme label="b" type="f" corresp="#s24:l3">geschonden</rhyme>:</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">Gelijcker-wijs, 't <rhyme label="a" type="m">geswier</rhyme></l>
        <l id="s25:l2" met="3" type="-+">Van 't moedige <rhyme label="a" type="m" corresp="#s25:l1">Banier</rhyme>,</l>
        <l id="s25:l3" met="3" type="-+">Uytnemend 'is <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">Hy oock alsoo <rhyme label="c" type="m">uytmuyt</rhyme>,</l>
        <l id="s25:l5" met="3" type="-+">In sterckte boven <rhyme label="c" type="m" corresp="#s25:l4">uyt</rhyme>,</l>
        <l id="s25:l6" met="3" type="-+">Van al die Leefd' of <rhyme label="b" type="f" corresp="#s25:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">Sijn Hooft [sijn <rhyme label="a" type="m">Godlick-rijck</rhyme>,</l>
        <l id="s26:l2" met="3" type="-+">Dat geen heeft sijns <rhyme label="a" type="m" corresp="#s26:l1">gelijck</rhyme>]</l>
        <l id="s26:l3" met="3" type="-+">Is vast als Gout; sijn <rhyme label="b" type="f">Locken</rhyme>,</l>
        <l id="s26:l4" met="3" type="-+">De sterckten van sijn <rhyme label="c" type="m">Hart</rhyme></l>
        <l id="s26:l5" met="3" type="-+">Als Ravens sijn soo <rhyme label="c" type="m" corresp="#s26:l4">swart</rhyme>,</l>
        <l id="s26:l6" met="3" type="-+">Gekrult op-ronde <rhyme label="b" type="f" corresp="#s26:l3">stocken</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="3" type="-+">Sijn Oogen [sijn <rhyme label="a" type="m">gesicht</rhyme>,</l>
        <l id="s27:l2" met="3" type="-+">Sijn wetenschap en <rhyme label="a" type="m" corresp="#s27:l1">licht</rhyme>]</l>
        <l id="s27:l3" met="3" type="-+">Zijn wit en klaer <rhyme label="b" type="f">gewassen</rhyme>,</l>
        <l id="s27:l4" met="3" type="-+">Gelijck der Duyven <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s27:l5" met="3" type="-+">Die cierelick en <rhyme label="c" type="m" corresp="#s27:l4">sijn</rhyme>,</l>
        <l id="s27:l6" met="3" type="-+">Als Ringen staen in <rhyme label="b" type="f" corresp="#s27:l3">kassen</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="3" type="-+">Zijn Wangen, sijne <rhyme label="a" type="m">gunst</rhyme></l>
        <l id="s28:l2" met="3" type="-+">Zijn Toorens, na de <rhyme label="a" type="m" corresp="#s28:l1">kunst</rhyme></l>
        <l id="s28:l3" met="3" type="-+">Van Specerie <rhyme label="b" type="f">beddens</rhyme>,</l>
        <l id="s28:l4" met="3" type="-+">Zijn Lippen, root en <rhyme label="c" type="m">schoon</rhyme>,</l>
        <l id="s28:l5" met="3" type="-+">Gelijck een Lely <rhyme label="c" type="m" corresp="#s28:l4">Kroon</rhyme>,</l>
        <l id="s28:l6" met="3" type="-+">Van Myrrh', nat als de <rhyme label="b" type="f" corresp="#s28:l3">Weddens</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="3" type="-+">Zijn Handen, [wat hy <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s29:l2" met="3" type="-+">Voor wie het is, ten <rhyme label="a" type="m" corresp="#s29:l1">goed'</rhyme>,]</l>
        <l id="s29:l3" met="3" type="-+">Zijn, als de goude <rhyme label="b" type="f">Ringen</rhyme>,</l>
        <l id="s29:l4" met="3" type="-+">Met Turre-kois <rhyme label="c" type="m">gevult</rhyme>.</l>
        <l id="s29:l5" met="3" type="-+">Zijn Lichaem, [zyn <rhyme label="c" type="m" corresp="#s29:l4">gedult</rhyme></l>
        <l id="s29:l6" met="3" type="-+">En liefden die hem <rhyme label="b" type="f" corresp="#s29:l3">dwingen</rhyme>]</l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="3" type="-+">Is blinckend' <rhyme label="a" type="m">Elpenbeen</rhyme>,</l>
        <l id="s30:l2" met="3" type="-+">Rontom en over <rhyme label="a" type="m" corresp="#s30:l1">heen</rhyme>,</l>
        <l id="s30:l3" met="3" type="-+">Met Sapphir <rhyme label="b" type="f">overtogen</rhyme>,</l>
        <l id="s30:l4" met="3" type="-+">Zyn Schenckelen, [zyn <rhyme label="c" type="m">sterckt'</rhyme>,</l>
        <l id="s30:l5" met="3" type="-+">Waer door hy wonder <rhyme label="c" type="m" corresp="#s30:l4">werckt</rhyme>,</l>
        <l id="s30:l6" met="3" type="-+">Zyn Goddelijck <rhyme label="b" type="f" corresp="#s30:l3">vermogen</rhyme>,]</l>
      </lg>
      <lg id="s31" type="stanza">
        <l id="s31:l1" met="3" type="-+">Zyn vast, als een <rhyme label="a" type="m">Pilaer</rhyme>,</l>
        <l id="s31:l2" met="3" type="-+">Van Marmer Wit en <rhyme label="a" type="m" corresp="#s31:l1">swaer</rhyme>,</l>
        <l id="s31:l3" met="3" type="-+">Gegront op goude <rhyme label="b" type="f">Voeten</rhyme>.</l>
        <l id="s31:l4" met="3" type="-+">'t Gestel van 't hele <rhyme label="c" type="m">Lijf</rhyme>,</l>
        <l id="s31:l5" met="3" type="-+">Zyn wesen en <rhyme label="c" type="m" corresp="#s31:l4">bedrijf</rhyme></l>
        <l id="s31:l6" met="3" type="-+">Gelijckenen wy <rhyme label="b" type="f" corresp="#s31:l3">moeten</rhyme></l>
      </lg>
      <lg id="s32" type="stanza">
        <l id="s32:l1" met="3" type="-+">By d'aldergrootste <rhyme label="a" type="m">Mast</rhyme>,</l>
        <l id="s32:l2" met="3" type="-+">Die op den Lib'non <rhyme label="a" type="m" corresp="#s32:l1">wast</rhyme>.</l>
        <l id="s32:l3" met="3" type="-+">'t Gehemelt dat ick <rhyme label="b" type="f">vonde</rhyme>,</l>
        <l id="s32:l4" met="3" type="-+">Dat is, [zyn heyligh <rhyme label="c" type="m">Woort</rhyme>,</l>
        <l id="s32:l5" met="3" type="-+">En wat ick hadt <rhyme label="c" type="m" corresp="#s32:l4">gehoort</rhyme>]</l>
        <l id="s32:l6" met="3" type="-+">Was my, en mynen <rhyme label="b" type="f" corresp="#s32:l3">Monde</rhyme></l>
      </lg>
      <lg id="s33" type="stanza">
        <l id="s33:l1" met="3" type="-+">Een enckel <rhyme label="a" type="m">soetigheyt</rhyme>,</l>
        <l id="s33:l2" met="3" type="-+">Met al wat aen hem <rhyme label="a" type="m" corresp="#s33:l1">leyt</rhyme>.</l>
        <l id="s33:l3" met="3" type="-+">Siet! siet! soo <rhyme label="b" type="f">uytverkoren</rhyme>,</l>
        <l id="s33:l4" met="3" type="-+">Soo schoon, soo <rhyme label="c" type="m">heerelick</rhyme>,</l>
        <l id="s33:l5" met="3" type="-+">Is hy myn <rhyme label="c" type="m" corresp="#s33:l4">Ander-Ick</rhyme>,</l>
        <l id="s33:l6" met="3" type="-+">Dien heb ick laes! <rhyme label="b" type="f" corresp="#s33:l3">verloren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
