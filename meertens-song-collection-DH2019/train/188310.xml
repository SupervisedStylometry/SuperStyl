    <melody>O dierbare vrijheid edele maagd</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Echtzon! doelwit van de <rhyme label="a" type="m">Min</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Versprei uw Goude <rhyme label="b" type="f">vonken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Doet Lyris, en zyn <rhyme label="a" type="m" corresp="#s1:l1">Zielsvriendin</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Met Huwlyks Loovren <rhyme label="b" type="f" corresp="#s1:l2">pronken</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Voert in gezelschap van uw <rhyme label="c" type="m">glans</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">De Vree en dartle vreugd ten <rhyme label="c" type="m" corresp="#s1:l5">dans</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Om 't Juichend Paar te <rhyme label="d" type="f">streelen</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Dan zal de lieve <rhyme label="e" type="m">Klorimeen</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Met n'yvren Lyris vrolyk <rhyme label="e" type="m" corresp="#s1:l8">treen</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">In 's Huwlyks Min <rhyme label="d" type="f" corresp="#s1:l7">gareelen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
