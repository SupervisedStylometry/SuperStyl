  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Hoe kan de <rhyme label="a" type="m">Wyn</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">'t Gebruyk der <rhyme label="b" type="f">sinnen</rhyme></l>
        <l id="s1:l3" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s1:l2">Verwinnen</rhyme>
        </l>
        <l id="s1:l4" met="3" type="-+">En roeren ons <rhyme label="a" type="m" corresp="#s1:l1">bryn</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Ja, gelijk <rhyme label="c" type="m">sot</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Stellen ten <rhyme label="c" type="m" corresp="#s1:l5">spot</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Die al te veel haar kragt <rhyme label="b" type="f" corresp="#s1:l2">beminnen</rhyme>!</l>
        <l id="s1:l8" met="2" type="-+">Hoe kan de <rhyme label="a" type="m" corresp="#s1:l1">wyn</rhyme></l>
        <l id="s1:l9" met="2" type="-+">'t Gebruyk der <rhyme label="b" type="f" corresp="#s1:l2">sinnen</rhyme></l>
        <l id="s1:l10" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s1:l2">Verwinnen</rhyme>
        </l>
        <l id="s1:l11" met="3" type="+-">En roeren ons <rhyme label="a" type="m" corresp="#s1:l1">bryn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Want hare <rhyme label="a" type="m">vreugd</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Is in dit <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s2:l3" met="1" type="-+"><rhyme label="b" type="f" corresp="#s2:l2">Gegeven</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op dat sy 't bryn <rhyme label="a" type="m" corresp="#s2:l1">verheugd</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">En alle <rhyme label="c" type="m">smert</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Dryft uyt het <rhyme label="c" type="m" corresp="#s2:l5">hert</rhyme>;</l>
        <l id="s2:l7" met="4" type="-+">Aan haar gebruyk wert niets <rhyme label="b" type="f" corresp="#s2:l2">misdreven</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Vant   haar <rhyme label="a" type="m" corresp="#s2:l1">vreugd</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Is in dit <rhyme label="b" type="f" corresp="#s2:l2">leven</rhyme></l>
        <l id="s2:l10" met="1" type="-+"><rhyme label="b" type="f" corresp="#s2:l2">Gegeven</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Op dat sy 't bryn <rhyme label="a" type="m" corresp="#s2:l1">verheugd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Maar vlied <rhyme label="a" type="m">altyd</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Het <rhyme label="b" type="f">overdadig</rhyme></l>
        <l id="s3:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s3:l2">stadig</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Misbruyk der <rhyme label="a" type="m" corresp="#s3:l1">gulsigheid</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Dat aan 't <rhyme label="c" type="m">gemoed</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Slegts schade <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme>;</l>
        <l id="s3:l7" met="4" type="-+">Die wellust maakt de Ziel <rhyme label="b" type="f" corresp="#s3:l2">misdadig</rhyme>.</l>
        <l id="s3:l8" met="2" type="-+">Maar vlied <rhyme label="a" type="m" corresp="#s3:l1">altyd</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Het <rhyme label="b" type="f" corresp="#s3:l2">overdadig</rhyme></l>
        <l id="s3:l10" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s3:l2">schadig</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Misbruyk der <rhyme label="a" type="m" corresp="#s3:l1">gulsigheid</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">De <rhyme label="a" type="m">overdaad</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Doet maat en <rhyme label="b" type="f">reden</rhyme></l>
        <l id="s4:l3" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s4:l2">Vertreden</rhyme>
        </l>
        <l id="s4:l4" met="3" type="-+">En baard een schandig <rhyme label="a" type="m" corresp="#s4:l1">kwaad</rhyme>;</l>
        <l id="s4:l5" met="2" type="-+">Krenkt het <rhyme label="c" type="m">verstand</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Dat sy <rhyme label="c" type="m" corresp="#s4:l5">vermand</rhyme></l>
        <l id="s4:l7" met="4" type="-+">En brengd veel kwalen aan de <rhyme label="b" type="f" corresp="#s4:l2">leden</rhyme>.</l>
        <l id="s4:l8" met="2" type="-+">De <rhyme label="a" type="m" corresp="#s4:l1">overdaad</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Doet maat en <rhyme label="b" type="f" corresp="#s4:l2">reden</rhyme></l>
        <l id="s4:l10" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s4:l2">Vertreden</rhyme>
        </l>
        <l id="s4:l11" met="3" type="+-">En baard een schandig <rhyme label="a" type="m" corresp="#s4:l1">kwaad</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Laat dan haar <rhyme label="a" type="m">kragt</rhyme></l>
        <l id="s5:l2" met="2" type="-+">En soete <rhyme label="b" type="f">smaken</rhyme></l>
        <l id="s5:l3" met="1" type="-+">Noyt <rhyme label="b" type="f" corresp="#s5:l2">maken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat gy de maat <rhyme label="a" type="m" corresp="#s5:l1">veragt</rhyme>;</l>
        <l id="s5:l5" met="2" type="-+">De vreugd <rhyme label="c" type="m">bestaat</rhyme></l>
        <l id="s5:l6" met="2" type="-+">In <rhyme label="c" type="m" corresp="#s5:l5">middelmaat</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Des laat sy matig u <rhyme label="b" type="f" corresp="#s5:l2">vermaken</rhyme>.</l>
        <l id="s5:l8" met="2" type="-+">Laat dan haar <rhyme label="a" type="m" corresp="#s5:l1">kragt</rhyme></l>
        <l id="s5:l9" met="2" type="-+">En soete <rhyme label="b" type="f" corresp="#s5:l2">smaken</rhyme></l>
        <l id="s5:l10" met="1" type="-+">Noyt <rhyme label="b" type="f" corresp="#s5:l2">maken</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Dat gy de maat <rhyme label="a" type="m" corresp="#s5:l1">veragt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
