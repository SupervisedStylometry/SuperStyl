<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157602</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nerea schoonste van uw geburen</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HOe bitter moest eens Rachel <rhyme label="a" type="f">weenen</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">Hoe droevigh was wel eer haer <rhyme label="b" type="m">stant</rhyme>?</l>
        <l id="s1:l3" met="4" type="-+">Wanneer haer Volck gevangen <rhyme label="a" type="f" corresp="#s1:l1">henen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Wiert wegh-gevoert in Babels <rhyme label="b" type="m" corresp="#s1:l2">Landt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Om thien-mael seven Iaer te <rhyme label="a" type="f">dwalen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Verschoven van Godts Huys en <rhyme label="b" type="m">Stadt</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Dees droefheyt is niet op te <rhyme label="a" type="f" corresp="#s2:l1">halen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Daer Godes Volck bedruckt in <rhyme label="b" type="m" corresp="#s2:l2">sat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als na dees tijdt verloopen <rhyme label="a" type="f">waren</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ses hondert Iaer en vijf daer <rhyme label="b" type="m">na</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Komt Godes Soon hem <rhyme label="a" type="f" corresp="#s3:l1">openbaren</rhyme></l>
        <l id="s3:l4" met="4" type="-+">In 't Vleesch tot Belthlem <rhyme label="b" type="m" corresp="#s3:l2">Ephrata</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Wijsen uyt den Oosten <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Om aen te bidden desen <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En hebben schatten <rhyme label="a" type="f" corresp="#s4:l1">mee-genomen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die sy hem schencken t'sijner <rhyme label="b" type="m" corresp="#s4:l2">eer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dit klinckt Herodes in sijn <rhyme label="a" type="f">ooren</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ierusalem staet als <rhyme label="b" type="m">verdooft</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hy mach van desen Vorst niet <rhyme label="a" type="f" corresp="#s5:l1">hooren</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Of meent de Kroon valt hem van 't <rhyme label="b" type="m" corresp="#s5:l2">hooft</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sijn Heydens hert met wraeck <rhyme label="a" type="f">beladen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Geeft hem geen rust in desen <rhyme label="b" type="m">stant</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Soo hy sijn boosheyt sal <rhyme label="a" type="f" corresp="#s6:l1">versaden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Soo moet d'onnoosele van <rhyme label="b" type="m" corresp="#s6:l2">kant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dies hy zijn Dienaers heeft <rhyme label="a" type="f">geboden</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Tot Bethlehem en daer <rhyme label="b" type="m">omtrent</rhyme></l>
        <l id="s7:l3" met="4" type="-+">De Kinders van twee Iaer te <rhyme label="a" type="f" corresp="#s7:l1">dooden</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En al die jonger zijn <rhyme label="b" type="m" corresp="#s7:l2">bekent</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Terstont aen 't hacken ende <rhyme label="a" type="f">kerven</rhyme>;</l>
        <l id="s8:l2" met="4" type="-+">Ia selfs de Kind'ren van de <rhyme label="b" type="m">Vorst</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">'t Moest al door hare handen <rhyme label="a" type="f" corresp="#s8:l1">sterven</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Soo in de Wiegh, als aen de <rhyme label="b" type="m" corresp="#s8:l2">borst</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O Rachel! weent, 't is tijdt van <rhyme label="a" type="f">weenen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Verheft u stem tot Godt u <rhyme label="b" type="m">Heer</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">Want 't bloedt loopt langhs de straten <rhyme label="a" type="f" corresp="#s9:l1">heenen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">U jonge jeught en is niet <rhyme label="b" type="m" corresp="#s9:l2">meer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ghy siet u teere Vrucht <rhyme label="a" type="f">vernielen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Geruckt, geslingert van u <rhyme label="b" type="m">schoot</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Om aen een post of steen <rhyme label="a" type="f" corresp="#s10:l1">t'ontzielen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">O moordt, en schrickelijcke <rhyme label="b" type="m" corresp="#s10:l2">doodt</rhyme>!</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Maer, ghy Herodes, meught wel <rhyme label="a" type="f">vreesen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">U Heydens hert doet hier een <rhyme label="b" type="m">quaet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Dat van de Werelt wort <rhyme label="a" type="f" corresp="#s11:l1">mispresen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">En in Godts schuldt-boeck open <rhyme label="b" type="m" corresp="#s11:l2">staet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">O Vrede-Vorst en Hemels <rhyme label="a" type="f">Koningh</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Hoe werdt u blijde komst <rhyme label="b" type="m">beschreyt</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Regeer ons hier, op dat u <rhyme label="a" type="f" corresp="#s12:l1">wooningh</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Voor ons sy in der <rhyme label="b" type="m" corresp="#s12:l2">eeuwigheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
