<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>1526</id>
    <date>
      <year-start>1613</year-start>
      <year-end>1613</year-end>
    </date>
    <location>
      <orginal-name>amsterdam?</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Houd aan ik zal u zingen voor</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+">
          <rhyme label="a" type="m">VErblijt</rhyme>
        </l>
        <l id="s1:l2" met="3" type="-+">Ghy Amoreuse <rhyme label="b" type="f">sinnen</rhyme>,</l>
        <l id="s1:l3" met="1" type="-+">
          <rhyme label="a" type="m" corresp="#s1:l1">Versmijt</rhyme>
        </l>
        <l id="s1:l4" met="3" type="-+">Druck die't hart quelt van <rhyme label="b" type="f" corresp="#s1:l2">binnen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En wilt op nieu <rhyme label="b" type="f" corresp="#s1:l2">beginnen</rhyme></l>
        <l id="s1:l6" met="1" type="-+">Te <rhyme label="b" type="f" corresp="#s1:l2">minnen</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Int nieuwe <rhyme label="c" type="m">Iaer</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Op dat ghy <rhyme label="d" type="m">meught</rhyme></l>
        <l id="s1:l9" met="2" type="-+">In vreught // <rhyme label="d" type="m" corresp="#s1:l8">verheught</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Werden hier <rhyme label="c" type="m" corresp="#s1:l7">naer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="1" type="-+">Voeght <rhyme label="a" type="m">u</rhyme></l>
        <l id="s2:l2" met="3" type="-+">By haer voor alle <rhyme label="b" type="f">saken</rhyme>,</l>
        <l id="s2:l3" met="1" type="-+">Ploeght <rhyme label="a" type="m" corresp="#s2:l1">nu</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Om in haer sin te <rhyme label="b" type="f" corresp="#s2:l2">raken</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Acht op gheen nijders <rhyme label="b" type="f" corresp="#s2:l2">kaken</rhyme></l>
        <l id="s2:l6" met="1" type="-+">Wilt <rhyme label="b" type="f" corresp="#s2:l2">laken</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Al haer <rhyme label="c" type="m">opset</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Die u dick <rhyme label="d" type="m">hier</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Met quaet <rhyme label="d" type="m" corresp="#s2:l8">bestier</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Zijn een <rhyme label="c" type="m" corresp="#s2:l7">belet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="1" type="-+">Op't <rhyme label="a" type="m">Ys</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Moet ghy u oock <rhyme label="b" type="f">versellen</rhyme></l>
        <l id="s3:l3" met="1" type="-+">Om <rhyme label="a" type="m" corresp="#s3:l1">prijs</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die Slede daer voor <rhyme label="b" type="f" corresp="#s3:l2">stellen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Behanght u Paert met <rhyme label="b" type="f" corresp="#s3:l2">Bellen</rhyme></l>
        <l id="s3:l6" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s3:l2">schellen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Soo vol alst <rhyme label="c" type="m">mach</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Dus <rhyme label="d" type="m">rijt</rhyme></l>
        <l id="s3:l9" met="2" type="-+">en <rhyme label="d" type="m" corresp="#s3:l8">vrijt</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Om u profijt den heelen <rhyme label="c" type="m" corresp="#s3:l7">dagh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="1" type="-+">Voort <rhyme label="a" type="m">an</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Volghen de <rhyme label="b" type="f">Copper-daghen</rhyme>,</l>
        <l id="s4:l3" met="1" type="-+">Spoort <rhyme label="a" type="m" corresp="#s4:l1">dan</rhyme></l>
        <l id="s4:l4" met="3" type="-+">V liefste te <rhyme label="b" type="f" corresp="#s4:l2">behaghen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die u hert doet <rhyme label="b" type="f" corresp="#s4:l2">knaghen</rhyme>,</l>
        <l id="s4:l6" met="1" type="-+">Wilt <rhyme label="b" type="f" corresp="#s4:l2">vraghen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">En nooden <rhyme label="c" type="m">haer</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Op een <rhyme label="d" type="m">bancket</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Daer ghy dan <rhyme label="d" type="m" corresp="#s4:l8">net</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Meught maken <rhyme label="c" type="m" corresp="#s4:l7">klaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="1" type="-+">
          <rhyme label="a" type="m">Prince</rhyme>
        </l>
        <l id="s5:l2" met="3" type="-+">En wilt den moedt niet <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s5:l3" met="1" type="-+">
          <rhyme label="a" type="m" corresp="#s5:l1">Mintse</rhyme>
        </l>
        <l id="s5:l4" met="3" type="-+">Wilt haer met liefd' <rhyme label="b" type="f" corresp="#s5:l2">aenkleven</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En alsoo al u <rhyme label="b" type="f" corresp="#s5:l2">leven</rhyme></l>
        <l id="s5:l6" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s5:l2">Beneven</rhyme>
        </l>
        <l id="s5:l7" met="2" type="-+">Haer blijven <rhyme label="c" type="m">trou</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">In eer <rhyme label="d" type="m">bequaem</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Tot dat ghy <rhyme label="d" type="m" corresp="#s5:l8">t'saem</rhyme></l>
        <l id="s5:l10" met="2" type="-+">Wert Man en <rhyme label="c" type="m" corresp="#s5:l7">Vrou</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
