<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156271</id>
    <date>
      <year-start>1712</year-start>
      <year-end>1712</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hansje sneed het koren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ik ga so <rhyme label="a" type="m">vaek</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Tot mijn <rhyme label="a" type="m" corresp="#s1:l1">vermaak</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Aan 't Beekje by de <rhyme label="b" type="f">Linde</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">En dat <rhyme label="c" type="m">alleen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Omdat ik <rhyme label="c" type="m" corresp="#s1:l4">meen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Mijn Nimphje daer te <rhyme label="b" type="f" corresp="#s1:l3">vinde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">'t Gebergt en <rhyme label="a" type="m">top</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Dat klim ik <rhyme label="a" type="m" corresp="#s2:l1">op</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En ga door het <rhyme label="b" type="f">boschage</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Waar Echo <rhyme label="c" type="m">meld</rhyme></l>
        <l id="s2:l5" met="2" type="-+">En my <rhyme label="c" type="m" corresp="#s2:l4">verteld</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Zoo menig <rhyme label="b" type="f" corresp="#s2:l3">vryerage</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Doch 't is <rhyme label="a" type="m">verdriet</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Als 't oog niet <rhyme label="a" type="m" corresp="#s3:l1">ziet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Waer hert en ziel na <rhyme label="b" type="f">haken</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Dat is het <rhyme label="c" type="m">schoon</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Van uw <rhyme label="c" type="m" corresp="#s3:l4">persoon</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En rood gebloosde <rhyme label="b" type="f" corresp="#s3:l3">kaken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Uw lipjes <rhyme label="a" type="m">teer</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Vermogen <rhyme label="a" type="m" corresp="#s4:l1">meer</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Als Phoebi gulde <rhyme label="b" type="f">stralen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Dewyl, <rhyme label="c" type="m">Me-vrou</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Des Hemels <rhyme label="c" type="m" corresp="#s4:l4">dou</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Komt op uw lipjes <rhyme label="b" type="f" corresp="#s4:l3">dalen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ach <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Mogt ik mijn <rhyme label="a" type="m" corresp="#s5:l1">Min</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Eens door uw lipjes <rhyme label="b" type="f">blussen</rhyme>!</l>
        <l id="s5:l4" met="2" type="-+">Een kleine <rhyme label="c" type="m">zaak</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Is mijn <rhyme label="c" type="m" corresp="#s5:l4">vermaak</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Mijn Nimphjen eens te <rhyme label="b" type="f" corresp="#s5:l3">kussen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
