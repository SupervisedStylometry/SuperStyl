<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8862</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ach Gott vom Himmel</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOdt swijcht doch niet, noch zijt so <rhyme label="a" type="m">stil</rhyme></l>
        <l id="s1:l2" met="3" type="-+">End' beydet niet so <rhyme label="b" type="f">langhe</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Verwoedet is des Vyandts <rhyme label="a" type="m" corresp="#s1:l1">wil</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En raeden ons so <rhyme label="b" type="f" corresp="#s1:l2">banghe</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dijn Haters met verbolghen <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Raedtslaen soo listich end' <rhyme label="c" type="m" corresp="#s1:l5">onvroet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Teghen dyn Volck <rhyme label="d" type="f">verborghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sy pooghen ons t'wtroeyen <rhyme label="a" type="m">fel</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat wy gheen Volck en <rhyme label="b" type="f">blijven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wat is des Naemens <rhyme label="a" type="m" corresp="#s2:l1">Israel</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat dencken sy <rhyme label="b" type="f" corresp="#s2:l2">t'ontlijven</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Vereenighen sy haer nu <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Om all' in een Verbondt te <rhyme label="c" type="m" corresp="#s2:l5">staen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Beyd' Ismael end' <rhyme label="d" type="f">Edom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Des Moabs end' des Hagars <rhyme label="a" type="m">Zaet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Tyrus end' <rhyme label="b" type="f">Philistijnen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Amon, Gebal, Amaleck <rhyme label="a" type="m" corresp="#s3:l1">quaet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Een yeder met den <rhyme label="b" type="f" corresp="#s3:l2">zijnen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Jaer Assur is daer oock <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Den Kindren Loths dit quade <rhyme label="c" type="m" corresp="#s3:l5">Feyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Om helpen te <rhyme label="d" type="f">bedrijven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer (Heer) doet haer als <rhyme label="a" type="m">Madian</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als Sissera met <rhyme label="b" type="f">Naemen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Als Jabin aen die Beeck <rhyme label="a" type="m" corresp="#s4:l1">Kison</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die by Endor om <rhyme label="b" type="f" corresp="#s4:l2">quamen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Als Oreb end' als ZEeb met <rhyme label="c" type="m">Macht</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Als Zeba end' Salmuna <rhyme label="c" type="m" corresp="#s4:l5">pracht</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Maeckt soo haer Vorsten <rhyme label="d" type="f">(HEERE</rhyme>)</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want sy haer Aenslach thoonen <rhyme label="a" type="m">fel</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Waer sy Godts Huyse <rhyme label="b" type="f">vinden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">HEER maecktse met onweder <rhyme label="a" type="m" corresp="#s5:l1">snel</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Als Stoppel voor den <rhyme label="b" type="f" corresp="#s5:l2">winde</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ghelijck een vuyr den Woud' <rhyme label="c" type="m">verbrant</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">En Blixems Vlamme wort <rhyme label="c" type="m" corresp="#s5:l5">bekant</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Midts groote Berch <rhyme label="d" type="f">ontsteken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">So dapper haer dyn Onweer <rhyme label="a" type="m">raeck</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat sy van Schrick <rhyme label="b" type="f">vertsaghen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Haer Aensicht soo vol schande <rhyme label="a" type="m" corresp="#s6:l1">maeck</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat sy noch moeten <rhyme label="b" type="f" corresp="#s6:l2">Vraghen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Nae dynen Naem tot dyner <rhyme label="c" type="m">Eer</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Verschricken altoos meer end' <rhyme label="c" type="m" corresp="#s6:l5">meer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">En schamen sonder <rhyme label="d" type="f">eynde</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ja schand' so groot word' haer te <rhyme label="a" type="m">deel</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dats' omcomen al <rhyme label="b" type="f">tsamen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Soo sullen sy bekennnen <rhyme label="a" type="m" corresp="#s7:l1">heel</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dat ghy met dynen <rhyme label="b" type="f" corresp="#s7:l2">Namen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Alleen zijt Godt end' heetet <rhyme label="c" type="m">HEER</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Ghy God de Hoochst' end' niemant <rhyme label="c" type="m" corresp="#s7:l5">meer</rhyme></l>
        <l id="s7:l7" met="3" type="-+">In alle Werelt <rhyme label="d" type="f">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
