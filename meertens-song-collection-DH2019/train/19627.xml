      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Polyphemus aan de stranden</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Thomas Fonteyn</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KOmt mijnZieltjen, wilt <rhyme label="a" type="f">be-oogen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Wiens <rhyme label="a" type="f" corresp="#s1:l1">vermoogen</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Gantsch en gaer leght in u <rhyme label="b" type="m">macht</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Wilt doch helpen uyt het <rhyme label="c" type="f">lijen</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s1:l4">verblijen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Die nae u Weer-liefde <rhyme label="b" type="m" corresp="#s1:l3">wacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wilt het woordtjen Neen <rhyme label="a" type="f">verdrencken</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Ende <rhyme label="a" type="f" corresp="#s2:l1">schencken</rhyme></l>
        <l id="s2:l3" met="4" type="+-">V Dienaer ‘t gewenschte <rhyme label="b" type="m">Iae</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">‘t Geen hy met een groot <rhyme label="c" type="f">verlangen</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Sal <rhyme label="c" type="f" corresp="#s2:l4">ontfangen</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">Spoeyt u Lief, ey doet sulcks <rhyme label="b" type="m" corresp="#s2:l3">dra</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dan sal trouwheydt steets <rhyme label="a" type="f">vermeeren</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Iae <rhyme label="a" type="f" corresp="#s3:l1">beheeren</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Onse herten, eens <rhyme label="b" type="m">gesint</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Die in Liefde sullen <rhyme label="c" type="f">bloeyen</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Ende <rhyme label="c" type="f" corresp="#s3:l4">groeyen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat geen trouwer Twee men <rhyme label="b" type="m" corresp="#s3:l3">vindt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Waer na langer onse <rhyme label="a" type="f">lusjes</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Soete <rhyme label="a" type="f" corresp="#s4:l1">kusjes</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Op-gehouden? Ach! ick <rhyme label="b" type="m">smacht</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Moet ick langer leggen <rhyme label="c" type="f">hijgen</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Nae ‘t <rhyme label="c" type="f" corresp="#s4:l4">verkrijghen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Van Mins soetheydt dagh en <rhyme label="b" type="m" corresp="#s4:l3">nacht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dus Beminde wilt u <rhyme label="a" type="f">Ooren</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Niet als <rhyme label="a" type="f" corresp="#s5:l1">voren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Neyghen van u touwe <rhyme label="b" type="m">Slaef</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Maer u soete <rhyme label="c" type="f">Minne-lonckjes</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Herte <rhyme label="c" type="f" corresp="#s5:l4">vonckjes</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Off’ren aen my, tot een <rhyme label="b" type="m" corresp="#s5:l3">gaef</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Tot een teecken van <rhyme label="a" type="f">Weer-minne</rhyme></l>
        <l id="s6:l2" met="2" type="+-">‘t Welck mijn <rhyme label="a" type="f" corresp="#s6:l1">sinnen</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Warren sal in Liefdes <rhyme label="b" type="m">net</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Daer ick nimmer uyt sal <rhyme label="c" type="f">raecken</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Maer u <rhyme label="c" type="f" corresp="#s6:l4">kaecken</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Kussen sal op’t <rhyme label="b" type="m" corresp="#s6:l3">Echte-bedt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
