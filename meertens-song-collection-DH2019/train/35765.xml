    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ANna de heylighste <rhyme label="a" type="m">Vrouw</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Was in <rhyme label="a" type="m" corresp="#s1:l1">rouw</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En haer Man Ioachim <rhyme label="b" type="f">mede</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Hy wast oudt, Sy <rhyme label="c" type="m">onvruchtbaer</rhyme>;</l>
        <l id="s1:l5" met="2" type="+-">'t Heyligh <rhyme label="c" type="m" corresp="#s1:l4">paer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Om een Kindt hebbe <rhyme label="b" type="f" corresp="#s1:l3">gebeden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Anna uyt een Engel <rhyme label="a" type="m">hoort</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">'t Blijdste <rhyme label="a" type="m" corresp="#s2:l1">woordt</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Godes Moeder sult ghy <rhyme label="b" type="f">baren</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Maria sal zijn haer <rhyme label="c" type="m">naem</rhyme>;</l>
        <l id="s2:l5" met="2" type="+-">Groot haer <rhyme label="c" type="m" corresp="#s2:l4">faem</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sal wesen, by Godes <rhyme label="b" type="f" corresp="#s2:l3">scharen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Den alderheylighste <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Die <rhyme label="a" type="m" corresp="#s3:l1">verjaeght</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Verplet 't hooft van d' helsche <rhyme label="b" type="f">Slangen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Sonder erf-sondt, ende <rhyme label="c" type="m">smet</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Reyn en <rhyme label="c" type="m" corresp="#s3:l4">net</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Is geboore, en <rhyme label="b" type="f" corresp="#s3:l3">ontfangen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Al leyt sy als 't minste <rhyme label="a" type="m">kindt</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Dat men <rhyme label="a" type="m" corresp="#s4:l1">vindt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">In haer nochtans zijn te <rhyme label="b" type="f">vinden</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Al Goodts gaven, die ghy <rhyme label="c" type="m">weet</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Niet <rhyme label="c" type="m" corresp="#s4:l4">vergeet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Deught in d' Eng'len, en Goodts <rhyme label="b" type="f" corresp="#s4:l3">vrinden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Onse Salighmaker <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Sonn' en <rhyme label="a" type="m" corresp="#s5:l1">Al</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Uyt dese dageraedt <rhyme label="b" type="f">rijsen</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Daer toe is bequaem <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Anders <rhyme label="c" type="m" corresp="#s5:l4">geen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Naest Godt wilt haer eer <rhyme label="b" type="f" corresp="#s5:l3">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Iesus, ons opperste <rhyme label="a" type="m">goedt</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Uyt haer <rhyme label="a" type="m" corresp="#s6:l1">bloedt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ende vleesch moet zijn <rhyme label="b" type="f">gesprooten</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Thoonen sal ons sondaers <rhyme label="c" type="m">snoot</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Mildheyt <rhyme label="c" type="m" corresp="#s6:l4">groot</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ons eeuwigh wee sal <rhyme label="b" type="f" corresp="#s6:l3">verstooten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Nu verblijdt, en sonder <rhyme label="a" type="m">sorgh</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Is 't <rhyme label="a" type="m" corresp="#s7:l1">Voorborgh</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Want Goodts Moeder is <rhyme label="b" type="f">gebooren</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Al d' Engelen groote <rhyme label="c" type="m">schaer</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Laten <rhyme label="c" type="m" corresp="#s7:l4">haer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Blijdtschap en vreught heden <rhyme label="b" type="f" corresp="#s7:l3">hooren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Laet ons singen: Weest <rhyme label="a" type="m">well'kom</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s8:l1">Iesse-blom</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Tot vermaeck van ons <rhyme label="b" type="f">gebooren</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Sult d' Hemelsche Vader <rhyme label="c" type="m">gram</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Door uw' <rhyme label="c" type="m" corresp="#s8:l4">Lam</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Versoenen sijn groote <rhyme label="b" type="f" corresp="#s8:l3">tooren</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Voor ons, Iesus uwe <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">'s Hemels <rhyme label="a" type="m" corresp="#s9:l1">throon</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Door sijn dierbaer bloedt sal <rhyme label="b" type="f">koopen</rhyme>.</l>
        <l id="s9:l4" met="4" type="+-">(Om uwen 't wil) d'Heyligh <rhyme label="c" type="m">Geest</rhyme></l>
        <l id="s9:l5" met="2" type="+-">Ons <rhyme label="c" type="m" corresp="#s9:l4">geneest</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Van 't quaedt; gaven veel laet <rhyme label="b" type="f" corresp="#s9:l3">loopen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Bevrijt ons van 't eeuwigh <rhyme label="a" type="m">wee</rhyme>.</l>
        <l id="s10:l2" met="2" type="+-">Sterr' der <rhyme label="a" type="m" corresp="#s10:l1">Zee</rhyme>!</l>
        <l id="s10:l3" met="3" type="-+">Staet ons by in 's wereldts <rhyme label="b" type="f">stroomen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Leyt ons, Hemels <rhyme label="c" type="m">Koninghin</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">Dat wy <rhyme label="c" type="m" corresp="#s10:l4">in</rhyme></l>
        <l id="s10:l6" met="3" type="-+">'s Hemels have daer na <rhyme label="b" type="f" corresp="#s10:l3">komen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
