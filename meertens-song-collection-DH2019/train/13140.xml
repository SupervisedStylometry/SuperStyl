<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13140</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Ik zucht zucht op zucht</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Christianus Vermeulen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe u werckeje niet, <rhyme label="a" type="m">Leeuwerck</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Hoe komt dat niet en <rhyme label="b" type="f">Orgelt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">In dese Tiende <rhyme label="a" type="m" corresp="#s1:l1">Kerck</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">U strotje dat so <rhyme label="b" type="f" corresp="#s1:l2">gorgeldt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daer't niemandt hoort te <rhyme label="c" type="f">velde</rhyme>?</l>
        <l id="s1:l6" met="3" type="-+">Wel aen dan, laet uw' <rhyme label="d" type="m">gunst</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Aen ons ghehoor oock <rhyme label="c" type="f" corresp="#s1:l5">melden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">'tMirakel van u <rhyme label="d" type="m" corresp="#s1:l6">kunst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Toont dat ghy danckbaer <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Voortaen te blijven <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van 'tbroodt dat ghy <rhyme label="a" type="m" corresp="#s2:l1">verslijt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Van 'tzaedt dat wy u <rhyme label="b" type="f" corresp="#s2:l2">gheven</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Van so veel <rhyme label="c" type="f">Klaver-zootjes</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van sulcken verschen <rhyme label="d" type="m">nat</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Van so veel groene <rhyme label="c" type="f" corresp="#s2:l5">lootjes</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Als ghy wel hebt <rhyme label="d" type="m" corresp="#s2:l6">ghehadt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoe sou'k ellendigh <rhyme label="a" type="m">Dier</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Vermaecken met mijn <rhyme label="b" type="f">zanghen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">'Tgehoor van mijn <rhyme label="a" type="m" corresp="#s3:l1">Cippier</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die my hier houdt <rhyme label="b" type="f" corresp="#s3:l2">ghevanghen</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">En wilt u niet <rhyme label="c" type="f">bedrieghen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">'Ken acht noch broodt noch <rhyme label="d" type="m">zaedt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Eer datmen my laet <rhyme label="c" type="f" corresp="#s3:l5">vlieghen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Te velde, voor <rhyme label="d" type="m" corresp="#s3:l6">weldaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den Aerdboom was mijn <rhyme label="a" type="m">vloer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Den Hemel mijn <rhyme label="b" type="f">verwellif</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Al 't groen ghewas mijn <rhyme label="a" type="m" corresp="#s4:l1">voer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Mijn Tafel-waerdt Godt <rhyme label="b" type="f" corresp="#s4:l2">sellif</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Hoe souden my de <rhyme label="c" type="f">kruymen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Verheughen van u <rhyme label="d" type="m">proy</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Nu ick den hemel <rhyme label="c" type="f" corresp="#s4:l5">ruymen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Moet voor een nauwe <rhyme label="d" type="m" corresp="#s4:l6">Koy</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn Schepper had my <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Doen baren van <rhyme label="b" type="f">gheveughelt</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">End' uw' ghewelt heeft <rhyme label="a" type="m" corresp="#s5:l1">my</rhyme></l>
        <l id="s5:l4" met="3" type="-+">De vrye vlucht <rhyme label="b" type="f" corresp="#s5:l2">beteughelt</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Ick hippel, end' ick <rhyme label="c" type="f">wemel</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Mijn zangh dijdt tot <rhyme label="d" type="m">gheschrey</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Want koy, en is gheen <rhyme label="c" type="f" corresp="#s5:l5">Hemel</rhyme>;</l>
        <l id="s5:l8" met="3" type="-+">En 'tzootjen is gheen <rhyme label="d" type="m" corresp="#s5:l6">Wey</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wilt ghy dan met <rhyme label="a" type="m">ghenucht</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mijn gorgeltje doen <rhyme label="b" type="f">quelen</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Sendt my weer na de <rhyme label="a" type="m" corresp="#s6:l1">Lucht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En 'tOrgeltjen sal <rhyme label="b" type="f" corresp="#s6:l2">spelen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Voor hem, die alle <rhyme label="c" type="f">dinghen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Schiep met een enckel <rhyme label="d" type="m">oogh</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Dies placht ick niet te <rhyme label="c" type="f" corresp="#s6:l5">singhen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dan als ick klim om <rhyme label="d" type="m" corresp="#s6:l6">hoogh</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hier sal u duncken <rhyme label="a" type="m">nieu</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Als gh'u sult hooren <rhyme label="b" type="f">noden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Door mijn adieu, dieu, <rhyme label="a" type="m" corresp="#s7:l1">dieu</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Te Godt, te Godt, te <rhyme label="b" type="f" corresp="#s7:l2">Gode</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">En tusschen't <rhyme label="c" type="f">tire-liren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Dieu, dieu, weer <rhyme label="d" type="m">telkemael</rhyme>;</l>
        <l id="s7:l7" met="3" type="-+">Om u te leeren <rhyme label="c" type="f" corresp="#s7:l5">vieren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Uw' Schepper met uw' <rhyme label="d" type="m" corresp="#s7:l6">tael</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
