  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Vol vreugde is ons <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Men ziet aan alle <rhyme label="a" type="m" corresp="#s1:l1">kant</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Schoone Eerboogen <rhyme label="b" type="f">planten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ligt schynen als <rhyme label="b" type="f" corresp="#s1:l3">briljanten</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ter Eere van onze <rhyme label="c" type="m">Vorst</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Roep viva uit de <rhyme label="c" type="m" corresp="#s1:l5">borst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zy hebben veel <rhyme label="a" type="m">Gesmeed</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat hem deed 't herte <rhyme label="a" type="m" corresp="#s2:l1">leed</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Het waare snoode <rhyme label="b" type="f">Rebellen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die zig tegen hem <rhyme label="b" type="f" corresp="#s2:l3">stellen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En dorsten naar zyn <rhyme label="c" type="m">bloed</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Maar God heeft hem <rhyme label="c" type="m" corresp="#s2:l5">behoed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy droeg het met <rhyme label="a" type="m">geduld</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Had aan de smaad geen <rhyme label="a" type="m" corresp="#s3:l1">schuld</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zy wilde de Stam <rhyme label="b" type="f">uitrukken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De Jonge Telgen <rhyme label="b" type="f" corresp="#s3:l3">afplukken</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maar God het niet <rhyme label="c" type="m">gedoog</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hem naar zyn Troon toe <rhyme label="c" type="m" corresp="#s3:l5">toog</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Was hy niet Neêrlands <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Toen hy lag aan de <rhyme label="a" type="m" corresp="#s4:l1">Borst</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Van zyn Geliefde <rhyme label="b" type="f">Moeder</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En God was zyn <rhyme label="b" type="f" corresp="#s4:l3">behoeder</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Gaf ons een dierbaar <rhyme label="c" type="m">pand</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Tot steun voor Kerk en <rhyme label="c" type="m" corresp="#s4:l5">Land</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Den Staat die bragt hem <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Al naar zyn Vaders <rhyme label="a" type="m" corresp="#s5:l1">dood</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Deeden hem zoo wel <rhyme label="b" type="f">bewaaren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tot Nederlands <rhyme label="b" type="f" corresp="#s5:l3">welvaaren</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">'t Is haar nu groote <rhyme label="c" type="m">smart</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat het Land zoo is <rhyme label="c" type="m" corresp="#s5:l5">verward</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy vond verwoest het <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Een Oorlogs vier los <rhyme label="a" type="m" corresp="#s6:l1">brand</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Voor 't eerste van zyn <rhyme label="b" type="f">leeven</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Moest hy zig gaan <rhyme label="b" type="f" corresp="#s6:l3">begeeven</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Gedwongen door <rhyme label="c" type="m">geweld</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Met Neêrlands Volk te <rhyme label="c" type="m" corresp="#s6:l5">Veld</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Het was voor hem een <rhyme label="a" type="m">post</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat het heeft bloed <rhyme label="a" type="m" corresp="#s7:l1">gekost</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hy kon 't niet anders <rhyme label="b" type="f">weeren</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Om veel Onheil te <rhyme label="b" type="f" corresp="#s7:l3">keeren</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Zyn <rhyme label="c" type="m">Edelmoedigheid</rhyme>.</l>
        <l id="s7:l6" met="3" type="-+">Zoekt Vreede, maar geen <rhyme label="c" type="m" corresp="#s7:l5">Stryd</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Het is genoeg <rhyme label="a" type="m">betreurt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ja zelfs nog nooit <rhyme label="a" type="m" corresp="#s8:l1">gebeurd</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Met eigen Volk te <rhyme label="b" type="f">vegten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">God zal 't alles <rhyme label="b" type="f" corresp="#s8:l3">regten</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Wee die booz' <rhyme label="c" type="m">Tiran</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Die daar de schuld heeft <rhyme label="c" type="m" corresp="#s8:l5">van</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Willem de Vyfde <rhyme label="a" type="m">zoet</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Geteeld uit Konings <rhyme label="a" type="m" corresp="#s9:l1">Bloed</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Met Uw lieve <rhyme label="b" type="f">Gemâlinne</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Die gy uyt suivere <rhyme label="b" type="f" corresp="#s9:l3">minne</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Uw hert en gunste <rhyme label="c" type="m">bood</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Uw dierbaare <rhyme label="c" type="m" corresp="#s9:l5">Egtgenood</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">God vermeere Uw <rhyme label="a" type="m">Zaat</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Zoo lang de Wereld <rhyme label="a" type="m" corresp="#s10:l1">staat</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Uw Stam mag altoos <rhyme label="b" type="f">bloeijen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Zyn Zegen doet hy <rhyme label="b" type="f" corresp="#s10:l3">vloeijen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Over Uw Vorstelyk <rhyme label="c" type="m">Huis</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Bevryd van Ramp en <rhyme label="c" type="m" corresp="#s10:l5">Kruis</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
