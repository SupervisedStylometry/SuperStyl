<song>
  <meta>
    <id>14058</id>
    <date>
      <year-start>1621</year-start>
      <year-end>1621</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Engelse klokkedans</melody>
  <author>Jan Jansz. Starter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Kond' Orpheus <rhyme label="a" type="f">stemme</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Kond' Orpheus <rhyme label="b" type="m">spel</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Kond' Orpheus <rhyme label="c" type="m">Liers-geklanck</rhyme></l>
        <l id="s1:l4" met="2" type="-+">De Dieren <rhyme label="a" type="f" corresp="#s1:l1">temmen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En wonder <rhyme label="b" type="m" corresp="#s1:l2">wel</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Doen danssen op zijn <rhyme label="c" type="m" corresp="#s1:l3">sanck</rhyme>;</l>
        <l id="s1:l7" met="2" type="-+">So naer syn <rhyme label="d" type="m">snaer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">De Zee syn baren <rhyme label="e" type="m">dringt</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">'t Geboomt haer tacken <rhyme label="e" type="m" corresp="#s1:l8">dwingt</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">En met haer blad'ren <rhyme label="e" type="m" corresp="#s1:l8">springt</rhyme>.</l>
        <l id="s1:l11" met="3" type="-+">O Dieren, Bomen, <rhyme label="f" type="m">Zeen</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Danst nu met alle <rhyme label="f" type="m" corresp="#s1:l11">re'en</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Want myn Astraea <rhyme label="e" type="m" corresp="#s1:l8">singt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Syn eens door <rhyme label="a" type="f">eenen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Amphions <rhyme label="b" type="m">geest</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En honigh-soete <rhyme label="c" type="m">praet</rhyme></l>
        <l id="s2:l4" met="2" type="-+">De koude <rhyme label="a" type="f" corresp="#s2:l1">steenen</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Beweeght <rhyme label="b" type="m" corresp="#s2:l2">geweest</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Te danssen op de <rhyme label="c" type="m" corresp="#s2:l3">maet</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Tot dat een <rhyme label="d" type="m">Stadt</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Daer uyt (dewyl hy <rhyme label="e" type="m">spreeckt</rhyme>)</l>
        <l id="s2:l9" met="3" type="-+">Met muyren sich <rhyme label="e" type="m" corresp="#s2:l8">opsteeckt</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Iae deur de Wolcken <rhyme label="e" type="m" corresp="#s2:l8">breeckt</rhyme>.</l>
        <l id="s2:l11" met="3" type="-+">O steenen luysterd <rhyme label="f" type="m">weer</rhyme>!</l>
        <l id="s2:l12" met="3" type="-+">En danst nu thienmael <rhyme label="f" type="m" corresp="#s2:l11">meer</rhyme>:</l>
        <l id="s2:l13" met="3" type="-+">Want myn Astraea <rhyme label="e" type="m" corresp="#s2:l8">spreeckt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Was eens <rhyme label="a" type="f">gedwongen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Een Draeck seer <rhyme label="b" type="m">fel</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Te minnen met <rhyme label="c" type="m">bescheyd</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Een Harders <rhyme label="a" type="f" corresp="#s3:l1">Iongen</rhyme>!</l>
        <l id="s3:l5" met="2" type="-+">Heeft d'Arend <rhyme label="b" type="m" corresp="#s3:l2">snel</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Een schoone Griexsche <rhyme label="c" type="m" corresp="#s3:l3">Meyd</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Besind, <rhyme label="d" type="m">bemind</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Iae vaeck om haer <rhyme label="e" type="m">gedaeld</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Van daer de Sonne <rhyme label="e" type="m" corresp="#s3:l8">praeld</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">En om haer heen <rhyme label="e" type="m" corresp="#s3:l8">gemaeld</rhyme>!</l>
        <l id="s3:l11" met="3" type="-+">Komt Vog'len, komt <rhyme label="f" type="m">gediert</rhyme></l>
        <l id="s3:l12" met="3" type="-+">En nu van minne <rhyme label="f" type="m" corresp="#s3:l11">tiert</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">Midts myn Astraea <rhyme label="e" type="m" corresp="#s3:l8">straeld</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Doch so de <rhyme label="a" type="f">bomen</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">De steenen, 't <rhyme label="b" type="m">Vee</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'t Gevogelt uyt de <rhyme label="c" type="m">Lucht</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Niet strax kan <rhyme label="a" type="f" corresp="#s4:l1">komen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">By mijn <rhyme label="b" type="m" corresp="#s4:l2">Astree</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En danssen van <rhyme label="c" type="m" corresp="#s4:l3">genucht</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">Maer lang en <rhyme label="d" type="m">bang</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In droefheyds weyden <rhyme label="e" type="m">graest</rhyme>:</l>
        <l id="s4:l9" met="3" type="-+">Denckt dat kleyn liefd wel <rhyme label="e" type="m" corresp="#s4:l8">haest</rhyme></l>
        <l id="s4:l10" met="3" type="+-">Begind, en dapper <rhyme label="e" type="m" corresp="#s4:l8">blaest</rhyme>:</l>
        <l id="s4:l11" met="3" type="-+">Maer dat so groote <rhyme label="f" type="m">min</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Als daeld van de <rhyme label="f" type="m" corresp="#s4:l11">Goddin</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Astraea, elck <rhyme label="e" type="m" corresp="#s4:l8">verbaest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Syn dan de <rhyme label="a" type="f">beesten</rhyme></l>
        <l id="s5:l2" met="2" type="-+">So seer <rhyme label="b" type="m">versuft</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als sy Astraea <rhyme label="c" type="m">sien</rhyme>:</l>
        <l id="s5:l4" met="2" type="-+">Wat sal den <rhyme label="a" type="f" corresp="#s5:l1">Geesten</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Die met <rhyme label="b" type="m" corresp="#s5:l2">vernuft</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Begaeft syn, dan <rhyme label="c" type="m" corresp="#s5:l3">geschie'n</rhyme>?</l>
        <l id="s5:l7" met="2" type="-+">Hoe seer! hoe <rhyme label="d" type="m">veer</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Of elck den geest <rhyme label="e" type="m">ontsinckt</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Die van haer minne <rhyme label="e" type="m" corresp="#s5:l8">drinckt</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">En op haer schoonheyd <rhyme label="e" type="m" corresp="#s5:l8">dinckt</rhyme>:</l>
        <l id="s5:l11" met="3" type="-+">'k Wed elck betoverd <rhyme label="f" type="m">staet</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Die d'oogen open <rhyme label="f" type="m" corresp="#s5:l11">slaet</rhyme>,</l>
        <l id="s5:l13" met="3" type="-+">Als myn Astraea <rhyme label="e" type="m" corresp="#s5:l8">blinckt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
