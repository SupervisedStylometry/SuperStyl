<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159618</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Corydon ontstoken</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">HOe wy langer <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Heefter een <rhyme label="b" type="m">geseyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Hoe men meer <rhyme label="a" type="f" corresp="#s1:l1">bedreven</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Werdt in kloeck <rhyme label="b" type="m" corresp="#s1:l2">beleyt</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Want ons teere <rhyme label="c" type="f">sinnen</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Door een nieuw <rhyme label="d" type="m">gesicht</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Sietmen vaeck <rhyme label="c" type="f" corresp="#s1:l5">verwinnen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Door een klaerder <rhyme label="d" type="m" corresp="#s1:l6">licht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Laest-mael quam ick <rhyme label="a" type="f">treden</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Op een seecker <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Daer ick nae mijn <rhyme label="a" type="f" corresp="#s2:l1">reden</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Vry wat wonders <rhyme label="b" type="m" corresp="#s2:l2">sagh</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Ick bestondt te <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">En men was <rhyme label="d" type="m">bereyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Want nae mijn <rhyme label="c" type="f" corresp="#s2:l5">behagen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Kreegh ick strack <rhyme label="d" type="m" corresp="#s2:l6">bescheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Nu om voorts te <rhyme label="a" type="f">komen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Klaerder aen den <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">'t Geen ick heb <rhyme label="a" type="f" corresp="#s3:l1">vernomen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">En met wonder <rhyme label="b" type="m" corresp="#s3:l2">sagh</rhyme>;</l>
        <l id="s3:l5" met="3" type="+-">'k Sal u onder <rhyme label="c" type="f">rechten</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Na hun lien <rhyme label="d" type="m">verklaer</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Want men gingh daer <rhyme label="c" type="f" corresp="#s3:l5">hechten</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Lappen aen <rhyme label="d" type="m" corresp="#s3:l6">malkaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Doncker-paersen <rhyme label="a" type="f">Laken</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">En oock groen en <rhyme label="b" type="m">blaeu</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Gingh men t'samen <rhyme label="a" type="f" corresp="#s4:l1">maken</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Met roodt, wit en <rhyme label="b" type="m" corresp="#s4:l2">graeu</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Geel en grijs te <rhyme label="c" type="f">samen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Iae schier <rhyme label="d" type="m">Sysatijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">En na hun liens <rhyme label="c" type="f" corresp="#s4:l5">ramen</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Soudt een Deken <rhyme label="d" type="m" corresp="#s4:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Ick docht al die <rhyme label="a" type="f">naden</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Dat waer veel te <rhyme label="b" type="m">slecht</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Maer door blijckens <rhyme label="a" type="f" corresp="#s5:l1">daden</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Wierd' ick <rhyme label="b" type="m" corresp="#s5:l2">onder-recht</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">Want men gingh het <rhyme label="c" type="f">sticken</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Met veel Zijden <rhyme label="d" type="m">kleur</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Daer men niet kond' <rhyme label="c" type="f" corresp="#s5:l5">micken</rhyme></l>
        <l id="s5:l8" met="3" type="+-">'t Lappigh Laken <rhyme label="d" type="m" corresp="#s5:l6">deur</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Want seer schoon en <rhyme label="a" type="f">even</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Sijn daer op <rhyme label="b" type="m">gheset</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Vogeltjes nae 't <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Met veel Bloemtjes <rhyme label="b" type="m" corresp="#s6:l2">net</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Krullen ende <rhyme label="c" type="f">Stricken</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Trocken deur <rhyme label="d" type="m">malkaer</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Maer van binnen <rhyme label="c" type="f" corresp="#s6:l5">sticken</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Waren 't <rhyme label="d" type="m" corresp="#s6:l6">allegaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Met dit slecht <rhyme label="a" type="f">aen-schouwen</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Kreegh ick stracks dees <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Dat veel menschen <rhyme label="a" type="f" corresp="#s7:l1">houwen</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Vaeck haer boosheyt <rhyme label="b" type="m" corresp="#s7:l2">in</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Die seer schoon voor <rhyme label="c" type="f">oogen</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Schijnen voor het <rhyme label="d" type="m">hooft</rhyme>;</l>
        <l id="s7:l7" met="3" type="+-">Maer men wordt <rhyme label="c" type="f" corresp="#s7:l5">bedrogen</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Soo men haer <rhyme label="d" type="m" corresp="#s7:l6">ghelooft</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Oorlof dan hier <rhyme label="a" type="f">mede</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Neemt mijn slecht <rhyme label="b" type="m">gedicht</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Aen met <rhyme label="a" type="f" corresp="#s8:l1">danckbaerheden</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">'t Is uyt lust <rhyme label="b" type="m" corresp="#s8:l2">gesticht</rhyme>.</l>
        <l id="s8:l5" met="3" type="+-">Wilt de schijn-deught <rhyme label="c" type="f">schouwen</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Met haer schoone <rhyme label="d" type="m">praet</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">'t Sal u dan niet <rhyme label="c" type="f" corresp="#s8:l5">rouwen</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Als het is te <rhyme label="d" type="m" corresp="#s8:l6">laet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
