<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>166303</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Corydon ontstoken</melody>
  <author>Franciscus de Laat</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SOete <rhyme label="a" type="m">Galathé</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">'k Heb u al den <rhyme label="b" type="m">nacht</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Met mijn <rhyme label="a" type="m" corresp="#s1:l1">Lammer-vee</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Op dees plaets <rhyme label="b" type="m" corresp="#s1:l2">ghewacht</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">VVel wie hoor ick <rhyme label="c" type="f">spreken</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Is 't mijn Thyrsis <rhyme label="d" type="m">niet</rhyme>?</l>
        <l id="s1:l7" met="3" type="+-">Die daer leyt <rhyme label="c" type="f" corresp="#s1:l5">versteken</rhyme></l>
        <l id="s1:l8" met="3" type="+-">By het jeudigh <rhyme label="d" type="m" corresp="#s1:l6">riet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Iae het <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">'t Is u trouwen <rhyme label="b" type="m">knecht</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Die nae uwe <rhyme label="a" type="m" corresp="#s2:l1">sin</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Syne paden <rhyme label="b" type="m" corresp="#s2:l2">recht</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Nimph u langh <rhyme label="c" type="f">afwesen</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Heeft my seer <rhyme label="d" type="m">bedroeft</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">VVant met groote <rhyme label="c" type="f" corresp="#s2:l5">vreese</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Heb ick u <rhyme label="d" type="m" corresp="#s2:l6">vertoeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Thyrsis ick ben <rhyme label="a" type="m">bly</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Dat ghy zijt <rhyme label="b" type="m">ghesont</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">En in uwe <rhyme label="a" type="m" corresp="#s3:l1">zy</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Van gheen beest <rhyme label="b" type="m" corresp="#s3:l2">ghewondt</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Laet ons vast <rhyme label="c" type="f">beklyven</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">En in alle <rhyme label="d" type="m">noodt</rhyme></l>
        <l id="s3:l7" met="3" type="+-">By malkander <rhyme label="c" type="f" corresp="#s3:l5">blyven</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Iae oock tot de <rhyme label="d" type="m" corresp="#s3:l6">doodt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Nimph de druck en <rhyme label="a" type="m">leet</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Sal ons duncken <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">VVant de trouwen <rhyme label="a" type="m" corresp="#s4:l1">eedt</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Maeckt de smert <rhyme label="b" type="m" corresp="#s4:l2">ghemeyn</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Ick wil u <rhyme label="c" type="f">nae-jaghen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">In het dorre <rhyme label="d" type="m">sandt</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">In de groene <rhyme label="c" type="f" corresp="#s4:l5">haghen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Of een ander <rhyme label="d" type="m" corresp="#s4:l6">kandt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Thirsi volght de <rhyme label="a" type="m">paên</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Van u <rhyme label="b" type="m">Galathé</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Soo sult ghy wel <rhyme label="a" type="m" corresp="#s5:l1">gaen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Met u wolligh <rhyme label="b" type="m" corresp="#s5:l2">vee</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">VVant alwaer de <rhyme label="c" type="f">weyden</rhyme></l>
        <l id="s5:l6" met="3" type="+-">'t Allergroenste <rhyme label="d" type="m">bloên</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Daer is voor ons <rhyme label="c" type="f" corresp="#s5:l5">beyden</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Meest van al te <rhyme label="d" type="m" corresp="#s5:l6">doen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
