  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">KOmt tot my, komt tot my, komt tot my, Op de <rhyme label="a" type="f">Baeren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gy syt van myn <rhyme label="b" type="m">bemint</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Ik sal sijn uw <rhyme label="b" type="m" corresp="#s1:l2">vrint</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Als gy wilt gaen <rhyme label="a" type="f" corresp="#s1:l1">vaeren</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">'t Schip lyt op <rhyme label="c" type="m">Ree</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Ionkman wilt gy <rhyme label="c" type="m" corresp="#s1:l5">mee</rhyme>;</l>
        <l id="s1:l7" met="3" type="+-">Gy sult syn te <rhyme label="c" type="m" corresp="#s1:l5">vree</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Als gy der wil gaen na <rhyme label="c" type="m" corresp="#s1:l5">zee</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Op Nepthunes <rhyme label="d" type="m">plats</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Sult gy vinden <rhyme label="d" type="m" corresp="#s1:l9">ras</rhyme>,</l>
        <l id="s1:l11" met="4" type="-+">Nu het Fortuyn verandert <rhyme label="d" type="m" corresp="#s1:l9">was</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Vreest niet, vreest <rhyme label="a" type="f">niet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Vreest voor geen guren <rhyme label="b" type="m">vlagen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Gy syt al myn <rhyme label="b" type="m" corresp="#s2:l2">plaisier</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Sonder schromen <rhyme label="a" type="f" corresp="#s2:l1">weer</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Sal ik van uw <rhyme label="c" type="m">Iagen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Gy sult sijn <rhyme label="c" type="m" corresp="#s2:l5">geÃ«ert</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">En <rhyme label="c" type="m" corresp="#s2:l5">gerespecteert</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Veel meer dan gy het <rhyme label="c" type="m" corresp="#s2:l5">begeert</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Want het gelt en <rhyme label="d" type="m">goed</rhyme>;</l>
        <l id="s2:l10" met="3" type="+-">Iaegt uw na met <rhyme label="d" type="m" corresp="#s2:l9">spoed</rhyme>:</l>
        <l id="s2:l11" met="4" type="-+">In gunst en in <rhyme label="d" type="m" corresp="#s2:l9">overvloet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Rosetta, <rhyme label="a" type="f">Rosetta</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Rosettaes lokkies <rhyme label="b" type="m">hangen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">haer aengename <rhyme label="b" type="m" corresp="#s3:l2">geur</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">En lieffelyke <rhyme label="a" type="f" corresp="#s3:l1">kleur</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Van haer Edele <rhyme label="c" type="m">Wangen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Haer sneeuw witte <rhyme label="c" type="m" corresp="#s3:l5">hant</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Oog als <rhyme label="c" type="m" corresp="#s3:l5">diamant</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Haer albasterde <rhyme label="c" type="m" corresp="#s3:l5">tant</rhyme>;</l>
        <l id="s3:l9" met="3" type="+-">Sy is de schoonste van ons <rhyme label="d" type="m">lant</rhyme>;</l>
        <l id="s3:l10" met="3" type="+-">En haer leeden sijn <rhyme label="d" type="m" corresp="#s3:l9">welgeschaepen</rhyme></l>
        <l id="s3:l11" met="4" type="-+">Sijn niet te grof of niet te <rhyme label="d" type="m" corresp="#s3:l9">klyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Wat sal ik; wat sal <rhyme label="a" type="f">ik</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">wat sal ik gaan <rhyme label="b" type="m">beginnen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Vaeren is al mijn <rhyme label="b" type="m" corresp="#s4:l2">sin</rhyme>;</l>
        <l id="s4:l4" met="3" type="+-">Nogtans ik <rhyme label="a" type="f" corresp="#s4:l1">bemin</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Uw schoone <rhyme label="c" type="m">Godin</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Moet ik dan voor <rhyme label="c" type="m" corresp="#s4:l5">altyd</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Neeme van uw myn <rhyme label="c" type="m" corresp="#s4:l5">afscheyt</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Hemel want myn hert dat barst van <rhyme label="c" type="m" corresp="#s4:l5">spyt</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Neemt mijn hert dan <rhyme label="d" type="m">mee</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Het Schip lyt op de <rhyme label="d" type="m" corresp="#s4:l9">Ree</rhyme>,</l>
        <l id="s4:l11" met="4" type="-+">Myn Liefje dat gaet 'er na <rhyme label="d" type="m" corresp="#s4:l9">zee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
