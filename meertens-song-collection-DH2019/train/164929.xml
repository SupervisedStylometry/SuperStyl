<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164929</id>
    <date>
      <year-start>1612</year-start>
      <year-end>1612</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Een lied eerbaar</melody>
  <author>J. Jacobsz. Harlingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ick sal voorwaer // den Hemelschen <rhyme label="a" type="m">vaer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Int openbaer // gaen <rhyme label="b" type="f">prijsen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Sijn lof seer claer // sal voor en <rhyme label="a" type="m" corresp="#s1:l1">naer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">By sijn dienaer // noch <rhyme label="b" type="f" corresp="#s1:l2">rijsen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Die siele mijn // sal met blijde <rhyme label="c" type="m">schijn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hem verheughe dijn // o <rhyme label="d" type="f">wijse</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Dattet hoore fijn // op dit <rhyme label="c" type="m" corresp="#s1:l5">termijn</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die ellendiche dijn // en <rhyme label="d" type="f" corresp="#s1:l6">verjolijse</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O Goddelijcke graen // met malcander <rhyme label="a" type="m">beraen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met druck belaen // <rhyme label="b" type="f">verschoven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Laet ons gheraen // met Davids <rhyme label="a" type="m" corresp="#s2:l1">vermaen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Des Heeren naem // gaen <rhyme label="b" type="f" corresp="#s2:l2">loven</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Doen ick den Heer // noch sochte van <rhyme label="c" type="m">veer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Hy antwoorde my weer // van <rhyme label="d" type="f">boven</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">En verloste my teer // door sijnen <rhyme label="c" type="m" corresp="#s2:l5">leer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Van alle mijn zeer // my <rhyme label="d" type="f" corresp="#s2:l6">beroofde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die hem aensiet // en tot hem <rhyme label="a" type="m">vliedt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die en wort niet // tot <rhyme label="b" type="f">schanden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Want God die biedt // die ellendige met <rhyme label="a" type="m" corresp="#s3:l1">vliet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die tot hem riep // sijn <rhyme label="b" type="f" corresp="#s3:l2">handen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En helptse voort // al wordense <rhyme label="c" type="m">verstoort</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wt alle noot // en <rhyme label="d" type="f">banden</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Sijn enghel hoort // beleghert met <rhyme label="c" type="m" corresp="#s3:l5">accoort</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Der gheloovigher oort // spijt <rhyme label="d" type="f" corresp="#s3:l6">vyanden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Siet ende smaect //watten vriendelijcken <rhyme label="a" type="m">saec</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat hem ghenaeckt // na <rhyme label="b" type="f">desen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die na God haeckt // en op hem <rhyme label="a" type="m" corresp="#s4:l1">waeckt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Niet en versaeckt // maer <rhyme label="b" type="f" corresp="#s4:l2">vreesen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Vreest God altijt // en hem <rhyme label="c" type="m">belijt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ghy die nu zijt // <rhyme label="d" type="f">verresen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Want die met vlijt // hem vreest en <rhyme label="c" type="m" corresp="#s4:l5">belijdt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">In desen tijt // sal zijn <rhyme label="d" type="f" corresp="#s4:l6">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Werpt u begheer // alleenich op den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Voort quaet ghebeer, // u wilt <rhyme label="b" type="f">hoeden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die hongherde seer // na sijn Goddelijcke <rhyme label="a" type="m" corresp="#s5:l1">eer</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Vervulde hy weer // met <rhyme label="b" type="f" corresp="#s5:l2">goeden</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Maer die hem versmaet // nu zijn <rhyme label="c" type="m">versaedt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Rijck hooch van staet // van <rhyme label="d" type="f">moede</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">God weer verlaet // nootdruftich <rhyme label="c" type="m" corresp="#s5:l5">gaet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En hem ghenaeckt veel <rhyme label="d" type="f" corresp="#s5:l6">teghenspoede</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hierom verlaet // dit vuyle <rhyme label="a" type="m">ghewaet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En sulcken staet // <rhyme label="b" type="f">mispresen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Gods vrede aenvaet // die s'menschen <rhyme label="a" type="m" corresp="#s6:l1">raet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Te boven gaet <rhyme label="b" type="f" corresp="#s6:l2">ghepresen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">En gheeft gheen wijck // ist <rhyme label="c" type="m">waerachtelijck</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Oprecht cuysch lieffelijck // int <rhyme label="d" type="f">wesen</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Ist wel luydelijck, ist <rhyme label="c" type="m" corresp="#s6:l5">deuchdelijck</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Ist lof eerlijck // <rhyme label="d" type="f" corresp="#s6:l6">gheresen</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Comt herwaerts hoort // Gods kinderen sijn <rhyme label="a" type="m">woort</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Na waerheyt spoort // te <rhyme label="b" type="f">deghen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Wie isser voort // die nae 't <rhyme label="a" type="m" corresp="#s7:l1">behoort</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Met goet accoort // te <rhyme label="b" type="f" corresp="#s7:l2">leven</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">En wie gherust // tot goede daghen heeft <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Iou tonghe uytblust // voor <rhyme label="d" type="f">sneven</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Iou lippen sust // van bedrieghen <rhyme label="c" type="m" corresp="#s7:l5">onrust</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En boose lust // te <rhyme label="d" type="f" corresp="#s7:l6">streven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Vant boos wilt vlien // doet goet <rhyme label="a" type="m">nadien</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Na vrede allien // wilt <rhyme label="b" type="f">jaghen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Want God ten tien // wil haer <rhyme label="a" type="m" corresp="#s8:l1">ansien</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die hem ghemien // <rhyme label="b" type="f" corresp="#s8:l2">behaghen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Oock haer ghebeen // en droevich <rhyme label="c" type="m">gheween</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Wilt de Heere alleen // <rhyme label="d" type="f">draghen</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Die haer afscheen // en boosheyt <rhyme label="c" type="m" corresp="#s8:l5">deen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Wil dit Heere van een // <rhyme label="d" type="f" corresp="#s8:l6">verjaghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die Heere voldoet // t'ghebedt seer <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Eens verslaghen ghemoet // vol <rhyme label="b" type="f">smerten</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En hy is goet // die tot hem <rhyme label="a" type="m" corresp="#s9:l1">spoet</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ghebroken vroet // van <rhyme label="b" type="f" corresp="#s9:l2">herten</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Die gherechticheydt // voor die menschen <rhyme label="c" type="m">verbreydt</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Van t'quaet hem scheyt // met <rhyme label="d" type="f">moeden</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Hoewel hy schreyt // hem zijn <rhyme label="c" type="m" corresp="#s9:l5">bereyt</rhyme></l>
        <l id="s9:l8" met="3" type="-+">En toegheseyt // veel <rhyme label="d" type="f" corresp="#s9:l6">goeden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Maer die nu lacht // en wort <rhyme label="a" type="m">gheacht</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Van s'menschen gheslacht // <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">En toont sijn macht // over <rhyme label="a" type="m" corresp="#s10:l1">t'gheslacht</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Die uyt al haer cracht // Godt <rhyme label="b" type="f" corresp="#s10:l2">vreesen</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Die wil God weer // werpen ter <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Sal worden seer // <rhyme label="d" type="f">begresen</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Door Godes leer // die <rhyme label="c" type="m" corresp="#s10:l5">nimmermeer</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Sal nemen keer // ist <rhyme label="d" type="f" corresp="#s10:l6">bewesen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Hierom ghy al // der Christenen <rhyme label="a" type="m">ghetal</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Treedt den wegh smal // na <rhyme label="b" type="f">t'leven</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Al rijster bal // druck <rhyme label="a" type="m" corresp="#s11:l1">ongheval</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Hier in dit dal // der <rhyme label="b" type="f" corresp="#s11:l2">sneven</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Voor t'cleyne verdriedt // d'welcken hier <rhyme label="c" type="m">schiedt</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En wilt doch niet // so <rhyme label="d" type="f">vreesen</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Maer hier op siet // wat God <rhyme label="c" type="m" corresp="#s11:l5">onthiet</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">En ons aenbiet // te <rhyme label="d" type="f" corresp="#s11:l6">gheven</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Hy roept comt by // die beladen <rhyme label="a" type="m">zy</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Ick sal u maken vry // van <rhyme label="b" type="f">sonden</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Scheyt uyt het ghecry // wilt wesen <rhyme label="a" type="m" corresp="#s12:l1">bly</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Mijn volck sult ghy // zijn <rhyme label="b" type="f" corresp="#s12:l2">bevonden</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Mijn jock is soet // dat op u <rhyme label="c" type="m">doet</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Mijn last is goet // den <rhyme label="d" type="f">ghesonden</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">Maer t'is al roet // en <rhyme label="c" type="m" corresp="#s12:l5">teghenspoet</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Die sijn verwoet // mijn <rhyme label="d" type="f" corresp="#s12:l6">gronden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">O Princelijcke Godt // zijt ghy ons deel en <rhyme label="a" type="m">lodt</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Bidden wy in slot // ons <rhyme label="b" type="f">sanghen</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Teghen s'werelts spot // door u <rhyme label="a" type="m" corresp="#s13:l1">ghebodt</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Sterckt uwen rodt // haer <rhyme label="b" type="f" corresp="#s13:l2">ganghen</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Vwen gheest sendt // dat wy <rhyme label="c" type="m">present</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">T'gheloove jent // <rhyme label="d" type="f">aenhanghen</rhyme>,</l>
        <l id="s13:l7" met="4" type="-+">En s'Hemels tent // na desen <rhyme label="c" type="m" corresp="#s13:l5">endt</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Seer excelent // <rhyme label="d" type="f" corresp="#s13:l6">ontfanghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
