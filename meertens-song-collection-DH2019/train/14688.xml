<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14688</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>L'Orangère</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter van Gelre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MAria weest <rhyme label="a" type="m">gegroet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O Coninginne <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Want ghy zijt door <rhyme label="b" type="m">medoogentheyt</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Ons Moeder der <rhyme label="b" type="m" corresp="#s1:l3">barmherticheyt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Die 't leven // quaemt <rhyme label="c" type="f">geven</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Doen wy noch <rhyme label="d" type="m">in</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Het graf van doodts <rhyme label="d" type="m" corresp="#s1:l6">onmin</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Als slaven // <rhyme label="e" type="f">begraven</rhyme>,</l>
        <l id="s1:l9" met="5" type="+-">Neer laghen, tot onse vyandts <rhyme label="d" type="m" corresp="#s1:l6">gewin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ons soeticheyt zijt <rhyme label="a" type="m">ghy</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Daerom soo groeten <rhyme label="a" type="m" corresp="#s2:l1">wy</rhyme></l>
        <l id="s2:l3" met="4" type="-+">V weer, o Duyve van <rhyme label="b" type="m">Noë</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die ons den Olyf bracht van <rhyme label="b" type="m" corresp="#s2:l3">vree'</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Wiens groenheyt // veel <rhyme label="c" type="f">koenheyt</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Ons ziele <rhyme label="d" type="m">geeft</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wanneer de Hel <rhyme label="d" type="m" corresp="#s2:l6">verheeft</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Haer wat'ren // met <rhyme label="e" type="f">schat'ren</rhyme></l>
        <l id="s2:l9" met="5" type="+-">Van aerdsche wellust, daer de doot na <rhyme label="d" type="m" corresp="#s2:l6">sweeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Tot u roepen wy <rhyme label="a" type="m">dan</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Als droeve kind'ren, <rhyme label="a" type="m" corresp="#s3:l1">van</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Eva, wilt open doen het <rhyme label="b" type="m">vat</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Van u devote rijcke <rhyme label="b" type="m" corresp="#s3:l3">schat</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Ons suchten // komt <rhyme label="c" type="f">vluchten</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Met groot <rhyme label="d" type="m">begeer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Tot u, dat ghy met <rhyme label="d" type="m" corresp="#s3:l6">eer</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Ons klagen // wilt <rhyme label="e" type="f">dragen</rhyme></l>
        <l id="s3:l9" met="5" type="+-">Door uwe Soons vijf open wonden <rhyme label="d" type="m" corresp="#s3:l6">neer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Och dan ons <rhyme label="a" type="m">Middelers</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">V ooghen slaet niet <rhyme label="a" type="m" corresp="#s4:l1">dwers</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Maer wiltse keeren tot ons <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Hier neder in dit droeve <rhyme label="b" type="m" corresp="#s4:l3">dal</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Laet wenden // <rhyme label="c" type="f">d'ellenden</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Als <rhyme label="d" type="m">onbequaem</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En wilt ons al te <rhyme label="d" type="m" corresp="#s4:l6">saem</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Vertoonen // u <rhyme label="e" type="f">Soone</rhyme></l>
        <l id="s4:l9" met="5" type="+-">Iesus ghebenedijt in u <rhyme label="d" type="m" corresp="#s4:l6">lichaem</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Voort sachtmoedige <rhyme label="a" type="m">Vrou</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Laet uyt het hoogh' <rhyme label="a" type="m" corresp="#s5:l1">gebou</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Des Hemels dalen, sulcken <rhyme label="b" type="m">bandt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Die na u treckt al ons <rhyme label="b" type="m" corresp="#s5:l3">verstant</rhyme></l>
        <l id="s5:l5" met="2" type="-+">En raet // ons // bevaet // <rhyme label="c" type="f">ons</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">In u <rhyme label="d" type="m">ghenae</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Eer dat het valt te <rhyme label="d" type="m" corresp="#s5:l6">spae</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Op dat // wy // een schat // <rhyme label="e" type="f">by</rhyme>,</l>
        <l id="s5:l9" met="5" type="+-">V zijn, O soete Maget <rhyme label="d" type="m" corresp="#s5:l6">Maria</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
