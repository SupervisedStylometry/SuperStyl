      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laat ons onse stemmen <rhyme label="a" type="f">paren</rhyme></l>
        <l id="s1:l2" met="4" type="+-">By het soet geluid der <rhyme label="a" type="f" corresp="#s1:l1">snaren</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Nu Baggus <rhyme label="b" type="m">moed</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Beswijkt voor Venus <rhyme label="b" type="m" corresp="#s1:l3">gloed</rhyme>.</l>
        <l id="s1:l5" met="1" type="-+">Dus <rhyme label="c" type="m">paard</rhyme></l>
        <l id="s1:l6" met="1" type="-+">Op <rhyme label="c" type="m" corresp="#s1:l5">aard</rhyme></l>
        <l id="s1:l7" met="3" type="-+">De vreugden met het <rhyme label="d" type="f">minnen</rhyme>;</l>
        <l id="s1:l8" met="2" type="+-">Voortaan <rhyme label="e" type="m">sal</rhyme></l>
        <l id="s1:l9" met="2" type="+-">'t Heele <rhyme label="e" type="m" corresp="#s1:l8">Al</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Sig sien door dees drift <rhyme label="d" type="f" corresp="#s1:l7">verwinnen</rhyme>.</l>
        <l id="s1:l11" met="2" type="+-">Min geeft <rhyme label="f" type="m">vreugd</rhyme></l>
        <l id="s1:l12" met="2" type="+-">Aan de <rhyme label="f" type="m" corresp="#s1:l11">Jeugd</rhyme></l>
        <l id="s1:l13" met="4" type="+-">Als hun Baggus vogt   <rhyme label="f" type="m" corresp="#s1:l11">verheugd</rhyme>.</l>
