  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Helaas! myne ziele met <rhyme label="a" type="f">zonden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Te zwaar belaên <rhyme label="a" type="f" corresp="#s1:l1">bevonden</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Helaas! die is benauwt ter <rhyme label="b" type="m">dood</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Heer! u klaag ik myn <rhyme label="b" type="m" corresp="#s1:l3">nood</rhyme>.</l>
      </lg>
