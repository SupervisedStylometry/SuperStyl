<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185850</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ave Maria Gods liefste lief idoon</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Eeuwich en <rhyme label="a" type="f">crachtich</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Godt vader ons <rhyme label="a" type="f" corresp="#s1:l1">almachtich</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Wiens troost <rhyme label="a" type="f" corresp="#s1:l1">waerachtich</rhyme></l>
        <l id="s1:l4" met="3" type="-+">In lijden ons <rhyme label="b" type="m">verblijt</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Wy roepen <rhyme label="a" type="f" corresp="#s1:l1">clachtich</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Verhoort ons stem <rhyme label="a" type="f" corresp="#s1:l1">eendrachtich</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Weest onser <rhyme label="a" type="f" corresp="#s1:l1">gedachtich</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ghy den inden hemel <rhyme label="b" type="m" corresp="#s1:l4">zijt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">verdrijft pijn, ancxt en <rhyme label="b" type="m" corresp="#s1:l4">strijt</rhyme></l>
        <l id="s1:l10" met="3" type="-+">In al ons levens <rhyme label="b" type="m" corresp="#s1:l4">tijt</rhyme></l>
        <l id="s1:l11" met="5" type="+-">Hier opter aerden helpt vader ons <rhyme label="b" type="m" corresp="#s1:l4">bevrijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Laet dijnen nae <rhyme label="a" type="f">me</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Soet lieflijck <rhyme label="a" type="f" corresp="#s2:l1">bequaeme</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Vader <rhyme label="a" type="f" corresp="#s2:l1">eersame</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In ons geheylicht <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Soo dat <rhyme label="a" type="f" corresp="#s2:l1">betame</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Siet niemandt hem en <rhyme label="a" type="f" corresp="#s2:l1">schaeme</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Vry sonder <rhyme label="a" type="f" corresp="#s2:l1">blame</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Die hier belijden <rhyme label="b" type="m" corresp="#s2:l4">dijn</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Met s’hertschen blijdt <rhyme label="b" type="m" corresp="#s2:l4">aenschijn</rhyme></l>
        <l id="s2:l10" met="3" type="-+">In vreucht en oock in <rhyme label="b" type="m" corresp="#s2:l4">pijn</rhyme></l>
        <l id="s2:l11" met="5" type="+-">Altijt vermeeren lof prijs en eere <rhyme label="b" type="m" corresp="#s2:l4">dijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dijn salich <rhyme label="a" type="f">rijcke</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Geduyrich sonder <rhyme label="a" type="f" corresp="#s3:l1">wijcke</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Ons al <rhyme label="a" type="f" corresp="#s3:l1">gelijcke</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Vader toecomen <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Op dat claer <rhyme label="a" type="f" corresp="#s3:l1">blijcke</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat alle aertsche <rhyme label="a" type="f" corresp="#s3:l1">practijcke</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Gelijcken <rhyme label="a" type="f" corresp="#s3:l1">slijcke</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Haestich leyt onder de <rhyme label="b" type="m" corresp="#s3:l4">voet</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Dijn gheest heylich en <rhyme label="b" type="m" corresp="#s3:l4">soet</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Ons hier regeeren <rhyme label="b" type="m" corresp="#s3:l4">moet</rhyme></l>
        <l id="s3:l11" met="5" type="+-">Dat wy v kennen ons heer ons Coninck <rhyme label="b" type="m" corresp="#s3:l4">goet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Naet <rhyme label="a" type="f">onbestreden</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Op aerden hier <rhyme label="a" type="f" corresp="#s4:l1">beneden</rhyme></l>
        <l id="s4:l3" met="2" type="-+">In alle <rhyme label="a" type="f" corresp="#s4:l1">steden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dijn wille sijn <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Gelijck met <rhyme label="a" type="f" corresp="#s4:l1">vreden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Al sonder <rhyme label="a" type="f" corresp="#s4:l1">overtreden</rhyme></l>
        <l id="s4:l7" met="2" type="-+">In shemels <rhyme label="a" type="f" corresp="#s4:l1">steden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Elck doe nae dijn <rhyme label="b" type="m" corresp="#s4:l4">vermaen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Vader wilt ons <rhyme label="b" type="m" corresp="#s4:l4">bystaen</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Dat wy de rechte <rhyme label="b" type="m" corresp="#s4:l4">baen</rhyme></l>
        <l id="s4:l11" met="5" type="+-">Dynre geboden sonder of wijcken <rhyme label="b" type="m" corresp="#s4:l4">gaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Vader <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Wy v kinderen <rhyme label="a" type="f" corresp="#s5:l1">verdreven</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Suchten en <rhyme label="a" type="f" corresp="#s5:l1">beven</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Tot v in onsen <rhyme label="b" type="m">noot</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Helpt ons die <rhyme label="a" type="f" corresp="#s5:l1">sneven</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In dit ellendich <rhyme label="a" type="f" corresp="#s5:l1">leven</rhyme></l>
        <l id="s5:l7" met="2" type="-+">En wilt ons <rhyme label="a" type="f" corresp="#s5:l1">geven</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Huyden ons dagelicx <rhyme label="b" type="m" corresp="#s5:l4">broodt</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Wy zijn arm naeckt en <rhyme label="b" type="m" corresp="#s5:l4">bloodt</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Al door v miltheyt <rhyme label="b" type="m" corresp="#s5:l4">groot</rhyme></l>
        <l id="s5:l11" met="5" type="+-">Wilt ons besorgen door v ghenade <rhyme label="b" type="m" corresp="#s5:l4">zoet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Door dijn <rhyme label="a" type="f">ghenaden</rhyme></l>
        <l id="s6:l2" met="2" type="-+">So staet ons Heer in <rhyme label="a" type="f" corresp="#s6:l1">staden</rhyme></l>
        <l id="s6:l3" met="2" type="-+">En wilt ons <rhyme label="a" type="f" corresp="#s6:l1">ontladen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van t’pack der sonden <rhyme label="b" type="m">zwaer</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Quijt onse <rhyme label="a" type="f" corresp="#s6:l1">misdaden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Gelijck wy die ons <rhyme label="a" type="f" corresp="#s6:l1">versmaden</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Quellen en <rhyme label="a" type="f" corresp="#s6:l1">schaden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Haer schult quijtschelden <rhyme label="b" type="m" corresp="#s6:l4">claer</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Want ghy vergeeft <rhyme label="b" type="m" corresp="#s6:l4">voorwaer</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Zijn schulden <rhyme label="b" type="m" corresp="#s6:l4">allegaer</rhyme></l>
        <l id="s6:l11" met="5" type="+-">Die zijnen naesten vergeeft sonder <rhyme label="b" type="m" corresp="#s6:l4">misbaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Sonder v te <rhyme label="a" type="f">verlaten</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Die wereldt, vleysch, en duyvel <rhyme label="a" type="f" corresp="#s7:l1">haten</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Die ons boven <rhyme label="a" type="f" corresp="#s7:l1">maten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Aendoen so groot <rhyme label="b" type="m">gequel</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Compt toch te <rhyme label="a" type="f" corresp="#s7:l1">baten</rhyme></l>
        <l id="s7:l6" met="3" type="-+">O Heer wt <rhyme label="a" type="f" corresp="#s7:l1">charitaten</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Ons crancke <rhyme label="a" type="f" corresp="#s7:l1">vaten</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Met dijne hulpe <rhyme label="b" type="m" corresp="#s7:l4">snel</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Gheeft ons te vechten <rhyme label="b" type="m" corresp="#s7:l4">wel</rhyme></l>
        <l id="s7:l10" met="3" type="-+">In onsen strijt seer <rhyme label="b" type="m" corresp="#s7:l4">fel</rhyme></l>
        <l id="s7:l11" met="5" type="+-">Dat wy verwinnen becooring ons <rhyme label="b" type="m" corresp="#s7:l4">rebel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">O Heer o <rhyme label="a" type="f">Vader</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Ons Trooster ons <rhyme label="a" type="f" corresp="#s8:l1">Berader</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Onse drucx <rhyme label="a" type="f" corresp="#s8:l1">ontlader</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ons hoop ons <rhyme label="b" type="m">toeverlaet</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Weest geen <rhyme label="a" type="f" corresp="#s8:l1">versmader</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Maer helpt ons alle <rhyme label="a" type="f" corresp="#s8:l1">gader</rhyme></l>
        <l id="s8:l7" met="2" type="-+">Deur v goetheyt <rhyme label="a" type="f" corresp="#s8:l1">ader</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Verlost ons Heer van <rhyme label="b" type="m" corresp="#s8:l4">t’quaet</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Want dijn rijck eewich <rhyme label="b" type="m" corresp="#s8:l4">staet</rhyme></l>
        <l id="s8:l10" met="3" type="-+">Dijn cracht is sonder <rhyme label="b" type="m" corresp="#s8:l4">maet</rhyme></l>
        <l id="s8:l11" met="5" type="+-">Dijn heerlickheyt nimmermeer en <rhyme label="b" type="m" corresp="#s8:l4">vergaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
