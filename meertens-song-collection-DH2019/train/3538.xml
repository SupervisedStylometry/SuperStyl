  <author>Gerrit Hendricksz. van Breughel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEt hert met recht <rhyme label="a" type="m">beschreyt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tgheen hem is <rhyme label="b" type="f">overcomen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Om dat door <rhyme label="a" type="m" corresp="#s1:l1">wreedicheyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ick ben mijn Lieff <rhyme label="b" type="f" corresp="#s1:l2">ontnomen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Maer wie dat <rhyme label="c" type="m">doet</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Het is <rhyme label="c" type="m" corresp="#s1:l5">onvroet</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Tot zijn <rhyme label="b" type="f" corresp="#s1:l2">onvromen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick had een eenich <rhyme label="a" type="m">Lieff</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dien ick bemind int <rhyme label="b" type="f">herte</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Maer Pyrrus als een <rhyme label="a" type="m" corresp="#s2:l1">dieff</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wechvoerden my met <rhyme label="b" type="f" corresp="#s2:l2">smerte</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Doch in dat <rhyme label="c" type="m">dal</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Hy winnen <rhyme label="c" type="m" corresp="#s2:l5">sal</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Loon voor dees <rhyme label="b" type="f" corresp="#s2:l2">perte</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Orestus is mijn <rhyme label="a" type="m">heer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Int herte <rhyme label="b" type="f">uytvercoren</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Gheen vreuchde <rhyme label="a" type="m" corresp="#s3:l1">nemmermeer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ick connen sal <rhyme label="b" type="f" corresp="#s3:l2">oorboren</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Soo langh my <rhyme label="c" type="m">hier</rhyme></l>
        <l id="s3:l6" met="2" type="-+">In dit <rhyme label="c" type="m" corresp="#s3:l5">quartier</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Pirrus comt <rhyme label="b" type="f" corresp="#s3:l2">voren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Princesselijck <rhyme label="a" type="m">bedroeft</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Blijff ick in swerelts <rhyme label="b" type="f">zalen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Midts ick het snoo <rhyme label="a" type="m" corresp="#s4:l1">gheboeft</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Op u persoon hoor <rhyme label="b" type="f" corresp="#s4:l2">smalen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Tdroeff hert <rhyme label="c" type="m">onbly</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Hoopt dat ghy <rhyme label="c" type="m" corresp="#s4:l5">my</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Wedrom sult <rhyme label="b" type="f" corresp="#s4:l2">haelen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
