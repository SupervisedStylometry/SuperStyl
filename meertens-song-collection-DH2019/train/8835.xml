<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8835</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ach Gott vom Himmel</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHenadich HEEr ghenadich <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Want mijn Siel dy <rhyme label="b" type="f">Vertrouwet</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Mijn Toevlucht syn die Vleugel <rhyme label="a" type="m" corresp="#s1:l1">dijn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Daerom my niet seer <rhyme label="b" type="f" corresp="#s1:l2">grouwet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Want d' Ongheluck sal over <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Den Hoochsten Godt dien roep ick <rhyme label="c" type="m" corresp="#s1:l5">aen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Mijn Jamers wordt een <rhyme label="d" type="f">Eynde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Van HEmel coemt syn Hulpe <rhyme label="a" type="m">vroet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Helpt my van s'Vyants <rhyme label="b" type="f">blame</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Sijn Goetheyt en syn Waerheyt <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die helpen all' <rhyme label="b" type="f" corresp="#s2:l2">bequame</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Mijn Siel leydt onder Leeuwen <rhyme label="c" type="m">zaet</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">De Menschen syn als Vlammen <rhyme label="c" type="m" corresp="#s2:l5">quaet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Haer Tanden, Spies, en <rhyme label="d" type="f">Pijlen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Haer Tongen syn als scherpe <rhyme label="a" type="m">sweerd'</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die Vromen <rhyme label="b" type="f">verderven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maer Godt ghy aller Eeren <rhyme label="a" type="m" corresp="#s3:l1">weert</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Behoort all' Dinck te <rhyme label="b" type="f" corresp="#s3:l2">Erven</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Godt wilt verheven hooch dijn <rhyme label="c" type="m">Eer</rhyme></l>
        <l id="s3:l6" met="4" type="-+">So wijt te gaen, end' noch al <rhyme label="c" type="m" corresp="#s3:l5">meer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Als Aerd' end' HEmel <rhyme label="d" type="f">reycken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Op mijnen ganck haer Nette <rhyme label="a" type="m">wacht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Nae mijn Bedruck sy <rhyme label="b" type="f">Loeren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Sy graven my een diepe <rhyme label="a" type="m" corresp="#s4:l1">Gracht</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maer selfs daer in <rhyme label="b" type="f" corresp="#s4:l2">versmooren</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Mijn Godt vergaen is all' mijn <rhyme label="c" type="m">Leydt</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Met Yver is mijn Hert <rhyme label="c" type="m" corresp="#s4:l5">bereydt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Te singhen end' te <rhyme label="d" type="f">Loven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Waeck op mijn Psalter end mijn <rhyme label="a" type="m">Eer</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Mijn Harp, ick wil vroe <rhyme label="b" type="f">Waecken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Den HEERe wil ick Dancken <rhyme label="a" type="m" corresp="#s5:l1">seer</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">End' gaen syn Loff groot <rhyme label="b" type="f" corresp="#s5:l2">maecken</rhyme></l>
        <l id="s5:l5" met="4" type="-+">By Volcken, end' int <rhyme label="c" type="m">Openbaer</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Verconden wil ick <rhyme label="c" type="m" corresp="#s5:l5">Allegaer</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wat Godt aen my <rhyme label="d" type="f">bewesen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">So wijt als (Heer) den Hemel <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">End' als de Wolcken <rhyme label="b" type="f">sweven</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">So wijt dijn Goetheyt gaet <rhyme label="a" type="m" corresp="#s6:l1">ghewis</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">End' dijn Waerheyt <rhyme label="b" type="f" corresp="#s6:l2">verheven</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Godt wilt verheven hooch dijn <rhyme label="c" type="m">Eer</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">So wijt te gaen end' noch al <rhyme label="c" type="m" corresp="#s6:l5">meer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Als Aerd' end' HEmel <rhyme label="d" type="f">reycken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
