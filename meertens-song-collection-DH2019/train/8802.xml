<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8802</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Von allen Menschen abgewand</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VAn alle Menschen ick my <rhyme label="a" type="m">keer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Alleen tot dy <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Heb ick mijn Ziel, O GOdt mijn <rhyme label="a" type="m" corresp="#s1:l1">HEER</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt my niet laten <rhyme label="b" type="f" corresp="#s1:l2">sneven</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">All' mijn Vertrouwen staet aen <rhyme label="c" type="m">dy</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Laet niet te schanden worden <rhyme label="c" type="m" corresp="#s1:l5">my</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Maeckt mijn Vyant niet <rhyme label="d" type="f">blijde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want niemant salder schaemroot <rhyme label="a" type="m">staen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van dien die op dy <rhyme label="b" type="f">bouwen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Aen dijner Hande sy seker <rhyme label="a" type="m" corresp="#s2:l1">gaen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die Coop sal haer niet <rhyme label="b" type="f" corresp="#s2:l2">Rouwen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Maer schaemroot moeten alle <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Die Leedt aen doen den Armen <rhyme label="c" type="m" corresp="#s2:l5">dijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Al sonder Rechte <rhyme label="d" type="f">saecken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wijst my den Rechten Wech tot <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">End' dijnen Pat my <rhyme label="b" type="f">leere</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">In dijner Waerheyt leydet <rhyme label="a" type="m" corresp="#s3:l1">my</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Want ghy zijt GOdt mijn <rhyme label="b" type="f" corresp="#s3:l2">HEEre</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Mijn Heyl en Troost, mijn Hulp en <rhyme label="c" type="m">Raet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Daer op ick my altijt <rhyme label="c" type="m" corresp="#s3:l5">verlaet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Jae staedich poch' en <rhyme label="d" type="f">Trotze</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Laet dy mijn HEer ter Herten <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">End' wilt daer aen <rhyme label="b" type="f">ghedencken</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hoe alle dien die met dy <rhyme label="a" type="m" corresp="#s4:l1">staen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dien ghy Ghenaed' gaet <rhyme label="b" type="f" corresp="#s4:l2">schencken</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Van Eewicheyt haer wt <rhyme label="c" type="m">voorsien</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Bewaert wel in den Raede <rhyme label="c" type="m" corresp="#s4:l5">dijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Daer door sy Salich <rhyme label="d" type="f">worden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Al mijner Jeucht <rhyme label="a" type="m">Onwetentheyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">End' alle mijne <rhyme label="b" type="f">Schulde</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Ghedenckt doch niet, sy zijn my <rhyme label="a" type="m" corresp="#s5:l1">leydt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maer (HEEr) nae dijner <rhyme label="b" type="f" corresp="#s5:l2">Hulde</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Wilt over my ontfermen <rhyme label="c" type="m">dy</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Van allen Sonden maecken <rhyme label="c" type="m" corresp="#s5:l5">vry</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Om dijner Goetheyt <rhyme label="d" type="f">wille</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De HEEr is soet, end' oprecht <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">All' dien die hem <rhyme label="b" type="f">aenhanghen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Al ist dat glijden deed' haer <rhyme label="a" type="m" corresp="#s6:l1">Voet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Hy sal sy doch <rhyme label="b" type="f" corresp="#s6:l2">ontfanghen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">End' leerense den Willen <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Gheschreven in haer Herte <rhyme label="c" type="m" corresp="#s6:l5">fijn</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Al nae syn <rhyme label="d" type="f">Welbehaghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Des Heeren Wege sijn <rhyme label="a" type="m">Waerheyt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">De Goetheyt end' <rhyme label="b" type="f">Ghenade</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Sijn Toesaeg' is in <rhyme label="a" type="m" corresp="#s7:l1">Trouwicheyt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Hy gheeftse dien al <rhyme label="b" type="f" corresp="#s7:l2">draede</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Die vraghen nae den Woorde <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Ghelooven wat hy leert daer <rhyme label="c" type="m" corresp="#s7:l5">in</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Als ons die Schrifft <rhyme label="d" type="f">verclaren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">All' die in GOdts Vrees <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Sal syne Goeden <rhyme label="b" type="f">Erven</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">All' die hem int Gheloof aen <rhyme label="a" type="m" corresp="#s8:l1">cleeft</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die sullen niet <rhyme label="b" type="f" corresp="#s8:l2">verderven</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Die HEEr is haer verborgen <rhyme label="c" type="m">Schilt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Sijn Testament hun Op'nen <rhyme label="c" type="m" corresp="#s8:l5">wilt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">End' synen Gheest haer <rhyme label="d" type="f">geven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Mijn Ooghen staedich sien tot <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Wilt my HEEr Godt <rhyme label="b" type="f">verquicken</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Coemt helpen wt den Nette <rhyme label="a" type="m" corresp="#s9:l1">my</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Daer in sy my <rhyme label="b" type="f" corresp="#s9:l2">bestricken</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Ontfermt u mijns, end' siet, my <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Want Arm ben ick, van alle <rhyme label="c" type="m" corresp="#s9:l5">Man</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Gheheel end' al <rhyme label="d" type="f">verlaten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Mijns Herten Wee is swaer end' <rhyme label="a" type="m">veel</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Ach Heer mijn Troost <rhyme label="b" type="f">verleene</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Aensiet hoe ick beancxtet <rhyme label="a" type="m" corresp="#s10:l1">heel</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Ben sonder Hulp <rhyme label="b" type="f" corresp="#s10:l2">alleene</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Daerom vergeeft de sonde <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Aensiet hoe veel der Vyand' <rhyme label="c" type="m" corresp="#s10:l5">zijn</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Die my t'Onrecht <rhyme label="d" type="f">vervolghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
