    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>O regina felix ave</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jan Mommaert</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ZYt ghegroet, o <rhyme label="a" type="f">Koninghinne</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Vergun ons doch de Sonn' uws <rhyme label="a" type="f" corresp="#s1:l1">minne</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Soo als sy over-straelt de <rhyme label="b" type="f">leden</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Van uwe Krygers in het <rhyme label="b" type="f" corresp="#s1:l3">Sweden</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s1:l5">kroonen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s1:l1">Heldinne</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s1:l1">Koninghinne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ghy ciert de leden uws <rhyme label="a" type="f">Staten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ghy zyt den glans <rhyme label="a" type="f" corresp="#s2:l1">onder-saten</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">O waeder licht van waerder <rhyme label="b" type="f">lieden</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Wie sal u eer naer waerde <rhyme label="b" type="f" corresp="#s2:l3">bieden</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s2:l5">kroonen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s2:l1">Heldinne</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s2:l1">Koninghinne</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die u vergoden met <rhyme label="a" type="f">gaven</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Van syn persoon u in te <rhyme label="a" type="f" corresp="#s3:l1">graven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die droegh al d'eer op aen u <rhyme label="b" type="f">schoonheydt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En leifde waer ghy uwen throon <rhyme label="b" type="f" corresp="#s3:l3">spreydt</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s3:l5">kroonen</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s3:l1">Heldinne</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s3:l1">Koninghinne</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">De schildery toont u <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Maer wat vermagh sy op u <rhyme label="a" type="f" corresp="#s4:l1">zeden</rhyme>?</l>
        <l id="s4:l3" met="4" type="-+">Vervalt-men op uw' buyten <rhyme label="b" type="f">zwieren</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Hoe sal u binnenst' ons <rhyme label="b" type="f" corresp="#s4:l3">bestieren</rhyme>?</l>
        <l id="s4:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s4:l5">kroonen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s4:l1">Heldinne</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s4:l1">Koninghinne</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'t Breyn van doeck en tabbaert 't <rhyme label="a" type="f">samen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Wat mans oft vrouwen oyt <rhyme label="a" type="f" corresp="#s5:l1">bequamen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Besit ghy, ghy kont uwen <rhyme label="b" type="f">waghen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Door alle sanden henen <rhyme label="b" type="f" corresp="#s5:l3">jaghen</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s5:l5">kroonen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s5:l1">Heldinne</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s5:l1">Koninghinne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">De wis-konst selv' is <rhyme label="a" type="f">on-seker</rhyme></l>
        <l id="s6:l2" met="4" type="-+">By uwen ed'len <rhyme label="a" type="f" corresp="#s6:l1">Hersens-beker</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Euclides sou sich eer <rhyme label="b" type="f">vertasten</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Als ghy in uwen plicht, en <rhyme label="b" type="f" corresp="#s6:l3">lasten</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s6:l5">kroonen</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s6:l1">Heldinne</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s6:l1">Koninghinne</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wat Syracus heeft <rhyme label="a" type="f">gheweten</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Wat Samien oyt  heeft <rhyme label="a" type="f" corresp="#s7:l1">beseten</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dat weet gy, zyt gy <rhyme label="b" type="f">door-gekomen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">O proef-stuck van al 's wereldts <rhyme label="b" type="f" corresp="#s7:l3">dromen</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s7:l5">kroonen</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s7:l1">Heldinne</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s7:l1">Koninghinne</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">D'aerd en kan u niet <rhyme label="a" type="f">bepalen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Ghy dooft des hemels <rhyme label="a" type="f" corresp="#s8:l1">sonne-stralen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ghy doet de lichten sonder licht <rhyme label="b" type="f">zyn</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Wat konst kan konst in uw gesicht <rhyme label="b" type="f" corresp="#s8:l3">zyn</rhyme>?</l>
        <l id="s8:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s8:l5">kroonen</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s8:l1">Heldinne</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s8:l1">Koninghinne</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Daerom is't goet maer te <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">En met een deun van u te <rhyme label="a" type="f" corresp="#s9:l1">springen</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">Wat baden wy u ander <rhyme label="b" type="f">wercken</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Wie sou niet vlughs ons zwackheyt <rhyme label="b" type="f" corresp="#s9:l3">mercken</rhyme>?</l>
        <l id="s9:l5" met="4" type="+-">Leeft o Schoone, in de <rhyme label="c" type="f">throonen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">In de Toonen, in de <rhyme label="c" type="f" corresp="#s9:l5">kroonen</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">In't Rycx hoonen, leeft <rhyme label="a" type="f" corresp="#s9:l1">Heldinne</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">En blyft u selv' tot een <rhyme label="a" type="f" corresp="#s9:l1">Koninghinne</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
