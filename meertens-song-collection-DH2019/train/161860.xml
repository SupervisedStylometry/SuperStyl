  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Schoone, werd  ook eens <rhyme label="a" type="f">bewogen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Laat mijn smeken dat <rhyme label="a" type="f" corresp="#s1:l1">vermogen</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Siet niet langer straf en <rhyme label="b" type="m">wreed</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Maar geneesd mijn smert en <rhyme label="b" type="m" corresp="#s1:l3">leed</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Maar geneesd mijn smert en <rhyme label="b" type="m" corresp="#s1:l3">leed</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ag, laat eens door mijne <rhyme label="a" type="f">klagten</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Uw versteende hert <rhyme label="a" type="f" corresp="#s2:l1">versagten</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Buygd eenmaal uw fiere <rhyme label="b" type="m">zin</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Voor de kragt van mijne <rhyme label="b" type="m" corresp="#s2:l3">min</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Voor de kragt van mijne <rhyme label="b" type="m" corresp="#s2:l3">min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maar kan mijn gevley niet <rhyme label="a" type="f">baten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Vaart gy voort in my te <rhyme label="a" type="f" corresp="#s3:l1">haten</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Sal my eerlang uwe <rhyme label="b" type="m">straf</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Voeren in het duyster <rhyme label="b" type="m" corresp="#s3:l3">graf</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Voeren in het duyster <rhyme label="b" type="m" corresp="#s3:l3">graf</rhyme>.</l>
      </lg>
    </lg>
  </text>
