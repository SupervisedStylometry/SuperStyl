  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EEr Apoll' zijn vier'ge <rhyme label="a" type="f">Paarden</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">In het Oost <rhyme label="a" type="f" corresp="#s1:l1">aanvaarden</rhyme>,</l>
        <l id="s1:l3" met="7" type="-+">Geleiden ik mijn grage geitjes, langs het <rhyme label="b" type="m">Zuier-dal</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Daar ik mijn <rhyme label="c" type="m">Zon</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Beschoten <rhyme label="c" type="m" corresp="#s1:l4">von</rhyme>;</l>
        <l id="s1:l6" met="2" type="-+">Mijn ogen <rhyme label="a" type="f" corresp="#s1:l1">staarden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Op 't mals <rhyme label="d" type="m">koraal</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">VViens zoet <rhyme label="d" type="m" corresp="#s1:l7">onthaal</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Mijn eerste vrijheit <rhyme label="b" type="m" corresp="#s1:l3">stal</rhyme>.</l>
      </lg>
    </lg>
  </text>
