<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35784</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Beata immaculata</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GEeft, O Jesu! my van <rhyme label="a" type="f">boven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Uyt uw' Hemelsche <rhyme label="b" type="m">Throon</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ick na uw' wil mach <rhyme label="a" type="f" corresp="#s1:l1">loven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Lucas onse <rhyme label="b" type="m" corresp="#s1:l2">Patroon</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Sint Lucas <rhyme label="c" type="m">eer</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Bemin ick <rhyme label="c" type="m" corresp="#s1:l5">seer</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Door hem ick soeck, u lof, o <rhyme label="c" type="m" corresp="#s1:l5">Heer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">t' Antiochi is <rhyme label="a" type="f">geboren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van rijck Edel <rhyme label="b" type="m">gheslacht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Na 's werelts buyght hart noch <rhyme label="a" type="f" corresp="#s2:l1">ooren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Niet als Godt socht, en <rhyme label="b" type="m" corresp="#s2:l2">acht</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Als sijn jeught <rhyme label="c" type="m">boeyt</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">In wijsheyt <rhyme label="c" type="m" corresp="#s2:l5">groeyt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Tot Godt en deughden <rhyme label="c" type="m" corresp="#s2:l5">overvloeyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Veel geleerden <rhyme label="a" type="f">overtreften</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Door sijn wijsheyt <rhyme label="b" type="m">groot</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Sint Lucas hem noyt <rhyme label="a" type="f" corresp="#s3:l1">verheften</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Schout dit meer als de <rhyme label="b" type="m" corresp="#s3:l2">doodt</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Leeft na Godts <rhyme label="c" type="m">Wet</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Suyver en <rhyme label="c" type="m" corresp="#s3:l5">net</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Hem van sonden houdt <rhyme label="c" type="m" corresp="#s3:l5">onbesmet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In de Cruyden was <rhyme label="a" type="f">ervaren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Was een vermaert <rhyme label="b" type="m">Doctoor</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Dese kunst laet hy <rhyme label="a" type="f" corresp="#s4:l1">varen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Godt roept hem, geeft <rhyme label="b" type="m" corresp="#s4:l2">gehoor</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Sijn Edel <rhyme label="c" type="m">bloedt</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">En al sijn <rhyme label="c" type="m" corresp="#s4:l5">goedt</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Verlaet, om Iesus woorden <rhyme label="c" type="m" corresp="#s4:l5">soet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Heeft Jesum hooren <rhyme label="a" type="f">verhalen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van sijne Hemels <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Sonder ergens na te <rhyme label="a" type="f" corresp="#s5:l1">talen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Verlaet al met <rhyme label="b" type="m" corresp="#s5:l2">geneught</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Met Jesu <rhyme label="c" type="m">gaet</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Waer dat hy <rhyme label="c" type="m" corresp="#s5:l5">staet</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Soo langh Iesus leeft, niet <rhyme label="c" type="m" corresp="#s5:l5">verlaet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer als Iesu was <rhyme label="a" type="f">verresen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Lucas na Emaus <rhyme label="b" type="m">gaet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Met Cleophas vol van <rhyme label="a" type="f" corresp="#s6:l1">vreesen</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Iesus met haer doen <rhyme label="b" type="m" corresp="#s6:l2">praet</rhyme>;</l>
        <l id="s6:l5" met="2" type="-+">Een Pelgrim <rhyme label="c" type="m">scheen</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Vraeght na haer <rhyme label="c" type="m" corresp="#s6:l5">re'en</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Na 't breecken des Broodts, hy <rhyme label="c" type="m" corresp="#s6:l5">verdween</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Leeft een Seraphijnsche <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Suyverheydt noyt <rhyme label="b" type="m">verliest</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">De Kerck Lucam heeft <rhyme label="a" type="f" corresp="#s7:l1">verheven</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die hem acht, en <rhyme label="b" type="m" corresp="#s7:l2">verkiest</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Als d'meeste <rhyme label="c" type="m">schat</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">En sonder <rhyme label="c" type="m" corresp="#s7:l5">dat</rhyme></l>
        <l id="s7:l7" met="4" type="-+">Aen Paulo geeft, dit heyligh <rhyme label="c" type="m" corresp="#s7:l5">Vat</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sint Paulus lijdt in sijn <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Slagen, en Hongers <rhyme label="b" type="m">noodt</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Als d'ander Discipels 't <rhyme label="a" type="f" corresp="#s8:l1">sagen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Vreesende al de <rhyme label="b" type="m" corresp="#s8:l2">doodt</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Sy vluchten <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Met groot <rhyme label="c" type="m" corresp="#s8:l5">getal</rhyme>;</l>
        <l id="s8:l7" met="4" type="-+">Lucas alleen blijft, en houdt <rhyme label="c" type="m" corresp="#s8:l5">stal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Van Paulo hem niet kost <rhyme label="a" type="f">scheyden</rhyme>;</l>
        <l id="s9:l2" met="3" type="-+">Soo vroom is hy <rhyme label="b" type="m">geweest</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Al wilt-men hem ter doodt <rhyme label="a" type="f" corresp="#s9:l1">leyden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Hy blijft heel <rhyme label="b" type="m" corresp="#s9:l2">onbevreest</rhyme>;</l>
        <l id="s9:l5" met="2" type="-+">Blijft altijdt <rhyme label="c" type="m">stijf</rhyme></l>
        <l id="s9:l6" met="2" type="-+">Met ziel en <rhyme label="c" type="m" corresp="#s9:l5">lijf</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Tot Paulus hulp, ende <rhyme label="c" type="m" corresp="#s9:l5">gerijf</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Sinte Paulus slaet sijn <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Op yder Godes <rhyme label="b" type="m">Klerck</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">In Sint Pieter niet <rhyme label="a" type="f" corresp="#s10:l1">gedoogen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Heeft, een kleyn sondigh <rhyme label="b" type="m" corresp="#s10:l2">werck</rhyme>:</l>
        <l id="s10:l5" met="2" type="-+">Om een kleyn <rhyme label="c" type="m">fout</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Tegens hem <rhyme label="c" type="m" corresp="#s10:l5">gront</rhyme></l>
        <l id="s10:l7" met="4" type="-+">Onberispelijck Lucam <rhyme label="c" type="m" corresp="#s10:l5">houdt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Godes wercken heeft <rhyme label="a" type="f">beschreven</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">In sijn <rhyme label="b" type="m">Euangely</rhyme>;</l>
        <l id="s11:l3" met="3" type="-+">En wat d'Apost'len <rhyme label="a" type="f" corresp="#s11:l1">bedreven</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Hebben, stelt hy daer <rhyme label="b" type="m" corresp="#s11:l2">by</rhyme>:</l>
        <l id="s11:l5" met="2" type="-+">Soo Leeck als <rhyme label="c" type="m">Klerck</rhyme>,</l>
        <l id="s11:l6" met="2" type="-+">Prijst dit fray <rhyme label="c" type="m" corresp="#s11:l5">werck</rhyme>;</l>
        <l id="s11:l7" met="4" type="-+">Hierom prijst hem de heele <rhyme label="c" type="m" corresp="#s11:l5">Kerck</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Als Gabriel komt <rhyme label="a" type="f">boodtschappen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Maria hare <rhyme label="b" type="m">Vrucht</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Wordt ontroert; al komt <rhyme label="a" type="f" corresp="#s12:l1">aen-stappen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Lucas s' was niet <rhyme label="b" type="m" corresp="#s12:l2">beducht</rhyme>.</l>
        <l id="s12:l5" met="2" type="-+">By haer <rhyme label="c" type="m">geme'en</rhyme>,</l>
        <l id="s12:l6" met="2" type="-+">Had hy veel <rhyme label="c" type="m" corresp="#s12:l5">re'en</rhyme>,</l>
        <l id="s12:l7" met="4" type="-+">Voor haer meer, dan een Engel <rhyme label="c" type="m" corresp="#s12:l5">sche'en</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Noch Lucam heeft <rhyme label="a" type="f">toe-gelaten</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Dat hy haer <rhyme label="b" type="m">conterfeyt</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Dit Beeldt in pest langhs de <rhyme label="a" type="f" corresp="#s13:l1">straten</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Gedragen, Pest <rhyme label="b" type="m" corresp="#s13:l2">afscheyt</rhyme>:</l>
        <l id="s13:l5" met="2" type="-+">Pestige <rhyme label="c" type="m">lucht</rhyme></l>
        <l id="s13:l6" met="2" type="-+">Van Roomen <rhyme label="c" type="m" corresp="#s13:l5">vlucht</rhyme>,</l>
        <l id="s13:l7" met="4" type="-+">Hierdoor op hil haer root <rhyme label="c" type="m" corresp="#s13:l5">gesucht</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Na Paulus doodt veele <rhyme label="a" type="f">Landen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Sint Lucas heeft <rhyme label="b" type="m">bekeert</rhyme>;</l>
        <l id="s14:l3" met="3" type="-+">Eerst dese tot Godes <rhyme label="a" type="f" corresp="#s14:l1">schanden</rhyme></l>
        <l id="s14:l4" met="3" type="-+">D'Afgodt hadden <rhyme label="b" type="m" corresp="#s14:l2">ge-eert</rhyme>:</l>
        <l id="s14:l5" met="2" type="-+">Al Satans <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s14:l6" met="2" type="-+">Al Satans <rhyme label="c" type="m" corresp="#s14:l5">macht</rhyme></l>
        <l id="s14:l7" met="4" type="-+">Worden verfoeyt, en Godt <rhyme label="c" type="m" corresp="#s14:l5">geacht</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Tot Bethyni oudt van <rhyme label="a" type="f">Iaren</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Preeckten hy Godes <rhyme label="b" type="m">woordt</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Daer eenige boose <rhyme label="a" type="f" corresp="#s15:l1">waren</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Hem hangen met een <rhyme label="b" type="m" corresp="#s15:l2">koort</rhyme></l>
        <l id="s15:l5" met="2" type="-+">Aen <rhyme label="c" type="m">d'Olijf-boom</rhyme>,</l>
        <l id="s15:l6" met="2" type="-+">Daer aen sterft <rhyme label="c" type="m" corresp="#s15:l5">vroom</rhyme>,</l>
        <l id="s15:l7" met="4" type="-+">Dit brocht hem aen vreught, en geen <rhyme label="c" type="m" corresp="#s15:l5">schroom</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
