  <author>Guillaume Abrahams Ooijevaer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Fyne Christalyne <rhyme label="a" type="f">Brillen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Groote Kaersen schoon van <rhyme label="b" type="m">licht</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Helpen geen verkeert <rhyme label="b" type="m" corresp="#s1:l2">gesicht</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Staegh gewendt tot malle <rhyme label="a" type="f" corresp="#s1:l1">grillen</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Want 't en baet noch Kaers, noch <rhyme label="c" type="m">Bril</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Als een Sot niet sien en <rhyme label="c" type="m" corresp="#s1:l5">wil</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Schoon van Aensicht, Lijf en <rhyme label="a" type="f">Leden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Schoon en kost'lijck in 't <rhyme label="b" type="m">gewaet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Schoon en vriendelijck van <rhyme label="b" type="m" corresp="#s2:l2">praet</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Sonder deught, en goede <rhyme label="a" type="f" corresp="#s2:l1">seden</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Is wanneer men 't wel <rhyme label="c" type="m">besiet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Maer een opgepronckte <rhyme label="c" type="m" corresp="#s2:l5">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Rijck van Vrienden en van <rhyme label="a" type="f">Maghen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Rijck van Schat, ja <rhyme label="b" type="m">overvloedt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Rijck van Geldt, en rijck van <rhyme label="b" type="m" corresp="#s3:l2">Goedt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Doet de Geldt-sucht niet <rhyme label="a" type="f" corresp="#s3:l1">vertragen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">'t Is gelijck het spreeck-woordt <rhyme label="c" type="m">seydt</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Gelt en stopt gheen <rhyme label="c" type="m" corresp="#s3:l5">Gierigheydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Veel besitten, meer <rhyme label="a" type="f">begeeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Niet uytgeven dan met <rhyme label="b" type="m">pijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Nimmermeer te vreden <rhyme label="b" type="m" corresp="#s4:l2">zijn</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Nau een stuyver te <rhyme label="a" type="f" corresp="#s4:l1">verteeren</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Niet dan Geldt by Geldt <rhyme label="c" type="m">vergaert</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Dat's een rechten gier'gen <rhyme label="c" type="m" corresp="#s4:l5">aert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Weynigh hebben, niet te <rhyme label="a" type="f">winnen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Veel verteeren by de <rhyme label="b" type="m">Maets</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Is wel oorsaeck van veel <rhyme label="b" type="m" corresp="#s5:l2">quaets</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Leege Beursen, yd'le <rhyme label="a" type="f" corresp="#s5:l1">sinnen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Beyde handen in het <rhyme label="c" type="m">haer</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dat valt dan gemeenlijck <rhyme label="c" type="m" corresp="#s5:l5">waer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die het Zeyl te hoogh op <rhyme label="a" type="f">halen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En in Hovaerdy en <rhyme label="b" type="m">pracht</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Brallen boven hare <rhyme label="b" type="m" corresp="#s6:l2">Macht</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Komend' eynd'lijck niet <rhyme label="a" type="f" corresp="#s6:l1">betalen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Speelen met eens anders <rhyme label="c" type="m">goedt</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Onbeschaemd'lijck <rhyme label="c" type="m" corresp="#s6:l5">Banckeroet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Schoone woorden sonder <rhyme label="a" type="f">meenen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Vriendtschap toonen Vyandt <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Is dat niet een boos <rhyme label="b" type="m" corresp="#s7:l2">fenijn</rhyme>?</l>
        <l id="s7:l4" met="4" type="+-">'t Is een aert van al de <rhyme label="a" type="f" corresp="#s7:l1">geene</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Die met schijn-deught (looft het <rhyme label="c" type="m">vry</rhyme>)</l>
        <l id="s7:l6" met="4" type="+-">Decken al haer <rhyme label="c" type="m" corresp="#s7:l5">schelmery</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Soud' ick al de <rhyme label="a" type="f">Guyteryen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Al de boosheyt die men <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Die nu hedensdaeghs <rhyme label="b" type="m" corresp="#s8:l2">geschiet</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Met mijn kleyne macht <rhyme label="a" type="f" corresp="#s8:l1">beschrijven</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">'t Waer vergeefs: want yeder <rhyme label="c" type="m">hooft</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Aen sich selven meest <rhyme label="c" type="m" corresp="#s8:l5">gelooft</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Soo de Mensch in alle <rhyme label="a" type="f">dingen</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Socht de gulde <rhyme label="b" type="m">Middelmaet</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">'t Waer wel goet: want maet houdt <rhyme label="b" type="m" corresp="#s9:l2">staet</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Menigh soud van vreughde <rhyme label="a" type="f" corresp="#s9:l1">singen</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Die nu met bedroeftheydt <rhyme label="c" type="m">leert</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Hoe de Wereldt loopt <rhyme label="c" type="m" corresp="#s9:l5">verkeert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
