<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4302</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Laatstmaal ging ik spanseren</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="emotions">emoties</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MInsaem soete <rhyme label="a" type="f">natuere</rhyme> //</l>
        <l id="s1:l2" met="3" type="-+">Die my <rhyme label="b" type="m">contrariert</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hoe seer dijn <rhyme label="a" type="f" corresp="#s1:l1">willecuere</rhyme> //</l>
        <l id="s1:l4" met="3" type="-+">Ootmoedich wert <rhyme label="b" type="m" corresp="#s1:l2">begeert</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Toogt heden // <rhyme label="c" type="m">maer</rhyme> //</l>
        <l id="s1:l6" met="2" type="+-">Een reden // <rhyme label="c" type="m" corresp="#s1:l5">claer</rhyme> //</l>
        <l id="s1:l7" met="3" type="-+">Waerom ghy <rhyme label="b" type="m" corresp="#s1:l2">rebelleert</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een wtgelesen // <rhyme label="a" type="f">vrouwe</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Scat ick naer weerde // <rhyme label="b" type="m">dy</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om wien ick desen // <rhyme label="a" type="f" corresp="#s2:l1">rouwe</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Lijdsaem aenveerde // <rhyme label="b" type="m" corresp="#s2:l2">vry</rhyme>:</l>
        <l id="s2:l5" met="2" type="+-">Geen lieuer <rhyme label="c" type="m">ieught</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Die my <rhyme label="c" type="m" corresp="#s2:l5">verheught</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Op geheel d'aerde // <rhyme label="b" type="m" corresp="#s2:l2">sy</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sal my maken <rhyme label="a" type="f">mismoedich</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Der wonden een, of <rhyme label="b" type="m">twee</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">O neen! ick strijde <rhyme label="a" type="f" corresp="#s3:l1">bloedich</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tot inde laetste <rhyme label="b" type="m" corresp="#s3:l2">wee</rhyme>:</l>
        <l id="s3:l5" met="2" type="+-">Al briesschen <rhyme label="c" type="m">som</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Vyandich <rhyme label="c" type="m" corresp="#s3:l5">om</rhyme></l>
        <l id="s3:l7" met="3" type="-+">My, als een wilde <rhyme label="b" type="m" corresp="#s3:l2">zee</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Verhaert vileyne // <rhyme label="a" type="f">tongen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Verberght u wreet <rhyme label="b" type="m">aenschijn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Vipers onreyne // <rhyme label="a" type="f" corresp="#s4:l1">iongen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Serpentich boos <rhyme label="b" type="m" corresp="#s4:l2">fenijn</rhyme>:</l>
        <l id="s4:l5" met="2" type="+-">V paden // <rhyme label="c" type="m">gaet</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Geen quaden // <rhyme label="c" type="m" corresp="#s4:l5">raet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Sal hier van weerde <rhyme label="b" type="m" corresp="#s4:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Edel, ia <rhyme label="a" type="f">magnifijcke</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Beelde, pointich <rhyme label="b" type="m">gewracht</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Theseus ick my <rhyme label="a" type="f" corresp="#s5:l1">gelijcke</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Doch Ariadne <rhyme label="b" type="m" corresp="#s5:l2">slacht</rhyme>:</l>
        <l id="s5:l5" met="2" type="+-">Soo werd ick <rhyme label="c" type="m">uyt</rhyme></l>
        <l id="s5:l6" met="2" type="+-">Den Dool-hoff <rhyme label="c" type="m" corresp="#s5:l5">ruyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">By uwer hant <rhyme label="b" type="m" corresp="#s5:l2">gebracht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Roup ick aldus om <rhyme label="a" type="f">bate</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Princesse, altijt <rhyme label="b" type="m">meer</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Sijt my een <rhyme label="a" type="f" corresp="#s6:l1">honighrate</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">In al mijn cranckheyt <rhyme label="b" type="m" corresp="#s6:l2">teer</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Daer vooren <rhyme label="c" type="m">sal</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Ick over <rhyme label="c" type="m" corresp="#s6:l5">al</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">V schoonheyt loven <rhyme label="b" type="m" corresp="#s6:l2">seer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
