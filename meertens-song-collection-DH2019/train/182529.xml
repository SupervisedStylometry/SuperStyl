  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik hoorde lest een <rhyme label="a" type="f">Dogter</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die haer stond al in haer <rhyme label="b" type="m">deur</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Zy liep al by haer <rhyme label="a" type="f" corresp="#s1:l1">Moeder</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En zy maekte groot <rhyme label="b" type="m" corresp="#s1:l2">getreur</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ach moeder hoord my <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En Wilt my wel <rhyme label="c" type="m" corresp="#s1:l5">verstaen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Kan ik geen Vryere <rhyme label="d" type="f">krygen</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Wel het is met my <rhyme label="c" type="m" corresp="#s1:l5">gedaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Moeder zegt wel <rhyme label="a" type="f">Dogter</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Stel dit mear uit uwe <rhyme label="b" type="m">zin</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Gy ziet de geen die <rhyme label="a" type="f" corresp="#s2:l1">paren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Het gaet zoet in 't <rhyme label="b" type="m" corresp="#s2:l2">begin</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Maer op het lesten <rhyme label="c" type="m">werd</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die zoetheid niet als <rhyme label="c" type="m" corresp="#s2:l5">smert</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Daerom hoord naer myn <rhyme label="d" type="f">reden</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">En in myn raed <rhyme label="c" type="m" corresp="#s2:l5">volherd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wel Moeder van u <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Waer gy my al voor wilt <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ben ik niet wel te <rhyme label="a" type="f" corresp="#s3:l1">vreden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zy staet my geensins <rhyme label="b" type="m" corresp="#s3:l2">aen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maer veel Ouders <rhyme label="c" type="m">hier</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Vergeeten hun <rhyme label="c" type="m" corresp="#s3:l5">plaissier</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Zy zyn van twintig <rhyme label="d" type="f">jaren</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Al getrouwd: ach wat <rhyme label="c" type="m" corresp="#s3:l5">dangier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wel Dogter uwe <rhyme label="a" type="f">woorden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Heel myn hart en ziel <rhyme label="b" type="m">doorsnyd</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'k Zal u de beenen <rhyme label="a" type="f" corresp="#s4:l1">breeken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Voor u schandelyk <rhyme label="b" type="m" corresp="#s4:l2">verwyt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zo doet dan uwen <rhyme label="c" type="m">zin</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En trouwd door dwase <rhyme label="c" type="m" corresp="#s4:l5">min</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Maer en 'komt my niet <rhyme label="d" type="f">klagen</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">O gy gekke malle <rhyme label="c" type="m" corresp="#s4:l5">zottin</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maer korten tyd na <rhyme label="a" type="f">desen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Zo werd derz' Dogter een <rhyme label="b" type="m">Paer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De bruiloft werd <rhyme label="a" type="f" corresp="#s5:l1">gehouden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Zy gaat op geen <rhyme label="b" type="m" corresp="#s5:l2">misbaer</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Den Speelman die zat op <rhyme label="c" type="m">dak</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Het gaet op het <rhyme label="c" type="m" corresp="#s5:l5">gemak</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Daer viel hy van de <rhyme label="d" type="f">zolder</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">En al zyn snaeren <rhyme label="c" type="m" corresp="#s5:l5">brak</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Daer kwam een groote <rhyme label="a" type="f">rusie</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Grouwelyk in 't <rhyme label="b" type="m">openbaer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Trynise kreeg twee blauw <rhyme label="a" type="f" corresp="#s6:l1">oogen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Door een zalig nieuwe <rhyme label="b" type="m" corresp="#s6:l2">jaer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En een dikken <rhyme label="c" type="m">neus</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Stond dat niet <rhyme label="c" type="m" corresp="#s6:l5">genereus</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Zy vliegt al van de <rhyme label="d" type="f">trappen</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Zy barst als een water <rhyme label="c" type="m" corresp="#s6:l5">d[...]eus</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Zy loopt regt by haer <rhyme label="a" type="f">moeder</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Om te klagen haer <rhyme label="b" type="m">verdriet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zy sprak wilt my <rhyme label="a" type="f" corresp="#s7:l1">aenhoren</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Nooit u dagen is 't <rhyme label="b" type="m" corresp="#s7:l2">geschied</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En dat eenen <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Hy slaet my stram en <rhyme label="c" type="m" corresp="#s7:l5">lam</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ach wil myn raed dan <rhyme label="d" type="f">geeven</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Ach ik sterf daer <rhyme label="c" type="m" corresp="#s7:l5">van</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sa dogter wilt <rhyme label="a" type="f">vertrekken</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Maekt u maer gaeuw uit myn <rhyme label="b" type="m">huis</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Hier en valt niet te <rhyme label="a" type="f" corresp="#s8:l1">gekken</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Of ie maken groot <rhyme label="b" type="m" corresp="#s8:l2">gedruis</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Had gy myne raed <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Gy waerd in geen <rhyme label="c" type="m" corresp="#s8:l5">getraen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Bedenkt gy nu myn <rhyme label="d" type="f">woorden</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Wat ik u zei <rhyme label="c" type="m" corresp="#s8:l5">voortaen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wel Moeder nooit u <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">En is u zulks <rhyme label="b" type="m">geschied</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ik kan het niet <rhyme label="a" type="f" corresp="#s9:l1">verdragen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Want ik heb te veel <rhyme label="b" type="m" corresp="#s9:l2">verdriet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Sa dogter gaat maar <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En wilt niet blyven <rhyme label="c" type="m" corresp="#s9:l5">staan</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Ik estimeer geen <rhyme label="d" type="f">Kinders</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Die haar Ouders raad <rhyme label="c" type="m" corresp="#s9:l5">versmaan</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Kan ik geen troost <rhyme label="a" type="f">bekomen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Wel het is met myn <rhyme label="b" type="m">gedaan</rhyme>;</l>
        <l id="s10:l3" met="3" type="-+">Had ik maar <rhyme label="a" type="f" corresp="#s10:l1">waargenomen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">En myn moeders raad <rhyme label="b" type="m" corresp="#s10:l2">gedaan</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Maar als het is te <rhyme label="c" type="m">laat</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Wel dan roept men om <rhyme label="c" type="m" corresp="#s10:l5">baat</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Ik zal myn kruisken <rhyme label="d" type="f">dragen</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">In verduldigheid en <rhyme label="c" type="m" corresp="#s10:l5">smaat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
