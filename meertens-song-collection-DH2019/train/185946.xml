        <l id="s1:l1" met="3" type="-+">IK weet wel wat gy <rhyme label="a" type="m">zoekt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">maar 'k zal het geenszins <rhyme label="b" type="f">waagen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Maar 'k zal het geenszins <rhyme label="b" type="f" corresp="#s1:l2">waagen</rhyme></l>
        <l id="s1:l4" met="6" type="-+">Uw' loosheit zouw ik my gewis in 't kort <rhyme label="b" type="f" corresp="#s1:l2">beklaagen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">In 't kort, in 't kort, in 't kort, in 't <rhyme label="c" type="m">kort</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">gewis in 't kort <rhyme label="b" type="f" corresp="#s1:l2">beklaagen</rhyme>.</l>
