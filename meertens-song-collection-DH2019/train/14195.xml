    <melody>Zou men zulk een koopmanschap doen</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Frans vander Straten de jongere</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">TOt Godt wil ick al mijnen <rhyme label="a" type="m">noot</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gaen claghen mijn <rhyme label="b" type="f">ellende</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Mijn siele lydt soo menigen <rhyme label="a" type="m" corresp="#s1:l1">stoot</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Den strijt en neempt ghen <rhyme label="b" type="f" corresp="#s1:l2">ende</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Hy blijft by <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Och dat ic my noyt van t'quaet <rhyme label="b" type="f" corresp="#s1:l2">enwende</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Hoe leet ist <rhyme label="c" type="m" corresp="#s1:l5">my</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hadde ick doch op Christum <rhyme label="a" type="m">gesien</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die my soo diere <rhyme label="b" type="f">cochte</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hy hadde my haest comen <rhyme label="a" type="m" corresp="#s2:l1">bevrien</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dwelck niemant en <rhyme label="b" type="f" corresp="#s2:l2">vermochte</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Dat dede <rhyme label="c" type="m">hy</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Och dat ick noyt het quaet en <rhyme label="b" type="f" corresp="#s2:l2">dochte</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Hoe leet ist <rhyme label="c" type="m" corresp="#s2:l5">my</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Met de sondighe <rhyme label="a" type="m">Magdaleen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Beclaghe ick mijn <rhyme label="b" type="f">misdaden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Och maeckt my doch uyt liefden <rhyme label="a" type="m" corresp="#s3:l1">reen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Verlost my van den <rhyme label="b" type="f" corresp="#s3:l2">quaden</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Ende my <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Van die mijn siele inwendich <rhyme label="b" type="f" corresp="#s3:l2">versmaden</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Hoe leet ist <rhyme label="c" type="m" corresp="#s3:l5">my</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het is my leet van herten <rhyme label="a" type="m">gront</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat ick u soo <rhyme label="b" type="f">mishaechde</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En ick bekent met mijnen <rhyme label="a" type="m" corresp="#s4:l1">mont</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat ick in liefden <rhyme label="b" type="f" corresp="#s4:l2">vertraechde</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Maer Heere van <rhyme label="c" type="m">dy</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Met al mijn herte dat ickt <rhyme label="b" type="f" corresp="#s4:l2">beclaechde</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Hoe leet ist <rhyme label="c" type="m" corresp="#s4:l5">my</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Deur u goetheyt en liefde <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Heere der <rhyme label="b" type="f">Heyrscharen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Soo waren vercoelt mijn sonden <rhyme label="a" type="m" corresp="#s5:l1">root</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Daer ick lach in <rhyme label="b" type="f" corresp="#s5:l2">beswaren</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Met s'Geests <rhyme label="c" type="m">Oly</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">O Heere  wilt my <rhyme label="b" type="f" corresp="#s5:l2">bewaren</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Hoe leet ist <rhyme label="c" type="m" corresp="#s5:l5">my</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">God helpt my deur zijn stercke <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En met zijn goetheyt <rhyme label="b" type="f">milde</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Veel vreucht ick in de werelt <rhyme label="a" type="m" corresp="#s6:l1">vant</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Jonckheyt track my naert <rhyme label="b" type="f" corresp="#s6:l2">wilde</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Met herten <rhyme label="c" type="m">bly</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Verleyt was ic dat niet veel en <rhyme label="b" type="f" corresp="#s6:l2">schilde</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Hoe leet ist <rhyme label="c" type="m" corresp="#s6:l5">my</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Tot God roepd'ick om hulpe <rhyme label="a" type="m">sterck</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die my uyt Egypten <rhyme label="b" type="f">haelde</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Hy dede aen my een heerlick <rhyme label="a" type="m" corresp="#s7:l1">werck</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Mijn schult voor my <rhyme label="b" type="f" corresp="#s7:l2">betaelde</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Alleene <rhyme label="c" type="m">ghy</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Maer dat ic soo van u waerheyt <rhyme label="b" type="f" corresp="#s7:l2">faelde</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Hoe leet ist <rhyme label="c" type="m" corresp="#s7:l5">my</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Princelick ghenadich <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gheeft cracht vroomer te <rhyme label="b" type="f">strijen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Want ick hope voor een <rhyme label="a" type="m" corresp="#s8:l1">slot</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Eeuwich met u te <rhyme label="b" type="f" corresp="#s8:l2">verblyen</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Dit segghen <rhyme label="c" type="m">wy</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Ick hope beter toe te <rhyme label="b" type="f" corresp="#s8:l2">sien</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Hoe leedt ist <rhyme label="c" type="m" corresp="#s8:l5">my</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
