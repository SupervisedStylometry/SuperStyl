      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Phillips Koning van Spanjen <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tot u doen ick mijn <rhyme label="b" type="f">klachten</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Soo ghy my nu geen by-stand <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Soo moet ick heel <rhyme label="b" type="f" corresp="#s1:l2">versmachten</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Jck word gequelt van een <rhyme label="c" type="m">Minnaer</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Dat heeft gheduert soo meenigh <rhyme label="c" type="m" corresp="#s1:l5">jaer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hy soeckt my te <rhyme label="b" type="f" corresp="#s1:l2">verkrachten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'s Hertogen-Bosch hout u <rhyme label="a" type="m">konstandt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En laet u niet <rhyme label="b" type="f">verleyden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Jck sal u niet laten in <rhyme label="a" type="m" corresp="#s2:l1">schant</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Maer sal mijn kans <rhyme label="b" type="f" corresp="#s2:l2">verbeyden</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Denckt hoe ghy in voorleden <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Van meenigh Minnaer sijt <rhyme label="c" type="m" corresp="#s2:l5">ghevrijdt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die mosten doch al <rhyme label="b" type="f" corresp="#s2:l2">scheyden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'Tis was den Graef van Holach <rhyme label="a" type="m">kloek</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Quam eens tot my wat <rhyme label="b" type="f">praten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Graef Maurits deed' ock een <rhyme label="a" type="m" corresp="#s3:l1">versoeck</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hy moest my Maget <rhyme label="b" type="f" corresp="#s3:l2">laten</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Maer nu komt Prins Hendrick <rhyme label="c" type="m">valjant</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Die heeft my hier den Mey <rhyme label="c" type="m" corresp="#s3:l5">geplant</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Weynigh tot mijnder <rhyme label="b" type="f" corresp="#s3:l2">baten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'s Hertogen-bosch hout u <rhyme label="a" type="m">konstant</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En wilt doch niet <rhyme label="b" type="f">versagen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Jck sal u haest'lijck doen <rhyme label="a" type="m" corresp="#s4:l1">by-stant</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En dat in korte <rhyme label="b" type="f" corresp="#s4:l2">dagen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Jck sal u helpen waer ick <rhyme label="c" type="m">kan</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Jck heb noch hondert duysent <rhyme label="c" type="m" corresp="#s4:l5">Man</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Die wil ick voor u <rhyme label="b" type="f" corresp="#s4:l2">wagen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Jck sorgh ghy hebt te langh <rhyme label="a" type="m">ghebeyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wat wilt ghy nu <rhyme label="b" type="f">beginnen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Nu den Prins sterck begraven <rhyme label="a" type="m" corresp="#s5:l1">leyt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Meynt ghy hem <rhyme label="b" type="f" corresp="#s5:l2">t'overwinnen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Al quamt ghy met u gantsche <rhyme label="c" type="m">macht</rhyme></l>
        <l id="s5:l6" met="4" type="-+">'Tis al om niet, 't en helpt geen <rhyme label="c" type="m" corresp="#s5:l5">kracht</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wilt u eerst wel <rhyme label="b" type="f" corresp="#s5:l2">versinnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">'s Hertogen-bos Brabantsche <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Tot u ben ick <rhyme label="b" type="f">genegen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Marquis Spinlo mijn trouwe <rhyme label="a" type="m" corresp="#s6:l1">knecht</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Blijft te langh onder <rhyme label="b" type="f" corresp="#s6:l2">wegen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Graef Hendrick is nu <rhyme label="c" type="m">Generael</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Hy sal de Geusen <rhyme label="c" type="m" corresp="#s6:l5">al-te-mael</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Jn korten tijd doen <rhyme label="b" type="f" corresp="#s6:l2">beven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Seght Marquis Spinola <rhyme label="a" type="m">valjant</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Waer hebt ghy dien <rhyme label="b" type="f">gelaten</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">Of is hy by den <rhyme label="a" type="m" corresp="#s7:l1">Admirant</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dat sy te saem wat <rhyme label="b" type="f" corresp="#s7:l2">praten</rhyme>?</l>
        <l id="s7:l5" met="4" type="-+">Of scheyt hy nu heel uyt den <rhyme label="c" type="m">Krijgh</rhyme>?</l>
        <l id="s7:l6" met="4" type="-+">Of hebt ghy hem een Spaensche <rhyme label="c" type="m" corresp="#s7:l5">Vijgh</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Gekoockt tot uwer <rhyme label="b" type="f" corresp="#s7:l2">baten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">'s Hertogen-bosch hout u <rhyme label="a" type="m">gestilt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Jck magh van sulx niet <rhyme label="b" type="f">hoore</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">My dunckt dat ghy Geus worden <rhyme label="a" type="m" corresp="#s8:l1">wilt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Of jeucken u de <rhyme label="b" type="f" corresp="#s8:l2">ooren</rhyme>?</l>
        <l id="s8:l5" met="4" type="-+">Of wilt ghy in u <rhyme label="c" type="m">ouderdom</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Geheel den Mantel hangen <rhyme label="c" type="m" corresp="#s8:l5">om</rhyme>?</l>
        <l id="s8:l7" met="3" type="-+">Soo ist moeyten <rhyme label="b" type="f" corresp="#s8:l2">verlooren</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Jck ben een Maeght seer out <rhyme label="a" type="m">bedaegt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Maer weet van geen <rhyme label="b" type="f">af-wijcken</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Die my 't soo dickwijls heeft <rhyme label="a" type="m" corresp="#s9:l1">gevraeght</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Die komt my nu <rhyme label="b" type="f" corresp="#s9:l2">bekijcken</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">By nacht en dagh, ja t'aller <rhyme label="c" type="m">uer</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Soo komt hy spelen voor mijn <rhyme label="c" type="m" corresp="#s9:l5">deur</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Jck sagh noyt sijns <rhyme label="b" type="f" corresp="#s9:l2">gelijcken</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Den Geus maeckt my den kop so <rhyme label="a" type="m">sol</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Mijn hart dat smelt van <rhyme label="b" type="f">binnen</rhyme>;</l>
        <l id="s10:l3" met="4" type="-+">Oldenseel, en die Stad <rhyme label="a" type="m" corresp="#s10:l1">Grol</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Most ick hem laten <rhyme label="b" type="f" corresp="#s10:l2">winnen</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Verlies ick nu den Bosch <rhyme label="c" type="m">playsant</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Soo sorgh ick voor geheel <rhyme label="c" type="m" corresp="#s10:l5">Brabant</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Wat sal ick gaen <rhyme label="b" type="f" corresp="#s10:l2">beginnen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Neemt den Keyser tot uwer <rhyme label="a" type="m">baet</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Met alle sijn <rhyme label="b" type="f">Krabaten</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">En helpt my doch in desen <rhyme label="a" type="m" corresp="#s11:l1">staet</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Of ick moet mijn maeghdom <rhyme label="b" type="f" corresp="#s11:l2">laten</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Prins Hendrick al van <rhyme label="c" type="m">Nassou</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Die bied my alle daegh sijn <rhyme label="c" type="m" corresp="#s11:l5">trou</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Hy sal my noch <rhyme label="b" type="f" corresp="#s11:l2">bepraten</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Wat helpt my al het <rhyme label="a" type="m">Krijgs-gewelt</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Van Ruyters ende <rhyme label="b" type="f">Knechten</rhyme>?</l>
        <l id="s12:l3" met="4" type="-+">Waer dat ick hoor men roept om <rhyme label="a" type="m" corresp="#s12:l1">gelt</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Of sy willen niet <rhyme label="b" type="f" corresp="#s12:l2">vechten</rhyme>;</l>
        <l id="s12:l5" met="4" type="-+">'t Kantoor is wegh, de Beurs is <rhyme label="c" type="m">leegh</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Hoe dat ick 't maeck, ten is geen <rhyme label="c" type="m" corresp="#s12:l5">deegh</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Wat kan ick veel <rhyme label="b" type="f" corresp="#s12:l2">uytrechten</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Sijn u Kantoren soo <rhyme label="a" type="m">ontstelt</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Eylaes! wat moet ick <rhyme label="b" type="f">horen</rhyme>?</l>
        <l id="s13:l3" met="4" type="-+">Spelen de Geusen met u <rhyme label="a" type="m" corresp="#s13:l1">gelt</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Soo ist met my <rhyme label="b" type="f" corresp="#s13:l2">verloren</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Soo is de kans met my <rhyme label="c" type="m">gedaen</rhyme></l>
        <l id="s13:l6" met="4" type="-+">Mijn maeghdom sal in perijckel <rhyme label="c" type="m" corresp="#s13:l5">staen</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Dat weet ick wel te <rhyme label="b" type="f" corresp="#s13:l2">vooren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Dat heeft den schalk Piet Heyn <rhyme label="a" type="m">gedaen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Wie sal ick 't anders <rhyme label="b" type="f">wijten</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Dat hy mijn Schepen swaer <rhyme label="a" type="m" corresp="#s14:l1">gelaen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Nam tot sijnder <rhyme label="b" type="f" corresp="#s14:l2">profijten</rhyme>,</l>
        <l id="s14:l5" met="4" type="-+">Met Gelt, Silver en ander <rhyme label="c" type="m">waer</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">Al leefden ick noch hondert <rhyme label="c" type="m" corresp="#s14:l5">jaer</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Het sou my altoos <rhyme label="b" type="f" corresp="#s14:l2">spijten</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Al is dit u een eeuwigh <rhyme label="a" type="m">vrock</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Hoe dat 't u heeft <rhyme label="b" type="f">gespeten</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Men seyt dat een wan-gonnen <rhyme label="a" type="m" corresp="#s15:l1">brock</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Somtijts wel wort <rhyme label="b" type="f" corresp="#s15:l2">gegeten</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">De Hollantsche Bataviers staen <rhyme label="c" type="m">ree</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">Sy spelen meester op de <rhyme label="c" type="m" corresp="#s15:l5">See</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Dat moet ghy selfs wel <rhyme label="b" type="f" corresp="#s15:l2">weten</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Oorlof alle Lief-hebbers <rhyme label="a" type="m">saen</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Die 't Vaderlant <rhyme label="b" type="f">beminnen</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">De twee Geboden sijn <rhyme label="a" type="m" corresp="#s16:l1">gegaen</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">'T derde sal nu <rhyme label="b" type="f" corresp="#s16:l2">beginnen</rhyme>:</l>
        <l id="s16:l5" met="4" type="-+">My dunckt de Bruiyloft wort <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s16:l6" met="4" type="-+">Jck wensch de Minnaee met dees <rhyme label="c" type="m" corresp="#s16:l5">Meyt</rhyme></l>
        <l id="s16:l7" met="3" type="-+">Geluck uyt goeder <rhyme label="b" type="f" corresp="#s16:l2">minnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
