<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183641</id>
    <date>
      <year-start>1739</year-start>
      <year-end>1739</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatst kwam Cupido</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">CUpido heeft myn hert <rhyme label="a" type="f">doorschooten</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Door zyn pyltjes van de <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik voel de <rhyme label="a" type="f" corresp="#s1:l1">minnelooten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Blaken ten boezem <rhyme label="b" type="m" corresp="#s1:l2">in</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Een Maget <rhyme label="a" type="f" corresp="#s1:l1">uitgesprooten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Staat 'er vast in myn <rhyme label="b" type="m" corresp="#s1:l2">zin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ik heb 'er zo lang gekwelt myn <rhyme label="a" type="f">zinnen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Om u o schoone jonge <rhyme label="b" type="m">blom</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die ik opregt <rhyme label="a" type="f" corresp="#s2:l1">beminnen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Kom dog myn Liefje <rhyme label="b" type="m" corresp="#s2:l2">kom</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Laat ik u hert <rhyme label="a" type="f" corresp="#s2:l1">verwinnen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En zyn u <rhyme label="b" type="m" corresp="#s2:l2">Bruidegom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want u zoet wezen en lieve <rhyme label="a" type="f">lonken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Van uw bruin oogen en zoet <rhyme label="b" type="m">gezigt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maakt myn van liefde <rhyme label="a" type="f" corresp="#s3:l1">dronken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En ik toon u myn <rhyme label="b" type="m" corresp="#s3:l2">pligt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Laat ik u hert <rhyme label="a" type="f" corresp="#s3:l1">ontvonken</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zo word myn pyn <rhyme label="b" type="m" corresp="#s3:l2">verligt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Kan niet bekooren myn <rhyme label="a" type="f">gemoet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Als gy myn had <rhyme label="b" type="m">gevreden</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En uwen lust <rhyme label="a" type="f" corresp="#s4:l1">gevoed</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Zoud gy myn gaan <rhyme label="b" type="m" corresp="#s4:l2">vertreden</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Daarom vertrekt met <rhyme label="a" type="f" corresp="#s4:l1">spoet</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">Op uwe lipjes Liefste <rhyme label="b" type="m" corresp="#s4:l2">myn</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">En van u kaakjes <rhyme label="a" type="f">plukken</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Roosjes als <rhyme label="b" type="m">Ambrosyn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dan zou die lieve <rhyme label="a" type="f" corresp="#s5:l1">nukken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Zoeter als Hooning <rhyme label="b" type="m" corresp="#s5:l2">zyn</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">En zwygt met al u malle <rhyme label="a" type="f" corresp="#s5:l1">praat</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ik wil de Min <rhyme label="b" type="m" corresp="#s5:l2">verzaken</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">En hou myn Maagden <rhyme label="a" type="f">staat</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Gy kent myn niet <rhyme label="b" type="m">vermaken</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Want ik u vlyen <rhyme label="a" type="f" corresp="#s6:l1">haat</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">En doed myn aan veel smaat en <rhyme label="b" type="m" corresp="#s6:l2">hoon</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Evenwel zal ik jou <rhyme label="a" type="f" corresp="#s6:l1">minnen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Want ziet gy spant de <rhyme label="b" type="m" corresp="#s6:l2">kroon</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Boven de Veld <rhyme label="a" type="f">Godinnen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Diaan en Venus <rhyme label="b" type="m">schoon</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Hebt gy verwonnen myn jonk <rhyme label="a" type="f" corresp="#s7:l1">hert</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ik gevoel myn van <rhyme label="b" type="m" corresp="#s7:l2">binnen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Mee door de min <rhyme label="a" type="f" corresp="#s7:l1">verwert</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Stelt nu gerust jou <rhyme label="b" type="m" corresp="#s7:l2">zinnen</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Gy zyt verlost van <rhyme label="a" type="f">smert</rhyme>.</l>
        <l id="s8:l2" met="4" type="-+">Ik ben verlost van <rhyme label="b" type="m">Minnepyn</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Nu dat ik zal <rhyme label="a" type="f" corresp="#s8:l1">vernagten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">By d'alderliefste van <rhyme label="b" type="m" corresp="#s8:l2">myn</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Groot zyn de <rhyme label="a" type="f" corresp="#s8:l1">Minnekragten</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Door Cupidootje <rhyme label="b" type="m" corresp="#s8:l2">klein</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
