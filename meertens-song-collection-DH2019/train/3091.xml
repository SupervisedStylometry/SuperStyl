    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Ik zucht ik klaag ik ween</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk weter een schoone <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Veel schoonder als Bloempjes en <rhyme label="b" type="f">Roosen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die op haer Kaeckjes <rhyme label="a" type="m" corresp="#s1:l1">draeght</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Het Bloeysel van <rhyme label="b" type="f" corresp="#s1:l2">Aprikoosen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ick meen ghy haer Aurora <rhyme label="a" type="m" corresp="#s1:l1">saeght</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ghy liet u purp'rich <rhyme label="b" type="f" corresp="#s1:l2">bloosen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ach soete Nimph' <rhyme label="a" type="m">Astree</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">U Naempje wijst my u te <rhyme label="b" type="f">strelen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ay! of ick 't eensjes <rhyme label="a" type="m" corresp="#s2:l1">dee</rhyme>?</l>
        <l id="s2:l4" met="3" type="-+">Maer 't mochter mijn Liefje <rhyme label="b" type="f" corresp="#s2:l2">vervelen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Om harent wil soo sou <rhyme label="a" type="m" corresp="#s2:l1">Orpee</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Noch wel een deuntje <rhyme label="b" type="f" corresp="#s2:l2">spelen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn schoonste <rhyme label="a" type="m">Paragon</rhyme></l>
        <l id="s3:l2" met="3" type="-+">U Oogjes gelonck ken <rhyme label="b" type="f">dwingen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">De Straeltjes van de <rhyme label="a" type="m" corresp="#s3:l1">Son</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Al doense de Wolckjes <rhyme label="b" type="f" corresp="#s3:l2">dringen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Om u blont Hayr sou <rhyme label="a" type="m" corresp="#s3:l1">Arion</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wel duysent veersjes <rhyme label="b" type="f" corresp="#s3:l2">singen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Haer voorhooft als een <rhyme label="a" type="m">dagh</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Doet Lely en Roos <rhyme label="b" type="f">ontluycken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Haer lieff'lick <rhyme label="a" type="m" corresp="#s4:l1">Tongs-geslach</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die weetse met maet te <rhyme label="b" type="f" corresp="#s4:l2">gebruycken</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Ick zweer, dat haer Diana <rhyme label="a" type="m" corresp="#s4:l1">sagh</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sy liet haer hoorens <rhyme label="b" type="f" corresp="#s4:l2">duycken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Op yder Memmetje <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Een bloeyende Kersje <rhyme label="b" type="f">teder</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En als haer asempje <rhyme label="a" type="m" corresp="#s5:l1">gaet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Soo zwoegen sy op en <rhyme label="b" type="f" corresp="#s5:l2">neder</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ick wed Silvanus die <rhyme label="a" type="m" corresp="#s5:l1">verlaet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Om u sijn schoonen <rhyme label="b" type="f" corresp="#s5:l2">Ceder</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Het aenschijn toont het <rhyme label="a" type="m">uyt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Haer poesele blancke <rhyme label="b" type="f">Leden</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Haer suchjes dat <rhyme label="a" type="m" corresp="#s6:l1">beduyt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Tot my haer <rhyme label="b" type="f" corresp="#s6:l2">genegentheden</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Ach Venis! mocht ick met mijn <rhyme label="a" type="m" corresp="#s6:l1">Bruydt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Eens in u lusjes <rhyme label="b" type="f" corresp="#s6:l2">treden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ick namse by der <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ick leydense aen u <rhyme label="b" type="f">Tempel</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En knielde met den <rhyme label="a" type="m" corresp="#s7:l1">brandt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Voor u Godinnetjes <rhyme label="b" type="f" corresp="#s7:l2">Drempel</rhyme></l>
        <l id="s7:l5" met="4" type="-+">En knoopten soo der Minnen <rhyme label="a" type="m" corresp="#s7:l1">bant</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Tot yder eens <rhyme label="b" type="f" corresp="#s7:l2">exempel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
