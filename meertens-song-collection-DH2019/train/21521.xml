    <melody>O triomfante vrouw</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Heere der <rhyme label="a" type="f">heyrscharen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Strijt teghen mijn <rhyme label="b" type="f">vianden</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Die my door druck <rhyme label="a" type="f" corresp="#s1:l1">beswaren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En willen my met <rhyme label="b" type="f" corresp="#s1:l2">schanden</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Uerdrincken oft <rhyme label="b" type="f" corresp="#s1:l2">verbranden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Al om dijn Goddelick <rhyme label="c" type="m">woort</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Lost my wt haren <rhyme label="b" type="f" corresp="#s1:l2">tanden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gheeft mynen gheest <rhyme label="c" type="m" corresp="#s1:l6">confoort</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Met sterckheyt en <rhyme label="c" type="m" corresp="#s1:l6">accoort</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Sijt mijn beschermer <rhyme label="d" type="m">stout</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Steect wt v handen <rhyme label="c" type="m" corresp="#s1:l6">voort</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Mijn siele op v <rhyme label="d" type="m" corresp="#s1:l10">betrout</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Aenhoort v dienaer <rhyme label="a" type="f">cleene</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En spreect tot my al <rhyme label="b" type="f">voren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ick ben v God <rhyme label="a" type="f" corresp="#s2:l1">alleene</rhyme></l>
        <l id="s2:l4" met="3" type="-+">U salicheyt <rhyme label="b" type="f" corresp="#s2:l2">verkoren</rhyme></l>
        <l id="s2:l5" met="3" type="-+">So mach ick troost <rhyme label="b" type="f" corresp="#s2:l2">oorboren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En vertreden haer boos <rhyme label="c" type="m">fenijn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Die my willen <rhyme label="b" type="f" corresp="#s2:l2">versmoren</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Al sonder cause of <rhyme label="c" type="m" corresp="#s2:l6">schijn</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Want haer bloetghierich <rhyme label="c" type="m" corresp="#s2:l6">fijn</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Strijt teghen mijnen <rhyme label="d" type="m">gheest</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Dies helpt my God <rhyme label="c" type="m" corresp="#s2:l6">diuijn</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Lost my wt haer <rhyme label="d" type="m" corresp="#s2:l10">tempeest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wee v ghy <rhyme label="a" type="f">ypocrijten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die v vooruaders <rhyme label="b" type="f">maten</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ueruult tot ws <rhyme label="a" type="f" corresp="#s3:l1">verwijten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In steden en in <rhyme label="b" type="f" corresp="#s3:l2">straten</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Had ick doch niet <rhyme label="b" type="f" corresp="#s3:l2">ghelaten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Al mijn boos weghen <rhyme label="c" type="m">swaer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">So haddy my mogen <rhyme label="b" type="f" corresp="#s3:l2">haten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En veruolghen hier en <rhyme label="c" type="m" corresp="#s3:l6">daer</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Maer nv so tooghdy <rhyme label="c" type="m" corresp="#s3:l6">claer</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Dynen gheueysden <rhyme label="d" type="m">nijt</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Aen Gods kinders <rhyme label="c" type="m" corresp="#s3:l6">voorwaer</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Dat ghy wt den duyuel <rhyme label="d" type="m" corresp="#s3:l10">zijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wildy tquaet <rhyme label="a" type="f">destrueren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">En beletten tsondich <rhyme label="b" type="f">sneuen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">So hadt mijn quaet <rhyme label="a" type="f" corresp="#s4:l1">vseren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hier voormaels dan <rhyme label="b" type="f" corresp="#s4:l2">verdreuen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hier tooghdy int <rhyme label="b" type="f" corresp="#s4:l2">aencleuen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat ghy de boosheyt <rhyme label="c" type="m">gheert</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Waerdy wt God <rhyme label="b" type="f" corresp="#s4:l2">verheuen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Ghy hadt my doen <rhyme label="c" type="m" corresp="#s4:l6">ghedeert</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Want twas al twist en <rhyme label="c" type="m" corresp="#s4:l6">sweert</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Dat ghy doen van my <rhyme label="d" type="m">saecht</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Maer om Gods woort <rhyme label="c" type="m" corresp="#s4:l6">expeert</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Moet ick nv zijn <rhyme label="d" type="m" corresp="#s4:l10">veriaecht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wanneer ick <rhyme label="a" type="f">triumpheerde</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Solaes en vruecht <rhyme label="b" type="f">hantierde</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En nachten lanck <rhyme label="a" type="f" corresp="#s5:l1">boeleerde</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En valsch bedroch <rhyme label="b" type="f" corresp="#s5:l2">versierde</rhyme></l>
        <l id="s5:l5" met="3" type="-+">En als ick oock <rhyme label="b" type="f" corresp="#s5:l2">logierde</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In alle dronckenschap <rhyme label="c" type="m">quaet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En mynen naesten <rhyme label="b" type="f" corresp="#s5:l2">schoffierde</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Doen leefde ick <rhyme label="c" type="m" corresp="#s5:l6">ongheschaet</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Maer nv als ick den <rhyme label="c" type="m" corresp="#s5:l6">raet</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Der Schriften hebbe <rhyme label="d" type="m">ontfaen</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Ben ick behaet <rhyme label="c" type="m" corresp="#s5:l6">versmaet</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Uan Babels dienaers <rhyme label="d" type="m" corresp="#s5:l10">saen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wanneer ick tuyschte en <rhyme label="a" type="f">speelde</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En street en keef <rhyme label="b" type="f">warachtich</rhyme></l>
        <l id="s6:l3" met="3" type="-+">En my dwerck Gods <rhyme label="a" type="f" corresp="#s6:l1">verueelde</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En swoer Gods crachten <rhyme label="b" type="f" corresp="#s6:l2">crachtich</rhyme></l>
        <l id="s6:l5" met="3" type="-+">En als ick tquaet <rhyme label="b" type="f" corresp="#s6:l2">voordachtich</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Bedreef seer <rhyme label="c" type="m">menighertier</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Was ick ter weerelt <rhyme label="b" type="f" corresp="#s6:l2">machtich</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Ghehouden een gheestich <rhyme label="c" type="m" corresp="#s6:l6">dier</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Maer als ick sduyuels <rhyme label="c" type="m" corresp="#s6:l6">ghier</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Hebbe ghelaten <rhyme label="d" type="m">wel</rhyme></l>
        <l id="s6:l11" met="3" type="+-">Ben ick veruolcht <rhyme label="c" type="m" corresp="#s6:l6">schier</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Uan dypocrijten <rhyme label="d" type="m" corresp="#s6:l10">snel</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Prince vliet dit <rhyme label="a" type="f">gheslachte</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Sy zijn ergher dan <rhyme label="b" type="f">slanghen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Want sy al tvolck met <rhyme label="a" type="f" corresp="#s7:l1">crachte</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Leyden in sduyuels <rhyme label="b" type="f" corresp="#s7:l2">ganghen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">De boom in zijn <rhyme label="b" type="f" corresp="#s7:l2">aenuanghen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Bekentmen aen de <rhyme label="c" type="m">vrucht</rhyme></l>
        <l id="s7:l7" met="3" type="-+">En wiltse niet <rhyme label="b" type="f" corresp="#s7:l2">aenhanghen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Sy doen de siele <rhyme label="c" type="m" corresp="#s7:l6">sucht</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Adieu nv neem ick <rhyme label="c" type="m" corresp="#s7:l6">vlucht</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Bidt voor my alle <rhyme label="d" type="m">ghemeen</rhyme></l>
        <l id="s7:l11" met="3" type="+-">Dat my vry sonder <rhyme label="c" type="m" corresp="#s7:l6">ducht</rhyme></l>
        <l id="s7:l12" met="3" type="-+">Den Heere wille <rhyme label="d" type="m" corresp="#s7:l10">gheleen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
