        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Que la débauche est délectable</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Guilielmus Bolognino</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Willibrorde, in ons <rhyme label="a" type="f">landen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Die hebt doen schijnen Christi <rhyme label="b" type="m">licht</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Door u ghecomen in't <rhyme label="b" type="m" corresp="#s1:l2">ghesicht</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Het liefdes vier daer in doen <rhyme label="a" type="f" corresp="#s1:l1">branden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Staet ons doch by uyt 's hemels <rhyme label="c" type="m">hof</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Bereet te singhen uwen <rhyme label="c" type="m" corresp="#s1:l5">lof</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een Maen die groeide is <rhyme label="a" type="f">ghecomen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">In u bevruchte moeders <rhyme label="b" type="m">mondt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Waer door met reden men <rhyme label="b" type="m" corresp="#s2:l2">verstondt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat door u schijnsel wegh <rhyme label="a" type="f" corresp="#s2:l1">ghenomen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Eens worden sou de <rhyme label="c" type="m">duysterheydt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Daer toe van Godt den Heer <rhyme label="c" type="m" corresp="#s2:l5">bereyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Uytrecht voor d' achste eeuw u <rhyme label="a" type="f">bouwen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Een kerck tot sinte Martens <rhyme label="b" type="m">eer</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Ghesien heeft, die, verblint nu <rhyme label="b" type="m" corresp="#s3:l2">seer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Van Santen eer niet en wilt <rhyme label="a" type="f" corresp="#s3:l1">houwen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Noch kent den Bischops stoel, <rhyme label="c" type="m">gheset</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Daer in van u, noch Bischops <rhyme label="c" type="m" corresp="#s3:l5">wet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den Vorst van Antwerpen, <rhyme label="a" type="f">bewoghen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Door uwer wonder deughden <rhyme label="b" type="m">maer</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Heeft ievrich u doen comen <rhyme label="b" type="m" corresp="#s4:l2">daer</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En heeft ghestelt in u <rhyme label="a" type="f" corresp="#s4:l1">vermoghen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Een kerck ghestaen in d' oude <rhyme label="c" type="m">Borght</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Heeft u van 't tijtelijck <rhyme label="c" type="m" corresp="#s4:l5">besorght</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daer hebdy wonder <rhyme label="a" type="f">uytgheschenen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">In iever; en met alle <rhyme label="b" type="m">deught</rhyme></l>
        <l id="s5:l3" met="4" type="-+">De Stadt ghesticht, in Godt <rhyme label="b" type="m" corresp="#s5:l2">verheught</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">De dolingh en de sond' <rhyme label="a" type="f" corresp="#s5:l1">verdwenen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Den hemel wasser in <rhyme label="c" type="m">verblijt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Den helschen vyandt borst van <rhyme label="c" type="m" corresp="#s5:l5">spijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">G'hebt wonder wercken oock <rhyme label="a" type="f">gheploghen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">G' hebt meer als eens vermeert den <rhyme label="b" type="m">wijn</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Waer me ghelaeft de arme <rhyme label="b" type="m" corresp="#s6:l2">zijn</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Soo groot by Gode was u <rhyme label="a" type="f" corresp="#s6:l1">vermoghen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Daer ghy noch meer nu by <rhyme label="c" type="m">vermeught</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">By hem ghestelt in hoochste <rhyme label="c" type="m" corresp="#s6:l5">vreught</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Uyt soute aerd' hebd' oock doen <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">In noodt een soete claer <rhyme label="b" type="m">fonteyn</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Waer van Godt loofden in 't <rhyme label="b" type="m" corresp="#s7:l2">ghemeyn</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Die droncken daer van, me <rhyme label="a" type="f" corresp="#s7:l1">ghecomen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Ghereyst aen den Zee-cost met <rhyme label="c" type="m">u</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Seer dorstich van den ganck en <rhyme label="c" type="m" corresp="#s7:l5">mu</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Helpt nu u Stadt met u <rhyme label="a" type="f">ghebeden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Waer den Apostel ghy van <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Van Godt door u <rhyme label="b" type="m" corresp="#s8:l2">ghebenedijt</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Brenght haer weer tot de oude <rhyme label="a" type="f" corresp="#s8:l1">seden</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En tot de ouw' <rhyme label="c" type="m">Godt-vruchticheydt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Daer sy van u toe is <rhyme label="c" type="m" corresp="#s8:l5">beleyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
