<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>33533</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <tags>
      <tag standard-name="verhalend lied" translation="narrative song">verhalend lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Es ist das Heil uns kommen her</melody>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Rumoldus Batavus</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ghy hebt O Heer als een wijs <rhyme label="a" type="m">man</rhyme></l>
        <l id="s1:l2" met="3" type="-+">V huys op een steen doen <rhyme label="b" type="f">bouwen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghy wist datter bloet sout comen <rhyme label="a" type="m" corresp="#s1:l1">an</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Opt zant waert niet te <rhyme label="b" type="f" corresp="#s1:l2">betrouwen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Want het was v zeer wel <rhyme label="c" type="m">bekent</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Datter soudt wayen menich <rhyme label="c" type="m" corresp="#s1:l5">wint</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die daer op blasen <rhyme label="b" type="f" corresp="#s1:l2">souwen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En om dat ghy dit hebt <rhyme label="a" type="m">gesien</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Soo hebt ghijt zo doen <rhyme label="b" type="f">stellen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Wel Vast op eenen herden <rhyme label="a" type="m" corresp="#s2:l1">stien</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Tegens alle poorten der <rhyme label="b" type="f" corresp="#s2:l2">hellen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Al bedrieueb desen groot <rhyme label="c" type="m">ghetier</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Door galgen, sweerden, ende <rhyme label="c" type="m" corresp="#s2:l5">vier</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Haer seluer zy meest <rhyme label="b" type="f" corresp="#s2:l2">quellen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Christus ons Heer die heeft <rhyme label="a" type="m">geseyt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En dese en zal niet <rhyme label="b" type="f">lieghen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Hy is almachtich en die <rhyme label="a" type="m" corresp="#s3:l1">waerheyt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dus zal hy ons niet <rhyme label="b" type="f" corresp="#s3:l2">bedriegen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Dese heeft opten steen die Petrus <rhyme label="c" type="m">hout</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Sijn nieuwe kercke vast <rhyme label="c" type="m" corresp="#s3:l5">gebout</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Die geen onderganck mach <rhyme label="b" type="f" corresp="#s3:l2">criegen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dit huys heeft nu dus lanck <rhyme label="a" type="m">ghestaen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">By sesthien-hondert <rhyme label="b" type="f">iaren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En ten is noch noyt <rhyme label="a" type="m" corresp="#s4:l1">vergaen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hoe veel stormen datter <rhyme label="b" type="f" corresp="#s4:l2">waren</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Het heeft verdraegen zo menigen <rhyme label="c" type="m">slach</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Soomen byden ouden lesen <rhyme label="c" type="m" corresp="#s4:l5">mach</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Noch heeft Godt het doen <rhyme label="b" type="f" corresp="#s4:l2">bewaeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die heydenen hebben zeer <rhyme label="a" type="m">gewoyt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Al met Gods volck te <rhyme label="b" type="f">dooden</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Want het heeft haer zeer <rhyme label="a" type="m" corresp="#s5:l1">gemoyt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Datmen brack haren <rhyme label="b" type="f" corresp="#s5:l2">afgoden</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Die Ioden waren oock seer <rhyme label="c" type="m">gram</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Mits datmen haer den weth <rhyme label="c" type="m" corresp="#s5:l5">afnam</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Die haer van Godt was <rhyme label="b" type="f" corresp="#s5:l2">gheboden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hier na zoo quamen die ketters <rhyme label="a" type="m">voort</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Nu d’een, ende dan den <rhyme label="b" type="f">ander</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Die hoe wel sy zelfs waeren <rhyme label="a" type="m" corresp="#s6:l1">discoort</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Accordeerden met <rhyme label="b" type="f" corresp="#s6:l2">malcander</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Om te vechten tegens Gods <rhyme label="c" type="m">bruyt</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Daer zy al ghecomen waren <rhyme label="c" type="m" corresp="#s6:l5">wt</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Dit was immers groote <rhyme label="b" type="f" corresp="#s6:l2">schanden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Al heeft haer rijck somtyts <rhyme label="a" type="m">gestaen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Seer veele ende langhe <rhyme label="b" type="f">iaren</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Dat is nochtans nu al <rhyme label="a" type="m" corresp="#s7:l1">vergaen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Mits het helsche poorten <rhyme label="b" type="f" corresp="#s7:l2">waren</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Al hadden zydie macht tot haren <rhyme label="c" type="m">hant</rhyme></l>
        <l id="s7:l6" met="4" type="-+">En waren verstroyt doort gansche <rhyme label="c" type="m" corresp="#s7:l5">lant</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Soo zyn zy nu al <rhyme label="b" type="f" corresp="#s7:l2">veruaren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Sy bruyckten haer macht met alle <rhyme label="a" type="m">gewelt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Om Gods tempel te <rhyme label="b" type="f">vernielen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Maer sy heeft als noch behouden het <rhyme label="a" type="m" corresp="#s8:l1">velt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Hoe sterck zy daer op <rhyme label="b" type="f" corresp="#s8:l2">vielen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Haer hooft dat sy altoos bouen <rhyme label="c" type="m">steeckt</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Als een stadt op een berch, zo Christus <rhyme label="c" type="m" corresp="#s8:l5">spreeckt</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Wiens ghesicht men altoos <rhyme label="b" type="f" corresp="#s8:l2">behielen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hoe wel daer nae veel disputen <rhyme label="a" type="m">gheschien</rhyme>.</l>
        <l id="s9:l2" met="3" type="-+">Om te soecken Christus <rhyme label="b" type="f">kercke</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat is claer, en goet om <rhyme label="a" type="m" corresp="#s9:l1">zien</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Wilt maer daer wel op <rhyme label="b" type="f" corresp="#s9:l2">mercken</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Daer den wint heeft tegens <rhyme label="c" type="m">gheblaest</rhyme></l>
        <l id="s9:l6" met="4" type="-+">En die groote goluen op hebben <rhyme label="c" type="m" corresp="#s9:l5">gheraest</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Dit is Christus ghebouw wel <rhyme label="b" type="f" corresp="#s9:l2">stercke</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Die Catholycke kercke die is zoo <rhyme label="a" type="m">out</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Soo alle ghetuyghen <rhyme label="b" type="f">belyden</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Dese heeft sesthienhondert iaren <rhyme label="a" type="m" corresp="#s10:l1">stout</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Wel dapper moeten <rhyme label="b" type="f" corresp="#s10:l2">strijden</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Tegen heydens, Iodts, en ketters <rhyme label="c" type="m">gewelt</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Daer zy haer vroom heeft tegens <rhyme label="c" type="m" corresp="#s10:l5">ghestelt</rhyme></l>
        <l id="s10:l7" met="3" type="-+">So wy zien oock by dese <rhyme label="b" type="f" corresp="#s10:l2">tijden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">O mensch wilt v ooghen maer <rhyme label="a" type="m">openslaen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Laet daer wt vallen die <rhyme label="b" type="f">schellen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">En ziet wat kercke daer heeft <rhyme label="a" type="m" corresp="#s11:l1">gestaen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Teghens alle poorten der <rhyme label="b" type="f" corresp="#s11:l2">hellen</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Als alleen die oude Roomsche <rhyme label="c" type="m">ziet</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Want desen nieuwen vint ghy <rhyme label="c" type="m" corresp="#s11:l5">niet</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Waerom wort ghy haer <rhyme label="b" type="f" corresp="#s11:l2">ghesellen</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Christus heeft den zynen voor <rhyme label="a" type="m">gheseyt</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Sy zouden steruen ende <rhyme label="b" type="f">lyden</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">In Aoicalipst wort het wt <rhyme label="a" type="m" corresp="#s12:l1">gheleyt</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Hoemen Gods volck zal <rhyme label="b" type="f" corresp="#s12:l2">bestrijden</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Maer dat zal dueren een corten <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Want Godt wil sulck vervolch <rhyme label="c" type="m" corresp="#s12:l5">niet</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Dat altoos dit sottde <rhyme label="b" type="f" corresp="#s12:l2">geschieden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Den vrede heeft ons Christus <rhyme label="a" type="m">ghebracht</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Als een Herder doet zyn <rhyme label="b" type="f">schapen</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Hy heeft op ons mueren doen stellen <rhyme label="a" type="m" corresp="#s13:l1">wacht</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Die nimmermeer zullen <rhyme label="b" type="f" corresp="#s13:l2">slaepen</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Niet alleene zyn Engelen <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s13:l6" met="4" type="-+">Maer oock herdersende leeraers <rhyme label="c" type="m" corresp="#s13:l5">vroet</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Om ghecleet met Godes <rhyme label="b" type="f" corresp="#s13:l2">wapen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Hy is ons Coninck een verwinder <rhyme label="a" type="m">stout</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Syn bruit zijn wy al te <rhyme label="b" type="f">samen</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Hy heeft ons inder eewicheyt <rhyme label="a" type="m" corresp="#s14:l1">getrout</rhyme></l>
        <l id="s14:l4" met="3" type="-+">En zal zijn lief niet <rhyme label="b" type="f" corresp="#s14:l2">beschamen</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Alsoo dat hy haer soude <rhyme label="c" type="m">afgaen</rhyme></l>
        <l id="s14:l6" met="4" type="-+">Mits hy heeft haer hant-trou <rhyme label="c" type="m" corresp="#s14:l5">gedaen</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Och sal sy met gheen ander <rhyme label="b" type="f" corresp="#s14:l2">versamen</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Hy is ons hooft, wy zijnt <rhyme label="a" type="m">lichaem</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Dus cunnen zy gheensins <rhyme label="b" type="f">scheiden</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Deen is sonder d’ander <rhyme label="a" type="m" corresp="#s15:l1">bequaem</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Om een goeden wandel te <rhyme label="b" type="f" corresp="#s15:l2">leyden</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">Want het hooft is aen het lichaem <rhyme label="c" type="m">vast</rhyme></l>
        <l id="s15:l6" met="4" type="-+">In het lichaem wt het hooft al <rhyme label="c" type="m" corresp="#s15:l5">wast</rhyme></l>
        <l id="s15:l7" met="3" type="-+">Om Godt veel leden te <rhyme label="b" type="f" corresp="#s15:l2">bereyden</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Desen Gheest die dit lichaem heeft <rhyme label="a" type="m">ontfaen</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Die en zal daer wt niet <rhyme label="b" type="f">trecken</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Maer sal van deen int ander <rhyme label="a" type="m" corresp="#s16:l1">gaen</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Hem tot het wterste wt <rhyme label="b" type="f" corresp="#s16:l2">strecken</rhyme>,</l>
        <l id="s16:l5" met="4" type="-+">Van het eene, tot het ander <rhyme label="c" type="m">gheslacht</rhyme></l>
        <l id="s16:l6" met="4" type="-+">Ghelyck altoos den dach en <rhyme label="c" type="m" corresp="#s16:l5">nacht</rhyme></l>
        <l id="s16:l7" met="3" type="-+">Die werelt sullen <rhyme label="b" type="f" corresp="#s16:l2">bedecken</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Dus zal oock staen eewich Gods <rhyme label="a" type="m">huys</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Als een calom sonder <rhyme label="b" type="f">schromen</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Teghens al het helsche <rhyme label="a" type="m" corresp="#s17:l1">ghespuys</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Daer die waerheyt wt moet <rhyme label="b" type="f" corresp="#s17:l2">comen</rhyme></l>
        <l id="s17:l5" met="4" type="-+">Want dit Ierusalem wort <rhyme label="c" type="m">verlicht</rhyme></l>
        <l id="s17:l6" met="4" type="-+">Daermen alleene die waerheyt <rhyme label="c" type="m" corresp="#s17:l5">sicht</rhyme></l>
        <l id="s17:l7" met="3" type="-+">En Gods stemme heeft <rhyme label="b" type="f" corresp="#s17:l2">vernomen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Christus heeft voor ons den doot <rhyme label="a" type="m">gheleen</rhyme></l>
        <l id="s18:l2" met="3" type="-+">En is ten Hemel <rhyme label="b" type="f">gheuaren</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Sijn Vader heeft hy voor ons <rhyme label="a" type="m" corresp="#s18:l1">ghebeen</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Dat hy ons toch wilde <rhyme label="b" type="f" corresp="#s18:l2">bewaeren</rhyme></l>
        <l id="s18:l5" met="4" type="-+">Ende zijn ghebedt is altoos <rhyme label="c" type="m">verhoort</rhyme></l>
        <l id="s18:l6" met="4" type="-+">Hy wil dat die vruchte soude wassen <rhyme label="c" type="m" corresp="#s18:l5">voort</rhyme></l>
        <l id="s18:l7" met="3" type="-+">Die zijn dienaers ginghen <rhyme label="b" type="f" corresp="#s18:l2">baeten</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Wy zijn niet van v vrucht <rhyme label="a" type="m">berooft</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Dat ghy tarwen cornken doet <rhyme label="b" type="f">draghen</rhyme></l>
        <l id="s19:l3" met="4" type="-+">Mits ghy hebt ons <rhyme label="a" type="m" corresp="#s19:l1">belooft</rhyme></l>
        <l id="s19:l4" met="3" type="-+">By ons te zyn alle <rhyme label="b" type="f" corresp="#s19:l2">daghen</rhyme></l>
        <l id="s19:l5" met="4" type="-+">Ende dat tot des werelts <rhyme label="c" type="m">endt</rhyme></l>
        <l id="s19:l6" met="4" type="-+">Want voor gheen wesen zyn wy <rhyme label="c" type="m" corresp="#s19:l5">bekent</rhyme></l>
        <l id="s19:l7" met="3" type="-+">Waerom wy noch niet <rhyme label="b" type="f" corresp="#s19:l2">versaeghen</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Alsten wy nu zeer veel op <rhyme label="a" type="m">staen</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Die daer vechten van alle <rhyme label="b" type="f">sijden</rhyme></l>
        <l id="s20:l3" met="4" type="-+">Om v kerck te doen onder <rhyme label="a" type="m" corresp="#s20:l1">gaen</rhyme></l>
        <l id="s20:l4" met="3" type="-+">D welck wy om ons sonder <rhyme label="b" type="f" corresp="#s20:l2">lijden</rhyme></l>
        <l id="s20:l5" met="4" type="-+">Wy betrouwen op uwe <rhyme label="c" type="m">bermherticheyt</rhyme></l>
        <l id="s20:l6" met="4" type="-+">Die ghy ons vast hebt toe <rhyme label="c" type="m" corresp="#s20:l5">gheseyt</rhyme></l>
        <l id="s20:l7" met="3" type="-+">En beuonden tot allen <rhyme label="b" type="f" corresp="#s20:l2">tijden</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Ick bidde v menschen wie ghy <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Wilt v salicheyt wel <rhyme label="b" type="f">betrachten</rhyme></l>
        <l id="s21:l3" met="4" type="-+">Weest zoo bitter op Christus leden <rhyme label="a" type="m" corresp="#s21:l1">niet</rhyme></l>
        <l id="s21:l4" met="3" type="-+">Die hy dus heeft doen <rhyme label="b" type="f" corresp="#s21:l2">bewachten</rhyme></l>
        <l id="s21:l5" met="4" type="-+">Maer buycht v verstand een weinich <rhyme label="c" type="m">neer</rhyme></l>
        <l id="s21:l6" met="4" type="-+">En aensiet die belofte van Godt den <rhyme label="c" type="m" corresp="#s21:l5">heer</rhyme></l>
        <l id="s21:l7" met="3" type="-+">Ghy sult zijn bruyt niet <rhyme label="b" type="f" corresp="#s21:l2">verachten</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Alleen die Catholicke <rhyme label="a" type="m">kerck</rhyme>,</l>
        <l id="s22:l2" met="3" type="-+">Heeft ons langhe claer <rhyme label="b" type="f">ghestanden</rhyme></l>
        <l id="s22:l3" met="4" type="-+">Als een doel midden in het <rhyme label="a" type="m" corresp="#s22:l1">perck</rhyme></l>
        <l id="s22:l4" met="3" type="-+">Wel beuochten van haer <rhyme label="b" type="f" corresp="#s22:l2">vyanden</rhyme></l>
        <l id="s22:l5" met="4" type="-+">Haer trompet heeft zy altoos <rhyme label="c" type="m">geblaest</rhyme></l>
        <l id="s22:l6" met="4" type="-+">Hoe wel den vyant heeft <rhyme label="c" type="m" corresp="#s22:l5">gheraest</rhyme></l>
        <l id="s22:l7" met="3" type="-+">Door alle heydensche <rhyme label="b" type="f" corresp="#s22:l2">landen</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Paulus die daer wiste <rhyme label="a" type="m">al</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Des zatans haet, en <rhyme label="b" type="f">boosheden</rhyme></l>
        <l id="s23:l3" met="4" type="-+">Die seyt dat hy zeer <rhyme label="a" type="m" corresp="#s23:l1">haestelijck</rhyme></l>
        <l id="s23:l4" met="3" type="-+">Onder haer worden <rhyme label="b" type="f" corresp="#s23:l2">vertreden</rhyme></l>
        <l id="s23:l5" met="4" type="-+">Het ghelooue Petri zal niet <rhyme label="c" type="m">vergaen</rhyme></l>
        <l id="s23:l6" met="4" type="-+">Daerom blijft vry vast daer op <rhyme label="c" type="m" corresp="#s23:l5">staen</rhyme></l>
        <l id="s23:l7" met="3" type="-+">Daer Christus voor heeft <rhyme label="b" type="f" corresp="#s23:l2">ghebeden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
