  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DIes zeg ik u van <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Nu, en myn leeven <rhyme label="b" type="m">lang</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Voor uwe Kruices <rhyme label="a" type="f" corresp="#s1:l1">smerten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">O Jesus, lof en <rhyme label="b" type="m" corresp="#s1:l2">dank</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Voor al uw lyden, angst en <rhyme label="c" type="m">nood</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Voor uwe groote <rhyme label="d" type="f">Liefde</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Voor uwe bloed'ge <rhyme label="c" type="m" corresp="#s1:l5">dood</rhyme>.</l>
      </lg>
    </lg>
