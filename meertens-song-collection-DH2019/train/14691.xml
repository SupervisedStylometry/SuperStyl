    <melody>Sei tanto graziosa</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter van Gelre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn ziel maeckt groot den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Die Hemel ende Aerde heeft <rhyme label="b" type="f">geschapen</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Die ons, na ons <rhyme label="a" type="f" corresp="#s1:l1">begeeren</rhyme>,</l>
        <l id="s1:l4" met="5" type="+-">Verlossen komt van't helsche stale <rhyme label="b" type="f" corresp="#s1:l2">wapen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Daer wy soo lang // Ellendigh, <rhyme label="c" type="m">bangh</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Droef hebben in <rhyme label="d" type="f">gheseten</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Van dat de <rhyme label="e" type="f">sonden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ons daer in met veel <rhyme label="e" type="f" corresp="#s1:l7">wonden</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Had <rhyme label="d" type="f" corresp="#s1:l6">ghesmeten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mijn Geest heeft haer met <rhyme label="a" type="f">vreuchden</rhyme></l>
        <l id="s2:l2" met="5" type="+-">Verheucht, in God mijn Salichmaker <rhyme label="b" type="f">soetlijck</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Want hy heeft op de <rhyme label="a" type="f" corresp="#s2:l1">deughden</rhyme></l>
        <l id="s2:l4" met="5" type="+-">Ghesien, van sijnen Dienst-maeght heel <rhyme label="b" type="f" corresp="#s2:l2">ootmoed'lijck</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Van desen tijdt // Soo sullen <rhyme label="c" type="m">wy't</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Al saligh mogen <rhyme label="d" type="f">achten</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Wat hier op <rhyme label="e" type="f">Aerde</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Den mensch vruchtbarich <rhyme label="e" type="f" corresp="#s2:l7">baerde</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">En op <rhyme label="d" type="f" corresp="#s2:l6">brachte</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Want hy heeft groote <rhyme label="a" type="f">dingen</rhyme></l>
        <l id="s3:l2" met="5" type="+-">Aen my gedaen, die machtich is in <rhyme label="b" type="f">wercken</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Daerom sal ick lof <rhyme label="a" type="f" corresp="#s3:l1">singen</rhyme>,</l>
        <l id="s3:l4" met="5" type="+-">Sijn Name, die soo waerdich is <rhyme label="b" type="f" corresp="#s3:l2">t'aenmercken</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Barmherticheyt // Van <rhyme label="c" type="m">eeuwicheyt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Is by hem soo <rhyme label="d" type="f">ghebleven</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Dat die hem <rhyme label="e" type="f">vreesen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hy haer sijn God'lijck <rhyme label="e" type="f" corresp="#s3:l7">wesen</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Weer sal <rhyme label="d" type="f" corresp="#s3:l6">geven</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy heeft een groote <rhyme label="a" type="f">crachte</rhyme></l>
        <l id="s4:l2" met="5" type="+-">Gedaen, door sijnen arm in als lof <rhyme label="b" type="f">weerdich</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Want hy heeft de <rhyme label="a" type="f" corresp="#s4:l1">ghedachten</rhyme></l>
        <l id="s4:l4" met="5" type="+-">Verstroyt, die al te seer waren <rhyme label="b" type="f" corresp="#s4:l2">hooveerdich</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">De Machtige // Heeft hy oock <rhyme label="c" type="m">me</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sijn stoel gebroken <rhyme label="d" type="f">spoedich</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">En hy heeft <rhyme label="e" type="f">weder</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Verheven, die was <rhyme label="e" type="f" corresp="#s4:l7">neder</rhyme></l>
        <l id="s4:l9" met="2" type="+-">En <rhyme label="d" type="f" corresp="#s4:l6">ootmoedich</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die hongerich neer <rhyme label="a" type="f">saten</rhyme>,</l>
        <l id="s5:l2" met="5" type="+-">Heeft hy met goet vervult, ende de <rhyme label="b" type="f">rijcken</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Heeft hy ydel <rhyme label="a" type="f" corresp="#s5:l1">ghelaten</rhyme>,</l>
        <l id="s5:l4" met="5" type="+-">Op dat sy van het werelts soude <rhyme label="b" type="f" corresp="#s5:l2">wijcken</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Maer Israel // Sijn kint, sijn <rhyme label="c" type="m">wel</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Bemind', heeft hy <rhyme label="d" type="f">ontfangen</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Die hy <rhyme label="e" type="f">barmhertich</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Sal wesen, om sijn <rhyme label="e" type="f" corresp="#s5:l7">smertich</rhyme></l>
        <l id="s5:l9" met="2" type="+-">Groot <rhyme label="d" type="f" corresp="#s5:l6">verlangen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gelijck hy heeft <rhyme label="a" type="f">ghesproken</rhyme>,</l>
        <l id="s6:l2" met="5" type="+-">Al langen tijdt voorheen, tot onse <rhyme label="b" type="f">Vad'ren</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Soo dat hy niet <rhyme label="a" type="f" corresp="#s6:l1">ghebroken</rhyme></l>
        <l id="s6:l4" met="5" type="+-">En heeft sijn woort, om haer weer te <rhyme label="b" type="f" corresp="#s6:l2">vergad'ren</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">'t Verbintenis, // Van Abram, <rhyme label="c" type="m">is</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Van hem voldaen oock <rhyme label="d" type="f">meden</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Als dat sijn <rhyme label="e" type="f">zade</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Soud eeuwich sijn <rhyme label="e" type="f" corresp="#s6:l7">ghenade</rhyme></l>
        <l id="s6:l9" met="2" type="+-">Sien met <rhyme label="d" type="f" corresp="#s6:l6">vrede</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
