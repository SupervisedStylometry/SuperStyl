<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>151073</id>
    <date>
      <year-start>1628</year-start>
      <year-end>1628</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik klaag u Venus' dieren</melody>
  <author>Augustinus van Teylingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Aenhoort ghy maechden <rhyme label="a" type="f">reyne</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Iesus heeft u <rhyme label="b" type="m">ghesint</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Hier in dit aertsche <rhyme label="a" type="f" corresp="#s1:l1">pleyne</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wt liefde hem <rhyme label="b" type="m" corresp="#s1:l2">bemint</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Met strijden soo <rhyme label="b" type="m" corresp="#s1:l2">verwint</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">V kranckheyt en <rhyme label="c" type="f">natuere</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Op dat ghy namaels <rhyme label="b" type="m" corresp="#s1:l2">vint</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Der maechden crone <rhyme label="c" type="f" corresp="#s1:l6">pure</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Boven al wilt <rhyme label="a" type="f">beminnen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Iesum den Bruydegom <rhyme label="b" type="m">reyn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">In u hert van <rhyme label="a" type="f" corresp="#s2:l1">binnen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hier in dit aertsche <rhyme label="b" type="m" corresp="#s2:l2">pleyn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Want hy mint u <rhyme label="b" type="m" corresp="#s2:l2">certeyn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met liefde <rhyme label="c" type="f">wtghelesen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dus wil hy oock <rhyme label="b" type="m" corresp="#s2:l2">alleyn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die alderliefste <rhyme label="c" type="f" corresp="#s2:l6">wesen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Const ghy te recht <rhyme label="a" type="f">bedencken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sijn groote liefde <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ghy sout hem weder <rhyme label="a" type="f" corresp="#s3:l1">schencke</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Danckbaerheyt met <rhyme label="b" type="m" corresp="#s3:l2">ootmoet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Zijn precioose <rhyme label="b" type="m" corresp="#s3:l2">bloet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Heeft hy gheheel <rhyme label="c" type="f">vergoten</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wt hem komt alle <rhyme label="b" type="m" corresp="#s3:l2">goet</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En salicheydt <rhyme label="c" type="f" corresp="#s3:l6">ghevloten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daerom set u <rhyme label="a" type="f">ghedachten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">In hem tot aller <rhyme label="b" type="m">tijt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En wilt seer neerstich <rhyme label="a" type="f" corresp="#s4:l1">wachten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Weest sterck inden <rhyme label="b" type="m" corresp="#s4:l2">strijt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Als wijse maechden <rhyme label="b" type="m" corresp="#s4:l2">zijt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">V lampen wilt <rhyme label="c" type="f">bereyden</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">V lief <rhyme label="b" type="m" corresp="#s4:l2">ghebenedijt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Sal u in vreuchden <rhyme label="c" type="f" corresp="#s4:l6">leyden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Edel wilt u toch <rhyme label="a" type="f">maecken</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met deuchden <rhyme label="b" type="m">excelent</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat boven is wilt <rhyme label="a" type="f" corresp="#s5:l1">smaken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daer woont u lief seer <rhyme label="b" type="m" corresp="#s5:l2">jent</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Tot hem u herte <rhyme label="b" type="m" corresp="#s5:l2">sent</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daer hy nu is <rhyme label="c" type="f">gheseten</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Op dat hy in u <rhyme label="b" type="m" corresp="#s5:l2">prent</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Zijn liefde <rhyme label="c" type="f" corresp="#s5:l6">onghemeten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Fier en reyn wilt u <rhyme label="a" type="f">houden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Bemint die <rhyme label="b" type="m">suyverheydt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Soo sult ghy Godt <rhyme label="a" type="f" corresp="#s6:l1">aenschouwen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Gelijck hy selve <rhyme label="b" type="m" corresp="#s6:l2">seyt</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Voor u heeft hy <rhyme label="b" type="m" corresp="#s6:l2">bereyt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Een plaetse om te <rhyme label="c" type="f">rusten</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Zijn hooge <rhyme label="b" type="m" corresp="#s6:l2">majesteyt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Sult ghy daer sien met <rhyme label="c" type="f" corresp="#s6:l6">lusten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy maegden wilt toch <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Eerbaer in uwen <rhyme label="b" type="m">mont</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Int bidden en in <rhyme label="a" type="f" corresp="#s7:l1">lesen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Devotich inden <rhyme label="b" type="m" corresp="#s7:l2">gront</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Maeckt hier een vast <rhyme label="b" type="m" corresp="#s7:l2">verbont</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">V Bruydegom te <rhyme label="c" type="f">eeren</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Inwendich wilt <rhyme label="b" type="m" corresp="#s7:l2">terstont</rhyme></l>
        <l id="s7:l8" met="3" type="-+">V herte tot hem <rhyme label="c" type="f" corresp="#s7:l6">keeren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hoe salich ist om <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Alsulck een Heere <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Die daer verlicht van <rhyme label="a" type="f" corresp="#s8:l1">binnen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">De geest met graci <rhyme label="b" type="m" corresp="#s8:l2">soet</rhyme></l>
        <l id="s8:l5" met="3" type="-+">In druck en <rhyme label="b" type="m" corresp="#s8:l2">teghenspoet</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Can hy patientie <rhyme label="c" type="f">gheven</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Hierom u beste <rhyme label="b" type="m" corresp="#s8:l2">doet</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Na syne wil te <rhyme label="c" type="f" corresp="#s8:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">In als zijt niet <rhyme label="a" type="f">hoveerdich</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Verheft u <rhyme label="b" type="m">nemmermeer</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">V Bruydegom is <rhyme label="a" type="f" corresp="#s9:l1">weerdich</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Lof, prijs, hoocheyt en <rhyme label="b" type="m" corresp="#s9:l2">eer</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Hy is u God ende <rhyme label="b" type="m" corresp="#s9:l2">Heer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Voor hem wilt u <rhyme label="c" type="f">verneren</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Hy sal u naemaels <rhyme label="b" type="m" corresp="#s9:l2">zeer</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Hooch gelijck <rhyme label="c" type="f" corresp="#s9:l6">exalteren</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Keert u ooghen der <rhyme label="a" type="f">sielen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Tot die hemelsche <rhyme label="b" type="m">vreucht</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Ootmoedâ€™lijck wilt <rhyme label="a" type="f" corresp="#s10:l1">knielen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Voor hem diet al <rhyme label="b" type="m" corresp="#s10:l2">verheucht</rhyme></l>
        <l id="s10:l5" met="3" type="-+">V teere jonghe <rhyme label="b" type="m" corresp="#s10:l2">jeucht</rhyme></l>
        <l id="s10:l6" met="3" type="-+">In suyverheyt wilt <rhyme label="c" type="f">bewaren</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Op dat ghy namaels <rhyme label="b" type="m" corresp="#s10:l2">meucht</rhyme></l>
        <l id="s10:l8" met="3" type="-+">V met de maechden <rhyme label="c" type="f" corresp="#s10:l6">paren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Laet dese blyschap <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Sy brengt droefheyt en <rhyme label="b" type="m">pijn</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Maer denckt doch op de <rhyme label="a" type="f" corresp="#s11:l1">scharen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Die inden Hemel <rhyme label="b" type="m" corresp="#s11:l2">zijn</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Die vanden soeten <rhyme label="b" type="m" corresp="#s11:l2">wijn</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Der vrolijckheden <rhyme label="c" type="f">drincken</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Aen Iesus Tafel <rhyme label="b" type="m" corresp="#s11:l2">fijn</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Sy puer en suyver <rhyme label="c" type="f" corresp="#s11:l6">blincken</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Maechden wilt toch <rhyme label="a" type="f">aencleven</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Desen Bruydegom <rhyme label="b" type="m">schoon</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Hy sal u certeyn <rhyme label="a" type="f" corresp="#s12:l1">gheven</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Een excellente <rhyme label="b" type="m" corresp="#s12:l2">croon</rhyme></l>
        <l id="s12:l5" met="3" type="-+">In synen hooghen <rhyme label="b" type="m" corresp="#s12:l2">throon</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Sal hy <rhyme label="c" type="f">vercieren</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Met nieuwen sanck <rhyme label="b" type="m" corresp="#s12:l2">idoon</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Sult ghy hem altijts <rhyme label="c" type="f" corresp="#s12:l6">vieren</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Nieuwen lofsang <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Sult ghy hem singen <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Synen naem zal staen <rhyme label="a" type="f" corresp="#s13:l1">gheschreven</rhyme></l>
        <l id="s13:l4" met="3" type="-+">In u voorhoofden <rhyme label="b" type="m" corresp="#s13:l2">claer</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Dat suyver Lam <rhyme label="b" type="m" corresp="#s13:l2">eerbaer</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Fonteyn van alle <rhyme label="c" type="f">deuchden</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Sult ghy volghen <rhyme label="b" type="m" corresp="#s13:l2">voorwaer</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Altijt met grooter <rhyme label="c" type="f" corresp="#s13:l6">vreuchden</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Om desen zanck te <rhyme label="a" type="f">singhen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Die werelt gantsch <rhyme label="b" type="m">versmaet</rhyme></l>
        <l id="s14:l3" met="3" type="-+">En alle aertsche <rhyme label="a" type="f" corresp="#s14:l1">dinghen</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Oft ander sonden <rhyme label="b" type="m" corresp="#s14:l2">quaet</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Wilt altoos vroech en <rhyme label="b" type="m" corresp="#s14:l2">laet</rhyme></l>
        <l id="s14:l6" met="3" type="-+">V Bruydegom <rhyme label="c" type="f">aenhanghen</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Die kroone <rhyme label="b" type="m" corresp="#s14:l2">delicaet</rhyme></l>
        <l id="s14:l8" met="3" type="-+">Sult ghy van hem <rhyme label="c" type="f" corresp="#s14:l6">ontfanghen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Princen en <rhyme label="a" type="f">Princessen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Die na desen staet <rhyme label="b" type="m">haeckt</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Al wort hy van <rhyme label="a" type="f" corresp="#s15:l1">diversche</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Menschen dickmael <rhyme label="b" type="m" corresp="#s15:l2">ghelaeckt</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Inwendich siet en <rhyme label="b" type="m" corresp="#s15:l2">smaeckt</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Die weerdicheydt van <rhyme label="c" type="f">desen</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Wat schimpicheydt u <rhyme label="b" type="m" corresp="#s15:l2">ghenaeckt</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Ghy sult te vreden <rhyme label="c" type="f" corresp="#s15:l6">wesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
