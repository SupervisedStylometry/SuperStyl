    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Le traquenard</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>J. Tradel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DEn met Godtverzoenden <rhyme label="a" type="m">mensch</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Heeft het heel na zijnen <rhyme label="a" type="m" corresp="#s1:l1">wensch</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Zoo hy zich ontrokken <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s1:l4" met="4" type="+-">'s Werelts doen dat kommer <rhyme label="b" type="m" corresp="#s1:l3">geeft</rhyme>;</l>
        <l id="s1:l5" met="2" type="+-">Want hy <rhyme label="c" type="m">dan</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Lustig <rhyme label="c" type="m" corresp="#s1:l5">an</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Hier kan <rhyme label="d" type="f">wezen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Heel <rhyme label="d" type="f" corresp="#s1:l7">volprezen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Want hy <rhyme label="c" type="m" corresp="#s1:l5">dan</rhyme></l>
        <l id="s1:l10" met="2" type="+-">Lustig <rhyme label="c" type="m" corresp="#s1:l5">an</rhyme></l>
        <l id="s1:l11" met="4" type="+-">Hier kan wezen lustig <rhyme label="c" type="m" corresp="#s1:l5">van</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Boven al heeft dit zijn <rhyme label="a" type="m">zwier</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Als ons Godt zeer <rhyme label="a" type="m" corresp="#s2:l1">goedertier</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Op ons eigen landt en <rhyme label="b" type="m">zant</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Dat men heeft van ouders <rhyme label="b" type="m" corresp="#s2:l3">handt</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">In een <rhyme label="c" type="m">huis</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Zonder <rhyme label="c" type="m" corresp="#s2:l5">kruis</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Heeft doen <rhyme label="d" type="f">wezen</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Heel <rhyme label="d" type="f" corresp="#s2:l7">volprezen</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">In een <rhyme label="c" type="m" corresp="#s2:l5">huis</rhyme></l>
        <l id="s2:l10" met="2" type="+-">Zonder <rhyme label="c" type="m" corresp="#s2:l5">kruis</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Daar het zuiver wijkt van 't <rhyme label="c" type="m" corresp="#s2:l5">gruis</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hy stelt zich dan tot 't <rhyme label="a" type="m">vermaak</rhyme></l>
        <l id="s3:l2" met="4" type="+-">In de een of d'ander <rhyme label="a" type="m" corresp="#s3:l1">zaak</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">In de onderscheiden <rhyme label="b" type="m">tijt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Dien hy dus met vreugd <rhyme label="b" type="m" corresp="#s3:l3">verslijt</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">En de <rhyme label="c" type="m">lucht</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Geeft hem <rhyme label="c" type="m" corresp="#s3:l5">vrucht</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Zoo in <rhyme label="d" type="f">bloemen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Om te <rhyme label="d" type="f" corresp="#s3:l7">roemen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">En de <rhyme label="c" type="m" corresp="#s3:l5">lucht</rhyme></l>
        <l id="s3:l10" met="2" type="+-">Geeft hem <rhyme label="c" type="m" corresp="#s3:l5">vrucht</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Uyt der aarden tot <rhyme label="c" type="m" corresp="#s3:l5">genucht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die ook noch brengt op zijn <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Vruchten dien het hert <rhyme label="a" type="m" corresp="#s4:l1">verblijdt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die tot smaak en voetsel <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Lekker ooft en vreugden <rhyme label="b" type="m" corresp="#s4:l3">wijn</rhyme>;</l>
        <l id="s4:l5" met="2" type="+-">Door wiens <rhyme label="c" type="m">deugt</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Hy <rhyme label="c" type="m" corresp="#s4:l5">verheugt</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Godt kan <rhyme label="d" type="f">eeren</rhyme></l>
        <l id="s4:l8" met="2" type="+-">In 't <rhyme label="d" type="f" corresp="#s4:l7">verteren</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Door wiens <rhyme label="c" type="m" corresp="#s4:l5">deugt</rhyme></l>
        <l id="s4:l10" met="2" type="+-">Hy <rhyme label="c" type="m" corresp="#s4:l5">verheugd</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Godt kan prijzen in zijn <rhyme label="c" type="m" corresp="#s4:l5">vreugt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dien hy ook zijn wondre <rhyme label="a" type="m">hant</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ziet in menig boom en <rhyme label="a" type="m" corresp="#s5:l1">plant</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Hoe 't geen kleyn was grooter <rhyme label="b" type="m">wert</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En ontfonkt alzoo zijn <rhyme label="b" type="m" corresp="#s5:l3">hert</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Dat hy <rhyme label="c" type="m">let</rhyme></l>
        <l id="s5:l6" met="2" type="+-">Om Godts <rhyme label="c" type="m" corresp="#s5:l5">Wet</rhyme></l>
        <l id="s5:l7" met="2" type="+-">Eer te <rhyme label="d" type="f">geven</rhyme></l>
        <l id="s5:l8" met="2" type="+-">In 't <rhyme label="d" type="f" corresp="#s5:l7">beleven</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Dat hy <rhyme label="c" type="m" corresp="#s5:l5">let</rhyme></l>
        <l id="s5:l10" met="2" type="+-">Om Godts <rhyme label="c" type="m" corresp="#s5:l5">Wet</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Na te leven <rhyme label="c" type="m" corresp="#s5:l5">onbesmet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hy vint ook zeer veel <rhyme label="a" type="m">genucht</rhyme></l>
        <l id="s6:l2" met="4" type="+-">In de aangename <rhyme label="a" type="m" corresp="#s6:l1">lucht</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En hy telt den zoeten <rhyme label="b" type="m">tijdt</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Naar 't gewas dat hem <rhyme label="b" type="m" corresp="#s6:l3">verblijdt</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">En daar <rhyme label="c" type="m">naar</rhyme></l>
        <l id="s6:l6" met="2" type="+-">Telt hy 't <rhyme label="c" type="m" corresp="#s6:l5">Jaar</rhyme></l>
        <l id="s6:l7" met="2" type="+-">Door de <rhyme label="d" type="f">tijden</rhyme></l>
        <l id="s6:l8" met="2" type="+-"><rhyme label="d" type="f" corresp="#s6:l7">t'Onderscheiden</rhyme>,</l>
        <l id="s6:l9" met="2" type="+-">En daar <rhyme label="c" type="m" corresp="#s6:l5">naar</rhyme></l>
        <l id="s6:l10" met="2" type="+-">Telt hy 't <rhyme label="c" type="m" corresp="#s6:l5">Jaar</rhyme></l>
        <l id="s6:l11" met="4" type="+-">Door de tijden voor en <rhyme label="c" type="m" corresp="#s6:l5">naar</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hy stelt ook den dag zijn <rhyme label="a" type="m">maat</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Naar de zon zijn gang <rhyme label="a" type="m" corresp="#s7:l1">bestaat</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En den lang of korten <rhyme label="b" type="m">dag</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Neemt hy waar zoo veel hy <rhyme label="b" type="m" corresp="#s7:l3">mag</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Ja hy <rhyme label="c" type="m">doet</rhyme></l>
        <l id="s7:l6" met="2" type="+-">Tot zijn <rhyme label="c" type="m" corresp="#s7:l5">goet</rhyme></l>
        <l id="s7:l7" met="2" type="+-">Veele <rhyme label="d" type="f">zaken</rhyme></l>
        <l id="s7:l8" met="2" type="+-">Die hem <rhyme label="d" type="f" corresp="#s7:l7">raken</rhyme>,</l>
        <l id="s7:l9" met="2" type="+-">Ja hy <rhyme label="c" type="m" corresp="#s7:l5">doet</rhyme></l>
        <l id="s7:l10" met="2" type="+-">Tot zijn <rhyme label="c" type="m" corresp="#s7:l5">goet</rhyme>,</l>
        <l id="s7:l11" met="4" type="+-">Veele zaken tot <rhyme label="c" type="m" corresp="#s7:l5">behoet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Hy is met het zijn <rhyme label="a" type="m">vernoegt</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Zoo als Godt hem dat <rhyme label="a" type="m" corresp="#s8:l1">toevoegt</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Schoon hy ziet een prachtig <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Hy acht zijn vernoegen <rhyme label="b" type="m" corresp="#s8:l3">meer</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Hy en <rhyme label="c" type="m">acht</rhyme></l>
        <l id="s8:l6" met="2" type="+-">'t Groots <rhyme label="c" type="m" corresp="#s8:l5">geslacht</rhyme>,</l>
        <l id="s8:l7" met="2" type="+-">Hier op <rhyme label="d" type="f">aarde</rhyme></l>
        <l id="s8:l8" met="2" type="+-">Van geen <rhyme label="d" type="f" corresp="#s8:l7">waerde</rhyme>,</l>
        <l id="s8:l9" met="2" type="+-">Hy en <rhyme label="c" type="m" corresp="#s8:l5">acht</rhyme></l>
        <l id="s8:l10" met="2" type="+-">'t Groots <rhyme label="c" type="m" corresp="#s8:l5">geslacht</rhyme></l>
        <l id="s8:l11" met="4" type="+-">Om haar praal niet als <rhyme label="c" type="m" corresp="#s8:l5">veracht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Want na dat ons Godt <rhyme label="a" type="m">vereert</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Wilt hy weder zijn <rhyme label="a" type="m" corresp="#s9:l1">ge-eert</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">In een nederig <rhyme label="b" type="m">gemoet</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Dat aan lijf en ziel doet <rhyme label="b" type="m" corresp="#s9:l3">goet</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Zoo dat <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s9:l6" met="2" type="+-">En <rhyme label="c" type="m" corresp="#s9:l5">geschiet</rhyme>,</l>
        <l id="s9:l7" met="2" type="+-">Zal het <rhyme label="d" type="f">wezen</rhyme></l>
        <l id="s9:l8" met="2" type="+-"><rhyme label="d" type="f" corresp="#s9:l7">Onvolprezen</rhyme>,</l>
        <l id="s9:l9" met="2" type="+-">Zoo dit <rhyme label="c" type="m" corresp="#s9:l5">niet</rhyme></l>
        <l id="s9:l10" met="2" type="+-">En <rhyme label="c" type="m" corresp="#s9:l5">geschiet</rhyme>,</l>
        <l id="s9:l11" met="4" type="+-">Wort het eindlijk ziel <rhyme label="c" type="m" corresp="#s9:l5">verdriet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Die dan Godt zijn gunst zoo <rhyme label="a" type="m">geeft</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Is een mensch die vrolijk <rhyme label="a" type="m" corresp="#s10:l1">leeft</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Hy is in zijn outheit <rhyme label="b" type="m">sterk</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">En hy stelt hem tot een <rhyme label="b" type="m" corresp="#s10:l3">perk</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">Godes <rhyme label="c" type="m">Wet</rhyme></l>
        <l id="s10:l6" met="2" type="+-">Dien hy <rhyme label="c" type="m" corresp="#s10:l5">net</rhyme>,</l>
        <l id="s10:l7" met="2" type="+-">Wil <rhyme label="d" type="f">beleven</rhyme></l>
        <l id="s10:l8" met="2" type="+-">Al zijn <rhyme label="d" type="f" corresp="#s10:l7">leven</rhyme>,</l>
        <l id="s10:l9" met="2" type="+-">Godes <rhyme label="c" type="m" corresp="#s10:l5">Wet</rhyme></l>
        <l id="s10:l10" met="2" type="+-">Dien hy <rhyme label="c" type="m" corresp="#s10:l5">net</rhyme></l>
        <l id="s10:l11" met="4" type="+-">Wil beleven <rhyme label="c" type="m" corresp="#s10:l5">onbesmet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
