    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Petrus Suetmans, Eustachius de Meyer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Sal my over-droeve <rhyme label="a" type="m">klacht</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Droeve <rhyme label="a" type="m" corresp="#s1:l1">klacht</rhyme>,</l>
        <l id="s1:l3" met="5" type="+-">Noch niet eynden daer myn ziel naer <rhyme label="a" type="m" corresp="#s1:l1">tracht</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Myn ziel naer <rhyme label="a" type="m" corresp="#s1:l1">tracht</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Wat hoor ick, een weer-galm die botst my <rhyme label="b" type="m">naer</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">'kWordt iet <rhyme label="b" type="m" corresp="#s1:l5">ghewaer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Soud' Echo wel haer <rhyme label="c" type="m">peyn</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Vermenghe met de <rhyme label="c" type="m" corresp="#s1:l7">myn</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Comt Nimph in <rhyme label="c" type="m" corresp="#s1:l7">scheyn</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'kHeb myn gonst tot noch <rhyme label="a" type="m">vol-hert</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Noch <rhyme label="a" type="m" corresp="#s2:l1">vol-hert</rhyme>,</l>
        <l id="s2:l3" met="5" type="+-">Herden sal ick, wier d'eynd', sonder <rhyme label="a" type="m" corresp="#s2:l1">smert</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">d'Eynd' sonder <rhyme label="a" type="m" corresp="#s2:l1">smert</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Ach! Echo, u weer-galm vlyt mynen <rhyme label="b" type="m">druck</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Soud' sulck <rhyme label="b" type="m" corresp="#s2:l5">gheluck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Noch eynden desen <rhyme label="c" type="m">strydt</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Vol druck, en <rhyme label="c" type="m" corresp="#s2:l7">suerigheydt</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Neen, Nimph ghy <rhyme label="c" type="m" corresp="#s2:l7">vleyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Sal ick dan noch zyn <rhyme label="a" type="m">verbleydt</rhyme>.</l>
        <l id="s3:l2" met="2" type="+-">Zyn <rhyme label="a" type="m" corresp="#s3:l1">verbleydt</rhyme>,</l>
        <l id="s3:l3" met="5" type="+-">Wanneer Echo, als Narcissus <rhyme label="a" type="m" corresp="#s3:l1">vleydt</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ciscissus <rhyme label="a" type="m" corresp="#s3:l1">vleydt</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Wie vleyter, ach! Echo, waer-om <rhyme label="b" type="m">gherust</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Vol-doet myn <rhyme label="b" type="m" corresp="#s3:l5">lust</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Want u de <rhyme label="c" type="m">minne-kracht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Heeft oock in druck <rhyme label="c" type="m" corresp="#s3:l7">ghebracht</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Aen-hoort myn <rhyme label="c" type="m" corresp="#s3:l7">klacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sal myn over-schoon <rhyme label="a" type="m">Vrindin</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Schoon <rhyme label="a" type="m" corresp="#s4:l1">Vrindin</rhyme>,</l>
        <l id="s4:l3" met="5" type="+-">Seght my Echo, oft sy leeft vol <rhyme label="a" type="m" corresp="#s4:l1">min</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Sy leeft vol <rhyme label="a" type="m" corresp="#s4:l1">min</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">Bemint sy, ach! hemel wat groote <rhyme label="b" type="m">vreucht</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">My nu <rhyme label="b" type="m" corresp="#s4:l5">verheucht</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Leeft Echo, Echo <rhyme label="c" type="m">leeft</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Den troost myn droefheyt <rhyme label="c" type="m" corresp="#s4:l7">sneeft</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Die ghy my <rhyme label="c" type="m" corresp="#s4:l7">gheeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
