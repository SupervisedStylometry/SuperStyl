    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Gott der vater wohn uns bei</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IEsus Christ waer Godes <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Waerachtich Mensch <rhyme label="b" type="f">gheboren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Voor allen Menschen Kindren <rhyme label="a" type="m" corresp="#s1:l1">schoon</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dus waer'n wy al <rhyme label="b" type="f" corresp="#s1:l2">verloren</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ghy hebt ons door dijnen <rhyme label="c" type="m">Doot</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat Eewich Rijck <rhyme label="d" type="f">verworven</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ghy zijt voor ons <rhyme label="d" type="f" corresp="#s1:l6">ghestorven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dus waer'n wy al <rhyme label="d" type="f" corresp="#s1:l6">verdorven</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Ghy zijt onse HEmels <rhyme label="c" type="m" corresp="#s1:l5">broot</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Van Vader ons <rhyme label="e" type="f">ghegheven</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Den doot te <rhyme label="e" type="f" corresp="#s1:l10">wederstreven</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Daer door wy Eewich <rhyme label="e" type="f" corresp="#s1:l10">Leven</rhyme>,</l>
        <l id="s1:l13" met="3" type="+-">Van Maria gheboren <rhyme label="f" type="m">bist</rhyme>,</l>
        <l id="s1:l14" met="4" type="-+">Ghelovet zijt ghy JEsu <rhyme label="f" type="m" corresp="#s1:l13">Christ</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Heylige Geest ghy derde <rhyme label="a" type="m">Persoon</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Der Godtheyt ghelijcker <rhyme label="b" type="f">Eeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Met den Vader en den <rhyme label="a" type="m" corresp="#s2:l1">Soon</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dijn Ghenade wy <rhyme label="b" type="f" corresp="#s2:l2">begeeren</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dien de HEEre ghelovet <rhyme label="c" type="m">heeft</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ons Armen en <rhyme label="d" type="f">Elenden</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Van Godt Vader te <rhyme label="d" type="f" corresp="#s2:l6">senden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Valsche Leer van ons te <rhyme label="d" type="f" corresp="#s2:l6">wenden</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Dat wy volghen dijner <rhyme label="c" type="m" corresp="#s2:l5">Raet</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">En by der Waerheyt <rhyme label="e" type="f">blijven</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">All' Huychelij van ons <rhyme label="e" type="f" corresp="#s2:l10">drijven</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">So kan GOds Woort <rhyme label="e" type="f" corresp="#s2:l10">beclijven</rhyme>,</l>
        <l id="s2:l13" met="3" type="+-">Alleluja singhen <rhyme label="f" type="m">wy</rhyme>,</l>
        <l id="s2:l14" met="4" type="-+">Nu help ons Heylige Geest tot <rhyme label="f" type="m" corresp="#s2:l13">dy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
