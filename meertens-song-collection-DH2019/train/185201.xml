    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 136 Datheen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Dancket Godt nu <rhyme label="a" type="m">opentlick</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hy is doch seer <rhyme label="a" type="m" corresp="#s1:l1">vriendelick</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want zijn groote <rhyme label="b" type="m">goedicheydt</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Gheduurt inder <rhyme label="b" type="m" corresp="#s1:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die van swerelts <rhyme label="a" type="m">aenbegin</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Heeft ghetoont zijn soete <rhyme label="a" type="m" corresp="#s2:l1">min</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want zijn groote <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Gheduurt in der <rhyme label="b" type="m" corresp="#s2:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die de werelt soo lief <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dat hy synen Sone <rhyme label="a" type="m" corresp="#s3:l1">geeft</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Want zijn groote <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Gheduurt in der <rhyme label="b" type="m" corresp="#s3:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die de menschen niet uyt <rhyme label="a" type="m">haet</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Maer uyt pure liefde <rhyme label="a" type="m" corresp="#s4:l1">slaet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want zijn groote <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Gheduurt in der <rhyme label="b" type="m" corresp="#s4:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hy en wil verliesen <rhyme label="a" type="m">geen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Van zijn schaepkens, jae niet <rhyme label="a" type="m" corresp="#s5:l1">een</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Want zijn groote <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Gheduurt in der <rhyme label="b" type="m" corresp="#s5:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Tot het alderminste <rhyme label="a" type="m">kint</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Is hy lieffelick <rhyme label="a" type="m" corresp="#s6:l1">ghesint</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Want syn groote <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Gheduurt in der <rhyme label="b" type="m" corresp="#s6:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die noch jonck zijn in â€˜t <rhyme label="a" type="m">verstant</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Reyckt hy minnelick de <rhyme label="a" type="m" corresp="#s7:l1">hant</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Want zijn groote <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Gheduurt in der <rhyme label="b" type="m" corresp="#s7:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Haer verstant wert <rhyme label="a" type="m">opgevoet</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Met die melck der liefden <rhyme label="a" type="m" corresp="#s8:l1">soet</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Want zijn groote <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Gheduurt inder <rhyme label="b" type="m" corresp="#s8:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Danckt hem die weldadich <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">En daer in ghestadich <rhyme label="a" type="m" corresp="#s9:l1">is</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Want zijn groote <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Gheduurt inder <rhyme label="b" type="m" corresp="#s9:l3">eeuwicheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
