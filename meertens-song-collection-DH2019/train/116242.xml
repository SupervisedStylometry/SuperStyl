<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>116242</id>
    <date>
      <year-start>1709</year-start>
      <year-end>1709</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Suivons l'amour</melody>
  <author>Cornelis Sweerts</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Point de chagrin dans notre <rhyme label="a" type="f">vie</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">By jonge jaaren voegt wel de <rhyme label="b" type="m">vreugt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Bannissons la <rhyme label="a" type="f" corresp="#s1:l1">melancolie</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Want hier bedroeft te zyn is juist geen <rhyme label="b" type="m" corresp="#s1:l2">deugt</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Que peut onfaire en la <rhyme label="a" type="f">vieillesse</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dan is men stram van keel en van <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Sans complaisance ni <rhyme label="a" type="f" corresp="#s2:l1">tendresse</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Daar menig mensch bedroeft om zuchten <rhyme label="b" type="m" corresp="#s2:l2">moet</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Chantons, dansons à la <rhyme label="a" type="f">naissance</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Want zy die nu van daag <rhyme label="b" type="m">verjaart</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Change jamais de <rhyme label="a" type="f" corresp="#s3:l1">complaisance</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">En acht ons ook haar by zyn heden <rhyme label="b" type="m" corresp="#s3:l2">waard</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Evitons nos <rhyme label="a" type="f">egorgerie</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ik loop niet op den <rhyme label="b" type="m">bedelzak</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Car franchement en <rhyme label="a" type="f" corresp="#s4:l1">poësie</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Dat heeft ook somtyts heel veel <rhyme label="b" type="m" corresp="#s4:l2">ongemak</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Qu'on voit le ciel toûjours <rhyme label="a" type="f">propice</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dus wensch ik wat gy wenschen <rhyme label="b" type="m">meugt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Qui en humeur est bien <rhyme label="a" type="f" corresp="#s5:l1">delice</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">En 't Puikje van al de Amsterdamsche <rhyme label="b" type="m" corresp="#s5:l2">jeugt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
