    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Godt onsen Heer u loven <rhyme label="a" type="m">wy</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Endâ€™ dancken dy met moede <rhyme label="a" type="m" corresp="#s1:l1">bly</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Want u ghenade en <rhyme label="b" type="m">goetheyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Duyrt nu en in der <rhyme label="b" type="m" corresp="#s1:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Uwen Heylighen name <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Singhen wy u lof prijs ende <rhyme label="a" type="m" corresp="#s2:l1">eer</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Heylich, heylich, heylich ghy <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ghy onsen Godt <rhyme label="b" type="m" corresp="#s2:l3">ghebenedijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">So langh wy hier zijn in dit <rhyme label="a" type="m">dal</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Sullen wy met vrolijck <rhyme label="a" type="m" corresp="#s3:l1">gheschal</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Met der Enghelen en Christen <rhyme label="b" type="m">schaer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">U loven prijsen <rhyme label="b" type="m" corresp="#s3:l3">allegaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Want ghy alleen eenighe <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Zijt onse toe-vlught, Borcht en <rhyme label="a" type="m" corresp="#s4:l1">Slot</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Uw gunst, ghenade, en <rhyme label="b" type="m">goetheyt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Hebdy over ons <rhyme label="b" type="m" corresp="#s4:l3">uyt-ghebreyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Des wy singhen al te <rhyme label="a" type="m">saem</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Lof, prijs, ende eere <rhyme label="a" type="m" corresp="#s5:l1">bequaem</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">U onsen Godt vriendelijck en <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Voor alle u weldaden <rhyme label="b" type="m" corresp="#s5:l3">goet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
