<song>
  <meta>
    <id>8882</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Nun lob meine Seele den Herrn</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NV Loeff mijn Siel den <rhyme label="a" type="f">HEERE</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wat in my is den Naeme <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sijn Weldaet hy <rhyme label="a" type="f" corresp="#s1:l1">vermeere</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Vergeet dat niet O Heere <rhyme label="b" type="m" corresp="#s1:l2">mijn</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Heeft dy dijn Sond' <rhyme label="c" type="f">vergheven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Gheneest dijn Swackheyt <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Verlost dijn Arme <rhyme label="c" type="f" corresp="#s1:l5">Leven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Neemt dy in zijnen <rhyme label="d" type="m" corresp="#s1:l6">Schoot</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Met Rijcken Troost <rhyme label="e" type="f">Beschuddet</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Verjong't den Ar'nt <rhyme label="f" type="m">ghelijck</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Oprecht hy oock <rhyme label="e" type="f" corresp="#s1:l9">Behoedet</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Die lijden om syn <rhyme label="f" type="m" corresp="#s1:l10">Rijck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy heeft ons weten <rhyme label="a" type="f">laten</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Syn Heylich Recht en zijn <rhyme label="b" type="m">Ghericht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Syn goetheyt boven <rhyme label="a" type="f" corresp="#s2:l1">maten</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Het feylt aen syn Ontfarmen <rhyme label="b" type="m" corresp="#s2:l2">nicht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Syn Thoorn laet hy wel <rhyme label="c" type="f">varen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Straft niet nae onse <rhyme label="d" type="m">Sundt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gaet syn Ghenaed' niet <rhyme label="c" type="f" corresp="#s2:l5">sparen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Veel goets den swacken <rhyme label="d" type="m" corresp="#s2:l6">gunt</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Syn goetheyt is ten <rhyme label="e" type="f">besten</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Op die hem vreesen <rhyme label="f" type="m">recht</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">So verd' als t' Oost van't <rhyme label="e" type="f" corresp="#s2:l9">Westen</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Doet hy ons Sonden <rhyme label="f" type="m" corresp="#s2:l10">wech</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als hem een Man <rhyme label="a" type="f">Erbarmet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Al over syne Kinder <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">De Heer hem soo <rhyme label="a" type="f" corresp="#s3:l1">Ontfarmet</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Oock over die hem vreesen <rhyme label="b" type="m" corresp="#s3:l2">reyn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hy kent syn Schepsel <rhyme label="c" type="f">teere</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En weet wy zijn maer <rhyme label="d" type="m">Stoff</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Een Mensch is hier niet <rhyme label="c" type="f" corresp="#s3:l5">meere</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ghelijck dan Gras en <rhyme label="d" type="m" corresp="#s3:l6">Looff</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Hy bloeyt wel als die <rhyme label="e" type="f">Bloemen</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Maer als een suer <rhyme label="f" type="m">Onweer</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">En Wint daer over <rhyme label="e" type="f" corresp="#s3:l9">comen</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Soo is hy haest niet <rhyme label="f" type="m" corresp="#s3:l10">meer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maer Gods Ghenaed' <rhyme label="a" type="f">alleyne</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Blijft stadich vast in <rhyme label="b" type="m">Eewicheyt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">By syn Vercoorn <rhyme label="a" type="f" corresp="#s4:l1">Gemeyne</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die in syn vrees staet wel <rhyme label="b" type="m" corresp="#s4:l2">bereyt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Die syn Verbondt <rhyme label="c" type="f">bewaren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ghehooren int <rhyme label="d" type="m">Hemelrijck</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Ghy Eng'len syn <rhyme label="c" type="f" corresp="#s4:l5">Heyrscharen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Loofft, Dient hem all' <rhyme label="d" type="m" corresp="#s4:l6">ghelijck</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Den grooten HEER ter <rhyme label="e" type="f">eeren</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">En drijft syn Heylich <rhyme label="f" type="m">Woort</rhyme>.</l>
        <l id="s4:l11" met="3" type="+-">Myn Siel sal oock <rhyme label="e" type="f" corresp="#s4:l9">vermeeren</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Syn Loff aen allen <rhyme label="f" type="m" corresp="#s4:l10">Oort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
