    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>O zalig heilig Bethlehem</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">OCh Vader! Zoon en heyligh <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Met uw' heylige <rhyme label="b" type="f">uytverkooren</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Wilt in dit tegenwoordigh <rhyme label="a" type="m" corresp="#s1:l1">feest</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Onse gebeden al <rhyme label="b" type="f" corresp="#s1:l2">verhooren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Och Godes Zoon! Door uw' reyn <rhyme label="a" type="m">bloedt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Doodt, nagels, pijnen, kruys en <rhyme label="b" type="f">wonden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wy bidden uw' met groot <rhyme label="a" type="m" corresp="#s2:l1">ootmoet</rhyme>;</l>
        <l id="s2:l4" met="4" type="-+">Vergeeft onse misdaedt en <rhyme label="b" type="f" corresp="#s2:l2">sonden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">3.Maria Maeght, en Moeder <rhyme label="a" type="m">Goods</rhyme>!</l>
        <l id="s3:l2" met="4" type="-+">Weest, blijft onse <rhyme label="b" type="f">Addoratersse</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Van nu tot in d' ure des <rhyme label="a" type="m" corresp="#s3:l1">doots</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Thoont dat ghy zijt ons' <rhyme label="b" type="f" corresp="#s3:l2">voorspraeckersse</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Onse Engel bewaerder <rhyme label="a" type="m">trouw</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Met de negen Engelen <rhyme label="b" type="f">Chooren</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Krijght dat wy door geluck noch <rhyme label="a" type="m" corresp="#s4:l1">rouw</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Noch ongeloof oyt gaen <rhyme label="b" type="f" corresp="#s4:l2">verlooren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Verlaters van al 't Werelts <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Apostels, Discipels des <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Krijght dat wy niet na 't aerdts of <rhyme label="a" type="m" corresp="#s5:l1">bloet</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Maer na Godt alleen ons hart <rhyme label="b" type="f" corresp="#s5:l2">keeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Patriarchen, Propheten <rhyme label="a" type="m">wijs</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Priesters, Diakens en <rhyme label="b" type="f">Propheten</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Helpt ons dat wy in dranck noch <rhyme label="a" type="m" corresp="#s6:l1">spijs</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Onse Godt en al, oyt <rhyme label="b" type="f" corresp="#s6:l2">vergeten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Goods Martelaren jongh en <rhyme label="a" type="m">out</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Staet ons by met hulp t' alle <rhyme label="b" type="f">tijden</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dat wy Godts naem en 't geloof <rhyme label="a" type="m" corresp="#s7:l1">stout</rhyme></l>
        <l id="s7:l4" met="4" type="-+">In Schepen en op 't Landt <rhyme label="b" type="f" corresp="#s7:l2">belijden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maeckt Consessoren kleyn en <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Heremijten ende <rhyme label="b" type="f">Doctoren</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Dat kettery Godtloos en <rhyme label="a" type="m" corresp="#s8:l1">snoodt</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">En ketters macht haest mach <rhyme label="b" type="f" corresp="#s8:l2">versmoren</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ghy Maeghden, die vroom tegen 't <rhyme label="a" type="m">vleysch</rhyme></l>
        <l id="s9:l2" met="4" type="-+">En 's Werelts lusten heb <rhyme label="b" type="f">gestreden</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">Verkrijght na ons begeer en <rhyme label="a" type="m" corresp="#s9:l1">eysch</rhyme>;</l>
        <l id="s9:l4" met="4" type="-+">Dat wy zijn reyn van hart en <rhyme label="b" type="f" corresp="#s9:l2">leden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Beheerders van ons <rhyme label="a" type="m">Vaderlandt</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Sint' Willibrordt met uw' <rhyme label="b" type="f">gesellen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Ontfanght voor ons uyt Godes <rhyme label="a" type="m" corresp="#s10:l1">handt</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Dat wy ontgaen de pijn der <rhyme label="b" type="f" corresp="#s10:l2">Hellen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Patroonen voor ons alle <rhyme label="a" type="m">bidt</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Dat wy in liefde, rust en <rhyme label="b" type="f">vreden</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Godt dienen met een ziele <rhyme label="a" type="m" corresp="#s11:l1">wit</rhyme>:</l>
        <l id="s11:l4" met="4" type="-+">Dat wy uw' voetstappen na <rhyme label="b" type="f" corresp="#s11:l2">treden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Voor ons Heylige <rhyme label="a" type="m">allegaer</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Krijght van een Godt, en drie <rhyme label="b" type="f">Persoonen</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Dat wy by u lieden daer <rhyme label="a" type="m" corresp="#s12:l1">naer</rhyme></l>
        <l id="s12:l4" met="4" type="-+">In 's Hemels vreughden mogen <rhyme label="b" type="f" corresp="#s12:l2">wonen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
