<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22394</id>
    <date>
      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Gelobt seist du Jesu Christ</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ghelooft zijt ghy Jhesu al <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat ghy Mensche gheboren <rhyme label="a" type="m" corresp="#s1:l1">zijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Van eender Maghet dat is <rhyme label="b" type="m">waer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Daer af verblijt die Enghelsche <rhyme label="b" type="m" corresp="#s1:l3">schaer</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+"><rhyme label="c" type="m">Kirieleys</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Des eewighen Vaders eenich <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Men nu in der Cribben <rhyme label="a" type="m" corresp="#s2:l1">vint</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">In onsen armen vleesch en <rhyme label="b" type="m">bloet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vercleet hem hier dat eewich <rhyme label="b" type="m" corresp="#s2:l3">goet</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+"><rhyme label="c" type="m">Kirieleys</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den al des werelts crinck niet <rhyme label="a" type="m">besloot</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Leyt daer in Marien <rhyme label="a" type="m" corresp="#s3:l1">schoot</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En is een kindekijn worden <rhyme label="b" type="m">cleyn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die alle dinck behoudt <rhyme label="b" type="m" corresp="#s3:l3">alleyn</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+"><rhyme label="c" type="m">Kierieleys</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat eewighe Licht van daer seer <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Gheeft der wereldt den nieuwen <rhyme label="a" type="m" corresp="#s4:l1">schijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Het schijnt wel inder <rhyme label="b" type="m">middernacht</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En maect ons kinders des lichts <rhyme label="b" type="m" corresp="#s4:l3">bedacht</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+"><rhyme label="c" type="m">Kirieleys</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Sone des Vaders een God oock <rhyme label="a" type="m">puer</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Wort een gast in tswerelts <rhyme label="a" type="m" corresp="#s5:l1">schuer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hy vuert ons wt dit <rhyme label="b" type="m">iammerdal</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En maeckt ons eruen in zijn <rhyme label="b" type="m" corresp="#s5:l3">ghetal</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+"><rhyme label="c" type="m">Kirieleys</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ter aerden arm hy comen <rhyme label="a" type="m">wou</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat hy onser erbarmen <rhyme label="a" type="m" corresp="#s6:l1">sou</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En in den Hemel maken <rhyme label="b" type="m">rijck</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Synen lieuen Enghelen <rhyme label="b" type="m" corresp="#s6:l3">ghelijck</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+"><rhyme label="c" type="m">Kirieleys</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dat heeft hy ons allen hier <rhyme label="a" type="m">ghedaen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Om zijn liefde te tooghen <rhyme label="a" type="m" corresp="#s7:l1">zaen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Des verhuecht haer al die <rhyme label="b" type="m">Christenheyt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En danckt hem des in <rhyme label="b" type="m" corresp="#s7:l3">eewicheyt</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+"><rhyme label="c" type="m">Kirieleys</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
