    <tags/>
    <melody>Tien geboden</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">AAnbiddelyke God en <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die my van alle <rhyme label="b" type="m">euwigheit</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Hebt voorgebragt, Ã´ bron en <rhyme label="a" type="f" corresp="#s1:l1">ader</rhyme></l>
        <l id="s1:l4" met="4" type="-+">De Heilige <rhyme label="b" type="m" corresp="#s1:l2">Drieenigheit</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zie hier ben ik, hier is u <rhyme label="a" type="f">Zone</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Op uwen wenk en wil, <rhyme label="b" type="m">bereit</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Om u betaam'lyk te doen <rhyme label="a" type="f" corresp="#s2:l1">tone</rhyme></l>
        <l id="s2:l4" met="4" type="-+">U gunst en <rhyme label="b" type="m" corresp="#s2:l2">Algenoegzaamheit</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laat dog den mens niet <rhyme label="a" type="f">nederdalen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Voor euwiglyk in 't nare <rhyme label="b" type="m">graf</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Ik zal een vol rantzoen <rhyme label="a" type="f" corresp="#s3:l1">betalen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En wenden hem de straffen <rhyme label="b" type="m" corresp="#s3:l2">af</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">U eis, u voorstel is zo <rhyme label="a" type="f">billig</rhyme></l>
        <l id="s4:l2" met="4" type="-+">En zo een God betaam'lyk <rhyme label="b" type="m">woort</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Ik stel my zelven straks <rhyme label="a" type="f" corresp="#s4:l1">gewillig</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Myn oren hebt gy my <rhyme label="b" type="m" corresp="#s4:l2">doorboort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">U wet is in myn <rhyme label="a" type="f">ingewanden</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En die te doen is al myn <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">De mensen lieft doet my <rhyme label="a" type="f" corresp="#s5:l1">ontbranden</rhyme></l>
        <l id="s5:l4" met="4" type="-+">En geeft m'een rusteloze <rhyme label="b" type="m" corresp="#s5:l2">rust</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ik moet, Ik wil, Ik zal my <rhyme label="a" type="f">zetten</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Tot heilant en tot <rhyme label="b" type="m">middelaar</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Ik zal de slangenkop <rhyme label="a" type="f" corresp="#s6:l1">verpletten</rhyme></l>
        <l id="s6:l4" met="4" type="-+">En rukken 't myn' uit <rhyme label="b" type="m" corresp="#s6:l2">helsgevaar</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ik zal my met het vlees <rhyme label="a" type="f">bekleden</rhyme></l>
        <l id="s7:l2" met="4" type="-+">En komen in gering <rhyme label="b" type="m">gelaat</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ik zal alleen de wynpers <rhyme label="a" type="f" corresp="#s7:l1">treden</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Van uwen toorn en zonden <rhyme label="b" type="m" corresp="#s7:l2">haat</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ik weiger niet de doot te <rhyme label="a" type="f">lyden</rhyme></l>
        <l id="s8:l2" met="4" type="-+">En alle ramp en ziel <rhyme label="b" type="m">gequel</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Als ik 'er vele kan <rhyme label="a" type="f" corresp="#s8:l1">bevryden</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Die ik tot myn bezitting <rhyme label="b" type="m" corresp="#s8:l2">stel</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">'k Zal haar u raatslag <rhyme label="a" type="f">openbaren</rhyme></l>
        <l id="s9:l2" met="4" type="-+">En wyzen 't pat ter <rhyme label="b" type="m">zaligheit</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Ik zal haar door myn Geest <rhyme label="a" type="f" corresp="#s9:l1">bewaren</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">En geven 't lot der <rhyme label="b" type="m" corresp="#s9:l2">euwigheit</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ik zal als Heer en <rhyme label="a" type="f">Opperkoning</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Haar sturen door myn heilig <rhyme label="b" type="m">woort</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">En brengen, Vader, in u <rhyme label="a" type="f" corresp="#s10:l1">woning</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Die na myn regt en wetten <rhyme label="b" type="m" corresp="#s10:l2">hoort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
