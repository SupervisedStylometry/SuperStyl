  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Toen <rhyme label="a" type="m">Klorimeen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Op aerd <rhyme label="a" type="m" corresp="#s1:l1">verscheen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Gaf Venus, die neder kwam <rhyme label="b" type="f">dalen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En 't Meisje met gonst wou <rhyme label="b" type="f" corresp="#s1:l3">bestralen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Aen haer bevallig <rhyme label="a" type="m" corresp="#s1:l1">heen</rhyme>.</l>
        <l id="s1:l6" met="5" type="-+">Waer door ze wordt door yder <rhyme label="a" type="m" corresp="#s1:l1">aengebeên</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">De Starren verçieren den Hemel <rhyme label="c" type="m">trans</rhyme>;</l>
        <l id="s1:l8" met="4" type="-+">De Rozen verçieren een bloemen <rhyme label="c" type="m" corresp="#s1:l7">krans</rhyme>;</l>
        <l id="s1:l9" met="4" type="-+">Maer't Meisje dat steeds myne Vryheit <rhyme label="d" type="m">verkracht</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Is hooger dan Starren en Rozen <rhyme label="d" type="m" corresp="#s1:l9">geacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
