<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24574</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Kinderen zwijg zo moog dij horen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEre Jhesus uwen brunen <rhyme label="a" type="m">cop</rhyme></l>
        <l id="s1:l2" met="4" type="-+">exe mondy <rhyme label="b" type="m">gaudio</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Hi bloeyt als enen wyngarts <rhyme label="a" type="m" corresp="#s1:l1">cnop</rhyme></l>
        <l id="s1:l4" met="4" type="-+">cia fya <rhyme label="b" type="m" corresp="#s1:l2">lencia</rhyme></l>
        <l id="s1:l5" met="3" type="-+">O vyrgo <rhyme label="b" type="m" corresp="#s1:l2">maria</rhyme></l>
        <l id="s1:l6" met="3" type="-+">O plena <rhyme label="b" type="m" corresp="#s1:l2">gracia</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Heer Jhesus rockelkyn dat was <rhyme label="a" type="m">groen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">exe mondi <rhyme label="b" type="m">gaudio</rhyme></l>
        <l id="s2:l3" met="4" type="-+">ende al syn lyfken als ene <rhyme label="a" type="m" corresp="#s2:l1">bloem</rhyme></l>
        <l id="s2:l4" met="4" type="-+">cia fia <rhyme label="b" type="m" corresp="#s2:l2">lencia</rhyme></l>
        <l id="s2:l5" met="3" type="-+">O virgo <rhyme label="b" type="m" corresp="#s2:l2">maria</rhyme></l>
        <l id="s2:l6" met="3" type="-+">O plenae   <rhyme label="b" type="m" corresp="#s2:l2">gracia</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Heer Jhesus is een <rhyme label="a" type="m">auentganger</rhyme></l>
        <l id="s3:l2" met="4" type="-+">exe mondi <rhyme label="b" type="m">gaudia</rhyme></l>
        <l id="s3:l3" met="4" type="-+">tot eenre Jonferen was alle syn <rhyme label="a" type="m" corresp="#s3:l1">ganc</rhyme></l>
        <l id="s3:l4" met="4" type="-+">cia fya <rhyme label="b" type="m" corresp="#s3:l2">lencia</rhyme></l>
        <l id="s3:l5" met="3" type="-+">O vyrgo <rhyme label="b" type="m" corresp="#s3:l2">maria</rhyme></l>
        <l id="s3:l6" met="3" type="-+">O plena <rhyme label="b" type="m" corresp="#s3:l2">gracia</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy hebbe my lief sy mynnen my <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s4:l2" met="4" type="-+">exe mondi <rhyme label="b" type="m">gaudia</rhyme></l>
        <l id="s4:l3" met="4" type="-+">daer om byn ic byden Joncferen <rhyme label="a" type="m" corresp="#s4:l1">geerne</rhyme></l>
        <l id="s4:l4" met="4" type="-+">cia fya <rhyme label="b" type="m" corresp="#s4:l2">lencia</rhyme></l>
        <l id="s4:l5" met="3" type="-+">O vyrgo <rhyme label="b" type="m" corresp="#s4:l2">maria</rhyme></l>
        <l id="s4:l6" met="3" type="-+">O plena <rhyme label="b" type="m" corresp="#s4:l2">gracia</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maria huedet uwen <rhyme label="a" type="m">soen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">exe mondi <rhyme label="b" type="m">gaudio</rhyme></l>
        <l id="s5:l3" met="4" type="-+">datten v die jonferen niet en <rhyme label="a" type="m" corresp="#s5:l1">nemen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">cia fya <rhyme label="b" type="m" corresp="#s5:l2">lencia</rhyme></l>
        <l id="s5:l5" met="3" type="-+">O vyrgo <rhyme label="b" type="m" corresp="#s5:l2">maria</rhyme></l>
        <l id="s5:l6" met="3" type="-+">O plena <rhyme label="b" type="m" corresp="#s5:l2">gracia</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ick en kans gehueden <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s6:l2" met="4" type="-+">exe mundo <rhyme label="b" type="m">gaudio</rhyme></l>
        <l id="s6:l3" met="4" type="-+">hi heeft die reyne herten <rhyme label="a" type="m" corresp="#s6:l1">lief</rhyme></l>
        <l id="s6:l4" met="4" type="-+">cia fia <rhyme label="b" type="m" corresp="#s6:l2">lencia</rhyme></l>
        <l id="s6:l5" met="3" type="-+">O virgo <rhyme label="b" type="m" corresp="#s6:l2">maria</rhyme></l>
        <l id="s6:l6" met="3" type="-+">o plena <rhyme label="b" type="m" corresp="#s6:l2">gracia</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ick heb sy oec alsoe duer <rhyme label="a" type="m">gecost</rhyme></l>
        <l id="s7:l2" met="4" type="-+">exe mondi <rhyme label="b" type="m">gaudia</rhyme></l>
        <l id="s7:l3" met="4" type="-+">daer om en mach icker niet wesen <rhyme label="a" type="m" corresp="#s7:l1">af</rhyme></l>
        <l id="s7:l4" met="4" type="-+">cia fya <rhyme label="b" type="m" corresp="#s7:l2">lencia</rhyme></l>
        <l id="s7:l5" met="3" type="-+">O vyrgo <rhyme label="b" type="m" corresp="#s7:l2">maria</rhyme></l>
        <l id="s7:l6" met="3" type="-+">O plena <rhyme label="b" type="m" corresp="#s7:l2">gracia</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Hy nam een corf in syn <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s8:l2" met="4" type="-+">exe mundi <rhyme label="b" type="m">gaudia</rhyme></l>
        <l id="s8:l3" met="4" type="-+">hi las die sprockelkyn daer hise <rhyme label="a" type="m" corresp="#s8:l1">vant</rhyme></l>
        <l id="s8:l4" met="4" type="-+">cia fya <rhyme label="b" type="m" corresp="#s8:l2">lencia</rhyme></l>
        <l id="s8:l5" met="3" type="-+">O vyrgo <rhyme label="b" type="m" corresp="#s8:l2">maria</rhyme></l>
        <l id="s8:l6" met="3" type="-+">O plena <rhyme label="b" type="m" corresp="#s8:l2">gracia</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hy nam die cruke hi haelden <rhyme label="a" type="m">water</rhyme></l>
        <l id="s9:l2" met="4" type="-+">exe mondi <rhyme label="b" type="m">gaudia</rhyme></l>
        <l id="s9:l3" met="4" type="-+">hy  halp sinre moeder dat moesken <rhyme label="a" type="m" corresp="#s9:l1">caken</rhyme></l>
        <l id="s9:l4" met="4" type="-+">cia fya <rhyme label="b" type="m" corresp="#s9:l2">lencia</rhyme></l>
        <l id="s9:l5" met="3" type="-+">o virgo <rhyme label="b" type="m" corresp="#s9:l2">maria</rhyme></l>
        <l id="s9:l6" met="3" type="-+">O plena <rhyme label="b" type="m" corresp="#s9:l2">gracia</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
