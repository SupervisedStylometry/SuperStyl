      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laten wy de liefde <rhyme label="a" type="f">haaten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Vlieden wy dat groot <rhyme label="b" type="m">gevaar</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Min die speelt by alle <rhyme label="a" type="f" corresp="#s1:l1">staaten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dikwils rollen droef en <rhyme label="b" type="m" corresp="#s1:l2">naâr</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En in wreedheên <rhyme label="a" type="f" corresp="#s1:l1">uitgelaten</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Wordmen 't gift te laat <rhyme label="b" type="m" corresp="#s1:l2">gewaar</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Laten wy de liefde <rhyme label="a" type="f" corresp="#s1:l1">haaten</rhyme>,</l>
