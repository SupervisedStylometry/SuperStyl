  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MInne wigtje, zagt en <rhyme label="a" type="m">teer</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Daal, op uwe blanke <rhyme label="a" type="m" corresp="#s1:l1">veer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Aan de blonde Merwe <rhyme label="a" type="m" corresp="#s1:l1">neer</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Met uw peilen, en uw <rhyme label="b" type="m">boog</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Tref het hart van myn <rhyme label="c" type="m">Klarin</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Die, Heelaas! te wreet van <rhyme label="c" type="m" corresp="#s1:l5">zin</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Spot met myne tedre <rhyme label="c" type="m" corresp="#s1:l5">min</rhyme>,</l>
        <l id="s1:l8" met="5" type="+-">Eer ik aan 't kwynent vuur <rhyme label="b" type="m" corresp="#s1:l4">vedroog</rhyme>,</l>
      </lg>
    </lg>
  </text>
