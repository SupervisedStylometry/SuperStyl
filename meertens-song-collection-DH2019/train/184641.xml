<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184641</id>
    <date>
      <year-start>1740</year-start>
      <year-end>1740</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Chantons l'heureuse naissance</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LAet ons de Geboorte <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die t'verstandt te boven <rhyme label="b" type="m">gaet</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Daer Godt thoont syn wonder <rhyme label="a" type="f" corresp="#s1:l1">dingen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En syn liefde boven <rhyme label="b" type="m" corresp="#s1:l2">maet</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Dat syn lof, en <rhyme label="c" type="f">mogentheden</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Geven door de locht <rhyme label="d" type="m">geklanck</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Laet ons voegen ons <rhyme label="c" type="f" corresp="#s1:l5">gebeden</rhyme></l>
        <l id="s1:l8" met="4" type="+-">By des Engels soet <rhyme label="d" type="m" corresp="#s1:l6">gesanck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wie kan dit oyt <rhyme label="a" type="f">achterhaelen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat het is geweest een <rhyme label="b" type="m">stal</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Om de glori' te <rhyme label="a" type="f" corresp="#s2:l1">bepaelen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van de Majesteyt van <rhyme label="b" type="m" corresp="#s2:l2">al</rhyme>!</l>
        <l id="s2:l5" met="4" type="+-">Die is eeuwigh, wordt <rhyme label="c" type="f">geboren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Die niet lyden kan, lydt <rhyme label="d" type="m">pyn</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">t'Woordt heeft syne spraek <rhyme label="c" type="f" corresp="#s2:l5">verloren</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">En de Sonn' is sonder <rhyme label="d" type="m" corresp="#s2:l6">schyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Het voorseggen der <rhyme label="a" type="f">Propheten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Desen dagh ons <rhyme label="b" type="m">uyt-geleyt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wordt volbrocht, gelyck wy <rhyme label="a" type="f" corresp="#s3:l1">weten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Door dees groote <rhyme label="b" type="m" corresp="#s3:l2">wonderheydt</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Eene Moeder, Maeght <rhyme label="c" type="f">gebleven</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Heeft verjaeght de <rhyme label="d" type="m">duysterheydt</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">De figuren, ons <rhyme label="c" type="f" corresp="#s3:l5">gegeven</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Wycken voor de <rhyme label="d" type="m" corresp="#s3:l6">klaerigheydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Naer den oorlogh komt den <rhyme label="a" type="f">vrede</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Godt stelt sich voor onsen <rhyme label="b" type="m">kant</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">d'Aerd' en wordt niet meer <rhyme label="a" type="f" corresp="#s4:l1">bestreden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">t'Hemels-hof benydt dien <rhyme label="b" type="m" corresp="#s4:l2">pant</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Ah! hoe groot werck laet Godt <rhyme label="c" type="f">blyken</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Als hy heden <rhyme label="d" type="m">neder-daelt</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Syne gramschap doet hy <rhyme label="c" type="f" corresp="#s4:l5">wycken</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En de liefde <rhyme label="d" type="m" corresp="#s4:l6">zegenpraelt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
