    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAt grepen heeft de <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ey ziet, hoe hy myn <rhyme label="a" type="m" corresp="#s1:l1">zin</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Dwingt tot de gene die ik <rhyme label="b" type="m">weet</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat aan een aar haar liefd' <rhyme label="b" type="m" corresp="#s1:l3">besteed</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En weder een tedere maagt <rhyme label="c" type="m">beknelt</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Dat zy in myn gewelt Haar leven wil <rhyme label="d" type="f">geven</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Maar word van my weer heel te rug <rhyme label="c" type="m" corresp="#s1:l5">gestelt</rhyme>.</l>
      </lg>
