<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>30095</id>
    <date>
      <year-start>1591</year-start>
      <year-end>1591</year-end>
    </date>
    <location>
      <orginal-name>middelburg</orginal-name>
      <standard-name>Middelburg, The Netherlands</standard-name>
      <coordinates>
        <lon>51.4997302</lon>
        <lat>3.6136962</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 136 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Philips van Marnix van Sint Aldegonde</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DAnckt God, geeft hem prijs end <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Want hy is genadich <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ende sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s1:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Loift Gods naem end <rhyme label="a" type="m">heugenis</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Die God aller Goden <rhyme label="a" type="m" corresp="#s2:l1">is</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Omdat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s2:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Loift God nv end <rhyme label="a" type="m">immermeer</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Die is aller Heeren <rhyme label="a" type="m" corresp="#s3:l1">Heer</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s3:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy die alleen wijs end <rhyme label="a" type="m">vroet</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Groote wonder wercken <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s4:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Die met wijsheyt <rhyme label="a" type="m">wonderbaer</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Heeft gemaect den Hemel <rhyme label="a" type="m" corresp="#s5:l1">claer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s5:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">End den kloot des eirdrijcks <rhyme label="a" type="m">rondt</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Uytgebreyt op swaters <rhyme label="a" type="m" corresp="#s6:l1">grondt</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s6:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die de groote lichten <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Heeft gehecht aen shemels <rhyme label="a" type="m" corresp="#s7:l1">throon</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s7:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Namelijck de Sonne <rhyme label="a" type="m">heet</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Die de heersching sdaechs <rhyme label="a" type="m" corresp="#s8:l1">beleedt</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s8:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">End de Mane die by <rhyme label="a" type="m">nacht</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Met de sterren bruyckt haer <rhyme label="a" type="m" corresp="#s9:l1">cracht</rhyme>.</l>
        <l id="s9:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s9:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Die Egypten heeft <rhyme label="a" type="m">berooft</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Van elck eerst geboren <rhyme label="a" type="m" corresp="#s10:l1">hooft</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s10:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">End heeft van hun <rhyme label="a" type="m">afgescheyt</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Israel, end <rhyme label="a" type="m" corresp="#s11:l1">uytgeleyt</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s11:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s11:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Met sijn handt end stercken <rhyme label="a" type="m">erm</rhyme>:</l>
        <l id="s12:l2" met="4" type="+-">Nemend' hem in sijnen <rhyme label="a" type="m" corresp="#s12:l1">scherm</rhyme>.</l>
        <l id="s12:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s12:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Die de roode biesen <rhyme label="a" type="m">zee</rhyme></l>
        <l id="s13:l2" met="4" type="+-">Heeft met cracht gedeylt in <rhyme label="a" type="m" corresp="#s13:l1">twee</rhyme>:</l>
        <l id="s13:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s13:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s13:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">End gevoert tvolck <rhyme label="a" type="m">Israel</rhyme></l>
        <l id="s14:l2" met="4" type="+-">Tusschen hare baren <rhyme label="a" type="m" corresp="#s14:l1">fel</rhyme>:</l>
        <l id="s14:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s14:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s14:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Die gestortet heeft int <rhyme label="a" type="m">meyr</rhyme></l>
        <l id="s15:l2" met="4" type="+-">Pharao, met al sijn <rhyme label="a" type="m" corresp="#s15:l1">heyr</rhyme>:</l>
        <l id="s15:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s15:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s15:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Die geleyt heeft vranck end <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s16:l2" met="4" type="+-">Sijn volck doir de <rhyme label="a" type="m" corresp="#s16:l1">woesteny</rhyme>:</l>
        <l id="s16:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s16:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s16:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Die geslagen heeft ter <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s17:l2" met="4" type="+-">Coningen end Vorsten <rhyme label="a" type="m" corresp="#s17:l1">groot</rhyme>:</l>
        <l id="s17:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s17:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s17:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Coningen van groote <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s18:l2" met="4" type="+-">Heeft hy schielijck <rhyme label="a" type="m" corresp="#s18:l1">omgebracht</rhyme>:</l>
        <l id="s18:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s18:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s18:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Sichon die in <rhyme label="a" type="m">Emorlant</rhyme></l>
        <l id="s19:l2" met="4" type="+-">Sijnen schepter had <rhyme label="a" type="m" corresp="#s19:l1">geplant</rhyme>:</l>
        <l id="s19:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheydt</rhyme></l>
        <l id="s19:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s19:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">End Og, die op tselve <rhyme label="a" type="m">pas</rhyme></l>
        <l id="s20:l2" met="4" type="+-">Over Basan Coninck <rhyme label="a" type="m" corresp="#s20:l1">was</rhyme>:</l>
        <l id="s20:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s20:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s20:l3">eewicheydt</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Ende schanck ter selver <rhyme label="a" type="m">stondt</rhyme></l>
        <l id="s21:l2" met="4" type="+-">Tot een erf, haer Landt end <rhyme label="a" type="m" corresp="#s21:l1">grondt</rhyme>:</l>
        <l id="s21:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s21:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s21:l3">eewicheydt</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Tot een erf end <rhyme label="a" type="m">kinderrecht</rhyme></l>
        <l id="s22:l2" met="4" type="+-">Aen Israel sijnen <rhyme label="a" type="m" corresp="#s22:l1">knecht</rhyme>:</l>
        <l id="s22:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s22:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s22:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Hy die op ons heeft <rhyme label="a" type="m">gedacht</rhyme></l>
        <l id="s23:l2" met="4" type="+-">Als wy tonder waren <rhyme label="a" type="m" corresp="#s23:l1">bracht</rhyme>:</l>
        <l id="s23:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s23:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s23:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Ende heeft ons alle <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s24:l2" met="4" type="+-">Van ons vyanden <rhyme label="a" type="m" corresp="#s24:l1">bevrijt</rhyme>:</l>
        <l id="s24:l3" met="4" type="+-">Om dat sijn groote <rhyme label="b" type="m">gunsticheydt</rhyme></l>
        <l id="s24:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s24:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">Die spijs allen vleesche <rhyme label="a" type="m">geeft</rhyme></l>
        <l id="s25:l2" met="4" type="+-">Ende voedt al watter <rhyme label="a" type="m" corresp="#s25:l1">leeft</rhyme>:</l>
        <l id="s25:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme></l>
        <l id="s25:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s25:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">Loift end dancket lanx so <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s26:l2" met="4" type="+-">God des hemels God end <rhyme label="a" type="m" corresp="#s26:l1">Heer</rhyme>:</l>
        <l id="s26:l3" met="4" type="+-">Om dat sijn <rhyme label="b" type="m">goetgunsticheyt</rhyme>.</l>
        <l id="s26:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s26:l3">eewicheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
