  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">Laat ons, laat ons de Min vereenen hert en <rhyme label="a" type="m">ziel</rhyme>,</l>
        <l id="s1:l2" met="6" type="+-">Ach! ach! ach! dat dit uw <rhyme label="a" type="m" corresp="#s1:l1">geviel</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Gaat offert hem uw <rhyme label="b" type="m">jeugt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">'t Is de vreugd van 't Lente <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Niets kan hem meerder <rhyme label="b" type="m" corresp="#s1:l3">vreugd</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Die mint in eer en <rhyme label="b" type="m" corresp="#s1:l3">deugd</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">In 't leeven <rhyme label="c" type="f" corresp="#s1:l4">geeven</rhyme>.</l>
      </lg>
    </lg>
  </text>
