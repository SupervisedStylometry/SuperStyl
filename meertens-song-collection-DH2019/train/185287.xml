<song>
  <meta>
    <id>185287</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mijn lieve Saartje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Myn lieve Saartje, die <rhyme label="a" type="m">altyd</rhyme></l>
        <l id="s1:l2" met="3" type="+-">U Potloot en <rhyme label="b" type="f">Blankett</rhyme></l>
        <l id="s1:l3" met="4" type="+-">'k Wou dat gy in de Modder <rhyme label="a" type="m" corresp="#s1:l1">viel</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Om u in 't nieuw te <rhyme label="b" type="f" corresp="#s1:l2">zetten</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ik zal u maken een <rhyme label="c" type="m">Japon</rhyme>:</l>
        <l id="s1:l6" met="3" type="+-">Voor u buyk als een <rhyme label="c" type="m" corresp="#s1:l5">zak</rhyme>.</l>
        <l id="s1:l7" met="3" type="+-">Voort, voort myn lieve <rhyme label="d" type="f">Saartje</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Al bent gy <rhyme label="e" type="m">schief</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Ik heb u <rhyme label="e" type="m" corresp="#s1:l8">lief</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">En hou wel van een <rhyme label="f" type="f">praatje</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Al met myn zoete <rhyme label="e" type="m" corresp="#s1:l8">lief</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">U Boggel die pareert <rhyme label="a" type="m">altyd</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Al zit gy maar te <rhyme label="b" type="f">braaje</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">U kostelyke <rhyme label="a" type="m" corresp="#s2:l1">puntigheid</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Dat doet u aardig <rhyme label="b" type="f" corresp="#s2:l2">naaije</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">U schoonheid diend <rhyme label="c" type="m">afgemaalt</rhyme>?</l>
        <l id="s2:l6" met="3" type="+-">En die dat doed die raakt <rhyme label="c" type="m" corresp="#s2:l5">verdwaald</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Voort, voort myn lieve <rhyme label="d" type="f">Saartje</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Al bent gy <rhyme label="e" type="m">schief</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Ik heb u <rhyme label="e" type="m" corresp="#s2:l8">lief</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">En hou wel van een <rhyme label="f" type="f">praatje</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Al met myn zoete <rhyme label="e" type="m" corresp="#s2:l8">lief</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">U Druypneus die een elk <rhyme label="a" type="m">bekoort</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Die kan men niet <rhyme label="b" type="f">waardeeren</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">U Oogen die staan in u <rhyme label="a" type="m" corresp="#s3:l1">hooft</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Als een Paar rype <rhyme label="b" type="f" corresp="#s3:l2">sweeren</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Dat komt al van de <rhyme label="c" type="m">Snuyf</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">En droppels die gy <rhyme label="c" type="m" corresp="#s3:l5">gebruykt</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Voort, voort myn lieve <rhyme label="d" type="f">Saartje</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Al bent gy <rhyme label="e" type="m">schief</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Ik heb u <rhyme label="e" type="m" corresp="#s3:l8">lief</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">En hou wel van een <rhyme label="f" type="f">praatje</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Al met myn zoete <rhyme label="e" type="m" corresp="#s3:l8">lief</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">U ouderdom belet u <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Zoo snoepig uit te <rhyme label="b" type="f">kyken</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wanneer ik u myn dienst aan <rhyme label="a" type="m" corresp="#s4:l1">biet</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Zou het u hartje <rhyme label="b" type="f" corresp="#s4:l2">lyken</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Ik zei myn lieve <rhyme label="c" type="m">Meid</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Gy zeid u Jaaren <rhyme label="c" type="m" corresp="#s4:l5">kwyt</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Voort, voort myn lieve <rhyme label="d" type="f">Saartje</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Al bent gy <rhyme label="e" type="m">schief</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Ik heb u <rhyme label="e" type="m" corresp="#s4:l8">lief</rhyme></l>
        <l id="s4:l10" met="3" type="+-">En hou wel van een <rhyme label="f" type="f">praatje</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Al met myn zoete <rhyme label="e" type="m" corresp="#s4:l8">lief</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">U Borsjes pronken aan u <rhyme label="a" type="m">hart</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Zy zyn als Rooije <rhyme label="b" type="f">koolen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">U Lipjes als geslepe <rhyme label="a" type="m" corresp="#s5:l1">kraal</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Die doen een Jonkman <rhyme label="b" type="f" corresp="#s5:l2">doolen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Ik zei myn lieve <rhyme label="c" type="m">kind</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Word gy zoo sterk <rhyme label="c" type="m" corresp="#s5:l5">bemind</rhyme>?</l>
        <l id="s5:l7" met="3" type="+-">Voort, voort myn lieve <rhyme label="d" type="f">Saartje</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Al bent gy <rhyme label="e" type="m">schief</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Ik heb u <rhyme label="e" type="m" corresp="#s5:l8">lief</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">En hou wel van een <rhyme label="f" type="f">praatje</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Al met myn zoete <rhyme label="e" type="m" corresp="#s5:l8">lief</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ik zei myn lieve <rhyme label="a" type="m">Ziels-vrindin</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Ik denk u nooit te <rhyme label="b" type="f">trouwen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Ik stel geheel u uit myn <rhyme label="a" type="m" corresp="#s6:l1">zin</rhyme>;</l>
        <l id="s6:l4" met="3" type="+-">Want ziet het mogt myn <rhyme label="b" type="f" corresp="#s6:l2">rouwen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">U troost die is de <rhyme label="c" type="m">Fles</rhyme></l>
        <l id="s6:l6" met="3" type="+-">En ik wensch u het <rhyme label="c" type="m" corresp="#s6:l5">best</rhyme></l>
        <l id="s6:l7" met="3" type="+-">Voort, voort myn lieve <rhyme label="d" type="f">Saartje</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-">Al bent gy <rhyme label="e" type="m">schief</rhyme>,</l>
        <l id="s6:l9" met="2" type="+-">Ik heb u <rhyme label="e" type="m" corresp="#s6:l8">lief</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">En hou wel van een <rhyme label="f" type="f">praaje</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Al met myn zoete <rhyme label="e" type="m" corresp="#s6:l8">lief</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
