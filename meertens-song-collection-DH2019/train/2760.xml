    <id>2760</id>
    <date>
      <year-start>1695</year-start>
      <year-end>1695</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="meilied" translation="May song">meilied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Venus gij en uw kind</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Bernardus Busschoff</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe lustigh en hoe <rhyme label="a" type="m">groen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Is 't in dit Meys <rhyme label="a" type="m" corresp="#s1:l1">saysoen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hoe lieflijck ist nu <rhyme label="b" type="f">buyten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Nu all' de jonge <rhyme label="b" type="f" corresp="#s1:l3">spruyten</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van gras, van kruyt, van <rhyme label="c" type="f">bloemen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Om strijdt den Heere <rhyme label="c" type="f" corresp="#s1:l5">roemen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al 't Veldt is nu ter <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als een schoon groen <rhyme label="a" type="m" corresp="#s2:l1">tapijt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat onlanghs stont als <rhyme label="b" type="f">Heyde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Is nu gras-rijcke <rhyme label="b" type="f" corresp="#s2:l3">weyde</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En d'Ackers met haer <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Staen groen van 't vuchtbaer <rhyme label="c" type="f" corresp="#s2:l5">Koren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Boomen hoogh en <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die onlanghs scheenen <rhyme label="a" type="m" corresp="#s3:l1">doodt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Als dorr' en gantsch <rhyme label="b" type="f">bevroren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Staen, als van nieuws <rhyme label="b" type="f" corresp="#s3:l3">geboren</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Met bladeren en <rhyme label="c" type="f">knoppen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Verciert tot in de <rhyme label="c" type="f" corresp="#s3:l5">toppen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De vogelkens <rhyme label="a" type="m">rondtsom</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die onlanghs scheenen <rhyme label="a" type="m" corresp="#s4:l1">stom</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Nu weer met held're <rhyme label="b" type="f">keelen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den lof haer 's Scheppers <rhyme label="b" type="f" corresp="#s4:l3">queelen</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Den Leeuw'rick klimt na <rhyme label="c" type="f">boven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Om daer den Heer te <rhyme label="c" type="f" corresp="#s4:l5">loven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Elck Schepsel na sijn <rhyme label="a" type="m">aert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sijn Scheppers lof <rhyme label="a" type="m" corresp="#s5:l1">verklaert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Komt edel <rhyme label="b" type="f">Creatuere</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Komt leert van de <rhyme label="b" type="f" corresp="#s5:l3">Natuere</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Komt Mensch, komt leert <rhyme label="c" type="f">slaen-gade</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Den Mey-tijdt der <rhyme label="c" type="f" corresp="#s5:l5">genade</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De Son aen 't <rhyme label="a" type="m">Firmament</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Haer kracht nu <rhyme label="a" type="m" corresp="#s6:l1">neder-sent</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Nu is sy hoogh <rhyme label="b" type="f">geresen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Stracks sietmen ander <rhyme label="b" type="f" corresp="#s6:l3">wesen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Aen boomen, kruyden, <rhyme label="c" type="f">dieren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Selfs aen de kleyne <rhyme label="c" type="f" corresp="#s6:l5">Mieren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Het krachtigh <rhyme label="a" type="m">Sonne-licht</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van Godts vrien'lijck <rhyme label="a" type="m" corresp="#s7:l1">gesicht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">'T welck breeckt de harde <rhyme label="b" type="f">steenen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Heeft langh u hert <rhyme label="b" type="f" corresp="#s7:l3">bescheenen</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Noch staet gy als te <rhyme label="c" type="f">vooren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Verkout, en gantsch <rhyme label="c" type="f" corresp="#s7:l5">vervrooren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Gy blijft van vrucht soo <rhyme label="a" type="m">bloodt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Als waert gy dorr' en <rhyme label="a" type="m" corresp="#s8:l1">doodt</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Soo eenigh sap en <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Is in u stam <rhyme label="b" type="f" corresp="#s8:l3">gebleven</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Begint nu vrucht te <rhyme label="c" type="f">dragen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In dees soo vruchtbaer <rhyme label="c" type="f" corresp="#s8:l5">dagen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Rijst uyt der sonden <rhyme label="a" type="m">stof</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Wast tot uw's Scheppers <rhyme label="a" type="m" corresp="#s9:l1">lof</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Als wel-geplante <rhyme label="b" type="f">Boomen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Nu sijn <rhyme label="b" type="f" corresp="#s9:l3">genaden-stroomen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">U wortel soo <rhyme label="c" type="f">besproeyen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Pooght nu in deught te <rhyme label="c" type="f" corresp="#s9:l5">groeyen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Laet Heer u hemelsch <rhyme label="a" type="m">vocht</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Af-druypen door de <rhyme label="a" type="m" corresp="#s10:l1">locht</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Laet op ons herten <rhyme label="b" type="f">dalen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Uw' aenschijns <rhyme label="b" type="f" corresp="#s10:l3">Sonne-stralen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Soo sullen wy <rhyme label="c" type="f">ontluycken</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Als Roosen op haer <rhyme label="c" type="f" corresp="#s10:l5">struycken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
