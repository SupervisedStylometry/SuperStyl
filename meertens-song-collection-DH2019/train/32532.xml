    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CLorind! als ick op <rhyme label="a" type="m">wegh</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Al u <rhyme label="b" type="f">besonderheden</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Alleen die <rhyme label="a" type="m" corresp="#s1:l1">overlegh</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Van u volmaeckte <rhyme label="b" type="f" corresp="#s1:l2">Leden</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die my, geduerich <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Voor d'oogen; in mijn <rhyme label="d" type="f">herte</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Voel ick alsdan <rhyme label="c" type="m" corresp="#s1:l5">vergaen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Mijn ingedruckte <rhyme label="d" type="f" corresp="#s1:l6">smerte</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">U Ooghskens ryn, en <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">My t' herte heel <rhyme label="b" type="f">door-wonden</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Doen in u gulden <rhyme label="a" type="m" corresp="#s2:l1">haer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Mijn ziel eerst wirdt <rhyme label="b" type="f" corresp="#s2:l2">gebonden</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">O soet! Ã´ God'lijck <rhyme label="c" type="m">saet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die t'Palladis <rhyme label="d" type="f">geslachte</rhyme></l>
        <l id="s2:l7" met="3" type="-+">In als te boven <rhyme label="c" type="m" corresp="#s2:l5">gaet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Door wysheyt, en in <rhyme label="d" type="f" corresp="#s2:l6">krachte</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die kondt den droeven <rhyme label="a" type="m">geest</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met vreughden doen <rhyme label="b" type="f">verlichten</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Den krancken <rhyme label="a" type="m" corresp="#s3:l1">al-der-meest</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Met uwe MINNE <rhyme label="b" type="f" corresp="#s3:l2">schichten</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Daer oock den wysten <rhyme label="c" type="m">tracht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">By u Clorind te <rhyme label="d" type="f">wesen</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Want u een jeder <rhyme label="c" type="m" corresp="#s3:l5">acht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Midts gy die condt <rhyme label="d" type="f" corresp="#s3:l6">genesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'t Gesicht van u <rhyme label="a" type="m">aenschyn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Is zedigh boven <rhyme label="b" type="f">maten</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En u twee Borstjens <rhyme label="a" type="m" corresp="#s4:l1">syn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Als geurige <rhyme label="b" type="f" corresp="#s4:l2">Granaten</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Waer uyt heel rynich <rhyme label="c" type="m">vloeyt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Mijn aertsche milde <rhyme label="d" type="f">vreugden</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Mijn Liefde soo <rhyme label="c" type="m" corresp="#s4:l5">besproeyt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En groeyen doet in <rhyme label="d" type="f" corresp="#s4:l6">deughden</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En uwe Lipkens <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">U roode Roose <rhyme label="b" type="f">wanghen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Verwecken t samen <rhyme label="a" type="m" corresp="#s5:l1">my</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Om minne Lust te <rhyme label="b" type="f" corresp="#s5:l2">vanghen</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">O suyvere <rhyme label="c" type="m">Goddin</rhyme>!</l>
        <l id="s5:l6" met="3" type="-+">Laet my dogh eens naer <rhyme label="d" type="f">luste</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Aenraecken; mijnen <rhyme label="c" type="m" corresp="#s5:l5">sin</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die stelde hem tot <rhyme label="d" type="f" corresp="#s5:l6">ruste</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
