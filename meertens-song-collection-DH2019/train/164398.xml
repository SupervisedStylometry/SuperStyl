<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164398</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat voor dagen heeft een hond</melody>
  <author>Aernout van Overbeke</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WElcke droefheyt baert een <rhyme label="a" type="m">hondt</rhyme>?</l>
        <l id="s1:l2" met="4" type="+-">Welcke droefheyt baert een <rhyme label="a" type="m" corresp="#s1:l1">hondt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Een getrouwe wacht van <rhyme label="b" type="f">braven</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Die in 't water wort <rhyme label="b" type="f" corresp="#s1:l3">begraven</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s1:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s1:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Gout noch silver konde <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Schoon 't soo meenigh Vorst <rhyme label="a" type="m" corresp="#s2:l1">verriet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">'t Edelmoedigh beest <rhyme label="b" type="f">bekooren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Dat het liet sijn Heer <rhyme label="b" type="f" corresp="#s2:l3">verlooren</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s2:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s2:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Het verstandigh beesje <rhyme label="a" type="m">vloogh</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Op een wencken van sijn <rhyme label="a" type="m" corresp="#s3:l1">oogh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En op handt en <rhyme label="b" type="f">vinger-teecken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Beter als veel knechts door 't <rhyme label="b" type="f" corresp="#s3:l3">spreecken</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s3:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s3:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als hy op een <rhyme label="a" type="m">biesesprey</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Wees, hoe aerdigh in de <rhyme label="a" type="m" corresp="#s4:l1">Mey</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Meysjes leggen op haer <rhyme label="b" type="f">rugge</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dan wiert selfs Cupido <rhyme label="b" type="f" corresp="#s4:l3">vlugge</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s4:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s4:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Doen hy met een stercke <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Stiet het slagh-swaert uit de <rhyme label="a" type="m" corresp="#s5:l1">handt</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Van manhafte <rhyme label="b" type="f">Soudenieren</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Toen wiert Mars vol <rhyme label="b" type="f" corresp="#s5:l3">minne-vieren</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s5:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s5:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hadt Diaen hem op de <rhyme label="a" type="m">jacht</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Mogen hebben in haer <rhyme label="a" type="m" corresp="#s6:l1">macht</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Om voor onraedt niet te <rhyme label="b" type="f">vreesen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Souw hy 't hooft der bosch-wacht <rhyme label="b" type="f" corresp="#s6:l3">wesen</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s6:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s6:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'t Geestigh lijf dat <rhyme label="a" type="m">quispelsteert</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Strijcke-voet en helden <rhyme label="a" type="m" corresp="#s7:l1">eert</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Kost aen plompe boots-luy <rhyme label="b" type="f">leeren</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dat men moet ontsien sijn <rhyme label="b" type="f" corresp="#s7:l3">Heeren</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s7:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s7:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Voor de minne tot de <rhyme label="a" type="m">hondt</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Die hy trouw en vriend'lijck <rhyme label="a" type="m" corresp="#s8:l1">vondt</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Kan in wack're Hopman <rhyme label="b" type="f">v</rhyme>...</l>
        <l id="s8:l4" met="4" type="+-">Geene min tot vrouwen <rhyme label="b" type="f" corresp="#s8:l3">halen</rhyme>?</l>
        <l id="s8:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s8:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s8:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">'t Beelt des honts verstoort sijn <rhyme label="a" type="m">lust</rhyme>;</l>
        <l id="s9:l2" met="4" type="+-">Hy heeft nacht noch dagh geen <rhyme label="a" type="m" corresp="#s9:l1">rust</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Sangh noch spel hem kan <rhyme label="b" type="f">bekooren</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Nu hy heeft de hondt <rhyme label="b" type="f" corresp="#s9:l3">verlooren</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s9:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s9:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Al de werelt hadt hem <rhyme label="a" type="m">lief</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Mits hy roover was noch <rhyme label="a" type="m" corresp="#s10:l1">dief</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Schoon hy kassen en <rhyme label="b" type="f">tresooren</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Op en toe deed' na <rhyme label="b" type="f" corresp="#s10:l3">behooren</rhyme>.</l>
        <l id="s10:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s10:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s10:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Beesje, spiegel van de <rhyme label="a" type="m">deught</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Gy benaemt uw Heer geen <rhyme label="a" type="m" corresp="#s11:l1">vreught</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Hadt ghy in der Leeuwen <rhyme label="b" type="f">kaken</rhyme></l>
        <l id="s11:l4" met="4" type="+-">'t Leven vechtend' mogen <rhyme label="b" type="f" corresp="#s11:l3">braken</rhyme>.</l>
        <l id="s11:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s11:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s11:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Sneuvelt om een goede <rhyme label="a" type="m">saeck</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">d'Oorloghs-helt met <rhyme label="a" type="m" corresp="#s12:l1">weder-wraeck</rhyme>;</l>
        <l id="s12:l3" met="4" type="+-">Een grootmoedigh Prins te <rhyme label="b" type="f">minder</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Reeckent de geleden <rhyme label="b" type="f" corresp="#s12:l3">hinder</rhyme>.</l>
        <l id="s12:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s12:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s12:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Maer, nu 't honts-gestarnte <rhyme label="a" type="m">blaft</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">En veel stilte op Zee <rhyme label="a" type="m" corresp="#s13:l1">verschaft</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Slaet de Doodt, die helsche <rhyme label="b" type="f">prye</rhyme></l>
        <l id="s13:l4" met="4" type="+-">U in 't breyn met <rhyme label="b" type="f" corresp="#s13:l3">rasernye</rhyme>.</l>
        <l id="s13:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s13:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s13:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Gy hadt vogels van <rhyme label="a" type="m">Jupijn</rhyme></l>
        <l id="s14:l2" met="4" type="+-">Noch tot voedsel konnen <rhyme label="a" type="m" corresp="#s14:l1">zyn</rhyme>;</l>
        <l id="s14:l3" met="4" type="+-">Maer, Ã´ smaet! niet uyt te <rhyme label="b" type="f">wisschen</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Ghy moet zijn het aes der <rhyme label="b" type="f" corresp="#s14:l3">visschen</rhyme>.</l>
        <l id="s14:l5" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m">hondt</rhyme>?</l>
        <l id="s14:l6" met="4" type="+-">Welcke droefheyt baert een <rhyme label="c" type="m" corresp="#s14:l5">hondt</rhyme>?</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Maer, de schoon' <rhyme label="a" type="m">Urania</rhyme></l>
        <l id="s15:l2" met="4" type="+-">Slaet mijn klachten lacchend' <rhyme label="a" type="m" corresp="#s15:l1">ga</rhyme>!</l>
        <l id="s15:l3" met="4" type="+-">Nu sy in het <rhyme label="b" type="f">honde-teecken</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Siet een tintellicht <rhyme label="b" type="f" corresp="#s15:l3">ontsteecken</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Tot een eere van den <rhyme label="c" type="m">hondt</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">Tot een eere van den <rhyme label="c" type="m" corresp="#s15:l5">hondt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Spoel dan Hopman uwe <rhyme label="a" type="m">smart</rhyme></l>
        <l id="s16:l2" met="4" type="+-">Met een glaesje van het <rhyme label="a" type="m" corresp="#s16:l1">hart</rhyme>:</l>
        <l id="s16:l3" met="4" type="+-">Want een starre wort <rhyme label="b" type="f">gebooren</rhyme></l>
        <l id="s16:l4" met="4" type="+-">Tot een eer van <rhyme label="b" type="f" corresp="#s16:l3">d'uytverkooren</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Tot een eere van den <rhyme label="c" type="m">hondt</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">Tot een eere van den <rhyme label="c" type="m" corresp="#s16:l5">hondt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
