  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Onder het groen <rhyme label="a" type="m">Geboomt</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Daar het water <rhyme label="a" type="m" corresp="#s1:l1">stroomt</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Daar het pluym gediert zingt in ’t <rhyme label="a" type="m" corresp="#s1:l1">geboomt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In ’t Groene <rhyme label="b" type="m">dal</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Hoort men de <rhyme label="b" type="m" corresp="#s1:l4">Nagtegaal</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Het moet wijke voor hem <rhyme label="b" type="m" corresp="#s1:l4">altemaal</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Hy zingt zo zoet, zo zoet, <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Hij zingt zoo zoet, zoet <rhyme label="c" type="m" corresp="#s1:l7">zoet</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">So zingt de <rhyme label="d" type="m">Nagtegaal</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Met zijn minnelyke <rhyme label="d" type="m" corresp="#s1:l9">Taal</rhyme>,</l>
        <l id="s1:l11" met="5" type="+-">Het moet weijke voor hem <rhyme label="d" type="m" corresp="#s1:l9">altemaal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al de Muize te <rhyme label="a" type="m">zaam</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Die zyn niet <rhyme label="a" type="m" corresp="#s2:l1">bekwaam</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Om te zingen zoet, zo zagt en <rhyme label="a" type="m" corresp="#s2:l1">aangenaam</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Want wat is in haar <rhyme label="b" type="m">klank</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Sonder Bachus <rhyme label="b" type="m" corresp="#s2:l4">Drank</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Het is welstand al voor ons <rhyme label="b" type="m" corresp="#s2:l4">Land</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Hy zingt zo zoet, zo zoet, <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Hij zingt zoo zoet, zoet <rhyme label="c" type="m" corresp="#s2:l7">zoet</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Al komt de Lyster <rhyme label="d" type="m">uyt</rhyme>.</l>
        <l id="s2:l10" met="4" type="+-">In zijn vol <rhyme label="d" type="m" corresp="#s2:l9">gefluyt</rhyme>,</l>
        <l id="s2:l11" met="5" type="+-">Hy moet wijken voor het zoet <rhyme label="d" type="m" corresp="#s2:l9">geluyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">‘Smorgens vroeg met den <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Als de Son zijn <rhyme label="a" type="m" corresp="#s3:l1">kragt</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Dan zo dryf ik mijn Scaapjes op de <rhyme label="a" type="m" corresp="#s3:l1">jagt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Over Berge en <rhyme label="b" type="m">Daal</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Hoort men de <rhyme label="b" type="m" corresp="#s3:l4">Nagtegaal</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Het moet wijke voor hem <rhyme label="b" type="m" corresp="#s3:l4">altemaal</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Hy zingt zo zoet, zo zoet, <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hij zingt zoo zoet, zoet <rhyme label="c" type="m" corresp="#s3:l7">zoet</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Onder het groene <rhyme label="d" type="m">kruyt</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Daar hij ook al <rhyme label="d" type="m" corresp="#s3:l9">fluyt</rhyme>,</l>
        <l id="s3:l11" met="5" type="+-">Het moet wijke voor zijn zoet <rhyme label="d" type="m" corresp="#s3:l9">geluyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dan zo gaan ik vol <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Nam zijn <rhyme label="a" type="m" corresp="#s4:l1">herderin</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Daar wy zame gaan zitte na onze <rhyme label="a" type="m" corresp="#s4:l1">zin</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Daar wy <rhyme label="b" type="m">onbemint</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Zitten in ’t Groene <rhyme label="b" type="m" corresp="#s4:l4">Wout</rhyme>.</l>
        <l id="s4:l6" met="5" type="-+">Daar de Nagtegaal ons <rhyme label="b" type="m" corresp="#s4:l4">onderhout</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Hy zingt zo zoet, zo zoet, <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Hij zingt zoo zoet, zoet <rhyme label="c" type="m" corresp="#s4:l7">zoet</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Dat aangename <rhyme label="d" type="m">dier</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Voelt ook het <rhyme label="d" type="m" corresp="#s4:l9">Minnevier</rhyme>.</l>
        <l id="s4:l11" met="5" type="+-">Want hij zingt zeer netjes na de <rhyme label="d" type="m" corresp="#s4:l9">zwier</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Herders <rhyme label="a" type="m">allegaar</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Die komen by <rhyme label="a" type="m" corresp="#s5:l1">malkaar</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Met haar Herderinnen daar zy hooren <rhyme label="a" type="m" corresp="#s5:l1">naar</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Den Nagtegaal zeer <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Heeft zoo een zoeten <rhyme label="b" type="m" corresp="#s5:l4">toon</rhyme>,</l>
        <l id="s5:l6" met="5" type="-+">De Herders krijgen ook tot <rhyme label="b" type="m" corresp="#s5:l4">loon</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Hy zingt zo zoet, zo zoet, <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Hij zingt zoo zoet, zoet <rhyme label="c" type="m" corresp="#s5:l7">zoet</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Elk van zijn <rhyme label="d" type="m">Herderin</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Krygen in ’t <rhyme label="d" type="m" corresp="#s5:l9">begin</rhyme>.</l>
        <l id="s5:l11" met="5" type="+-">Maar daar na speelen het spel der <rhyme label="d" type="m" corresp="#s5:l9">min</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dan nog eens voor het <rhyme label="a" type="m">lest</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Ider doet zyn <rhyme label="a" type="m" corresp="#s6:l1">best</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Maar het Nagtegaaltje boven al de <rhyme label="a" type="m" corresp="#s6:l1">rest</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Hij zingt keurlyk en <rhyme label="b" type="m">net</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Indien gij ‘er wel op <rhyme label="b" type="m" corresp="#s6:l4">let</rhyme>.</l>
        <l id="s6:l6" met="5" type="-+">Want hy hout geduurig eene <rhyme label="b" type="m" corresp="#s6:l4">tret</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Hy zingt zo zoet, zo zoet, <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Hij zingt zoo zoet, zoet <rhyme label="c" type="m" corresp="#s6:l7">zoet</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Hij zingt moedig en <rhyme label="d" type="m">blij</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">In de Boomen <rhyme label="d" type="m" corresp="#s6:l9">vry</rhyme>,</l>
        <l id="s6:l11" met="5" type="+-">Voor al in de zoete maand van <rhyme label="d" type="m" corresp="#s6:l9">My</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
