    <tags/>
    <melody>Psalm 077 Datheen</melody>
  <author>François Halma</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HEere, neig tot my uw <rhyme label="a" type="f">ooren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hoor my, want door uwen <rhyme label="a" type="f" corresp="#s1:l1">tooren</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Ben ik in elende en <rhyme label="b" type="m">leet</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En nootdruftig, als gy <rhyme label="b" type="m" corresp="#s1:l3">weet</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Wil, Heer, mijne ziel <rhyme label="c" type="f">bewaaren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En uw' gunstgenoot toch <rhyme label="c" type="f" corresp="#s1:l5">spaaren</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">O myn Godt, verlos uw' <rhyme label="d" type="m">knecht</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Die op u betrouwt in 't <rhyme label="d" type="m" corresp="#s1:l7">recht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zyt my dan, ô Heer, <rhyme label="a" type="f">genadig</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Gy die goet zyt en <rhyme label="a" type="f" corresp="#s2:l1">weldadig</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want ik roep den ganschen <rhyme label="b" type="m">dag</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Tot u, met een zwaar <rhyme label="b" type="m" corresp="#s2:l3">geklag</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Kom de ziele dan <rhyme label="c" type="f">verblyden</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van uw' knecht in 't angstig <rhyme label="c" type="f" corresp="#s2:l5">lyden</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Want tot u, ô Heer, <rhyme label="d" type="m">verheft</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Zich myn ziel, dewyl 't haar <rhyme label="d" type="m" corresp="#s2:l7">treft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want gy, Heer, zyt <rhyme label="a" type="f">goedertieren</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Voor hen die uw wetten <rhyme label="a" type="f" corresp="#s3:l1">vieren</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Gy zyt goet voor al dat <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Als die gaarne hun <rhyme label="b" type="m" corresp="#s3:l3">vergeeft</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Die zich naar uw goedheit <rhyme label="c" type="f">wenden</rhyme>.</l>
        <l id="s3:l6" met="4" type="+-">Heer, wil my uw bystant <rhyme label="c" type="f" corresp="#s3:l5">zenden</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">'k Bidde dat uw oor toch <rhyme label="d" type="m">let</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Op myn smeeking en <rhyme label="d" type="m" corresp="#s3:l7">gebedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Roep u ten benaauden <rhyme label="a" type="f">dage</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Als beneepen door de <rhyme label="a" type="f" corresp="#s4:l1">plaage</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Die my drukt, en gy <rhyme label="b" type="m">verhoort</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Myne beeden, naar uw <rhyme label="b" type="m" corresp="#s4:l3">woordt</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Onder aller volken <rhyme label="c" type="f">Goden</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Is 'er niemant, die in <rhyme label="c" type="f" corresp="#s4:l5">nooden</rhyme></l>
        <l id="s4:l7" met="4" type="+-">U gelyk is, noch in <rhyme label="d" type="m">werk</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Boven alle paal en <rhyme label="d" type="m" corresp="#s4:l7">perk</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Alle heidens, die gy, <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Hebt geschapen, zullen <rhyme label="a" type="f" corresp="#s5:l1">eere</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Toonen aan uw <rhyme label="b" type="m">Majesteit</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En uw' naam, by hen <rhyme label="b" type="m" corresp="#s5:l3">verbreidt</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">En zich willig <rhyme label="c" type="f">nederbuigen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Voor uw aanschyn, als <rhyme label="c" type="f" corresp="#s5:l5">getuigen</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Van uw grootheit, datge <rhyme label="d" type="m">alleen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Godt zyt, groot in <rhyme label="d" type="m" corresp="#s5:l7">wonderheên</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Leer my, Heer, den weg <rhyme label="a" type="f">bewandlen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Van uw waarheit, en zoo <rhyme label="a" type="f" corresp="#s6:l1">handlen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat myn hart, van quaat <rhyme label="b" type="m">gespeent</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Tot uw vreeze wordt <rhyme label="b" type="m" corresp="#s6:l3">vereent</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Heer, myn Godt, ik zal u <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En verheffen 't hart naar <rhyme label="c" type="f" corresp="#s6:l5">boven</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">'k Zal uw' naame, met <rhyme label="d" type="m">bescheit</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Eeren tot in <rhyme label="d" type="m" corresp="#s6:l7">eeuwigheit</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Want uw <rhyme label="a" type="f">goedertierenheden</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Zyn doorluchtig, naar myn <rhyme label="a" type="f" corresp="#s7:l1">beden</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Over my, gy quaamt myn <rhyme label="b" type="m">ziel</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Toenze in doodts benaautheit <rhyme label="b" type="m" corresp="#s7:l3">viel</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Uit den diepsten grave <rhyme label="c" type="f">rukken</rhyme>.</l>
        <l id="s7:l6" met="4" type="+-">Heer, voor wien het al moet <rhyme label="c" type="f" corresp="#s7:l5">bukken</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Trotzen zyn nu <rhyme label="d" type="m">opgestaan</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Tegen my, die my <rhyme label="d" type="m" corresp="#s7:l7">verraân</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Zie de boozen in hun <rhyme label="a" type="f">kringen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">En hun <rhyme label="a" type="f" corresp="#s8:l1">moordvergaderingen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Tegen myne ziele in 't <rhyme label="b" type="m">veldt</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">'t Oog is niet op u <rhyme label="b" type="m" corresp="#s8:l3">gestelt</rhyme>.</l>
        <l id="s8:l5" met="4" type="+-">Maar, ô Heer, gy zyt <rhyme label="c" type="f">lankmoedig</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">En barmhartig, <rhyme label="c" type="f" corresp="#s8:l5">overvloedig</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">In uw <rhyme label="d" type="m">goedertierenheit</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">En uw waarheit, ons <rhyme label="d" type="m" corresp="#s8:l7">verbreidt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Wendt u, Heer, zyt my <rhyme label="a" type="f">genadig</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Geef toch uwen knecht <rhyme label="a" type="f" corresp="#s9:l1">miltdadig</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Sterkte en heil; verlos den <rhyme label="b" type="m">zoon</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Uwer dienstmaagdt uit den <rhyme label="b" type="m" corresp="#s9:l3">hoon</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Doe een teken my ten <rhyme label="c" type="f">goede</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Dat myn haters, in hun <rhyme label="c" type="f" corresp="#s9:l5">woede</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">'t Zien, beschaamt, wanneer gy, <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">My zult helpen t'uwer <rhyme label="d" type="m" corresp="#s9:l7">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
