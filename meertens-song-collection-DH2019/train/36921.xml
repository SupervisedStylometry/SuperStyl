    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>O droeve Echo hoor mijn klacht</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Livinus van der Minnen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">GHy zyt ô <rhyme label="a" type="m">schoonste</rhyme></l>
        <l id="s1:l2" met="2" type="+-"><rhyme label="b" type="m">Herderin</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">D'eyghen <rhyme label="a" type="m" corresp="#s1:l1">woonste</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Van de <rhyme label="b" type="m" corresp="#s1:l2">Min</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Door uw' <rhyme label="c" type="m">schoonheydt</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Die ten thoon <rhyme label="d" type="m">leydt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">In den <rhyme label="d" type="m" corresp="#s1:l6">schyn</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Van uw minnelyck <rhyme label="e" type="m">aen-schyn</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Uw tael’ Is <rhyme label="f" type="f">joviael</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">U lipkens blôsen’ Als de <rhyme label="f" type="f" corresp="#s1:l9">Rôsen</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">En <rhyme label="e" type="m" corresp="#s1:l8">Corael</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">
          <rhyme label="g" type="f">Peerle-banden</rhyme>
        </l>
        <l id="s1:l13" met="2" type="+-">Zyn uw' <rhyme label="h" type="f">tanden</rhyme>,</l>
        <l id="s1:l14" met="2" type="+-">D'Ooghskens <rhyme label="h" type="f" corresp="#s1:l13">schynen</rhyme></l>
        <l id="s1:l15" met="2" type="+-">Als den <rhyme label="i" type="m">fynen</rhyme></l>
        <l id="s1:l16" met="2" type="+-"><rhyme label="j" type="m">Diamant</rhyme>,</l>
        <l id="s1:l17" met="2" type="+-">Die my in’ Uwe <rhyme label="j" type="m" corresp="#s1:l16">min</rhyme></l>
        <l id="s1:l18" met="2" type="+-">Onsteckt, en <rhyme label="i" type="m" corresp="#s1:l15">brandt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Al is ons <rhyme label="a" type="m">Philis</rhyme></l>
        <l id="s2:l2" met="2" type="+-"><rhyme label="b" type="m">Minnelyck</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">
          <rhyme label="a" type="m" corresp="#s2:l1">Amarilis</rhyme>
        </l>
        <l id="s2:l4" met="2" type="+-"><rhyme label="b" type="m" corresp="#s2:l2">Vriendelyck</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Al is Flora’ En <rhyme label="c" type="m">Aurora</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Schoon en <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ghy zyt schoonder als sy <rhyme label="d" type="m" corresp="#s2:l6">zyn</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Ghy maeckt myn jeugt <rhyme label="e" type="m">verheugt</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Door uw schoon <rhyme label="f" type="f">leden</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">Eerbaer <rhyme label="f" type="f" corresp="#s2:l9">zeden</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Ende <rhyme label="e" type="m" corresp="#s2:l8">deught</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Uw soet wesen’ Noyt vol <rhyme label="g" type="f">presen</rhyme></l>
        <l id="s2:l13" met="2" type="+-">Seght van <rhyme label="h" type="f">binnen</rhyme></l>
        <l id="s2:l14" met="2" type="+-">Tot myn' <rhyme label="h" type="f" corresp="#s2:l13">sinnen</rhyme>,</l>
        <l id="s2:l15" met="2" type="+-">Wie kan <rhyme label="i" type="m">sien</rhyme></l>
        <l id="s2:l16" met="2" type="+-">Die schoon <rhyme label="j" type="m">Vrouw</rhyme></l>
        <l id="s2:l17" met="2" type="+-">Sonder <rhyme label="j" type="m" corresp="#s2:l16">Trouw</rhyme>,</l>
        <l id="s2:l18" met="2" type="+-">En dienst te <rhyme label="i" type="m" corresp="#s2:l15">bi</rhyme>[...]</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Uw' soete <rhyme label="a" type="m">lonckskens</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Gheestigh <rhyme label="b" type="m">Dier</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Zyn de <rhyme label="a" type="m" corresp="#s3:l1">vonckskens</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Van myn <rhyme label="b" type="m" corresp="#s3:l2">vier</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Uw teer handen’ Zyn de <rhyme label="c" type="m">banden</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Die myn <rhyme label="d" type="m">min</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Aen u binden schoon <rhyme label="d" type="m" corresp="#s3:l6">Vrindin</rhyme>;</l>
        <l id="s3:l8" met="3" type="-+">Ach! wat een soet <rhyme label="e" type="m">vertoogh</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Als de klaer <rhyme label="f" type="f">stralen</rhyme></l>
        <l id="s3:l10" met="2" type="+-">Die daer <rhyme label="f" type="f" corresp="#s3:l9">dalen</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Uyt uw' <rhyme label="e" type="m" corresp="#s3:l8">oogh</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Door hun blakẽ’ Den dagh <rhyme label="g" type="f">makẽ</rhyme>;</l>
        <l id="s3:l13" met="2" type="+-">En <rhyme label="h" type="f">verdwynen</rhyme></l>
        <l id="s3:l14" met="2" type="+-">Doen myn <rhyme label="h" type="f" corresp="#s3:l13">pynen</rhyme></l>
        <l id="s3:l15" met="2" type="+-">Door het <rhyme label="i" type="m">licht</rhyme>,</l>
        <l id="s3:l16" met="2" type="+-">En den <rhyme label="j" type="m">Schyn</rhyme></l>
        <l id="s3:l17" met="2" type="+-">Van uw <rhyme label="j" type="m" corresp="#s3:l16">reyn</rhyme></l>
        <l id="s3:l18" met="2" type="+-">En soet <rhyme label="i" type="m" corresp="#s3:l15">ghesicht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
