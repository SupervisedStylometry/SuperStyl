<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35813</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Des morgens voor dag in het veld al bij de weg</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">LOoft Menschen <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="3" type="-+">U Heer, u Godt, u <rhyme label="b" type="f">Koningh</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Die op dit <rhyme label="a" type="m" corresp="#s1:l1">dal</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Der tranen krijght sijn <rhyme label="b" type="f" corresp="#s1:l2">woningh</rhyme></l>
        <l id="s1:l5" met="2" type="-+">In een <rhyme label="c" type="m">Spelonck</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Heel sonder <rhyme label="c" type="m" corresp="#s1:l5">pronck</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Heel son', heel son', heel sonder <rhyme label="c" type="m" corresp="#s1:l5">pronck</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Van eenen Maget <rhyme label="c" type="m" corresp="#s1:l5">jonck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">'t Kindt is als <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Geweest van d' <rhyme label="b" type="f">Eeuwicheden</rhyme>;</l>
        <l id="s2:l3" met="2" type="-+">In 't <rhyme label="a" type="m" corresp="#s2:l1">Beeste-kot</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ontfanght sijn mensche <rhyme label="b" type="f" corresp="#s2:l2">leden</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Sijn Godtheyt <rhyme label="c" type="m">klaer</rhyme></l>
        <l id="s2:l6" met="2" type="-+">En Menscheyt <rhyme label="c" type="m" corresp="#s2:l5">waer</rhyme></l>
        <l id="s2:l7" met="4" type="+-">En Mensch, en Mensch, en Menschheydt <rhyme label="c" type="m" corresp="#s2:l5">waer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Worden een by <rhyme label="c" type="m" corresp="#s2:l5">mal'kaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Tusschen een <rhyme label="a" type="m">Os</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En Ezel wort <rhyme label="b" type="f">gevonden</rhyme>;</l>
        <l id="s3:l3" met="2" type="-+">Hy leyt niet <rhyme label="a" type="m" corresp="#s3:l1">los</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Maer in doecken <rhyme label="b" type="f" corresp="#s3:l2">gebonden</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">De grootste <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Leyt sonder <rhyme label="c" type="m" corresp="#s3:l5">eer</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Leyt zon', leyt zon' leyt zonder <rhyme label="c" type="m" corresp="#s3:l5">eer</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In 't snootste Kribtje <rhyme label="c" type="m" corresp="#s3:l5">ne'er</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Hy let op <rhyme label="a" type="m">stroy</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met al zijn teere <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">d' Engelen <rhyme label="a" type="m" corresp="#s4:l1">moy</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Door sangh wenschen ons <rhyme label="b" type="f" corresp="#s4:l2">vrede</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Dit Kindt dat <rhyme label="c" type="m">beeft</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Ons alles <rhyme label="c" type="m" corresp="#s4:l5">geeft</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Ons al, ons al, ons alles <rhyme label="c" type="m" corresp="#s4:l5">geeft</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">Voor hem 't minste niet <rhyme label="c" type="m" corresp="#s4:l5">heeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">d' Englen <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Wilde hy niet <rhyme label="b" type="f">aenvaerden</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Maer Abr'ams <rhyme label="a" type="m" corresp="#s5:l1">zaet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Achte hooger van <rhyme label="b" type="f" corresp="#s5:l2">waerde</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Hy wilde <rhyme label="c" type="m">rijck</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Ons stof en <rhyme label="c" type="m" corresp="#s5:l5">slijck</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Ons stof, ons stof, ons stof en <rhyme label="c" type="m" corresp="#s5:l5">slijck</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Maecken, en <rhyme label="c" type="m" corresp="#s5:l5">Goddelijck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Hy komt <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Om ons zaligh te <rhyme label="b" type="f">maken</rhyme>;</l>
        <l id="s6:l3" met="2" type="-+">Noyt Engel <rhyme label="a" type="m" corresp="#s6:l1">een</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Verdoemt sal daer toe <rhyme label="b" type="f" corresp="#s6:l2">raken</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Hy weent, hy <rhyme label="c" type="m">kermt</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">En ons <rhyme label="c" type="m" corresp="#s6:l5">ontfermt</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">En ons, en ons, en ons <rhyme label="c" type="m" corresp="#s6:l5">ontfermt</rhyme>;</l>
        <l id="s6:l8" met="3" type="-+">Van gramschap ons <rhyme label="c" type="m" corresp="#s6:l5">beschermt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Meer als <rhyme label="a" type="m">Samson</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Verwint onse <rhyme label="b" type="f">Vyanden</rhyme>.</l>
        <l id="s7:l3" met="2" type="-+">De'es klare <rhyme label="a" type="m" corresp="#s7:l1">son</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Verlicht onse <rhyme label="b" type="f" corresp="#s7:l2">verstanden</rhyme>;</l>
        <l id="s7:l5" met="2" type="-+">Als <rhyme label="c" type="m">Medecijn</rhyme></l>
        <l id="s7:l6" met="2" type="-+">By ons sal <rhyme label="c" type="m" corresp="#s7:l5">sijn</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">By ons, by ons, by ons sal <rhyme label="c" type="m" corresp="#s7:l5">sijn</rhyme></l>
        <l id="s7:l8" met="3" type="-+">In droefheyt, strijt en <rhyme label="c" type="m" corresp="#s7:l5">pijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">'t Is onse <rhyme label="a" type="m">Borgh</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En leydtsman in de <rhyme label="b" type="f">wegen</rhyme>.</l>
        <l id="s8:l3" met="2" type="-+">Voor ons draeght <rhyme label="a" type="m" corresp="#s8:l1">sorgh</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Ons mededeelt sijn <rhyme label="b" type="f" corresp="#s8:l2">zegen</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">Om dat dit <rhyme label="c" type="m">kint</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Ons zoo <rhyme label="c" type="m" corresp="#s8:l5">bemint</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Ons zoo, ons zoo, ons zoo <rhyme label="c" type="m" corresp="#s8:l5">bemint</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Schouwt dat de Ziel <rhyme label="c" type="m" corresp="#s8:l5">verblint</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Van minst tot <rhyme label="a" type="m">meest</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Voor u, Iesu; wy <rhyme label="b" type="f">alle</rhyme>.</l>
        <l id="s9:l3" met="2" type="-+">Met lijf en <rhyme label="a" type="m" corresp="#s9:l1">geest</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ootmoedich <rhyme label="b" type="f" corresp="#s9:l2">neder-valle</rhyme>.</l>
        <l id="s9:l5" met="2" type="-+">Geeft nae ons' <rhyme label="c" type="m">eysch</rhyme>:</l>
        <l id="s9:l6" met="2" type="-+">U troost en <rhyme label="c" type="m" corresp="#s9:l5">peys</rhyme></l>
        <l id="s9:l7" met="4" type="+-">U troost, u troost, u troost en <rhyme label="c" type="m" corresp="#s9:l5">peys</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">En naemaels u <rhyme label="c" type="m" corresp="#s9:l5">Palleys</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
