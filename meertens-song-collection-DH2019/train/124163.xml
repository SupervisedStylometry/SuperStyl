  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ALs schoonste <rhyme label="a" type="m">Carileen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Eens wandelden <rhyme label="a" type="m" corresp="#s1:l1">alleen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Zo zong zy lustig <rhyme label="b" type="m">uyt</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Met zoet <rhyme label="b" type="m" corresp="#s1:l3">geluyd</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Waar van den Echo 't laatste <rhyme label="c" type="m">woord</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Altyd na <rhyme label="d" type="f">baude</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">En <rhyme label="d" type="f" corresp="#s1:l6">herkaude</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Als zy 't heeft <rhyme label="c" type="m" corresp="#s1:l5">gehoord</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want vraagden zy wie 't <rhyme label="a" type="m">deed</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">Den Echo vraagden 't <rhyme label="a" type="m" corresp="#s2:l1">meed'</rhyme>!</l>
        <l id="s2:l3" met="3" type="-+">Als zy het spreeken <rhyme label="b" type="m">liet</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Hoorde zy <rhyme label="b" type="m" corresp="#s2:l3">niet</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Waar door het Meysje was in de <rhyme label="c" type="m">ley</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Als Carileen <rhyme label="d" type="f">Weende</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Den Echo <rhyme label="d" type="f" corresp="#s2:l6">steende</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Met vraagde <rhyme label="c" type="m" corresp="#s2:l5">zy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Geeft ghy my Lief <rhyme label="a" type="m">gena</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Den Echo zeyde <rhyme label="a" type="m" corresp="#s3:l1">Ia</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Mind hy niet <rhyme label="b" type="m">Carileen</rhyme>?</l>
        <l id="s3:l4" met="2" type="-+">De Stem riep <rhyme label="b" type="m" corresp="#s3:l3">Neen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Waar door zy liep als heel <rhyme label="c" type="m">ontzind</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Door digte <rhyme label="d" type="f">Boomen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">En zag daar <rhyme label="d" type="f" corresp="#s3:l6">komen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Haar Lief <rhyme label="c" type="m" corresp="#s3:l5">Garint</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doen was zy zo <rhyme label="a" type="m">ontsteld</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Zy heeft hem straks <rhyme label="a" type="m" corresp="#s4:l1">verteld</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Met een ontrust <rhyme label="b" type="m">gemoed</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Haar <rhyme label="b" type="m" corresp="#s4:l3">tegenspoed</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Hy lagte om haar zoete <rhyme label="c" type="m">reen</rhyme>;</l>
        <l id="s4:l6" met="2" type="-+">Heeft haar met <rhyme label="d" type="f">lusjes</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">En duyzend <rhyme label="d" type="f" corresp="#s4:l6">kusjes</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Gesteld te <rhyme label="c" type="m" corresp="#s4:l5">vreen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
