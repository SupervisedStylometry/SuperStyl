    <melody>Mon mari est à la taverne</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Klorimeen! bevallig <rhyme label="a" type="f">Blondje</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Uw Doris, door uw schoon <rhyme label="b" type="m">geraakt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Kust uw bevallig <rhyme label="a" type="f" corresp="#s1:l1">Rozemondje</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Daar hy van minne vlammen <rhyme label="b" type="m" corresp="#s1:l2">blaakt</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Hoe zal hy Venus Feestdag <rhyme label="c" type="f">vieren</rhyme>!</l>
        <l id="s1:l6" met="2" type="-+"><rhyme label="d" type="m">Faladeri</rhyme>...</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Beschouw ô Vrienden, Bruigoms <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En Bruidjes vriendelyk <rhyme label="b" type="m">gelag</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Goôn! hoe zal Doris, <rhyme label="a" type="f" corresp="#s2:l1">opgetoogen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Al Kusjes steelen Nagt en <rhyme label="b" type="m" corresp="#s2:l2">Dag</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Hoe zal hy met haar Roosje <rhyme label="c" type="f">Swieren</rhyme>.</l>
        <l id="s2:l6" met="2" type="-+"><rhyme label="d" type="m">Faladeri</rhyme>.....</l>
      </lg>
    </lg>
  </text>
</song>
