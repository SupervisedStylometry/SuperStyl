<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9007</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Ich ruf zu dir Herr Jesu Christ</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk Roep tot dy HEer Jesu <rhyme label="a" type="m">Christ</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ick bid' verhoort myn <rhyme label="b" type="f">Claghen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Verleent my G'naed' toe deser <rhyme label="a" type="m" corresp="#s1:l1">frist</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Laet my doch niet <rhyme label="b" type="f" corresp="#s1:l2">vertsaghen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Den rechten Ghelooven Heer ick <rhyme label="c" type="m">meyn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dien woudt ghy my doch <rhyme label="d" type="f">gheven</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Dy te <rhyme label="d" type="f" corresp="#s1:l6">Leven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Myn Naesten nutte <rhyme label="c" type="m" corresp="#s1:l5">zijn</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Dijn Woort te houden <rhyme label="d" type="f" corresp="#s1:l6">Even</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick bid' noch meer O Heere <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ghy connet my wel <rhyme label="b" type="f">gheven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat ick niet weder word' te <rhyme label="a" type="m" corresp="#s2:l1">spot</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die Hoping' gheeft daer <rhyme label="b" type="f" corresp="#s2:l2">neven</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Voor al, als ick moet van hier <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat ick dy mach <rhyme label="d" type="f">Betrouwen</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">End niet <rhyme label="d" type="f" corresp="#s2:l6">bouwen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Noch op myn wercken <rhyme label="c" type="m" corresp="#s2:l5">staen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Het mocht my Eewich <rhyme label="d" type="f" corresp="#s2:l6">Rouwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Verleent dat ick wt Herten <rhyme label="a" type="m">grondt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Myn Vyand' mach <rhyme label="b" type="f">vergheven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Vergheeft my oock all' myne <rhyme label="a" type="m" corresp="#s3:l1">Sond'</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Werckt my een nieuwe <rhyme label="b" type="f" corresp="#s3:l2">Leven</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dijn Woort myn spijs laet altoos <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Myn Siel daer met <rhyme label="d" type="f">gheneeren</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">My te <rhyme label="d" type="f" corresp="#s3:l6">weeren</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Als Ong'lijck comt en <rhyme label="c" type="m" corresp="#s3:l5">pijn</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Dat my haest mocht <rhyme label="d" type="f" corresp="#s3:l6">verkeeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Laet my geen Lust, noch Vrees van <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">In dees Werelt <rhyme label="b" type="f">affweynden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Volstandich zijn aent Eynd' gheeft <rhyme label="a" type="m" corresp="#s4:l1">my</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ghy hebt in dijnen <rhyme label="b" type="f" corresp="#s4:l2">Handen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">End wien ghy gheeft, die heeft om <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Niemant en mach <rhyme label="d" type="f">verwerven</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Noch <rhyme label="d" type="f" corresp="#s4:l6">gheerven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Door wercken dijn <rhyme label="c" type="m" corresp="#s4:l5">Genaed'</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Die ons verlost van <rhyme label="d" type="f" corresp="#s4:l6">sterven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick ligh in strijdt end <rhyme label="a" type="m">teghenstreeff</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Helpt ghy Heer Christ den <rhyme label="b" type="f">swacken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Aen dijn Ghenaed' alleyn ick <rhyme label="a" type="m" corresp="#s5:l1">cleeff</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ghy condt my stercker <rhyme label="b" type="f" corresp="#s5:l2">maecken</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Coemt nu Aenvechtingh' (HEER) soo <rhyme label="c" type="m">keer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat sy my niet om <rhyme label="d" type="f">vaten</rhyme></l>
        <l id="s5:l7" met="2" type="+-">Ghy condt <rhyme label="d" type="f" corresp="#s5:l6">maten</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dats' my niet en <rhyme label="c" type="m" corresp="#s5:l5">verveer</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Ghy sult het niet toe <rhyme label="d" type="f" corresp="#s5:l6">laten</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
