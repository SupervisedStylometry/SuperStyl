<song>
  <meta>
    <id>9555</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Kom Venus uit uw hoge troon</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrick Jansz. Prins</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Godt! ghy zijt mijn God seer <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ick soeck u in den daeghraet <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Na u dorset mijn <rhyme label="b" type="f">Ziele</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Mijn vleesch oock <rhyme label="c" type="m">desghelijck</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Is als het dor en mat <rhyme label="c" type="m" corresp="#s1:l4">Aerdtrijck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick heb in't Heylighdom <rhyme label="a" type="m">voorwaer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dat is u reyne Godts-dienst <rhyme label="a" type="m" corresp="#s2:l1">klaer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Gesien u <rhyme label="b" type="f">eygenschappen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">V sterckheyt, en u <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dat ghy zijt een volkomen <rhyme label="c" type="m" corresp="#s2:l4">Heer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want uwe <rhyme label="a" type="m">goedertierenheyt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Van geenen mensen kan zijn <rhyme label="a" type="m" corresp="#s3:l1">verbreyt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">S' is beter dan het <rhyme label="b" type="f">leven</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Mijn lippen maeckt <rhyme label="c" type="m">bequaem</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dan sal ick prijsen uwen <rhyme label="c" type="m" corresp="#s3:l4">Naem</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En u oock loven in mijn <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Mijn handen op-heffen met <rhyme label="a" type="m" corresp="#s4:l1">vlijt</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Dan sal versadight <rhyme label="b" type="f">werden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Mijn Ziel met vet en <rhyme label="c" type="m">smeer</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Oock vrolijck singen u ter <rhyme label="c" type="m" corresp="#s4:l4">eer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Als ick aen u ghedenck met <rhyme label="a" type="m">vre'e</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Wel dickmael op mijn <rhyme label="a" type="m" corresp="#s5:l1">leger-stee</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Want ghy zijt mijne <rhyme label="b" type="f">hulpe</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Geweest: dit peyns ick <rhyme label="c" type="m">vaeck</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">'t Zy in de nacht of <rhyme label="c" type="m" corresp="#s5:l4">morgen-waeck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Onder u vleugels <rhyme label="a" type="m">schaduwe</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Daer sal ick vrolijck singe <rhyme label="a" type="m" corresp="#s6:l1">mee</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Aen u kleeft vast mijn <rhyme label="b" type="f">Ziele</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Dat is, ick volgh u <rhyme label="c" type="m">na</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Soo dat ick nimmer van u <rhyme label="c" type="m" corresp="#s6:l4">ga</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Want uwe rechter-handt is <rhyme label="a" type="m">sterck</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Die onderstut my in dit <rhyme label="a" type="m" corresp="#s7:l1">werck</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Maer die mijn Ziele <rhyme label="b" type="f">soecken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Om te verwoesten <rhyme label="c" type="m">haer</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Die komen in een groot <rhyme label="c" type="m" corresp="#s7:l4">ghevaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Men salse storten door het <rhyme label="a" type="m">sweert</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Dat sy bloedigh vallen ter <rhyme label="a" type="m" corresp="#s8:l1">eert</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">De Vossen sullens' <rhyme label="b" type="f">eeten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Dewijlse zijn haer <rhyme label="c" type="m">deel</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Want sy door-lopen 't landt <rhyme label="c" type="m" corresp="#s8:l4">geheel</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ick David, die nu Koninck <rhyme label="a" type="m">ben</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">En u voor mijnen Godt <rhyme label="a" type="m" corresp="#s9:l1">erken</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">'k Sal my in u <rhyme label="b" type="f">verblijden</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">'t Is goedt die by u <rhyme label="c" type="m">sweert</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">En u ten rechten dient en <rhyme label="c" type="m" corresp="#s9:l4">eert</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Maer al de geen die leugen <rhyme label="a" type="m">spreeckt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">En vrome lie'n haer eere <rhyme label="a" type="m" corresp="#s10:l1">breeckt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hoe listigh sy 't <rhyme label="b" type="f">beleggen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Van Godt in korten <rhyme label="c" type="m">stondt</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Sal toe-gestopt zijn haren <rhyme label="c" type="m" corresp="#s10:l4">mondt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
