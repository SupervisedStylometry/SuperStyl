<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4207</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoonste nimf van het woud</melody>
  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SIngt, en springht, en weest <rhyme label="a" type="m">verheugd</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Doch in <rhyme label="a" type="m" corresp="#s1:l1">deugd</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Laat u stem, en woorden, <rhyme label="b" type="f">horen</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Tot een téken van de <rhyme label="c" type="m">vreugd</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Die de <rhyme label="c" type="m" corresp="#s1:l4">jeugd</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Voên, in 't minnelijk <rhyme label="b" type="f" corresp="#s1:l3">bekoren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Het bewégen van de <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Trekt de <rhyme label="a" type="m" corresp="#s2:l1">sin</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Tot de <rhyme label="b" type="f">minnelyke-lusjes</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Als de (graag-gesoende) <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s2:l5" met="2" type="+-">'t Harte <rhyme label="c" type="m" corresp="#s2:l4">wond</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Door de Weelderige <rhyme label="b" type="f" corresp="#s2:l3">kusjes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Soet, en dartel <rhyme label="a" type="m">Poppe-goet</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Siet hoe <rhyme label="a" type="m" corresp="#s3:l1">soet</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Voegen sich dees' twee te <rhyme label="b" type="f">samen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Soetertjes, ey volgd haar <rhyme label="c" type="m">Wet</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">
          <rhyme label="c" type="m" corresp="#s3:l4">Onbesmet</rhyme>
        </l>
        <l id="s3:l6" met="4" type="+-">Niemnd hoefd: sich dat te <rhyme label="b" type="f" corresp="#s3:l3">schamen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
