<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>51990</id>
    <date>
      <year-start>1810</year-start>
      <year-end>1810</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Joseph Sadones</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VReeden Mars, wilt vans ons <rhyme label="a" type="f">vlugten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">weet dat uwen tyd zeer <rhyme label="b" type="m">kort</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">gy hebt volk genoeg doen <rhyme label="a" type="f" corresp="#s1:l1">zugten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">daer is bloed genoeg <rhyme label="b" type="m" corresp="#s1:l2">gestort</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">ziet de peys-godinne <rhyme label="c" type="f">knielen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">zy stiert d'Opper-magt haer <rhyme label="d" type="m">bed'</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">staekt het van menschen te <rhyme label="c" type="f" corresp="#s1:l5">ontzieien</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">hemel, hemel, jont ons <rhyme label="d" type="m" corresp="#s1:l6">vred'</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wet-gevers van <rhyme label="a" type="f">Vrankeryke</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">die 't welvaerd der menschen <rhyme label="b" type="m">tragt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">laet ont den vrede <rhyme label="a" type="f" corresp="#s2:l1">bereyken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">spaert het menschelyk <rhyme label="b" type="m" corresp="#s2:l2">geslagt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">al het volk roept om den <rhyme label="c" type="f">vrede</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">vrede, vrede, is het <rhyme label="d" type="m">woord</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">zoo wel op het land als <rhyme label="c" type="f" corresp="#s2:l5">stede</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">van oost, west, zuyden tot <rhyme label="d" type="m" corresp="#s2:l6">noord</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Vorsten, keyzer en <rhyme label="a" type="f">koningen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">gy die gebieders zyt van <rhyme label="b" type="m">d'aerd</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">gy die wilt de volken <rhyme label="a" type="f" corresp="#s3:l1">dwingen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">door het vuer en door het <rhyme label="b" type="m" corresp="#s3:l2">zwaerd</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">hoort de peys-godinne haer <rhyme label="c" type="f">stemme</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">zy smeekt 't Opper-wezen <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">ziet, zy komt in traenen <rhyme label="c" type="f" corresp="#s3:l5">zwemmen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">om den vrede te <rhyme label="d" type="m" corresp="#s3:l6">ontfaÃªn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wilt gy nae haer stem niet <rhyme label="a" type="f">luysteren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">peyst een vorst die vrede <rhyme label="b" type="m">smaed</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">komt zyn naem en faem <rhyme label="a" type="f" corresp="#s4:l1">verduysteren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">vrede, vrede ende <rhyme label="b" type="m" corresp="#s4:l2">genaed</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">roepen d'ouders voor hun <rhyme label="c" type="f">kind'ren</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">vrede roept de teere <rhyme label="d" type="m">jeugd</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">vrede zal de smert <rhyme label="c" type="f" corresp="#s4:l5">vermind'ren</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">vrede is den grondsteen der <rhyme label="d" type="m" corresp="#s4:l6">deugd</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Eeuwig, eeuwig <rhyme label="a" type="f">Opper-wezen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">gy die alles hoort en <rhyme label="b" type="m">ziet</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">wilt den oorlogs-drift <rhyme label="a" type="f" corresp="#s5:l1">genezen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">van die voerd 't <rhyme label="b" type="m" corresp="#s5:l2">opper-gebied</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">vrede, vrede, zyn de <rhyme label="c" type="f">wenschen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">vrede ende <rhyme label="d" type="m">rechtveirdigheyd</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">roepen alle braeve <rhyme label="c" type="f" corresp="#s5:l5">menschen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">in dezen bedroefden <rhyme label="d" type="m" corresp="#s5:l6">tyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
