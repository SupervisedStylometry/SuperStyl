  <author>François Foret</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Groote Moeder sterke <rhyme label="a" type="f">Maget</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Zek're sterre van de <rhyme label="b" type="m">zee</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die gedurig zorge <rhyme label="a" type="f" corresp="#s1:l1">draegen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Voor uw' schaepkens, voor uw <rhyme label="b" type="m" corresp="#s1:l2">vee</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Wilt-ze met uw' jonst' <rhyme label="c" type="f">gemoeten</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Kinders, vrouwen, ende <rhyme label="d" type="m">mans</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die u hier komen <rhyme label="c" type="f" corresp="#s1:l5">begroeten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Met den weirden <rhyme label="d" type="m" corresp="#s1:l6">Roozenkrans</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Laet eens uw gezichte <rhyme label="a" type="f">daelen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ivers uyt een' klaere <rhyme label="b" type="m">wolk'</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Nae de westersche Vlaemsche <rhyme label="a" type="f" corresp="#s2:l1">paelen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Op dat vierig <rhyme label="b" type="m" corresp="#s2:l2">Akkervolk</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Wilt-ze niet uw' jonst' <rhyme label="c" type="f">gemoeten</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Kinders, vrouwen, ende <rhyme label="d" type="m">mans</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Die u hier komen <rhyme label="c" type="f" corresp="#s2:l5">begroeten</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Met den weirden <rhyme label="d" type="m" corresp="#s2:l6">Roozenkrans</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ziet met voordaed en met <rhyme label="a" type="f">preken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Heeft Dominicus <rhyme label="b" type="m">geslacht'</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Met deugds werken 't is <rhyme label="a" type="f" corresp="#s3:l1">gebleken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Zulks in het gebruyk <rhyme label="b" type="m" corresp="#s3:l2">gebragt</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Wilt-ze met uw' jonst' <rhyme label="c" type="f">gemoeten</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Kinders, vrouwen, ende <rhyme label="d" type="m">mans</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Die u hier komen <rhyme label="c" type="f" corresp="#s3:l5">begroeten</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Met den weirden <rhyme label="d" type="m" corresp="#s3:l6">Roozenkrans</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Prochievolk gansch <rhyme label="a" type="f">uytgelaeten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Komt u hier vallen te <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En worden all' <rhyme label="a" type="f" corresp="#s4:l1">onderzaeten</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Van Marias <rhyme label="b" type="m" corresp="#s4:l2">Roozenhoed</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Wilt-ze met uw' jonst' <rhyme label="c" type="f">gemoeten</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Kinders, vrouwen, ende <rhyme label="d" type="m">mans</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Die u hier komen <rhyme label="c" type="f" corresp="#s4:l5">begroeten</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Met den weirden <rhyme label="d" type="m" corresp="#s4:l6">Roozenkrans</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Moeder Gods wilt my <rhyme label="a" type="f">ontfermen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">En al 't volk van uwen <rhyme label="b" type="m">krans</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Wilt u goed en land <rhyme label="a" type="f" corresp="#s5:l1">beschermen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Voor den Geus, en voor den <rhyme label="b" type="m" corresp="#s5:l2">Frans</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Wilt-ze met uw' jonst' <rhyme label="c" type="f">gemoeten</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Kinders, vrouwen, ende <rhyme label="d" type="m">mans</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Die u hier komen <rhyme label="c" type="f" corresp="#s5:l5">begroeten</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Met den weirden <rhyme label="d" type="m" corresp="#s5:l6">Roozenkrans</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Maekt dat deze Davids <rhyme label="a" type="f">slinger</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Pletten mag dat rouwig <rhyme label="b" type="m">hoofd</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Van dien Israël <rhyme label="a" type="f" corresp="#s6:l1">bespringer</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Die u van uw' eer' <rhyme label="b" type="m" corresp="#s6:l2">berooft</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Wilt-ze met uw jonst' <rhyme label="c" type="f">gemoeten</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Kinders, vrouwen, ende <rhyme label="d" type="m">mans</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Die u hier komen <rhyme label="c" type="f" corresp="#s6:l5">begroeten</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Met den weirden <rhyme label="d" type="m" corresp="#s6:l6">Roozenkrans</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
