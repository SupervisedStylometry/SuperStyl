<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4248</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Allemande du court</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="emotions">emoties</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SAl ick nu niet <rhyme label="a" type="f">clagen</rhyme> //</l>
        <l id="s1:l2" met="3" type="+-">mijn swaer <rhyme label="b" type="m">ongeluck</rhyme> //</l>
        <l id="s1:l3" met="3" type="+-">Hoe can t'hert <rhyme label="a" type="f" corresp="#s1:l1">verdragen</rhyme> //</l>
        <l id="s1:l4" met="3" type="+-">sulcken wreeden <rhyme label="b" type="m" corresp="#s1:l2">stuck</rhyme> //</l>
        <l id="s1:l5" met="4" type="+-">Als my nu wert <rhyme label="c" type="m">aengedaen</rhyme></l>
        <l id="s1:l6" met="2" type="-+">De <rhyme label="d" type="f">welgeminde</rhyme> //</l>
        <l id="s1:l7" met="2" type="-+">Liefste <rhyme label="d" type="f" corresp="#s1:l6">gesinde</rhyme> //</l>
        <l id="s1:l8" met="3" type="+-">Die int hert mocht <rhyme label="c" type="m" corresp="#s1:l5">staen</rhyme> //</l>
        <l id="s1:l9" met="4" type="+-">Eylaes is my <rhyme label="c" type="m" corresp="#s1:l5">afgegaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">V an mijn kintsche <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Heeft zy my <rhyme label="b" type="m">verheucht</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">In mijn zins <rhyme label="a" type="f" corresp="#s2:l1">behagen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Stont geen lieuer <rhyme label="b" type="m" corresp="#s2:l2">Jeught</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Die my mocht verblijden <rhyme label="c" type="m">gans</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Lijf, goet, en <rhyme label="d" type="f">leuen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Was haer <rhyme label="d" type="f" corresp="#s2:l6">gegeven</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Maer zy gaet <rhyme label="c" type="m" corresp="#s2:l5">nochtans</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Met een ander aen den <rhyme label="c" type="m" corresp="#s2:l5">dans</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Redelick <rhyme label="a" type="f">ontfermen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Heeft sy my <rhyme label="b" type="m">ontseyt</rhyme>:</l>
        <l id="s3:l3" met="3" type="+-">Zuchten, ende <rhyme label="a" type="f" corresp="#s3:l1">kermen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Is my op <rhyme label="b" type="m" corresp="#s3:l2">geleyt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Als een die zijn liefste <rhyme label="c" type="m">derft</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Vande <rhyme label="d" type="f">vercoren</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Schoonste <rhyme label="d" type="f" corresp="#s3:l6">geboren</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Ben ick nu <rhyme label="c" type="m" corresp="#s3:l5">onterft</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Ay my! mijn ionck herte <rhyme label="c" type="m" corresp="#s3:l5">sterft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Tot mijns stervens <rhyme label="a" type="f">ure</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Claegh ick sulck <rhyme label="b" type="m">verlies</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">Wt duysent de <rhyme label="a" type="f" corresp="#s4:l1">cuere</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Was sy mijn <rhyme label="b" type="m" corresp="#s4:l2">verkies</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Die my nu dus wreet <rhyme label="c" type="m">aensiet</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Duer quade // <rhyme label="d" type="f">becken</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">(Die schade // <rhyme label="d" type="f" corresp="#s4:l6">wecken</rhyme>)</l>
        <l id="s4:l8" met="3" type="+-">Coem ick int <rhyme label="c" type="m" corresp="#s4:l5">verdriet</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Ick en weet de cause <rhyme label="c" type="m" corresp="#s4:l5">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Es behulp, en <rhyme label="a" type="f">bate</rhyme></l>
        <l id="s5:l2" met="3" type="+-">My dus <rhyme label="b" type="m">ongereet</rhyme>:</l>
        <l id="s5:l3" met="3" type="+-">O doot, touft niet <rhyme label="a" type="f" corresp="#s5:l1">late</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Stelpt mijn bitter <rhyme label="b" type="m" corresp="#s5:l2">leet</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Geen secours en is voor <rhyme label="c" type="m">my</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Lieuer te <rhyme label="d" type="f">steruen</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Dan haer te <rhyme label="d" type="f" corresp="#s5:l6">deruen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Want ter werelt <rhyme label="c" type="m" corresp="#s5:l5">vry</rhyme></l>
        <l id="s5:l9" met="4" type="+-">Verhueght my niemant dan <rhyme label="c" type="m" corresp="#s5:l5">zy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Prince God der <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">V is meest de <rhyme label="b" type="m">schult</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Want dijn soet <rhyme label="a" type="f" corresp="#s6:l1">beginnen</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Is met leet <rhyme label="b" type="m" corresp="#s6:l2">vervult</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Haer semblant heeft my <rhyme label="c" type="m">verdooft</rhyme>:</l>
        <l id="s6:l6" met="2" type="-+">Voor vaste <rhyme label="d" type="f">trouwe</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Geeft zy my <rhyme label="d" type="f" corresp="#s6:l6">rouwe</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Als mijn <rhyme label="c" type="m" corresp="#s6:l5">overhooft</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Ken hadts nimmermeer <rhyme label="c" type="m" corresp="#s6:l5">gelooft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
