    <melody>Iets moet ik u Laura vragen</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Bernardus Busschoff</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GRoote Godt, die d' Aerdtsche <rhyme label="a" type="f">Goden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">(Als uw' Dienaers ende <rhyme label="a" type="f" corresp="#s1:l1">Boden</rhyme>)</l>
        <l id="s1:l3" met="4" type="+-">Hoogh en laegh regeeren <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dien de aldergrootste <rhyme label="c" type="f">Koning'</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Van sijn rijck, ja van sijn <rhyme label="c" type="f" corresp="#s1:l4">wooning'</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Als een Leen-Heer kennen <rhyme label="b" type="m" corresp="#s1:l3">moet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die de herten van de <rhyme label="a" type="f">Grooten</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Neyght en buyght als <rhyme label="a" type="f" corresp="#s2:l1">water-gooten</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die tot in den gront door <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Aller Koningen <rhyme label="c" type="f">gedachten</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En al wat in duyster <rhyme label="c" type="f" corresp="#s2:l4">nachten</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Haestigh in haer sinnen <rhyme label="b" type="m" corresp="#s2:l3">schiet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die met toomen en met <rhyme label="a" type="f">ringen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">De Tyrannen kondt <rhyme label="a" type="f" corresp="#s3:l1">bedwingen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die haer sterck gespannen <rhyme label="b" type="m">boogh</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Als een riet-stock kondt <rhyme label="c" type="f">verbreken</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Die haer doet het zweert <rhyme label="c" type="f" corresp="#s3:l4">op-steken</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Met een wencken van u <rhyme label="b" type="m" corresp="#s3:l3">oogh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Siet gy niet hoe Babels <rhyme label="a" type="f">Hoere</rhyme></l>
        <l id="s4:l2" met="4" type="+-">All' de wereldt steldt in <rhyme label="a" type="f" corresp="#s4:l1">roere</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">Alles t' onderst' boven <rhyme label="b" type="m">keert</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Hoe sy pleeght haer vuyle <rhyme label="c" type="f">minne</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Als een heete <rhyme label="c" type="f" corresp="#s4:l4">Kemelinne</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Met de Koningen <rhyme label="b" type="m" corresp="#s4:l3">boeleert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hoe sy maeckt de Vorsten <rhyme label="a" type="f">droncken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Met haer stercken Wijn <rhyme label="a" type="f" corresp="#s5:l1">beschoncken</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Om te doen haer Moorders <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Om van d' Aerde te <rhyme label="c" type="f">vernielen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Al wat schrickt voor haer te <rhyme label="c" type="f" corresp="#s5:l4">knielen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Al wat uwen Soone <rhyme label="b" type="m" corresp="#s5:l3">kust</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Immers kondt gy 't sien en <rhyme label="a" type="f">weten</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Godt gy zijt wel hoogh <rhyme label="a" type="f" corresp="#s6:l1">geseten</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Maer u wonder scherp <rhyme label="b" type="m">gesicht</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Kan met groote kracht <rhyme label="c" type="f">door-dringen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Soo dat voor u alle <rhyme label="c" type="f" corresp="#s6:l4">dingen</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Zijn soo klaer gelijck het <rhyme label="b" type="m" corresp="#s6:l3">licht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Selfs de Hel is voor u <rhyme label="a" type="f">open</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">U vyer-vlammigh oogen <rhyme label="a" type="f" corresp="#s7:l1">lopen</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Boven, onder, over <rhyme label="b" type="m">al</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">'t Stilste luyst'ren kondt gy <rhyme label="c" type="f">hooren</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Ja gy weet al langh te <rhyme label="c" type="f" corresp="#s7:l4">vooren</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Wat een mensch noch dencken <rhyme label="b" type="m" corresp="#s7:l3">sal</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Siet gy dan de Hoer soo <rhyme label="a" type="f">woelen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Haer onreyne handen <rhyme label="a" type="f" corresp="#s8:l1">spoelen</rhyme></l>
        <l id="s8:l3" met="4" type="+-">In het reyn vergooten <rhyme label="b" type="m">bloedt</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Uwer knechten, dat voor <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s8:l5" met="4" type="+-">In uw' oogen pleeght te <rhyme label="c" type="f" corresp="#s8:l4">wesen</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Kostelijck en dierbaer <rhyme label="b" type="m" corresp="#s8:l3">goedt</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Kondt gy 't sien, en soo <rhyme label="a" type="f">verdragen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Sonder vyer en ander <rhyme label="a" type="f" corresp="#s9:l1">plagen</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Over haer te storten <rhyme label="b" type="m">neer</rhyme>?</l>
        <l id="s9:l4" met="4" type="+-">Moet het u Vriendinn' al <rhyme label="c" type="f">meugen</rhyme>?</l>
        <l id="s9:l5" met="4" type="+-">Moet sy all' die bitt're <rhyme label="c" type="f" corresp="#s9:l4">teugen</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Van de Hoer verzwelgen <rhyme label="b" type="m" corresp="#s9:l3">Heer</rhyme>?</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Heyligh zijn o Godt uw' <rhyme label="a" type="f">wegen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Wy en spreecken u niet <rhyme label="a" type="f" corresp="#s10:l1">tegen</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Wy aen-bidden u <rhyme label="b" type="m">beleydt</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Als een Mensch met u wil <rhyme label="c" type="f">twisten</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">All' sijn vonden ende <rhyme label="c" type="f" corresp="#s10:l4">listen</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Zijn voor u maer <rhyme label="b" type="m" corresp="#s10:l3">ydelheydt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Maer o Heer (vergeeft ons 't <rhyme label="a" type="f">vragen</rhyme>)</l>
        <l id="s11:l2" met="4" type="+-">Sal u Bruydt dan altijdt <rhyme label="a" type="f" corresp="#s11:l1">klagen</rhyme>?</l>
        <l id="s11:l3" met="4" type="+-">Altijdt lijden hoon en <rhyme label="b" type="m">spijdt</rhyme>?</l>
        <l id="s11:l4" met="4" type="+-">Sullen op haer bleecke <rhyme label="c" type="f">wangen</rhyme></l>
        <l id="s11:l5" met="4" type="+-">Altijdt heete tranen <rhyme label="c" type="f" corresp="#s11:l4">hangen</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Tot dat komt uw's Oordeels <rhyme label="b" type="m" corresp="#s11:l3">tijdt</rhyme>?</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Siet o Vader eens van <rhyme label="a" type="f">boven</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Siet eens door der Princen <rhyme label="a" type="f" corresp="#s12:l1">Hoven</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Die uw' Kercke wenschen <rhyme label="b" type="m">quaet</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">Wilt uw' ooren doch eens <rhyme label="c" type="f">setten</rhyme></l>
        <l id="s12:l5" met="4" type="+-">Aen haer dichte <rhyme label="c" type="f" corresp="#s12:l4">Cabinetten</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Hoort eens wat daer omme <rhyme label="b" type="m" corresp="#s12:l3">gaet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Sy besluyten in 't <rhyme label="a" type="f">verborgen</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Ons tot een toe te <rhyme label="a" type="f" corresp="#s13:l1">verworgen</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Gantsch en gaer te roepen <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Als men pleeght uyt d' Aerd' te <rhyme label="c" type="f">plucken</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Met den wortel wegh te <rhyme label="c" type="f" corresp="#s13:l4">rucken</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">'t Snood' en ongezegent <rhyme label="b" type="m" corresp="#s13:l3">kruyt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">'t Is in Duytslandt wel <rhyme label="a" type="f">gebleecken</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Daer de stroomen en de <rhyme label="a" type="f" corresp="#s14:l1">beecken</rhyme></l>
        <l id="s14:l3" met="4" type="+-">Root geworden zijn van <rhyme label="b" type="m">bloedt</rhyme>:</l>
        <l id="s14:l4" met="4" type="+-">Daer den Arendt en de <rhyme label="c" type="f">Gieren</rhyme>,</l>
        <l id="s14:l5" met="4" type="+-">Ravens, Honden, ander' <rhyme label="c" type="f" corresp="#s14:l4">Dieren</rhyme></l>
        <l id="s14:l6" met="4" type="+-">Zijn met menschen vleesch <rhyme label="b" type="m" corresp="#s14:l3">gevoedt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">'t Arm Boheemsche <rhyme label="a" type="f">Koninckrijcke</rhyme>,</l>
        <l id="s15:l2" met="4" type="+-">Hungeryen <rhyme label="a" type="f" corresp="#s15:l1">desgelijcke</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">En die Landen daer <rhyme label="b" type="m">ontrent</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Ach wat hebben s' al <rhyme label="c" type="f">geleden</rhyme>!</l>
        <l id="s15:l5" met="4" type="+-">Ach wat lijdens' al op <rhyme label="c" type="f" corresp="#s15:l4">heden</rhyme>!</l>
        <l id="s15:l6" met="4" type="+-">Noch en heeft haer smert geen <rhyme label="b" type="m" corresp="#s15:l3">endt</rhyme>!</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Duysenden van vroome <rhyme label="a" type="f">zielen</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Hebben sy daer sien <rhyme label="a" type="f" corresp="#s16:l1">vernielen</rhyme>,</l>
        <l id="s16:l3" met="4" type="+-">Na die droeve <rhyme label="b" type="m">nederlaegh</rhyme>!</l>
        <l id="s16:l4" met="4" type="+-">s' Hebben menigh hooft sien <rhyme label="c" type="f">springen</rhyme></l>
        <l id="s16:l5" met="4" type="+-">Over die bebloeyde <rhyme label="c" type="f" corresp="#s16:l4">klingen</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">En noch sien sy 't alle <rhyme label="b" type="m" corresp="#s16:l3">daegh</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Grijse Mannen, teere <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s17:l2" met="4" type="+-">Zijn aen stucken daer <rhyme label="a" type="f" corresp="#s17:l1">gehouwen</rhyme>,</l>
        <l id="s17:l3" met="4" type="+-">Ja de Kinders van de <rhyme label="b" type="m">borst</rhyme>:</l>
        <l id="s17:l4" met="4" type="+-">Noch en kan die Hoer niet <rhyme label="c" type="f">laten</rhyme>,</l>
        <l id="s17:l5" met="4" type="+-">'t Bloedt te tappen als uyt <rhyme label="c" type="f" corresp="#s17:l4">vaten</rhyme>,</l>
        <l id="s17:l6" met="4" type="+-">'t Drincken maeckt haer grooter <rhyme label="b" type="m" corresp="#s17:l3">dorst</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">d' Edel Paltz die smelt van <rhyme label="a" type="f">rouwe</rhyme></l>
        <l id="s18:l2" met="4" type="+-">Als een arm verachte <rhyme label="a" type="f" corresp="#s18:l1">Vrouwe</rhyme></l>
        <l id="s18:l3" met="4" type="+-">Van haer eer en Heer <rhyme label="b" type="m">ontbloodt</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Die moet nemen in haer <rhyme label="c" type="f">ermen</rhyme>,</l>
        <l id="s18:l5" met="4" type="+-">En in haren schoot <rhyme label="c" type="f" corresp="#s18:l4">verwermen</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">Dien sy haet meer als de <rhyme label="b" type="m" corresp="#s18:l3">doodt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">'t Heerloos, weerloos volck met <rhyme label="a" type="f">tranen</rhyme>!</l>
        <l id="s19:l2" met="4" type="+-">Duysentmael magh 's daeghs <rhyme label="a" type="f" corresp="#s19:l1">vermanen</rhyme></l>
        <l id="s19:l3" met="4" type="+-">Haren Prince <rhyme label="b" type="m">Frederick</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">Die met hertzweer en met <rhyme label="c" type="f">suchten</rhyme></l>
        <l id="s19:l5" met="4" type="+-">Uyt het Landt heeft moeten <rhyme label="c" type="f" corresp="#s19:l4">vluchten</rhyme></l>
        <l id="s19:l6" met="4" type="+-">Voor 't gewapent <rhyme label="b" type="m" corresp="#s19:l3">ongelijck</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">'t Is daer gantsch een ander <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s20:l2" met="4" type="+-">Alst daer pleeght te zijn voor <rhyme label="a" type="f" corresp="#s20:l1">desen</rhyme>,</l>
        <l id="s20:l3" met="4" type="+-">'t Schijnt te zijn een ander <rhyme label="b" type="m">Landt</rhyme>:</l>
        <l id="s20:l4" met="4" type="+-">'t Was een Landt daer vrede <rhyme label="c" type="f">bloeyde</rhyme>,</l>
        <l id="s20:l5" met="4" type="+-">Honigh, Melck en Wijn daer <rhyme label="c" type="f" corresp="#s20:l4">vloeyde</rhyme>,</l>
        <l id="s20:l6" met="4" type="+-">Door u zegen-rijcke <rhyme label="b" type="m" corresp="#s20:l3">handt</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Nu eylaes! om hare <rhyme label="a" type="f">sonden</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">Wordt van vremde al <rhyme label="a" type="f" corresp="#s21:l1">verslonden</rhyme>,</l>
        <l id="s21:l3" met="4" type="+-">Wat 't verdorven Aertrijck <rhyme label="b" type="m">voedt</rhyme>!</l>
        <l id="s21:l4" met="4" type="+-">'t Heyl is gantsch van daer <rhyme label="c" type="f">geweecken</rhyme>,</l>
        <l id="s21:l5" met="4" type="+-">'t Arme Landt is <rhyme label="c" type="f" corresp="#s21:l4">overstreecken</rhyme></l>
        <l id="s21:l6" met="4" type="+-">Met een bloedt en <rhyme label="b" type="m" corresp="#s21:l3">tranen-vloet</rhyme>!</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">De vorst'lijcke <rhyme label="a" type="f">Heerschappyen</rhyme>,</l>
        <l id="s22:l2" met="4" type="+-">Gulick, Cleef, wat <rhyme label="a" type="f" corresp="#s22:l1">tyrannyen</rhyme>,</l>
        <l id="s22:l3" met="4" type="+-">Wat gewelt en <rhyme label="b" type="m">overlast</rhyme></l>
        <l id="s22:l4" met="4" type="+-">Lijden sy door sroovers <rhyme label="c" type="f">handen</rhyme>!</l>
        <l id="s22:l5" met="4" type="+-">En de Mercksch' en' Bercksche <rhyme label="c" type="f" corresp="#s22:l4">landen</rhyme></l>
        <l id="s22:l6" met="4" type="+-">Zijn wel dapper <rhyme label="b" type="m" corresp="#s22:l3">aen-getast</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Hoogh en laege staets <rhyme label="a" type="f">Persoonen</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Die als Ballingen hier <rhyme label="a" type="f" corresp="#s23:l1">woonen</rhyme>,</l>
        <l id="s23:l3" met="4" type="+-">Soo beschrijven haren <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s23:l4" met="4" type="+-">Dat een mensch moet zijn van <rhyme label="c" type="f">steenen</rhyme>,</l>
        <l id="s23:l5" met="4" type="+-">Dien het niet door mergh en <rhyme label="c" type="f" corresp="#s23:l4">beenen</rhyme></l>
        <l id="s23:l6" met="4" type="+-">Binnen tot in't herte <rhyme label="b" type="m" corresp="#s23:l3">gaet</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Als wy Hier wat lager <rhyme label="a" type="f">dalen</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">Met ons droeve sinnen <rhyme label="a" type="f" corresp="#s24:l1">dwalen</rhyme>,</l>
        <l id="s24:l3" met="4" type="+-">Door het groote Fransch <rhyme label="b" type="m">gebiedt</rhyme>,</l>
        <l id="s24:l4" met="4" type="+-">Dach en nacht sien wy daer <rhyme label="c" type="f">wercken</rhyme>,</l>
        <l id="s24:l5" met="4" type="+-">Om uw' langh vervolghde <rhyme label="c" type="f" corresp="#s24:l4">Kercken</rhyme>,</l>
        <l id="s24:l6" met="4" type="+-">Nu te maecken gantsch te <rhyme label="b" type="m" corresp="#s24:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">Ach wy konnen sonder <rhyme label="a" type="f">tranen</rhyme>!</l>
        <l id="s25:l2" met="4" type="+-">Nimmer spreecken of <rhyme label="a" type="f" corresp="#s25:l1">vermanen</rhyme>,</l>
        <l id="s25:l3" met="4" type="+-">Van Rochel nu <rhyme label="b" type="m">overheert</rhyme>!</l>
        <l id="s25:l4" met="4" type="+-">'t Welck naest u o Heer voor <rhyme label="c" type="f">desen</rhyme></l>
        <l id="s25:l5" met="4" type="+-">Als een Vry-Stadt pleeght te <rhyme label="c" type="f" corresp="#s25:l4">wesen</rhyme>,</l>
        <l id="s25:l6" met="4" type="+-">Van dat volck dat u <rhyme label="b" type="m" corresp="#s25:l3">vereert</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">Ach die Stadt heeft moeten <rhyme label="a" type="f">lijden</rhyme>!</l>
        <l id="s26:l2" met="4" type="+-">Dat door 't ongenadigh <rhyme label="a" type="f" corresp="#s26:l1">snijden</rhyme>,</l>
        <l id="s26:l3" met="4" type="+-">Van het scherpe hongers <rhyme label="b" type="m">zweert</rhyme>,</l>
        <l id="s26:l4" met="4" type="+-">Meest haer Burgers zijn <rhyme label="c" type="f">verslonden</rhyme>,</l>
        <l id="s26:l5" met="4" type="+-">Zijnd' op straten doodt <rhyme label="c" type="f" corresp="#s26:l4">gevonden</rhyme>,</l>
        <l id="s26:l6" met="4" type="+-">Tot het been toe <rhyme label="b" type="m" corresp="#s26:l3">uyt-geteert</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="+-">Waer uw' ogen heenen <rhyme label="a" type="f">wenden</rhyme>,</l>
        <l id="s27:l2" met="4" type="+-">Oost of West, aen allen <rhyme label="a" type="f" corresp="#s27:l1">enden</rhyme>,</l>
        <l id="s27:l3" met="4" type="+-">Sien wy soo veel hoon en <rhyme label="b" type="m">smaet</rhyme>:</l>
        <l id="s27:l4" met="4" type="+-">Soo veel leets u Kerck in <rhyme label="c" type="f">schencken</rhyme>,</l>
        <l id="s27:l5" met="4" type="+-">Dat wy in ons zwackheyt <rhyme label="c" type="f" corresp="#s27:l4">dencken</rhyme>,</l>
        <l id="s27:l6" met="4" type="+-">Of ghy niet te hart en <rhyme label="b" type="m" corresp="#s27:l3">slaet</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="+-">Maer als wy ons recht <rhyme label="a" type="f">bedaren</rhyme>,</l>
        <l id="s28:l2" met="4" type="+-">Ons verstant by een <rhyme label="a" type="f" corresp="#s28:l1">vergaren</rhyme>,</l>
        <l id="s28:l3" met="4" type="+-">Geven wy ons selfs de <rhyme label="b" type="m">schult</rhyme>:</l>
        <l id="s28:l4" met="4" type="+-">Want wy sien dat ons <rhyme label="c" type="f">misdaden</rhyme></l>
        <l id="s28:l5" met="4" type="+-">Al dees' lasten op ons <rhyme label="c" type="f" corresp="#s28:l4">laden</rhyme>!</l>
        <l id="s28:l6" met="4" type="+-">En wy prijsen uw' <rhyme label="b" type="m" corresp="#s28:l3">gedult</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="+-">Waert ghy Heer niet seer <rhyme label="a" type="f">langhmoedig</rhyme>,</l>
        <l id="s29:l2" met="4" type="+-">Uw' gedult niet <rhyme label="a" type="f" corresp="#s29:l1">overvloedigh</rhyme>,</l>
        <l id="s29:l3" met="4" type="+-">Uw' genaed' niet <rhyme label="b" type="m">onbepaelt</rhyme>,</l>
        <l id="s29:l4" met="4" type="+-">Tot het Volck van u <rhyme label="c" type="f">verkooren</rhyme>,</l>
        <l id="s29:l5" met="4" type="+-">O wy waren langh <rhyme label="c" type="f" corresp="#s29:l4">verlooren</rhyme>!</l>
        <l id="s29:l6" met="4" type="+-">Langh ter hellen <rhyme label="b" type="m" corresp="#s29:l3">afgedaelt</rhyme>.</l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="4" type="+-">Nu wy goede Godt in 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s30:l2" met="4" type="+-">Door u goetheydt zijn <rhyme label="a" type="f" corresp="#s30:l1">gebleven</rhyme>,</l>
        <l id="s30:l3" met="4" type="+-">Voor 't verdiend' verderf <rhyme label="b" type="m">bewaert</rhyme>:</l>
        <l id="s30:l4" met="4" type="+-">Sullen wy altijdt <rhyme label="c" type="f">belijden</rhyme>,</l>
        <l id="s30:l5" met="4" type="+-">Dat gy midden in 't <rhyme label="c" type="f" corresp="#s30:l4">kastijden</rhyme></l>
        <l id="s30:l6" met="4" type="+-">Houdt u Vaderlijcken <rhyme label="b" type="m" corresp="#s30:l3">aert</rhyme>.</l>
      </lg>
      <lg id="s31" type="stanza">
        <l id="s31:l1" met="4" type="+-">Maer liev' Vader laet <rhyme label="a" type="f">ontbranden</rhyme>,</l>
        <l id="s31:l2" met="4" type="+-">Eens u teere <rhyme label="a" type="f" corresp="#s31:l1">ingewanden</rhyme></l>
        <l id="s31:l3" met="4" type="+-">Over 't over-groot <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s31:l4" met="4" type="+-">Uwes Volcks, dat als <rhyme label="c" type="f">verwesen</rhyme></l>
        <l id="s31:l5" met="4" type="+-">Tot der doodt, in duysent <rhyme label="c" type="f" corresp="#s31:l4">vreesen</rhyme>,</l>
        <l id="s31:l6" met="4" type="+-">Altijdt op u handen <rhyme label="b" type="m" corresp="#s31:l3">siet</rhyme>.</l>
      </lg>
      <lg id="s32" type="stanza">
        <l id="s32:l1" met="4" type="+-">Streckt eens uyt uw' stercke <rhyme label="a" type="f">handen</rhyme>,</l>
        <l id="s32:l2" met="4" type="+-">Breeckt der Philistijnen <rhyme label="a" type="f" corresp="#s32:l1">banden</rhyme>,</l>
        <l id="s32:l3" met="4" type="+-">Als een dun versenghden <rhyme label="b" type="m">draet</rhyme>.</l>
        <l id="s32:l4" met="4" type="+-">Maeckt dat al haer liguen <rhyme label="c" type="f">liegen</rhyme>,</l>
        <l id="s32:l5" met="4" type="+-">Dat haer magh die hoop <rhyme label="c" type="f" corresp="#s32:l4">bedriegen</rhyme>,</l>
        <l id="s32:l6" met="4" type="+-">Die op haer verbonden <rhyme label="b" type="m" corresp="#s32:l3">staet</rhyme>.</l>
      </lg>
      <lg id="s33" type="stanza">
        <l id="s33:l1" met="4" type="+-">Sendt een War-geest tusschen <rhyme label="a" type="f">beyden</rhyme>,</l>
        <l id="s33:l2" met="4" type="+-">Dat sy van elckander <rhyme label="a" type="f" corresp="#s33:l1">scheyden</rhyme>,</l>
        <l id="s33:l3" met="4" type="+-">Dat door onderlingh <rhyme label="b" type="m">gekijf</rhyme>,</l>
        <l id="s33:l4" met="4" type="+-">Sy malkanderen <rhyme label="c" type="f">bevechten</rhyme>,</l>
        <l id="s33:l5" met="4" type="+-">Als de Midiaensche <rhyme label="c" type="f" corresp="#s33:l4">knechten</rhyme></l>
        <l id="s33:l6" met="4" type="+-">Deeden door u vreemt <rhyme label="b" type="m" corresp="#s33:l3">bedrijf</rhyme>.</l>
      </lg>
      <lg id="s34" type="stanza">
        <l id="s34:l1" met="4" type="+-">Godt u macht is niet <rhyme label="a" type="f">vermindert</rhyme>,</l>
        <l id="s34:l2" met="4" type="+-">Maer ons boosheydt heeft <rhyme label="a" type="f" corresp="#s34:l1">verhindert</rhyme></l>
        <l id="s34:l3" met="4" type="+-">All'ons heyl tot desen <rhyme label="b" type="m">dagh</rhyme>.</l>
        <l id="s34:l4" met="4" type="+-">Onse sonden hoogh als <rhyme label="c" type="f">bergen</rhyme>,</l>
        <l id="s34:l5" met="4" type="+-">'t Licht uws aenschijns soo <rhyme label="c" type="f" corresp="#s34:l4">verbergen</rhyme>,</l>
        <l id="s34:l6" met="4" type="+-">Dat 't ons niet beschijnen <rhyme label="b" type="m" corresp="#s34:l3">mach</rhyme>.</l>
      </lg>
      <lg id="s35" type="stanza">
        <l id="s35:l1" met="4" type="+-">Werpt o Vader onse <rhyme label="a" type="f">sonden</rhyme>,</l>
        <l id="s35:l2" met="4" type="+-">Die ons slaen dees diepe <rhyme label="a" type="f" corresp="#s35:l1">wonden</rhyme>,</l>
        <l id="s35:l3" met="4" type="+-">In de Zee van boven <rhyme label="b" type="m">neer</rhyme>:</l>
        <l id="s35:l4" met="4" type="+-">Daerse niemandt kan weer <rhyme label="c" type="f">halen</rhyme>,</l>
        <l id="s35:l5" met="4" type="+-">Laetse tot den Afgront <rhyme label="c" type="f" corresp="#s35:l4">dalen</rhyme>,</l>
        <l id="s35:l6" met="4" type="+-">Van uw' diep genaden <rhyme label="b" type="m" corresp="#s35:l3">meer</rhyme>.</l>
      </lg>
      <lg id="s36" type="stanza">
        <l id="s36:l1" met="4" type="+-">Toont dat gy noch zijt op <rhyme label="a" type="f">heden</rhyme></l>
        <l id="s36:l2" met="4" type="+-">Die gy waert van <rhyme label="a" type="f" corresp="#s36:l1">eeuwigheden</rhyme>,</l>
        <l id="s36:l3" met="4" type="+-">In 't vergeven mildt en <rhyme label="b" type="m">trouw'</rhyme>,</l>
        <l id="s36:l4" met="4" type="+-">Over dien die haer <rhyme label="c" type="f">gebreecken</rhyme></l>
        <l id="s36:l5" met="4" type="+-">Niet verbergen noch <rhyme label="c" type="f" corresp="#s36:l4">versteecken</rhyme>,</l>
        <l id="s36:l6" met="4" type="+-">Maer bekennen met <rhyme label="b" type="m" corresp="#s36:l3">berouw</rhyme>.</l>
      </lg>
      <lg id="s37" type="stanza">
        <l id="s37:l1" met="4" type="+-">Kan u al ons droevigh <rhyme label="a" type="f">kermen</rhyme>!</l>
        <l id="s37:l2" met="4" type="+-">'t Hert niet roeren tot <rhyme label="a" type="f" corresp="#s37:l1">ontfermen</rhyme>?</l>
        <l id="s37:l3" met="4" type="+-">Kan al 't bloedt vergieten <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s37:l4" met="4" type="+-">'t Ingewant u niet <rhyme label="c" type="f">bewegen</rhyme>?</l>
        <l id="s37:l5" met="4" type="+-">Toont dan dat u is <rhyme label="c" type="f" corresp="#s37:l4">gelegen</rhyme></l>
        <l id="s37:l6" met="4" type="+-">Aen uw' Waerheydt en uw' <rhyme label="b" type="m" corresp="#s37:l3">Eer</rhyme>.</l>
      </lg>
      <lg id="s38" type="stanza">
        <l id="s38:l1" met="4" type="+-">Voor u Eer pleeght gy te <rhyme label="a" type="f">strijden</rhyme>,</l>
        <l id="s38:l2" met="4" type="+-">En niet ongestraft te <rhyme label="a" type="f" corresp="#s38:l1">lijden</rhyme></l>
        <l id="s38:l3" met="4" type="+-">'t Schenden van uw' Heyligh <rhyme label="b" type="m">Woordt</rhyme>,</l>
        <l id="s38:l4" met="4" type="+-">Sult gy dien dan niet <rhyme label="c" type="f">vergelden</rhyme>,</l>
        <l id="s38:l5" met="4" type="+-">Die u Leer en Eer soo <rhyme label="c" type="f" corresp="#s38:l4">schelden</rhyme>,</l>
        <l id="s38:l6" met="4" type="+-">Datmen schrickt, wanneer men 't <rhyme label="b" type="m" corresp="#s38:l3">hoort</rhyme>?</l>
      </lg>
      <lg id="s39" type="stanza">
        <l id="s39:l1" met="4" type="+-">Heer en wilt niet langer <rhyme label="a" type="f">slapen</rhyme>,</l>
        <l id="s39:l2" met="4" type="+-">Treckt eens aen uw's yvers <rhyme label="a" type="f" corresp="#s39:l1">wapen</rhyme>,</l>
        <l id="s39:l3" met="4" type="+-">Gordd' u wraeck-zweert op u <rhyme label="b" type="m">zijd'</rhyme>:</l>
        <l id="s39:l4" met="4" type="+-">Neemt u Boogh, u snelle <rhyme label="c" type="f">Flitsen</rhyme>,</l>
        <l id="s39:l5" met="4" type="+-">Valt met kracht op Babels <rhyme label="c" type="f" corresp="#s39:l4">spitsen</rhyme>,</l>
        <l id="s39:l6" met="4" type="+-">Toont dat gy haer Vyandt <rhyme label="b" type="m" corresp="#s39:l3">zijt</rhyme></l>
      </lg>
      <lg id="s40" type="stanza">
        <l id="s40:l1" met="4" type="+-">Slaet den snooden mensch der <rhyme label="a" type="f">sonde</rhyme>,</l>
        <l id="s40:l2" met="4" type="+-">Door uw's aessem kracht te <rhyme label="a" type="f" corresp="#s40:l1">gronde</rhyme>:</l>
        <l id="s40:l3" met="4" type="+-">Laet dees' zwart en duyster <rhyme label="b" type="m">Wolck</rhyme>,</l>
        <l id="s40:l4" met="4" type="+-">Door uw's aenschijns glans <rhyme label="c" type="f">verdwijnen</rhyme>,</l>
        <l id="s40:l5" met="4" type="+-">Laet de Vreughden Sonn' eens <rhyme label="c" type="f" corresp="#s40:l4">schijnen</rhyme></l>
        <l id="s40:l6" met="4" type="+-">Over u ellendigh <rhyme label="b" type="m" corresp="#s40:l3">Volck</rhyme>!</l>
      </lg>
      <lg id="s41" type="stanza">
        <l id="s41:l1" met="4" type="+-">Laet ons klachten u <rhyme label="a" type="f">op-wecken</rhyme></l>
        <l id="s41:l2" met="4" type="+-">Helpt ons sonder lang <rhyme label="a" type="f" corresp="#s41:l1">vertrecken</rhyme>,</l>
        <l id="s41:l3" met="4" type="+-">Helpt ons om des Heeren <rhyme label="b" type="m">wil</rhyme>.</l>
        <l id="s41:l4" met="4" type="+-">Eyndigend' ons droeve <rhyme label="c" type="f">klachten</rhyme></l>
        <l id="s41:l5" met="4" type="+-">Sullen wy u heyl <rhyme label="c" type="f" corresp="#s41:l4">verwachten</rhyme>,</l>
        <l id="s41:l6" met="4" type="+-">En in hoope zwijgen <rhyme label="b" type="m" corresp="#s41:l3">stil</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
