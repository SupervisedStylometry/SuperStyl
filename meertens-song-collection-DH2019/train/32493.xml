  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">MInnelijck schoon,, <rhyme label="a" type="m">gelaet</rhyme>,,</l>
        <l id="s1:l2" met="3" type="+-">Rancken myns levens <rhyme label="b" type="f">waerdigh</rhyme></l>
        <l id="s1:l3" met="3" type="+-">In Venus throon,, <rhyme label="a" type="m" corresp="#s1:l1">bestaet</rhyme>,,</l>
        <l id="s1:l4" met="3" type="+-">Gy, als een Bloemken <rhyme label="b" type="f" corresp="#s1:l2">aerdigh</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Mocht my tot <rhyme label="c" type="m">loff</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">U schoonheyt eens <rhyme label="d" type="f">gelucken</rhyme>?</l>
        <l id="s1:l7" met="3" type="+-">Ick sou u <rhyme label="d" type="f" corresp="#s1:l6">aff-plucken</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Voor 't schoonste inden <rhyme label="c" type="m" corresp="#s1:l5">Hoff</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Daer en is geene,, <rhyme label="a" type="m">Maeght</rhyme>,,</l>
        <l id="s2:l2" met="3" type="+-">Die myne smerten,, <rhyme label="b" type="f">bluste</rhyme>,,</l>
        <l id="s2:l3" met="3" type="+-">Dan gy alleene,, <rhyme label="a" type="m" corresp="#s2:l1">draeght</rhyme>,,</l>
        <l id="s2:l4" met="3" type="+-">'t Versterck der herten,, <rhyme label="b" type="f" corresp="#s2:l2">ruste</rhyme>,,</l>
        <l id="s2:l5" met="3" type="+-">Door-schoten,, <rhyme label="c" type="m">Lieff</rhyme>,,</l>
        <l id="s2:l6" met="3" type="+-">Jont my u soetelijck <rhyme label="d" type="f">wesen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Sent my dogh 't <rhyme label="d" type="f" corresp="#s2:l6">genesen</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">In een besloten,, <rhyme label="c" type="m" corresp="#s2:l5">Brieff</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Ingelijt weder,, <rhyme label="a" type="m">paer</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Voor swaer tempeest, en <rhyme label="b" type="f">donder</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Syt gy den Ceder,, <rhyme label="a" type="m" corresp="#s3:l1">daer</rhyme>,,</l>
        <l id="s3:l4" met="3" type="+-">Mijne ziel sal rusten <rhyme label="b" type="f" corresp="#s3:l2">onder</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Der Sonnen,, <rhyme label="c" type="m">schyn</rhyme>,,</l>
        <l id="s3:l6" met="3" type="+-">Passeert gy in 't <rhyme label="d" type="f">geheele</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Valt gy 't mijnen <rhyme label="d" type="f" corresp="#s3:l6">deele</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">'t Sal veel gewonnen,, <rhyme label="c" type="m" corresp="#s3:l5">sijn</rhyme>,,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Sulck en behaegen,, <rhyme label="a" type="m">vast</rhyme>,,</l>
        <l id="s4:l2" met="3" type="+-">Draegh ick u jeught,, ter,, <rhyme label="b" type="f">eeren</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Geheel der plaegen,, <rhyme label="a" type="m" corresp="#s4:l1">last</rhyme>,,</l>
        <l id="s4:l4" met="3" type="+-">Kont gy in vreught,, ver,, <rhyme label="b" type="f" corresp="#s4:l2">keeren</rhyme>,,</l>
        <l id="s4:l5" met="3" type="+-">Ter schanden,, <rhyme label="c" type="m">laet</rhyme>,,</l>
        <l id="s4:l6" met="3" type="+-">U Dienaer niet <rhyme label="d" type="f">bederven</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Mijn leven, en <rhyme label="d" type="f" corresp="#s4:l6">sterven</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">In uwe handen,, <rhyme label="c" type="m" corresp="#s4:l5">staet</rhyme>,,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Ach mijn Princersse,, <rhyme label="a" type="m">soet</rhyme>,,</l>
        <l id="s5:l2" met="3" type="+-">In danck wilt dit <rhyme label="b" type="f">aenvaeten</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Jae mijn troostersse,, <rhyme label="a" type="m" corresp="#s5:l1">moet</rhyme>,,</l>
        <l id="s5:l4" met="3" type="+-">Gy sijn sonder <rhyme label="b" type="f" corresp="#s5:l2">aff-laeten</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Hier mede,, <rhyme label="c" type="m">schreeft</rhyme>,,</l>
        <l id="s5:l6" met="3" type="+-">Mijn handt dat u <rhyme label="d" type="f">ontmoeten</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Hondert duysent <rhyme label="d" type="f" corresp="#s5:l6">groeten</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">En voorts in vrede,, <rhyme label="c" type="m" corresp="#s5:l5">leeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
