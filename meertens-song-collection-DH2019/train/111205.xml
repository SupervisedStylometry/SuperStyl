  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Sijn Vader was een <rhyme label="b" type="m">Kingh</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En als de Bel gebroken <rhyme label="a" type="m" corresp="#s1:l1">is</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dan kloptmen met den <rhyme label="b" type="m" corresp="#s1:l2">ring</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Prins Robbert was, doch 't <rhyme label="a" type="m">Parlement</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Nu Meester is van 't <rhyme label="b" type="m">Spel</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En als den Hemel niet en <rhyme label="a" type="m" corresp="#s2:l1">wil</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Het waeght het met de <rhyme label="b" type="m" corresp="#s2:l2">Hel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Het Parlement een <rhyme label="b" type="m">Boer</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">En Fairfax is de <rhyme label="a" type="m" corresp="#s3:l1">Besem-stock</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daer vaeghtmen mee <rhyme label="b" type="m" corresp="#s3:l2">de</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Het Parlement een <rhyme label="b" type="m">Foel</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Het bruyt sijn Koningh in een <rhyme label="a" type="m" corresp="#s4:l1">gat</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En set sich in den <rhyme label="b" type="m" corresp="#s4:l2">Stoel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Het Parlement is <rhyme label="b" type="m">dul</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Het neemt den slechten Adel <rhyme label="a" type="m" corresp="#s5:l1">an</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Voor 's Koninghs Heyligh <rhyme label="b" type="m" corresp="#s5:l2">Hul</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Het Parlement is <rhyme label="b" type="m">dat</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Het neemt de Kroon al van sijn <rhyme label="a" type="m" corresp="#s6:l1">Hooft</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En set die op sijn <rhyme label="b" type="m" corresp="#s6:l2">gat</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Het Parlement is <rhyme label="b" type="m">braef</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Het scheurt sijn <rhyme label="a" type="m" corresp="#s7:l1">Privilegi-brief</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En maeckt het Lant een <rhyme label="b" type="m" corresp="#s7:l2">Slaef</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Het Raedt-Hof een <rhyme label="b" type="m">Tyrant</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Het tijght sijn Koningh leugens <rhyme label="a" type="m" corresp="#s8:l1">an</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Rooft, Moort het gantsche <rhyme label="b" type="m" corresp="#s8:l2">Lant</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Het Parlement wat <rhyme label="b" type="m">gauws</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">Verrijst, ach Quijn <rhyme label="a" type="m" corresp="#s9:l1">Elisabeth</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">De Duyvel wort hier <rhyme label="b" type="m" corresp="#s9:l2">Paus</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Uw' zegen, u gesalfde <rhyme label="a" type="m">Hant</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die Godt u Vollick <rhyme label="b" type="m">gaf</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Die bant men nu uyt <rhyme label="a" type="m" corresp="#s10:l1">Engelant</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Van alle Zielen <rhyme label="b" type="m" corresp="#s10:l2">af</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Uw' Godt-dienst, met u <rhyme label="a" type="m">Kerck-boeck</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Die traptmen met den <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">End' al u Priesters Hembden <rhyme label="a" type="m" corresp="#s11:l1">wit</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Zijn droevigh Mart'laers <rhyme label="b" type="m" corresp="#s11:l2">bloet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Het Parlement <rhyme label="b" type="m">vermaert</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Dat kapt de Hel haer Staerten <rhyme label="a" type="m" corresp="#s12:l1">af</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Blijft selfs den droes op <rhyme label="b" type="m" corresp="#s12:l2">Aert</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Quijn Lijsbeth; wat is nu u <rhyme label="a" type="m">Rijck</rhyme>?</l>
        <l id="s13:l2" met="3" type="-+">'t Gebiedt van een <rhyme label="b" type="m">Soldaet</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Of een seer slechte <rhyme label="a" type="m" corresp="#s13:l1">Republijck</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Die onder Cromwel <rhyme label="b" type="m" corresp="#s13:l2">staet</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Prins Robbert is een <rhyme label="a" type="m">Gentilman</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Het Parlement is <rhyme label="b" type="m">sot</rhyme>:</l>
        <l id="s14:l3" met="4" type="-+">Om dat het nu geen Koningh <rhyme label="a" type="m" corresp="#s14:l1">heeft</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">En is oock sonder <rhyme label="b" type="m" corresp="#s14:l2">Godt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
