<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>189778</id>
    <date>
      <year-start>1750</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Windje daar het bos af drilt</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">AArdig Nimfje hoe dus te <rhyme label="a" type="m">staan</rhyme>?</l>
        <l id="s1:l2" met="4" type="+-">Koom laat ik u bieden <rhyme label="a" type="m" corresp="#s1:l1">aan</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Zoo veel <rhyme label="b" type="f">kuschjes</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Als 'k wel <rhyme label="b" type="f" corresp="#s1:l3">lusjes</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Om u voe, en geeft 'er <rhyme label="c" type="m">myn</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zoo veel als in uw schoonheen <rhyme label="c" type="m" corresp="#s1:l5">zyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Herder, wel aan, laat ons een <rhyme label="a" type="m">net</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Maaken van gekus, waar <rhyme label="a" type="m" corresp="#s2:l1">met</rhyme>.</l>
        <l id="s2:l3" met="2" type="+-">Ons de <rhyme label="b" type="f">minne</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">'t Zaam mag <rhyme label="b" type="f" corresp="#s2:l3">winnen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Koom doch laat uw mond aan <rhyme label="c" type="m">myn</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En een bie en een bloempje <rhyme label="c" type="m" corresp="#s2:l5">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nimfje hoe voel 'k my <rhyme label="a" type="m">gewondt</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Door dees uw zoeten <rhyme label="a" type="m" corresp="#s3:l1">mondt</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Wiens schoon <rhyme label="b" type="f">blaken</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Straks zal <rhyme label="b" type="f" corresp="#s3:l3">maaken</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Dat elks ziel een Phoenix <rhyme label="c" type="m">wert</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En een zonne bloem zyn <rhyme label="c" type="m" corresp="#s3:l5">hert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">ô! Wat voel ik in myn <rhyme label="a" type="m">zin</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Vreuchde door uw zoete <rhyme label="a" type="m" corresp="#s4:l1">min</rhyme>?</l>
        <l id="s4:l3" met="2" type="+-">Wat voor <rhyme label="b" type="f">lusjes</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Door uw <rhyme label="b" type="f" corresp="#s4:l3">kusjes</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">My dagt dat ik uit een <rhyme label="c" type="m">zee</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Van zyn Coraal dronk zoete <rhyme label="c" type="m" corresp="#s4:l5">mee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Zoo lang de klyn gevlerkte <rhyme label="a" type="m">biên</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Zullen om haar hooning <rhyme label="a" type="m" corresp="#s5:l1">vlien</rhyme></l>
        <l id="s5:l3" met="2" type="+-">Tot de <rhyme label="b" type="f">roosjes</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Ryk van <rhyme label="b" type="f" corresp="#s5:l3">bloosjes</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Zal ik dragen in myn <rhyme label="c" type="m">oog</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Laura, die meest myn ziel <rhyme label="c" type="m" corresp="#s5:l5">ontoog</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zoo lang de jonge <rhyme label="a" type="m">tullipaan</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Vreucht zal door de zon <rhyme label="a" type="m" corresp="#s6:l1">ontfaan</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Die 's het <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s6:l4" met="2" type="+-">Scheint te <rhyme label="b" type="f" corresp="#s6:l3">geven</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Zal ik dragen in myn <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Van wie 'k in 't oog gedragen <rhyme label="c" type="m" corresp="#s6:l5">wert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
