  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zoete en alderliefste <rhyme label="a" type="f">Roosje</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Zielsbeminde, lieve <rhyme label="a" type="f" corresp="#s1:l1">troosje</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">'k Bid u, blyft een weinig <rhyme label="b" type="m">staan</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">'k Zeg u, Cloris, laat my <rhyme label="b" type="m" corresp="#s1:l3">gaan</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'k Moet myn Schaapjes gaan <rhyme label="c" type="f">verweien</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">Engel, laat ik u <rhyme label="c" type="f" corresp="#s1:l5">geleien</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">'t Is niet noodig, blyft maar <rhyme label="d" type="m">hier</rhyme>.</l>
        <l id="s1:l8" met="4" type="+-">Stookster van myn <rhyme label="d" type="m" corresp="#s1:l7">minnevier</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Zult gy my dan eeuwig <rhyme label="e" type="f">haaten</rhyme>?</l>
        <l id="s1:l10" met="4" type="+-">'k Weet niet wat je al moogt <rhyme label="e" type="f" corresp="#s1:l9">praaten</rhyme>.</l>
      </lg>
    </lg>
  </text>
