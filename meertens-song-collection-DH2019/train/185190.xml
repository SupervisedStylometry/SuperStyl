<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185190</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Engelse fortuin</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Den soeten <rhyme label="a" type="m">dwanck</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Van ‘t Euangely <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Maeckt mijnen <rhyme label="a" type="m" corresp="#s1:l1">ganck</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghewilligh tot het <rhyme label="b" type="m" corresp="#s1:l2">goet</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Soo dat ick <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Tuyghen voor yder <rhyme label="d" type="m">man</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">De Liefde <rhyme label="c" type="m" corresp="#s1:l5">doet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dat vreese niet en <rhyme label="d" type="m" corresp="#s1:l6">can</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Vreese der <rhyme label="a" type="m">Wet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Slaefachtigh my <rhyme label="b" type="m">ghebiedt</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">En <rhyme label="a" type="m" corresp="#s2:l1">altemet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wt vreese doen ick <rhyme label="b" type="m" corresp="#s2:l2">yet</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">En om ‘t <rhyme label="c" type="m">gheniet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Begin ick wel den <rhyme label="d" type="m">strijdt</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Maer Liefde <rhyme label="c" type="m" corresp="#s2:l5">siet</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Maeckt alle vreese <rhyme label="d" type="m" corresp="#s2:l6">quyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Liefde <rhyme label="a" type="m">verteert</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Haer selven <rhyme label="b" type="m">onbeswaert</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">En ‘s en <rhyme label="a" type="m" corresp="#s3:l1">begeert</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Niet dat onwille <rhyme label="b" type="m" corresp="#s3:l2">baert</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Haer edel <rhyme label="c" type="m">aert</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Noyt op vergeldingh <rhyme label="d" type="m">sach</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Want hare <rhyme label="c" type="m" corresp="#s3:l5">waerd</rhyme>’</l>
        <l id="s3:l8" met="3" type="-+">Niemant betalen <rhyme label="d" type="m" corresp="#s3:l6">mach</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Liefde <rhyme label="a" type="m">ontfanght</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Alles met <rhyme label="b" type="m">danckbaerheydt</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">En wat <rhyme label="a" type="m" corresp="#s4:l1">s’uytlanght</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gheschiet uyt <rhyme label="b" type="m" corresp="#s4:l2">mildicheydt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Wijt <rhyme label="c" type="m">uytghebreydt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Is sy boven <rhyme label="d" type="m">natuur</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Haren <rhyme label="c" type="m" corresp="#s4:l5">arbeyt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Valt nimmermeer te <rhyme label="d" type="m" corresp="#s4:l6">suur</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Dit is den <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van al wat Godt <rhyme label="b" type="m">beveelt</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Spiegelt u <rhyme label="a" type="m" corresp="#s5:l1">in</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat Goddelijcke <rhyme label="b" type="m" corresp="#s5:l2">beelt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Wt hem <rhyme label="c" type="m">gheteelt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Den Soon vol Liefden <rhyme label="d" type="m">gloet</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Die niet en <rhyme label="c" type="m" corresp="#s5:l5">heelt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Van al sijns vaders <rhyme label="d" type="m" corresp="#s5:l6">goet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Elck <rhyme label="a" type="m">disch-genood</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Hy aen sijn tafel <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Willigh <rhyme label="a" type="m" corresp="#s6:l1">onbloot</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En maecktet al <rhyme label="b" type="m" corresp="#s6:l2">gemeyn</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Wt sijn <rhyme label="c" type="m">fonteyn</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Drinckt elck wie daer <rhyme label="d" type="m">comt</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Dit edel <rhyme label="c" type="m" corresp="#s6:l5">greyn</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Heeft noyt yemant <rhyme label="d" type="m" corresp="#s6:l6">verdoemt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Sijn wesen <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Elck een het leven <rhyme label="b" type="m">gaf</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">‘t Verkeert <rhyme label="a" type="m" corresp="#s7:l1">gemoed</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Valt selver van hem <rhyme label="b" type="m" corresp="#s7:l2">af</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Gelijck het <rhyme label="c" type="m">caf</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Verweystert van den <rhyme label="d" type="m">wint</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Sijn eygen <rhyme label="c" type="m" corresp="#s7:l5">straf</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Is hy, die ‘t quaet <rhyme label="d" type="m" corresp="#s7:l6">bemint</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">O Liefden <rhyme label="a" type="m">vyer</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">Dat deught volbrenght <rhyme label="b" type="m">alleen</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Seer <rhyme label="a" type="m" corresp="#s8:l1">goedertier</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Maeckt u met ons <rhyme label="b" type="m" corresp="#s8:l2">ghemeen</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Op dat ons <rhyme label="c" type="m">geen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Verkeertheyd en <rhyme label="d" type="m">bevanght</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Want met u <rhyme label="c" type="m" corresp="#s8:l5">een</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Wort hy, die u <rhyme label="d" type="m" corresp="#s8:l6">aenhanght</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
