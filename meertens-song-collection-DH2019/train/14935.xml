  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">TRotse Velt <rhyme label="a" type="f">Goddinne</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Die u wooningh <rhyme label="b" type="m">hout</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Int murgh van myn <rhyme label="a" type="f" corresp="#s1:l1">minne</rhyme></l>
        <l id="s1:l4" met="3" type="+-">My nu eens <rhyme label="b" type="m" corresp="#s1:l2">beschout</rhyme></l>
        <l id="s1:l5" met="4" type="+-">D’ellend die ghy my doet <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ofte u niet <rhyme label="b" type="m" corresp="#s1:l2">berouwt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ick ben heel <rhyme label="a" type="f">verbroocken</rhyme></l>
        <l id="s2:l2" met="3" type="+-">En soo dor als <rhyme label="b" type="m">hout</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Mijn ghebeente <rhyme label="a" type="f" corresp="#s2:l1">kroocken</rhyme></l>
        <l id="s2:l4" met="3" type="+-">En myn siel is <rhyme label="b" type="m" corresp="#s2:l2">kout</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Siet d’ellend die ick moet <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ofte u niet <rhyme label="b" type="m" corresp="#s2:l2">berouwt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Ick beschrey mijn <rhyme label="a" type="f">jeughde</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Die nu dood en <rhyme label="b" type="m">out</rhyme></l>
        <l id="s3:l3" met="3" type="+-">In mijn sonden <rhyme label="a" type="f" corresp="#s3:l1">vreuchde</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Staeght vergaet en <rhyme label="b" type="m" corresp="#s3:l2">stout</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Siet d’ellent die ick moet <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ofte u niet <rhyme label="b" type="m" corresp="#s3:l2">berout</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Mijn uytgheteerde <rhyme label="a" type="f">leden</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Door u min <rhyme label="b" type="m">ghebrout</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">My slepen nae <rhyme label="a" type="f" corresp="#s4:l1">beneden</rhyme></l>
        <l id="s4:l4" met="3" type="+-">In des doot <rhyme label="b" type="m" corresp="#s4:l2">behout</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Siet d’ellend die ick moet <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ofte u niet <rhyme label="b" type="m" corresp="#s4:l2">berout</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Mijn siel voel ick <rhyme label="a" type="f">vluchten</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Die soo menich <rhyme label="b" type="m">fout</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Nu heeft moeten <rhyme label="a" type="f" corresp="#s5:l1">suchten</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Om dat ghy saeght <rhyme label="b" type="m" corresp="#s5:l2">stout</rhyme></l>
        <l id="s5:l5" met="4" type="+-">D ellent die ghy my doet <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Sonder datse u <rhyme label="b" type="m" corresp="#s5:l2">berouwt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
