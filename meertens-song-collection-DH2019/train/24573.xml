<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24573</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
      <tag standard-name="verhalend lied" translation="narrative song">verhalend lied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SIet ende merket een schoen <rhyme label="a" type="f">agniete</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die myn herte seer <rhyme label="b" type="m">wtuercoes</rhyme></l>
        <l id="s1:l3" met="4" type="-+">ende laetes v niet <rhyme label="a" type="f" corresp="#s1:l1">verdrieten</rhyme></l>
        <l id="s1:l4" met="4" type="-+">mynnet dese schoene <rhyme label="b" type="m" corresp="#s1:l2">roese</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Sy is een maen een maen een <rhyme label="c" type="f">mane</rhyme></l>
        <l id="s1:l6" met="4" type="-+">blenckende inden hogen <rhyme label="d" type="m">throen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">bauen ander maegden <rhyme label="e" type="f">voele</rhyme></l>
        <l id="s1:l8" met="4" type="-+">soe blencket sy wtter maten <rhyme label="d" type="m" corresp="#s1:l6">schoen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">God die here heeft hoer <rhyme label="a" type="f">gegeuen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">enen rynck van golde <rhyme label="b" type="m">fyn</rhyme></l>
        <l id="s2:l3" met="4" type="-+">christus bruyt is sy <rhyme label="a" type="f" corresp="#s2:l1">gebleuen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">bauen alle maegde <rhyme label="b" type="m" corresp="#s2:l2">fyn</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Sy is een maen een maen een <rhyme label="c" type="f">mane</rhyme></l>
        <l id="s2:l6" met="4" type="-+">blenckende inden hogen <rhyme label="d" type="m">throen</rhyme></l>
        <l id="s2:l7" met="4" type="-+">bauen ander maegden <rhyme label="e" type="f">voele</rhyme></l>
        <l id="s2:l8" met="4" type="-+">soe blencket sy wtter maten <rhyme label="d" type="m" corresp="#s2:l6">schoen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Des richters gelt ende al syn <rhyme label="a" type="f">raet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">des Jongelynges <rhyme label="b" type="m">edeldom</rhyme></l>
        <l id="s3:l3" met="4" type="-+">dat heeft sy altemael <rhyme label="a" type="f" corresp="#s3:l1">versmaet</rhyme></l>
        <l id="s3:l4" met="4" type="-+">om christus hoeren <rhyme label="b" type="m" corresp="#s3:l2">brudegom</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Sy is een maen een maen een <rhyme label="c" type="f">mane</rhyme></l>
        <l id="s3:l6" met="4" type="-+">blenckende inden hogen <rhyme label="d" type="m">throen</rhyme></l>
        <l id="s3:l7" met="4" type="-+">bauen ander maegden <rhyme label="e" type="f">voele</rhyme></l>
        <l id="s3:l8" met="4" type="-+">soe blencket sy wtter maten <rhyme label="d" type="m" corresp="#s3:l6">schoen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">In een bordeel waert sy <rhyme label="a" type="f">geleyt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">een camerkyn was hoer <rhyme label="b" type="m">bereyt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">onbeulect ende gade <rhyme label="a" type="f" corresp="#s4:l1">bekent</rhyme></l>
        <l id="s4:l4" met="4" type="-+">bleef sy in hoere <rhyme label="b" type="m" corresp="#s4:l2">suuerheit</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Sy is een maen een maen een <rhyme label="c" type="f">mane</rhyme></l>
        <l id="s4:l6" met="4" type="-+">blenckende inden hogen <rhyme label="d" type="m">throen</rhyme></l>
        <l id="s4:l7" met="4" type="-+">bauen ander maegden <rhyme label="e" type="f">voele</rhyme></l>
        <l id="s4:l8" met="4" type="-+">soe blencket sy wtter maten <rhyme label="d" type="m" corresp="#s4:l6">schoen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Agniete dede datmen hoer <rhyme label="a" type="f">bat</rhyme></l>
        <l id="s5:l2" met="4" type="-+">sy viel in hoer <rhyme label="b" type="m">gebedekyn</rhyme></l>
        <l id="s5:l3" met="4" type="-+">ende den die engel geslagen <rhyme label="a" type="f" corresp="#s5:l1">had</rhyme></l>
        <l id="s5:l4" met="4" type="-+">den heeft sy vander doet <rhyme label="b" type="m" corresp="#s5:l2">verwecket</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Sy is een maen een maen een <rhyme label="c" type="f">mane</rhyme></l>
        <l id="s5:l6" met="4" type="-+">blenckende inden hogen <rhyme label="d" type="m">throen</rhyme></l>
        <l id="s5:l7" met="4" type="-+">bauen ander maegden <rhyme label="e" type="f">voele</rhyme></l>
        <l id="s5:l8" met="4" type="-+">soe blencket sy wtter maten <rhyme label="d" type="m" corresp="#s5:l6">schoen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hoer naectheit was mit haer <rhyme label="a" type="f">bedecket</rhyme></l>
        <l id="s6:l2" met="4" type="-+">in een vuer waert sy <rhyme label="b" type="m">geset</rhyme></l>
        <l id="s6:l3" met="4" type="-+">daer bleef sy staen al <rhyme label="a" type="f" corresp="#s6:l1">onbeulecket</rhyme></l>
        <l id="s6:l4" met="4" type="-+">vander vlammen <rhyme label="b" type="m" corresp="#s6:l2">onbelet</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Sy is een maen een maen een <rhyme label="c" type="f">mane</rhyme></l>
        <l id="s6:l6" met="4" type="-+">blenckende inden hogen <rhyme label="d" type="m">throen</rhyme></l>
        <l id="s6:l7" met="4" type="-+">bauen ander maegden <rhyme label="e" type="f">voele</rhyme></l>
        <l id="s6:l8" met="4" type="-+">soe blencket sy wtter maten <rhyme label="d" type="m" corresp="#s6:l6">schoen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">In hoeren hals waert sy <rhyme label="a" type="f">gesneden</rhyme></l>
        <l id="s7:l2" met="4" type="-+">mit enen sweerde seer <rhyme label="b" type="m">Jamerlic</rhyme></l>
        <l id="s7:l3" met="4" type="-+">dus heeft sy den doet <rhyme label="a" type="f" corresp="#s7:l1">geleden</rhyme></l>
        <l id="s7:l4" met="4" type="-+">die daer leuet <rhyme label="b" type="m" corresp="#s7:l2">ewelic</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Sy is een maen een maen een <rhyme label="c" type="f">mane</rhyme></l>
        <l id="s7:l6" met="4" type="-+">blenckende inden hogen <rhyme label="d" type="m">throen</rhyme></l>
        <l id="s7:l7" met="4" type="-+">bauen ander maegden <rhyme label="e" type="f">voele</rhyme></l>
        <l id="s7:l8" met="4" type="-+">soe blencket sy wtter maten <rhyme label="d" type="m" corresp="#s7:l6">schoen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dese bruyt laet ons <rhyme label="a" type="f">aendencken</rhyme></l>
        <l id="s8:l2" met="4" type="-+">ende bidden oer seer <rhyme label="b" type="m">ynnichlic</rhyme></l>
        <l id="s8:l3" met="4" type="-+">dat sy ons wil lieflick <rhyme label="a" type="f" corresp="#s8:l1">schencken</rhyme></l>
        <l id="s8:l4" met="4" type="-+">gades hulpe van <rhyme label="b" type="m" corresp="#s8:l2">hemelryke</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Sy is een maen een maen een <rhyme label="c" type="f">maene</rhyme></l>
        <l id="s8:l6" met="4" type="-+">blenckende inden hogen <rhyme label="d" type="m">throen</rhyme></l>
        <l id="s8:l7" met="4" type="-+">bauen ander maegden <rhyme label="e" type="f">voele</rhyme></l>
        <l id="s8:l8" met="4" type="-+">soe blencket sy wtter maten <rhyme label="d" type="m" corresp="#s8:l6">schoen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
