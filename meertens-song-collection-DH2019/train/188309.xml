<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188309</id>
    <date>
      <year-start>1780</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach Phyllis lief mijn hartedief</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Zoelen <rhyme label="a" type="m">Wind</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Die dartlend <rhyme label="a" type="m" corresp="#s1:l1">Mind</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En speelt met groene <rhyme label="b" type="f">bladen</rhyme>:</l>
        <l id="s1:l4" met="2" type="-+">De Bloempjes <rhyme label="c" type="m">kust</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En zagjes <rhyme label="c" type="m" corresp="#s1:l4">rust</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Op Floraas Feest <rhyme label="b" type="f" corresp="#s1:l3">gewaden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">'k Beny uw <rhyme label="a" type="m">vreugd</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">En zoet <rhyme label="a" type="m" corresp="#s2:l1">geneugd</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wyl ik moet eenzaam <rhyme label="b" type="f">zwerven</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">En zugt en <rhyme label="c" type="m">kwyn</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Om <rhyme label="c" type="m" corresp="#s2:l4">Rozalyn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die ik helaas moet <rhyme label="b" type="f" corresp="#s2:l3">derven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Vergeefs klaag <rhyme label="a" type="m">ik</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Elk <rhyme label="a" type="m" corresp="#s3:l1">oogenblik</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Myn leed aan stomme <rhyme label="b" type="f">Boomen</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Myn <rhyme label="c" type="m">Rozalyn</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Om wie ik <rhyme label="c" type="m" corresp="#s3:l4">kwyn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Heeft my myn vreugd <rhyme label="b" type="f" corresp="#s3:l3">ontnoomen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Haar wreed <rhyme label="a" type="m">gemoed</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Geen meely <rhyme label="a" type="m" corresp="#s4:l1">voed</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Nog Honing zoete <rhyme label="b" type="f">Kusjes</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Die <rhyme label="c" type="m">Herderin</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Verbant de <rhyme label="c" type="m" corresp="#s4:l4">Min</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En dartle Minne <rhyme label="b" type="f" corresp="#s4:l3">Lusjes</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Nu zy my <rhyme label="a" type="m">haat</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">En wreed <rhyme label="a" type="m" corresp="#s5:l1">verlaat</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ontwyk ik ook haar <rhyme label="b" type="f">Oogen</rhyme>:</l>
        <l id="s5:l4" met="2" type="-+">Want al myn <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">En min <rhyme label="c" type="m" corresp="#s5:l4">geneugd</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Is met haar weg <rhyme label="b" type="f" corresp="#s5:l3">gevloogen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
