<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>164016</id>
    <date>
      <year-start>1780</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam (reguliersbreestraat)</orginal-name>
      <standard-name>Reguliersbreestraat, Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3665346</lon>
        <lat>4.8948014</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Hoe zoet is het daar de vriendschap woont</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WY wenschen uw Geluk, ô! <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met Hollands <rhyme label="b" type="m">Staats-bewind</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Het Ampt dat uwe Schouders <rhyme label="a" type="m" corresp="#s1:l1">torst</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maakt gantsch Euroop’ <rhyme label="b" type="m" corresp="#s1:l2">bevrind</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">God schenk uw Kragt en wys <rhyme label="c" type="m">Beleid</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">In dees zo kommerlyke <rhyme label="c" type="m" corresp="#s1:l5">Tyd</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Hoezee, Hoezee, <rhyme label="d" type="m">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Iuich dan, ô! Amstel, Maas en <rhyme label="a" type="m">Vegt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Iuich dan, ô! <rhyme label="b" type="m">Batavier</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Bekroon zyn Kruin en Blonde <rhyme label="a" type="m" corresp="#s2:l1">Vlegt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met Kranszen van <rhyme label="b" type="m" corresp="#s2:l2">Lauwrier</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Geef Willem altoos Eer en <rhyme label="c" type="m">Lof</rhyme>:</l>
        <l id="s2:l6" met="4" type="-+">Orange tot in ‘t Hemels <rhyme label="c" type="m" corresp="#s2:l5">Hof</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Hoezee, Hoezee, <rhyme label="d" type="m">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laat Frisoo’s Naam zyn in ons <rhyme label="a" type="m">Hart</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zyn Deugd zy ons een <rhyme label="b" type="m">Muur</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Waar op de Nyd en Afgunst <rhyme label="a" type="m" corresp="#s3:l1">zart</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Tot spyt der <rhyme label="b" type="m" corresp="#s3:l2">Pallinuur</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Want God verlaat zyn Vromen <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Orange Troost ons in ’t <rhyme label="c" type="m" corresp="#s3:l5">Verdriet</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Hoezee, Hoezee, <rhyme label="d" type="m">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Orange leeft in Neerlands <rhyme label="a" type="m">Thuin</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De Leeuw beschut zyn <rhyme label="b" type="m">Held</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Hy toont zyn Klaauw uit Stof en <rhyme label="a" type="m" corresp="#s4:l1">Puin</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Door Vryheids Maagd <rhyme label="b" type="m" corresp="#s4:l2">hersteld</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Hy mind de Vreede, lang <rhyme label="c" type="m">verwagt</rhyme>;</l>
        <l id="s4:l6" met="4" type="-+">Orange Bloeid in volle <rhyme label="c" type="m" corresp="#s4:l5">kragt</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Hoezee, Hoezee, <rhyme label="d" type="m">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">ô! Hemel! gun dien waarde <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Zyn Annaa’s Lust en <rhyme label="b" type="m">Schat</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Een plaats in uwe <rhyme label="a" type="m" corresp="#s5:l1">Tempelbouw</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">In Salems goude <rhyme label="b" type="m" corresp="#s5:l2">Stad</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Weest gy haar Schild en <rhyme label="c" type="m">Beukelaar</rhyme>:</l>
        <l id="s5:l6" met="4" type="-+">Orange hoed ons voor <rhyme label="c" type="m" corresp="#s5:l5">Gevaar</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Hoezee, Hoezee, <rhyme label="d" type="m">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">ô! Sterke God, geeft <rhyme label="a" type="m">Carolyn</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Veel Zegen uit uw <rhyme label="b" type="m">Throon</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Laat uwe Geest haar sproeijen <rhyme label="a" type="m" corresp="#s6:l1">Rein</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Met Dauw van uw <rhyme label="b" type="m" corresp="#s6:l2">Geboon</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Weest gy haar Burg en vaste <rhyme label="c" type="m">Band</rhyme>:</l>
        <l id="s6:l6" met="4" type="-+">Orange zy ons Hart en <rhyme label="c" type="m" corresp="#s6:l5">Hand</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Hoezee, Hoezee, <rhyme label="d" type="m">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy gaf op ’s Volks Gebed aan ’t <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Een Spruit, en Manlyk <rhyme label="b" type="m">Zaad</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Staat eeuwig aan zyn <rhyme label="a" type="m" corresp="#s7:l1">Regterhand</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Tot steun van Kerk en <rhyme label="b" type="m" corresp="#s7:l2">Staat</rhyme>;</l>
        <l id="s7:l5" met="4" type="-+">Verlaat hem nimmer in de <rhyme label="c" type="m">Nood</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Orangje tot de bleeke <rhyme label="c" type="m" corresp="#s7:l5">Dood</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Hoezee, Hoezee, <rhyme label="d" type="m">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Zo Leeft de dierb’re Vryheid <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Zo Bloeit de <rhyme label="b" type="m">Koopmanschap</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Want alles komt van u den <rhyme label="a" type="m" corresp="#s8:l1">Heer</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Help ons ter <rhyme label="b" type="m" corresp="#s8:l2">Glorie-Trap</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">Sta Holland by in Druk of <rhyme label="c" type="m">Stryd</rhyme>;</l>
        <l id="s8:l6" met="4" type="-+">Orangje tot in <rhyme label="c" type="m" corresp="#s8:l5">Eeuwigheid</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Hoezee, Hoezee, <rhyme label="d" type="m">Hoezee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
