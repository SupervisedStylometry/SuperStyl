<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183107</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zoutmans glorie</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Komt nu, trouwe <rhyme label="a" type="f">Batavieren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wilt nu voor de <rhyme label="b" type="m">Oranje-Spruit</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Onze Huizen weêr <rhyme label="a" type="f" corresp="#s1:l1">versieren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Zingen wy weêr <rhyme label="b" type="m" corresp="#s1:l2">overluid</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Komt, ô Stad en <rhyme label="c" type="f">Dorpelingen</rhyme>!</l>
        <l id="s1:l6" met="4" type="+-">Laat ons voor de jongste <rhyme label="d" type="m">vrugt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Van Prins Willems liefde <rhyme label="c" type="f" corresp="#s1:l5">zingen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Dat het dringt al door de <rhyme label="d" type="m" corresp="#s1:l6">lugt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wilt geen vreugt nog blydschap <rhyme label="a" type="f">sparen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Gaat aan reye, hand aan <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Willem George zal <rhyme label="a" type="f" corresp="#s2:l1">vergaaren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Weest verheugd, ô <rhyme label="b" type="m" corresp="#s2:l2">Nederland</rhyme>!</l>
        <l id="s2:l5" met="4" type="+-">Daar men voor de lieve <rhyme label="c" type="f">jongen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Die 't Prinslievend hart <rhyme label="d" type="m">bekoord</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Nooit een liedje heeft <rhyme label="c" type="f" corresp="#s2:l5">gezongen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Werd het thans van ons <rhyme label="d" type="m" corresp="#s2:l6">gehoord</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laat ons groene koordjes <rhyme label="a" type="f">vlegten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En die met <rhyme label="b" type="m">Oranje-lint</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Aan versierde boogjes <rhyme label="a" type="f" corresp="#s3:l1">hegten</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Voor Prins Willems <rhyme label="b" type="m" corresp="#s3:l2">liefde-kind</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Laat dan Maagd en <rhyme label="c" type="f">Jongelingen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Grys en ouderdom, en <rhyme label="d" type="m">jeugd</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Voor het jongste Prinsje <rhyme label="c" type="f" corresp="#s3:l5">zingen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Met een welgepaste <rhyme label="d" type="m" corresp="#s3:l6">vreugd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laat nu onze stemme <rhyme label="a" type="f">paaren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Met het aangenaam <rhyme label="b" type="m">geluid</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Van de vluggeroerde <rhyme label="a" type="f" corresp="#s4:l1">snaaren</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Der vioolen, bas en <rhyme label="b" type="m" corresp="#s4:l2">fluit</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Laat ons nu met blyde <rhyme label="c" type="f">klanken</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Vol van zuivere <rhyme label="d" type="m">dankbaarheid</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Des Heeren milde goedheid <rhyme label="c" type="f" corresp="#s4:l5">danken</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Voor zyn groote <rhyme label="d" type="m" corresp="#s4:l6">goedigheid</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dat hy Vyfde Willem <rhyme label="a" type="f">spaarden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">En in zyn verliefde <rhyme label="b" type="m">ziel</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Zulk een schat van deugd <rhyme label="a" type="f" corresp="#s5:l1">bewaarden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Die Sophia zo <rhyme label="b" type="m" corresp="#s5:l2">beviel</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Dat zy zig met hart en <rhyme label="c" type="f">handen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Aan dien waare <rhyme label="d" type="m">Vryheids-Zoon</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Door het Huwelyk liet <rhyme label="c" type="f" corresp="#s5:l5">verpanden</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Daar zyn deugd hem mint en <rhyme label="d" type="m" corresp="#s5:l6">loont</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dat 'er drie <rhyme label="a" type="f">Oranje-looten</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Door des Hemels <rhyme label="b" type="m">Albestuur</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Uit dit Huwelyk zyn <rhyme label="a" type="f" corresp="#s6:l1">gesprooten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Dat de jongste in dit <rhyme label="b" type="m" corresp="#s6:l2">uur</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Onze harte kan <rhyme label="c" type="f">verheugen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Terwyl zyn hart voor 't <rhyme label="d" type="m">Vaderland</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">(Spyt de lastertaal en <rhyme label="c" type="f" corresp="#s6:l5">leugen</rhyme>,)</l>
        <l id="s6:l8" met="4" type="+-">Reeds van zuivre liefde <rhyme label="d" type="m" corresp="#s6:l6">brand</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">ô Bataviers! blyft hy in 't <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Dan zal hy aan <rhyme label="b" type="m">Nederland</rhyme></l>
        <l id="s7:l3" met="4" type="+-">De allergrootste blyken <rhyme label="a" type="f" corresp="#s7:l1">geeven</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Van zyn goedheid en <rhyme label="b" type="m" corresp="#s7:l2">verstand</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Hy zal voor 's Lands Regten <rhyme label="c" type="f">stryden</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Vol beleid en <rhyme label="d" type="m">heldenmoed</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Ons van slaverny <rhyme label="c" type="f" corresp="#s7:l5">bevryden</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Waakende voor speer en <rhyme label="d" type="m" corresp="#s7:l6">hoed</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">En, schoon valsche <rhyme label="a" type="f">Vryheids-helden</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Onze <rhyme label="b" type="m">Opper-Generaal</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Voor een Landverraader <rhyme label="a" type="f" corresp="#s8:l1">schelden</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Nog geen nood, zyn <rhyme label="b" type="m" corresp="#s8:l2">zegepraal</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Zal den laster zelfs doen <rhyme label="c" type="f">zwygen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">En de God van <rhyme label="d" type="m">Nederland</rhyme></l>
        <l id="s8:l7" met="4" type="+-">Zal zyn onschuld regt doen <rhyme label="c" type="f" corresp="#s8:l5">krygen</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Door zyn wonderdoende <rhyme label="d" type="m" corresp="#s8:l6">Hand</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Deez' dag kan ons <rhyme label="a" type="f">bekooren</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Laaten wy dan regt <rhyme label="b" type="m">verheugd</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Onze wenschen bly doen <rhyme label="a" type="f" corresp="#s9:l1">hooren</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Aan het puik van Neêrlands <rhyme label="b" type="m" corresp="#s9:l2">jeugd</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Jonge Vorst! wy alle <rhyme label="c" type="f">wenschen</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Dat uw niet dan heil <rhyme label="d" type="m">geschied</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Dat gy boven andere <rhyme label="c" type="f" corresp="#s9:l5">Menschen</rhyme></l>
        <l id="s9:l8" met="4" type="+-">Altoos 's Hemels gunst <rhyme label="d" type="m" corresp="#s9:l6">geniet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Dat gy altoos tot <rhyme label="a" type="f">genoegen</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Van uw braave Ouders <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Dat gy uw tot deugd wilt <rhyme label="a" type="f" corresp="#s10:l1">voegen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Daar de lastertaal voor <rhyme label="b" type="m" corresp="#s10:l2">beeft</rhyme>.</l>
        <l id="s10:l5" met="4" type="+-">Dat uw nimmer onheil <rhyme label="c" type="f">deeren</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Dat gy door uw kloek <rhyme label="d" type="m">verstand</rhyme></l>
        <l id="s10:l7" met="4" type="+-">Helsche Dwinglandy mogt <rhyme label="c" type="f" corresp="#s10:l5">keeren</rhyme></l>
        <l id="s10:l8" met="4" type="+-">Uit een vrygevogte <rhyme label="d" type="m" corresp="#s10:l6">Land</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Leeft nog veele blyde <rhyme label="a" type="f">jaaren</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Leeft, leeft, al wat leeft, <rhyme label="b" type="m">bemind</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">'s Hemels gunst moog u <rhyme label="a" type="f" corresp="#s11:l1">bewaaren</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Leef voor Vryheid, Jongste <rhyme label="b" type="m" corresp="#s11:l2">Vrind</rhyme>!</l>
        <l id="s11:l5" met="4" type="+-">Volg de lesse van uw <rhyme label="c" type="f">Vader</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Volg uw Moeders kloek <rhyme label="d" type="m">verstand</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Stoor uw aan geen <rhyme label="c" type="f" corresp="#s11:l5">Landverraaders</rhyme></l>
        <l id="s11:l8" met="4" type="+-">Leef tot heil van 't <rhyme label="d" type="m" corresp="#s11:l6">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Laaten wy op deeze <rhyme label="a" type="f">wensche</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Die wy alle, bly van <rhyme label="b" type="m">geest</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Voor de Jongste Willem <rhyme label="a" type="f" corresp="#s12:l1">deede</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Op zyn hoog <rhyme label="b" type="m" corresp="#s12:l2">Geboorte-Feest</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Wenschende in 't ronde <rhyme label="c" type="f">klinken</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Lang, ja lang, leeft Willems <rhyme label="d" type="m">Zoon</rhyme>!</l>
        <l id="s12:l7" met="4" type="+-">Laaten wy zyn welvaard <rhyme label="c" type="f" corresp="#s12:l5">drinken</rhyme>,</l>
        <l id="s12:l8" met="4" type="+-">Dat zyn deugd mogt zyn <rhyme label="d" type="m" corresp="#s12:l6">beloond</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Langleeft Willem van <rhyme label="a" type="f">Oranje</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Lang leeft Willems <rhyme label="b" type="m">Gemaalin</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Voortgebragt uit Oud <rhyme label="a" type="f" corresp="#s13:l1">Germanje</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Pruisens groote <rhyme label="b" type="m" corresp="#s13:l2">Ryks-Vorstin</rhyme>!</l>
        <l id="s13:l5" met="4" type="+-">Lang moog Willem Fredrik <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Lang leeft Willems oudste <rhyme label="d" type="m">Telg</rhyme>,</l>
        <l id="s13:l7" met="4" type="+-">Eeuwig moog de laster <rhyme label="c" type="f" corresp="#s13:l5">beeven</rhyme></l>
        <l id="s13:l8" met="4" type="+-">Voor dien vrygevogte <rhyme label="d" type="m" corresp="#s13:l6">Telg</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Nog voor 't laatst eens <rhyme label="a" type="f">ingeschonken</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">En by 't zwaaijen van de <rhyme label="b" type="m">hoed</rhyme></l>
        <l id="s14:l3" met="4" type="+-">Deeze beker leêg <rhyme label="a" type="f" corresp="#s14:l1">gedronken</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Lang leeft 't <rhyme label="b" type="m" corresp="#s14:l2">Oranje-bloed</rhyme>!</l>
        <l id="s14:l5" met="4" type="+-">Al die zig op hem <rhyme label="c" type="f">verlaaten</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Zingen hertelyk tot <rhyme label="d" type="m">besluit</rhyme>,</l>
        <l id="s14:l7" met="4" type="+-">Lang leeve onze Heeren <rhyme label="c" type="f" corresp="#s14:l5">Staaten</rhyme>,</l>
        <l id="s14:l8" met="4" type="+-">Lang leeft de <rhyme label="d" type="m" corresp="#s14:l6">Oranje-Spruit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
