<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4177</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">SEgt mijn <rhyme label="a" type="f">bekje</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Hartens <rhyme label="b" type="f">diefje</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Waarom <rhyme label="a" type="f" corresp="#s1:l1">gekje</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Met-je <rhyme label="b" type="f" corresp="#s1:l2">liefje</rhyme>?</l>
        <l id="s1:l5" met="2" type="+-">Die u <rhyme label="c" type="m">als</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Sijn hart <rhyme label="d" type="m">bemind</rhyme>:</l>
        <l id="s1:l7" met="2" type="+-">Nimmer <rhyme label="c" type="m" corresp="#s1:l5">vals</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Gy hem <rhyme label="d" type="m" corresp="#s1:l6">bevind</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Uwe <rhyme label="a" type="f">suchjes</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Uwe <rhyme label="b" type="f">klachjes</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Uwe <rhyme label="a" type="f" corresp="#s2:l1">kluchjes</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Uwe <rhyme label="b" type="f" corresp="#s2:l2">lachjes</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Uwe <rhyme label="c" type="m">pijn</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">En u <rhyme label="d" type="m">vermaak</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Immer <rhyme label="c" type="m" corresp="#s2:l5">zijn</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Mijn levens <rhyme label="d" type="m" corresp="#s2:l6">baak</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Zijt gy <rhyme label="a" type="f">vrolijk</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Ik ben <rhyme label="b" type="f">blijde</rhyme>:</l>
        <l id="s3:l3" met="2" type="+-">Zijt gy <rhyme label="a" type="f" corresp="#s3:l1">olijk</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Ik <rhyme label="b" type="f" corresp="#s3:l2">vermijde</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Alle <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">In mijn <rhyme label="d" type="m">gelaat</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">'t Geen de <rhyme label="c" type="m" corresp="#s3:l5">jeugd</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Nochtans <rhyme label="d" type="m" corresp="#s3:l6">toe-staat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Zijn u <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Niet <rhyme label="b" type="f">genegen</rhyme></l>
        <l id="s4:l3" met="2" type="+-">Tot het <rhyme label="a" type="f" corresp="#s4:l1">minnen</rhyme>?</l>
        <l id="s4:l4" met="2" type="+-">Ik hier <rhyme label="b" type="f" corresp="#s4:l2">tegen</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Min: en <rhyme label="c" type="m">voel</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Een vlammend' <rhyme label="d" type="m">vuur</rhyme>:</l>
        <l id="s4:l7" met="2" type="+-">Daar gy <rhyme label="c" type="m" corresp="#s4:l5">koel</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Zijt van <rhyme label="d" type="m" corresp="#s4:l6">natuur</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Soete <rhyme label="a" type="f">Meysje</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Lieve <rhyme label="b" type="f">Prijster</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Seg een <rhyme label="a" type="f" corresp="#s5:l1">reysje</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Is so <rhyme label="b" type="f" corresp="#s5:l2">byster</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Dit <rhyme label="c" type="m">verschil</rhyme>?</l>
        <l id="s5:l6" met="2" type="-+">Ey hebt <rhyme label="d" type="m">geduld</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">'t Is geen <rhyme label="c" type="m" corresp="#s5:l5">wil</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Maar liefdens <rhyme label="d" type="m" corresp="#s5:l6">schuld</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Noch in <rhyme label="a" type="f">plegen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">En <rhyme label="b" type="f">verkiesen</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Noch in <rhyme label="a" type="f" corresp="#s6:l1">zegen</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">En <rhyme label="b" type="f" corresp="#s6:l2">verliesen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Noch in <rhyme label="c" type="m">iet</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Van 't geen ons <rhyme label="d" type="m">is</rhyme>:</l>
        <l id="s6:l7" met="2" type="+-">Daarom <rhyme label="c" type="m" corresp="#s6:l5">vliedt</rhyme></l>
        <l id="s6:l8" met="2" type="-+">d' <rhyme label="d" type="m" corresp="#s6:l6">In-beeldenis</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">Veyld u <rhyme label="a" type="f">lipjes</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Biedt u <rhyme label="b" type="f">mondje</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Geeft haar <rhyme label="a" type="f" corresp="#s7:l1">tipjes</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">Dit <rhyme label="b" type="f" corresp="#s7:l2">verbondje</rhyme>:</l>
        <l id="s7:l5" met="2" type="+-">Op dat <rhyme label="c" type="m">ik</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Wanneer gy <rhyme label="d" type="m">spreekt</rhyme>)</l>
        <l id="s7:l7" met="2" type="+-">Maak een <rhyme label="c" type="m" corresp="#s7:l5">strik</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Dat nimmer <rhyme label="d" type="m" corresp="#s7:l6">breekt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
