<song>
  <meta>
    <id>187235</id>
    <date>
      <year-start>1794</year-start>
      <year-end>1794</year-end>
    </date>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Komt vrienden, wenscht, met bly geluid, ô <rhyme label="a" type="m">ja</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Uw heil aan Bruidegom en Bruid, ô <rhyme label="a" type="m" corresp="#s1:l1">ja</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Maar plengt daarby den eeren <rhyme label="b" type="m">wyn</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dan zal de wensch meer klemmend <rhyme label="b" type="m" corresp="#s1:l3">zyn</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">drinkt uit, drinkt uit, drinkt <rhyme label="c" type="m">uit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
