<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8717</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Vrouwtje is uw man niet thuis</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SPeelgenootjes die my <rhyme label="a" type="m">raen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Dat ick 't helder <rhyme label="b" type="f">blaken</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van Philander sou <rhyme label="a" type="m" corresp="#s1:l1">versmaen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">En niet eens mijn <rhyme label="b" type="f" corresp="#s1:l2">kaken</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Laten op de suyv're <rhyme label="c" type="m">vlam</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Sijner liefde <rhyme label="b" type="f" corresp="#s1:l2">raken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wilt gy doch uyt <rhyme label="a" type="m">Maegdeplicht</rhyme></l>
        <l id="s2:l2" met="3" type="+-">My ten besten <rhyme label="b" type="f">raden</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Legh mijn dan geen lasten <rhyme label="a" type="m" corresp="#s2:l1">op</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Die my <rhyme label="b" type="f" corresp="#s2:l2">overladen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Wijst mijn liever 'tgeen ick <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Dragen sonder <rhyme label="b" type="f" corresp="#s2:l2">schade</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Leert mijn oock 't verhoolen <rhyme label="a" type="m">vyer</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Aen Philander <rhyme label="b" type="f">toonen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En hoe ick zijn heusche <rhyme label="a" type="m" corresp="#s3:l1">mont</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Sal met kusjes <rhyme label="b" type="f" corresp="#s3:l2">kroonen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Want ick meer als duysent <rhyme label="c" type="m">werf</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Sijn verdriet sal <rhyme label="b" type="f" corresp="#s3:l2">loonen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als ick dit dan heb <rhyme label="a" type="m">volbracht</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">En mijn stuere <rhyme label="b" type="f">sinnen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Sijn gesmolten door de <rhyme label="a" type="m" corresp="#s4:l1">kracht</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">En door 't Heyligh <rhyme label="b" type="f" corresp="#s4:l2">minnen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Van den Harder, die 'k mijn <rhyme label="c" type="m">Koets</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Willigh laet <rhyme label="b" type="f" corresp="#s4:l2">verwinnen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dan sal ick u laten <rhyme label="a" type="m">sien</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Wat voor wijde <rhyme label="b" type="f">wegen</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Tusschen 't beter hebben, <rhyme label="a" type="m" corresp="#s5:l1">en</rhyme></l>
        <l id="s5:l4" met="3" type="+-">'t Wenschen zy <rhyme label="b" type="f" corresp="#s5:l2">gelegen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">En wat holle vreugd <rhyme label="c" type="m">verscheet</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Van verknopte <rhyme label="b" type="f" corresp="#s5:l2">zegen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Soo u dit dan niet <rhyme label="a" type="m">beweecht</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Wilt u dan niet <rhyme label="b" type="f">stooren</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Als ghy (waent een ander <rhyme label="a" type="m" corresp="#s6:l1">lust</rhyme>)</l>
        <l id="s6:l4" met="3" type="+-">Tegen danck moet <rhyme label="b" type="f" corresp="#s6:l2">hooren</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Dat g'u tijt en schoonheyd <rhyme label="c" type="m">hebt</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Sonder nut <rhyme label="b" type="f" corresp="#s6:l2">verlooren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
