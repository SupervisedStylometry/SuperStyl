  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Welkoom, zoon der deugd, der <rhyme label="a" type="f">reden</rhyme>.</l>
        <l id="s1:l2" met="4" type="+-">Welkoom in hun <rhyme label="b" type="m">heiligdom</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Oefen wat gij hebt <rhyme label="a" type="f" corresp="#s1:l1">beleeden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Weer der ondeugd vuigen <rhyme label="b" type="m" corresp="#s1:l2">drom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Thands ziet gij aan uw <rhyme label="a" type="f">verlangen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Blijd, naar uwen wensch, <rhyme label="b" type="m">voldaan</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Thands ziet gij, om u te <rhyme label="a" type="f" corresp="#s2:l1">ontvangen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Broeder - armen open <rhyme label="b" type="m" corresp="#s2:l2">staan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Welkoom, zoon der deugd, der <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Welkoom in hun <rhyme label="b" type="m">heiligdom</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Oefen wat gij hebt <rhyme label="a" type="f" corresp="#s3:l1">beleeden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Weer der ondeugd vuigen <rhyme label="b" type="m" corresp="#s3:l2">drom</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Thands, als van den dood aan 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Van de duisternis aan 't <rhyme label="b" type="m">licht</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dankend, juigchend, <rhyme label="a" type="f" corresp="#s4:l1">weÃªrgegeeven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Leest gij vreugd in elks <rhyme label="b" type="m" corresp="#s4:l2">gezicht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Welkoom, zoon der deugd, der <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Welkoom in hun <rhyme label="b" type="m">heiligdom</rhyme>!</l>
        <l id="s5:l3" met="4" type="+-">Oefen wat gij hebt <rhyme label="a" type="f" corresp="#s5:l1">beleeden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Weer der ondeugd vuigen <rhyme label="b" type="m" corresp="#s5:l2">drom</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dat de trouw, ons nu <rhyme label="a" type="f">gezwooren</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Niets slechts zij die van den <rhyme label="b" type="m">mond</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Maar van 't hart eens <rhyme label="a" type="f" corresp="#s6:l1">uitverkooren'</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Tot ons broederlijk <rhyme label="b" type="m" corresp="#s6:l2">verbond</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Welkoom, zoon der deugd, der <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Welkoom in hun <rhyme label="b" type="m">heiligdom</rhyme>!</l>
        <l id="s7:l3" met="4" type="+-">Oefen wat gij hebt <rhyme label="a" type="f" corresp="#s7:l1">beleeden</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Weer der ondeugd vuigen <rhyme label="b" type="m" corresp="#s7:l2">drom</rhyme>.</l>
      </lg>
    </lg>
  </text>
