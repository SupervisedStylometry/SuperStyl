  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIt is een dagh van <rhyme label="a" type="m">vrolijckheydt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dit is een blijden <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dit is een dagh van <rhyme label="a" type="m" corresp="#s1:l1">vrolijckheydt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dit is een blijden <rhyme label="b" type="m" corresp="#s1:l2">dagh</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Daer is een vaste knoop <rhyme label="a" type="m" corresp="#s1:l1">'cleydt</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Een knoop <rhyme label="a" type="m" corresp="#s1:l1">'eleydt</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Een knoop <rhyme label="a" type="m" corresp="#s1:l1">'eleydt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die niet weer los en <rhyme label="b" type="m" corresp="#s1:l2">mach</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Laet u nu vrolijck wesen <rhyme label="a" type="m">dan</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En schuwen quaet <rhyme label="b" type="m">geklap</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Laet ons nu vrolijck wesen <rhyme label="a" type="m" corresp="#s2:l1">dan</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En schuwen quaet <rhyme label="b" type="m" corresp="#s2:l2">geklap</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Wy hebben nu de Wijn-koop <rhyme label="a" type="m" corresp="#s2:l1">van</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">De Wijn-koop <rhyme label="a" type="m" corresp="#s2:l1">van</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">De Wijn-koop <rhyme label="a" type="m" corresp="#s2:l1">van</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">De groote <rhyme label="b" type="m" corresp="#s2:l2">Koopmanschap</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Al zijn wy nu na ons <rhyme label="a" type="m">begeer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hier vrolijck <rhyme label="b" type="m">ongetreurt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Al zijn  wij nu na ons <rhyme label="a" type="m" corresp="#s3:l1">begeer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hier vrolijck <rhyme label="b" type="m" corresp="#s3:l2">ongetreurt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Wy wenschen dat dit <rhyme label="a" type="m" corresp="#s3:l1">nimmermeer</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Dit <rhyme label="a" type="m" corresp="#s3:l1">nimmermeer</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Dit <rhyme label="a" type="m" corresp="#s3:l1">nimmermeer</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Met een van bey <rhyme label="b" type="m" corresp="#s3:l2">gebeurt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer dat een ander uyt den <rhyme label="a" type="m">hoop</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Een goet en aerdigh <rhyme label="b" type="m">Geest</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Maer dat een ander uyt den <rhyme label="a" type="m" corresp="#s4:l1">hoop</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Een goet en aerdigh <rhyme label="b" type="m" corresp="#s4:l2">Geest</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Haest mede doet alsulcken <rhyme label="a" type="m" corresp="#s4:l1">koop</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Alsulcken <rhyme label="a" type="m" corresp="#s4:l1">koop</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Alsulcken <rhyme label="a" type="m" corresp="#s4:l1">koop</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En nooden ons ter <rhyme label="b" type="m" corresp="#s4:l2">Feest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Prince, Princessen oudt en <rhyme label="a" type="m">jonck</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Komt laet ons <rhyme label="b" type="m">alle-gaer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Prince, Princessen oudt en <rhyme label="a" type="m" corresp="#s5:l1">jonck</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Komt laet ons <rhyme label="b" type="m" corresp="#s5:l2">alle-gaer</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Doen elck nu eens een goeden <rhyme label="a" type="m" corresp="#s5:l1">dronck</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Een goeden <rhyme label="a" type="m" corresp="#s5:l1">dronck</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Een goeden <rhyme label="a" type="m" corresp="#s5:l1">dronck</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Ter eeren 't jonge <rhyme label="b" type="m" corresp="#s5:l2">Paer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
