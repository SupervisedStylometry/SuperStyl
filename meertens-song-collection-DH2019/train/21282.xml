    <melody>Rijke God wie zal ik klagen</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh God aenhoort ons <rhyme label="a" type="f">claghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">U cuddeken nv <rhyme label="b" type="m">bestaet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Diemen nv siet <rhyme label="a" type="f" corresp="#s1:l1">veriaghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Wt Antwerpen door nijdighen <rhyme label="b" type="m" corresp="#s1:l2">haet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Uan Bels gheslachte seer <rhyme label="c" type="f">coene</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die Pharao zijn <rhyme label="d" type="m">versaemt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Om v hoopken te nieute te <rhyme label="c" type="f" corresp="#s1:l5">doene</rhyme></l>
        <l id="s1:l8" met="3" type="-+">O Heere maectse doch <rhyme label="d" type="m" corresp="#s1:l6">beschaemt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want ghy door v <rhyme label="a" type="f">ghenade</rhyme></l>
        <l id="s2:l2" met="3" type="-+">U Ghemeynte hier hebt <rhyme label="b" type="m">gheplant</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Beschermtse vroech ende <rhyme label="a" type="f" corresp="#s2:l1">spade</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wy betrouwen op v, o God <rhyme label="b" type="m" corresp="#s2:l2">valliant</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dat ghy niet en sult <rhyme label="c" type="f">ghehinghen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Al lijdt v cleyn hoopken <rhyme label="d" type="m">verdriet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Dat zijt te nieute sullen <rhyme label="c" type="f" corresp="#s2:l5">bringhen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Want ghy haren raet <rhyme label="d" type="m" corresp="#s2:l6">aensiet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy bekennen o God <rhyme label="a" type="f">verheuen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat onse sonden die schulden <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat wy dus worden <rhyme label="a" type="f" corresp="#s3:l1">verdreuen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">O Heere wilt v <rhyme label="b" type="m" corresp="#s3:l2">aenschijn</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Uan onse sonden <rhyme label="c" type="f">keeren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">U ghenade ons <rhyme label="d" type="m">aenschout</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Uwen lof sullen wy <rhyme label="c" type="f" corresp="#s3:l5">vermeeren</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Hoe seere wy worden <rhyme label="d" type="m" corresp="#s3:l6">benout</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sterct ons met uwen <rhyme label="a" type="f">gheeste</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Op dat wy tot alder <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Teghen die Babylonsche <rhyme label="a" type="f" corresp="#s4:l1">beeste</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Uromelick gaen al inden <rhyme label="b" type="m" corresp="#s4:l2">strijt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">U cracht laet in ons <rhyme label="c" type="f">blijcken</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wy die v handtwerck <rhyme label="d" type="m">zijn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En laet ons gheensins <rhyme label="c" type="f" corresp="#s4:l5">wijcken</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Uan v edele woort <rhyme label="d" type="m" corresp="#s4:l6">diuijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">U werck in ons <rhyme label="a" type="f">begonnen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Wilt o Heere volbringhen <rhyme label="b" type="m">wel</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dat Antechrist mach zijn <rhyme label="a" type="f" corresp="#s5:l1">verwonnen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Die v hoopken aen doet <rhyme label="b" type="m" corresp="#s5:l2">ghequel</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Wy zijn bereyt o <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Uwen name te belijdene <rhyme label="d" type="m">hier</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Al ist dat sy ons ledekens <rhyme label="c" type="f" corresp="#s5:l5">teere</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Uerdrucken door sweert ende <rhyme label="d" type="m" corresp="#s5:l6">vier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Al heetmen ons te <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Oproerders muytmakirs <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Om den name dijns soons <rhyme label="a" type="f" corresp="#s6:l1">ghepresen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Uerdraghen wijt vroech ende <rhyme label="b" type="m" corresp="#s6:l2">laet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Den knecht tot gheenen <rhyme label="c" type="f">stonden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Beter dan synen meester en mach <rhyme label="d" type="m">zijn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">O Heere vergheeft haer hare <rhyme label="c" type="f" corresp="#s6:l5">sonden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Ons veruolghers tot elcken <rhyme label="d" type="m" corresp="#s6:l6">termijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Noch zijnder die teghen ons <rhyme label="a" type="f">spreken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Segghende met woorden <rhyme label="b" type="m">opstinaet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Dat wy die ouerheyt <rhyme label="a" type="f" corresp="#s7:l1">versteken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Doende teghen des Keysers <rhyme label="b" type="m" corresp="#s7:l2">mandaet</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Den Keyser willen wy <rhyme label="c" type="f">gheuen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">So ons Christus doet <rhyme label="d" type="m">vermaen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat hem toe behoort in dit <rhyme label="c" type="f" corresp="#s7:l5">leuen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Gods ghebot bouen al moet <rhyme label="d" type="m" corresp="#s7:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Princelick God vol <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Onderricht ons alle <rhyme label="b" type="m">ghemeyn</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Op dat wy moghen <rhyme label="a" type="f" corresp="#s8:l1">verwinnen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ons vianden door uwe woorden <rhyme label="b" type="m" corresp="#s8:l2">reyn</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Wy bidden v alle <rhyme label="c" type="f">ghelijcke</rhyme></l>
        <l id="s8:l6" met="3" type="-+">U woort in ons herte <rhyme label="d" type="m">prent</rhyme></l>
        <l id="s8:l7" met="3" type="-+">So en sal ons sweerelts <rhyme label="c" type="f" corresp="#s8:l5">practijcke</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Niet letten, noch haer <rhyme label="d" type="m" corresp="#s8:l6">torment</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
