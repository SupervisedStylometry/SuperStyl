  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">BAchus maakt goede <rhyme label="a" type="m">Cier</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En schenkt ons Wijn en <rhyme label="a" type="m" corresp="#s1:l1">Bier</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Ziet wy komen <rhyme label="a" type="m" corresp="#s1:l1">hier</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Met vrolijk <rhyme label="a" type="m" corresp="#s1:l1">getier</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Vlegten Venus <rhyme label="a" type="m" corresp="#s1:l1">Minnevier</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wy komen <rhyme label="b" type="m">allegaar</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Tot uwen <rhyme label="b" type="m" corresp="#s1:l6">Altaar</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Drinken den Wijn <rhyme label="b" type="m" corresp="#s1:l6">klaar</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Uw Nat ons zeer wel <rhyme label="c" type="m">diend</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Gy word meer <rhyme label="c" type="m" corresp="#s1:l9">bemind</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Als het <rhyme label="c" type="m" corresp="#s1:l9">Venus-kind</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Wy zijn tot uw <rhyme label="c" type="m" corresp="#s1:l9">gezind</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Nobele <rhyme label="a" type="m">Kwant</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Welkom in ’t <rhyme label="a" type="m" corresp="#s2:l1">Vaderland</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Wy zijn uw <rhyme label="a" type="m" corresp="#s2:l1">constant</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Vloeken <rhyme label="a" type="m" corresp="#s2:l1">Minne-brand</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En geven uw de <rhyme label="a" type="m" corresp="#s2:l1">Hand</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">God Cupidoos <rhyme label="b" type="m">Pijl</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Is maar <rhyme label="b" type="m" corresp="#s2:l6">Minne-gijl</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Baard maar <rhyme label="b" type="m" corresp="#s2:l6">onheyl</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Ik haat het <rhyme label="c" type="m">Minne-spel</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">’k Volg uw <rhyme label="c" type="m" corresp="#s2:l9">bevel</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">God Bachus zeer <rhyme label="c" type="m" corresp="#s2:l9">wel</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">En neem een Volle <rhyme label="c" type="m" corresp="#s2:l9">Snel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
