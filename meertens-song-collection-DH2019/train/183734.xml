  <author>Jan Steven van Esveldt Holtrop, Willem Holtrop</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Broeders, Vrije - <rhyme label="a" type="f">Metselaaren</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Die op ieder <rhyme label="b" type="m">Broederfeest</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Vreugd met nut weet saam te <rhyme label="a" type="f" corresp="#s1:l1">paaren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Tot verkwikking van den <rhyme label="b" type="m" corresp="#s1:l2">geest</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Kan ons werk uw zinnen <rhyme label="c" type="f">streelen</rhyme>?</l>
        <l id="s1:l6" met="4" type="+-">Wilt ge in aller blijdschap <rhyme label="c" type="f" corresp="#s1:l5">deelen</rhyme>?</l>
        <l id="s1:l7" met="4" type="+-">Zingt, in chorus, dan met <rhyme label="d" type="m">mij</rhyme>:</l>
        <l id="s1:l8" met="4" type="+-">Groei en bloei, <rhyme label="d" type="m" corresp="#s1:l7">Vrij-Metslaarij</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Kunt ge in d'arbeid u <rhyme label="a" type="f">vermaaken</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Die deez' avond wordt <rhyme label="b" type="m">verricht</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">En het zoetst genoegen <rhyme label="a" type="f" corresp="#s2:l1">smaaken</rhyme></l>
        <l id="s2:l4" met="4" type="+-">In 't volbrengen van uw' <rhyme label="b" type="m" corresp="#s2:l2">pligt</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">Kunnen des Vrij-Metslaars <rhyme label="c" type="f">werken</rhyme></l>
        <l id="s2:l6" met="4" type="+-">In uw' ijver u <rhyme label="c" type="f" corresp="#s2:l5">versterken</rhyme>?</l>
        <l id="s2:l7" met="4" type="+-">Roept, in chorus, dan met <rhyme label="d" type="m">mij</rhyme>:</l>
        <l id="s2:l8" met="4" type="+-">Groei en bloei, <rhyme label="d" type="m" corresp="#s2:l7">Vrij-Metslaarij</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Houd gij, schoon ons de <rhyme label="a" type="f">ongewijden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Aan 't vooröordeel blind <rhyme label="b" type="m">verpand</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Met hun lastertaal <rhyme label="a" type="f" corresp="#s3:l1">bestrijden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Door uw deugd kloekmoedig <rhyme label="b" type="m" corresp="#s3:l2">stand</rhyme>?</l>
        <l id="s3:l5" met="4" type="+-">Tracht gij hen bedaard, door <rhyme label="c" type="f">reden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Niet door twisten, te <rhyme label="c" type="f" corresp="#s3:l5">overreeden</rhyme>?</l>
        <l id="s3:l7" met="4" type="+-">Roept, in chorus, dan met <rhyme label="d" type="m">mij</rhyme>:</l>
        <l id="s3:l8" met="4" type="+-">Groei en bloei, <rhyme label="d" type="m" corresp="#s3:l7">Vrij-Metslaarij</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ziju uw eedle harten <rhyme label="a" type="f">vatbaar</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Voor de vriendschap en haar <rhyme label="b" type="m">zoet</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">Acht gij haar als een <rhyme label="a" type="f" corresp="#s4:l1">onschatbaar</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Als een allerwenschlijkst <rhyme label="b" type="m" corresp="#s4:l2">goed</rhyme>?</l>
        <l id="s4:l5" met="4" type="+-">Tracht ge in u haar' gloed te <rhyme label="c" type="f">ontsteeken</rhyme></l>
        <l id="s4:l6" met="4" type="+-">En alöm haar aan te <rhyme label="c" type="f" corresp="#s4:l5">kweeken</rhyme>?</l>
        <l id="s4:l7" met="4" type="+-">Roept, in chorus, dan met <rhyme label="d" type="m">mij</rhyme>:</l>
        <l id="s4:l8" met="4" type="+-">Groei en bloei, <rhyme label="d" type="m" corresp="#s4:l7">Vrij-Metslaarij</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Mint gij het gezellig <rhyme label="a" type="f">leven</rhyme>?</l>
        <l id="s5:l2" met="4" type="+-">Schuwt gij de <rhyme label="b" type="m">eigenzinnigheid</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Ziet gij u tot vreugd <rhyme label="a" type="f" corresp="#s5:l1">gedreven</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Als zij-zelf den disch <rhyme label="b" type="m" corresp="#s5:l2">bereid</rhyme>?</l>
        <l id="s5:l5" met="4" type="+-">Vind ge in onze <rhyme label="c" type="f">tempelkooren</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dat genoegen u <rhyme label="c" type="f" corresp="#s5:l5">beschoren</rhyme>?</l>
        <l id="s5:l7" met="4" type="+-">Roept, in chorus, dan met <rhyme label="d" type="m">mij</rhyme>:</l>
        <l id="s5:l8" met="4" type="+-">Groei en bloei, <rhyme label="d" type="m" corresp="#s5:l7">Vrij-Metslaarij</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wilt ge een' kiesschen wellust <rhyme label="a" type="f">smaaken</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Die, wanneer zij is <rhyme label="b" type="m">voorbij</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Na 't genot van <rhyme label="a" type="f" corresp="#s6:l1">zinvermaaken</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Voor 't berouw beveiligd <rhyme label="b" type="m" corresp="#s6:l2">zij</rhyme>?</l>
        <l id="s6:l5" met="4" type="+-">Hier kunt gij dien wellust <rhyme label="c" type="f">vinden</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Bij een reeks van <rhyme label="c" type="f" corresp="#s6:l5">eensgezinden</rhyme>;</l>
        <l id="s6:l7" met="4" type="+-">Roept, in chorus, dus met <rhyme label="d" type="m">mij</rhyme>:</l>
        <l id="s6:l8" met="4" type="+-">Groei en bloei, <rhyme label="d" type="m" corresp="#s6:l7">Vrij-Metslaarij</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Laat ons dan den band <rhyme label="a" type="f">waardeeren</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Die ons boeit en hart en <rhyme label="b" type="m">zin</rhyme>;</l>
        <l id="s7:l3" met="4" type="+-">En den naam van Broeder <rhyme label="a" type="f" corresp="#s7:l1">eeren</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Door oprechte <rhyme label="b" type="m" corresp="#s7:l2">Broedermin</rhyme>!</l>
        <l id="s7:l5" met="4" type="+-">Laat hieröp den nectar <rhyme label="c" type="f">vloeijen</rhyme>:</l>
        <l id="s7:l6" met="4" type="+-">Lang moog' de Orde groeijen, <rhyme label="c" type="f" corresp="#s7:l5">bloeijen</rhyme>!</l>
        <l id="s7:l7" met="4" type="+-">Ieder Broeder roep met <rhyme label="d" type="m">mij</rhyme>:</l>
        <l id="s7:l8" met="4" type="+-">Groei en bloei, <rhyme label="d" type="m" corresp="#s7:l7">Vrij-Metslaarij</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
