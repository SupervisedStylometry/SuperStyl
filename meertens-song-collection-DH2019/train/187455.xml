<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187455</id>
    <date>
      <year-start>1796</year-start>
      <year-end>1796</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 128 Datheen</melody>
  <author>Johannes Wilhelmus Bussingh</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">De Zon verliest haer <rhyme label="a" type="f">luister</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">De held're dag wordt <rhyme label="b" type="m">nacht</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">En jesus heeft <rhyme label="c" type="f">verwonnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hij roept: Het is <rhyme label="b" type="m" corresp="#s1:l2">volbracht</rhyme>! -</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De harde rotsen <rhyme label="a" type="f">scheuren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het berggevaerte <rhyme label="b" type="m">beeft</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">De graven splijten <rhyme label="c" type="f">open</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Een heir van doden <rhyme label="b" type="m" corresp="#s2:l2">leeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Satan wordt <rhyme label="a" type="f">verslagen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Nu wijkt zijn list en <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En jesus heeft <rhyme label="c" type="f">volstreden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hij roept: Het is <rhyme label="b" type="m" corresp="#s3:l2">volbracht</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Voldaen is nu <rhyme label="a" type="f">volkomen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Aen d'eisch van 't <rhyme label="b" type="m">Gods-Verbond</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Hij sterft; en heil en <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Stroomt neêr voor 't <rhyme label="b" type="m" corresp="#s4:l2">weereldrond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu wordt geen <rhyme label="a" type="f">schaduw-offer</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Voor 't Menschdom meer <rhyme label="b" type="m">geslacht</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Neen; - jesus heeft, voor <rhyme label="c" type="f">Zondaers</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">In eeuwigheid <rhyme label="b" type="m" corresp="#s5:l2">volbracht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">ô Groote <rhyme label="a" type="f">Hogepriester</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En 's Weerelds <rhyme label="b" type="m">Opperheer</rhyme>!</l>
        <l id="s6:l3" met="3" type="-+">Uw Offer brengt <rhyme label="c" type="f">genade</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Voor 't zuchtend Schepsel <rhyme label="b" type="m" corresp="#s6:l2">weêr</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nu heerscht alöm de <rhyme label="a" type="f">vrede</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Nu is het pleit <rhyme label="b" type="m">beslecht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En Gij regeert de <rhyme label="c" type="f">volken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Met waerheid en met <rhyme label="b" type="m" corresp="#s7:l2">recht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Gij komt, en voert Uw <rhyme label="a" type="f">kinders</rhyme></l>
        <l id="s8:l2" met="3" type="-+">In Uwes Vaders <rhyme label="b" type="m">rijk</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Gij maekt Uw <rhyme label="c" type="f">gunstgenoten</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Den Engelen <rhyme label="b" type="m" corresp="#s8:l2">gelijk</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">ô jesus! Hoe <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Hoe eind'loos is Uw <rhyme label="b" type="m">macht</rhyme>!</l>
        <l id="s9:l3" met="3" type="-+">Hoor heel de schepping <rhyme label="c" type="f">juichen</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Gij hebt het al <rhyme label="b" type="m" corresp="#s9:l2">volbracht</rhyme>! -</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dat mij Uw bijstand <rhyme label="a" type="f">sterke</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Bevestig mijn <rhyme label="b" type="m">geloof</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Dat nooit de gloed der <rhyme label="c" type="f">liefde</rhyme></l>
        <l id="s10:l4" met="3" type="-+">In mijne borst <rhyme label="b" type="m" corresp="#s10:l2">verdoof</rhyme>! -</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Zou ik den dood nog <rhyme label="a" type="f">vrezen</rhyme>? -</l>
        <l id="s11:l2" met="3" type="-+">Neen; Heer, mijn <rhyme label="b" type="m">levenskracht</rhyme>!</l>
        <l id="s11:l3" met="3" type="-+">Ook ik zal <rhyme label="c" type="f">zegepralen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Gij hebt voor mij <rhyme label="b" type="m" corresp="#s11:l2">volbracht</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Nu zal ik moedig <rhyme label="a" type="f">strijden</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Met Weereld, Dood, en <rhyme label="b" type="m">Hel</rhyme>;</l>
        <l id="s12:l3" met="3" type="-+">'k Ben onder 't schild <rhyme label="c" type="f">beveiligd</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Van mijn <rhyme label="b" type="m" corresp="#s12:l2">Emanuël</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">En is mijn strijd <rhyme label="a" type="f">volstreden</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Mijn loopbaen <rhyme label="b" type="m">afgegaen</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Dan neemt in Zijne <rhyme label="c" type="f">handen</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Mij jesus Vader <rhyme label="b" type="m" corresp="#s13:l2">aen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">ô Dood! hoe kalm, hoe <rhyme label="a" type="f">vrolijk</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Wordt nu Uw komst <rhyme label="b" type="m">verwacht</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Ja; 't sterven is mij <rhyme label="c" type="f">zalig</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Want jesus heeft <rhyme label="b" type="m" corresp="#s14:l2">volbracht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
