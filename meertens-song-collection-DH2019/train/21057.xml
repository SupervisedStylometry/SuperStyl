<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21057</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">EEuwigh <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Eens <rhyme label="a" type="f" corresp="#s1:l1">verresen</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Noyt <rhyme label="a" type="f" corresp="#s1:l1">volpresen</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-"><rhyme label="b" type="m">Middelaer</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Laet u <rhyme label="c" type="f">stralen</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Vyt u <rhyme label="c" type="f" corresp="#s1:l5">zaelen</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Gunstigh <rhyme label="c" type="f" corresp="#s1:l5">dalen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Op dit <rhyme label="b" type="m" corresp="#s1:l4">paer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Wil de <rhyme label="a" type="f">vlagen</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Van u <rhyme label="a" type="f" corresp="#s2:l1">slagen</rhyme></l>
        <l id="s2:l3" met="2" type="+-">Die <rhyme label="a" type="f" corresp="#s2:l1">vertsagen</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Als een <rhyme label="b" type="m">straf</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Niet <rhyme label="c" type="f">verdagen</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Maer <rhyme label="c" type="f" corresp="#s2:l5">vertragen</rhyme></l>
        <l id="s2:l7" met="2" type="+-">En eer <rhyme label="c" type="f" corresp="#s2:l5">jagen</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Van haer <rhyme label="b" type="m" corresp="#s2:l4">af</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Daer en <rhyme label="a" type="f">tegen</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Stort een <rhyme label="a" type="f" corresp="#s3:l1">regen</rhyme></l>
        <l id="s3:l3" met="2" type="+-">Van u <rhyme label="a" type="f" corresp="#s3:l1">zegen</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Op haer <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Die ter <rhyme label="c" type="f">degen</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Mach <rhyme label="c" type="f" corresp="#s3:l5">bewegen</rhyme></l>
        <l id="s3:l7" met="2" type="+-">Vreughd' te <rhyme label="c" type="f" corresp="#s3:l5">plegen</rhyme></l>
        <l id="s3:l8" met="2" type="+-">t' Vwer <rhyme label="b" type="m" corresp="#s3:l4">eer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Soo g'haer 't kruys <rhyme label="a" type="f">doch</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Dat noch sluys, <rhyme label="a" type="f" corresp="#s4:l1">noch</rhyme></l>
        <l id="s4:l3" met="2" type="+-">Strooms gedruys <rhyme label="a" type="f" corresp="#s4:l1">(och</rhyme>!)</l>
        <l id="s4:l4" met="2" type="+-">Niet <rhyme label="b" type="m">ghelijckt</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Toe wilt <rhyme label="c" type="f">zenden</rhyme></l>
        <l id="s4:l6" met="2" type="+-">En u <rhyme label="c" type="f" corresp="#s4:l5">schenden</rhyme></l>
        <l id="s4:l7" met="2" type="+-">Henen <rhyme label="c" type="f" corresp="#s4:l5">wenden</rhyme></l>
        <l id="s4:l8" met="2" type="+-">Waer 't paer <rhyme label="b" type="m" corresp="#s4:l4">wijckt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Laet haer <rhyme label="a" type="f">klachten</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Die wel <rhyme label="a" type="f" corresp="#s5:l1">plachten</rhyme></l>
        <l id="s5:l3" met="2" type="+-">Straf te <rhyme label="a" type="f" corresp="#s5:l1">sachten</rhyme></l>
        <l id="s5:l4" met="2" type="+-">t' Aller <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Dan niet <rhyme label="c" type="f">trachten</rhyme></l>
        <l id="s5:l6" met="2" type="+-">Na 't <rhyme label="c" type="f" corresp="#s5:l5">verachten</rhyme></l>
        <l id="s5:l7" met="2" type="+-">Maer haer <rhyme label="c" type="f" corresp="#s5:l5">klachten</rhyme></l>
        <l id="s5:l8" met="2" type="+-">Starck in <rhyme label="b" type="m" corresp="#s5:l4">strijdt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Laet in 't <rhyme label="a" type="f">woelen</rhyme></l>
        <l id="s6:l2" met="2" type="+-">Haer <rhyme label="a" type="f" corresp="#s6:l1">ghevoelen</rhyme></l>
        <l id="s6:l3" met="2" type="+-">Dat u doel, <rhyme label="a" type="f" corresp="#s6:l1">en</rhyme></l>
        <l id="s6:l4" met="2" type="+-"><rhyme label="b" type="m">Oogemarck</rhyme>;</l>
        <l id="s6:l5" met="2" type="+-">'t Eeuwigh <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s6:l6" met="2" type="+-">Is te <rhyme label="c" type="f" corresp="#s6:l5">geven</rhyme></l>
        <l id="s6:l7" met="2" type="+-">Na het <rhyme label="c" type="f" corresp="#s6:l5">beven</rhyme></l>
        <l id="s6:l8" met="2" type="+-">In u <rhyme label="b" type="m" corresp="#s6:l4">Kerck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">V <rhyme label="a" type="f">behaghen</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Sy die <rhyme label="a" type="f" corresp="#s7:l1">plagen</rhyme></l>
        <l id="s7:l3" met="2" type="+-">Dan te <rhyme label="a" type="f" corresp="#s7:l1">dragen</rhyme></l>
        <l id="s7:l4" met="2" type="+-">Na een <rhyme label="b" type="m">oort</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Daer de <rhyme label="c" type="f">daghen</rhyme></l>
        <l id="s7:l6" met="2" type="+-">V noyt <rhyme label="c" type="f" corresp="#s7:l5">klaghen</rhyme>,</l>
        <l id="s7:l7" met="2" type="+-">Noch oock <rhyme label="c" type="f" corresp="#s7:l5">vraghen</rhyme></l>
        <l id="s7:l8" met="2" type="+-">Na u <rhyme label="b" type="m" corresp="#s7:l4">woordt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-">Twist en <rhyme label="a" type="f">Vede</rhyme></l>
        <l id="s8:l2" met="2" type="+-">Stadigh <rhyme label="a" type="f" corresp="#s8:l1">mede</rhyme></l>
        <l id="s8:l3" met="2" type="+-">Voor de <rhyme label="a" type="f" corresp="#s8:l1">vrede</rhyme></l>
        <l id="s8:l4" met="2" type="+-">Duncken <rhyme label="b" type="m">laet</rhyme>;</l>
        <l id="s8:l5" met="2" type="+-">Liefd' laet <rhyme label="c" type="f">bloeyen</rhyme>,</l>
        <l id="s8:l6" met="2" type="+-">Eendracht <rhyme label="c" type="f" corresp="#s8:l5">groeyen</rhyme></l>
        <l id="s8:l7" met="2" type="+-">Maer <rhyme label="c" type="f" corresp="#s8:l5">besnoeyen</rhyme></l>
        <l id="s8:l8" met="2" type="+-">Weer den <rhyme label="b" type="m" corresp="#s8:l4">haet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="+-">Segh haer: kind' <rhyme label="a" type="f">ren</rhyme></l>
        <l id="s9:l2" met="2" type="+-">'k Wil vermind' <rhyme label="a" type="f" corresp="#s9:l1">ren</rhyme></l>
        <l id="s9:l3" met="2" type="+-">Ja verhind' <rhyme label="a" type="f" corresp="#s9:l1">ren</rhyme></l>
        <l id="s9:l4" met="2" type="+-">'t <rhyme label="b" type="m">Schadelijck</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">En mijn <rhyme label="c" type="f">Erven</rhyme></l>
        <l id="s9:l6" met="2" type="+-">Sult ghy <rhyme label="c" type="f" corresp="#s9:l5">erven</rhyme></l>
        <l id="s9:l7" met="2" type="+-">Naer u <rhyme label="c" type="f" corresp="#s9:l5">sterven</rhyme></l>
        <l id="s9:l8" met="2" type="+-"><rhyme label="b" type="m" corresp="#s9:l4">Eeuwighlijck</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="+-">Dese <rhyme label="a" type="f">beden</rhyme></l>
        <l id="s10:l2" met="2" type="+-">Send ick <rhyme label="a" type="f" corresp="#s10:l1">heden</rhyme></l>
        <l id="s10:l3" met="2" type="+-">Van <rhyme label="a" type="f" corresp="#s10:l1">beneden</rhyme></l>
        <l id="s10:l4" met="2" type="+-">Voor haer <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">Laet u <rhyme label="c" type="f">ooren</rhyme></l>
        <l id="s10:l6" met="2" type="+-">'t Smeken <rhyme label="c" type="f" corresp="#s10:l5">hooren</rhyme>,</l>
        <l id="s10:l7" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s10:l5">bekooren</rhyme></l>
        <l id="s10:l8" met="2" type="+-">V <rhyme label="b" type="m" corresp="#s10:l4">genaed'</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
