  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ick groet’ u, Maeght vol <rhyme label="a" type="f">eere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die vol genade <rhyme label="b" type="m">sijt</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Met u is Godt de <rhyme label="a" type="f" corresp="#s1:l1">Heere</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ghy sijt <rhyme label="b" type="m" corresp="#s1:l2">gebenedijdt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Gelijck tot gheender <rhyme label="c" type="m">tijdt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Was eenigh’ <rhyme label="d" type="m">vrou-persoon</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Oock door de wereldt <rhyme label="c" type="m" corresp="#s1:l5">wijt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ghe-eert is uwen <rhyme label="d" type="m" corresp="#s1:l6">Soon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O Moeder Godts, <rhyme label="a" type="f">geboren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wt ’s Koninghs Davids <rhyme label="b" type="m">saedt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wilt dit gebedt <rhyme label="a" type="f" corresp="#s2:l1">aenhoren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Helpt ons door uwen <rhyme label="b" type="m" corresp="#s2:l2">raedt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Komt den sondaers te <rhyme label="c" type="m">baet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Nu, en oock nae den <rhyme label="d" type="m">doot</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Stelt ons in goeden <rhyme label="c" type="m" corresp="#s2:l5">staet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Ontfangt ons in u <rhyme label="d" type="m" corresp="#s2:l6">schoot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
