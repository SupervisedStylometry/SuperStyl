<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21912</id>
    <date>
      <year-start>1580</year-start>
      <year-end>1580</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 136 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Philips van Marnix</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LOft Godt, gheeft hem prijs end' <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Want hy is ghenadigh <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">End' zijn <rhyme label="b" type="m">goeder-tierenheyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s1:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">* Looft end' prijst Godt met <rhyme label="a" type="m">gheschal</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Bouen dander' goden <rhyme label="a" type="m" corresp="#s2:l1">al</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s2:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">* Looft Godt nu end' <rhyme label="a" type="m">ymmermeer</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Die is aller Heeren <rhyme label="a" type="m" corresp="#s3:l1">Heer</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">End' zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s3:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">* Hy alleen met zijn handt <rhyme label="a" type="m">bloot</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Richt uyt, wonderwercken <rhyme label="a" type="m" corresp="#s4:l1">groot</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s4:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">* Die den hemel met zijn <rhyme label="a" type="m">handt</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Schiep in wijsheyt end' <rhyme label="a" type="m" corresp="#s5:l1">verstant</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s5:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">T'aertrijck heeft hy <rhyme label="a" type="m">uytghebreydt</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">End' op s'waters grondt <rhyme label="a" type="m" corresp="#s6:l1">gheleydt</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s6:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">* Groote lichten heeft <rhyme label="a" type="m">ghesticht</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">End' de werelt schoon <rhyme label="a" type="m" corresp="#s7:l1">verlicht</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s7:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">De Sonn' die met haren <rhyme label="a" type="m">schijn</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">S'daghes herscher soude <rhyme label="a" type="m" corresp="#s8:l1">zijn</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s8:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">* End' de Maen die bruyckt haer <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Met de Sterren inder <rhyme label="a" type="m" corresp="#s9:l1">nacht</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s9:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">* D'eerst gheboor' nen heeft <rhyme label="a" type="m">gheslaen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">In Egypten end' <rhyme label="a" type="m" corresp="#s10:l1">verdaen</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s10:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">* Ende daer uyt heeft <rhyme label="a" type="m">ghebracht</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Israel met grooter <rhyme label="a" type="m" corresp="#s11:l1">macht</rhyme>:</l>
        <l id="s11:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s11:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">* Door zijn stercke handt end' <rhyme label="a" type="m">scherm</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Met een uytghereckten <rhyme label="a" type="m" corresp="#s12:l1">erm</rhyme>:</l>
        <l id="s12:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s12:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">* Die de roode biesen <rhyme label="a" type="m">zee</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Met ghewelt verdeyld' in <rhyme label="a" type="m" corresp="#s13:l1">twee</rhyme>:</l>
        <l id="s13:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s13:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">* End' voer zijn volck <rhyme label="a" type="m">Israel</rhyme></l>
        <l id="s14:l2" met="4" type="+-">Midden door haer baren <rhyme label="a" type="m" corresp="#s14:l1">fel</rhyme>:</l>
        <l id="s14:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s14:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">* Pharao met all' zijn <rhyme label="a" type="m">heyr</rhyme>,</l>
        <l id="s15:l2" met="4" type="+-">Veld' hy midden in het <rhyme label="a" type="m" corresp="#s15:l1">meyr</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s15:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">End' voorts leyde metter <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s16:l2" met="4" type="+-">Sijn volck door het woeste <rhyme label="a" type="m" corresp="#s16:l1">landt</rhyme>:</l>
        <l id="s16:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s16:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">* Die de Coninghen seer <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s17:l2" met="4" type="+-">Toornich sloech end' bracht ter <rhyme label="a" type="m" corresp="#s17:l1">doot</rhyme>:</l>
        <l id="s17:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s17:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">* Vorsten van seer grooter <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s18:l2" met="4" type="+-">Heeft hy schielijck <rhyme label="a" type="m" corresp="#s18:l1">omghebracht</rhyme>:</l>
        <l id="s18:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s18:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">* Sichon heeft ghebracht te <rhyme label="a" type="m">schandt</rhyme>,</l>
        <l id="s19:l2" met="4" type="+-">Coninck wyl' in <rhyme label="a" type="m" corresp="#s19:l1">Emorlandt</rhyme>:</l>
        <l id="s19:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s19:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">* End' Og mede den <rhyme label="a" type="m">tyran</rhyme>,</l>
        <l id="s20:l2" met="4" type="+-">Die regeerde tot <rhyme label="a" type="m" corresp="#s20:l1">Basan</rhyme>:</l>
        <l id="s20:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s20:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s20:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">* End' heeft haer landt ende <rhyme label="a" type="m">werf</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">Wtghegheuen tot een <rhyme label="a" type="m" corresp="#s21:l1">erf</rhyme>:</l>
        <l id="s21:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s21:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s21:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">* Tot een erf van <rhyme label="a" type="m">Israel</rhyme></l>
        <l id="s22:l2" met="4" type="+-">Sijns knechts die deed' zijn <rhyme label="a" type="m" corresp="#s22:l1">bevel</rhyme>:</l>
        <l id="s22:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s22:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s22:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">* Die op ons in ons' <rhyme label="a" type="m">ootmoet</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Heeft ghedacht met herten <rhyme label="a" type="m" corresp="#s23:l1">goet</rhyme>:</l>
        <l id="s23:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s23:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s23:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">* Heeft ons verlost end' <rhyme label="a" type="m">bevrijt</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">Van des vyants swaren <rhyme label="a" type="m" corresp="#s24:l1">strijt</rhyme>:</l>
        <l id="s24:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s24:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s24:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">* Die spys' ende kost <rhyme label="a" type="m">bescheirt</rhyme>,</l>
        <l id="s25:l2" met="4" type="+-">End' voort alle vleesch <rhyme label="a" type="m" corresp="#s25:l1">gheneirt</rhyme>:</l>
        <l id="s25:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s25:l4" met="4" type="+-">Duert tot inder <rhyme label="b" type="m" corresp="#s25:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">* Looft end' dancket wel <rhyme label="a" type="m">ghewis</rhyme>,</l>
        <l id="s26:l2" met="4" type="+-">Godt die Heer des hemels <rhyme label="a" type="m" corresp="#s26:l1">is</rhyme>,</l>
        <l id="s26:l3" met="4" type="+-">Want zijn <rhyme label="b" type="m">goedertierenheyt</rhyme>,</l>
        <l id="s26:l4" met="4" type="+-">Duert in aller <rhyme label="b" type="m" corresp="#s26:l3">eewicheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
