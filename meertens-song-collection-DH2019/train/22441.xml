      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="danklied" translation="gratitude song">danklied</tag>
      <tag standard-name="morgenlied" translation="morning song">morgenlied</tag>
    </tags>
    <melody>Psalm 128 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ick danck u lieue <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat ghy my hebt <rhyme label="b" type="m">bewaert</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">In desen nacht met <rhyme label="a" type="f" corresp="#s1:l1">eere</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Daer in ick lach <rhyme label="b" type="m" corresp="#s1:l2">beswaert</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Met duysterheyt <rhyme label="c" type="f">bevanghen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En oock in grooten <rhyme label="d" type="m">noot</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Twelck ick al ben <rhyme label="c" type="f" corresp="#s1:l5">ontganghen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Godt door u hulpe <rhyme label="d" type="m" corresp="#s1:l6">groot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Met Sanck wil ick u <rhyme label="a" type="f">louen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O ghy mijn Heere <rhyme label="b" type="m">goet</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">In den Hemel hier <rhyme label="a" type="f" corresp="#s2:l1">bouen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">My oock den dach <rhyme label="b" type="m" corresp="#s2:l2">behoet</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Hierom stort ick mijn <rhyme label="c" type="f">beden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Macht dijnen wille <rhyme label="d" type="m">zijn</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Leydt my in dijner <rhyme label="c" type="f" corresp="#s2:l5">vreden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En breeckt den wille <rhyme label="d" type="m" corresp="#s2:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laet my Heer niet <rhyme label="a" type="f">afwijcken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van dijner rechter <rhyme label="b" type="m">baen</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Helpt my Sathan <rhyme label="a" type="f" corresp="#s3:l1">ontstrijcken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Opdat ik niet dole <rhyme label="b" type="m" corresp="#s3:l2">zaen</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Bewaert my door u <rhyme label="c" type="f">gauen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat bid ick <rhyme label="d" type="m">vierichlijck</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Voor Sduyvels listich <rhyme label="c" type="f" corresp="#s3:l5">drauen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En aenual <rhyme label="d" type="m" corresp="#s3:l6">gierichlijck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wilt my tghelooue <rhyme label="a" type="f">gheuen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Aen Jhesum Christum <rhyme label="b" type="m">altijt</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Vrijt my vant sondich <rhyme label="a" type="f" corresp="#s4:l1">leuen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Op deser aerden <rhyme label="b" type="m" corresp="#s4:l2">wijt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Ghy sult my niet <rhyme label="c" type="f">ontsegghen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Wat ghy my hebt <rhyme label="d" type="m">belooft</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Christus wil op hem <rhyme label="c" type="f" corresp="#s4:l5">legghen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Mijn sonden <rhyme label="d" type="m" corresp="#s4:l6">ongheclooft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gheeft my oock hope <rhyme label="a" type="f">crachtich</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die niet tot mismoet <rhyme label="b" type="m">staet</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Daer by die liefde <rhyme label="a" type="f" corresp="#s5:l1">machtich</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tot hem die my <rhyme label="b" type="m" corresp="#s5:l2">versmaet</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Dat ick hem duecht <rhyme label="c" type="f">bewijse</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Niet soeckende <rhyme label="d" type="m">proffijt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Vliedende sweerelts <rhyme label="c" type="f" corresp="#s5:l5">gijse</rhyme>:</l>
        <l id="s5:l8" met="3" type="-+">Gheueynst en <rhyme label="d" type="m" corresp="#s5:l6">hypocrijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Laet my u woort <rhyme label="a" type="f">bekennen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Voor dese weerelt <rhyme label="b" type="m">fel</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Dat ick oock sonder <rhyme label="a" type="f" corresp="#s6:l1">spennen</rhyme>/</l>
        <l id="s6:l4" met="3" type="-+">Niet spaere goet, noch <rhyme label="b" type="m" corresp="#s6:l2">vel</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Al dat my mach <rhyme label="c" type="f">verleyden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van dijne waerheyt <rhyme label="d" type="m">claer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Wilt my dear af <rhyme label="c" type="f" corresp="#s6:l5">verscheyden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">In Jhesu Christo <rhyme label="d" type="m" corresp="#s6:l6">daer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Laet my den dach <rhyme label="a" type="f">voleynden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Met lof des naemens <rhyme label="b" type="m">dijn</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Laet my niet van u <rhyme label="a" type="f" corresp="#s7:l1">weynden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Doet my ghestadich <rhyme label="b" type="m" corresp="#s7:l2">zijn</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Bewaert my lijf en <rhyme label="c" type="f">leuen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En die vruchten opt <rhyme label="d" type="m">Lant</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Wat ghy my hebt <rhyme label="c" type="f" corresp="#s7:l5">ghegheuen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Staet tsamen in u <rhyme label="d" type="m" corresp="#s7:l6">hant</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dijn lof Heer ick <rhyme label="a" type="f">ghewaghe</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Voor dijne weldaet <rhyme label="b" type="m">al</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Die ghy my desen <rhyme label="a" type="f" corresp="#s8:l1">daghe</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Bewijset in dit <rhyme label="b" type="m" corresp="#s8:l2">dal</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Dijnen name wil ick <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Die daer alleen is <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Wilt my met u lijf <rhyme label="c" type="f" corresp="#s8:l5">spijsen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">En lauen met u reyn <rhyme label="d" type="m" corresp="#s8:l6">bloet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">U dient alleen die <rhyme label="a" type="f">eere</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">U dient alleen den <rhyme label="b" type="m">prijs</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Dat ick in duecht <rhyme label="a" type="f" corresp="#s9:l1">vermeere</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Gheeft my ghenaed <rhyme label="b" type="m" corresp="#s9:l2">wijs</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Dat wy met vrede <rhyme label="c" type="f">ontslapen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Sonder wanhopen <rhyme label="d" type="m">hier</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Gheeft ons gheloouens <rhyme label="c" type="f" corresp="#s9:l5">wapen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Teghen des duyuels <rhyme label="d" type="m" corresp="#s9:l6">vier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
