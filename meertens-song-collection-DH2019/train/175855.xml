    <melody>Wilder dan wild</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Singt nu Victory, hout in <rhyme label="a" type="f">memory</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Singt nu Victory, fris wel <rhyme label="b" type="m">gemoet</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Singht nu Victory, hout in <rhyme label="a" type="f" corresp="#s1:l1">memory</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Van het tyrannische Spaense <rhyme label="b" type="m" corresp="#s1:l2">bloet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die tegen ons stonden, gelijck <rhyme label="a" type="f">bloethonden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die tegen ons stonden met trotsen <rhyme label="b" type="m">moet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die tegen ons stonden, om te <rhyme label="a" type="f" corresp="#s2:l1">verslonden</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Al ons arm Christenen <rhyme label="b" type="m" corresp="#s2:l2">bloet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want als sy hoorden, als de <rhyme label="a" type="f">verstoorden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Want als sy hoorden soo seer <rhyme label="b" type="m">versaeght</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Want als sy hoorden, met soete <rhyme label="a" type="f" corresp="#s3:l1">woorden</rhyme></l>
        <l id="s3:l4" met="4" type="-+">De Prins sou trouwen de Bosse <rhyme label="b" type="m" corresp="#s3:l2">Maegt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Quamen hoog-moedig, ja also <rhyme label="a" type="f">spoedig</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Quamen hoog-moedigh, blaffen in 't <rhyme label="b" type="m">veld</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Quamen hoog-moedig, als bloet-honden <rhyme label="a" type="f" corresp="#s4:l1">woedigh</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Om den Bos t'ontsetten al met <rhyme label="b" type="m" corresp="#s4:l2">gewelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer geen kans siende, slagen daer toe <rhyme label="a" type="f">diende</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Maer geen kans siende, sloegen sy <rhyme label="b" type="m">raet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Om voort te trecken al met haer <rhyme label="a" type="f" corresp="#s5:l1">spreken</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Al na de Veluw metter <rhyme label="b" type="m" corresp="#s5:l2">daet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Daer toe quamen, die heel <rhyme label="a" type="f">onbeschamen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Daer tot haer quade, van moorders <rhyme label="b" type="m">aert</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Daer toe quamen, die men noemt met <rhyme label="a" type="f" corresp="#s6:l1">namen</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Moorders, rovers branders alias <rhyme label="b" type="m" corresp="#s6:l2">krabaet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Daer liepe dees sielen, al met <rhyme label="a" type="f">kakhielen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Daer liepen dees sielen langs den <rhyme label="b" type="m">rijnkant</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Om over te komen, tot haer <rhyme label="a" type="f" corresp="#s7:l1">dromen</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Jn de Betuw dat vette <rhyme label="b" type="m" corresp="#s7:l2">Land</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Een Patriot goedigh, Graef Ernst <rhyme label="a" type="f">spoedigh</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Een Patriot goedigh van 't <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Die heeft haer moedig, daer toe seer <rhyme label="a" type="f" corresp="#s8:l1">spoedigh</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Tegen gestaen aen alle <rhyme label="b" type="m" corresp="#s8:l2">kant</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maer dees bloet-honden, hoort mijn <rhyme label="a" type="f">vermonden</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Maer dees bloet-honden seer <rhyme label="b" type="m">obstinaet</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Maer dees bloet-honden, doe sy niet <rhyme label="a" type="f" corresp="#s9:l1">konden</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Komen tot haer voornemen <rhyme label="b" type="m" corresp="#s9:l2">quaet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Gingen sy branden, roofden de <rhyme label="a" type="f">landen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Gingen sy branden aen alle <rhyme label="b" type="m">kant</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Gingen sy branden, in Veluws <rhyme label="a" type="f" corresp="#s10:l1">Landen</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Doch mosten wijcken met <rhyme label="b" type="m" corresp="#s10:l2">schant</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Want als kloek-moedig, Graef Ernst <rhyme label="a" type="f">spoedigh</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Met sijn Soldaten kloeck seer <rhyme label="b" type="m">valjant</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Marceerde krachtig, over d'Yssel <rhyme label="a" type="f" corresp="#s11:l1">magtig</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Tot aen de Yssel seer wel <rhyme label="b" type="m" corresp="#s11:l2">bekant</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Eer sy vernomen, dat daer sou <rhyme label="a" type="f">komen</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Eer sy vernomen den vromen <rhyme label="b" type="m">Helt</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Scheten dees sielen, al langs haer <rhyme label="a" type="f" corresp="#s12:l1">hielen</rhyme></l>
        <l id="s12:l4" met="4" type="-+">En mosten schampen door ons <rhyme label="b" type="m" corresp="#s12:l2">gewelt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Baden om genade, 't was vroeg of <rhyme label="a" type="f">spade</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Baden om genade en vryen <rhyme label="b" type="m">pas</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Nu leggen haer brillen, hasp'len en <rhyme label="a" type="f" corresp="#s13:l1">spillen</rhyme></l>
        <l id="s13:l4" met="4" type="-+">En haren panne-koeck leyt in de <rhyme label="b" type="m" corresp="#s13:l2">as</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Den Bos verloren, seyt Paep <rhyme label="a" type="f">geschoren</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Den Bosch verloren en Wesel <rhyme label="b" type="m">quijt</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Den Bosch verloren, spijt Paep <rhyme label="a" type="f" corresp="#s14:l1">geschoren</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Paep uyt, Paep uyt, ghy krijgt de <rhyme label="b" type="m" corresp="#s14:l2">stuyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
