<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>136584</id>
    <date>
      <year-start>1769</year-start>
      <year-end>1769</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik lig somtijds gehele nachten</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Al de Zeven <rhyme label="a" type="f">Staet-genooten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Door de Liefde zaem <rhyme label="a" type="f" corresp="#s1:l1">geslooten</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Zyn verheugd om deeze <rhyme label="b" type="m">Maer</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat Prins Willem en <rhyme label="c" type="f">Sophye</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Sig in Liefde zaemen <rhyme label="c" type="f" corresp="#s1:l4">vleyen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En Vereenen aen <rhyme label="b" type="m" corresp="#s1:l3">elkaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Berlyn, Potsdam, al de <rhyme label="a" type="f">Saelen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Van de Hofstoet vrolyk <rhyme label="a" type="f" corresp="#s2:l1">pralen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Koning Frederik de <rhyme label="b" type="m">Groot</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Komt zyn Aldelyke <rhyme label="c" type="f">Nichte</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Eene Goude Kroon toe <rhyme label="c" type="f" corresp="#s2:l4">digten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En een Kleed, een Duur <rhyme label="b" type="m" corresp="#s2:l3">Kleynoot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Huis van Bronswyk en <rhyme label="a" type="f">Hanover</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Strooyt vergulde Palm en <rhyme label="a" type="f" corresp="#s3:l1">Lover</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Wolffenbuttel raept by <rhyme label="b" type="m">een</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Veele Heil en Kroon <rhyme label="c" type="f">Cieraeden</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Het stond alles in <rhyme label="c" type="f" corresp="#s3:l4">Paraede</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Met hy daer kwam <rhyme label="b" type="m" corresp="#s3:l3">doorgereen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ongemeene Eere <rhyme label="a" type="f">staetie</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dankbetuiging, Lof en <rhyme label="a" type="f" corresp="#s4:l1">graetie</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Kreeg zyn Hoogheid van <rhyme label="b" type="m">alom</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Alwaer hy is door <rhyme label="c" type="f">gereezen</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Sag men blydschap op het <rhyme label="c" type="f" corresp="#s4:l4">Weezen</rhyme>;</l>
        <l id="s4:l6" met="4" type="+-">Trots een Staet- en <rhyme label="b" type="m" corresp="#s4:l3">Koningdom</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Stammen, Maegschap en <rhyme label="a" type="f">Geslagten</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Toonen hunnen vreugd in <rhyme label="a" type="f" corresp="#s5:l1">Kragten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Om dit Koninglyke <rhyme label="b" type="m">Paer</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Het Soepeeren ende <rhyme label="c" type="f">Baelen</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Vivaes en de Vreugde <rhyme label="c" type="f" corresp="#s5:l4">maelen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Klinken wonder door <rhyme label="b" type="m" corresp="#s5:l3">elkaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Des Bruids Moeder vol <rhyme label="a" type="f">vernoegen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Kroond dit Heilig <rhyme label="a" type="f" corresp="#s6:l1">zaemenvoegen</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">En zyn Oom Vorst <rhyme label="b" type="m">Lodewyk</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Laet zyn vreugd en blydschap <rhyme label="c" type="f">hooren</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Dat het vliegt tot 's Hemels <rhyme label="c" type="f" corresp="#s6:l4">Chooren</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Om dit Princelyk <rhyme label="b" type="m" corresp="#s6:l3">Huwelyk</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Uit het Koninklyke <rhyme label="a" type="f">Cleeve</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Werd op 's Konings last <rhyme label="a" type="f" corresp="#s7:l1">geschreeven</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Dat men 't alom melden <rhyme label="b" type="m">zal</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">En deez' Trouwdag <rhyme label="c" type="f">Celebreeren</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Met veel Majesteid en <rhyme label="c" type="f" corresp="#s7:l4">Eeren</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">In de Tempels met <rhyme label="b" type="m" corresp="#s7:l3">geschal</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Groningen ende <rhyme label="a" type="f">Ommelanden</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Wil dat men zal Vuuren <rhyme label="a" type="f" corresp="#s8:l1">Branden</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Om het Huw'lyk reeds <rhyme label="b" type="m">geschied</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Al de Klokken moeten <rhyme label="c" type="f">brommen</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Lof geschal en <rhyme label="c" type="f" corresp="#s8:l4">Heiligdommen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Men aldaer en elders <rhyme label="b" type="m" corresp="#s8:l3">ziet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Deeze Trouwdag reeds <rhyme label="a" type="f">verscheenen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Van 't Hoogstaetelyk <rhyme label="a" type="f" corresp="#s9:l1">Vereenen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Strekt tot vreugd van <rhyme label="b" type="m">Nederland</rhyme>.</l>
        <l id="s9:l4" met="4" type="+-">Steeden, Dorpen <rhyme label="c" type="f">opgetoogen</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">'s Hemels groote Naem <rhyme label="c" type="f" corresp="#s9:l4">verhoogen</rhyme>:</l>
        <l id="s9:l6" met="4" type="+-">Om deez' Goddlyken <rhyme label="b" type="m" corresp="#s9:l3">Band</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Al de Groote en <rhyme label="a" type="f">Hooge-Schoolen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Aen Minerva's zorg <rhyme label="a" type="f" corresp="#s10:l1">bevoolen</rhyme>;</l>
        <l id="s10:l3" met="4" type="+-">Schaet'ren op Catheders <rhyme label="b" type="m">zaem</rhyme></l>
        <l id="s10:l4" met="4" type="+-">En zy strooyen <rhyme label="c" type="f">Lauwerblaeden</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Op Prins Willem en zyn <rhyme label="c" type="f" corresp="#s10:l4">Gaede</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Tot eer van des Hemels <rhyme label="b" type="m" corresp="#s10:l3">Naem</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Leyden, Utrecht en <rhyme label="a" type="f">Schoonhoven</rhyme>.</l>
        <l id="s11:l2" met="4" type="+-">Dordregt, Haarlem zamen <rhyme label="a" type="f" corresp="#s11:l1">looven</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Deezen Hoog-gedugten <rhyme label="b" type="m">Echt</rhyme>.</l>
        <l id="s11:l4" met="4" type="+-">Delft, den Haag met al de <rhyme label="c" type="f">Grooten</rhyme>:</l>
        <l id="s11:l5" met="4" type="+-">Eeren deeze <rhyme label="c" type="f" corresp="#s11:l4">Trouw-genooten</rhyme>!</l>
        <l id="s11:l6" met="4" type="+-">En het Lauwerieren <rhyme label="b" type="m" corresp="#s11:l3">vlegt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Rotterdam en Gorkum <rhyme label="a" type="f">meede</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Hunner Beider Naem <rhyme label="a" type="f" corresp="#s12:l1">verbreede</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Breda en de <rhyme label="b" type="m">Barony</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Met het Graafelyke <rhyme label="c" type="f">Buuren</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Stigten Dank- en <rhyme label="c" type="f" corresp="#s12:l4">Vreugde-vuuren</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Zyn in Ziel en Lichaem <rhyme label="b" type="m" corresp="#s12:l3">bly</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Middelburg de Zeeuwsche <rhyme label="a" type="f">Steeden</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Goes en Zirkzee al <rhyme label="a" type="f" corresp="#s13:l1">meede</rhyme>.</l>
        <l id="s13:l3" met="4" type="+-">Vlissingen, voor al ter <rhyme label="b" type="m">Veer</rhyme>:</l>
        <l id="s13:l4" met="4" type="+-">Kroonen 't Koninklyke <rhyme label="c" type="f">Paeren</rhyme>!</l>
        <l id="s13:l5" met="4" type="+-">Het vlegt vreugd en blyde <rhyme label="c" type="f" corresp="#s13:l4">snaeren</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Met dankzegging aen den <rhyme label="b" type="m" corresp="#s13:l3">Heer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Arnhem, Zutphen en <rhyme label="a" type="f">Nimweegen</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Cieren deeze <rhyme label="a" type="f" corresp="#s14:l1">Huw'lyks-zeegen</rhyme>:</l>
        <l id="s14:l3" met="4" type="+-">Met een Staetsie vol van <rhyme label="b" type="m">Eer</rhyme>.</l>
        <l id="s14:l4" met="4" type="+-">'t Koninklyke Loo <rhyme label="c" type="f">alomme</rhyme>,</l>
        <l id="s14:l5" met="4" type="+-">Van de vreugd in glorie <rhyme label="c" type="f" corresp="#s14:l4">brommen</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Met erkentenis van hun <rhyme label="b" type="m" corresp="#s14:l3">Heer</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Amsteldam in Vest en <rhyme label="a" type="f">Wallen</rhyme></l>
        <l id="s15:l2" met="4" type="+-">Ziet men Vreugde-vuuren <rhyme label="a" type="f" corresp="#s15:l1">Brallen</rhyme></l>
        <l id="s15:l3" met="4" type="+-">Oost-en <rhyme label="b" type="m">Wester-Maetschappy</rhyme>!</l>
        <l id="s15:l4" met="4" type="+-">Laeten hunne Wimpels <rhyme label="c" type="f">zwieren</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Groot en kleine willig <rhyme label="c" type="f" corresp="#s15:l4">vieren</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">Willem en Princes <rhyme label="b" type="m" corresp="#s15:l3">Zophy</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Harderwyk Elburg en <rhyme label="a" type="f">meede</rhyme></l>
        <l id="s16:l2" met="4" type="+-">Hasselt, Hattem, en de <rhyme label="a" type="f" corresp="#s16:l1">Steeden</rhyme></l>
        <l id="s16:l3" met="4" type="+-">Deventer, en ook <rhyme label="b" type="m">Steenwyk</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Vollenhoof, Breevoort en <rhyme label="c" type="f">Almen</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Odenzaal, die vlegten <rhyme label="c" type="f" corresp="#s16:l4">Palmen</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">Op dit Vorstelyk <rhyme label="b" type="m" corresp="#s16:l3">Huwelyk</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Schiedam, Heusden en de <rhyme label="a" type="f">Bossche</rhyme>,</l>
        <l id="s17:l2" met="4" type="+-">'t Kanon van de Wallen <rhyme label="a" type="f" corresp="#s17:l1">lossen</rhyme>,</l>
        <l id="s17:l3" met="4" type="+-">'t Eert en groet dit Vorstelyk <rhyme label="b" type="m">Paer</rhyme>.</l>
        <l id="s17:l4" met="4" type="+-">Al de Wyze <rhyme label="c" type="f">Proffezooren</rhyme>;</l>
        <l id="s17:l5" met="4" type="+-">Doen hunne Catherders <rhyme label="c" type="f" corresp="#s17:l4">hooren</rhyme>,</l>
        <l id="s17:l6" met="4" type="+-">Van deez' blyde <rhyme label="b" type="m" corresp="#s17:l3">Vreugde-Maer</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Briel en ook het Land van <rhyme label="a" type="f">Voorn</rhyme>,</l>
        <l id="s18:l2" met="4" type="+-">Laeten hunne galmen <rhyme label="a" type="f" corresp="#s18:l1">hooren</rhyme>.</l>
        <l id="s18:l3" met="4" type="+-">Vlaardingenen ook <rhyme label="b" type="m">Maasluis</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Eeren deeze Huwelyks <rhyme label="c" type="f">steatsie</rhyme>!</l>
        <l id="s18:l5" met="4" type="+-">Met een ongemeene <rhyme label="c" type="f" corresp="#s18:l4">graetsie</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">En een vrolyk zoet <rhyme label="b" type="m" corresp="#s18:l3">gedruis</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Alkmaar komt zig <rhyme label="a" type="f">verkwikken</rhyme>,</l>
        <l id="s19:l2" met="4" type="+-">In dit Heug'lyk <rhyme label="a" type="f" corresp="#s19:l1">Trouwe-beschikken</rhyme>!</l>
        <l id="s19:l3" met="4" type="+-">Van Prins Willem en <rhyme label="b" type="m">Zophy</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">'t Hof in 't Bosch is <rhyme label="c" type="f">uitgelaaten</rhyme>,</l>
        <l id="s19:l5" met="4" type="+-">Om de vreugde <rhyme label="c" type="f" corresp="#s19:l4">bovenmaeten</rhyme>!</l>
        <l id="s19:l6" met="4" type="+-">En het koppelt Dans en <rhyme label="b" type="m" corresp="#s19:l3">Rey</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">De vermaerde Stad <rhyme label="a" type="f">Enkhuyzen</rhyme>,</l>
        <l id="s20:l2" met="4" type="+-">Daer de Zee komt aen te <rhyme label="a" type="f" corresp="#s20:l1">bruyzen</rhyme>,</l>
        <l id="s20:l3" met="4" type="+-">Eeren deezen Huwelyks <rhyme label="b" type="m">stoet</rhyme>.</l>
        <l id="s20:l4" met="4" type="+-">Oost- en West-Zaandam te <rhyme label="c" type="f">zaemen</rhyme>,</l>
        <l id="s20:l5" met="4" type="+-">Edam Munnikendam met <rhyme label="c" type="f" corresp="#s20:l4">naeme</rhyme>,</l>
        <l id="s20:l6" met="4" type="+-">'t Vorstelyk Paer met Zeegen <rhyme label="b" type="m" corresp="#s20:l3">groet</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Hoorn en ook <rhyme label="a" type="f">Purmerende</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">Medenblik hun Heil-wensch <rhyme label="a" type="f" corresp="#s21:l1">zenden</rhyme>,</l>
        <l id="s21:l3" met="4" type="+-">Naer dit Vorstelyke <rhyme label="b" type="m">Paer</rhyme>.</l>
        <l id="s21:l4" met="4" type="+-">Zwol, Doesburg, Grol en <rhyme label="c" type="f">Campen</rhyme>,</l>
        <l id="s21:l5" met="4" type="+-">Met Handklappen, hun <rhyme label="c" type="f" corresp="#s21:l4">aenklampen</rhyme>,</l>
        <l id="s21:l6" met="4" type="+-">'t Looft in hun den <rhyme label="b" type="m" corresp="#s21:l3">Zeegenaer</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Amsteldam de kroon der <rhyme label="a" type="f">Steeden</rhyme>,</l>
        <l id="s22:l2" met="4" type="+-">Eert met vreugd en <rhyme label="a" type="f" corresp="#s22:l1">vrolykheeden</rhyme></l>
        <l id="s22:l3" met="4" type="+-">Deez' gewenschte Huwelyks <rhyme label="b" type="m">dag</rhyme>.</l>
        <l id="s22:l4" met="4" type="+-">Wie weet hoe de <rhyme label="c" type="f">Patriotten</rhyme>,</l>
        <l id="s22:l5" met="4" type="+-">In de Liefde zaemen <rhyme label="c" type="f" corresp="#s22:l4">vlotten</rhyme></l>
        <l id="s22:l6" met="4" type="+-">Door 't uitsteeken van de <rhyme label="b" type="m" corresp="#s22:l3">Vlag</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Yder roemt met mond en <rhyme label="a" type="f">herte</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Leef Oranje zonder <rhyme label="a" type="f" corresp="#s23:l1">smerten</rhyme>!</l>
        <l id="s23:l3" met="4" type="+-">Leef Oranje in een <rhyme label="b" type="m">Zaed</rhyme>;</l>
        <l id="s23:l4" met="4" type="+-">Leeft Prins Willem en <rhyme label="c" type="f">Wilmyne</rhyme>!</l>
        <l id="s23:l5" met="4" type="+-">God bewaer Uw, en de <rhyme label="c" type="f" corresp="#s23:l4">zyne</rhyme>!</l>
        <l id="s23:l6" met="4" type="+-">Tot Heil, zoo van Kerk als <rhyme label="b" type="m" corresp="#s23:l3">Staet</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Nooit moet onweer, druk of <rhyme label="a" type="f">rampen</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">Het Oranje Huis <rhyme label="a" type="f" corresp="#s24:l1">aenklampen</rhyme>:</l>
        <l id="s24:l3" met="4" type="+-">Wysheid, Geest, <rhyme label="b" type="m">Voorzigtigheid</rhyme>!</l>
        <l id="s24:l4" met="4" type="+-">Vallen op uw zaem <rhyme label="c" type="f">beneeden</rhyme>,</l>
        <l id="s24:l5" met="4" type="+-">Oud en Af, een Hemel <rhyme label="c" type="f" corresp="#s24:l4">vreede</rhyme>,</l>
        <l id="s24:l6" met="4" type="+-">In Gods volle <rhyme label="b" type="m" corresp="#s24:l3">Heerlykheid</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
