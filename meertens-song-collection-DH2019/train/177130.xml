  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Celinde, de <rhyme label="a" type="m">smart</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Die my treft het <rhyme label="a" type="m" corresp="#s1:l1">hart</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En alle de <rhyme label="b" type="f">klachten</rhyme></l>
        <l id="s1:l4" met="2" type="-+">En droeve <rhyme label="b" type="f" corresp="#s1:l3">gedachten</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Ben ik in <rhyme label="a" type="m" corresp="#s1:l1">verward</rhyme>;</l>
        <l id="s1:l6" met="2" type="-+">Maar, 'k zal u de <rhyme label="c" type="f">reden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Zoo aanstonds <rhyme label="c" type="f" corresp="#s1:l6">ontleden</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Van 't geen my <rhyme label="a" type="m" corresp="#s1:l1">benard</rhyme>:</l>
        <l id="s1:l9" met="2" type="-+">Alleen <rhyme label="d" type="m">Galathee</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Haar byzyn te <rhyme label="e" type="f">derven</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Doet my dus hier <rhyme label="e" type="f" corresp="#s1:l10">zwerven</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Gescheiden van 't <rhyme label="d" type="m" corresp="#s1:l9">vee</rhyme>;</l>
        <l id="s1:l13" met="2" type="-+">'k Beklaag hier het <rhyme label="f" type="m">lot</rhyme></l>
        <l id="s1:l14" met="2" type="-+">En liefdens <rhyme label="f" type="m" corresp="#s1:l13">genot</rhyme>,</l>
        <l id="s1:l15" met="2" type="-+">ô Waarde <rhyme label="g" type="f">Celinde</rhyme>!</l>
        <l id="s1:l16" met="2" type="-+">Van haar die 'k <rhyme label="g" type="f" corresp="#s1:l15">beminde</rhyme>.</l>
        <l id="s1:l17" met="2" type="-+">Op 's Vrindschaps <rhyme label="f" type="m" corresp="#s1:l13">gebod</rhyme>,</l>
        <l id="s1:l18" met="2" type="-+">Moet ik in een <rhyme label="d" type="m" corresp="#s1:l9">zee</rhyme></l>
        <l id="s1:l19" met="2" type="-+">Van rampen my <rhyme label="g" type="f" corresp="#s1:l15">vinden</rhyme>,</l>
        <l id="s1:l20" met="2" type="-+">Zonder rust of <rhyme label="d" type="m" corresp="#s1:l9">vreê</rhyme>.</l>
      </lg>
    </lg>
  </text>
