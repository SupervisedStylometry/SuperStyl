<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>11820</id>
    <date>
      <year-start>1648</year-start>
      <year-end>1648</year-end>
    </date>
    <tags/>
    <melody>Psalm 100 Datheen</melody>
  <author>Dirck Pietersz Pers</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEt wonder-werck dat Christus <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat zy een troost in ons <rhyme label="a" type="m" corresp="#s1:l1">gemoedt</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Die met twee Vischjes en vijf <rhyme label="b" type="m">Broon</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Vijf duysent derf te gaste <rhyme label="b" type="m" corresp="#s1:l3">noon</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En trecken noch, voor ons <rhyme label="a" type="m">genot</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Twaelf korven, voor het <rhyme label="a" type="m" corresp="#s2:l1">overschot</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Ghy Menschen kindt, die 't daeghlijcks <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Versmaet sijn gunst en achtse <rhyme label="b" type="m" corresp="#s2:l3">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
