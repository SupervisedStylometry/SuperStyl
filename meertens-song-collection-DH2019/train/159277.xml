<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159277</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Lofzang van Maria</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">KOmt laet ons nu den <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Lof singen, en zijn <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Uytbreyden, en <rhyme label="b" type="f">vermelden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hier is een hooge <rhyme label="c" type="m">stof</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">O! singht den Heere <rhyme label="c" type="m" corresp="#s1:l4">Lof</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met d' Herders op den <rhyme label="b" type="f" corresp="#s1:l3">velde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Waeckt op mijn zieltje <rhyme label="a" type="m">bloot</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">De vreucht die is seer <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Den Hemel is <rhyme label="b" type="f">ontsloten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">De aerd' die is <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Van Godes goetheyt <rhyme label="c" type="m" corresp="#s2:l4">klaer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Seer heerlijck <rhyme label="b" type="f" corresp="#s2:l3">overgoten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een soete <rhyme label="a" type="m">melody</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Vertoont hem hier seer <rhyme label="a" type="m" corresp="#s3:l1">bly</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Van Hemelsche <rhyme label="b" type="f">heyrscharen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die Godes groote <rhyme label="c" type="m">macht</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Sijn heerlijckheyt vol <rhyme label="c" type="m" corresp="#s3:l4">kracht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ons wonderlijck <rhyme label="b" type="f" corresp="#s3:l3">verklaren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den Schepper hoogh <rhyme label="a" type="m">vermaert</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Van Hemel, Zee en <rhyme label="a" type="m" corresp="#s4:l1">Aerdt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat heyligh eeuwigh <rhyme label="b" type="f">wesen</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Dat daelt uyt 's Hemels <rhyme label="c" type="m">throon</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En wort eens menschen <rhyme label="c" type="m" corresp="#s4:l4">Soon</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Gelijck wy kunnen <rhyme label="b" type="f" corresp="#s4:l3">lesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gedenckt, hoe 't Hemels <rhyme label="a" type="m">Lof</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Vol glory en vol <rhyme label="a" type="m" corresp="#s5:l1">lof</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Beroert was, en vol <rhyme label="b" type="f">schromen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat sulck een <rhyme label="c" type="m">Ambassaet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ia Godt selfs in der <rhyme label="c" type="m" corresp="#s5:l4">daet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Tot ons soud' <rhyme label="b" type="f" corresp="#s5:l3">neder-komen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Lof zy u <rhyme label="a" type="m">Majesteyt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">O Heer! in <rhyme label="a" type="m" corresp="#s6:l1">eeuwigheyt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wilt ons voortaen <rhyme label="b" type="f">bewaren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Voor leet, of <rhyme label="c" type="m">ziels-gevaer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En in dit <rhyme label="c" type="m" corresp="#s6:l4">Nieuwe-Iaer</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Tot saligheyt ons <rhyme label="b" type="f" corresp="#s6:l3">sparen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
