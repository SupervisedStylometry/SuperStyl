      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Galathe dreef haer <rhyme label="a" type="m">vee</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Strandewaerts al by de <rhyme label="a" type="m" corresp="#s1:l1">zee</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Over del en <rhyme label="b" type="m">duyn</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Tot op 't schelpigh <rhyme label="c" type="m">strandt</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Daer haer Lammertjes vak'ren in 't <rhyme label="c" type="m" corresp="#s1:l4">zandt</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Van een hooghe <rhyme label="b" type="m" corresp="#s1:l3">kruyn</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Siet haer <rhyme label="d" type="m">Coridon</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Voor d'hitte van de <rhyme label="d" type="m" corresp="#s1:l7">Son</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Moeder naeckt <rhyme label="e" type="m">ontkleen</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Van het top al tot de <rhyme label="e" type="m" corresp="#s1:l9">teen</rhyme>,</l>
        <l id="s1:l11" met="4" type="-+">En wassen haer albast're <rhyme label="e" type="m" corresp="#s1:l9">leen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wijl sy plast, en haer <rhyme label="a" type="m">wast</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Hy van acht'ren haer <rhyme label="a" type="m" corresp="#s2:l1">verrast</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">Want hy sluyps-wijs <rhyme label="b" type="m">komt</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Langhs de duynen <rhyme label="c" type="m">heen</rhyme>,</l>
        <l id="s2:l5" met="5" type="+-">En neemt sachjes wegh haer hembt en <rhyme label="c" type="m" corresp="#s2:l4">kleen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">En voorts daer op <rhyme label="b" type="m" corresp="#s2:l3">bromt</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">En maeckt een <rhyme label="d" type="m">geluyt</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Sy verbaesdelyck <rhyme label="d" type="m" corresp="#s2:l7">uyt</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Het water vlucht, <rhyme label="e" type="m">maer</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Hy haer grijpt, en is <rhyme label="e" type="m" corresp="#s2:l9">aldaer</rhyme></l>
        <l id="s2:l11" met="4" type="-+">Van hare schoonheyt <rhyme label="e" type="m" corresp="#s2:l9">oordelaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
