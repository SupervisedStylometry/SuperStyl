  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">PHillis die mijn <rhyme label="a" type="f">sin</rhyme></l>
        <l id="s1:l2" met="3" type="+-">En mijn Ziel moet <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Schoonste <rhyme label="a" type="f" corresp="#s1:l1">Harderin</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Van den gantschen <rhyme label="b" type="m" corresp="#s1:l2">Rhijn</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Damon niet soo <rhyme label="c" type="f">vaerdigh</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Want mijn <rhyme label="d" type="m">Galathe</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Is haer al wel <rhyme label="c" type="f" corresp="#s1:l5">waerdigh</rhyme></l>
        <l id="s1:l8" met="3" type="+-">En u Phillis <rhyme label="d" type="m" corresp="#s1:l6">mee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Thirsis hoogh <rhyme label="a" type="f">genoegh</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Laet dat prijsen <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Eer dat ick <rhyme label="a" type="f" corresp="#s2:l1">verdroegh</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Gingh ick hier van <rhyme label="b" type="m" corresp="#s2:l2">daen</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Damon ick sal <rhyme label="c" type="f">spreecken</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Als ick oorsaeck <rhyme label="d" type="m">vin</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Tot mijn Tongh sal <rhyme label="c" type="f" corresp="#s2:l5">breecken</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Voor mijn <rhyme label="d" type="m" corresp="#s2:l6">Herderin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Wel soo veel te <rhyme label="a" type="f">meer</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Alsmen soeter <rhyme label="b" type="m">acht</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">'s Middaeghs schoone <rhyme label="a" type="f" corresp="#s3:l1">weer</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Voor de koude <rhyme label="b" type="m" corresp="#s3:l2">nacht</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">En de Son <rhyme label="c" type="f">bequamer</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Als de <rhyme label="d" type="m">Mane-schijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Soo veel <rhyme label="c" type="f" corresp="#s3:l5">aengenamer</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Moet mijn Phillis <rhyme label="d" type="m" corresp="#s3:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Soo veel alsmen <rhyme label="a" type="f">mach</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Schoone <rhyme label="b" type="m">dageraet</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Prijsen voor den <rhyme label="a" type="f" corresp="#s4:l1">dagh</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Die op 't heetste <rhyme label="b" type="m" corresp="#s4:l2">staet</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">En het Licht te <rhyme label="c" type="f">boven</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Gaet de <rhyme label="d" type="m">Duysternis</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Soo veel meer te <rhyme label="c" type="f" corresp="#s4:l5">loven</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Mijn Galathea <rhyme label="d" type="m" corresp="#s4:l6">is</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Achtmen niet de <rhyme label="a" type="f">hay</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Voor het dorre <rhyme label="b" type="m">strandt</rhyme>.</l>
        <l id="s5:l3" met="3" type="+-">Achtmen niet de <rhyme label="a" type="f" corresp="#s5:l1">Way</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Voor het dorre <rhyme label="b" type="m" corresp="#s5:l2">Landt</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Thirsis laet haer <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Daer ick haer voor <rhyme label="d" type="m">eer</rhyme>.</l>
        <l id="s5:l7" met="3" type="+-">Damon s'is <rhyme label="c" type="f" corresp="#s5:l5">gepresen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Maer mijn Nimphje <rhyme label="d" type="m" corresp="#s5:l6">meer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Schoonheyt die sy <rhyme label="a" type="f">heyt</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Acht ick waer mijn <rhyme label="b" type="m">leet</rhyme>.</l>
        <l id="s6:l3" met="3" type="+-">Wat is <rhyme label="a" type="f" corresp="#s6:l1">schoonigheyt</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Als ick schoonder <rhyme label="b" type="m" corresp="#s6:l2">weet</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">Yders die <rhyme label="c" type="f">beminnen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Acht sijn eygen <rhyme label="d" type="m">best</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Jae maer 't is <rhyme label="c" type="f" corresp="#s6:l5">vinnen</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Alsmen 't soeckt op 't <rhyme label="d" type="m" corresp="#s6:l6">lest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
