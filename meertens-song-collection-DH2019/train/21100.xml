    <tags/>
    <melody>Gott der vater wohn uns bei</melody>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOdt, Vader, Soon, en Heyligen <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Een Godt van allen <rhyme label="b" type="f">goeden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wy dancken u in dese <rhyme label="a" type="m" corresp="#s1:l1">Feest</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat ghy ons dus gaet <rhyme label="b" type="f" corresp="#s1:l2">voeden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Met spijs en dranck in <rhyme label="c" type="m">over-vloet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Matelijck te <rhyme label="d" type="f">genieten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En niet te veel <rhyme label="d" type="f" corresp="#s1:l6">inschieten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Tot des lichaems <rhyme label="d" type="f" corresp="#s1:l6">verdrieten</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Maer tot 's levens welstant <rhyme label="c" type="m" corresp="#s1:l5">soet</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Danckbaerlijck t' uwer <rhyme label="e" type="f">eeren</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Laet ons u lof <rhyme label="e" type="f" corresp="#s1:l10">vermeeren</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">En voedt ons door u <rhyme label="e" type="f" corresp="#s1:l10">leeren</rhyme>,</l>
        <l id="s1:l13" met="4" type="+-">Amen, Amen, Waer en <rhyme label="f" type="m">Ja</rhyme>,</l>
        <l id="s1:l14" met="4" type="-+">Des singen wy <rhyme label="f" type="m" corresp="#s1:l13">Halelua</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
