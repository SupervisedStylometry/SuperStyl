    <melody>De sterretjes glinsteren</melody>
  <author>Cornelis Sweerts</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De starretjes glinstren aan 's hemels <rhyme label="a" type="m">trans</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Wel schoon van <rhyme label="a" type="m" corresp="#s1:l1">glans</rhyme>;</l>
        <l id="s1:l3" met="2" type="-+">Maar Fillisjes <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Van grooter <rhyme label="b" type="f" corresp="#s1:l3">vermoogen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Verspreyen een <rhyme label="c" type="m">licht</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Dat vlammende brand in de herten <rhyme label="c" type="m" corresp="#s1:l5">sticht</rhyme>.</l>
      </lg>
    </lg>
