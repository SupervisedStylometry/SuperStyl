<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186894</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Van de korporaal</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">SA helden spoed u <rhyme label="a" type="m">voort</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Rechtschape <rhyme label="b" type="f">Batavieren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Voert Kruyt en loot aen <rhyme label="a" type="m" corresp="#s1:l1">boord</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En laet de Bloed-vlag <rhyme label="b" type="f" corresp="#s1:l2">zwieren</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">De Brit verbreekt ô <rhyme label="c" type="m">schand</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">'t Verbond met <rhyme label="c" type="m" corresp="#s1:l5">Nederland</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Sa helden spoed u <rhyme label="a" type="m" corresp="#s1:l1">voort</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Voert kruyt en lood aen <rhyme label="a" type="m" corresp="#s1:l1">boord</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dien onverzaedbre <rhyme label="a" type="m">Dog</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Durft uw gelaede <rhyme label="b" type="f">Scheepen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Berooven en dat <rhyme label="a" type="m" corresp="#s2:l1">nog</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In zyne havens <rhyme label="b" type="f" corresp="#s2:l2">sleepen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dus raekt gy welk een <rhyme label="c" type="m">spyt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Uw ryke kielen <rhyme label="c" type="m" corresp="#s2:l5">kwyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Sa Helden spoed u <rhyme label="a" type="m" corresp="#s2:l1">voort</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Voert kruid en lood aen <rhyme label="a" type="m" corresp="#s2:l1">boord</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zou Neerlands fiere <rhyme label="a" type="m">Leeuw</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die 't recht nooit heeft <rhyme label="b" type="f">geschonden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Verschrikken voor 't <rhyme label="a" type="m" corresp="#s3:l1">geschreeuw</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Van Engelands <rhyme label="b" type="f" corresp="#s3:l2">Waterhonden</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">ô Neen! hy heeft <rhyme label="c" type="m">moed</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En zuiver Hollands <rhyme label="c" type="m" corresp="#s3:l5">bloed</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Sa Helden spoed dan <rhyme label="a" type="m" corresp="#s3:l1">voort</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Voert kruid en lood aen <rhyme label="a" type="m" corresp="#s3:l1">Boord</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Vadren van den <rhyme label="a" type="m">Staet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die voor ons welzyn <rhyme label="b" type="f">waeken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En steeds met ryp <rhyme label="a" type="m" corresp="#s4:l1">beraed</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Handhaeven onze <rhyme label="b" type="f" corresp="#s4:l2">zaeken</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die zullen de Eer van 't <rhyme label="c" type="m">Land</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Behoen voor smaed en <rhyme label="c" type="m" corresp="#s4:l5">schand</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Sa Helden spoed dan <rhyme label="a" type="m" corresp="#s4:l1">voort</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Voert kruid en lood aen <rhyme label="a" type="m" corresp="#s4:l1">boort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gy moogt naer 't <rhyme label="a" type="m">Staets-besluit</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met uwe sterke <rhyme label="b" type="f">kielen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Nu vaeren gaen op <rhyme label="a" type="m" corresp="#s5:l1">buit</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Om Engeland te <rhyme label="b" type="f" corresp="#s5:l2">vernielen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Wat loflyk <rhyme label="c" type="m">gunstbewys</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Al wat gy rooft is <rhyme label="c" type="m" corresp="#s5:l5">prys</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Sa Helden spoed u <rhyme label="a" type="m" corresp="#s5:l1">voort</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Voert kruid en lood aen <rhyme label="a" type="m" corresp="#s5:l1">boord</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Vrees voor geen snood <rhyme label="a" type="m">gebroed</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van britsche <rhyme label="b" type="f">Moordenaeren</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Zy missen <rhyme label="a" type="m" corresp="#s6:l1">Heldenmoet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Gy kunt hun ras <rhyme label="b" type="f" corresp="#s6:l2">vervaeren</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Uw magt is lang <rhyme label="c" type="m">bekent</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Op het vloeibaar <rhyme label="c" type="m" corresp="#s6:l5">Element</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Sa Helden spoed uw <rhyme label="a" type="m" corresp="#s6:l1">voort</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Voert kruid en lood aan <rhyme label="a" type="m" corresp="#s6:l1">boord</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Vervolgt verjaeg den <rhyme label="a" type="m">Brit</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Eer hy hier aen komt <rhyme label="b" type="f">Landen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En door de roof <rhyme label="a" type="m" corresp="#s7:l1">verhit</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ons Land zelfs aen komt <rhyme label="b" type="f" corresp="#s7:l2">randen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">'t Is tyd, betoont uw <rhyme label="c" type="m">moed</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Verdryf hem van de <rhyme label="c" type="m" corresp="#s7:l5">vloed</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Sa Helden spoed u <rhyme label="a" type="m" corresp="#s7:l1">voort</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Voerd kruid en lood aen <rhyme label="a" type="m" corresp="#s7:l1">boord</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Haest zal de Brit uw <rhyme label="a" type="m">schae</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">U met zyn bloed <rhyme label="b" type="f">betaelen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Gy zult hoe 't nu ook <rhyme label="a" type="m" corresp="#s8:l1">gae</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Uw leed op hem <rhyme label="b" type="f" corresp="#s8:l2">verhaelen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Als hy de neerlaeg <rhyme label="c" type="m">krygt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En naer zyn Adem <rhyme label="c" type="m" corresp="#s8:l5">heygt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Sa Helden spoed u <rhyme label="a" type="m" corresp="#s8:l1">voort</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Voert kruid en lood aen <rhyme label="a" type="m" corresp="#s8:l1">boord</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Hoe zullen wy <rhyme label="a" type="m">alsdan</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Verrykt door de <rhyme label="b" type="f">Oorlogskanssen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Den trotschen <rhyme label="a" type="m" corresp="#s9:l1">Engelschman</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Naar onze peip doen <rhyme label="b" type="f" corresp="#s9:l2">danssen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Hoe word zyn snood <rhyme label="c" type="m">geweld</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Dan pael en perk <rhyme label="c" type="m" corresp="#s9:l5">gesteld</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Sa Helden spoed u <rhyme label="a" type="m" corresp="#s9:l1">voort</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Voerd kruid en lood aen <rhyme label="a" type="m" corresp="#s9:l1">boord</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Zoo zal de <rhyme label="a" type="m">Koopvaerdy</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Van Neerlands <rhyme label="b" type="f">onderzaeten</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">In spyt der <rhyme label="a" type="m" corresp="#s10:l1">dwinglandy</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Zich op uw hulp <rhyme label="b" type="f" corresp="#s10:l2">verlaeten</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Zoo word uw moed <rhyme label="c" type="m">erkent</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Tot aen des waerelds <rhyme label="c" type="m" corresp="#s10:l5">end</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Sa Helden spoed u <rhyme label="a" type="m" corresp="#s10:l1">voort</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Voert kruid en lood aen <rhyme label="a" type="m" corresp="#s10:l1">boord</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wel aen dan <rhyme label="a" type="m">Heldendrom</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Rechtschaepe <rhyme label="b" type="f">Batavieren</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Slae roer den Oorlogs <rhyme label="a" type="m" corresp="#s11:l1">Trom</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Verwerft u <rhyme label="b" type="f" corresp="#s11:l2">Eer-Lauwrieren</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Zet weer de Theems in <rhyme label="c" type="m">brand</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Wreek! wreek uw <rhyme label="c" type="m" corresp="#s11:l5">Vaderland</rhyme>.</l>
        <l id="s11:l7" met="3" type="-+">Sa Helden spoed uw <rhyme label="a" type="m" corresp="#s11:l1">voort</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Voert kruit en lood aen <rhyme label="a" type="m" corresp="#s11:l1">boord</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
