    <melody>Psalm 020 Datheen</melody>
  <author>Joannes Six van Chandelier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De Heer, o vorst, wil antwoord <rhyme label="a" type="f">geeven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In uw benaauwden <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Gods naam, die Jakob heeft <rhyme label="a" type="f" corresp="#s1:l1">verheeven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verhoog u in den <rhyme label="b" type="m" corresp="#s1:l2">slag</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Hy schik, van uit syn <rhyme label="c" type="f">heiligheeden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Syn hulp, die u <rhyme label="d" type="m">beschutte</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">En ondersteun, uit Zions <rhyme label="c" type="f" corresp="#s1:l5">hutte</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Met syne hand, uw <rhyme label="d" type="m" corresp="#s1:l6">leeden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hy denk aan al uw <rhyme label="a" type="f">offerhanden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En riek wat spys gy <rhyme label="b" type="m">schonkt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hy doe tot asch uw offer <rhyme label="a" type="f" corresp="#s2:l1">branden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Door hemelsch vier <rhyme label="b" type="m" corresp="#s2:l2">ontvonkt</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Hy geeve, naa uws harten <rhyme label="c" type="f">beede</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Geluk in uwe <rhyme label="d" type="m">daaden</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Syn gunst vervul, tot rust, en <rhyme label="c" type="f" corresp="#s2:l5">vreede</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Al watge zult <rhyme label="d" type="m" corresp="#s2:l6">beraaden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Uw heil zal ons tot juichen <rhyme label="a" type="f">keeren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En, in Gods naam, de <rhyme label="b" type="m">vaan</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Doen rechten, want uw gansch <rhyme label="a" type="f" corresp="#s3:l1">begeeren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Zal hy vervullen <rhyme label="b" type="m" corresp="#s3:l2">gaan</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Nu weet ik 't, God behiel syn <rhyme label="c" type="f">kooning</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hy liet hem antwoord <rhyme label="d" type="m">hooren</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Syn hand heeft hem, uit 's heemels <rhyme label="c" type="f" corresp="#s3:l5">wooning</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dit heil, door magt, <rhyme label="d" type="m" corresp="#s3:l6">beschooren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dees dorst op waagens ons <rhyme label="a" type="f">trotseeren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die op syn magt te <rhyme label="b" type="m">paard</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Wy meldden van den naam des <rhyme label="a" type="f" corresp="#s4:l1">Heeren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Door onsen God <rhyme label="b" type="m" corresp="#s4:l2">bewaart</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Sy kromden sich, van ons <rhyme label="c" type="f">geslaagen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En vielen op de <rhyme label="d" type="m">velden</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Wy reesen, naa wy schier al <rhyme label="c" type="f" corresp="#s4:l5">laagen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En bleeven staan, als <rhyme label="d" type="m" corresp="#s4:l6">helden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
