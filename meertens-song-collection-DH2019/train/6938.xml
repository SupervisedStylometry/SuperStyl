    <melody>Ik ben tot Amsterdam geweest hu hu</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Willem Dirckszoon Hooft, Jan Jansz. Starter, Jacob Westerbaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">NOch twiffelend schuwe Herderin, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Ghy momt u maer met schyn van min, hu, <rhyme label="a" type="m" corresp="#s1:l1">hu</rhyme>,</l>
        <l id="s1:l3" met="7" type="-+">Met schijn int diepste vant hert ghy staet, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s1:l4" met="7" type="-+">Soo luyt al d'Herders coore praet, sarila, sarila, <rhyme label="b" type="m" corresp="#s1:l3">la</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Drijft ghy met mijn beclach u spot, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">tSoet Fluytken draeyt de quartel sot, hu, <rhyme label="a" type="m" corresp="#s2:l1">hu</rhyme>,</l>
        <l id="s2:l3" met="7" type="-+">Of meynje dat ick veynsen can, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s2:l4" met="7" type="-+">Ia, ick sie u voort een schalck an, sarila, sarila, <rhyme label="b" type="m" corresp="#s2:l3">la</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Mijn suchjens bang gheeft dan gheloof, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Mijn ooren zijn daer voor noch doof, hu, <rhyme label="a" type="m" corresp="#s3:l1">hu</rhyme>,</l>
        <l id="s3:l3" met="7" type="-+">Ghelooft de traentges die ghy siet, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s3:l4" met="7" type="-+">t'Zijn valsche tuyghen k'achtse niet, sarila, sarila, <rhyme label="b" type="m" corresp="#s3:l3">la</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ach deunje met de waerheyt noch, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">De vryers min is meest bedroch, hu, <rhyme label="a" type="m" corresp="#s4:l1">hu</rhyme>,</l>
        <l id="s4:l3" met="7" type="-+">Acht dies te meer mijn gulle trouw, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s4:l4" met="7" type="-+">De wijse vrees hoet nae berou, sarila, sarila, <rhyme label="b" type="m" corresp="#s4:l3">la</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Lust u mijn doodt wreed' Herderin, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Men sterft soo licht niet van de min, hu, <rhyme label="a" type="m" corresp="#s5:l1">hu</rhyme>,</l>
        <l id="s5:l3" met="7" type="-+">Wat claerder blijck begeerdy dan, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s5:l4" met="7" type="-+">Wy spreecken morghen wel daer van, sarila, sarila, <rhyme label="b" type="m" corresp="#s5:l3">la</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Secht my, o Schoonste Nimph cleyn, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Wandelend' in dees weyden, hu, <rhyme label="a" type="m" corresp="#s6:l1">hu</rhyme>,</l>
        <l id="s6:l3" met="7" type="-+">Sal ick u Coridon zijn, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s6:l4" met="7" type="-+">Of sal ick noch langher beyden, sarila, sarila, <rhyme label="b" type="m" corresp="#s6:l3">la</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Eenich verslijt ghy uwen tijdt:, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">Hoedende uwe Schaepen, hu, <rhyme label="a" type="m" corresp="#s7:l1">hu</rhyme>,</l>
        <l id="s7:l3" met="7" type="-+">En laes u Coridon lijt, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s7:l4" met="7" type="-+">Als andere Herders slaepen, sarila, sarila, <rhyme label="b" type="m" corresp="#s7:l3">la</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Neemt my doch aen voor uwen knecht, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">Soo sal ick haest ghenesen, hu, <rhyme label="a" type="m" corresp="#s8:l1">hu</rhyme>,</l>
        <l id="s8:l3" met="7" type="-+">En soo sullen wy te recht, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s8:l4" met="7" type="-+">Door liefde vereenicht wesen, sarila, sarila, <rhyme label="b" type="m" corresp="#s8:l3">la</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">Saecht ghy wel schoone Margriton, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s9:l2" met="5" type="-+">D'andere Herderinnen, hu, <rhyme label="a" type="m" corresp="#s9:l1">hu</rhyme>,</l>
        <l id="s9:l3" met="7" type="-+">Int groene schouwen de Son, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s9:l4" met="7" type="-+">Met Liefkens die sy beminnen, sarila, sarila, <rhyme label="b" type="m" corresp="#s9:l3">la</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="-+">Ick sie ghy nae mijn Schaepkens siet, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s10:l2" met="5" type="-+">Die by de uwe weyden:, hu, <rhyme label="a" type="m" corresp="#s10:l1">hu</rhyme>,</l>
        <l id="s10:l3" met="7" type="-+">Ick bidt u en teltsed doch niet, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s10:l4" met="7" type="-+">Noch wiltse van een niet scheyden, sarila, sarila, <rhyme label="b" type="m" corresp="#s10:l3">la</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="5" type="-+">Laetse by een mijn soete caer, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s11:l2" met="5" type="-+">Soo sal ick vorst der knechten, hu, <rhyme label="a" type="m" corresp="#s11:l1">hu</rhyme>,</l>
        <l id="s11:l3" met="7" type="-+">Ront om u schoone kruyst haer, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s11:l4" met="7" type="-+">Een Kransken van bloemen vlechten, sarila, sarila, <rhyme label="b" type="m" corresp="#s11:l3">la</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="5" type="-+">Soo sullen wy ons jonghe jeucht, hu, <rhyme label="a" type="m">hu</rhyme>,</l>
        <l id="s12:l2" met="5" type="-+">D'een d'ander overgheven, hu, <rhyme label="a" type="m" corresp="#s12:l1">hu</rhyme>,</l>
        <l id="s12:l3" met="7" type="-+">En soo sullen wy met vreucht, sarila, sarila, <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s12:l4" met="7" type="-+">Door Liefde vereenicht wesen, sarila, sarila, <rhyme label="b" type="m" corresp="#s12:l3">la</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
