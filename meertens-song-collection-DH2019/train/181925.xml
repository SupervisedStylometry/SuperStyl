<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181925</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mijn schone herderin</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HElaas wat is de <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wanneer zy in ’t <rhyme label="a" type="m" corresp="#s1:l1">begin</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Een jonkmans hert komt neemen <rhyme label="a" type="m" corresp="#s1:l1">in</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die niet (als een <rhyme label="b" type="m">Soldaat</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zyn vyand buiten <rhyme label="b" type="m" corresp="#s1:l4">staat</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die toont dat hy zyn vyand <rhyme label="b" type="m" corresp="#s1:l4">haat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Helaas! myn eigen <rhyme label="a" type="m">oog</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">Was ’t dat my eerst <rhyme label="a" type="m" corresp="#s2:l1">bewoog</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Te agten ’t minnen Hemel <rhyme label="a" type="m" corresp="#s2:l1">hoog</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Toen ik aanzag een <rhyme label="b" type="m">maagt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Zo schoon de aarde <rhyme label="b" type="m" corresp="#s2:l4">draagt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die heeft myn hert terstond <rhyme label="b" type="m" corresp="#s2:l4">behaagt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zy toonde haar niet <rhyme label="a" type="m">straf</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Nog sloeg my ook niet <rhyme label="a" type="m" corresp="#s3:l1">af</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat myne min couragie <rhyme label="a" type="m" corresp="#s3:l1">gaf</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Maer sy versogt <rhyme label="b" type="m">respyt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Tot op een ander <rhyme label="b" type="m" corresp="#s3:l4">tyd</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Eer sy haer maekte het Jawoord <rhyme label="b" type="m" corresp="#s3:l4">kwyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dus vryde ik jaer en <rhyme label="a" type="m">dag</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat ik haer schoonheid <rhyme label="a" type="m" corresp="#s4:l1">sag</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Sy hoorde beleeft myn min <rhyme label="a" type="m" corresp="#s4:l1">geklag</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Beleeft was ieder <rhyme label="b" type="m">woord</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat van haer wierd <rhyme label="b" type="m" corresp="#s4:l4">gehoord</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Dus heeft sy myn nog meer <rhyme label="b" type="m" corresp="#s4:l4">bekoort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">’t Volmaekte Lief was <rhyme label="a" type="m">lank</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En als de sneeuw soo <rhyme label="a" type="m" corresp="#s5:l1">blank</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Een stem als Nagtegaels <rhyme label="a" type="m" corresp="#s5:l1">gesank</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">’t Was matiglyk <rhyme label="b" type="m">geset</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En fier en trots van <rhyme label="b" type="m" corresp="#s5:l4">tret</rhyme>:</l>
        <l id="s5:l6" met="4" type="-+">Sagjes van vel en poesel <rhyme label="b" type="m" corresp="#s5:l4">vet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Een ieder kaekjen <rhyme label="a" type="m">koos</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Tot eigen kleur en <rhyme label="a" type="m" corresp="#s6:l1">bloos</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Gelijk een aengename <rhyme label="a" type="m" corresp="#s6:l1">Roos</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Haer lipjes als <rhyme label="b" type="m">Korael</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Gelijk een dunne <rhyme label="b" type="m" corresp="#s6:l4">Wal</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Voort witte tantjes schoon van <rhyme label="b" type="m" corresp="#s6:l4">stal</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Een bruin gesigt en <rhyme label="a" type="m">als</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En Lelie wit haer <rhyme label="a" type="m" corresp="#s7:l1">hals</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">En lief van tael met veel <rhyme label="a" type="m" corresp="#s7:l1">geschals</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En boven ieder <rhyme label="b" type="m">oog</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Een smalle zwarte <rhyme label="b" type="m" corresp="#s7:l4">boog</rhyme>:</l>
        <l id="s7:l6" met="4" type="-+">Van voorhooft schoon en magtig <rhyme label="b" type="m" corresp="#s7:l4">hoog</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">En als se een deuntje <rhyme label="a" type="m">kweeld</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Vertoond sy ’t schoonste <rhyme label="a" type="m" corresp="#s8:l1">Beeld</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dat door natuur ooit is <rhyme label="a" type="m" corresp="#s8:l1">Geteeld</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Het scheynd een <rhyme label="b" type="m">Engelin</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ja selfs de Min <rhyme label="b" type="m" corresp="#s8:l4">Godin</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Is niet volmaekter tot de <rhyme label="b" type="m" corresp="#s8:l4">Min</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Na dat ik lange <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Haer schoonheid heb <rhyme label="a" type="m" corresp="#s9:l1">gevryd</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Zoo heeftse my bot <rhyme label="a" type="m" corresp="#s9:l1">afgeseid</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ja met een <rhyme label="b" type="m">vriendelykheid</rhyme>;</l>
        <l id="s9:l5" met="3" type="-+">Die my nog meer <rhyme label="b" type="m" corresp="#s9:l4">verleyd</rhyme>:</l>
        <l id="s9:l6" met="4" type="-+">Maer ach helaes! ik bense <rhyme label="b" type="m" corresp="#s9:l4">kwyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">ô Heerlyk Schepsel <rhyme label="a" type="m">ach</rhyme>!</l>
        <l id="s10:l2" met="3" type="-+">Wat baert gy my <rhyme label="a" type="m" corresp="#s10:l1">geklag</rhyme>!</l>
        <l id="s10:l3" met="4" type="-+">Wyl ik u niet genieten <rhyme label="a" type="m" corresp="#s10:l1">mag</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Wie dat u schoonheid <rhyme label="b" type="m">siet</rhyme>;</l>
        <l id="s10:l5" met="3" type="-+">Geraekt in groot <rhyme label="b" type="m" corresp="#s10:l4">verdriet</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Door dien hy mind en niet <rhyme label="b" type="m" corresp="#s10:l4">geniet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Vaert wel dan schoone <rhyme label="a" type="m">vrouw</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Helaes! ik ben in <rhyme label="a" type="m" corresp="#s11:l1">rouw</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Nogtans ik eerd’ en diend u <rhyme label="a" type="m" corresp="#s11:l1">trouw</rhyme>;</l>
        <l id="s11:l4" met="3" type="-+">Maer ach! wanneer een <rhyme label="b" type="m">maegd</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Een Jonkman niet <rhyme label="b" type="m" corresp="#s11:l4">behaegd</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Dat is een min die word <rhyme label="b" type="m" corresp="#s11:l4">beklaegt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Vaert wel volmaekte <rhyme label="a" type="m">Bloem</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">En aller Maegden <rhyme label="a" type="m" corresp="#s12:l1">roem</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Dat ik Godin der Liefde <rhyme label="a" type="m" corresp="#s12:l1">noem</rhyme>:</l>
        <l id="s12:l4" met="3" type="-+">Onvergelykelyk <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Vaer wel en syt tot <rhyme label="b" type="m" corresp="#s12:l4">loon</rhyme>:</l>
        <l id="s12:l6" met="4" type="-+">Van een veel meer bemint <rhyme label="b" type="m" corresp="#s12:l4">Persoon</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Vaer wel myn <rhyme label="a" type="m">Engelin</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Vaer wel myn <rhyme label="a" type="m" corresp="#s13:l1">Afgodin</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Vaer wel afbeeldsel van de <rhyme label="a" type="m" corresp="#s13:l1">Min</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Vaer wel, vaer eeuwig <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Gy laet my in het <rhyme label="b" type="m" corresp="#s13:l4">gekwel</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Ik blyf in druk hoe dat ik ’t <rhyme label="b" type="m" corresp="#s13:l4">stel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
