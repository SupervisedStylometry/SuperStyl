<song>
  <meta>
    <id>12060</id>
    <date>
      <year-start>1617</year-start>
      <year-end>1617</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>In Jezus' naam broeders eerzaam</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">IN Iesus <rhyme label="a" type="f">name</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Broeders <rhyme label="a" type="f" corresp="#s1:l1">eersame</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Stelt v <rhyme label="b" type="m">eenpaer</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Nu te <rhyme label="c" type="f">beghinnen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Hem te <rhyme label="c" type="f" corresp="#s1:l4">beminnen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Al in dit soete nieuwe <rhyme label="b" type="m" corresp="#s1:l3">Iaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hy heeft <rhyme label="a" type="f">gheleden</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">En is <rhyme label="a" type="f" corresp="#s2:l1">besneden</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Dat’s <rhyme label="b" type="m">openbaer</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Wilt v <rhyme label="c" type="f">besnijden</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Sonder <rhyme label="c" type="f" corresp="#s2:l4">vermijden</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Al in dit soete nieuwe <rhyme label="b" type="m" corresp="#s2:l3">Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Een herte <rhyme label="a" type="f">reene</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">En anders <rhyme label="a" type="f" corresp="#s3:l1">gheene</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Dat sal <rhyme label="b" type="m">voorwaer</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Winnen <rhyme label="c" type="f">alleene</rhyme></l>
        <l id="s3:l5" met="2" type="-+">T’kindeken <rhyme label="c" type="f" corresp="#s3:l4">kleene</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Al in dit soete nieuwe <rhyme label="b" type="m" corresp="#s3:l3">Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Sinnelijck <rhyme label="a" type="f">steruen</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Ghenoechte <rhyme label="a" type="f" corresp="#s4:l1">deruen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Al valt het <rhyme label="b" type="m">swaer</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Het doet <rhyme label="c" type="f">verweruen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">D’hemelsche <rhyme label="c" type="f" corresp="#s4:l4">eruen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Al in dit soete nieuwe <rhyme label="b" type="m" corresp="#s4:l3">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
