  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk weet een schoon <rhyme label="a" type="f">Goddinne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die ick in 't hert <rhyme label="a" type="f" corresp="#s1:l1">beminne</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Veel meer als Juno <rhyme label="b" type="m">self</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Veel meer als Juno <rhyme label="b" type="m" corresp="#s1:l3">self</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Geen Pallas noch geen <rhyme label="c" type="f">Thaetis</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Noch d' Heuysvrouw van <rhyme label="c" type="f" corresp="#s1:l5">Prometis</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Noch geen uyt 't blaeu <rhyme label="b" type="m" corresp="#s1:l3">Gewelf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want mijn Liefs <rhyme label="a" type="f">aerdigheden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En aengename <rhyme label="a" type="f" corresp="#s2:l1">reden</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Haer brave <rhyme label="b" type="m">geestigheydt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Haer brave <rhyme label="b" type="m" corresp="#s2:l3">geestigheydt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Die is hoogh <rhyme label="c" type="f">geresen</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Soo dat haer jeudigh <rhyme label="c" type="f" corresp="#s2:l5">wesen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">'t Hans over al <rhyme label="b" type="m" corresp="#s2:l3">verspreydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick kreegh nu laetst de <rhyme label="a" type="f">tijding'</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Als dat sy tot <rhyme label="a" type="f" corresp="#s3:l1">verblijding'</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van 't Jongh geselschap <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Van 't Jongh geselschap <rhyme label="b" type="m" corresp="#s3:l3">heeft</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Tot singen en tot <rhyme label="c" type="f">Danssen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Op-hangen doen drie <rhyme label="c" type="f" corresp="#s3:l5">kranssen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met vrolijckheyt <rhyme label="b" type="m" corresp="#s3:l3">beleeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een was van Bloem, en <rhyme label="a" type="f">Roosen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Gelijck haer Kaeckjes <rhyme label="a" type="f" corresp="#s4:l1">bloosen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Een van gevlochten <rhyme label="b" type="m">kras</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Een van gevlochten <rhyme label="b" type="m" corresp="#s4:l3">kras</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dat beelden uyt de <rhyme label="c" type="f">kleurtjes</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En lieffelijcke <rhyme label="c" type="f" corresp="#s4:l5">geurtjes</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Van hare Kipjes <rhyme label="b" type="m" corresp="#s4:l3">vars</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Den derden vol <rhyme label="a" type="f">cieraden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die was van groene <rhyme label="a" type="f" corresp="#s5:l1">Bladen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Met verschen douw <rhyme label="b" type="m">besproeyt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met verschen douw <rhyme label="b" type="m" corresp="#s5:l3">besproeyt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Geen Bloempje dit <rhyme label="c" type="f">verschoonde</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Het jeugdigh lof <rhyme label="c" type="f" corresp="#s5:l5">betoonde</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Hoe dat haer jonckheyt <rhyme label="b" type="m" corresp="#s5:l3">bloeyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dus heeftmen daer <rhyme label="a" type="f">gesongen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Gedansen en <rhyme label="a" type="f" corresp="#s6:l1">gesprongen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Tot men de kransjes <rhyme label="b" type="m">won</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Tot men de kransjes <rhyme label="b" type="m" corresp="#s6:l3">won</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Met boertery, en <rhyme label="c" type="f">lachjes</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Sy loofden sulcke <rhyme label="c" type="f" corresp="#s6:l5">Nachjes</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Meer als de klare <rhyme label="b" type="m" corresp="#s6:l3">Son</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ach! Venus; laet u <rhyme label="a" type="f">Soontje</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Als dan het vierde <rhyme label="a" type="f" corresp="#s7:l1">kroontje</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Op-hangen voor ons <rhyme label="b" type="m">twee</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Op-hangen voor ons <rhyme label="b" type="m" corresp="#s7:l3">twee</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">En maeckt my met mijn <rhyme label="c" type="f">Engel</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Een vaste bandt, een <rhyme label="c" type="f" corresp="#s7:l5">stengel</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dit is mijn hoogste <rhyme label="b" type="m" corresp="#s7:l3">bee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
