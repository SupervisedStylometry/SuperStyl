    </categories>
  <author>Philips van Marnix van Sint Aldegonde</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOd sy ons gunstig end <rhyme label="a" type="f">genadig</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">End wil ons seignen al te <rhyme label="b" type="m">gaer</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Hy geef dat over ons <rhyme label="a" type="f" corresp="#s1:l1">gestadig</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Sijn aensicht lichte hel end <rhyme label="b" type="m" corresp="#s1:l2">klaer</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Op dat, Heer, dijn <rhyme label="c" type="f">wegen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Voir recht, end van <rhyme label="c" type="f" corresp="#s1:l5">degen</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Werden hier <rhyme label="d" type="m">bekent</rhyme>:</l>
        <l id="s1:l8" met="3" type="+-">End dat tvolck op <rhyme label="e" type="f">d'eirde</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">dijnen Heyl <rhyme label="e" type="f" corresp="#s1:l8">aenveirde</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Tot aen sweirelts <rhyme label="d" type="m" corresp="#s1:l7">end</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat dy, O God, de volcken <rhyme label="a" type="f">prijsen</rhyme>:</l>
        <l id="s2:l2" met="4" type="-+">Dat alle lien dy seggen <rhyme label="b" type="m">danck</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Dat alle stammen vreugt <rhyme label="a" type="f" corresp="#s2:l1">bewijsen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">End vrolijck singen haer <rhyme label="b" type="m" corresp="#s2:l2">gesanck</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Want du salst tvolck <rhyme label="c" type="f">richten</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Ende dijn rijck <rhyme label="c" type="f" corresp="#s2:l5">stichten</rhyme></l>
        <l id="s2:l7" met="3" type="+-">In <rhyme label="d" type="m">gerechticheyt</rhyme>.</l>
        <l id="s2:l8" met="3" type="+-">Du salst <rhyme label="e" type="f">goedertieren</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Tvolck op d'eirtrijck <rhyme label="e" type="f" corresp="#s2:l8">stieren</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Onder dijn <rhyme label="d" type="m" corresp="#s2:l7">beleyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat dy de volcken, Heer, <rhyme label="a" type="f">lofsingen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dat dy lofsing al watter <rhyme label="b" type="m">leeft</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Het eirtrijck sal sijn vruchten <rhyme label="a" type="f" corresp="#s3:l1">bringen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Want onsen God ons segen <rhyme label="b" type="m" corresp="#s3:l2">geeft</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Ia God sal ons <rhyme label="c" type="f">goedich</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Segen <rhyme label="c" type="f" corresp="#s3:l5">overvloedich</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Geven al te <rhyme label="d" type="m">saem</rhyme>.</l>
        <l id="s3:l8" met="3" type="+-">End alle <rhyme label="e" type="f">geslachten</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Sullen met <rhyme label="e" type="f" corresp="#s3:l8">aendachten</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Vreesen sijnen <rhyme label="d" type="m" corresp="#s3:l7">naem</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
