    <tags>
      <tag standard-name="verhalend lied" translation="narrative song">verhalend lied</tag>
    </tags>
    <melody>Ik stond op hoge bergen</melody>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EN ick soude tot minnen boele <rhyme label="a" type="f">gaen</rhyme>.</l>
        <l id="s1:l2" met="3" type="-+">Den wech valt my so <rhyme label="b" type="m">leyt</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Soo wye den bosse schade <rhyme label="c" type="m">seyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die doet den grauwen ros <rhyme label="b" type="m" corresp="#s1:l2">aen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den sneeu die was <rhyme label="a" type="f">gesmolten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">daer lieper een bosselijn <rhyme label="b" type="m">door</rhyme></l>
        <l id="s2:l3" met="4" type="-+">So wie zijn lief behouden <rhyme label="c" type="m">wil</rhyme></l>
        <l id="s2:l4" met="3" type="-+">die stelle zijn lijf daer <rhyme label="b" type="m" corresp="#s2:l2">voer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ic stont op hooghen <rhyme label="a" type="f">berghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ic sach een steynen <rhyme label="b" type="m">hous</rhyme></l>
        <l id="s3:l3" met="4" type="-+">daer rijden alle <rhyme label="c" type="m">merghen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">drie stoute slumerkens <rhyme label="b" type="m" corresp="#s3:l2">ous</rhyme>. |</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die een dat was een ruyter <rhyme label="a" type="f">stout</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Die ander was een boermans <rhyme label="b" type="m">soon</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Die derde was een lantsknecht <rhyme label="c" type="m">goet</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Hy woude tbruyn mechdelijn <rhyme label="b" type="m" corresp="#s4:l2">haen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn moeder dede mi <rhyme label="a" type="f">vragen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Oft ic den ruyter woude <rhyme label="b" type="m">haen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">So moeste ic desen somer <rhyme label="c" type="m">lanc</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Sijn leersen en sporen doen <rhyme label="b" type="m" corresp="#s5:l2">aen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Mijn moeder dede mi <rhyme label="a" type="f">vragen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">oft ic den keerl woude <rhyme label="b" type="m">haen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">So moeste ic desen somer <rhyme label="c" type="m">lanc</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met zijnen rossen tso velde <rhyme label="b" type="m" corresp="#s6:l2">gaen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn moeder dede mi <rhyme label="a" type="f">vraghen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">oft ic den lantsknecht woude <rhyme label="b" type="m">haen</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">So moeste ic desen somer <rhyme label="c" type="m">lanc</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Metter flesschen so wijne <rhyme label="b" type="m" corresp="#s7:l2">gaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Boeren dat zijn <rhyme label="a" type="f">boeren</rhyme>.</l>
        <l id="s8:l2" met="3" type="-+">Sy drincken so selden den <rhyme label="b" type="m">wijn</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">So | en doet die vrome lansknecht <rhyme label="c" type="m">niet</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Hi schencter so dapperlic <rhyme label="b" type="m" corresp="#s8:l2">in</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Boeren dat zijn <rhyme label="a" type="f">boeren</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Si drincken so selden den <rhyme label="b" type="m">wijn</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Si drincken lieuer <rhyme label="c" type="m">botermelc</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dan den hueschen coelen <rhyme label="b" type="m" corresp="#s9:l2">wijn</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Die dit liedeken <rhyme label="a" type="f">dichte</rhyme></l>
        <l id="s10:l2" met="3" type="-+">dat wasser een lantsknecht <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Sijn herte dat <rhyme label="c" type="m">verlichte</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Tso Heynsberch inden <rhyme label="b" type="m" corresp="#s10:l2">wijn</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Maer diet eerst heeft <rhyme label="a" type="f">ghesongen</rhyme>.</l>
        <l id="s11:l2" met="3" type="-+">Dat wasser een Lantsknecht <rhyme label="b" type="m">goet</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Hi heeft daer aenden dans <rhyme label="c" type="m">geweest</rhyme>.</l>
        <l id="s11:l4" met="3" type="-+">Met ons Keyser dat edel <rhyme label="b" type="m" corresp="#s11:l2">bloet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
