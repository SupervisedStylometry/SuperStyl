<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186878</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Hoe zoet is het daar de vriendschap woont</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">LAet ons, als 't Neerlands <rhyme label="a" type="m">voegt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gezeten in het <rhyme label="b" type="m">rond</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Zyn eensgesind en wel <rhyme label="a" type="m" corresp="#s1:l1">vernoegt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Getrouw met hart en <rhyme label="b" type="m" corresp="#s1:l2">mond</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Getrouw voor de eer van 't <rhyme label="c" type="m">Vaderland</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Vat elk het glas in zyne <rhyme label="c" type="m" corresp="#s1:l5">hand</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En schenk' en schenk' het <rhyme label="d" type="m">vol</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zie daer, myn glaesje vol myn <rhyme label="a" type="m">Wyn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zelfs vol tot aen den <rhyme label="b" type="m">rand</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Weläan 't moet leeg gedronken <rhyme label="a" type="m" corresp="#s2:l1">zyn</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op 't heil van 't <rhyme label="b" type="m" corresp="#s2:l2">Vaderland</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Men roep' men schreeuw', men gill'het <rhyme label="c" type="m">uit</rhyme></l>
        <l id="s2:l6" met="4" type="-+">‘Dat niets de dierbre Welvaert <rhyme label="c" type="m" corresp="#s2:l5">sluit'</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Van dit gezegend <rhyme label="d" type="m">oord</rhyme>!’</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat niets den hechten <rhyme label="a" type="m">Eendragts-band</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Der Batavieren <rhyme label="b" type="m">breek'</rhyme>!</l>
        <l id="s3:l3" met="4" type="-+">ô Neen, zyn knoop houde eeuwig <rhyme label="a" type="m" corresp="#s3:l1">stand</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In Vryheids vaste <rhyme label="b" type="m" corresp="#s3:l2">streek</rhyme>!</l>
        <l id="s3:l5" met="4" type="-+">Elk roept elkander toe in 't <rhyme label="c" type="m">rond</rhyme>:</l>
        <l id="s3:l6" met="4" type="-+">‘Dees zuil, in 's Vad'ren bloed <rhyme label="c" type="m" corresp="#s3:l5">gegrond</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Is Neerlands <rhyme label="d" type="m">Heiligdom</rhyme>!’</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die 't wel meent met dit vry <rhyme label="a" type="m">gewest</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dien zegen groey' staeg <rhyme label="b" type="m">aan</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Die 't kwaad meent haten we als <rhyme label="a" type="m" corresp="#s4:l1">pest</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En moet te gronde <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme>!</l>
        <l id="s4:l5" met="4" type="-+">Die 't minste zaet van tweedragt <rhyme label="c" type="m">zoekt</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Zy van elk Patriot <rhyme label="c" type="m" corresp="#s4:l5">vervloekt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Tot in zyn <rhyme label="d" type="m">Nageslagt</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Schenkt in, schenk in, ô <rhyme label="a" type="m">vrindenry</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Schenk, Patriotten! <rhyme label="b" type="m">schenk</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Schenk nu deez' groten Beker <rhyme label="a" type="m" corresp="#s5:l1">vry</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Gantsch vol, wyl Vryheid <rhyme label="b" type="m" corresp="#s5:l2">wenkt</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">‘Drink uit, (dusspreek ze,) ô <rhyme label="c" type="m">Heldenstoet</rhyme>.</l>
        <l id="s5:l6" met="4" type="-+">‘Men wensch' dat Bato's <rhyme label="c" type="m" corresp="#s5:l5">heldenmoed</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Op nieuws met glorie <rhyme label="d" type="m">pryk'</rhyme>!’</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Drygt ons 't ondankbaer <rhyme label="a" type="m">Engeland</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Wat nood daer Eendragt <rhyme label="b" type="m">woont</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Word hem eerlang haar strydbre <rhyme label="a" type="m" corresp="#s6:l1">hand</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Te lang getergt, <rhyme label="b" type="m" corresp="#s6:l2">getoont</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Het blyk', die 't wel met Neêrland <rhyme label="c" type="m">meent</rhyme>.</l>
        <l id="s6:l6" met="4" type="-+">Houd zich met Neêrland lot <rhyme label="c" type="m" corresp="#s6:l5">verëent</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">En sterft of <rhyme label="d" type="m">overwint</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nog eens; Schenk in, voor 't <rhyme label="a" type="m">Vaderland</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Men drink' der Staten <rhyme label="b" type="m">heil</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Men wensch' hun wysheid en <rhyme label="a" type="m" corresp="#s7:l1">verstand</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En zegen boven 't <rhyme label="b" type="m" corresp="#s7:l2">peil</rhyme>!</l>
        <l id="s7:l5" met="4" type="-+">Men zweer' (deez Eed zy nooit <rhyme label="c" type="m">ontzeid</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">‘Wy houden 't met onze <rhyme label="c" type="m" corresp="#s7:l5">ovrigheid</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Tot aen den laetsten <rhyme label="d" type="m">snik</rhyme>!’</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Kom, volgt ons Bato's edel <rhyme label="a" type="m">zaet</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">In dit verpligt <rhyme label="b" type="m">besluit</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Op dezen gouden harnas <rhyme label="a" type="m" corresp="#s8:l1">plaet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Word Englands trots <rhyme label="b" type="m" corresp="#s8:l2">gestuit</rhyme>;</l>
        <l id="s8:l5" met="4" type="-+">Ga, vraeg 't bewys aan Spanje <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Ze ontrok ons van zyn <rhyme label="c" type="m" corresp="#s8:l5">dwinglandy</rhyme>;</l>
        <l id="s8:l7" met="3" type="-+">Beef, England, voor haar <rhyme label="d" type="m">kragt</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
