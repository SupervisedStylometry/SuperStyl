  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VReugde schep ik Venus <rhyme label="a" type="f">wigje</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Nu gy met uw boog en <rhyme label="a" type="f" corresp="#s1:l1">Schigje</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Hebt getroffen 't fiere <rhyme label="b" type="m">Hart</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Van haer die myn <rhyme label="c" type="f">Ingewanden</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Set in <rhyme label="d" type="m">Vlam</rhyme>:</l>
        <l id="s1:l6" met="2" type="+-">Toen ik 't <rhyme label="d" type="m" corresp="#s1:l5">Lam</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Onlangs in myn armen <rhyme label="d" type="m" corresp="#s1:l5">nam</rhyme>.</l>
        <l id="s1:l8" met="2" type="+-">Set in <rhyme label="d" type="m" corresp="#s1:l5">Vlam</rhyme>:</l>
        <l id="s1:l9" met="2" type="+-">Toen ik 't <rhyme label="d" type="m" corresp="#s1:l5">Lam</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Onlangs in myn armen <rhyme label="d" type="m" corresp="#s1:l5">nam</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Kleynen Schutter, <rhyme label="a" type="f">Minnekweker</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Leven wekker, Harten <rhyme label="a" type="f" corresp="#s2:l1">breker</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Die 't heel al door Min <rhyme label="b" type="m">regeerd</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Gehebt het hart dan <rhyme label="c" type="f">overwonnen</rhyme></l>
        <l id="s2:l5" met="2" type="+-">Dat myn <rhyme label="d" type="m">Ziel</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Dus <rhyme label="d" type="m" corresp="#s2:l5">beviel</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Toen ik ze in myn armen <rhyme label="d" type="m" corresp="#s2:l5">hiel</rhyme>.</l>
        <l id="s2:l8" met="2" type="+-">Dat myn <rhyme label="d" type="m" corresp="#s2:l5">Ziel</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Dus <rhyme label="d" type="m" corresp="#s2:l5">beviel</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Toen ik ze in myn armen <rhyme label="d" type="m" corresp="#s2:l5">hiel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laet my nimmermeer <rhyme label="a" type="f">verlegen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wen ik op het dons <rhyme label="a" type="f" corresp="#s3:l1">gezegen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">In Climenes Boutjes <rhyme label="b" type="m">rust</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Wyl ik met <rhyme label="c" type="f">Eerbiedigheden</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Voor u <rhyme label="d" type="m">buk</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Gun my 't <rhyme label="d" type="m" corresp="#s3:l5">luk</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Dat ik ze in myn Armpjes <rhyme label="d" type="m" corresp="#s3:l5">druk</rhyme>.</l>
        <l id="s3:l8" met="2" type="+-">Voor u <rhyme label="d" type="m" corresp="#s3:l5">buk</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Gun my 't <rhyme label="d" type="m" corresp="#s3:l5">luk</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Dat ik ze in myn Armpjes <rhyme label="d" type="m" corresp="#s3:l5">druk</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Zal nog Mirth, nog Bloempjes <rhyme label="a" type="f">sparen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Om te Offeren op de <rhyme label="a" type="f" corresp="#s4:l1">Altaren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die de Min zyn toe <rhyme label="b" type="m">gewyd</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Maer in 't Lief gestreel <rhyme label="c" type="f">volherden</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Dat myn <rhyme label="d" type="m">Lam</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Zet in <rhyme label="d" type="m" corresp="#s4:l5">Vlam</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">Als ik ze in myn armen <rhyme label="d" type="m" corresp="#s4:l5">nam</rhyme>.</l>
        <l id="s4:l8" met="2" type="+-">Dat myn <rhyme label="d" type="m" corresp="#s4:l5">Lam</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Zet in <rhyme label="d" type="m" corresp="#s4:l5">Vlam</rhyme>;</l>
        <l id="s4:l10" met="4" type="+-">Als ik ze in myn armen <rhyme label="d" type="m" corresp="#s4:l5">nam</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
