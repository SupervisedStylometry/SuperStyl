    <melody>Psalm 033 Datheen</melody>
  <author>François Halma</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Godt, onze Godt, zy ons <rhyme label="a" type="f">genadig</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En zegene ons met <rhyme label="b" type="m">overvloedt</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Zyn aanschyn straale op ons <rhyme label="a" type="f" corresp="#s1:l1">gestadig</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En schiete licht in ons <rhyme label="b" type="m" corresp="#s1:l2">gemoedt</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Opdat d'aarde u <rhyme label="c" type="f">kenne</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Elk het hart <rhyme label="c" type="f" corresp="#s1:l5">gewenne</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Aan uw' weg <rhyme label="d" type="m">rontom</rhyme>.</l>
        <l id="s1:l8" met="3" type="+-">'t Heil tot aan de <rhyme label="e" type="f">wolken</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Voor al 's wereldts <rhyme label="e" type="f" corresp="#s1:l8">volken</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Ryze in 't <rhyme label="d" type="m" corresp="#s1:l7">heidendom</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De volken zullen u dan <rhyme label="a" type="f">looven</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">O Godt, de volken <rhyme label="b" type="m">altemaal</rhyme></l>
        <l id="s2:l3" met="4" type="-+">U pryzen, om uw heil van <rhyme label="a" type="f" corresp="#s2:l1">boven</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En juichen om het bly <rhyme label="b" type="m" corresp="#s2:l2">verhaal</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Blyschap zullenze <rhyme label="c" type="f">uiten</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Als gy 't heil <rhyme label="c" type="f" corresp="#s2:l5">ontsluiten</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Zult, en 't aardtryk <rhyme label="d" type="m">richt</rhyme></l>
        <l id="s2:l8" met="3" type="+-">In <rhyme label="e" type="f">rechtmatigheden</rhyme>.</l>
        <l id="s2:l9" met="3" type="+-">'t Volk van hier <rhyme label="e" type="f" corresp="#s2:l8">beneden</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Leidtge in 't zalig <rhyme label="d" type="m" corresp="#s2:l7">licht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De volken zullen u dan <rhyme label="a" type="f">pryzen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">O Godt, de volken <rhyme label="b" type="m">altemaal</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Hun lof zal aan de wolken <rhyme label="a" type="f" corresp="#s3:l1">ryzen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Als 't aardryk, onbebouwt en <rhyme label="b" type="m" corresp="#s3:l2">schraal</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Zyn gewas zal <rhyme label="c" type="f">geeven</rhyme>.</l>
        <l id="s3:l6" met="3" type="+-">Godt schenkt ons het <rhyme label="c" type="f" corresp="#s3:l5">leven</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">En zendt zegen <rhyme label="d" type="m">neêr</rhyme>:</l>
        <l id="s3:l8" met="3" type="+-">Al des wereldts <rhyme label="e" type="f">enden</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Zullen zich ook <rhyme label="e" type="f" corresp="#s3:l8">wenden</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Tot zyn vreeze en <rhyme label="d" type="m" corresp="#s3:l7">leer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
