<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156090</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Om een nieuw jaar zij haar man bad</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALs Godt sijn milde handt op <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Om ons hier te <rhyme label="b" type="f">versaden</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">En wy des zegens <rhyme label="a" type="m" corresp="#s1:l1">overvloet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Genieten uyt <rhyme label="b" type="f" corresp="#s1:l2">genaden</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Als hy ons geeft Broodt, Oly, <rhyme label="c" type="m">Wijn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Oock Wol en Vlas tot <rhyme label="d" type="f">Kleeden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Goudt, Silver <rhyme label="c" type="m" corresp="#s1:l5">fijn</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Wie van ons al soud hier <rhyme label="d" type="f" corresp="#s1:l6">beneden</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Niet danckbaer <rhyme label="c" type="m" corresp="#s1:l5">zijn</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maer neen, dit raeckt ons niet aen 't <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wy achten geen <rhyme label="b" type="f">weldaden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Of men van Godt gezegent <rhyme label="a" type="m" corresp="#s2:l1">wert</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Het loon is boos ten <rhyme label="b" type="f" corresp="#s2:l2">quaden</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Men vindt nauw een, wiens vroom <rhyme label="c" type="m">gemoet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Kan ware danck <rhyme label="d" type="f">betuygen</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">Wat Godt ons <rhyme label="c" type="m" corresp="#s2:l5">doet</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Men wil sijn stijve neck niet <rhyme label="d" type="f" corresp="#s2:l6">buygen</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Tot danck voor 't <rhyme label="c" type="m" corresp="#s2:l5">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dies komt sijn Oordeel op ons <rhyme label="a" type="m">an</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Om dat wy hem <rhyme label="b" type="f">verlaten</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Is 't wonder? Neen 't: want seeker <rhyme label="a" type="m" corresp="#s3:l1">kan</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ons doen sijn staet niet <rhyme label="b" type="f" corresp="#s3:l2">baten</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Het Iaer van tseventigh tuyght <rhyme label="c" type="m">klaer</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En Sesthien-hondert <rhyme label="d" type="f">even</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Dat Godts handt <rhyme label="c" type="m" corresp="#s3:l5">swaer</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Om ons te straffen <rhyme label="d" type="f" corresp="#s3:l6">op-geheven</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Al vaerdigh <rhyme label="c" type="m" corresp="#s3:l5">waer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dit druckt ons lieve <rhyme label="a" type="m">Vaderlandt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Enschuysen voelt dees <rhyme label="b" type="f">neepen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Doen sachmen Zeylen langhs haer <rhyme label="a" type="m" corresp="#s4:l1">strant</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Twee hondert tachtigh <rhyme label="b" type="f" corresp="#s4:l2">Schepen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Die alle wel versien, <rhyme label="c" type="m">bemant</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Te samen seer <rhyme label="d" type="f">verlangen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Om met het <rhyme label="c" type="m" corresp="#s4:l5">Want</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Haer Ruym met Haringh vol te <rhyme label="d" type="f" corresp="#s4:l6">vangen</rhyme></l>
        <l id="s4:l9" met="2" type="-+">By 't <rhyme label="c" type="m" corresp="#s4:l5">Hitse-lant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dies yeder kloeck is in de <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sy trecken al aen 't <rhyme label="b" type="f">Vissen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Maer och der aller Vissen <rhyme label="a" type="m" corresp="#s5:l1">Heer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Doet haren handel <rhyme label="b" type="f" corresp="#s5:l2">missen</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Daer is geen Haringh in het <rhyme label="c" type="m">Net</rhyme>;</l>
        <l id="s5:l6" met="3" type="-+">Men hoort een droevigh <rhyme label="d" type="f">klagen</rhyme>;</l>
        <l id="s5:l7" met="2" type="-+">Van waer komt <rhyme label="c" type="m" corresp="#s5:l5">het</rhyme>?</l>
        <l id="s5:l8" met="4" type="-+">Is dit oock een van Godes <rhyme label="d" type="f" corresp="#s5:l6">plagen</rhyme>?</l>
        <l id="s5:l9" met="2" type="-+">En staen <rhyme label="c" type="m" corresp="#s5:l5">verset</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De eene wil het om de <rhyme label="a" type="m">Noordt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En dicht by Fulo <rhyme label="b" type="f">wagen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Een aer zeylt Boeckenes <rhyme label="a" type="m" corresp="#s6:l1">aen-boort</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat kan hem best <rhyme label="b" type="f" corresp="#s6:l2">behagen</rhyme>;</l>
        <l id="s6:l5" met="4" type="-+">Een ander 't Oostwaert <rhyme label="c" type="m">over-leyt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Men meynt ten sal niet <rhyme label="d" type="f">missen</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Maer hoe <rhyme label="c" type="m" corresp="#s6:l5">verspreyt</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Haer Want en kan geen Haringh <rhyme label="d" type="f" corresp="#s6:l6">vissen</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">In 't kort <rhyme label="c" type="m" corresp="#s6:l5">geseyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">De Iagers vry een groot <rhyme label="a" type="m">getal</rhyme></l>
        <l id="s7:l2" met="3" type="-+">By nae meest leedigh <rhyme label="b" type="f">komen</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Daernae komt Buys-man aen ons <rhyme label="a" type="m" corresp="#s7:l1">Wal</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Bedroeft en vol van <rhyme label="b" type="f" corresp="#s7:l2">schroomen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Krusy en September sijn <rhyme label="c" type="m">voorby</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En niet of wat <rhyme label="d" type="f">gevangen</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Dees <rhyme label="c" type="m" corresp="#s7:l5">Visschery</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Daer veel nae plachten te <rhyme label="d" type="f" corresp="#s7:l6">verlangen</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">Leyt nu aen <rhyme label="c" type="m" corresp="#s7:l5">Ly</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De hoogst heeft vijf ses last <rhyme label="a" type="m">gevaen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">De meeste soo veel <rhyme label="b" type="f">Tonnen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Voorts is de rest soo slecht <rhyme label="a" type="m" corresp="#s8:l1">gelaen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Of daer noyt was <rhyme label="b" type="f" corresp="#s8:l2">begonnen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Wie sach de Vloot soo komen <rhyme label="c" type="m">an</rhyme>?</l>
        <l id="s8:l6" met="3" type="-+">Vraeghd dat u Vriendt en <rhyme label="d" type="f">Macker</rhyme>:</l>
        <l id="s8:l7" met="2" type="-+">Och niet een <rhyme label="c" type="m" corresp="#s8:l5">Man</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Hoe oudt, hoe koudt, hoe loom, hoe <rhyme label="d" type="f" corresp="#s8:l6">wacker</rhyme>,</l>
        <l id="s8:l9" met="2" type="-+">Die 't heugen <rhyme label="c" type="m" corresp="#s8:l5">kan</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Och Heere Godt, heb doch <rhyme label="a" type="m">gedult</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Vergeef ons ons' <rhyme label="b" type="f">misdaden</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Wy seggen, 't is ons aller <rhyme label="a" type="m" corresp="#s9:l1">schult</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Betoon ons u <rhyme label="b" type="f" corresp="#s9:l2">genaden</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Wy willen van het quaet <rhyme label="c" type="m">af-staen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En beter gangen <rhyme label="d" type="f">maken</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Houd op van <rhyme label="c" type="m" corresp="#s9:l5">slaen</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Wil in Diep-water by haer <rhyme label="d" type="f" corresp="#s9:l6">waken</rhyme></l>
        <l id="s9:l9" met="2" type="-+">Dan sal' t noch <rhyme label="c" type="m" corresp="#s9:l5">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
