<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>114920</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Schoon lief wil mij troost geven</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL die hier sijn <rhyme label="a" type="f">verscheenen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En oock genoot voor <rhyme label="a" type="f" corresp="#s1:l1">heenen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van 't Nieu-getroude <rhyme label="b" type="m">Paer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die sal ick niet <rhyme label="c" type="f">vergeten</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Om wellekoom te <rhyme label="c" type="f" corresp="#s1:l4">heten</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In liefde met <rhyme label="b" type="m" corresp="#s1:l3">malkaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ons Bruydegom <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En oock sijn Bruydt daer <rhyme label="a" type="f" corresp="#s2:l1">neven</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die bidden u te <rhyme label="b" type="m">saem</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Om niet te leggen <rhyme label="c" type="f">tesen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Maer vrolijck hier te <rhyme label="c" type="f" corresp="#s2:l4">wesen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat is haer <rhyme label="b" type="m" corresp="#s2:l3">aenenaem</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Kom wilje t'saem <rhyme label="a" type="f">vergaren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En aen de Tafel <rhyme label="a" type="f" corresp="#s3:l1">paren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">In 't midden moet de <rhyme label="b" type="m">Bruyt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De Bruydegom daer <rhyme label="c" type="f">over</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Al valt sijn stem wat <rhyme label="c" type="f" corresp="#s3:l4">grover</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hy mach het Glaesjen <rhyme label="b" type="m" corresp="#s3:l3">uyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wel is nu elck <rhyme label="a" type="f">geseten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Soo wil dan niet <rhyme label="a" type="f" corresp="#s4:l1">vergeten</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Te bidden onsen <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat hy wil door sijn <rhyme label="c" type="f">gaven</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ons dorst en honger <rhyme label="c" type="f" corresp="#s4:l4">laven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Want dat is sijn <rhyme label="b" type="m" corresp="#s4:l3">Gebodt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu Vrienden, treckt aen 't <rhyme label="a" type="f">Kluyven</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sie daer staen Hoenders, <rhyme label="a" type="f" corresp="#s5:l1">Duyven</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En daer een vette <rhyme label="b" type="m">Bout</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Of hebje lieft <rhyme label="c" type="f">Konijnen</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">Daer staense, wiltse <rhyme label="c" type="f" corresp="#s5:l4">mijnen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Al eer sy worden <rhyme label="b" type="m" corresp="#s5:l3">kout</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Is u de Harst noch <rhyme label="a" type="f">weerder</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Oft Varckens-bil <rhyme label="a" type="f" corresp="#s6:l1">begeerder</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Tast toe, ey zit niet <rhyme label="b" type="m">stil</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En brenght daer op een <rhyme label="c" type="f">Glaesjen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Aen Trijntjen of aen <rhyme label="c" type="f" corresp="#s6:l4">Claesjen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Het is haer beyder <rhyme label="b" type="m" corresp="#s6:l3">wil</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nu Vryers eens <rhyme label="a" type="f">gedroncken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">U keeltjen wilt <rhyme label="a" type="f" corresp="#s7:l1">ontfoncken</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Ter eeren van dit <rhyme label="b" type="m">Feest</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Hoe zit ghy dus de <rhyme label="c" type="f">dralen</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">De Wijn die sal <rhyme label="c" type="f" corresp="#s7:l4">verschalen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Maeckt een verheughden <rhyme label="b" type="m" corresp="#s7:l3">geest</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hebt ghy nu wel <rhyme label="a" type="f">gegeten</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Wilt Godt dan niet <rhyme label="a" type="f" corresp="#s8:l1">vergeten</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">De Gever van dit <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Wil hem een Gracy <rhyme label="c" type="f">singen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En dan eens <rhyme label="c" type="f" corresp="#s8:l4">omme-springen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Al eer men scheyden <rhyme label="b" type="m" corresp="#s8:l3">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
