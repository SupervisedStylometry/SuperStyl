<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4256</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="afscheidslied" translation="farewell song">afscheidslied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Engelse fortuin</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="occasions">gelegenheden</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">EErbare, soete // <rhyme label="a" type="m">ieught</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Bloemken weert alder <rhyme label="b" type="f">eeren</rhyme>:</l>
        <l id="s1:l3" met="3" type="+-">Dat u ontmoete // <rhyme label="a" type="m" corresp="#s1:l1">vreught</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wensch' ick, int <rhyme label="b" type="f" corresp="#s1:l2">salueeren</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Ghy zijt <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Een Peerle onder <rhyme label="d" type="f">vele</rhyme>:</l>
        <l id="s1:l7" met="3" type="+-">T'elcken avont <rhyme label="d" type="f" corresp="#s1:l6">spele</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Is uwen naem <rhyme label="c" type="m" corresp="#s1:l5">lofbaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Laet my dijn name // <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Vol prijsen naer haer <rhyme label="b" type="f">weerde</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Siende bequame // <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Hoe ghy betreet de <rhyme label="b" type="f" corresp="#s2:l2">eerde</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Seer <rhyme label="c" type="m">minnelick</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Moet mijnen geest <rhyme label="d" type="f">ontspringen</rhyme>:</l>
        <l id="s2:l7" met="3" type="+-">Ghy zijt in u <rhyme label="d" type="f" corresp="#s2:l6">dingen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Volmaect, <rhyme label="c" type="m" corresp="#s2:l5">godinnelick</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Ionge volmaecte // <rhyme label="a" type="m">bruyt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Beleeft in u <rhyme label="b" type="f">faconde</rhyme>&lt;</l>
        <l id="s3:l3" met="3" type="+-">Noyt man en smaecte // <rhyme label="a" type="m" corresp="#s3:l1">fruyt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Soo soet, tot zijn <rhyme label="b" type="f" corresp="#s3:l2">gesonde</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Veel smerte // <rhyme label="c" type="m">dect</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ghy, deur troostbaer <rhyme label="d" type="f">genesen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">V amoureus <rhyme label="d" type="f" corresp="#s3:l6">wesen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Liefs hert aen t'herte // <rhyme label="c" type="m" corresp="#s3:l5">trect</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Siende dijn wangen // <rhyme label="a" type="m">root</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Int herte ick <rhyme label="b" type="f">verschiete</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">Ghy sult den strangen // <rhyme label="a" type="m" corresp="#s4:l1">noot</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dijns liefs, haest doen te <rhyme label="b" type="f" corresp="#s4:l2">niete</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Als hy sulcx <rhyme label="c" type="m">merct</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ghy bluscht hem al zijn <rhyme label="d" type="f">pijne</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Soo de <rhyme label="d" type="f" corresp="#s4:l6">medecijne</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Een crancke siele <rhyme label="c" type="m" corresp="#s4:l5">sterct</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Acht dit de waerheyt // <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Noch meer mocht hier <rhyme label="b" type="f">ontbreken</rhyme>:</l>
        <l id="s5:l3" met="3" type="+-">Niemant de claerheyt // <rhyme label="a" type="m" corresp="#s5:l1">sal</rhyme></l>
        <l id="s5:l4" met="3" type="-+">(Dijns lofs) geheel <rhyme label="b" type="f" corresp="#s5:l2">wtspreken</rhyme>:</l>
        <l id="s5:l5" met="2" type="-+">Doch euen <rhyme label="c" type="m">wel</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Al zijt ghy weert de <rhyme label="d" type="f">croone</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Maeghden fris, en <rhyme label="d" type="f" corresp="#s5:l6">schoone</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Zijn meest haer lief <rhyme label="c" type="m" corresp="#s5:l5">rebel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Bloemkens van geure // <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Connen veel schoonheyts <rhyme label="b" type="f">dragen</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Maer u coleure // <rhyme label="a" type="m" corresp="#s6:l1">moet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">My bouen al <rhyme label="b" type="f" corresp="#s6:l2">behagen</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Vaste <rhyme label="c" type="m">pilaer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Manierich, ende <rhyme label="d" type="f">stille</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Coemt u liefkens <rhyme label="d" type="f" corresp="#s6:l6">wille</rhyme></l>
        <l id="s6:l8" met="3" type="-+">In alle heusheyt <rhyme label="c" type="m" corresp="#s6:l5">naer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Es t'uwen houe // <rhyme label="a" type="m">rijck</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Aengenaem dit <rhyme label="b" type="f">gedichte</rhyme>:</l>
        <l id="s7:l3" met="3" type="+-">Soo ist wel loue // <rhyme label="a" type="m" corresp="#s7:l1">lijck</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dat ick een schoon <rhyme label="b" type="f" corresp="#s7:l2">gesichte</rhyme></l>
        <l id="s7:l5" met="2" type="-+">(Deur dit cleyn <rhyme label="c" type="m">werck</rhyme>)</l>
        <l id="s7:l6" met="3" type="-+">Int minste mach <rhyme label="d" type="f">believen</rhyme>:</l>
        <l id="s7:l7" met="3" type="+-">Ick sondt anders <rhyme label="d" type="f" corresp="#s7:l6">brieven</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Waer ick ter cunsten <rhyme label="c" type="m" corresp="#s7:l5">sterck</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">T'is tijt om scheyden // <rhyme label="a" type="m">nu</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dits dan mijn <rhyme label="b" type="f">supplieeren</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">God wil geleyden // <rhyme label="a" type="m" corresp="#s8:l1">u</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Tot dat wy <rhyme label="b" type="f" corresp="#s8:l2">wederkeeren</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Princesse <rhyme label="c" type="m">net</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Wilt ons vrientschap niet <rhyme label="d" type="f">mincken</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Alst u sal <rhyme label="d" type="f" corresp="#s8:l6">gedincken</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Peynst om my <rhyme label="c" type="m" corresp="#s8:l5">altemet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
