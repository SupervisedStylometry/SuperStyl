  <author>Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HAd Phar'o staegh den raet des <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghevolght, en <rhyme label="b" type="m">toeghestaen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hy hadd' de straf wel moghen <rhyme label="a" type="f" corresp="#s1:l1">keeren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En van sen hals <rhyme label="b" type="m" corresp="#s1:l2">ontslaen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Maer door sijn ongehoorsaem <rhyme label="c" type="f">streven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En hartneckigh <rhyme label="d" type="m">ghebaer</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Heeft Godt hem oock straf loon <rhyme label="c" type="f" corresp="#s1:l5">gegeven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En joegh hem vol <rhyme label="d" type="m" corresp="#s1:l6">ghevaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maar 't Heydens volck, door Jonas <rhyme label="a" type="f">preken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En Goddelijcken <rhyme label="b" type="m">raedt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En lieten geen gevolgh <rhyme label="a" type="f" corresp="#s2:l1">ontbreken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maer deden 't metter <rhyme label="b" type="m" corresp="#s2:l2">daedt</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">De straf verswint, sy zijn <rhyme label="c" type="f">behouwen</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Goen raedt ghedaen, baert <rhyme label="d" type="m">goedt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Bysonder die 't op Godt <rhyme label="c" type="f" corresp="#s2:l5">betrouwen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat hy 't voordraghen <rhyme label="d" type="m" corresp="#s2:l6">doet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Goe'n raet weeght swaer, als duyrbre <rhyme label="a" type="f">schatten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">S' is waerdigher dan <rhyme label="b" type="m">Goudt</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Die goeden raedt wel spits <rhyme label="a" type="f" corresp="#s3:l1">bevatten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Waer op hy is <rhyme label="b" type="m" corresp="#s3:l2">ghebout</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Sal vinden dat hy heyl doet <rhyme label="c" type="f">schaffen</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Maer die den raedt <rhyme label="d" type="m">veracht</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Voelt ende heeft ontelbre <rhyme label="c" type="f" corresp="#s3:l5">straffen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Op synen neck <rhyme label="d" type="m" corresp="#s3:l6">gebracht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
