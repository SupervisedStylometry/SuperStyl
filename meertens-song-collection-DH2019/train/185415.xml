  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Welkoom! Welkoom! lieve <rhyme label="a" type="f">vrienden</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Die zig hier ter Bruiloft <rhyme label="a" type="f" corresp="#s1:l1">vinden</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Juich / en zing met blyde <rhyme label="b" type="m">geest</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">Danst / en spring op 't <rhyme label="b" type="m" corresp="#s1:l3">Huwelyksfeest</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Kom! de glaasjes vol <rhyme label="c" type="f">geschonken</rhyme> /</l>
        <l id="s1:l6" met="4" type="+-">En de kelken uit <rhyme label="c" type="f" corresp="#s1:l5">gedronken</rhyme> /</l>
        <l id="s1:l7" met="4" type="+-">Juich / en zing met blyde <rhyme label="b" type="m" corresp="#s1:l3">geest</rhyme>!</l>
        <l id="s1:l8" met="4" type="+-">Danst / en springt op 't Huwelyks <rhyme label="b" type="m" corresp="#s1:l3">feest</rhyme>.</l>
      </lg>
