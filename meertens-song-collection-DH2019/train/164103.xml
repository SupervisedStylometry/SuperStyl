<song>
  <meta>
    <id>164103</id>
    <date>
      <year-start>1730</year-start>
      <year-end>1730</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatstmaal ging ik ter jacht</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WEl dus bedroeft <rhyme label="a" type="m">Ionckvrouw</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn Liefste lief <rhyme label="b" type="f">verheven</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Waer toe so grooten <rhyme label="a" type="m" corresp="#s1:l1">rouw</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In 't bloeyen van u <rhyme label="b" type="f" corresp="#s1:l2">leven</rhyme>?</l>
        <l id="s1:l5" met="4" type="-+">Hoe komt dat men u oogen <rhyme label="c" type="m">siet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Met traentjes nat, door groot <rhyme label="c" type="m" corresp="#s1:l5">verdriet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wat leet is u <rhyme label="c" type="m" corresp="#s1:l5">geschiet</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ionghman vraeght gy noch <rhyme label="a" type="m">dat</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wat leedt dat my komt <rhyme label="b" type="f">quellen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Gy vraeght na 't kundigh <rhyme label="a" type="m" corresp="#s2:l1">padt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Als veel loose <rhyme label="b" type="f" corresp="#s2:l2">gesellen</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Of gy my schoon voor oogen <rhyme label="c" type="m">vleyt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Gy hebt acht mijn rugh <rhyme label="c" type="m" corresp="#s2:l5">geseyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ick spotte met de <rhyme label="c" type="m" corresp="#s2:l5">Meyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ionckvrouw dat 's <rhyme label="a" type="m">klapperny</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Mijn hert dat is <rhyme label="b" type="f">geloogen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Mijn lief gelooft doch <rhyme label="a" type="m" corresp="#s3:l1">mij</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wilt u traentjes af <rhyme label="b" type="f" corresp="#s3:l2">droogen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Mijn komst geschiet op trouw en <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Gelooft de klappers <rhyme label="c" type="m" corresp="#s3:l5">nimmermeer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die ons benijden <rhyme label="c" type="m" corresp="#s3:l5">seer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ionghman uw woorden <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ontraden my het <rhyme label="b" type="f">sorgen</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Dochter of uw hert is <rhyme label="a" type="m" corresp="#s4:l1">goet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat is voor my <rhyme label="b" type="f" corresp="#s4:l2">verborgen</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Doch so gy my bemint <rhyme label="c" type="m">Ionghman</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">So toon daer een merck-teeken <rhyme label="c" type="m" corresp="#s4:l5">van</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ick ben te vreeden <rhyme label="c" type="m" corresp="#s4:l5">dan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Om dat gy brave <rhyme label="a" type="m">vrouw</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Niet meer behoeft te <rhyme label="b" type="f">vreesen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Moet gy van mijne <rhyme label="a" type="m" corresp="#s5:l1">Trouw</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Terstont verseeckert <rhyme label="b" type="f" corresp="#s5:l2">weesen</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Daer is mijn hant mijn Lief, mijn <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Op trouw wort gy van mijn <rhyme label="c" type="m" corresp="#s5:l5">gekust</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Mijn Engel zijt <rhyme label="c" type="m" corresp="#s5:l5">gerust</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ionghman nu is mijn <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van alle druck <rhyme label="b" type="f">ontslagen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ick ben bevrijdt van <rhyme label="a" type="m" corresp="#s6:l1">smert</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Adieu bedroefde <rhyme label="b" type="f" corresp="#s6:l2">dagen</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Al wat voor desen is <rhyme label="c" type="m">geschiet</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Dat ick uyt liefde u toe <rhyme label="c" type="m" corresp="#s6:l5">liet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">En roert de klappers <rhyme label="c" type="m" corresp="#s6:l5">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
