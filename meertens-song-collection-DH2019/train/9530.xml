    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Als ik uit wandelen ga</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrick Jansz. Prins</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Bruyt zijt wel <rhyme label="a" type="m">gherust</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Want uw' verlanghens <rhyme label="a" type="m" corresp="#s1:l1">lust</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Mijn Suster, wort nu <rhyme label="a" type="m" corresp="#s1:l1">uytgeblust</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Siet ick ben in mijn Hof <rhyme label="b" type="m">gheruckt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ick heb de Mirh <rhyme label="b" type="m" corresp="#s1:l4">ghepluckt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Het isser alles wel <rhyme label="b" type="m" corresp="#s1:l4">gheluckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick at den <rhyme label="a" type="m">Honinghraet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Van u ghesonde <rhyme label="a" type="m" corresp="#s2:l1">praet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Van u devoir en goede <rhyme label="a" type="m" corresp="#s2:l1">daet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">De soete Melck en klare <rhyme label="b" type="m">Wijn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Van u verblijdt <rhyme label="b" type="m" corresp="#s2:l4">aenschijn</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Met vreughd' van my gedroncken <rhyme label="b" type="m" corresp="#s2:l4">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy Vrienden <rhyme label="a" type="m">allegaer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'t Zy mensch, of <rhyme label="a" type="m" corresp="#s3:l1">Engel-schaer</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Geniet met my de vreuchd' <rhyme label="a" type="m" corresp="#s3:l1">eenpaer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die van mijn Liefs <rhyme label="b" type="m">getrouwigheyt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Tot u is <rhyme label="b" type="m" corresp="#s3:l4">uyt-gebreyt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">En lecker tot verzadingh <rhyme label="b" type="m" corresp="#s3:l4">leyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hoe is mijn Lief soo <rhyme label="a" type="m">goedt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat hy niet eenen <rhyme label="a" type="m" corresp="#s4:l1">voet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Van my verset, noch wijcken <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme>?</l>
        <l id="s4:l4" met="4" type="-+">Of hy komt wederom d'er <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Voor mijnen deure <rhyme label="b" type="m" corresp="#s4:l4">staen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Wanneer ick was te bed' <rhyme label="b" type="m" corresp="#s4:l4">gegaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick was in slaep <rhyme label="a" type="m">gheraeckt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En noch het herte <rhyme label="a" type="m" corresp="#s5:l1">waeckt'</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">('t Geloof en was niet gants <rhyme label="a" type="m" corresp="#s5:l1">gestaeckt</rhyme>)</l>
        <l id="s5:l4" met="4" type="-+">Doe quam sijn stemme met een <rhyme label="b" type="m">klop</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Doe my mijn Suster <rhyme label="b" type="m" corresp="#s5:l4">op</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Die vol ben van <rhyme label="b" type="m" corresp="#s5:l4">ghenaden-drop</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Mijn Duyve, mijn <rhyme label="a" type="m">Vriendin</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Volmaeckte laet m' en <rhyme label="a" type="m" corresp="#s6:l1">in</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Staet op met een vernieuwde <rhyme label="a" type="m" corresp="#s6:l1">sin</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Want sie, mijn hooft is vol van <rhyme label="b" type="m">dauw'</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">De nacht-drop maeckt me <rhyme label="b" type="m" corresp="#s6:l4">flauw</rhyme>:</l>
        <l id="s6:l6" met="4" type="-+">Doet op, en zijt niet langer <rhyme label="b" type="m" corresp="#s6:l4">lauw</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Doch ick was niet <rhyme label="a" type="m">gereedt</rhyme>.</l>
        <l id="s7:l2" met="3" type="-+">Ick seyd', ick ben <rhyme label="a" type="m" corresp="#s7:l1">ontkleedt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ick mach niet weder tot'et <rhyme label="a" type="m" corresp="#s7:l1">leedt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Van kleeden en besmetten <rhyme label="b" type="m">gaen</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Ick liet de klopper <rhyme label="b" type="m" corresp="#s7:l4">staen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">En paste niet op sijn <rhyme label="b" type="m" corresp="#s7:l4">vermaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Maer hy sijn stercke <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">In 't gat der deuren <rhyme label="a" type="m" corresp="#s8:l1">sandt</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Dat roerde my het <rhyme label="a" type="m" corresp="#s8:l1">ingewandt</rhyme>;</l>
        <l id="s8:l4" met="4" type="-+">Doe stond' ick op, wel <rhyme label="b" type="m">onbesuyst</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">'t Slot vatt' ick met de <rhyme label="b" type="m" corresp="#s8:l4">vuyst</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">En siet het was met Mirh <rhyme label="b" type="m" corresp="#s8:l4">begruyst</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Mijn handen van het <rhyme label="a" type="m">nat</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Dat op de grendels <rhyme label="a" type="m" corresp="#s9:l1">sat</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Van Mirh, die werden glad en <rhyme label="a" type="m" corresp="#s9:l1">rat</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Ick deed' hem op. Maer laes m'en <rhyme label="b" type="m">Lief</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Was wegh. O <rhyme label="b" type="m" corresp="#s9:l4">ongerief</rhyme>!</l>
        <l id="s9:l6" met="4" type="-+">Vergeefs tot hem mijn stem ick <rhyme label="b" type="m" corresp="#s9:l4">hief</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">De Wachters in de <rhyme label="a" type="m">Stadt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die hebben my <rhyme label="a" type="m" corresp="#s10:l1">gevat</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Sy sloegen my en wond', en <rhyme label="a" type="m" corresp="#s10:l1">gat</rhyme>:</l>
        <l id="s10:l4" met="4" type="-+">Sy namen my den Sluyer <rhyme label="b" type="m">af</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Doe wierd' ick mat en <rhyme label="b" type="m" corresp="#s10:l4">laf</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Mijn gangh was een vertraeghde <rhyme label="b" type="m" corresp="#s10:l4">draf</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Ick sprack met Eeden <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Die m' in gheloof <rhyme label="a" type="m" corresp="#s11:l1">bestaen</rhyme>:</l>
        <l id="s11:l3" met="4" type="-+">Laet uw' ghebeden voor me <rhyme label="a" type="m" corresp="#s11:l1">gaen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Want ick van liefde krancker <rhyme label="b" type="m">ben</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Dan ick u segghe <rhyme label="b" type="m" corresp="#s11:l4">ken</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Mijn beed' is niet dan <rhyme label="b" type="m" corresp="#s11:l4">hert-gesten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
