<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182991</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Paul Jonas</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Nu komt de blyde dag eens <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Daar wy zo na <rhyme label="b" type="f">verlangden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Nu moeten zy verleegen <rhyme label="a" type="m" corresp="#s1:l1">staan</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die onzen Prins zoo <rhyme label="b" type="f" corresp="#s1:l2">prangden</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s1:l5">gevaar</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s1:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nu moet men zwygen van <rhyme label="a" type="m">verraad</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Den Prins heeft niets <rhyme label="b" type="f">misdreeven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hy waakte voor den vryen <rhyme label="a" type="m" corresp="#s2:l1">Staat</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En is getrouw <rhyme label="b" type="f" corresp="#s2:l2">gebleeven</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s2:l5">gevaar</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s2:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waar is nu de <rhyme label="a" type="m">Logengeest</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die zo kwam <rhyme label="b" type="f">bedillen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En elk heeft aan 't oor <rhyme label="a" type="m" corresp="#s3:l1">gekweest</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Geen laster te <rhyme label="b" type="f" corresp="#s3:l2">stillen</rhyme>?</l>
        <l id="s3:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s3:l5">gevaar</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s3:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Alles wat Prins Willem <rhyme label="a" type="m">deed</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Was zeer wel <rhyme label="b" type="f">beraden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hy was tot onze hulp <rhyme label="a" type="m" corresp="#s4:l1">gereed</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat bleek uit zyn <rhyme label="b" type="f" corresp="#s4:l2">daden</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s4:l5">gevaar</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s4:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'t Is zo klaar als 't wezen <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat hy aan zyn <rhyme label="b" type="f">pligten</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Als een wyze Vorst <rhyme label="a" type="m" corresp="#s5:l1">voldoet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Laat de nyt nu <rhyme label="b" type="f" corresp="#s5:l2">ewigten</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s5:l5">gevaar</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s5:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Veel meer dan men had <rhyme label="a" type="m">verwagt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ziet men nu op <rhyme label="b" type="f">deezen</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Oranje blyft by ons <rhyme label="a" type="m" corresp="#s6:l1">geagt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Niemant hoeft te <rhyme label="b" type="f" corresp="#s6:l2">vreezen</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s6:l5">gevaar</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s6:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nergens heeft hy in <rhyme label="a" type="m">misdaan</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hy is getrouw <rhyme label="b" type="f">gebleeven</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Oranje zal blyven <rhyme label="a" type="m" corresp="#s7:l1">staan</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wild hem zyn eere <rhyme label="b" type="f" corresp="#s7:l2">geeven</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s7:l5">gevaar</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s7:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Alles dat hy deed is <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ieder moet hem <rhyme label="b" type="f">pryzen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">En met een Bataafsch <rhyme label="a" type="m" corresp="#s8:l1">gemoed</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Hem alle Eer <rhyme label="b" type="f" corresp="#s8:l2">bewyzen</rhyme>.</l>
        <l id="s8:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s8:l5">gevaar</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s8:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hy is groot aan alle <rhyme label="a" type="m">kant</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Men kan hem <rhyme label="b" type="f">betrouwen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Hy bewaard ons <rhyme label="a" type="m" corresp="#s9:l1">Vaderland</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Als 't raakt in <rhyme label="b" type="f" corresp="#s9:l2">benouwen</rhyme>.</l>
        <l id="s9:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s9:l5">gevaar</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s9:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s9:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">God wil hem tot steun van 't <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Nog lang het leeven <rhyme label="b" type="f">spaaren</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Hy geleid hem door zyn <rhyme label="a" type="m" corresp="#s10:l1">hand</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Op aard' nog veele <rhyme label="b" type="f" corresp="#s10:l2">jaaren</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s10:l5">gevaar</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s10:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s10:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Van het eerste tydstip <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Dat hy meÃª <rhyme label="b" type="f">regeerde</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Was hy Holland <rhyme label="a" type="m" corresp="#s11:l1">aangenaam</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">De Nyd 't al <rhyme label="b" type="f" corresp="#s11:l2">verkeerde</rhyme>.</l>
        <l id="s11:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s11:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s11:l5">gevaar</rhyme>,</l>
        <l id="s11:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s11:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s11:l7">heerlyk</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Zingen wy Oranjes <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Zyn eer, roem en <rhyme label="b" type="f">glorie</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">God geleid hem op 't <rhyme label="a" type="m" corresp="#s12:l1">Hof</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">In den Raad. <rhyme label="b" type="f" corresp="#s12:l2">Victorie</rhyme>.</l>
        <l id="s12:l5" met="3" type="+-">Alles is nu <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s12:l6" met="3" type="+-">Daar is geen <rhyme label="c" type="m" corresp="#s12:l5">gevaar</rhyme>,</l>
        <l id="s12:l7" met="2" type="-+">Hy draagt zich <rhyme label="d" type="f">eerlyk</rhyme>,</l>
        <l id="s12:l8" met="2" type="-+">En defendeert zich <rhyme label="d" type="f" corresp="#s12:l7">heerlyk</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
