<song>
  <meta>
    <id>2964</id>
    <date>
      <year-start>1624</year-start>
      <year-end>1624</year-end>
    </date>
    <location>
      <orginal-name>[hoorn]</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>In des beminde lof</melody>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IN des beminden <rhyme label="a" type="m">lof</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wierd nooyt beminder <rhyme label="b" type="m">moê</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Voor al, als lovens <rhyme label="a" type="m" corresp="#s1:l1">stof</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Vloeyt overvloedigh <rhyme label="b" type="m" corresp="#s1:l2">toe</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Die liefdes aerdt verstaet en <rhyme label="c" type="m">lieft</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Het valt hem pijn, zwijght hyder <rhyme label="d" type="m">van</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">En 't is, of schijnt een <rhyme label="c" type="m" corresp="#s1:l5">eeren-dieft'</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Het goet niet prijzen alsmen <rhyme label="d" type="m" corresp="#s1:l6">kan</rhyme>.</l>
        <l id="s1:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s1:l9">my</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s1:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s1:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s1:l13">stem</rhyme></l>
        <l id="s1:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s1:l11">in</rhyme></l>
        <l id="s1:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s1:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vol van <rhyme label="a" type="m">Lanckmoedicheydt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is 't hert daer Liefd' heeft <rhyme label="b" type="m">stee</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Vriend'lijcke <rhyme label="a" type="m" corresp="#s2:l1">goedicheydt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Brengt zy gedurigh <rhyme label="b" type="m" corresp="#s2:l2">me'e</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Wech stugheydt, wraeck, en <rhyme label="c" type="m">hert-gestoor</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Daer zich de ware Liefde <rhyme label="d" type="m">vindt</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">De minnaer doet en lijdt veel <rhyme label="c" type="m" corresp="#s2:l5">Door</rhyme></l>
        <l id="s2:l8" met="4" type="-+">En Van de gene die hy <rhyme label="d" type="m" corresp="#s2:l6">mint</rhyme>.</l>
        <l id="s2:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s2:l9">my</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s2:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s2:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s2:l13">stem</rhyme></l>
        <l id="s2:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s2:l11">in</rhyme></l>
        <l id="s2:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s2:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">D'onmenschelijcke <rhyme label="a" type="m">aerdt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die lust heeft en <rhyme label="b" type="m">verblijdt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">In 's naestens <rhyme label="a" type="m" corresp="#s3:l1">qualijck-vaert</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En anders, smerte <rhyme label="b" type="m" corresp="#s3:l2">lijdt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Groeyt nooyt daer Liefde heeft haer <rhyme label="c" type="m">zaedt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Kracht'loos is alle nijdts <rhyme label="d" type="m">venijn</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Die Christ'lijck lieft, acht goedt en <rhyme label="c" type="m" corresp="#s3:l5">quaedt</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Zijns even-mensches voor het <rhyme label="d" type="m" corresp="#s3:l6">zijn</rhyme>.</l>
        <l id="s3:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s3:l9">my</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s3:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s3:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s3:l13">stem</rhyme></l>
        <l id="s3:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s3:l11">in</rhyme></l>
        <l id="s3:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s3:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Liefd' lijdt geen slimme <rhyme label="a" type="m">daedt</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">Geen trotz' hoogh <rhyme label="b" type="m">aerdicheydt</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Geen argerlijck <rhyme label="a" type="m" corresp="#s4:l1">gelaet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Noch zott' <rhyme label="b" type="m" corresp="#s4:l2">aelwaerdicheydt</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Nooyt de'e de Liever 't Lieve <rhyme label="c" type="m">leyt</rhyme>.</l>
        <l id="s4:l6" met="4" type="-+">'t Beminde acht de Minner <rhyme label="d" type="m">zeer</rhyme>;</l>
        <l id="s4:l7" met="4" type="-+">Die acht, eert in <rhyme label="c" type="m" corresp="#s4:l5">ootmoedicheydt</rhyme>.</l>
        <l id="s4:l8" met="4" type="-+">Oprechte Liefde luystert <rhyme label="d" type="m" corresp="#s4:l6">teer</rhyme>.</l>
        <l id="s4:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s4:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s4:l9">my</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s4:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s4:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s4:l13">stem</rhyme></l>
        <l id="s4:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s4:l11">in</rhyme></l>
        <l id="s4:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s4:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De taeye <rhyme label="a" type="m">Eygen-baet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die anders zich <rhyme label="b" type="m">vertoont</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dan zy in 't herte <rhyme label="a" type="m" corresp="#s5:l1">staet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Moet wech daer Liefde <rhyme label="b" type="m" corresp="#s5:l2">woont</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">O aldergoddelijckste <rhyme label="c" type="m">Deuchdt</rhyme>!</l>
        <l id="s5:l6" met="4" type="-+">Al waer't met eygen scha'e en <rhyme label="d" type="m">smert</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">'t Beminde weldoen, is een <rhyme label="c" type="m" corresp="#s5:l5">vreuchdt</rhyme></l>
        <l id="s5:l8" met="4" type="-+">In eens oprechten minnaers <rhyme label="d" type="m" corresp="#s5:l6">hert</rhyme>.</l>
        <l id="s5:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s5:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s5:l9">my</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s5:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s5:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s5:l13">stem</rhyme></l>
        <l id="s5:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s5:l11">in</rhyme></l>
        <l id="s5:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s5:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Geen opgeruy noch <rhyme label="a" type="m">praet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Daer zich de Liefd' aen <rhyme label="b" type="m">stoort</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">De Liefd' vermoedt geen <rhyme label="a" type="m" corresp="#s6:l1">quaedt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dan daer zy 't ziet en <rhyme label="b" type="m" corresp="#s6:l2">hoort</rhyme>;</l>
        <l id="s6:l5" met="4" type="-+">En als zy 't al ten vollen <rhyme label="c" type="m">weet</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Zoo schrijft zy 't niet in 't herte <rhyme label="d" type="m">op</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Quaedt moeten dencken, is Liefs <rhyme label="c" type="m" corresp="#s6:l5">leet</rhyme>.</l>
        <l id="s6:l8" met="4" type="-+">En ware Liefde heeft geen <rhyme label="d" type="m" corresp="#s6:l6">krop</rhyme>.</l>
        <l id="s6:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s6:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s6:l9">my</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s6:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s6:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s6:l13">stem</rhyme></l>
        <l id="s6:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s6:l11">in</rhyme></l>
        <l id="s6:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s6:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Liefd' verblijdt haer <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wanneerze een <rhyme label="b" type="m">vyandt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">In zond gevallen <rhyme label="a" type="m" corresp="#s7:l1">ziet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En by elck een in <rhyme label="b" type="m" corresp="#s7:l2">schandt</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">De Liever gunt ('t zy hoe <rhyme label="c" type="m">gering</rhyme>)</l>
        <l id="s7:l6" met="4" type="-+">Aen die hy lieft geen quaedt met <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Veel min de zond, zoo vuylen <rhyme label="c" type="m" corresp="#s7:l5">ding</rhyme></l>
        <l id="s7:l8" met="4" type="-+">En stof van 't hooghste <rhyme label="d" type="m" corresp="#s7:l6">ongeval</rhyme>.</l>
        <l id="s7:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s7:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s7:l9">my</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s7:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s7:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s7:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s7:l13">stem</rhyme></l>
        <l id="s7:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s7:l11">in</rhyme></l>
        <l id="s7:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s7:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Een vreuchde is 't voor <rhyme label="a" type="m">dien</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Die Christi Liefde <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dat Waerheydt ('t zy van <rhyme label="a" type="m" corresp="#s8:l1">wien</rhyme>)</l>
        <l id="s8:l4" met="3" type="-+">Gelooft wordt en <rhyme label="b" type="m" corresp="#s8:l2">beleeft</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Lief gunt aen Lief de lust, 't <rhyme label="c" type="m">vermaeck</rhyme></l>
        <l id="s8:l6" met="4" type="-+">En alles wat het goedts <rhyme label="d" type="m">bedenckt</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Veel meer de Deuchdt, zoo waerden <rhyme label="c" type="m" corresp="#s8:l5">zaeck</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">En daer Godt 't hooghste Goedt voor <rhyme label="d" type="m" corresp="#s8:l6">schenckt</rhyme></l>
        <l id="s8:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s8:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s8:l9">my</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s8:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s8:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s8:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s8:l13">stem</rhyme></l>
        <l id="s8:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s8:l11">in</rhyme></l>
        <l id="s8:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s8:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Liefd' is der Deuchden <rhyme label="a" type="m">hooft</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Haer krachten <rhyme label="b" type="m">zonderling</rhyme>;</l>
        <l id="s9:l3" met="3" type="-+">Want zy verdraeght, <rhyme label="a" type="m" corresp="#s9:l1">gelooft</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Hoopt, duldet alle <rhyme label="b" type="m" corresp="#s9:l2">ding</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Daer zy geen quaedt met allen, <rhyme label="c" type="m">merckt</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Gelooft zy gaerne alle <rhyme label="d" type="m">goedt</rhyme>;</l>
        <l id="s9:l7" met="4" type="-+">En daermen't quaedt ooghschijnlijck <rhyme label="c" type="m" corresp="#s9:l5">werckt</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Houdt zy tot bet'ring noch al <rhyme label="d" type="m" corresp="#s9:l6">moedt</rhyme>.</l>
        <l id="s9:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s9:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s9:l9">my</rhyme>,</l>
        <l id="s9:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s9:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s9:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s9:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s9:l13">stem</rhyme></l>
        <l id="s9:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s9:l11">in</rhyme></l>
        <l id="s9:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s9:l12">heeft</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dees Deuchden, en noch <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Doet rechte Liefd'al <rhyme label="b" type="m">doen</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">Die haer niet heeft, blijft <rhyme label="a" type="m" corresp="#s10:l1">veer</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Van ware hoop te <rhyme label="b" type="m" corresp="#s10:l2">voên</rhyme>:</l>
        <l id="s10:l5" met="4" type="-+">Hy is gelijck als een <rhyme label="c" type="m">metael</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Dat hel en schel is van <rhyme label="d" type="m">geluydt</rhyme>:</l>
        <l id="s10:l7" met="4" type="-+">Maer onderscheydelijcke <rhyme label="c" type="m" corresp="#s10:l5">tael</rhyme></l>
        <l id="s10:l8" met="4" type="-+">Of zang, en geeft het nimmer <rhyme label="d" type="m" corresp="#s10:l6">uyt</rhyme>.</l>
        <l id="s10:l9" met="2" type="+-">Koom dan <rhyme label="e" type="m">by</rhyme>,</l>
        <l id="s10:l10" met="2" type="+-">Zing met <rhyme label="e" type="m" corresp="#s10:l9">my</rhyme>,</l>
        <l id="s10:l11" met="3" type="+-">Minnaers die Godts <rhyme label="f" type="m">minn'</rhyme></l>
        <l id="s10:l12" met="3" type="-+">Trou lieft en trou <rhyme label="g" type="m">beleeft</rhyme>.</l>
        <l id="s10:l13" met="2" type="+-">'t Lof heeft <rhyme label="h" type="m">klem</rhyme></l>
        <l id="s10:l14" met="2" type="+-">Dat door <rhyme label="h" type="m" corresp="#s10:l13">stem</rhyme></l>
        <l id="s10:l15" met="3" type="+-">Van zulck mensch koomt <rhyme label="f" type="m" corresp="#s10:l11">in</rhyme></l>
        <l id="s10:l16" met="3" type="-+">Die 't ding verstaet en <rhyme label="g" type="m" corresp="#s10:l12">heeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
