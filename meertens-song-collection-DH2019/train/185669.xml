  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'t Kransje van vergeet mij <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wil ik, lieve jeugd! u <rhyme label="b" type="f">wijden</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">'t Kransje van vergeet mij <rhyme label="a" type="m" corresp="#s1:l1">niet</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Is het laatste <rhyme label="a" type="m" corresp="#s1:l1">kinderlied</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Lieve jeugd! vergeet het <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat ge in Neêrland zijt <rhyme label="b" type="f">geboren</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Lieve jeugd! vergeet toch <rhyme label="a" type="m" corresp="#s2:l1">niet</rhyme></l>
        <l id="s2:l4" met="4" type="+-">'t Bloed, dat u in de aders <rhyme label="a" type="m" corresp="#s2:l1">vliet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Neêrlandsch jeugd! vergeet ook <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dat ge ons eenmaal op zult <rhyme label="b" type="f">volgen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Neêrlandsch jeugd! vergeet ook <rhyme label="a" type="m" corresp="#s3:l1">niet</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Dat men op u hopend <rhyme label="a" type="m" corresp="#s3:l1">ziet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
