    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Adieu nymphe du bois</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHy zijt Jesu! soo <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat uwe goetheyt <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">My in u gaven <rhyme label="b" type="f">swemmen</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Ã´ Goetheyt! goetheydt <rhyme label="c" type="m">Goods</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">'k Moet u tot d' uur des <rhyme label="c" type="m" corresp="#s1:l4">doodts</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Prijsen met 't hart en <rhyme label="b" type="f" corresp="#s1:l3">stemmen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als ick was min als <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ghy maeckte mijn tot <rhyme label="a" type="m" corresp="#s2:l1">yet</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">'t Doopsel end' Roomsch <rhyme label="b" type="f">geloven</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ende u eyge <rhyme label="c" type="m">beelt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Hebt ghy my mee <rhyme label="c" type="m" corresp="#s2:l4">gedeelt</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Een Engel daer en <rhyme label="b" type="f" corresp="#s2:l3">boven</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Kennis Goods, end' <rhyme label="a" type="m">verstandt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gesonde leden <rhyme label="a" type="m" corresp="#s3:l1">kant</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hebt ghy my milt <rhyme label="b" type="f">gegeven</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Met Vogels, Visch en <rhyme label="c" type="m">Vee</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Met dranck, en kleed'ren <rhyme label="c" type="m" corresp="#s3:l4">mee</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ghy onderhout mijn <rhyme label="b" type="f" corresp="#s3:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy hebt my we'er <rhyme label="a" type="m">gehaelt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Als ick liep verr' <rhyme label="a" type="m" corresp="#s4:l1">verdwaelt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Van u, door mijn <rhyme label="b" type="f">sonden</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">U Vader op my <rhyme label="c" type="m">quaet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy als een <rhyme label="c" type="m" corresp="#s4:l4">Advocaet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Versoent door u vijf <rhyme label="b" type="f" corresp="#s4:l3">wonden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy zijt mijn Godt, mijn <rhyme label="a" type="m">Al</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Voor my in eenen <rhyme label="a" type="m" corresp="#s5:l1">stal</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Versmaet, veracht <rhyme label="b" type="f">geboren</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Ghy heb mijn ziel <rhyme label="c" type="m">verlost</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">'t Heeft u leven <rhyme label="c" type="m" corresp="#s5:l4">gekost</rhyme>.</l>
        <l id="s5:l6" met="3" type="-+">Sonder u, 'k was <rhyme label="b" type="f" corresp="#s5:l3">verlooren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Met u vleesch ende <rhyme label="a" type="m">bloet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mijn ziel ghy spijst en <rhyme label="a" type="m" corresp="#s6:l1">voet</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Ia, d'Hemel doet ghy <rhyme label="b" type="f">oopen</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Ghy heb my van de <rhyme label="c" type="m">doodt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Van prijck'len kleyn en <rhyme label="c" type="m" corresp="#s6:l4">groot</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Bewaert, verlost met <rhyme label="b" type="f" corresp="#s6:l3">hoopen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy hebt my tot een <rhyme label="a" type="m">staet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">(Die 't lijf en ziel doet <rhyme label="a" type="m" corresp="#s7:l1">baet</rhyme>)</l>
        <l id="s7:l3" met="3" type="-+">Geroepen na mijn <rhyme label="b" type="f">wenschen</rhyme>.</l>
        <l id="s7:l4" met="3" type="-+">Ghy geeft my tot <rhyme label="c" type="m">genucht</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Beesten, bloem, boom en <rhyme label="c" type="m" corresp="#s7:l4">vrucht</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Sangh van Vogels en <rhyme label="b" type="f" corresp="#s7:l3">Menschen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Och Jesu! ick <rhyme label="a" type="m">beken</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat ick met 't hart noch <rhyme label="a" type="m" corresp="#s8:l1">pen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">U goetheyt kan <rhyme label="b" type="f">verhalen</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Ick kan met lijf noch <rhyme label="c" type="m">geest</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Uw' gaven minst noch <rhyme label="c" type="m" corresp="#s8:l4">meest</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Vergelden, noch <rhyme label="b" type="f" corresp="#s8:l3">betalen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Tot danckbaerheyt, O <rhyme label="a" type="m">Heer</rhyme>!</l>
        <l id="s9:l2" met="3" type="-+">Ick val voor u vlack <rhyme label="a" type="m" corresp="#s9:l1">neer</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Offer u ziel en <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ey! dese Mijtjes <rhyme label="c" type="m">twee</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">(Als ghy van 't Weeuwtje <rhyme label="c" type="m" corresp="#s9:l4">dee</rhyme>)</l>
        <l id="s9:l6" met="3" type="-+">Ontfanghtse van my <rhyme label="b" type="f" corresp="#s9:l3">meden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">U liefde heeft geen <rhyme label="a" type="m">endt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Blijft heel en <rhyme label="a" type="m" corresp="#s10:l1">ongeschent</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">O middel punt der <rhyme label="b" type="f">minne</rhyme>!</l>
        <l id="s10:l4" met="3" type="-+">Mijn goede Godes <rhyme label="c" type="m">Soon</rhyme>!</l>
        <l id="s10:l5" met="3" type="-+">Geeft my u tot een <rhyme label="c" type="m" corresp="#s10:l4">loon</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Leydt my in d'Hemel <rhyme label="b" type="f" corresp="#s10:l3">binnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
