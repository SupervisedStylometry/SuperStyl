  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Heb geen vrees, myn <rhyme label="a" type="f">uitverkooren</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Eer van seyros en't <rhyme label="b" type="m">heelal</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">'k Ben voor uwe min <rhyme label="a" type="f" corresp="#s1:l1">gebooren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die my steeds bekooren <rhyme label="b" type="m" corresp="#s1:l2">zal</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Ik ben door uw min <rhyme label="c" type="f">gevangen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Droog de traanen van uw' <rhyme label="c" type="f" corresp="#s1:l5">wangen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die my al te dierbaar <rhyme label="d" type="m">zyn</rhyme>.</l>
        <l id="s1:l8" met="2" type="+-">Wil niet <rhyme label="e" type="f">vreezen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">'k Zal steeds <rhyme label="e" type="f" corresp="#s1:l8">weezen</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Die om uwe liefde <rhyme label="d" type="m" corresp="#s1:l7">kwyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
