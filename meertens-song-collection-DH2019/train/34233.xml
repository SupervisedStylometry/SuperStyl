    <melody>O zalig heilig Bethlehem</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jan Mommaert</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De suster van de <rhyme label="a" type="m">toovery</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En Venus naeste <rhyme label="b" type="f">bloedt-vriendinne</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vooghdess’ van ’s wereldts <rhyme label="a" type="m" corresp="#s1:l1">heerschappy</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En aller herten <rhyme label="b" type="f" corresp="#s1:l2">Koninginne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die onse zielen boeyt, en <rhyme label="a" type="m">bindt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Met stricxkens van ghevlochten <rhyme label="b" type="f">hayren</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Die met een loncxken <rhyme label="a" type="m" corresp="#s2:l1">over-wint</rhyme></l>
        <l id="s2:l4" met="4" type="-+">’t Gheweldt van vele <rhyme label="b" type="f" corresp="#s2:l2">Oorloghs-scharen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die ’t alles voor haer buyghen <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die door een stale-borst kan <rhyme label="b" type="f">booren</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die d’aller-herdtste van <rhyme label="a" type="m" corresp="#s3:l1">ghemoedt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Met een soet lachsken kan <rhyme label="b" type="f" corresp="#s3:l2">bekooren</rhyme>;</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die heeft my in haer hooghe <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die dwinght my tot een vyerigh <rhyme label="b" type="f">minnen</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Is ’t wonder! Neen: haer schoonheydts <rhyme label="a" type="m" corresp="#s4:l1">kracht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Kan d’aller-kloeckste wel <rhyme label="b" type="f" corresp="#s4:l2">verwinnen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wanneer ghy in de koelte <rhyme label="a" type="m">rust</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Soo drilt, en trilt het kruydt van <rhyme label="b" type="f">vreughden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Oft ’t in uw’ by-zijn waer <rhyme label="a" type="m" corresp="#s5:l1">verlust</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En in uw’ komste sich <rhyme label="b" type="f" corresp="#s5:l2">verheughden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Lely is jalours van <rhyme label="a" type="m">dy</rhyme>;</l>
        <l id="s6:l2" met="4" type="-+">De Goudt-bloem staet, vol spijt, <rhyme label="b" type="f">ghedoocken</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">De Roos wijckt, schaem-roodt, aen d’een <rhyme label="a" type="m" corresp="#s6:l1">zy</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Soo haest s’uw’ adem heeft <rhyme label="b" type="f" corresp="#s6:l2">gheroocken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wanneer ghy dan uw’ stemme <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Menght onder ’t hell’ gheklanck der <rhyme label="b" type="f">snaren</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ons op-ghelichte zielen, <rhyme label="a" type="m" corresp="#s7:l1">doet</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Ghy (door ’t gheluyt) ten hemel <rhyme label="b" type="f" corresp="#s7:l2">varen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Wel aen, mijn soete <rhyme label="a" type="m">Tooverinn</rhyme>’,</l>
        <l id="s8:l2" met="4" type="-+">Ghy sluyt my in soo sachte <rhyme label="b" type="f">banden</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Bindt vast, mijn lieve <rhyme label="a" type="m" corresp="#s8:l1">Vyandinn</rhyme>’,</l>
        <l id="s8:l4" met="4" type="-+">Ay! bluscht niet uyt dit vrolijck <rhyme label="b" type="f" corresp="#s8:l2">branden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
