  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SIngt vrolijck nu een Nieuwe <rhyme label="a" type="m">Liedt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Den Heylandt onsen <rhyme label="b" type="f">HEEren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die Heylighen so wel <rhyme label="a" type="m" corresp="#s1:l1">verblijdt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Sullen Syn Loff <rhyme label="b" type="f" corresp="#s1:l2">vermeeren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Israel seer verheughet <rhyme label="c" type="m">sy</rhyme></l>
        <l id="s1:l6" met="4" type="-+">In dien, die hem gaet maecken <rhyme label="c" type="m" corresp="#s1:l5">bly</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Tot syn Volck end zijn <rhyme label="d" type="f">Erve</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wel Vrolijck mogen Zyons <rhyme label="a" type="m">Kindt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van haren Coninck <rhyme label="b" type="f">singhen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">End Loven synen Naem <rhyme label="a" type="m" corresp="#s2:l1">bemint</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met Harpen Lieflick <rhyme label="b" type="f" corresp="#s2:l2">Clinghen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">De HEEre heeft een <rhyme label="c" type="m">Welbehaegh'</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Aen synen Volcke t'Allen <rhyme label="c" type="m" corresp="#s2:l5">Daegh'</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die Armen helpt hy <rhyme label="d" type="f">Heerlijck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die Heylighen (also <rhyme label="a" type="m">t'behoort</rhyme>)</l>
        <l id="s3:l2" met="3" type="-+">Vrolijck hem Loff <rhyme label="b" type="f">bewijsen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End Roemen blijde haer <rhyme label="a" type="m" corresp="#s3:l1">Confoort</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Haer Mondt sal Godt hooch <rhyme label="b" type="f" corresp="#s3:l2">Prijsen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">End hebben in haer handen <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Oock scharpe Sweerden <rhyme label="c" type="m" corresp="#s3:l5">allegaer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Verstaet des Gheestes <rhyme label="d" type="f">Wapen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Om Wraeck aen die Heyden te <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die Volcken te <rhyme label="b" type="f">Castyen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Jae, vinden hare Coning' <rhyme label="a" type="m" corresp="#s4:l1">koen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Haer Eed'lingh ende <rhyme label="b" type="f" corresp="#s4:l2">Vryen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Met Ketten end met stercke <rhyme label="c" type="m">Band'</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Dat sy bevanghen soo met <rhyme label="c" type="m" corresp="#s4:l5">schand'</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Des Heeren handt <rhyme label="d" type="f">bekennen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Als dan geschiet aen haer dat <rhyme label="a" type="m">Recht</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daer van is claer <rhyme label="b" type="f">beschreven</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">End syner Heyl'ghen heel <rhyme label="a" type="m" corresp="#s5:l1">gheslecht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Sal heerlijck syn <rhyme label="b" type="f" corresp="#s5:l2">verheven</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Om t'hebben sulcke <rhyme label="c" type="m">Weerdicheyt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Verfoeyen s'Werelts <rhyme label="c" type="m" corresp="#s5:l5">dwasicheyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">End singhen <rhyme label="d" type="f">Alleluya</rhyme>.</l>
      </lg>
    </lg>
  </text>
