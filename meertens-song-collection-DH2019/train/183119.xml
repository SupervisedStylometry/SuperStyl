  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Flora vlegt de <rhyme label="a" type="f">Oranje-Kroonen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En versierd het <rhyme label="b" type="m">Oranje-Hof</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ik wil my met Flora gaan <rhyme label="c" type="f">begeeven</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Om de Prins te zingen <rhyme label="b" type="m" corresp="#s1:l2">lof</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Aan een <rhyme label="d" type="m">waterkant</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Midden in <rhyme label="d" type="m" corresp="#s1:l5">Holland</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Daar staan de Oranje-vlaggen <rhyme label="d" type="m" corresp="#s1:l5">geplant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Treed hier maar in gy <rhyme label="a" type="f">Musikanten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Treed hier maar in de <rhyme label="b" type="m">Oranje-Zaal</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Roept Appollus met zyn <rhyme label="c" type="f">snaaren</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Maakt een accoord op onze <rhyme label="b" type="m" corresp="#s2:l2">taal</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Tusschen snaar en <rhyme label="d" type="m">klank</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Tusschen speel en <rhyme label="d" type="m" corresp="#s2:l5">zang</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">En roept met een stem Prins Willem leeft <rhyme label="d" type="m" corresp="#s2:l5">lang</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ik wil de Prins geluk gaan <rhyme label="a" type="f">wenschen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Met zyn <rhyme label="b" type="m">meerderjaarigheid</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Het is tot roem van alle <rhyme label="c" type="f">menschen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat hy leeft nog lange <rhyme label="b" type="m" corresp="#s3:l2">tyd</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ja dat den Boom wel <rhyme label="d" type="m">leeft</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat hy vrugten <rhyme label="d" type="m" corresp="#s3:l5">geeft</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Ja de Prins van Oranje lang <rhyme label="d" type="m" corresp="#s3:l5">leeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ik wil de Oranje-Appels gaan <rhyme label="a" type="f">plukken</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">De gulde Appels na den <rhyme label="b" type="m">schyn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Het is om de liefde uit te <rhyme label="c" type="f">drukken</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Voor de Prins in het <rhyme label="b" type="m" corresp="#s4:l2">gemeen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">De liefde hy <rhyme label="d" type="m">bemind</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Tot de Prins is hy <rhyme label="d" type="m" corresp="#s4:l5">gezind</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Ja wederliefde die hy <rhyme label="d" type="m" corresp="#s4:l5">verdient</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ziet Wilhelmus zyn Jonge <rhyme label="a" type="f">Spruiten</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Dat haar Echo geeft <rhyme label="b" type="m">wederklank</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En haar stem beantwoord een <rhyme label="c" type="f">ieder</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Deeze is den hoogste <rhyme label="b" type="m" corresp="#s5:l2">zang</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ja dat den Boom wel <rhyme label="d" type="m">bloeit</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En hy vol vrugten <rhyme label="d" type="m" corresp="#s5:l5">groeit</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Ja dat den Stam nooit word <rhyme label="d" type="m" corresp="#s5:l5">uitgeroeit</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Batavieren, maakt <rhyme label="a" type="f">plaizieren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Want ons Prins word <rhyme label="b" type="m">ingehaald</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Met Oranje willen wy hem <rhyme label="c" type="f">versieren</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">'s Haage is met vreugd <rhyme label="b" type="m" corresp="#s6:l2">vervuld</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ja dat zyn naam hier <rhyme label="d" type="m">zweeft</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En roepen dat God <rhyme label="d" type="m" corresp="#s6:l5">geeft</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Dat den Prins van Oranje lang <rhyme label="d" type="m" corresp="#s6:l5">leeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
