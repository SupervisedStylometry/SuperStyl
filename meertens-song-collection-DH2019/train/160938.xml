    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Neen mijn Philis, verg my <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Dat mijn hert so ernstig <rhyme label="a" type="m" corresp="#s1:l1">vlied</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Wild my van die bee <rhyme label="b" type="f">verschonen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Mijn geselschap sou <rhyme label="c" type="m">gewis</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Damons oog en grootsheyd <rhyme label="b" type="f" corresp="#s1:l3">honen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Die so opgeblasen <rhyme label="c" type="m" corresp="#s1:l4">is</rhyme>.</l>
      </lg>
    </lg>
