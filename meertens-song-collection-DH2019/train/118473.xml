  <author>Willem Sluiter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Als oogen in 't <rhyme label="a" type="m">gemeen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sien uit na u <rhyme label="a" type="m" corresp="#s1:l1">alleen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">O lieve Godt en <rhyme label="b" type="f">Vader</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Gy geeft, op uwe <rhyme label="c" type="m">wijs</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hen t'sijner tijt haar <rhyme label="c" type="m" corresp="#s1:l4">spijs</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En voedsel <rhyme label="b" type="f" corresp="#s1:l3">allegader</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Uw over-milde <rhyme label="a" type="m">hant</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Doet gy, door 't gansche <rhyme label="a" type="m" corresp="#s2:l1">lant</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ons open, t'allen <rhyme label="b" type="f">dagen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En gy versadigt <rhyme label="c" type="m">al</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wat leeft in 't aardsche <rhyme label="c" type="m" corresp="#s2:l4">dal</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Na uw goed <rhyme label="b" type="f" corresp="#s2:l3">wel-behagen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daarom wy komen <rhyme label="a" type="m">nu</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Al 'tsamen dus tot <rhyme label="a" type="m" corresp="#s3:l1">u</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">O Godt, wy komen <rhyme label="b" type="f">weder</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Uw hooge <rhyme label="c" type="m">Majesteit</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Siet doch, in <rhyme label="c" type="m" corresp="#s3:l4">vriendlikheit</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Op den geringsten <rhyme label="b" type="f" corresp="#s3:l3">neder</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hadt gy vergolden 't <rhyme label="a" type="m">quaat</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van onse snoode <rhyme label="a" type="m" corresp="#s4:l1">daat</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Soo waar nu <rhyme label="b" type="f">afgesneden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">De spijs voor ons' <rhyme label="c" type="m">gesicht</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">De blijdschap waar <rhyme label="c" type="m" corresp="#s4:l4">geslicht</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In uwen huis' <rhyme label="b" type="f" corresp="#s4:l3">alrede</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ia soo waar al <rhyme label="a" type="m">vergaan</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Het kooren ende <rhyme label="a" type="m" corresp="#s5:l1">graan</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En onder sijne <rhyme label="b" type="f">kluiten</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ten eenemaal <rhyme label="c" type="m">verrot</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Daar waar geen <rhyme label="c" type="m" corresp="#s5:l4">oveschot</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat iemant weg mog <rhyme label="b" type="f" corresp="#s5:l3">sluiten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maar 't blijkt wel, dat gy <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Op onse sonden <rhyme label="a" type="m" corresp="#s6:l1">siet</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Gy kent uw' handen <rhyme label="b" type="f">werken</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Hoe broosch wy zijn van <rhyme label="c" type="m">aart</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Dies hebt gy ons <rhyme label="c" type="m" corresp="#s6:l4">gespaart</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En wilt voortaan ons <rhyme label="b" type="f" corresp="#s6:l3">sterken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Vermits gy ons noch <rhyme label="a" type="m">voedt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Met sulk een <rhyme label="a" type="m" corresp="#s7:l1">overvloet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Van allerhande <rhyme label="b" type="f">spijsen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En liefelijke <rhyme label="c" type="m">drank</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Waar voor wy met <rhyme label="c" type="m" corresp="#s7:l4">gesank</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Uw naam soo vrolik <rhyme label="b" type="f" corresp="#s7:l3">prijsen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Gy zijt het die <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ons steeds houdt op de <rhyme label="a" type="m" corresp="#s8:l1">been'</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Hoe haast zijn wy <rhyme label="b" type="f">besweken</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En in gevaar des <rhyme label="c" type="m">doots</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Als gy den straf des <rhyme label="c" type="m" corresp="#s8:l4">broots</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Maar eenmaal komt te <rhyme label="b" type="f" corresp="#s8:l3">breken</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Gelijk gy dan, O <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Ons uwe tafel <rhyme label="a" type="m" corresp="#s9:l1">weer</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Gedekt hebt voor ons' <rhyme label="b" type="f">oogen</rhyme>;</l>
        <l id="s9:l4" met="3" type="-+">Soo geef uw zegen <rhyme label="c" type="m">t'saam</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Dat wy 't in uwen <rhyme label="c" type="m" corresp="#s9:l4">Naam</rhyme></l>
        <l id="s9:l6" met="3" type="-+">En dankbaar nutt'gen <rhyme label="b" type="f" corresp="#s9:l3">mogen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Leer ons bedenken <rhyme label="a" type="m">staag</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Dat d'overladen <rhyme label="a" type="m" corresp="#s10:l1">maag</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Tot alles ongeschikt <rhyme label="b" type="f">is</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Maar die sig matig <rhyme label="c" type="m">voedt</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Aan lichaam en <rhyme label="c" type="m" corresp="#s10:l4">gemoet</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Op 't allerbeste verquikt <rhyme label="b" type="f" corresp="#s10:l3">is</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Terwijl gy ons <rhyme label="a" type="m">alhier</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">O Godt, soo <rhyme label="a" type="m" corresp="#s11:l1">goedertier</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Dees' aardsche spijs komt <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Laat ons niet zijn <rhyme label="c" type="m">ontbloot</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Van 't lieflijk hemelsch <rhyme label="c" type="m" corresp="#s11:l4">broot</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Waar by de ziel moet <rhyme label="b" type="f" corresp="#s11:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Wy setten ons dan <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Op dit vertrouwen, <rhyme label="a" type="m" corresp="#s12:l1">neer</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Om dees' uw schoone <rhyme label="b" type="f">gaven</rhyme></l>
        <l id="s12:l4" met="3" type="-+">T'ontfangen met <rhyme label="c" type="m">geneugt</rhyme>:</l>
        <l id="s12:l5" met="3" type="-+">Gy wilt met spijs en <rhyme label="c" type="m" corresp="#s12:l4">vreugt</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Doch onse herten <rhyme label="b" type="f" corresp="#s12:l3">laven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
