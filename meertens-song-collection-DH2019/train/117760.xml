    <melody>Mijn hartje is heel doorwond</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Silvester en <rhyme label="a" type="m">Koridon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die gingen na de <rhyme label="b" type="f">Linden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Omtrent de Zuyer <rhyme label="a" type="m" corresp="#s1:l1">Bron</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Om Grasjes t'zaam te <rhyme label="b" type="f" corresp="#s1:l2">binden</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zy kwamen ter Plaatze <rhyme label="a" type="m">daar</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">By een zeer Hogen <rhyme label="b" type="f">Ceder</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Daar 't zeer genoegelyk <rhyme label="a" type="m" corresp="#s2:l1">waar</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Zy gingen Zitten <rhyme label="b" type="f" corresp="#s2:l2">neder</rhyme>,</l>
        <l id="s2:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy Vlyden haar om een <rhyme label="a" type="m">Kus</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En om haar Borsjes te <rhyme label="b" type="f">voelen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zy zey, Ey Koridon <rhyme label="a" type="m" corresp="#s3:l1">flus</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ik voel myn Hartje <rhyme label="b" type="f" corresp="#s3:l2">Krioele</rhyme>,</l>
        <l id="s3:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy zag haar zo Vriendelyk <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En voelden daar met zyn <rhyme label="b" type="f">Handen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Of 't Polsje begon te <rhyme label="a" type="m" corresp="#s4:l1">slaan</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maar ag! haar Hartje <rhyme label="b" type="f" corresp="#s4:l2">Branden</rhyme>,</l>
        <l id="s4:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Haar Borsjes heeft hy der <rhyme label="a" type="m">ontdaan</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daar zag hy de Blanke <rhyme label="b" type="f">Leden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Met twe RÃ´ Tepeltjes <rhyme label="a" type="m" corresp="#s5:l1">staan</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En voelde voort <rhyme label="b" type="f" corresp="#s5:l2">Beneden</rhyme>,</l>
        <l id="s5:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sylverstertje og! myn <rhyme label="a" type="m">Lief</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Zy Koridon met een <rhyme label="b" type="f">Kusje</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ey, doet eens myn <rhyme label="a" type="m" corresp="#s6:l1">Gerief</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Myn Hart Opstygent <rhyme label="b" type="f" corresp="#s6:l2">Lusje</rhyme>,</l>
        <l id="s6:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Og zydze myn <rhyme label="a" type="m">Koridon</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Mins togje moet ik 'er <rhyme label="b" type="f">bedwingen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">De Ceder en deze <rhyme label="a" type="m" corresp="#s7:l1">Bron</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">De Vogels zouden uyt <rhyme label="b" type="f" corresp="#s7:l2">Zingen</rhyme>,</l>
        <l id="s7:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Geen Vogeltjes nog <rhyme label="a" type="m">Geboomt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Geen Water zal ons <rhyme label="b" type="f">Beklappen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Het Beekje dat Ruyzent <rhyme label="a" type="m" corresp="#s8:l1">Stroomt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En zal ons niet <rhyme label="b" type="f" corresp="#s8:l2">betrappen</rhyme>,</l>
        <l id="s8:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Maar Koridon zal u <rhyme label="a" type="m">Trouw</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">My Nimmer of Oyt <rhyme label="b" type="f">begeven</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Dat ik als Harders <rhyme label="a" type="m" corresp="#s9:l1">Vrouw</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">By u mag zyn <rhyme label="b" type="f" corresp="#s9:l2">verheven</rhyme>,</l>
        <l id="s9:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Betrouw my dat Vryelyk <rhyme label="a" type="m">toe</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Zilverstertje op! my <rhyme label="b" type="f">Waarde</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Zo ik u Ontrouw <rhyme label="a" type="m" corresp="#s10:l1">doe</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Zo Schent my Lugt en <rhyme label="b" type="f" corresp="#s10:l2">Aarde</rhyme>,</l>
        <l id="s10:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O Hemel wat ik daar <rhyme label="a" type="m">Zag</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Zy Speelden het was een <rhyme label="b" type="f">Wonder</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Dat Koridon boven <rhyme label="a" type="m" corresp="#s11:l1">Lag</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Zilverstertje raak <rhyme label="b" type="f" corresp="#s11:l2">onder</rhyme>,</l>
        <l id="s11:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">De Bladertjes maakten <rhyme label="a" type="m">Gedril</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Mit gaf Zilvester een <rhyme label="b" type="f">Zugje</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Dog Kupido zat <rhyme label="a" type="m" corresp="#s12:l1">stil</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En Zag dit Geestig <rhyme label="b" type="f" corresp="#s12:l2">Klugje</rhyme>,</l>
        <l id="s12:l5" met="5" type="+-">Fa la, fa la la la la la <rhyme label="c" type="m">la</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
