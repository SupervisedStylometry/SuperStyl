      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Waerom Ã´ Zang <rhyme label="a" type="f">Godinnen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Is 't dat gy niet en <rhyme label="b" type="m">trouwt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wyl velen u <rhyme label="a" type="f" corresp="#s1:l1">beminnen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En elk veel van u <rhyme label="b" type="m" corresp="#s1:l2">houdt</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Elk blaekt om u met teedren <rhyme label="c" type="m">gloed</rhyme>!</l>
        <l id="s1:l6" met="3" type="-+">Maer ik kan 't wel <rhyme label="d" type="f">bevatten</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Gy hebt geen ryke <rhyme label="d" type="f" corresp="#s1:l6">schatten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En yder trouwt om 't <rhyme label="c" type="m" corresp="#s1:l5">goet</rhyme>.</l>
      </lg>
