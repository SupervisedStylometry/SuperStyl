  <author>Johan Deknatel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DAar is maar een uit aller <rhyme label="a" type="m">tal</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die lievens waardig <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En dien ik eeuwig kiezen <rhyme label="a" type="m" corresp="#s1:l1">zal</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">De Heere Jesus <rhyme label="b" type="m" corresp="#s1:l2">Christ</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hem mynen Heer en God <rhyme label="a" type="m">allein</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Is myne ziel <rhyme label="b" type="m">gewydt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hem stae ik vaardig en <rhyme label="a" type="m" corresp="#s2:l1">bereidt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En ben van herten <rhyme label="b" type="m" corresp="#s2:l2">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Aan Jesus heb ik wat ik <rhyme label="a" type="m">wil</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">In allen <rhyme label="b" type="m">overvloed</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En leev' uit zyne volheit <rhyme label="a" type="m" corresp="#s3:l1">stil</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En heb het hoogste <rhyme label="b" type="m" corresp="#s3:l2">Goed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Op zyn genaade gae ik <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En wyk geen treê nog <rhyme label="b" type="m">schreê</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">My volgt naar zyn beloft' en <rhyme label="a" type="m" corresp="#s4:l1">woord</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Zyn goede zeegen <rhyme label="b" type="m" corresp="#s4:l2">meê</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Zyn arrem kroont myn <rhyme label="a" type="m">arrebeid</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En schenkt my leevens <rhyme label="b" type="m">zap</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">'t Geloof aan zyne <rhyme label="a" type="m" corresp="#s5:l1">Majesteit</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Voert myne <rhyme label="b" type="m" corresp="#s5:l2">pelgrimschap</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
