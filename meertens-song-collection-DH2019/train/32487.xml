  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">KRachtigh,, <rhyme label="a" type="m">noyt</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Geen gesont,, hert <rhyme label="b" type="m">gewont</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Als het mijne <rhyme label="c" type="m">overschoont</rhyme></l>
        <l id="s1:l4" met="2" type="+-">klachtigh,, <rhyme label="a" type="m" corresp="#s1:l1">oyt</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Mijnen Mont,, gansch den <rhyme label="a" type="m" corresp="#s1:l1">gront</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Mijnder Ziele heeft <rhyme label="c" type="m" corresp="#s1:l3">verthoont</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Aen een Jonghe <rhyme label="d" type="m">Maeght</rhyme>’</l>
        <l id="s1:l8" met="4" type="+-">Die my jaeght,, <rhyme label="d" type="m" corresp="#s1:l7">onversaeght</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">En gants desperaet,, <rhyme label="e" type="m">laet</rhyme>,,</l>
        <l id="s1:l10" met="3" type="+-">'tIs om niet <rhyme label="d" type="m" corresp="#s1:l7">geklaeght</rhyme>,,</l>
        <l id="s1:l11" met="2" type="+-">Haer <rhyme label="d" type="m" corresp="#s1:l7">behaeght</rhyme>,,</l>
        <l id="s1:l12" met="4" type="+-">Desen mijnen droeven <rhyme label="e" type="m" corresp="#s1:l9">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Al dat <rhyme label="a" type="m">in</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Mijnen geest,, <rhyme label="b" type="m">onbevreest</rhyme>’</l>
        <l id="s2:l3" met="4" type="+-">Plach te leven sonder <rhyme label="c" type="m">noot</rhyme>:</l>
        <l id="s2:l4" met="2" type="+-">Heeft den <rhyme label="a" type="m" corresp="#s2:l1">sin</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Alder-meest,, met <rhyme label="a" type="m" corresp="#s2:l1">tempeest</rhyme>’</l>
        <l id="s2:l6" met="4" type="+-">Vast benaut, tot inde <rhyme label="c" type="m" corresp="#s2:l3">doot</rhyme>;</l>
        <l id="s2:l7" met="3" type="+-">Eylaes! wie sal <rhyme label="d" type="m">my</rhyme>’</l>
        <l id="s2:l8" met="4" type="+-">Maecken vry,, nu dat <rhyme label="d" type="m" corresp="#s2:l7">sy</rhyme>,,</l>
        <l id="s2:l9" met="4" type="+-">My haut inden last,, <rhyme label="e" type="m">vast</rhyme>;</l>
        <l id="s2:l10" met="3" type="+-">Die my gaen <rhyme label="d" type="m" corresp="#s2:l7">voor-by</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Roepen <rhyme label="d" type="m" corresp="#s2:l7">sy</rhyme></l>
        <l id="s2:l12" met="4" type="+-">Over my ellendigh <rhyme label="e" type="m" corresp="#s2:l9">gast</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">'t Herte <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Sucht, en treurt,, anghstich <rhyme label="b" type="m">scheurt</rhyme>,,</l>
        <l id="s3:l3" met="4" type="+-">In benautheyt schier <rhyme label="c" type="m">versmacht</rhyme>:</l>
        <l id="s3:l4" met="2" type="+-">Moet het <rhyme label="a" type="m" corresp="#s3:l1">sijn</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Dus besuert,, my <rhyme label="a" type="m" corresp="#s3:l1">gebuert</rhyme>,,</l>
        <l id="s3:l6" met="4" type="+-">(Ellendigh mensch) meer dan <rhyme label="c" type="m" corresp="#s3:l3">kracht</rhyme>;</l>
        <l id="s3:l7" met="3" type="+-">Heb ick <rhyme label="d" type="m">metter-daet</rhyme>,,</l>
        <l id="s3:l8" met="4" type="+-">Haeren raet,, oyt <rhyme label="d" type="m" corresp="#s3:l7">versmaet</rhyme>,,</l>
        <l id="s3:l9" met="4" type="+-">Oft geacht te kleen,, <rhyme label="e" type="m">neen</rhyme>,,</l>
        <l id="s3:l10" met="3" type="+-">Maer wel tot mijn <rhyme label="d" type="m" corresp="#s3:l7">baet</rhyme>,,</l>
        <l id="s3:l11" met="2" type="+-"><rhyme label="d" type="m" corresp="#s3:l7">Toe-verlaet</rhyme>,,</l>
        <l id="s3:l12" met="4" type="+-">Haer ter noot bekent <rhyme label="e" type="m" corresp="#s3:l9">alleen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Anders <rhyme label="a" type="m">geen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Troost soo soet,, die <rhyme label="b" type="m">'tgemoet</rhyme>,,</l>
        <l id="s4:l3" met="4" type="+-">Geeft de ruste <rhyme label="c" type="m">principael</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Als dat <rhyme label="a" type="m" corresp="#s4:l1">reen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Edel Bloet,, die my <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme>,,</l>
        <l id="s4:l6" met="4" type="+-">Daeghs suchten thien <rhyme label="c" type="m" corresp="#s4:l3">duysent-mael</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Valt sy my wat <rhyme label="d" type="m">swaer</rhyme>,,</l>
        <l id="s4:l8" met="4" type="+-">Korts hier naer,, werdt <rhyme label="d" type="m" corresp="#s4:l7">troost-baer</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Al wat sy bekleeft,, <rhyme label="e" type="m">geeft</rhyme>:</l>
        <l id="s4:l10" met="3" type="+-">Sulcx betrouw ick <rhyme label="d" type="m" corresp="#s4:l7">haer</rhyme>,,</l>
        <l id="s4:l11" met="2" type="+-">Die <rhyme label="d" type="m" corresp="#s4:l7">voor-waer</rhyme>,,</l>
        <l id="s4:l12" met="4" type="+-">Een goet hert tot my waert <rhyme label="e" type="m" corresp="#s4:l9">heeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Rustigh <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Ick met vlijt,, sijn <rhyme label="b" type="m">altyt</rhyme>,,</l>
        <l id="s5:l3" met="4" type="+-">Dienst-baer vast in haer <rhyme label="c" type="m">verbont</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">t' Sy oft <rhyme label="a" type="m" corresp="#s5:l1">al</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Mijn profyt,, storm, en <rhyme label="a" type="m" corresp="#s5:l1">stryt</rhyme>,,</l>
        <l id="s5:l6" met="4" type="+-">Wesen sal, terlaester <rhyme label="c" type="m" corresp="#s5:l3">stont</rhyme>:</l>
        <l id="s5:l7" met="3" type="+-">Daer my anders <rhyme label="d" type="m">niet</rhyme>,,</l>
        <l id="s5:l8" met="4" type="+-">Dan verdriet,, en <rhyme label="d" type="m" corresp="#s5:l7">geschiet</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Wacht ick goet accoort,, <rhyme label="e" type="m">voort</rhyme>,,</l>
        <l id="s5:l10" met="3" type="+-">Soo sy my <rhyme label="d" type="m" corresp="#s5:l7">niet</rhyme>,,</l>
        <l id="s5:l11" met="2" type="+-">Ende <rhyme label="d" type="m" corresp="#s5:l7">biet</rhyme>,,</l>
        <l id="s5:l12" met="4" type="+-">Een gesont maeckende <rhyme label="e" type="m" corresp="#s5:l9">woort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Naer dîen <rhyme label="a" type="m">sy</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Even vast, groeyt, en <rhyme label="b" type="m">wast</rhyme>,,</l>
        <l id="s6:l3" met="4" type="+-">In mijn hert sonder <rhyme label="c" type="m">aff-gaen</rhyme></l>
        <l id="s6:l4" met="2" type="+-">Vind' ick <rhyme label="a" type="m" corresp="#s6:l1">my</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Aen-getast,, <rhyme label="a" type="m" corresp="#s6:l1">overlast</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">(Boven reden) <rhyme label="c" type="m" corresp="#s6:l3">aen-gedaen</rhyme>:</l>
        <l id="s6:l7" met="3" type="+-">Ick moet <rhyme label="d" type="m">even-wel</rhyme>,,</l>
        <l id="s6:l8" met="4" type="+-">Mijn gequel,, achten <rhyme label="d" type="m" corresp="#s6:l7">spel</rhyme>,,</l>
        <l id="s6:l9" met="4" type="+-">Wat baet veel geklach,, <rhyme label="e" type="m">ach</rhyme>,,</l>
        <l id="s6:l10" met="3" type="+-">Al valt sy <rhyme label="d" type="m" corresp="#s6:l7">rebel</rhyme>,,</l>
        <l id="s6:l11" met="2" type="+-">Niemant <rhyme label="d" type="m" corresp="#s6:l7">el</rhyme>,,</l>
        <l id="s6:l12" met="4" type="+-">Mijn versuchten eynden <rhyme label="e" type="m" corresp="#s6:l9">mach</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
