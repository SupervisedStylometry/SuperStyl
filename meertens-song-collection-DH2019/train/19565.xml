  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O, <rhyme label="a" type="m">Minnevuir</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zo kraftig in ‘et <rhyme label="b" type="f">stooken</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Zmeulen, en <rhyme label="b" type="f" corresp="#s1:l2">rooken</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat ik by na <rhyme label="c" type="m">verdwijn</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">O! znoode <rhyme label="a" type="m" corresp="#s1:l1">uir</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die mijn lust hebt <rhyme label="b" type="f" corresp="#s1:l2">gebroken</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">En al mijn <rhyme label="b" type="f" corresp="#s1:l2">ontlooken</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Vreugd, verkeerd in <rhyme label="c" type="m" corresp="#s1:l4">pijn</rhyme>.</l>
        <l id="s1:l9" met="4" type="+-">O verleidelijkke <rhyme label="d" type="f">Minne</rhyme>!</l>
        <l id="s1:l10" met="4" type="+-">O hartnekkige <rhyme label="d" type="f" corresp="#s1:l9">Godinne</rhyme>!</l>
        <l id="s1:l11" met="2" type="+-">Hoe is mijn <rhyme label="e" type="m">Hart</rhyme></l>
        <l id="s1:l12" met="3" type="-+">In Minn’ verkeerd, <rhyme label="e" type="m" corresp="#s1:l11">verward</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
