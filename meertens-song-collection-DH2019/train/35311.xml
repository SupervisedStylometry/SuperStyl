<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35311</id>
    <date>
      <year-start>1712</year-start>
      <year-end>1712</year-end>
    </date>
    <location>
      <orginal-name>duinkerken</orginal-name>
      <standard-name>Duinkerken, France</standard-name>
      <coordinates>
        <lon>51.1907405</lon>
        <lat>2.4978867</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Is liefde zoet lijden</melody>
  <author>Jacobus de Ruyter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Goeden dagh mijn <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Anne Metje mijn <rhyme label="b" type="m">lief</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Uw' proper <rhyme label="a" type="f" corresp="#s1:l1">persoone</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Is mijn hert een <rhyme label="b" type="m" corresp="#s1:l2">dief</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Uw' charmante <rhyme label="c" type="f">oogen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Die loncken op <rhyme label="d" type="m">my</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Wilt liefde <rhyme label="c" type="f" corresp="#s1:l5">betoogen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Maeckt mijn herte <rhyme label="d" type="m" corresp="#s1:l6">bly</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Uw' sneeuw-witte <rhyme label="e" type="f">borsten</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Kaeck en lipkens <rhyme label="f" type="m">roodt</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Sy doen my seer <rhyme label="e" type="f" corresp="#s1:l9">dorsten</rhyme></l>
        <l id="s1:l12" met="3" type="+-">Naer uw' <rhyme label="f" type="m" corresp="#s1:l10">maeghde-schoot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Wie hoor' ick daer <rhyme label="a" type="f">klagen</rhyme>?</l>
        <l id="s2:l2" met="3" type="+-">Soo droevigh voor mijn <rhyme label="b" type="m">deur</rhyme>?</l>
        <l id="s2:l3" met="3" type="+-">Die my trouw komt <rhyme label="a" type="f" corresp="#s2:l1">vragen</rhyme>?</l>
        <l id="s2:l4" met="3" type="+-">Vertreckt vry <rhyme label="b" type="m" corresp="#s2:l2">monsieur</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Gy komt my <rhyme label="c" type="f">flatteren</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Alsoo ick <rhyme label="d" type="m">verstaen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Wilt doch <rhyme label="c" type="f" corresp="#s2:l5">retireren</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Naer een ander <rhyme label="d" type="m" corresp="#s2:l6">gaen</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">'K en wil u niet <rhyme label="e" type="f">hooren</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Hey! staeckt uw' <rhyme label="f" type="m">verdriet</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Want al uw' <rhyme label="e" type="f" corresp="#s2:l9">devoiren</rhyme></l>
        <l id="s2:l12" met="3" type="+-">En moeyt' is om <rhyme label="f" type="m" corresp="#s2:l10">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Mijn Engel <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">'K sterv' van <rhyme label="b" type="m">minne-pijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Lief wilt doch <rhyme label="a" type="f" corresp="#s3:l1">genesen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">'T jonck herte van <rhyme label="b" type="m" corresp="#s3:l2">mijn</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Gy staet in mijn <rhyme label="c" type="f">sinnen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Mijn Engeltjen <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Laet m' uw' gratie <rhyme label="c" type="f" corresp="#s3:l5">winnen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Eer ick sterven <rhyme label="d" type="m" corresp="#s3:l6">moet</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Geeft my uw' jonck <rhyme label="e" type="f">herte</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Voor een soet <rhyme label="f" type="m">present</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Voor mijn pijn en <rhyme label="e" type="f" corresp="#s3:l9">smerte</rhyme>,</l>
        <l id="s3:l12" met="3" type="+-">Ick houd' my <rhyme label="f" type="m" corresp="#s3:l10">content</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Veel Jonghmans die <rhyme label="a" type="f">spreken</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Soet met hunnen <rhyme label="b" type="m">mondt</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Maer 't zijn loose <rhyme label="a" type="f" corresp="#s4:l1">treken</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">'T hert' is valsch van <rhyme label="b" type="m" corresp="#s4:l2">grondt</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Om een Maget <rhyme label="c" type="f">teere</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Te brengen in <rhyme label="d" type="m">schandt</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Dan <rhyme label="c" type="f" corresp="#s4:l5">abondonneeren</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">En trecken uit 't <rhyme label="d" type="m" corresp="#s4:l6">Landt</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">'K en laet soo niet <rhyme label="e" type="f">plucken</rhyme></l>
        <l id="s4:l10" met="3" type="+-">Mijn bloemken soo <rhyme label="f" type="m">gouw</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">'T en zal u niet <rhyme label="e" type="f" corresp="#s4:l9">lucken</rhyme></l>
        <l id="s4:l12" met="3" type="+-">Voor wy zijn in <rhyme label="f" type="m" corresp="#s4:l10">trouw</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Mijn Lief uwe <rhyme label="a" type="f">reden</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Die doen my de <rhyme label="b" type="m">doodt</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Aen-hoort mijn <rhyme label="a" type="f" corresp="#s5:l1">gebeden</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Helpt my uyt den <rhyme label="b" type="m" corresp="#s5:l2">noodt</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">En wilt niet <rhyme label="c" type="f">gelooven</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Dat ick daerom <rhyme label="d" type="m">kom</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Om uw' eer' te <rhyme label="c" type="f" corresp="#s5:l5">rooven</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Neen, neen schoone <rhyme label="d" type="m" corresp="#s5:l6">bl√¥m</rhyme>;</l>
        <l id="s5:l9" met="3" type="+-">Op trouw ick u <rhyme label="e" type="f">geven</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Een rinck aen uw' <rhyme label="f" type="m">handt</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">En oock mijn jonck <rhyme label="e" type="f" corresp="#s5:l9">leven</rhyme>,</l>
        <l id="s5:l12" met="3" type="+-">Tot vast <rhyme label="f" type="m" corresp="#s5:l10">onderpandt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Wel is g'heel uw' <rhyme label="a" type="f">herte</rhyme></l>
        <l id="s6:l2" met="3" type="+-">En sinnen tot <rhyme label="b" type="m">mijn</rhyme>?</l>
        <l id="s6:l3" met="3" type="+-">Uw' droefheydt en <rhyme label="a" type="f" corresp="#s6:l1">smerte</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Doet my selver <rhyme label="b" type="m" corresp="#s6:l2">pijn</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Komt laet my <rhyme label="c" type="f">ontfangen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Een kusken op <rhyme label="d" type="m">trouw</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Op mijn roode <rhyme label="c" type="f" corresp="#s6:l5">wangen</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Ick wil zijn uw' <rhyme label="d" type="m" corresp="#s6:l6">vrouw</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">'K wil leven te <rhyme label="e" type="f">gader</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">Tot de doodt ons <rhyme label="f" type="m">scheydt</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">'K Verlaet moeder, <rhyme label="e" type="f" corresp="#s6:l9">vader</rhyme></l>
        <l id="s6:l12" met="3" type="+-">Om uw' <rhyme label="f" type="m" corresp="#s6:l10">trouwigheydt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Wat troost ick <rhyme label="a" type="f">genieten</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Wat vreught voel ick <rhyme label="b" type="m">daer</rhyme>?</l>
        <l id="s7:l3" met="3" type="+-">Cupido komt <rhyme label="a" type="f" corresp="#s7:l1">schieten</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Door 't herte van <rhyme label="b" type="m" corresp="#s7:l2">haer</rhyme></l>
        <l id="s7:l5" met="3" type="+-">Mijn droefheydt <rhyme label="c" type="f">verandert</rhyme></l>
        <l id="s7:l6" met="3" type="+-">In een groote <rhyme label="d" type="m">vreught</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Wij zijn by <rhyme label="c" type="f" corresp="#s7:l5">malkander</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">In eer' en <rhyme label="d" type="m" corresp="#s7:l6">deught</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">Gy doet my <rhyme label="e" type="f">herleven</rhyme></l>
        <l id="s7:l10" met="3" type="+-">En stelt my <rhyme label="f" type="m">gerust</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Mijn Trouw ick u <rhyme label="e" type="f" corresp="#s7:l9">geven</rhyme></l>
        <l id="s7:l12" met="3" type="+-">Sa Lief eens <rhyme label="f" type="m" corresp="#s7:l10">gekust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
