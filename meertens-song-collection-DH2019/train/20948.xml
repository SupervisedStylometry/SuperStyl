<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20948</id>
    <date>
      <year-start>1655</year-start>
      <year-end>1655</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Phyllis kwam zich buigen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Alderwaerdeste <rhyme label="a" type="f">Moeder</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Die Godt ons' <rhyme label="a" type="f" corresp="#s1:l1">Behoeder</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Maghet hebt <rhyme label="b" type="m">gebaert</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">Ick verkies u tot <rhyme label="c" type="m">Me-Vrouw</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En tot Patroners op <rhyme label="d" type="f">heden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Op u stel ick mijn <rhyme label="c" type="m" corresp="#s1:l4">berouw</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ick sal t'allen <rhyme label="a" type="f">tijden</rhyme></l>
        <l id="s2:l2" met="3" type="+-">U, Me-Vrouw <rhyme label="a" type="f" corresp="#s2:l1">belijden</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">En mijn <rhyme label="b" type="m">Voorsprakers</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">En ick vast'lijck neem voor <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dat ick u noyt sal <rhyme label="d" type="f">verlaten</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">Ick voor Godt dit nu <rhyme label="c" type="m" corresp="#s2:l4">bely</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Ick sal noyt <rhyme label="a" type="f">onbreken</rhyme></l>
        <l id="s3:l2" met="3" type="+-">In uw' lof te <rhyme label="a" type="f" corresp="#s3:l1">spreken</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">'k Sal u spreken <rhyme label="b" type="m">voor</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Ick sal tegen u niet <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Noch oock laten yet <rhyme label="d" type="f">geschieden</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">Godt wil my daer voor <rhyme label="c" type="m" corresp="#s3:l4">behoe'n</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Maghet <rhyme label="a" type="f">uytgelesen</rhyme>!</l>
        <l id="s4:l2" met="3" type="+-">Ick sal altijdt <rhyme label="a" type="f" corresp="#s4:l1">wesen</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Tot uw' dienst <rhyme label="b" type="m">bereydt</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Soo veel als in my sal <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Sal ick maecken te <rhyme label="d" type="f">beletten</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Tegen u het <rhyme label="c" type="m" corresp="#s4:l4">tongh-fenijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Ick sal staeghs u <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Ia uw' eer <rhyme label="a" type="f" corresp="#s5:l1">vermeeren</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Maer ick kan of <rhyme label="b" type="m">mach</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Des soo neemt tot slaef my <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Is dat 't eenichsins kan <rhyme label="d" type="f">wesen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Wilt tot Dienaer my <rhyme label="c" type="m" corresp="#s5:l4">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Och of ick kon <rhyme label="a" type="f">mercken</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Dat gh' in all' mijn <rhyme label="a" type="f" corresp="#s6:l1">wercken</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Vroomich my stondt <rhyme label="b" type="m">by</rhyme>!</l>
        <l id="s6:l4" met="4" type="+-">Ey! en wilt verlaten <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s6:l5" met="4" type="+-">My, uw' dienaer, in mijn <rhyme label="d" type="f">sterven</rhyme>!</l>
        <l id="s6:l6" met="4" type="+-">Dan met gracy my <rhyme label="c" type="m" corresp="#s6:l4">versiet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Als my sal <rhyme label="a" type="f">omringen</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Satan, en <rhyme label="a" type="f" corresp="#s7:l1">bespringen</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Staet doch my dan <rhyme label="b" type="m">by</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Ey! wilt in des Hemels <rhyme label="c" type="m">Hof</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Waerde Moeder Godts! my <rhyme label="d" type="f">brengen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Waer gesongen wort Godts <rhyme label="c" type="m" corresp="#s7:l4">lof</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
