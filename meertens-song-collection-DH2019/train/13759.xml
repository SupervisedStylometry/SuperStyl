  <author>anoniem</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">RAmpsalighe <rhyme label="a" type="f">Diane</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Ghy die verlaten zijt van alle <rhyme label="b" type="f">weelden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Komt Atrops wilt mijn <rhyme label="a" type="f" corresp="#s1:l1">baene</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Mijn levens endt, Waer zijt ghy die mijn <rhyme label="b" type="f" corresp="#s1:l2">teelden</rhyme>?</l>
        <l id="s1:l5" met="2" type="-+">Komt siet u <rhyme label="c" type="m">kindt</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die inde <rhyme label="c" type="m" corresp="#s1:l5">windt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Van ‘t wreede noot-lots <rhyme label="d" type="f">vlaeghen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Wert voort <rhyme label="e" type="f">ghedreven</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Dies sy het endt haers <rhyme label="e" type="f" corresp="#s1:l8">leven</rhyme></l>
        <l id="s1:l10" met="2" type="+-">Slijt in <rhyme label="d" type="f" corresp="#s1:l7">klaeghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O oorsaeck van mijn <rhyme label="a" type="f">klaegen</rhyme>!</l>
        <l id="s2:l2" met="5" type="-+">Die eertijts de oorsaeck van mijn <rhyme label="b" type="f">vreughden</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ghedenckt u niet de <rhyme label="a" type="f" corresp="#s2:l1">daghen</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Florentius, ick wense dat u <rhyme label="b" type="f" corresp="#s2:l2">heughden</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Doen wy dees <rhyme label="c" type="m">strant</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Gingh handt aen <rhyme label="c" type="m" corresp="#s2:l5">handt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">In eenigheydt <rhyme label="d" type="f">betreeden</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Daer ghy <rhyme label="e" type="f">bekoorde</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Met u schijn-ware <rhyme label="e" type="f" corresp="#s2:l8">woorden</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">My met <rhyme label="d" type="f" corresp="#s2:l7">eeden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daer ghy swoert by de <rhyme label="a" type="f">starren</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Dat Son, en Maen het hemels-blaeuw souw <rhyme label="b" type="f">laeten</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En onder een <rhyme label="a" type="f" corresp="#s3:l1">verwarren</rhyme>:</l>
        <l id="s3:l4" met="5" type="-+">Iae dat het schob-gediert de zee souw <rhyme label="b" type="f" corresp="#s3:l2">haeten</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Eer dat u <rhyme label="c" type="m">trouw</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Door ramp of <rhyme label="c" type="m" corresp="#s3:l5">rouw</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">In ‘t minste deel souw <rhyme label="d" type="f">stuyten</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Dit zijn u <rhyme label="e" type="f">reden</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">En dier-geswooren <rhyme label="e" type="f" corresp="#s3:l8">eeden</rhyme>;</l>
        <l id="s3:l10" met="2" type="+-">Guyt der <rhyme label="d" type="f" corresp="#s3:l7">Guyten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O Prins! dees snoode <rhyme label="a" type="f">daeden</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Sal zijn een smet voor u, en u <rhyme label="b" type="f">gheslachte</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Vermits ghy hebt <rhyme label="a" type="f" corresp="#s4:l1">verraden</rhyme></l>
        <l id="s4:l4" met="5" type="-+">Mijn suyvre eer, daer ghy soo geyl nae <rhyme label="b" type="f" corresp="#s4:l2">trachte</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Doen u het <rhyme label="c" type="m">lot</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Gaf het <rhyme label="c" type="m" corresp="#s4:l5">ghenot</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Van liefdens <rhyme label="d" type="f">soetigheden</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Vlucht ghy nae <rhyme label="e" type="f">Griecken</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Met snel-ghevlerckte <rhyme label="e" type="f" corresp="#s4:l8">wiecken</rhyme>,</l>
        <l id="s4:l10" met="2" type="+-">Sonder <rhyme label="d" type="f" corresp="#s4:l7">reden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
