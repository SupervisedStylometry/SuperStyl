    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Quam Mariam dulcibus</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Philippus Jennyn</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">HEt is dertigh daghen nu <rhyme label="a" type="f">gheleden</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Dat Godts Lamke wierdt voor ons <rhyme label="a" type="f" corresp="#s1:l1">besneden</rhyme>,</l>
        <l id="s1:l3" met="5" type="+-">Het wordt heden voor ons al te <rhyme label="b" type="f">pande</rhyme></l>
        <l id="s1:l4" met="5" type="+-">Godt gheschoncken tot een <rhyme label="b" type="f" corresp="#s1:l3">Offerande</rhyme>.</l>
        <l id="s1:l5" met="7" type="+-">Weest ghegroet MARIA Moeder ende Maghet <rhyme label="c" type="m">reyn</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Die den Heere // 't Lamke <rhyme label="d" type="f">teere</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Offert, met twee duyfkens <rhyme label="c" type="m" corresp="#s1:l5">kleyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Siet MARIA, Moeder <rhyme label="a" type="f">wtverkoren</rhyme>,</l>
        <l id="s2:l2" met="5" type="+-">Die blijft altijdt suyver Maeght als <rhyme label="a" type="f" corresp="#s2:l1">voren</rhyme>,</l>
        <l id="s2:l3" met="5" type="+-">Wil haer onder Moyses wet <rhyme label="b" type="f">verkleenen</rhyme>,</l>
        <l id="s2:l4" met="5" type="+-">Om met Godt haer ziele te <rhyme label="b" type="f" corresp="#s2:l3">vereenen</rhyme>.</l>
        <l id="s2:l5" met="7" type="+-">Weest ghegroet MARIA Moeder ende Maghet <rhyme label="c" type="m">reyn</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">Die den Heere // 't Lamke <rhyme label="d" type="f">teere</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Offert, met twee duyfkens <rhyme label="c" type="m" corresp="#s2:l5">kleyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Den besneden IESUS, door een <rhyme label="a" type="f">wonde</rhyme></l>
        <l id="s3:l2" met="5" type="+-">Wilde draghen teecken van de <rhyme label="a" type="f" corresp="#s3:l1">sonde</rhyme>;</l>
        <l id="s3:l3" met="5" type="+-">En MARIA, Vat der <rhyme label="b" type="f">Heylighede</rhyme>,</l>
        <l id="s3:l4" met="5" type="+-">Doet haer kerck-gangh vol van <rhyme label="b" type="f" corresp="#s3:l3">suyverhede</rhyme>.</l>
        <l id="s3:l5" met="7" type="+-">Weest ghegroet MARIA Moeder, ende Maghet <rhyme label="c" type="m">reyn</rhyme>;</l>
        <l id="s3:l6" met="4" type="+-">Die den Heere // 't Lamke <rhyme label="d" type="f">teere</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Offert, met twee duyfkens <rhyme label="c" type="m" corresp="#s3:l5">kleyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Na den Tempel met twee suyver <rhyme label="a" type="f">dieren</rhyme></l>
        <l id="s4:l2" met="5" type="+-">Gaet MARIA Godes wetten <rhyme label="a" type="f" corresp="#s4:l1">vieren</rhyme></l>
        <l id="s4:l3" met="5" type="+-">Om daer mede haeren <rhyme label="b" type="f">eerst-ghebooren</rhyme></l>
        <l id="s4:l4" met="5" type="+-">Te verlossen vandes Heeren <rhyme label="b" type="f" corresp="#s4:l3">thooren</rhyme>.</l>
        <l id="s4:l5" met="7" type="+-">Weest ghegroet MARIA Moeder, ende Maghet <rhyme label="c" type="m">reyn</rhyme>;</l>
        <l id="s4:l6" met="4" type="+-">Die den Heere // 'tLamke <rhyme label="d" type="f">teere</rhyme></l>
        <l id="s4:l7" met="4" type="+-">Offert, met twee duyfkens <rhyme label="c" type="m" corresp="#s4:l5">kleyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="+-">Met die suyver Gheesten van hier <rhyme label="a" type="f">boven</rhyme>,</l>
        <l id="s5:l2" met="5" type="+-">Laet ons dese Maghet altijdt <rhyme label="a" type="f" corresp="#s5:l1">loven</rhyme>,</l>
        <l id="s5:l3" met="5" type="+-">Die vol deughden, in dat eeuwigh <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s5:l4" met="5" type="+-">Is vol vreughden, boven al <rhyme label="b" type="f" corresp="#s5:l3">verheven</rhyme>.</l>
        <l id="s5:l5" met="7" type="+-">Weest ghegroet MARIA Moeder, ende Maghet <rhyme label="c" type="m">reyn</rhyme>:</l>
        <l id="s5:l6" met="4" type="+-">Die den Heere // 'tLamke <rhyme label="d" type="f">teere</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Offert, met twee duyfkens <rhyme label="c" type="m" corresp="#s5:l5">kleyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
