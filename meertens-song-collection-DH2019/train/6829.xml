<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6829</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>emmerick</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Wij loven U klein en groot</melody>
    <categories>
      <category translation="emotions">emoties</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wy loven u kleyn ende <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Lof Iesus lief <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die vooronsstorf den bitter <rhyme label="a" type="m" corresp="#s1:l1">doodt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Daerm wy u aen <rhyme label="b" type="f" corresp="#s1:l2">kleven</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Als een goet Heer’ <rhyme label="b" type="f" corresp="#s1:l2">beseven</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Door u liefde <rhyme label="c" type="f">gepresen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ghy hebt gestort u <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">O Iesu Christe <rhyme label="d" type="m" corresp="#s1:l7">soet</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">V doodt seer <rhyme label="d" type="m" corresp="#s1:l7">goet</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Die heeft ons al <rhyme label="c" type="f" corresp="#s1:l6">genesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Door Adam waren wy <rhyme label="a" type="m">besmet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Soo dat wy moesten <rhyme label="b" type="f">sterven</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Wy lagen doodt onder de <rhyme label="a" type="m" corresp="#s2:l1">Wet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wy moesten al <rhyme label="b" type="f" corresp="#s2:l2">bederven</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Maar (Iesu) wy <rhyme label="b" type="f" corresp="#s2:l2">verwerven</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Door u, leven, <rhyme label="c" type="f">gepresen</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Ghy hebt gestort u <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">O Iesu Christe <rhyme label="d" type="m" corresp="#s2:l7">soet</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">V doodt seer <rhyme label="d" type="m" corresp="#s2:l7">goet</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Die heeft ons al <rhyme label="c" type="f" corresp="#s2:l6">genesen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy daldet uyt dijns Vaders <rhyme label="a" type="m">schoot</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In een suyver <rhyme label="b" type="f">Kerssouwe</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat’s Maria schoon Roose <rhyme label="a" type="m" corresp="#s3:l1">root</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Wy sijn verost uyt <rhyme label="b" type="f" corresp="#s3:l2">rouwe</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Dies seggen wy <rhyme label="b" type="f" corresp="#s3:l2">getrouwe</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Heer Iesus weert <rhyme label="c" type="f">gepresen</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Ghy hebt gestort u <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">O Iesu Christe <rhyme label="d" type="m" corresp="#s3:l7">soet</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">V doodt seer <rhyme label="d" type="m" corresp="#s3:l7">goet</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Die heeft ons al <rhyme label="c" type="f" corresp="#s3:l6">genesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">V doodt heeft ons d’leven <rhyme label="a" type="m">gebrocht</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghy hebt sonder <rhyme label="b" type="f">beswaren</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Der menschen salicheyt <rhyme label="a" type="m" corresp="#s4:l1">gewrocht</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Doe wy verloren <rhyme label="b" type="f" corresp="#s4:l2">waren</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Dus wy u <rhyme label="b" type="f" corresp="#s4:l2">openbaren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Thien duysent lof <rhyme label="c" type="f">gepresen</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Ghy hebt gestort u <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">O Iesu Christe <rhyme label="d" type="m" corresp="#s4:l7">soet</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">V doodt seer <rhyme label="d" type="m" corresp="#s4:l7">goet</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Die heeft ons al <rhyme label="c" type="f" corresp="#s4:l6">genesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy sijt voor ons den <rhyme label="a" type="m">Advocaet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Den peys die quaemt ghy <rhyme label="b" type="f">maken</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Dies wy Iesus vroech ende <rhyme label="a" type="m" corresp="#s5:l1">laet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Naer u seer vierich <rhyme label="b" type="f" corresp="#s5:l2">haken</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Om by u te <rhyme label="b" type="f" corresp="#s5:l2">geraken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In hemelrijck <rhyme label="c" type="f">gepresen</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Ghy hebt gestort u <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">O Iesu Christe <rhyme label="d" type="m" corresp="#s5:l7">soet</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">V doodt seer <rhyme label="d" type="m" corresp="#s5:l7">goet</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Die heeft ons al <rhyme label="c" type="f" corresp="#s5:l6">genesen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy hebt ons tot een <rhyme label="a" type="m">Testament</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Op de wereldt <rhyme label="b" type="f">gelaten</rhyme></l>
        <l id="s6:l3" met="4" type="-+">V vleesch en bloet seer <rhyme label="a" type="m" corresp="#s6:l1">excellent</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Om nutten t’onser <rhyme label="b" type="f" corresp="#s6:l2">baten</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Wt rechter <rhyme label="b" type="f" corresp="#s6:l2">charitaten</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">O Iesu lief <rhyme label="c" type="f">gepresen</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Ghy hebt gestort u <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">O Iesu Christe <rhyme label="d" type="m" corresp="#s6:l7">soet</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">V doodt seer <rhyme label="d" type="m" corresp="#s6:l7">goet</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Die heeft ons al <rhyme label="c" type="f" corresp="#s6:l6">genesen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O Prince hooch <rhyme label="a" type="m">gebenedijt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wilt onsen geest <rhyme label="b" type="f">verstercken</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Om u te loven t’aller <rhyme label="a" type="m" corresp="#s7:l1">tijdt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Hier en in alle <rhyme label="b" type="f" corresp="#s7:l2">perckien</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Want groot sijn al u <rhyme label="b" type="f" corresp="#s7:l2">wercken</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Die ghy doet weert <rhyme label="c" type="f">gepresen</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Ghy hebt gestort u <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">O Iesu Christe <rhyme label="d" type="m" corresp="#s7:l7">soet</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">V doodt seer <rhyme label="d" type="m" corresp="#s7:l7">goet</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Die heeft ons al <rhyme label="c" type="f" corresp="#s7:l6">genesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
