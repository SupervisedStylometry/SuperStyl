    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Florida zo het wezen mag</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Guillaume Abrahams Ooijevaer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De overschoone Maeghde <rhyme label="a" type="m">Ieught</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die stadigh staen nae Eer en <rhyme label="a" type="m" corresp="#s1:l1">Deught</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En daer in dan soo <rhyme label="b" type="f">rennen</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Wiens ganck leydt nae des Levens <rhyme label="c" type="m">padt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zijn 's Wereldts domheydt <rhyme label="c" type="m" corresp="#s1:l4">zadt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En soecken Deught te <rhyme label="b" type="f" corresp="#s1:l3">wennen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wiens soet en lodderlijck <rhyme label="a" type="m">ghelonck</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Zijn Pronckjes in de <rhyme label="a" type="m" corresp="#s2:l1">Minne-vonck</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met oversoete <rhyme label="b" type="f">Deughtjes</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Sy tarten Iuno uyt den <rhyme label="c" type="m">Throon</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En Pallas, Venus <rhyme label="c" type="m" corresp="#s2:l4">schoon</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Baren niet sulcke <rhyme label="b" type="f" corresp="#s2:l3">Vruechtjes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Het zijn Goddinnen van de <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die Liefde draghen met een <rhyme label="a" type="m" corresp="#s3:l1">sin</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En baren soete <rhyme label="b" type="f">lusjes</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Die ons versachten pijn in 't <rhyme label="c" type="m">Hart</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat wy zijn uyt de <rhyme label="c" type="m" corresp="#s3:l4">smart</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van mondeloose <rhyme label="b" type="f" corresp="#s3:l3">Kusjes</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Haert overglinsterend <rhyme label="a" type="m">ghelaet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ver boven Diamanten <rhyme label="a" type="m" corresp="#s4:l1">gaet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ghelijck men kan <rhyme label="b" type="f">betoghen</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Want Diamant en is maer <rhyme label="c" type="m">steen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Niet nutter ons als <rhyme label="c" type="m" corresp="#s4:l4">been</rhyme>:</l>
        <l id="s4:l6" met="3" type="-+">Waer toe kan ons sulckx <rhyme label="b" type="f" corresp="#s4:l3">doghen</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wat kan ons meerder nut dan <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Als een Deughdigh Nymphje te <rhyme label="a" type="m" corresp="#s5:l1">hoen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En daer toe schoon van <rhyme label="b" type="f">Leden</rhyme>?</l>
        <l id="s5:l4" met="4" type="-+">Want haer aerdigh vleysje <rhyme label="c" type="m">sacht</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Gheeft ons een soete <rhyme label="c" type="m" corresp="#s5:l4">kracht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met duysent waerde <rhyme label="b" type="f" corresp="#s5:l3">reden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Gheen Corael of oock <rhyme label="a" type="m">Gheperelt'</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Van de gantsche heele <rhyme label="a" type="m" corresp="#s6:l1">Wereldt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">En acht ick noyt soo <rhyme label="b" type="f">waerdigh</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Als dese Maeghden <rhyme label="c" type="m">oversoet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Die ons zijn wonder <rhyme label="c" type="m" corresp="#s6:l4">goedt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Om haer Deught alsoo <rhyme label="b" type="f" corresp="#s6:l3">aerdigh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
