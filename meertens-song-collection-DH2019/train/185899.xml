<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185899</id>
    <date>
      <year-start>1790</year-start>
      <year-end>1790</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De schoonheid van mijn zielsvriendin</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De Schoonheid van mijn Ziels <rhyme label="a" type="m">vrindin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">De glans van myn <rhyme label="b" type="f">Nanette</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Doet myn hert van liefde <rhyme label="c" type="f">blaeken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Waerom ik moet eeuwig <rhyme label="c" type="f" corresp="#s1:l3">haeken</rhyme>?</l>
        <l id="s1:l5" met="4" type="-+">Na het voorwerp van myn <rhyme label="d" type="m">Min</rhyme>…</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Haer heldere stem, haer lieffelyk <rhyme label="a" type="m">sier</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Haer straelende oogjes en schitterend <rhyme label="b" type="f">vier</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Haer oogjes stort in myn teder <rhyme label="c" type="f">hert</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Een onuitspreekbare <rhyme label="c" type="f" corresp="#s2:l3">smert</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Alle daegen uur op <rhyme label="d" type="m">uur</rhyme>…</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Myn ziel verteert in haere <rhyme label="a" type="m">gloed</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Als ik haere hand mag <rhyme label="b" type="f">raeke</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wild myn smerte niet <rhyme label="c" type="f">vermeeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">In haer boezem niet <rhyme label="c" type="f" corresp="#s3:l3">verteeren</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">O dat doet myn ziel zoo <rhyme label="d" type="m">goed</rhyme>…</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O Flonkerster die my <rhyme label="a" type="m">doorwond</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Houd op van my te <rhyme label="b" type="f">blaeken</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Wild myn smerte niet <rhyme label="c" type="f">vermeeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">In u boezem niet <rhyme label="c" type="f" corresp="#s4:l3">verteeren</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Maer bemind myn weer zoo <rhyme label="d" type="m">goed</rhyme>…</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Och hemel wat een droef <rhyme label="a" type="m">geluyt</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Hoor ik nu tot myn <rhyme label="b" type="f">koomen</rhyme>?</l>
        <l id="s5:l3" met="4" type="-+">Moet ik dan van droefheid <rhyme label="c" type="f">sterven</rhyme>?</l>
        <l id="s5:l4" met="4" type="-+">Kan ik dan geen troost <rhyme label="c" type="f" corresp="#s5:l3">verwerven</rhyme>?</l>
        <l id="s5:l5" met="4" type="-+">Van een die ik teer <rhyme label="d" type="m">bemin</rhyme>…</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wat hoor ik daer voor <rhyme label="a" type="m">lieflykheid</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Wie komt myn Ziel zoo <rhyme label="b" type="f">stooren</rhyme>?</l>
        <l id="s6:l3" met="4" type="-+">Is 't Nanette na 't <rhyme label="c" type="f">verklaeren</rhyme>?</l>
        <l id="s6:l4" met="4" type="-+">Of den Echo myn <rhyme label="c" type="f" corresp="#s6:l3">vervaeren</rhyme>?</l>
        <l id="s6:l5" met="4" type="-+">Roept nog eens ik ben te <rhyme label="d" type="m">vree</rhyme>…</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wel aen daer is myn hert en <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wilt uw met myn <rhyme label="b" type="f">verbinden</rhyme>!</l>
        <l id="s7:l3" met="4" type="-+">Laet ons in den Echt <rhyme label="c" type="f">vergaeren</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En ons ziel te zaemen <rhyme label="c" type="f" corresp="#s7:l3">paeren</rhyme>!</l>
        <l id="s7:l5" met="4" type="-+">Ons twee herten zynen <rhyme label="d" type="m">een</rhyme>…</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Wild nu dan op Trompetten <rhyme label="a" type="m">blaeze</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Op dit heerlyk huwelyks <rhyme label="b" type="f">Feest</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Van een Maegt van Vyftien <rhyme label="c" type="f">Jaeren</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Die nu op dit Feest zal <rhyme label="c" type="f" corresp="#s8:l3">paeren</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Ja met Luden haere <rhyme label="d" type="m">vriend</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Komt herders vlegt een <rhyme label="a" type="m">Bloemen-Krans</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Van Mirthen en <rhyme label="b" type="f">Lauwrieren</rhyme>!</l>
        <l id="s9:l3" met="4" type="-+">Wildze op 't hooft gaen <rhyme label="c" type="f">zetten</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Van die schoone Bruyt <rhyme label="c" type="f" corresp="#s9:l3">Nanette</rhyme></l>
        <l id="s9:l5" met="4" type="-+">En op Luden haere <rhyme label="d" type="m">Vrind</rhyme>…</l>
      </lg>
    </lg>
  </text>
</song>
