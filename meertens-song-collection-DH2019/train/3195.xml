      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">DIe des Wereldts <rhyme label="a" type="f">tooysel</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">En 'er vreemde <rhyme label="b" type="f">vouwen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Die des Wereldts <rhyme label="a" type="f" corresp="#s1:l1">mooysel</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Eens wil recht <rhyme label="b" type="f" corresp="#s1:l2">beschouwen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Sal, is mijn <rhyme label="b" type="f" corresp="#s1:l2">vertrouwen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wel seghen dat 'er <rhyme label="c" type="m">pracht</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Haer kleeren // en <rhyme label="d" type="f">veeren</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Haer quicken // en <rhyme label="e" type="f">stricken</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Zijn weert <rhyme label="c" type="m" corresp="#s1:l6">belacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Hoe zijn nu de <rhyme label="a" type="f">Ioffers</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Opghesmuyckt met <rhyme label="b" type="f">Kantjes</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Even als de <rhyme label="a" type="f" corresp="#s2:l1">Doffers</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Overal met <rhyme label="b" type="f" corresp="#s2:l2">bandtjes</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Daer met <rhyme label="b" type="f" corresp="#s2:l2">Diamantjes</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En hier met Goudt of <rhyme label="c" type="m">Krael</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">'t Gun flonckert // en <rhyme label="d" type="f">lonckert</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Met lichjes // en <rhyme label="e" type="f">schichjes</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">En weytse <rhyme label="c" type="m" corresp="#s2:l6">prael</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Maer de goede <rhyme label="a" type="f">zeden</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Gheven meerder <rhyme label="b" type="f">netsel</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">En ghesonde <rhyme label="a" type="f" corresp="#s3:l1">reden</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Dan 'et mooy <rhyme label="b" type="f" corresp="#s3:l2">Blancketsel</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">'t Is maer leelijck <rhyme label="b" type="f" corresp="#s3:l2">letsel</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Wanneer een suyver <rhyme label="c" type="m">nat</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Met vlieten // en <rhyme label="d" type="f">gieten</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">En planghen // de <rhyme label="e" type="f">wanghen</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Maer <rhyme label="c" type="m" corresp="#s3:l6">overspat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Deftige <rhyme label="a" type="f">manieren</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Soetigheydt in <rhyme label="b" type="f">eeren</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Sal een Maeght meer <rhyme label="a" type="f" corresp="#s4:l1">cieren</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Dan de pracht van <rhyme label="b" type="f" corresp="#s4:l2">Kleeren</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">Deught sal meer <rhyme label="b" type="f" corresp="#s4:l2">vermeeren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Haer schoonheydt en <rhyme label="c" type="m">cieraedt</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Dan 't pralen // en <rhyme label="d" type="f">dralen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Dan 't strijcken // en <rhyme label="e" type="f">prijcken</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">En wulps <rhyme label="c" type="m" corresp="#s4:l6">ghelaet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Zions Maeghden <rhyme label="a" type="f">raeckten</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Heel in Godts <rhyme label="b" type="f">verachten</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Om dat sy 'er <rhyme label="a" type="f" corresp="#s5:l1">maeckten</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Soo veel vreemde <rhyme label="b" type="f" corresp="#s5:l2">drachten</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Ende noyt en <rhyme label="b" type="f" corresp="#s5:l2">trachten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Te kle'en haer naeckte <rhyme label="c" type="m">Ziel</rhyme>;</l>
        <l id="s5:l7" met="2" type="-+">Heur schroeven // en <rhyme label="d" type="f">toeven</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Heur brallen // en <rhyme label="e" type="f">mallen</rhyme></l>
        <l id="s5:l9" met="2" type="+-">Dee datse <rhyme label="c" type="m" corresp="#s5:l6">viel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Beter pracht <rhyme label="a" type="f">verlooren</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Dan de Ziel te <rhyme label="b" type="f">derven</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">Beter pronck <rhyme label="a" type="f" corresp="#s6:l1">verzwooren</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">En de Deught te <rhyme label="b" type="f" corresp="#s6:l2">erven</rhyme>;</l>
        <l id="s6:l5" met="3" type="+-">Deught die doet <rhyme label="b" type="f" corresp="#s6:l2">verwerven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Der Deuchden heyl en <rhyme label="c" type="m">loon</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Vriendinnen // wilt <rhyme label="d" type="f">minnen</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Wilt slaven // en <rhyme label="e" type="f">draven</rhyme></l>
        <l id="s6:l9" met="2" type="+-">Nae 's levens <rhyme label="c" type="m" corresp="#s6:l6">Kroon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
