<song>
  <meta>
    <id>177986</id>
    <date>
      <year-start>1674</year-start>
      <year-end>1674</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schone Cato</melody>
  <author>Daniel Bellemans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">DE soete <rhyme label="a" type="f">tyden</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Van den koelen <rhyme label="b" type="m">Mey</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Die ons <rhyme label="a" type="f" corresp="#s1:l1">verblyden</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Komen ons nu <rhyme label="b" type="m" corresp="#s1:l2">by</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Den Mey van <rhyme label="c" type="f">Deughden</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">En van <rhyme label="d" type="m">Heyligheydt</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Brenght voordts de <rhyme label="c" type="f" corresp="#s1:l5">vreughden</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Van ons <rhyme label="d" type="m" corresp="#s1:l6">Saligheydt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Den silv'ren <rhyme label="a" type="f">reghen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Van den koelen <rhyme label="b" type="m">Mey</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Brenghr meerder <rhyme label="a" type="f" corresp="#s2:l1">zeghen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Aen een dorre <rhyme label="b" type="m" corresp="#s2:l2">Wey</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">De <rhyme label="c" type="f">trane-plassen</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Van den jonghen <rhyme label="d" type="m">tydt</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Doen beter <rhyme label="c" type="f" corresp="#s2:l5">wassen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">'t Graen van <rhyme label="d" type="m" corresp="#s2:l6">Saligheydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">De Sonne - <rhyme label="a" type="f">stralen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Die met bly <rhyme label="b" type="m">ghelaet</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Op 't Aerdt-ryck <rhyme label="a" type="f" corresp="#s3:l1">dalen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">In den <rhyme label="b" type="m" corresp="#s3:l2">daghe-raet</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Zyn <rhyme label="c" type="f">aen-ghenamer</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Aen het jeughdigh <rhyme label="d" type="m">Kruyt</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">En veel <rhyme label="c" type="f" corresp="#s3:l5">bequamer</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Op dat 't Graen <rhyme label="d" type="m" corresp="#s3:l6">uyt-spruyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Waer schenckt m'aen <rhyme label="a" type="f">iemand</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Een verslenschte <rhyme label="b" type="m">Blom</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Een Blom die <rhyme label="a" type="f" corresp="#s4:l1">niemandt</rhyme></l>
        <l id="s4:l4" met="3" type="+-">En is <rhyme label="b" type="m" corresp="#s4:l2">willekom</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Sal ick Godt <rhyme label="c" type="f">gheven</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Een verslenschte <rhyme label="d" type="m">jeught</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Een oudt kout <rhyme label="c" type="f" corresp="#s4:l5">leven</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Dat niet meer en <rhyme label="d" type="m" corresp="#s4:l6">deught</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Gheen drooghe <rhyme label="a" type="f">Beenen</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Gheeft-men aen den <rhyme label="b" type="m">Hondt</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Sal ick dan <rhyme label="a" type="f" corresp="#s5:l1">eenen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Uyt-ghedrooghden <rhyme label="b" type="m" corresp="#s5:l2">grondt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Ouw' dorre <rhyme label="c" type="f">beenen</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Sonder jeught, oft <rhyme label="d" type="m">vreught</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Aen Godt <rhyme label="c" type="f" corresp="#s5:l5">verleenen</rhyme></l>
        <l id="s5:l8" met="3" type="+-">In plaets' van de <rhyme label="d" type="m" corresp="#s5:l6">jeught</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Sal ick <rhyme label="a" type="f">bewaren</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Voor Godts <rhyme label="b" type="m">Majesteyt</rhyme></l>
        <l id="s6:l3" met="2" type="-+">De dorre <rhyme label="a" type="f" corresp="#s6:l1">jaren</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Van den ouden <rhyme label="b" type="m" corresp="#s6:l2">tydt</rhyme>?</l>
        <l id="s6:l5" met="2" type="-+">Hoe sou 't <rhyme label="c" type="f">behaghen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Aen den <rhyme label="d" type="m">Bruydegom</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Hem op <rhyme label="c" type="f" corresp="#s6:l5">te-draghen</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Semels sonder <rhyme label="d" type="m" corresp="#s6:l6">Blom</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Eer 't Lyf om <rhyme label="a" type="f">stucken</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Is van <rhyme label="b" type="m">ouderdom</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Eer ick op <rhyme label="a" type="f" corresp="#s7:l1">Krucken</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Huppel om end' <rhyme label="b" type="m" corresp="#s7:l2">om</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Wil ick <rhyme label="c" type="f">beginnen</rhyme></l>
        <l id="s7:l6" met="3" type="+-">In myn jonghe <rhyme label="d" type="m">jeught</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Godt te <rhyme label="c" type="f" corresp="#s7:l5">beminnen</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Met de schoone <rhyme label="d" type="m" corresp="#s7:l6">deught</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Sullen <rhyme label="a" type="f">ruienen</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Godt zyn <rhyme label="b" type="m">toe-ghewyt</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Sal ick Godt <rhyme label="a" type="f" corresp="#s8:l1">dienen</rhyme></l>
        <l id="s8:l4" met="3" type="+-">In den ouden <rhyme label="b" type="m" corresp="#s8:l2">tydt</rhyme>?</l>
        <l id="s8:l5" met="2" type="-+">'k Wil Godt <rhyme label="c" type="f">toe-wyden</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Een nieuw' Kerck en <rhyme label="d" type="m">Huys</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Myn jonghe <rhyme label="c" type="f" corresp="#s8:l5">tyden</rhyme></l>
        <l id="s8:l8" met="3" type="+-">Gheven aen syn <rhyme label="d" type="m" corresp="#s8:l6">Kruys</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">'k Wil my Ã´ <rhyme label="a" type="f">Heere</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Mynen <rhyme label="b" type="m">Bruydegom</rhyme></l>
        <l id="s9:l3" met="2" type="-+">Van 's morghens <rhyme label="a" type="f" corresp="#s9:l1">keeren</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Met de <rhyme label="b" type="m" corresp="#s9:l2">Sonne-blom</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Ick wil u <rhyme label="c" type="f">jonnen</rhyme></l>
        <l id="s9:l6" met="3" type="+-">Mynen jonghen <rhyme label="d" type="m">tydt</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">O klare <rhyme label="c" type="f" corresp="#s9:l5">Sonne</rhyme></l>
        <l id="s9:l8" met="3" type="+-">Van <rhyme label="d" type="m" corresp="#s9:l6">Rechtveerdigheydt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Ick wil 't rys <rhyme label="a" type="f">booghen</rhyme></l>
        <l id="s10:l2" met="3" type="+-">In syn' jonghe <rhyme label="b" type="m">jeught</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Van jonghs myn' <rhyme label="a" type="f" corresp="#s10:l1">ooghen</rhyme></l>
        <l id="s10:l4" met="3" type="+-">Stieren tot de <rhyme label="b" type="m" corresp="#s10:l2">deught</rhyme></l>
        <l id="s10:l5" met="2" type="-+">Ick wil gaen <rhyme label="c" type="f">planten</rhyme></l>
        <l id="s10:l6" met="3" type="+-">Van jonghs voor myn <rhyme label="d" type="m">Huys</rhyme></l>
        <l id="s10:l7" met="2" type="-+">Den seer <rhyme label="c" type="f" corresp="#s10:l5">playsanten</rhyme></l>
        <l id="s10:l8" met="3" type="+-">Mey-boom van het <rhyme label="d" type="m" corresp="#s10:l6">Kruys</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
