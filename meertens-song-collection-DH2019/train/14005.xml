    <melody>Aan watervlieten Babylon</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DAt doch die Heer si ghemaect <rhyme label="a" type="m">groo</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Israhels Godt si <rhyme label="b" type="f">ghepresen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Die zijn Volck heeft in angst en <rhyme label="a" type="m" corresp="#s1:l1">noot</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Besocht, en verlost wt <rhyme label="b" type="f" corresp="#s1:l2">desen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">En den Hoorn des heyls <rhyme label="c" type="m">opgherecht</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Int Huys van Dauidt zijnen <rhyme label="c" type="m" corresp="#s1:l5">Knecht</rhyme></l>
        <l id="s1:l7" met="4" type="-+">So hy hadde te <rhyme label="d" type="f">vooren</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Door sine Heylighen Propheten <rhyme label="e" type="m">mont</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Wel voorseyt tot menigher <rhyme label="e" type="m" corresp="#s1:l8">stont</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Den Vaderen van hem <rhyme label="d" type="f" corresp="#s1:l7">vtuercoren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat wi souden wesen <rhyme label="a" type="m">beurijt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Van der macht onser <rhyme label="b" type="f">vyanden</rhyme></l>
        <l id="s2:l3" met="4" type="-+">En so ghemaeckt zijn nv ter <rhyme label="a" type="m" corresp="#s2:l1">tijt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Wt der onghunstigher <rhyme label="b" type="f" corresp="#s2:l2">handen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Op dat hy toonde zijn <rhyme label="c" type="m">ghoetheyt</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Onsen Vaderen <rhyme label="c" type="m" corresp="#s2:l5">toegheseyt</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Dat hi oock is <rhyme label="d" type="f">ghedachtich</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Sijns verbonts en ooc sijns eets <rhyme label="e" type="m">voortaen</rhyme></l>
        <l id="s2:l9" met="4" type="-+">Dien hi Abraham heeft <rhyme label="e" type="m" corresp="#s2:l8">ghedaen</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Onsen vader in tghelooue <rhyme label="d" type="f" corresp="#s2:l7">crachtich</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat wy vry zijnde vander <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Der ghener die ons seer <rhyme label="b" type="f">haten</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Hem souden zonder vrees of <rhyme label="a" type="m" corresp="#s3:l1">schant</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Dienen, en zijn Woort recht <rhyme label="b" type="f" corresp="#s3:l2">vaten</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Gherechtelick en <rhyme label="c" type="m">heylichlick</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Al ons leuen <rhyme label="c" type="m" corresp="#s3:l5">ghewillichlick</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Daer toe zijnde <rhyme label="d" type="f">gheneghen</rhyme></l>
        <l id="s3:l8" met="4" type="-+">En ghi mijn Soon sult een Propheet <rhyme label="e" type="m">zijn</rhyme></l>
        <l id="s3:l9" met="4" type="-+">Voor dat aensicht des Heeren <rhyme label="e" type="m" corresp="#s3:l8">mijn</rhyme></l>
        <l id="s3:l10" met="4" type="-+">Om voor hem te bereyden zijn <rhyme label="d" type="f" corresp="#s3:l7">weghen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat ghi den Volcke vroech en <rhyme label="a" type="m">spaet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Sijn salicheyt moecht <rhyme label="b" type="f">verconden</rhyme></l>
        <l id="s4:l3" met="4" type="-+">De welck in zonderheyt <rhyme label="a" type="m" corresp="#s4:l1">bestaet</rhyme></l>
        <l id="s4:l4" met="4" type="-+">In vergheuing zijner <rhyme label="b" type="f" corresp="#s4:l2">zonden</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Door Gods ghoetheyt die ons <rhyme label="c" type="m">alleyn</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Heeft als een Son wt tOoste <rhyme label="c" type="m" corresp="#s4:l5">reyn</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Besocht, en niet <rhyme label="d" type="f">vergheten</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Op dat hy mochte verlichten <rhyme label="e" type="m">claer</rhyme></l>
        <l id="s4:l9" met="4" type="-+">Die in die duyster schadu <rhyme label="e" type="m" corresp="#s4:l8">swaer</rhyme></l>
        <l id="s4:l10" met="4" type="-+">Des doots, dus langhe hebben <rhyme label="d" type="f" corresp="#s4:l7">gheseten</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
