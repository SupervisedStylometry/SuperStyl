  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">O Koningin, uyt herte, ziel, en <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Naest uwen Soon ick u het meest <rhyme label="a" type="m" corresp="#s1:l1">bemin</rhyme>:</l>
        <l id="s1:l3" met="3" type="+-">Weest gegroet, die <rhyme label="b" type="m">syt</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Al ons hoop' in <rhyme label="b" type="m" corresp="#s1:l3">stryt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Al ons troost, ons hulp, en <rhyme label="c" type="f">leven</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">Maria, Maeght, vol <rhyme label="d" type="m">soetigheydt</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">O Moeder van <rhyme label="d" type="m" corresp="#s1:l6">bermhertigheydt</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Aenhoort ons in <rhyme label="e" type="m">pyn</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Eva's kinders <rhyme label="e" type="m" corresp="#s1:l8">syn</rhyme></l>
        <l id="s1:l10" met="4" type="+-">In een Ballinghschap <rhyme label="c" type="f" corresp="#s1:l5">gedreven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Siet, hoe vol sucht ons herte tot u <rhyme label="a" type="m">vlucht</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Ah! neemt-doch-wegh ons wee, en <rhyme label="a" type="m" corresp="#s2:l1">ongenucht</rhyme>.</l>
        <l id="s2:l3" met="3" type="+-">Wy, van vreught <rhyme label="b" type="m">ontbloot</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Naer uw Moeders <rhyme label="b" type="m" corresp="#s2:l3">schoot</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Roepen in dit Dal der <rhyme label="c" type="f">traenen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Wel aen dan Maeght, ons <rhyme label="d" type="m">Patrooners</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Thoont, dat gy syt ons <rhyme label="d" type="m" corresp="#s2:l6">Voorspraekers</rhyme>;</l>
        <l id="s2:l8" met="2" type="-+">Uw oogh van <rhyme label="e" type="m">genaed'</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Op uw Kinders <rhyme label="e" type="m" corresp="#s2:l8">slaet</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Wilt des Hemels wegh hun <rhyme label="c" type="f" corresp="#s2:l5">baenen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Wanneer de doodt sal in den lesten <rhyme label="a" type="m">noodt</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Ons rucken uyt des Wereldts droeven <rhyme label="a" type="m" corresp="#s3:l1">schoot</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Thoont ons den <rhyme label="b" type="m">trap</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Naer dit <rhyme label="b" type="m" corresp="#s3:l3">Ballinghschap</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Om tot uwen Soon te <rhyme label="c" type="f">naeken</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Thoont Jesus ons, uw's Lichaems <rhyme label="d" type="m">Vrucht</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Doet ons te saem syn in <rhyme label="d" type="m" corresp="#s3:l6">genucht</rhyme>;</l>
        <l id="s3:l8" met="2" type="-+">O genaede <rhyme label="e" type="m">Maeght</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Die ons Liefde <rhyme label="e" type="m" corresp="#s3:l8">draeght</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Ey doet ons by u <rhyme label="c" type="f" corresp="#s3:l5">geraeken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
