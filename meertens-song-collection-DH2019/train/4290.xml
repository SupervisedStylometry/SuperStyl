<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4290</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Allemande nonnette</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="emotions">emoties</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Soetvloeyende <rhyme label="a" type="f">be-ke</rhyme> //</l>
        <l id="s1:l2" met="3" type="+-">Reyn speciale // <rhyme label="b" type="m">dracht</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Iont (troostbaer <rhyme label="a" type="f" corresp="#s1:l1">apote-ke</rhyme>) //</l>
        <l id="s1:l4" met="3" type="+-">Medecinale // <rhyme label="b" type="m" corresp="#s1:l2">cracht</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">En maect mijn quale // <rhyme label="b" type="m" corresp="#s1:l2">sacht</rhyme> //</l>
        <l id="s1:l6" met="3" type="-+">Want hier ter werelt <rhyme label="c" type="f">geene</rhyme> //</l>
        <l id="s1:l7" met="3" type="-+">En heeft dan ghy <rhyme label="c" type="f" corresp="#s1:l6">alleene</rhyme> //</l>
        <l id="s1:l8" met="3" type="-+">De principale // <rhyme label="b" type="m" corresp="#s1:l2">macht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoon lief, in wien ick <rhyme label="a" type="f">stille</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Heusche maniere // <rhyme label="b" type="m">vant</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Ontfermt (ist uwen <rhyme label="a" type="f" corresp="#s2:l1">wille</rhyme>)</l>
        <l id="s2:l4" met="3" type="+-">T'hert, dwelck in viere // <rhyme label="b" type="m" corresp="#s2:l2">brant</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">O schoon Lauriere // <rhyme label="b" type="m" corresp="#s2:l2">plant</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Al doet ghy my dees <rhyme label="c" type="f">smerte</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Noch schinck ick v mijn <rhyme label="c" type="f" corresp="#s2:l6">herte</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Ist niet een diere // <rhyme label="b" type="m" corresp="#s2:l2">pant</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dit pant, als cleen van <rhyme label="a" type="f">weerde</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Ghy luttel gade // <rhyme label="b" type="m">slaet</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Ick moet hemel // en <rhyme label="a" type="f" corresp="#s3:l1">eerde</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Clagen, dees quade // <rhyme label="b" type="m" corresp="#s3:l2">daet</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Lief, eens te rade // <rhyme label="b" type="m" corresp="#s3:l2">gaet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Of ghy geen troost zijt <rhyme label="c" type="f">schuldich</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">V dienaer, die <rhyme label="c" type="f" corresp="#s3:l6">gehuldich</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Naer v genade // <rhyme label="b" type="m" corresp="#s3:l2">staet</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al ist, dat ick <rhyme label="a" type="f">ontsinne</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Ick heb om clagen // <rhyme label="b" type="m">noot</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Want onvergolden <rhyme label="a" type="f" corresp="#s4:l1">minne</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Moet ick nu dragen // <rhyme label="b" type="m" corresp="#s4:l2">bloot</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Liever geslagen // <rhyme label="b" type="m" corresp="#s4:l2">doot</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dan te minnen met <rhyme label="c" type="f">pijne</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En niet gemint te <rhyme label="c" type="f" corresp="#s4:l6">zijne</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">T'is een mishagen // <rhyme label="b" type="m" corresp="#s4:l2">groot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wat baet dat aertsche <rhyme label="a" type="f">weelde</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Is om verwerven // <rhyme label="b" type="m">goet</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Als ghy (o schoonste <rhyme label="a" type="f" corresp="#s5:l1">beelde</rhyme>)</l>
        <l id="s5:l4" met="3" type="+-">Mijn herten sterven // <rhyme label="b" type="m" corresp="#s5:l2">doet</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Cruyt, noch conserven // <rhyme label="b" type="m" corresp="#s5:l2">soet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Connen geen droufheyt <rhyme label="c" type="f">scheuren</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Al s'weerelts vreught is <rhyme label="c" type="f" corresp="#s5:l6">treuren</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Als ick u deruen // <rhyme label="b" type="m" corresp="#s5:l2">moet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Princelick sal ick <rhyme label="a" type="f">blijuen</rhyme></l>
        <l id="s6:l2" met="3" type="+-">V dienaer euen // <rhyme label="b" type="m">fijn</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Al wildy my <rhyme label="a" type="f" corresp="#s6:l1">ontlijuen</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">En voor troost, geuen // <rhyme label="b" type="m" corresp="#s6:l2">brijn</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Coemt met verheuen // <rhyme label="b" type="m" corresp="#s6:l2">wijn</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En laeft my, o <rhyme label="c" type="f">Meestresse</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Want ghy sult mijn <rhyme label="c" type="f" corresp="#s6:l6">Princesse</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Nu, en naer t'leuen // <rhyme label="b" type="m" corresp="#s6:l2">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
