<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4184</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nog weet ik eens graven dochtertje</melody>
  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">AUrora sat in Phoebus <rhyme label="a" type="m">schoot</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Waar door Dyana van hem <rhyme label="a" type="m" corresp="#s1:l1">vlood</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En socht haar hoofd te <rhyme label="b" type="f">schuylen</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Als Daphne, die hy vurig <rhyme label="a" type="m" corresp="#s1:l1">bood</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Sijn Min an haar te <rhyme label="b" type="f" corresp="#s1:l3">ruylen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al-slapend, sach ik doen mijn <rhyme label="a" type="m">Lief</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die ik oyt in mijn hart <rhyme label="a" type="m" corresp="#s2:l1">verhief</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Dies wierd mijn ziel met lus <rhyme label="b" type="f">jes</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Bevangen, om te zijn een <rhyme label="a" type="m" corresp="#s2:l1">dief</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">En stal van haar drie <rhyme label="b" type="f" corresp="#s2:l3">kusjes</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Drie kusjes van haar <rhyme label="a" type="m">rode-mond</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">In die seer koele <rhyme label="a" type="m" corresp="#s3:l1">morgen-stond</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Waar voor sy heeft <rhyme label="b" type="f">gestolen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Mijn hart, 't geen eertijts was <rhyme label="a" type="m" corresp="#s3:l1">gesont</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En heden, my doet <rhyme label="b" type="f" corresp="#s3:l3">dolen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ik was een dief uyt <rhyme label="a" type="m">lustens-wil</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Sy wierd een dief, gerust, en <rhyme label="a" type="m" corresp="#s4:l1">stil</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Sy heeft sijn hart <rhyme label="b" type="f">genomen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Waar op een koude-vlamme <rhyme label="a" type="m" corresp="#s4:l1">vil</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die hy niet kon <rhyme label="b" type="f" corresp="#s4:l3">ontkomen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">ô! Over-soete kusjes, <rhyme label="a" type="m">drie</rhyme>:</l>
        <l id="s5:l2" met="4" type="-+">Een voor de mond, en d'ander <rhyme label="a" type="m" corresp="#s5:l1">twie</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Op bey haar <rhyme label="b" type="f">schone-wangen</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">(Die ik met sulken vreugd <rhyme label="a" type="m" corresp="#s5:l1">an-sie</rhyme>)</l>
        <l id="s5:l5" met="3" type="-+">Gy leyd my, als <rhyme label="b" type="f" corresp="#s5:l3">gevangen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">ô! Rode lipjes van <rhyme label="a" type="m">koraal</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die mijn besloten hart (in <rhyme label="a" type="m" corresp="#s6:l1">staal</rhyme>)</l>
        <l id="s6:l3" met="3" type="-+">So man'lijk hebt <rhyme label="b" type="f">verwonnen</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Door u schoonheden <rhyme label="a" type="m" corresp="#s6:l1">altemaal</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Is mijn verdriet <rhyme label="b" type="f" corresp="#s6:l3">begonnen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maar wakker zijnde, door 't <rhyme label="a" type="m">gekus</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">Sey ik tot haar, mijn <rhyme label="a" type="m" corresp="#s7:l1">lieve-sus</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Door u ben ik <rhyme label="b" type="f">geslagen</rhyme>:</l>
        <l id="s7:l4" met="4" type="-+">Om dat ik uwe lipjes, <rhyme label="a" type="m" corresp="#s7:l1">flus</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Kuste na mijn <rhyme label="b" type="f" corresp="#s7:l3">behagen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ik sach haar bruyne-kijkers <rhyme label="a" type="m">gaan</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Die als twee schone Starren <rhyme label="a" type="m" corresp="#s8:l1">staan</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Ja, die als fakkels <rhyme label="b" type="f">lichten</rhyme>:</l>
        <l id="s8:l4" met="4" type="-+">Waar voor men siet de <rhyme label="a" type="m" corresp="#s8:l1">silv're-Maan</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Als voor Apollo, <rhyme label="b" type="f" corresp="#s8:l3">swichten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Sy vlood voor my (in 't lopen <rhyme label="a" type="m">ras</rhyme>)</l>
        <l id="s9:l2" met="4" type="-+">Terstond, door 't nat bedaude <rhyme label="a" type="m" corresp="#s9:l1">gras</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Doch ik volgden haar <rhyme label="b" type="f">voetjes</rhyme>:</l>
        <l id="s9:l4" met="4" type="-+">Maar doen sy niet te krijgen <rhyme label="a" type="m" corresp="#s9:l1">was</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Ging ik in 't eynde <rhyme label="b" type="f" corresp="#s9:l3">soetjes</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">En scheyde van haar met een <rhyme label="a" type="m">lied</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Van het beginsel mijn's <rhyme label="a" type="m" corresp="#s10:l1">verdriet</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hoe dat ik door haar <rhyme label="b" type="f">lipjes</rhyme>)</l>
        <l id="s10:l4" met="4" type="-+">Eerst raakten onder haar <rhyme label="a" type="m" corresp="#s10:l1">gebiedt</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Als op der Minnen <rhyme label="b" type="f" corresp="#s10:l3">klipjes</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Vaart wel (mijn Lieve-lief) vaart <rhyme label="a" type="m">wel</rhyme>:</l>
        <l id="s11:l2" met="4" type="-+">Denkt om u dienaars groot <rhyme label="a" type="m" corresp="#s11:l1">gequel</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Waar mé hy is <rhyme label="b" type="f">beladen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Die an u soekt (met een <rhyme label="a" type="m" corresp="#s11:l1">op-stel</rhyme>)</l>
        <l id="s11:l5" met="3" type="-+">Sijn liefde te <rhyme label="b" type="f" corresp="#s11:l3">versaden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
