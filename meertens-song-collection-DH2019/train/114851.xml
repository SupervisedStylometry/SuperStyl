        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Phyllis me fait voir sans cesse</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Guilielmus Bolognino</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIe sal u ghenoech <rhyme label="a" type="f">verheffen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Edel France <rhyme label="b" type="m">Coninghin</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Die met al sagh <rhyme label="a" type="f" corresp="#s1:l1">overtreffen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">In volmaecktheydt van Godts <rhyme label="b" type="m" corresp="#s1:l2">min</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Die een lelie zijt <rhyme label="c" type="f">ghebleven</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Door het suyver <rhyme label="c" type="f" corresp="#s1:l5">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sien ick op u kindtse <rhyme label="a" type="f">daghen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Soo staen ick verwondert <rhyme label="b" type="m">seer</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat ick sien soo vroegh <rhyme label="a" type="f" corresp="#s2:l1">behaghen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">U in alle deught den <rhyme label="b" type="m" corresp="#s2:l2">Heer</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Wijs verr' boven uwe <rhyme label="c" type="f">jaren</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Manne-wercken <rhyme label="c" type="f" corresp="#s2:l5">baren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Out noch zijnde maer ses <rhyme label="a" type="f">jaren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Vierigh ghy Mariam <rhyme label="b" type="m">badt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Datsy u doch sou <rhyme label="a" type="f" corresp="#s3:l1">verclaren</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Wat sy voor het liefste <rhyme label="b" type="m" corresp="#s3:l2">hadt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Daer gh'aer meer soudt me <rhyme label="c" type="f">behaghen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Dit bleefdy haer <rhyme label="c" type="f" corresp="#s3:l5">vraeghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die u quam doen <rhyme label="a" type="f">openbaren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dat gh' een Orden stichten <rhyme label="b" type="m">soudt</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Tot haer eer. U in die <rhyme label="a" type="f" corresp="#s4:l1">jaren</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Heeft de Maeght soo verr' <rhyme label="b" type="m" corresp="#s4:l2">betrouwt</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Dit is u gheheel u <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s4:l6" met="3" type="+-">In den sin <rhyme label="c" type="f" corresp="#s4:l5">ghebleven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Tot dat is den tijdt <rhyme label="a" type="f">ghecomen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dat van u volbroght dit <rhyme label="b" type="m">werdt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Tot Godts bruydt nu <rhyme label="a" type="f" corresp="#s5:l1">opghenomen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Diep gheschreven in sijn <rhyme label="b" type="m" corresp="#s5:l2">hert</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Die u willen heeft <rhyme label="c" type="f">vergrooten</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Van Louis <rhyme label="c" type="f" corresp="#s5:l5">verstooten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Van u noch reyn maeght <rhyme label="a" type="f">ghescheyden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Om ghemeynd' <rhyme label="b" type="m">onvruchtbaerheydt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Waer Godt door sigh ghingh <rhyme label="a" type="f" corresp="#s6:l1">bereyden</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Kinders van de <rhyme label="b" type="m" corresp="#s6:l2">suyverheydt</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Die den hemel nu <rhyme label="c" type="f">vervullen</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Oft daer comen <rhyme label="c" type="f" corresp="#s6:l5">sullen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Veel hebt ghy altijt <rhyme label="a" type="f">gheleden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Seltsaem in <rhyme label="b" type="m">verduldicheydt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Met Godts will' altijt te <rhyme label="a" type="f" corresp="#s7:l1">vreden</rhyme></l>
        <l id="s7:l4" met="4" type="+-">In een diep' <rhyme label="b" type="m" corresp="#s7:l2">ootmoedicheydt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Veel u hert soo eel <rhyme label="c" type="f">gheboren</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Was in Godt <rhyme label="c" type="f" corresp="#s7:l5">verloren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Daer ghy 't nu hebt in <rhyme label="a" type="f">ghevonden</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Door een hoochster glories <rhyme label="b" type="m">licht</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Vast door liefd' aen hem <rhyme label="a" type="f" corresp="#s8:l1">ghebonden</rhyme></l>
        <l id="s8:l4" met="4" type="+-">In sijns schoonheyts claer <rhyme label="b" type="m" corresp="#s8:l2">ghesicht</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Daer 't van vreught sal eeuwigh <rhyme label="c" type="f">springhen</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Lof den Bruygom <rhyme label="c" type="f" corresp="#s8:l5">singhen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Wilt daer op u dochters <rhyme label="a" type="f">peysen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Krijght hen gratien van den <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Dat sy noot van hem en <rhyme label="a" type="f" corresp="#s9:l1">deysen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Maer 't Cruys voeren meer en <rhyme label="b" type="m" corresp="#s9:l2">meer</rhyme></l>
        <l id="s9:l5" met="4" type="+-">In hun herten all' hun <rhyme label="c" type="f">daghen</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Op de borst die 't <rhyme label="c" type="f" corresp="#s9:l5">draghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
