<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186319</id>
    <date>
      <year-start>1725</year-start>
      <year-end>1725</year-end>
    </date>
    <tags/>
    <melody>Prins Robberts mars</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Io Triumphe, Io, <rhyme label="a" type="m">Io</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Het ja-woord is <rhyme label="b" type="m">geseyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ex conjungale <rhyme label="c" type="m">vinculum</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En ’’t knoopje vast <rhyme label="b" type="m" corresp="#s1:l2">geleyt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Transfacta, tactaque, <rhyme label="d" type="m">omnia</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">De Bruyt is in de <rhyme label="e" type="m">schuyt</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Pudita <rhyme label="f" type="m">pertinatia</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Van weygeren heeft <rhyme label="e" type="m" corresp="#s1:l6">uyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Quae pectore <rhyme label="a" type="m">indomabilis</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En scheen zoo wreed als <rhyme label="b" type="m">kuys</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Fit tota, tota <rhyme label="c" type="m">amabilis</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">’’t Is alles dubbelt <rhyme label="b" type="m" corresp="#s2:l2">pluys</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Regina cordis <rhyme label="d" type="m">Regii</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Leyt ’’t hooft nu in de <rhyme label="e" type="m">schoot</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Blandeq; credit <rhyme label="f" type="m">Nuptia</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">En maak sig nu niet <rhyme label="e" type="m" corresp="#s2:l6">groot</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Amator &amp; <rhyme label="a" type="m">Amasia</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Zyn eens geworden <rhyme label="b" type="m">een</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Habentque, mentes <rhyme label="c" type="m">membraque</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En alle ding <rhyme label="b" type="m" corresp="#s3:l2">gemeen</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">O suave <rhyme label="d" type="m">contubernium</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van onderlinge <rhyme label="e" type="m">brand</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">O ter beata <rhyme label="f" type="m">copula</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van onderlinge <rhyme label="e" type="m" corresp="#s3:l6">brand</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ocellus illa virginum <rhyme label="a" type="m">est</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Die hem nu liever <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Suis ocellis <rhyme label="c" type="m">Proprijs</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Als alles wat ’’er <rhyme label="b" type="m" corresp="#s4:l2">leeft</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Hac non Venus <rhyme label="d" type="m">venustrior</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Als die op ’’t lieflijkst <rhyme label="e" type="m">lonkt</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Non gratiores <rhyme label="f" type="m">Gratiz</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Al zyn zy <rhyme label="e" type="m" corresp="#s4:l6">opgepronkt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Non vultus <rhyme label="a" type="m">emendatior</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Is aan het jonge <rhyme label="b" type="m">Wijf</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Quae Chyntii <rhyme label="c" type="m">praenuncia</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Vroeg reyst van Chitons <rhyme label="b" type="m" corresp="#s5:l2">lijf</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Amasia quae Virgo <rhyme label="d" type="m">nunc</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met zyne toenaam <rhyme label="e" type="m">praalt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Nec, credo; furget, <rhyme label="f" type="m">antequam</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Zy alles dan <rhyme label="e" type="m" corresp="#s5:l6">inhaalt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Labella Pithus, <rhyme label="a" type="m">osculum</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Daar in de Nectar <rhyme label="b" type="m">woont</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Gena rubentes <rhyme label="c" type="m">purpura</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Daar hy sig ’’t snee-wit <rhyme label="b" type="m" corresp="#s6:l2">toont</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Pectuaculum <rhyme label="d" type="m">serorians</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Een zuyv’’re zwane <rhyme label="e" type="m">hals</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Nunc trader illa <rhyme label="f" type="m">Conjungi</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En soetigheid van <rhyme label="e" type="m" corresp="#s6:l6">als</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nunc ex maniplo <rhyme label="a" type="m">Virginum</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Moet zy, moet zy <rhyme label="b" type="m">gerukt</rhyme>;</l>
        <l id="s7:l3" met="4" type="-+">Nunc vernus ille <rhyme label="c" type="m">flosculus</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Moet werden <rhyme label="b" type="m" corresp="#s7:l2">afgeplukt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">In hortuso <rhyme label="d" type="m">Cupidinis</rhyme>.</l>
        <l id="s7:l6" met="3" type="-+">Die draagt een vreugde <rhyme label="e" type="m">schat</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Ritus josusque, quem <rhyme label="f" type="m">colit</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En ik en weet niet <rhyme label="e" type="m" corresp="#s7:l6">wat</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Euge, o gemelli <rhyme label="a" type="m">turtures</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Houd so ineer en <rhyme label="b" type="m">deugt</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Cerrame asque <rhyme label="c" type="m">induocijs</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">En oeffent zo u <rhyme label="b" type="m" corresp="#s8:l2">jeugt</rhyme>;</l>
        <l id="s8:l5" met="4" type="-+">Idem puella multa <rhyme label="d" type="m">ager</rhyme>.</l>
        <l id="s8:l6" met="3" type="-+">En menig <rhyme label="e" type="m">Vryer-man</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Puella multa cum <rhyme label="f" type="m">volet</rhyme></l>
        <l id="s8:l8" met="3" type="-+">De Vryer als hy <rhyme label="e" type="m" corresp="#s8:l6">kan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
