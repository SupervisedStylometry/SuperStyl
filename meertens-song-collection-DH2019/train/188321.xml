    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Liefde die van alle tijden</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KLoris die met uw bruin <rhyme label="a" type="f">Oogen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Duizend Minnegoodjes <rhyme label="b" type="m">teeldt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die my boeit aan uw <rhyme label="a" type="f" corresp="#s1:l1">vermogen</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">En met Kusjes schaars <rhyme label="b" type="m" corresp="#s1:l2">bedeeldt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">O! twede <rhyme label="c" type="f">Leven</rhyme>!</l>
        <l id="s1:l6" met="2" type="-+">Kom in deez <rhyme label="c" type="f" corresp="#s1:l5">dreeven</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Daar 't Pluimgediert wel lustig <rhyme label="b" type="m" corresp="#s1:l2">speelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zie hoe duizend Paarlen <rhyme label="a" type="f">dropjes</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Rollen langs de Bloempjes <rhyme label="b" type="m">heen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Hoe de ontlooken <rhyme label="a" type="f" corresp="#s2:l1">Roze-knopjes</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Sierlyk pronken onder <rhyme label="b" type="m" corresp="#s2:l2">een</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Og! kom myn <rhyme label="c" type="f">Engel</rhyme>!</l>
        <l id="s2:l6" met="2" type="-+">Op dat ik <rhyme label="c" type="f" corresp="#s2:l5">strengel</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Myn armen om uw blanke <rhyme label="b" type="m" corresp="#s2:l2">Leen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wilt natuur te hulpe <rhyme label="a" type="f">koomen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wyl de Zon zyn togt <rhyme label="b" type="m">begint</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Doet het zwygend beekje <rhyme label="a" type="f" corresp="#s3:l1">Stroomen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Teeld de lieve <rhyme label="b" type="m" corresp="#s3:l2">Zuidewind</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Stort van uw <rhyme label="c" type="f">Lipjes</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Langs de Marmre <rhyme label="c" type="f" corresp="#s3:l5">klipjes</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">De Nektar voor uw Harte <rhyme label="b" type="m" corresp="#s3:l2">vrind</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dan zal 't Nagtegaaltje <rhyme label="a" type="f">kweelen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">U ter eer een Englen <rhyme label="b" type="m">wys</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">'t Duifje trekkebekkend <rhyme label="a" type="f" corresp="#s4:l1">Speelen</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Alles Juichen u ten <rhyme label="b" type="m" corresp="#s4:l2">prys</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Kom tweede <rhyme label="c" type="f">leven</rhyme>!</l>
        <l id="s4:l6" met="2" type="-+">Herschept dees <rhyme label="c" type="f" corresp="#s4:l5">dreeven</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">In een wellustig <rhyme label="b" type="m" corresp="#s4:l2">Paradys</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
