  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">GLorie sy Godt inden <rhyme label="a" type="f">hoochsten</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Ende inder aerden vrede den <rhyme label="b" type="f">menschen</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Die van goeden wille <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Wy loven u wy ghebenedijen <rhyme label="d" type="m">u</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wy aen bidden u, ende wy prijsen <rhyme label="d" type="m" corresp="#s1:l4">u</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wy dancken u om u groote <rhyme label="e" type="f">glorien</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Heere Godt Coninc der hemelen Godt des <rhyme label="f" type="f">vaders</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Ghy zijt <rhyme label="g" type="f">almachtich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Heere ghy sijt een eenich geboren sone Jesu <rhyme label="a" type="f">Christe</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Heere Godt, dat Lam Gods Sone des <rhyme label="b" type="f">Vaders</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die af neemt de sonden des <rhyme label="c" type="m">werelts</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Ontfermt u <rhyme label="d" type="m">onser</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Die af neemt de sonden des <rhyme label="d" type="m" corresp="#s2:l4">werelts</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Ontfanght al ons <rhyme label="e" type="f">bidden</rhyme>,</l>
        <l id="s2:l7" met="5" type="-+">Die daer sidt aen de recht handt des <rhyme label="f" type="f">Vaders</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Ontfermt u <rhyme label="g" type="f">onser</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
