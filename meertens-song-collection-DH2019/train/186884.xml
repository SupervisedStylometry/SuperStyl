<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186884</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Psalm 081 Datheen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Dwang en <rhyme label="a" type="m">Slaverney</rhyme>!</l>
        <l id="s1:l2" met="3" type="+-">Naeuw verbonden <rhyme label="b" type="f">Steeden</rhyme>;</l>
        <l id="s1:l3" met="3" type="+-">Vry van <rhyme label="a" type="m" corresp="#s1:l1">Monarchy</rhyme>!</l>
        <l id="s1:l4" met="3" type="+-">Hebt gy dog <rhyme label="b" type="f" corresp="#s1:l2">geleeden</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Te lang ô <rhyme label="c" type="m">Holland</rhyme>!</l>
        <l id="s1:l6" met="3" type="+-">Heeft d'Engelschen <rhyme label="c" type="m" corresp="#s1:l5">band</rhyme>;</l>
        <l id="s1:l7" met="3" type="+-">En Kromwels <rhyme label="d" type="f">Tractaeten</rhyme>;</l>
        <l id="s1:l8" met="3" type="+-">Belemmert ons <rhyme label="c" type="m" corresp="#s1:l5">Strand</rhyme>;</l>
        <l id="s1:l9" met="3" type="+-">Wilt die vry <rhyme label="d" type="f" corresp="#s1:l7">Verlaaten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Herleeft <rhyme label="a" type="m">Koopmanschap</rhyme>!</l>
        <l id="s2:l2" met="3" type="+-">Troost der <rhyme label="b" type="f">Batavieren</rhyme>!</l>
        <l id="s2:l3" met="3" type="+-">Laet ons Helden <rhyme label="a" type="m" corresp="#s2:l1">rap</rhyme>!</l>
        <l id="s2:l4" met="3" type="+-">Het alzo <rhyme label="b" type="f" corresp="#s2:l2">bestieren</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Als Ruyter <rhyme label="c" type="m">voorheen</rhyme>!</l>
        <l id="s2:l6" met="3" type="+-">En Tromp <rhyme label="c" type="m" corresp="#s2:l5">ongemeen</rhyme>!</l>
        <l id="s2:l7" met="3" type="+-">Tot ons 's Lands <rhyme label="d" type="f">welvaeren</rhyme>!</l>
        <l id="s2:l8" met="3" type="+-">Uyt Heldenmoed <rhyme label="c" type="m" corresp="#s2:l5">deën</rhyme>!</l>
        <l id="s2:l9" met="3" type="+-">Getuygt dit ô <rhyme label="d" type="f" corresp="#s2:l7">Baeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">'t Gaet wel, <rhyme label="a" type="m">dwingeland</rhyme>!</l>
        <l id="s3:l2" met="3" type="+-">Ons kleyn aental <rhyme label="b" type="f">Scheepen</rhyme>!</l>
        <l id="s3:l3" met="3" type="+-">Bestiert met <rhyme label="a" type="m" corresp="#s3:l1">Verstand</rhyme>!</l>
        <l id="s3:l4" met="3" type="+-">Zullen d'uwe <rhyme label="b" type="f" corresp="#s3:l2">Sleepen</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Als Ruyter <rhyme label="c" type="m">voorheen</rhyme>!</l>
        <l id="s3:l6" met="3" type="+-">En Tromp <rhyme label="c" type="m" corresp="#s3:l5">Ongemeen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Of ook wel van <rhyme label="d" type="f">Gaalen</rhyme>!</l>
        <l id="s3:l8" met="3" type="+-">In haar eygen <rhyme label="c" type="m" corresp="#s3:l5">Rheên</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">U Wisten te <rhyme label="d" type="f" corresp="#s3:l7">haalen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Hollands <rhyme label="a" type="m">Admiraal</rhyme>.</l>
        <l id="s4:l2" met="3" type="+-">Stamhouder dier <rhyme label="b" type="f">Helden</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Die zoo <rhyme label="a" type="m" corresp="#s4:l1">meenigmaal</rhyme>.</l>
        <l id="s4:l4" met="3" type="+-">Ons Vryheid <rhyme label="b" type="f" corresp="#s4:l2">herstelden</rhyme>!</l>
        <l id="s4:l5" met="3" type="+-">Godt geeft U dien <rhyme label="c" type="m">moed</rhyme>;</l>
        <l id="s4:l6" met="3" type="+-">Die in 't Nassaus <rhyme label="c" type="m" corresp="#s4:l5">bloed</rhyme>;</l>
        <l id="s4:l7" met="3" type="+-">Zoo Adelyk <rhyme label="d" type="f">Stroomden</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Dien Ouden <rhyme label="c" type="m" corresp="#s4:l5">Voorspoed</rhyme>!</l>
        <l id="s4:l9" met="3" type="+-">Daer Vyand voor <rhyme label="d" type="f" corresp="#s4:l7">Schroomden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
