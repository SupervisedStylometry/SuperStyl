    <melody>J'aimerai en ce village</melody>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">IEugt laet vreugt nu <rhyme label="a" type="f">blijcken</rhyme>:</l>
        <l id="s1:l2" met="3" type="+-">Phebus heeft met <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Boream doen <rhyme label="a" type="f" corresp="#s1:l1">wijcken</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Dies de Lenten <rhyme label="b" type="m" corresp="#s1:l2">lacht</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Siende oock des <rhyme label="c" type="f">kouden</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Winters'fel bedwanck// te <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Daerom in de <rhyme label="c" type="f" corresp="#s1:l5">Wouden</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Den Victory sanck// <rhyme label="d" type="m" corresp="#s1:l6">geschiet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Iuno heeft den <rhyme label="a" type="f">Gordel</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Van Venus <rhyme label="b" type="m">ontleent</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Iupiter tot <rhyme label="a" type="f" corresp="#s2:l1">vordeel</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Is met haer <rhyme label="b" type="m" corresp="#s2:l2">vereent</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Vesta nae 't <rhyme label="c" type="f">betamen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Doet op haren wijden// <rhyme label="d" type="m">schoot</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Dies de Nimphen <rhyme label="c" type="f" corresp="#s2:l5">t'samen</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Leven in verblijden// <rhyme label="d" type="m" corresp="#s2:l6">groot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Driades <rhyme label="a" type="f">Godinnen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Worden nieuw <rhyme label="b" type="m">ghekroont</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">In't bewijs van <rhyme label="a" type="f" corresp="#s3:l1">Minnen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Tot Adonis <rhyme label="b" type="m" corresp="#s3:l2">toont</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Venus soet van <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Haer bloedt aen de Roosen// <rhyme label="d" type="m">roodt</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Al dede Mars <rhyme label="c" type="f" corresp="#s3:l5">desen</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Lijden een soo boosen// <rhyme label="d" type="m" corresp="#s3:l6">doodt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Faunen en <rhyme label="a" type="f">Silvanen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Hebben wilde <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Verse koele <rhyme label="a" type="f" corresp="#s4:l1">Tranen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Lieflijck en vol <rhyme label="b" type="m" corresp="#s4:l2">deught</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">D'alderschoonste <rhyme label="c" type="f">Flora</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Des morghens ontfanghen// <rhyme label="d" type="m">doet</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Als de schoon <rhyme label="c" type="f" corresp="#s4:l5">Aurora</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Komt met roode Wanghen// <rhyme label="d" type="m" corresp="#s4:l6">soet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Zyphir Aura <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Blasen koeltjes <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Napeen hebben <rhyme label="a" type="f" corresp="#s5:l1">vrede</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Met ghebloemt en <rhyme label="b" type="m" corresp="#s5:l2">kruydt</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">En de Borne <rhyme label="c" type="f">Morren</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Loopen af de steenen// <rhyme label="d" type="m">langhst</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Voor Phetons <rhyme label="c" type="f" corresp="#s5:l5">verdorren</rhyme></l>
        <l id="s5:l8" met="4" type="-+">En is nu oock gheenen// <rhyme label="d" type="m" corresp="#s5:l6">anghst</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Philomela <rhyme label="a" type="f">kele</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Hoortmen nacht en <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Echo volght soo <rhyme label="a" type="f" corresp="#s6:l1">vele</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Als zy immers <rhyme label="b" type="m" corresp="#s6:l2">magh</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">In Bosschen en <rhyme label="c" type="f">Rotsen</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Men haer doen het beste// <rhyme label="d" type="m">hoort</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Stemme nae te <rhyme label="c" type="f" corresp="#s6:l5">botsen</rhyme></l>
        <l id="s6:l8" met="4" type="-+">Zy houdt gaerne 't leste// <rhyme label="d" type="m" corresp="#s6:l6">woordt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Ceres drijft en <rhyme label="a" type="f">varet</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Met haer groen <rhyme label="b" type="m">ghewas</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Op het Veldt al <rhyme label="a" type="f" corresp="#s7:l1">waret</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Den Nephtunus <rhyme label="b" type="m" corresp="#s7:l2">plas</rhyme>:</l>
        <l id="s7:l5" met="3" type="+-">En Eolus <rhyme label="c" type="f">Knapen</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Als broodt-droncke Wichten// <rhyme label="d" type="m">snel</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Schijnen vreugt te <rhyme label="c" type="f" corresp="#s7:l5">rapen</rhyme></l>
        <l id="s7:l8" met="4" type="-+">Hier in veel te stichten// <rhyme label="d" type="m" corresp="#s7:l6">spel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">In desen tijt <rhyme label="a" type="f">lustigh</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Door Cypris <rhyme label="b" type="m">bestier</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Elck gedient <rhyme label="a" type="f" corresp="#s8:l1">onrustigh</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Voelt der liefden <rhyme label="b" type="m" corresp="#s8:l2">vier</rhyme>:</l>
        <l id="s8:l5" met="3" type="+-">Maer begaeft met <rhyme label="c" type="f">reden</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Mensch u wel te letten// <rhyme label="d" type="m">staet</rhyme></l>
        <l id="s8:l7" met="3" type="+-">In oprechte <rhyme label="c" type="f" corresp="#s8:l5">zeden</rhyme></l>
        <l id="s8:l8" met="4" type="-+">T'houden vast aen wet en// <rhyme label="d" type="m" corresp="#s8:l6">maet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Denckt hoe dat veel <rhyme label="a" type="f">blomen</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Door verliefde <rhyme label="b" type="m">jeugt</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Zijn te voorschijn <rhyme label="a" type="f" corresp="#s9:l1">komen</rhyme>:</l>
        <l id="s9:l4" met="3" type="+-">Dus ghy leeren <rhyme label="b" type="m" corresp="#s9:l2">meugt</rhyme></l>
        <l id="s9:l5" met="3" type="+-">V gedachten <rhyme label="c" type="f">wennen</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Want u oock gegeven <rhyme label="d" type="m">wort</rhyme></l>
        <l id="s9:l7" met="3" type="+-">Door't gebloemt te <rhyme label="c" type="f" corresp="#s9:l5">kennen</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">'t Voorspel van u leven// <rhyme label="d" type="m" corresp="#s9:l6">kort</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">So PoÃ«ten <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Liefd' is blint en <rhyme label="b" type="m">mal</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Sy is yders <rhyme label="a" type="f" corresp="#s10:l1">dingen</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Sy verwind het <rhyme label="b" type="m" corresp="#s10:l2">al</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Maer is <rhyme label="c" type="f">ongestadigh</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Vol tranen en swaer// <rhyme label="d" type="m">gesucht</rhyme>,</l>
        <l id="s10:l7" met="3" type="+-">Met veel smerten <rhyme label="c" type="f" corresp="#s10:l5">schadigh</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">Zijnde meest een paer// <rhyme label="d" type="m" corresp="#s10:l6">bevrucht</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Het mocht den <rhyme label="a" type="f">Cupido</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Wel vervloecken <rhyme label="b" type="m">seer</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">Ariadne, <rhyme label="a" type="f" corresp="#s11:l1">Dido</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">En veel duysent <rhyme label="b" type="m" corresp="#s11:l2">meer</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">Die door <rhyme label="c" type="f">ontgetrouwe</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Minnaers, vol bedrogh// en <rhyme label="d" type="m">list</rhyme>,</l>
        <l id="s11:l7" met="3" type="+-">Komen zijn in <rhyme label="c" type="f" corresp="#s11:l5">rouwe</rhyme>,</l>
        <l id="s11:l8" met="4" type="-+">'t Welck niet al oock noch// en <rhyme label="d" type="m" corresp="#s11:l6">mist</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Edel reyn <rhyme label="a" type="f">Princessen</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">Vry van hart en <rhyme label="b" type="m">mont</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">D'alderbeste <rhyme label="a" type="f" corresp="#s12:l1">lessen</rhyme></l>
        <l id="s12:l4" met="3" type="+-">Die ghy leeren <rhyme label="b" type="m" corresp="#s12:l2">kont</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">Weest altijt <rhyme label="c" type="f">voorsichtigh</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Blijft van Heymnes beter// <rhyme label="d" type="m">vry</rhyme>,</l>
        <l id="s12:l7" met="3" type="+-">Sonder trouwen, <rhyme label="c" type="f" corresp="#s12:l5">lichtigh</rhyme>,</l>
        <l id="s12:l8" met="4" type="-+">Als het laten beter// <rhyme label="d" type="m" corresp="#s12:l6">zy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
