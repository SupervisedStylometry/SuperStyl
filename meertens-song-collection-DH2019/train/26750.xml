  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Nu laetstu dynen <rhyme label="a" type="m">knecht</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In vrede Heer gaen <rhyme label="a" type="m" corresp="#s1:l1">gherecht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Uolghende  dynen <rhyme label="b" type="f">worde</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Nae dat myn ooghen <rhyme label="c" type="m">vry</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Deelachtich syn doer <rhyme label="c" type="m" corresp="#s1:l4">dy</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dyns salicheyts <rhyme label="b" type="f" corresp="#s1:l3">gheworden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die Du hebst <rhyme label="a" type="m">toegheseyt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">End ionstelick <rhyme label="a" type="m" corresp="#s2:l1">bereydt</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Uoer dâ€™anschyn aller <rhyme label="b" type="f">volcken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Een licht der Heydens <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">End een eere <rhyme label="c" type="m" corresp="#s2:l4">openbaer</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Israels dyns <rhyme label="b" type="f" corresp="#s2:l3">verkoren</rhyme>.</l>
      </lg>
    </lg>
