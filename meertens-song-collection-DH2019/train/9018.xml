  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Siele looft den Heere <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die met Cieraet soo <rhyme label="b" type="f">Leuchtet</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Syn Heerlickheyt in s'Hemels <rhyme label="a" type="m" corresp="#s1:l1">Throon</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Syn Goetheyt d'Aerd' <rhyme label="b" type="f" corresp="#s1:l2">beveuchtet</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Datse breng Vrucht op Berg' en <rhyme label="c" type="m">Dal</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Cruyt, Coren ende Boomen <rhyme label="c" type="m" corresp="#s1:l5">al</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ten dienst den Vee end <rhyme label="d" type="f">Menschen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Heer ghy laet Broot all' ons te <rhyme label="a" type="m">goed'</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Opwassen wt der <rhyme label="b" type="f">Aerden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Oock Wijn die maecket blijden <rhyme label="a" type="m" corresp="#s2:l1">Moed</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">D'Oly laet ghy ons <rhyme label="b" type="f" corresp="#s2:l2">weerden</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dijn wonder wercken over <rhyme label="c" type="m">al</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Dijn rijcke Gaeven sonder <rhyme label="c" type="m" corresp="#s2:l5">Tal</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Vervullen alle <rhyme label="d" type="f">Landen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">All' Creatuer wachten op <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat ghy haer ghevet <rhyme label="b" type="f">Spijse</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Want als ghy gheeft soo nemen <rhyme label="a" type="m" corresp="#s3:l1">sy</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als ghy dijn Handt haer <rhyme label="b" type="f" corresp="#s3:l2">wijset</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Soo wordense versaet met <rhyme label="c" type="m">Goedt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Maer machtloos end heel <rhyme label="c" type="m" corresp="#s3:l5">onghemoedt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Als ghy dijn G'naed' <rhyme label="d" type="f">verberghet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Soo haest als ghy haer Geest <rhyme label="a" type="m">afweynt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Tot Stoff sy moeten <rhyme label="b" type="f">weerden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">End als ghy dijnen Geest wt <rhyme label="a" type="m" corresp="#s4:l1">seynt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Vernieuwet wordt die <rhyme label="b" type="f" corresp="#s4:l2">Aerde</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Den Heeren al myn leven <rhyme label="c" type="m">lang'</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Wil loven ick met mynen <rhyme label="c" type="m" corresp="#s4:l5">sang</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">End singhen <rhyme label="d" type="f">Alleluja</rhyme></l>
      </lg>
    </lg>
  </text>
