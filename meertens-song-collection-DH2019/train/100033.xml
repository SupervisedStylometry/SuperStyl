<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100033</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Daar lag haar zieltje uitgestrekt</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">CIpresse Venus sonder <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s1:l2" met="6" type="-+">Dewijl mijn Lief u Soon Cupido heeft <rhyme label="a" type="m" corresp="#s1:l1">verkracht</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Doe hy in't Gras heel doof van <rhyme label="b" type="m">lust</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Zijn Geesjen <rhyme label="b" type="m" corresp="#s1:l3">rust</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">En na de koelten vande winden <rhyme label="a" type="m" corresp="#s1:l1">wacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nam zy zijn banden t'sijner <rhyme label="a" type="m">schandt</rhyme>,</l>
        <l id="s2:l2" met="6" type="-+">Die sy noch vast aen zijn Pijl-Koocker <rhyme label="a" type="m" corresp="#s2:l1">vandt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En bondt zijn Vleugels handen <rhyme label="b" type="m">t'saem</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Hy <rhyme label="b" type="m" corresp="#s2:l3">onbequaem</rhyme></l>
        <l id="s2:l5" met="5" type="-+">Zijn Godsheyt door zijn eygen sagh <rhyme label="a" type="m" corresp="#s2:l1">verwant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ontwakent knielt hy voor haer <rhyme label="a" type="m">neer</rhyme>,</l>
        <l id="s3:l2" met="6" type="-+">En bad demoedigh om zijn eerste vryheyt <rhyme label="a" type="m" corresp="#s3:l1">weer</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Maer zy en acht den Lecker <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Maer van hem <rhyme label="b" type="m" corresp="#s3:l3">vliet</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">En liet den Bengel leggen in't <rhyme label="a" type="m" corresp="#s3:l1">verdriet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zy die sijn macht en Pijl <rhyme label="a" type="m">beheert</rhyme>,</l>
        <l id="s4:l2" met="6" type="-+">Zy kent de kracht, zy doet al 'tgene zy <rhyme label="a" type="m" corresp="#s4:l1">begeert</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Geen Cupido nu meerder <rhyme label="b" type="m">woedt</rhyme>.</l>
        <l id="s4:l4" met="2" type="-+">En scheutjes <rhyme label="b" type="m" corresp="#s4:l3">doet</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">Dat ick op haer Altaar dus stoocken <rhyme label="a" type="m" corresp="#s4:l1">moet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wegh, wegh, verloren blinden <rhyme label="a" type="m">sot</rhyme>,</l>
        <l id="s5:l2" met="6" type="-+">Want mijn beminden stelt my nu haer soet <rhyme label="a" type="m" corresp="#s5:l1">gebodt</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Zy voert mijn Geesje waer zy <rhyme label="b" type="m">wil</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Al swijgh ick <rhyme label="b" type="m" corresp="#s5:l3">stil</rhyme>:</l>
        <l id="s5:l5" met="5" type="-+">Vermits ghy al seen O in't Cijffer <rhyme label="a" type="m" corresp="#s5:l1">vil</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Vliegh Fama vlieght en blaest haer <rhyme label="a" type="m">lof</rhyme></l>
        <l id="s6:l2" met="6" type="-+">In elckers ooren, want ghy hebt gewisse <rhyme label="a" type="m" corresp="#s6:l1">stof</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Maeckt dat geen Minnaer zijn <rhyme label="b" type="m">gebeen</rhyme></l>
        <l id="s6:l4" met="2" type="-+">En droef <rhyme label="b" type="m" corresp="#s6:l3">gesteen</rhyme>,</l>
        <l id="s6:l5" met="5" type="-+">Aen dees beroyden Schutter gaet <rhyme label="a" type="m" corresp="#s6:l1">ontleen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
