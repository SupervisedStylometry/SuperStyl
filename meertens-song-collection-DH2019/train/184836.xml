    <melody>Chantons l'heureuse naissance</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SOete Bosschen, still' <rhyme label="a" type="f">Valleyen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Liefste plaets van <rhyme label="b" type="m">eenigheydt</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Myne rust doet gy <rhyme label="a" type="f" corresp="#s1:l1">verbreyen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">In dees blyde <rhyme label="b" type="m" corresp="#s1:l2">stilligheydt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Ah! den vrede van myn <rhyme label="c" type="f">herte</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Brenght my hier t'geluckigh <rhyme label="d" type="m">lot</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">Verr' van my dryft alle <rhyme label="c" type="f" corresp="#s1:l5">smerte</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Ick leef hier voor mynen <rhyme label="d" type="m" corresp="#s1:l6">Godt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Komt de guld' Aurora <rhyme label="a" type="f">schynen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Is tot Godt myn eerst' <rhyme label="b" type="m">gedacht</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">t'Is die sterr', die doet <rhyme label="a" type="f" corresp="#s2:l1">verdwynen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Al de duysterheydt der <rhyme label="b" type="m" corresp="#s2:l2">nacht</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Hoor ick singen <rhyme label="c" type="f">Nachtergaelen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Nooden sy my op myn <rhyme label="d" type="m">keer</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Hunne soete stem <rhyme label="c" type="f" corresp="#s2:l5">t'herhaelen</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Van de liefde tot den <rhyme label="d" type="m" corresp="#s2:l6">Heer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Het gedruys der soete <rhyme label="a" type="f">beken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dat my hier vermaeckt al <rhyme label="b" type="m">uur'</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Doet met hun tot Godt my <rhyme label="a" type="f" corresp="#s3:l1">spreken</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Syt gelooft, Heer van <rhyme label="b" type="m" corresp="#s3:l2">natuer</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Als ick onder blaeders <rhyme label="c" type="f">trede</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Daer den windt mackt soet <rhyme label="d" type="m">gerucht</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Voegh ick een gedruys daer <rhyme label="c" type="f" corresp="#s3:l5">mede</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Naer den Hemel klimt myn <rhyme label="d" type="m" corresp="#s3:l6">sucht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sien ick eenen boom ter <rhyme label="a" type="f">syden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Die vol vruchten staet en <rhyme label="b" type="m">bloeydt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Gaen ick Godt <rhyme label="a" type="f" corresp="#s4:l1">gebenedyden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Door wiens goetheydt alles <rhyme label="b" type="m" corresp="#s4:l2">groeydt</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Als de Sonn' gaet <rhyme label="c" type="f">onder-daelen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Noodt sy my oock tot de <rhyme label="d" type="m">rust</rhyme>:</l>
        <l id="s4:l7" met="4" type="+-">My den soeten slaep komt <rhyme label="c" type="f" corresp="#s4:l5">haelen</rhyme>;</l>
        <l id="s4:l8" met="4" type="+-">d'Eenigheydt geeft al wat <rhyme label="d" type="m" corresp="#s4:l6">lust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
