    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEt out van 't groen <rhyme label="a" type="m">ontwent</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Voelt nu een nieuwe <rhyme label="a" type="m" corresp="#s1:l1">Lent</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dees onghemeyne <rhyme label="b" type="m">vreught</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Kan Bes, en <rhyme label="c" type="m">Bestevaar</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Doen hebben nieuwe <rhyme label="b" type="m" corresp="#s1:l3">jeught</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In 't spyt hun gryse <rhyme label="c" type="m" corresp="#s1:l4">hair</rhyme>.</l>
      </lg>
    </lg>
