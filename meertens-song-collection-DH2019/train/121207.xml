<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>121207</id>
    <date>
      <year-start>1786</year-start>
      <year-end>1786</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Malbrough s'en va-t-en guerre</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Malbroek zal <rhyme label="a" type="f">vatangeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Men Vrouw die ziet men <rhyme label="b" type="f">garen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Malbroek is <rhyme label="a" type="f" corresp="#s1:l1">overleden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gebleeven in het <rhyme label="c" type="m">Velt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Malbroek die kloeken <rhyme label="c" type="m" corresp="#s1:l4">Helt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ik hoop dat hy rust in <rhyme label="a" type="f" corresp="#s1:l1">vreeden</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s1:l2">mirontaine</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ik hoop dat hy rust in <rhyme label="a" type="f" corresp="#s1:l1">vreeden</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Naar al dat groot <rhyme label="c" type="m" corresp="#s1:l4">gewelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Daar zyn zoo veel <rhyme label="a" type="f">Malbroeken</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Men zalze op gaan <rhyme label="a" type="f" corresp="#s2:l1">zoeken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die regt Malbroeken <rhyme label="c" type="m">zyn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Veel Jonkmans in de <rhyme label="c" type="m" corresp="#s2:l4">schyn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En ook veel malle <rhyme label="a" type="f" corresp="#s2:l1">doeken</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s2:l2">mirontaine</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En ook veel malle <rhyme label="a" type="f" corresp="#s2:l1">doeken</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Juffrouwen groot en <rhyme label="c" type="m" corresp="#s2:l4">klyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Jonkmans na de <rhyme label="a" type="f">Mode</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die pronken als aardse <rhyme label="a" type="f" corresp="#s3:l1">Gooden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met haare ronde <rhyme label="c" type="m">hoed</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Maar gy Simpelen <rhyme label="c" type="m" corresp="#s3:l4">bloed</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Is dat een nieuw <rhyme label="a" type="f" corresp="#s3:l1">Matroone</rhyme>?</l>
        <l id="s3:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s3:l2">mirontaine</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Is dat een nieuw <rhyme label="a" type="f" corresp="#s3:l1">Matroone</rhyme>?</l>
        <l id="s3:l9" met="3" type="-+">Ziet wat de liefde <rhyme label="c" type="m" corresp="#s3:l4">doet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Beziet de <rhyme label="a" type="f">kaskanaden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Zy maken veel <rhyme label="a" type="f" corresp="#s4:l1">paraden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Al benne zy nog zoo <rhyme label="c" type="m">bot</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Gelyk een klaaver <rhyme label="c" type="m" corresp="#s4:l4">zot</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Zy zyn met min <rhyme label="a" type="f" corresp="#s4:l1">beladen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s4:l2">mirontaine</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zy zyn met min <rhyme label="a" type="f" corresp="#s4:l1">beladen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Al heeft het zin nog <rhyme label="c" type="m" corresp="#s4:l4">slot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Dogters <rhyme label="a" type="f">onverweten</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die zyne zoo <rhyme label="a" type="f" corresp="#s5:l1">vermeeten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met Kleesels om <rhyme label="c" type="m">gestigt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Haar Neusdoeken veels te <rhyme label="c" type="m" corresp="#s5:l4">ligt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Te spelden zynze <rhyme label="a" type="f" corresp="#s5:l1">vergeeten</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s5:l2">mirontaine</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Te spelden zynze <rhyme label="a" type="f" corresp="#s5:l1">vergeeten</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Gelyk een <rhyme label="c" type="m" corresp="#s5:l4">Venuswigt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Haar Koppen wilt <rhyme label="a" type="f">aanschouwen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met doeken heel <rhyme label="a" type="f" corresp="#s6:l1">ontfouwen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Versiert met strik en <rhyme label="c" type="m">lint</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Hoe zynze zo <rhyme label="c" type="m" corresp="#s6:l4">verblint</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Hoe zyt gy zoo <rhyme label="a" type="f" corresp="#s6:l1">verkouwen</rhyme>?</l>
        <l id="s6:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s6:l2">mirontaine</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Hoe zyt gy zoo <rhyme label="a" type="f" corresp="#s6:l1">verkouwen</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Al door de liefde <rhyme label="c" type="m" corresp="#s6:l4">blind</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daar zyn getroude <rhyme label="a" type="f">Venters</rhyme>;</l>
        <l id="s7:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wyven als <rhyme label="a" type="f" corresp="#s7:l1">Serrepenten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Met kyven <rhyme label="c" type="m">overvloet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Het is alleby niet <rhyme label="c" type="m" corresp="#s7:l4">goed</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Naar al de <rhyme label="a" type="f" corresp="#s7:l1">selderementen</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s7:l2">mirontaine</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Naar al de <rhyme label="a" type="f" corresp="#s7:l1">selderementen</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Op het laatst een armen <rhyme label="c" type="m" corresp="#s7:l4">bloed</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">De Keuken zonder <rhyme label="a" type="f">Eeten</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Het Kind dat leyd <rhyme label="a" type="f" corresp="#s8:l1">bescheeten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Men hoort niet als <rhyme label="c" type="m">geschry</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Naar al dat groot <rhyme label="c" type="m" corresp="#s8:l4">slavery</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Getrouwde wilt het <rhyme label="a" type="f" corresp="#s8:l1">weeten</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s8:l2">mirontaine</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Getrouwde wilt het <rhyme label="a" type="f" corresp="#s8:l1">weeten</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Bemint de <rhyme label="c" type="m" corresp="#s8:l4">vreedzaamheid</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Knegten en <rhyme label="a" type="f">Kamenieren</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ze maken goede <rhyme label="a" type="f" corresp="#s9:l1">sieren</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ze drinken Bier en <rhyme label="c" type="m">Wyn</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Als Heeren in de <rhyme label="c" type="m" corresp="#s9:l4">schyn</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Zy kannen zo niet <rhyme label="a" type="f" corresp="#s9:l1">zwieren</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s9:l2">mirontaine</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Zy kannen zo niet <rhyme label="a" type="f" corresp="#s9:l1">zwieren</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Alze op haar eigen, <rhyme label="c" type="m" corresp="#s9:l4">zyn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ik wil het liet gaan <rhyme label="a" type="f">staken</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Ik mog te ver <rhyme label="a" type="f" corresp="#s10:l1">geraken</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Me Pen vliegt in den <rhyme label="c" type="m">hoek</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Maar laat ons drinken <rhyme label="c" type="m" corresp="#s10:l4">kloek</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En zingen met <rhyme label="a" type="f" corresp="#s10:l1">vermaaken</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f" corresp="#s10:l2">mirontaine</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">En zingen met <rhyme label="a" type="f" corresp="#s10:l1">vermaaken</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">En roept viva <rhyme label="c" type="m" corresp="#s10:l4">Malbroek</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
