  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vrij - Metslaarij, die ons <rhyme label="a" type="m">verëent</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Daalde op ons neêr uit hooger <rhyme label="b" type="f">kringen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Daalde op ons neêr uit hooger <rhyme label="b" type="f" corresp="#s1:l2">kringen</rhyme>;</l>
        <l id="s1:l4" met="4" type="-+">Laat ons dus haare <rhyme label="c" type="m">zaligheên</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Verëend van hart, verrukt <rhyme label="b" type="f" corresp="#s1:l2">bezingen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zoo moet ons schuldloos-vrolijk <rhyme label="a" type="m">hars</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Het woên van nijd en boosheid <rhyme label="b" type="f">tarten</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Het woên van nijd en boosheid <rhyme label="b" type="f" corresp="#s2:l2">tarten</rhyme>;</l>
        <l id="s2:l4" met="4" type="-+">Van 's leevens ongestoord <rhyme label="c" type="m">genot</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Verbannen angst en zorg en <rhyme label="b" type="f" corresp="#s2:l2">smarten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Beminnelijke <rhyme label="a" type="m">eenstemmigheid</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Zal de aarde ons tot een Eden <rhyme label="b" type="f">maaken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Zal de aarde ons tot een Eden <rhyme label="b" type="f" corresp="#s3:l2">maaken</rhyme>;</l>
        <l id="s3:l4" met="4" type="-+">En, door de deugd zelv' <rhyme label="c" type="m">onderricht</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Leere elk haar hemelvreugde <rhyme label="b" type="f" corresp="#s3:l2">smaaken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zoo moet ons schuldloos-vrolijk <rhyme label="a" type="m">hart</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Het woên van nijd en boosheid <rhyme label="b" type="f">tarten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Het woên van nijd en boosheid <rhyme label="b" type="f" corresp="#s4:l2">tarten</rhyme>;</l>
        <l id="s4:l4" met="4" type="-+">Van 's leevens ongestoord <rhyme label="c" type="m">genot</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Verbannen angst en zorg en <rhyme label="b" type="f" corresp="#s4:l2">smarten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Verlichting! rukk', met eigen <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Weldra den blinddoek van elks <rhyme label="b" type="f">oogen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Weldra den blinddoek van elks <rhyme label="b" type="f" corresp="#s5:l2">oogen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Zoo word' de duisterling <rhyme label="c" type="m">alöm</rhyme></l>
        <l id="s5:l5" met="4" type="-+">'s Vooröordeels donkren nagt <rhyme label="b" type="f" corresp="#s5:l2">onttogen</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zoo moet ons schuldloos-vrolijk <rhyme label="a" type="m">hart</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Het woên van nijd en boosheid <rhyme label="b" type="f">tarten</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Het woên van nijd en boosheid <rhyme label="b" type="f" corresp="#s6:l2">tarten</rhyme>;</l>
        <l id="s6:l4" met="4" type="-+">Van 's leevens ongestoord <rhyme label="c" type="m">genot</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Verbannen angst en zorg en <rhyme label="b" type="f" corresp="#s6:l2">smarten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
