  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SChoonste Maagd heb <rhyme label="a" type="f">mededogen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Schoonste Maagd daar ik naar <rhyme label="b" type="m">tracht</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Blus myn gloed, verhoor myn' <rhyme label="b" type="m" corresp="#s1:l2">klacht</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Schoonste Maagd of ik <rhyme label="b" type="m" corresp="#s1:l2">versmacht</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Door den gloed van uw' schoone <rhyme label="a" type="f" corresp="#s1:l1">oogen</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Schoonste Maagd ik mis myn <rhyme label="b" type="m" corresp="#s1:l2">kracht</rhyme>.</l>
      </lg>
    </lg>
