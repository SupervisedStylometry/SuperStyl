      <category translation="religion">godsdienst</category>
    </categories>
  <author>C. J. Wits</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Heyligh Volck, daer Godt de <rhyme label="a" type="f">stralen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van sijn gunst soo mild' <rhyme label="b" type="m">op-schiet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Glans en Majesteyt laet <rhyme label="a" type="f" corresp="#s1:l1">dalen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die ghy door sijn Soon <rhyme label="b" type="m" corresp="#s1:l2">gheniet</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Daer door ghy u vroom'lijck <rhyme label="c" type="m">stelt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Als een onvertsaeghden <rhyme label="c" type="m" corresp="#s1:l5">Heldt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Konincklijcke <rhyme label="a" type="f">Achtbaerheden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Priesterlijcke <rhyme label="b" type="m">Waerdigheyt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En Propheetse Eer oock <rhyme label="a" type="f" corresp="#s2:l1">mede</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Gheeft u Godt uyt <rhyme label="b" type="m" corresp="#s2:l2">goedigheyt</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Die u uyt de <rhyme label="c" type="m">duysterheyt</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Heeft in 't suyv're Licht <rhyme label="c" type="m" corresp="#s2:l5">gheleyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Geestelijcke <rhyme label="a" type="f">Heerschappyen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Voert ghy loff'lijck na sijn <rhyme label="b" type="m">raedt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Sonde, Werelt, u <rhyme label="a" type="f" corresp="#s3:l1">pertyen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En 't gheen noch al meer <rhyme label="b" type="m" corresp="#s3:l2">op-staet</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Voor u swichten tot haer <rhyme label="c" type="m">spijt</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Want ghy haer Verwinner <rhyme label="c" type="m" corresp="#s3:l5">zijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Uwe Konincklijck <rhyme label="a" type="f">Regeeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Blijckt uyt al u wijs <rhyme label="b" type="m">beleyt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want den grootsten Heer der <rhyme label="a" type="f" corresp="#s4:l1">Heeren</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Deelt u me'e sijn <rhyme label="b" type="m" corresp="#s4:l2">Majesteyt</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">En met Deucht en Eere <rhyme label="c" type="m">kroont</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Hemels glants in u <rhyme label="c" type="m" corresp="#s4:l5">vertoont</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Daer door staen al u <rhyme label="a" type="f">Vyanden</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Voor u waerdigheyt <rhyme label="b" type="m">versuft</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Want ghy houdt het Sweerdt in <rhyme label="a" type="f" corresp="#s5:l1">handen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">('t Is Gods Woort) 't welck haer <rhyme label="b" type="m" corresp="#s5:l2">verbluft</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Daer voor al het Helsch <rhyme label="c" type="m">ghebroet</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Tot haer schand' af-wijcken <rhyme label="c" type="m" corresp="#s5:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ghy zijt niet als 's Werelts <rhyme label="a" type="f">Slaven</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Die men vaeck als Heeren <rhyme label="b" type="m">groet</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Maer als Slaven loopen, <rhyme label="a" type="f" corresp="#s6:l1">draven</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Om een lastigh <rhyme label="b" type="m" corresp="#s6:l2">overvloet</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">‘Wat baet haer eens Heeren <rhyme label="c" type="m">schijn</rhyme> ?</l>
        <l id="s6:l6" met="4" type="+-">‘Die Affectens Slaven <rhyme label="c" type="m" corresp="#s6:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ghy zijt Rijck: Ghy gaet <rhyme label="a" type="f">genoegen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Als ghy maet'lijck noodruft <rhyme label="b" type="m">hebt</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Al wat Godt u gaet <rhyme label="a" type="f" corresp="#s7:l1">toe-voeghen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Ghy u reyn vermaeck in <rhyme label="b" type="m" corresp="#s7:l2">schept</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Die ghy danckbaerlijck <rhyme label="c" type="m">gheniet</rhyme>:</l>
        <l id="s7:l6" met="4" type="+-">Overvloedt en acht ghy <rhyme label="c" type="m" corresp="#s7:l5">niet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">GEestelijcke <rhyme label="a" type="f">Offerhanden</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Doet ghy na Gods wil <rhyme label="b" type="m">bequaem</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Want sy t'sijnder Eere <rhyme label="a" type="f" corresp="#s8:l1">branden</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Zijn door Christum <rhyme label="b" type="m" corresp="#s8:l2">aenghenaem</rhyme>.</l>
        <l id="s8:l5" met="4" type="+-">Draeght u vroom'lijck t'allen <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Want ghy Koon'gen, Priesters <rhyme label="c" type="m" corresp="#s8:l5">zijt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Dit zijn niet de hooghste <rhyme label="a" type="f">saecken</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Daer door dat ghy zijt <rhyme label="b" type="m">beroemt</rhyme>.</l>
        <l id="s9:l3" met="4" type="+-">Godt die gaet u grooter <rhyme label="a" type="f" corresp="#s9:l1">maecken</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Want hy u sijn Kind'ren <rhyme label="b" type="m" corresp="#s9:l2">noemt</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Och ! hoe heer'lijck is u <rhyme label="c" type="m">lodt</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Kind'ren van den Grooten <rhyme label="c" type="m" corresp="#s9:l5">Godt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Schoon ghy noch op Aerd' moet <rhyme label="a" type="f">woonen</rhyme></l>
        <l id="s10:l2" met="4" type="+-">By den Aerdtschen Aerdt <rhyme label="b" type="m">verkeert</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Daer u Licht sijn glans komt <rhyme label="a" type="f" corresp="#s10:l1">toonen</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Godt, u Vader ghy <rhyme label="b" type="m" corresp="#s10:l2">vereert</rhyme>.</l>
        <l id="s10:l5" met="4" type="+-">Die u uyt dit <rhyme label="c" type="m">Tranen-dal</rhyme></l>
        <l id="s10:l6" met="4" type="+-">In sijn vreuchde halen <rhyme label="c" type="m" corresp="#s10:l5">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
