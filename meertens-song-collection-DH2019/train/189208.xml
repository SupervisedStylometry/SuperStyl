  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Wyntje! gy zyt al myn <rhyme label="a" type="m">vreugd</rhyme>.</l>
        <l id="s1:l2" met="4" type="-+">O Wyntje! gy zyt al myn <rhyme label="a" type="m" corresp="#s1:l1">vreugd</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Hoe smaakt het sap dat my <rhyme label="a" type="m" corresp="#s1:l1">verheugt</rhyme>!</l>
        <l id="s1:l4" met="4" type="-+">Dat my verheugd! Dat my <rhyme label="a" type="m" corresp="#s1:l1">verheugd</rhyme>!</l>
        <l id="s1:l5" met="4" type="-+">O geur'ge roos myn zoete <rhyme label="b" type="f">flesje</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Gy zyt voor eeuwig myn <rhyme label="b" type="f" corresp="#s1:l5">Matresje</rhyme>.</l>
      </lg>
    </lg>
  </text>
