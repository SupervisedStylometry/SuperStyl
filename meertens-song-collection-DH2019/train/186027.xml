    <tags/>
    <melody>Ik lach met Venus' kwikken</melody>
  <author>Hermanus van den Burg</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">IK lach met Venus <rhyme label="a" type="f">kwikken</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zy zoekt my te <rhyme label="a" type="f" corresp="#s1:l1">verstrikken</rhyme>,</l>
        <l id="s1:l3" met="7" type="-+">Ik belach, Ik belach, Ik belach haar <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">En verbreek de <rhyme label="c" type="f">pylen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Van Kupido, haar <rhyme label="d" type="m">Zoon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Min doet myn bloed <rhyme label="a" type="f">verteeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wyn kan de ribben <rhyme label="a" type="f" corresp="#s2:l1">smeeren</rhyme>.</l>
        <l id="s2:l3" met="7" type="-+">'k Heb zoôn dorst, 'k Heb zoôn dorst, 'k Heb zoôn dorst schenk <rhyme label="b" type="m">in</rhyme>;</l>
        <l id="s2:l4" met="3" type="+-">Als ik Wyn kan <rhyme label="c" type="f">krygen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Scheer ik de gek met de <rhyme label="d" type="m">Min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="-+">Ik kies voor Venus <rhyme label="a" type="f">Borsten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een paar Westvaalse <rhyme label="a" type="f" corresp="#s3:l1">Worsten</rhyme>;</l>
        <l id="s3:l3" met="7" type="-+">Daar op smaakt, Daar op smaakt, Daar op smaakt de <rhyme label="b" type="m">Wyn</rhyme>.</l>
        <l id="s3:l4" met="3" type="+-">Wie kan zonder <rhyme label="c" type="f">Bachus</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Naar behooren vrolyk <rhyme label="d" type="m">zyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="-+">Al wie niet meê wil <rhyme label="a" type="f">klinken</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">En rustig met my <rhyme label="a" type="f" corresp="#s4:l1">drinken</rhyme>,</l>
        <l id="s4:l3" met="7" type="-+">Loop de Min, Loop de Min, Loop de Min naar 't <rhyme label="b" type="m">gat</rhyme>.</l>
        <l id="s4:l4" met="3" type="+-">Kryg ik lust tot <rhyme label="c" type="f">zoenen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ik zoen den mond van 't <rhyme label="d" type="m">Vat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
