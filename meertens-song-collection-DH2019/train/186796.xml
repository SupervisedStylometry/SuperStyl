    <melody>Psalm 121 Datheen</melody>
  <author>Joannes Six van Chandelier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Al hef ik naa 't gebergt myn <rhyme label="a" type="m">oog</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Van waar of, in dit <rhyme label="b" type="m">dal</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Myn noodhulp koomen <rhyme label="b" type="m" corresp="#s1:l2">zal</rhyme>?</l>
        <l id="s1:l4" met="4" type="-+">Myn hulp is by den Heer om <rhyme label="a" type="m" corresp="#s1:l1">hoog</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die heemel, zee, en <rhyme label="c" type="f">aarde</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Gemaakt heeft, en <rhyme label="c" type="f" corresp="#s1:l5">bewaarde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hy zal niet geeven dat uw <rhyme label="a" type="m">voet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ooit wankelbaar <rhyme label="b" type="m">verschiet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Uw hoeder sluimert <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Sie, hy, die IsraÃ«l <rhyme label="a" type="m" corresp="#s2:l1">behoedt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Sluimt nimmer by syn <rhyme label="c" type="f">schaapen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Noch minder zal hy <rhyme label="c" type="f" corresp="#s2:l5">slaapen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Heer behoedt u waarge <rhyme label="a" type="m">zyt</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">En blyft, tot uw <rhyme label="b" type="m">bestand</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Aan uwe <rhyme label="b" type="m" corresp="#s3:l2">rechterhand</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">De Heer omschaaduwt u <rhyme label="a" type="m" corresp="#s3:l1">altyd</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Geen son zal 's daags u <rhyme label="c" type="f">steeken</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Geen maan zal 's nachts u <rhyme label="c" type="f" corresp="#s3:l5">bleeken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Heer behoedt uw lyf voor <rhyme label="a" type="m">quaad</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En allerhande <rhyme label="b" type="m">wee</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Uw ziel, behoedt hy <rhyme label="b" type="m" corresp="#s4:l2">mee</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Het zy gy uit, of inwaarts <rhyme label="a" type="m" corresp="#s4:l1">gaat</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">De Heer behoedt uw <rhyme label="c" type="f">treeden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van nu, in <rhyme label="c" type="f" corresp="#s4:l5">eeuwigheeden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
