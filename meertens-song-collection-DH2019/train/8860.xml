    <melody>Ein feste Burg</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SIngt vrolijck (Godt is onse <rhyme label="a" type="m">Vest</rhyme>)</l>
        <l id="s1:l2" met="3" type="-+">Des Jacobs Godt, met <rhyme label="b" type="f">Jeuchte</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Met Psalmen end met Bommen <rhyme label="a" type="m" corresp="#s1:l1">best</rhyme></l>
        <l id="s1:l4" met="3" type="-+">End' Harpen van <rhyme label="b" type="f" corresp="#s1:l2">Ghenuechte</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Int Looffhutten <rhyme label="c" type="m">Fest</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die Basuyn niet en <rhyme label="c" type="m" corresp="#s1:l5">Rest</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Want in <rhyme label="d" type="m">Israel</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Sulcx is een Wijse <rhyme label="d" type="m" corresp="#s1:l7">wel</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Een Recht des Godes <rhyme label="e" type="f">Jacob</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sulcx heeft hy als een Tuychnis <rhyme label="a" type="m">sett</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Onder Joseph <rhyme label="b" type="f">Vercooren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Doen sy vry van Egyptens <rhyme label="a" type="m" corresp="#s2:l1">Net</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Nieuw' Spraecke mochten <rhyme label="b" type="f" corresp="#s2:l2">hooren</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Haer Schouder hy <rhyme label="c" type="m">haest</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van allen <rhyme label="c" type="m" corresp="#s2:l5">Overlast</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Verlost' end' <rhyme label="d" type="m">Bevrijt</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Haer Handen waren <rhyme label="d" type="m" corresp="#s2:l7">quijt</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Van slaverny so <rhyme label="e" type="f">suere</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als ghy Aenriept my in der <rhyme label="a" type="m">Noot</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Quam dy mijn Hulp te <rhyme label="b" type="f">Handen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End' als ghy met Versoeckingh' <rhyme label="a" type="m" corresp="#s3:l1">groot</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Kevet tot uwer <rhyme label="b" type="f" corresp="#s3:l2">schanden</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Daer t'Water van <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Den Naem noch hebben <rhyme label="c" type="m" corresp="#s3:l5">snoot</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Probeerd' ick dy <rhyme label="d" type="m">wel</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hoort mijn Volck <rhyme label="d" type="m" corresp="#s3:l7">Israel</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">De Waerheyt ick <rhyme label="e" type="f">betuyghe</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Geen ander Godt sult hebben <rhyme label="a" type="m">ghy</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gheen vreemden Godt <rhyme label="b" type="f">aenbeden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Want ick de HEER, Alleyne <rhyme label="a" type="m" corresp="#s4:l1">sy</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dijn Godt, Houdt mijne <rhyme label="b" type="f" corresp="#s4:l2">Zeden</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Dijn Mondt laet van <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Altijdt ghevullet <rhyme label="c" type="m" corresp="#s4:l5">zijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Maer mijn Volck wil <rhyme label="d" type="m">hem</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Niet keeren aen mijn <rhyme label="d" type="m" corresp="#s4:l7">Stem</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Israel my <rhyme label="e" type="f">versmaedet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daerom nae haeres herten <rhyme label="a" type="m">Waen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Laet ickse gaen end' <rhyme label="b" type="f">varen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Wou mijn Volck mijne Wege <rhyme label="a" type="m" corresp="#s5:l1">gaen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Godtsalichlijck <rhyme label="b" type="f" corresp="#s5:l2">bewaren</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Sy souden <rhyme label="c" type="m">subijt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Des Vyants worden <rhyme label="c" type="m" corresp="#s5:l5">quijt</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">End' ick sou mijn <rhyme label="d" type="m">Handt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Gaen maecken wel <rhyme label="d" type="m" corresp="#s5:l7">bekant</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Teghen all' haer <rhyme label="e" type="f">Partijen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">End' all' de Vyand' des Heeren <rhyme label="a" type="m">quaet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Moesten aen hem <rhyme label="b" type="f">Faelgieren</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maer Eewich dueren soud' haer <rhyme label="a" type="m" corresp="#s6:l1">staet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ick wouse <rhyme label="b" type="f" corresp="#s6:l2">Proviandeeren</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Met weyten seer <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">End' souden Honingh' <rhyme label="c" type="m" corresp="#s6:l5">soet</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Wt Berghen en <rhyme label="d" type="m">Steen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Ghenieten all' <rhyme label="d" type="m" corresp="#s6:l7">ghemeen</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">End' wel versadet <rhyme label="e" type="f">werden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
