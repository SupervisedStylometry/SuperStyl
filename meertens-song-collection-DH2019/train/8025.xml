<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8025</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gezongen onze vader" translation="sung Lord's prayer">gezongen onze vader</tag>
    </tags>
    <melody>Onze vader in hemelrijk</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ONse Vader in <rhyme label="a" type="m">Hemelrijck</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Die ons heet kinders <rhyme label="a" type="m" corresp="#s1:l1">alghelijck</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En wilt dat wy u roepen <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Als wy met noode zijn <rhyme label="b" type="m" corresp="#s1:l3">bevaen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Gheeft dat niet bidt alleen den <rhyme label="c" type="m">mondt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Maer dat het gae van 'sherten <rhyme label="c" type="m" corresp="#s1:l5">gront</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gheheylight uwen name <rhyme label="a" type="m">zy</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">V woordt ongevalscht blijft ons <rhyme label="a" type="m" corresp="#s2:l1">by</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Dat wy oock leven <rhyme label="b" type="m">heylighlijck</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Nae uwen name <rhyme label="b" type="m" corresp="#s2:l3">weerdighlijck</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Behoet ons Heer, voor valsche <rhyme label="c" type="m">leer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dat arm vervoerde volck <rhyme label="c" type="m" corresp="#s2:l5">bekeer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">V Coninghrijck com', o Heer <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hier end' hier nae den trooster <rhyme label="a" type="m" corresp="#s3:l1">soet</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Gheeft ons dien Christus ons <rhyme label="b" type="m">toesey</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Met zijn gaven <rhyme label="b" type="m" corresp="#s3:l3">menigherley</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Breeckt Satans toorn end' groot <rhyme label="c" type="m">ghewalt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Voor zijn ergheyd u Kerck <rhyme label="c" type="m" corresp="#s3:l5">erhalt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">V wil geschied, o Heer, <rhyme label="a" type="m">ghelijck</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Op Aerden als in <rhyme label="a" type="m" corresp="#s4:l1">Hemelrijck</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Gheeft ons gedult in lijdens <rhyme label="b" type="m">pijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ghehoorsaem allesins te <rhyme label="b" type="m" corresp="#s4:l3">zijn</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Neemt van ons wech vleesch ende <rhyme label="c" type="m">bloet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dat teghen uwen wille <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gheeft ons huyden ons daegh'lijcks <rhyme label="a" type="m">broot</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En ons behoefte tot lijfs <rhyme label="a" type="m" corresp="#s5:l1">noodt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Behoet ons Heer voor twist en <rhyme label="b" type="m">strijdt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Voor pest en oock voor dieren <rhyme label="b" type="m" corresp="#s5:l3">tijdt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Dat wy in goeden vrede <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Doet sorghvuldigheyt van ons <rhyme label="c" type="m" corresp="#s5:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Onse schulden vergheeft ons <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat sy ons niet bedroeven <rhyme label="a" type="m" corresp="#s6:l1">meer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Soo wy oock die ons schuldigh <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Haer schult vergheven dit <rhyme label="b" type="m" corresp="#s6:l3">termijn</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Tot haren dienst maeckt ons <rhyme label="c" type="m">bereyt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">In rechter liefd' en <rhyme label="c" type="m" corresp="#s6:l5">eenigheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Leyt ons, Heer, in becoringh' <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Als ons de boose gheest strijdt <rhyme label="a" type="m" corresp="#s7:l1">biet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ter rechter of ter slincker <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Helpt ons te doen fijn <rhyme label="b" type="m" corresp="#s7:l3">wederstandt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">In recht betrouwen <rhyme label="c" type="m">onbevreest</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Door den trooster den heyl'ghen <rhyme label="c" type="m" corresp="#s7:l5">Gheest</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Van alle quaedt verlost ons <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">In dese quade tijden, <rhyme label="a" type="m" corresp="#s8:l1">Heer</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Vrijt ons van den eeuwigen <rhyme label="b" type="m">doot</rhyme>:</l>
        <l id="s8:l4" met="4" type="-+">En troost ons in den laetsten <rhyme label="b" type="m" corresp="#s8:l3">noot</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">Doet ons altijdt goet <rhyme label="c" type="m">onderstant</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Neemt onse zielen in u <rhyme label="c" type="m" corresp="#s8:l5">handt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Want u, o Vader, is dat <rhyme label="a" type="m">rijck</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">En de kracht over al <rhyme label="a" type="m" corresp="#s9:l1">gelijck</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">V is oock alle <rhyme label="b" type="m">heerlijckheyt</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Van nu tot in der <rhyme label="b" type="m" corresp="#s9:l3">eeuwigheyt</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Met Christo uwen Soon <rhyme label="c" type="m">alleyn</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">End' den heyligen Gheest <rhyme label="c" type="m" corresp="#s9:l5">gemeyn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Amen, dat is het werde <rhyme label="a" type="m">waer</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Sterckt ons geloove <rhyme label="a" type="m" corresp="#s10:l1">wanckelbaer</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Op dat wy niet twijf'len daer <rhyme label="b" type="m">aen</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">Wy sullen dit alles <rhyme label="b" type="m" corresp="#s10:l3">ontfaen</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Na uwen wil om Christi <rhyme label="c" type="m">naem</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Door welcken ons beed' is <rhyme label="c" type="m" corresp="#s10:l5">gedaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
