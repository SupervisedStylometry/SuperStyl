<song>
  <meta>
    <id>34084</id>
    <date>
      <year-start>1660</year-start>
      <year-end>1660</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Branle Parthenice</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Peeter de Vleeschoudere</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEft op uw' stem', wilt gheestigh <rhyme label="a" type="f">singhen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Op desen <rhyme label="b" type="m">dagh'</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die ons een nieuw licht komt <rhyme label="a" type="f" corresp="#s1:l1">aen-bringhen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Als wy <rhyme label="b" type="m" corresp="#s1:l2">ghewagh</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Van Benedict met blydschap <rhyme label="c" type="f">maecken</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die ons <rhyme label="d" type="m">ver-heught</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Want wy de vrucht in ons' fiel' <rhyme label="c" type="f" corresp="#s1:l5">smaecken</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Van syn goey <rhyme label="d" type="m" corresp="#s1:l6">deughdt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghelyck een son' ryst uyt den <rhyme label="a" type="f">oosten</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Met syn klaer <rhyme label="b" type="m">licht</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Den Patriarch, die 't kot komt <rhyme label="a" type="f" corresp="#s2:l1">troosten</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Van hem <rhyme label="b" type="m" corresp="#s2:l2">ghesticht</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Ver-licht door-luchtigh met syn <rhyme label="c" type="f">straelen</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Dit traenen <rhyme label="d" type="m">dal</rhyme>;</l>
        <l id="s2:l7" met="4" type="-+">Als Abraham, oock komt <rhyme label="c" type="f" corresp="#s2:l5">ont-haelen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Syn broeders <rhyme label="d" type="m" corresp="#s2:l6">al</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">In een speloncke quam hem <rhyme label="a" type="f">spysen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">De raef met <rhyme label="b" type="m">brood</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Om soo Gods knecht, Gods hulp te <rhyme label="a" type="f" corresp="#s3:l1">wysen</rhyme></l>
        <l id="s3:l4" met="2" type="-+">In synen <rhyme label="b" type="m" corresp="#s3:l2">nood</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Dus met Elias hy <rhyme label="c" type="f">gheleken</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Magh zyn <rhyme label="d" type="m">voor-waer</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Want synen gheest heeft oock <rhyme label="c" type="f" corresp="#s3:l5">ghebleken</rhyme></l>
        <l id="s3:l8" met="2" type="-+">In hem te <rhyme label="d" type="m" corresp="#s3:l6">gaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Als Moyses hy door Gods <rhyme label="a" type="f">ghehinghen</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Heeft met syn <rhyme label="b" type="m">roed'</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Het waeter uyt den steen doen <rhyme label="a" type="f" corresp="#s4:l1">springhen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Met vollen <rhyme label="b" type="m" corresp="#s4:l2">vloed</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">De ysers oock, en de <rhyme label="c" type="f">metaelen</rhyme>;</l>
        <l id="s4:l6" met="2" type="-+">Als <rhyme label="d" type="m">d'Helizeên</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Hy uyt den waeter-grond kost <rhyme label="c" type="f" corresp="#s4:l5">haelen</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Door syn <rhyme label="d" type="m" corresp="#s4:l6">ghebeên</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Met suyverheyd, en reyne <rhyme label="a" type="f">seden</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Was hy <rhyme label="b" type="m">ver-cierdt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Als Joseph heeft hy oock <rhyme label="a" type="f" corresp="#s5:l1">be-streden</rhyme></l>
        <l id="s5:l4" met="2" type="-+">'t On-kuysch <rhyme label="b" type="m" corresp="#s5:l2">ghediert'</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Dus hy der Maeghden krans magh <rhyme label="c" type="f">draghen</rhyme></l>
        <l id="s5:l6" met="2" type="-+">In 's Hemels <rhyme label="d" type="m">hof</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En van ons zyn ons levens <rhyme label="c" type="f" corresp="#s5:l5">daghen</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Ver-eerdt met <rhyme label="d" type="m" corresp="#s5:l6">lof</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als Jacob heeft hy wonder <rhyme label="a" type="f">dinghen</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Aen veel <rhyme label="b" type="m">voor-seydt</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Laet ons hem dan den lof-sangh <rhyme label="a" type="f" corresp="#s6:l1">singhen</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Daer toe <rhyme label="b" type="m" corresp="#s6:l2">be-reydt</rhyme>;</l>
        <l id="s6:l5" met="4" type="-+">En bidden dat hy zy <rhyme label="c" type="f">ghedachtigh</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Ons <rhyme label="d" type="m">al-ghelyck</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">En brengh, door syn ghebeden <rhyme label="c" type="f" corresp="#s6:l5">krachtigh</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">In 't <rhyme label="d" type="m" corresp="#s6:l6">Hemel-ryck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
