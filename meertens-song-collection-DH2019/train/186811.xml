<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186811</id>
    <date>
      <year-start>1674</year-start>
      <year-end>1674</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 136 Datheen</melody>
  <author>Joannes Six van Chandelier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ieder doe <rhyme label="a" type="m">belydenis</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dat de Heere goedig <rhyme label="a" type="m" corresp="#s1:l1">is</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s1:l3">eeuwigheit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Elk belyde dat <rhyme label="a" type="m">besluit</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Voor der gooden God <rhyme label="a" type="m" corresp="#s2:l1">ronduit</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s2:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Elk belyde dat <rhyme label="a" type="m">verblydt</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Voor der heeren Heer <rhyme label="a" type="m" corresp="#s3:l1">altyd</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s3:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy alleen is 't, die zo <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Groote wonderwerken <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s4:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dien den heemel, door syn <rhyme label="a" type="m">woord</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Met verstand gemaakt, <rhyme label="a" type="m" corresp="#s5:l1">behoort</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s5:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die het aardryk heeft <rhyme label="a" type="m">ontdekt</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En op 't waater <rhyme label="a" type="m" corresp="#s6:l1">uitgestrekt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s6:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die der groote lichten <rhyme label="a" type="m">licht</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Heeft gemaakt, en <rhyme label="a" type="m" corresp="#s7:l1">opgericht</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s7:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">'t Sonnelicht, om, met haar <rhyme label="a" type="m">schyn</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Heerschster van den dag te <rhyme label="a" type="m" corresp="#s8:l1">zyn</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s8:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Maan, en star, ter <rhyme label="a" type="m">heerschappy</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Van het duistre <rhyme label="a" type="m" corresp="#s9:l1">nachtgety</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s9:l3">eeuwigheit</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Hy sloeg den <rhyme label="a" type="m">Egiptenaar</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">In syn eerstgeboore <rhyme label="a" type="m" corresp="#s10:l1">schaar</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s10:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">En bragt Israëls <rhyme label="a" type="m">geslacht</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Uit het midden van syn <rhyme label="a" type="m" corresp="#s11:l1">magt</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s11:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Door syn erm, en sterke <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Uitgestrekt, tot haar <rhyme label="a" type="m" corresp="#s12:l1">bestand</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s12:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Hy is 't, die de <rhyme label="a" type="m">biesenzee</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Aan twee deelen, als <rhyme label="a" type="m" corresp="#s13:l1">doorlnee</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s13:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Die, door 't midden van die <rhyme label="a" type="m">gracht</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Israël daar oover <rhyme label="a" type="m" corresp="#s14:l1">bragt</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s14:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Die doe Faro, en syn <rhyme label="a" type="m">heit</rhyme></l>
        <l id="s15:l2" met="4" type="+-">Omsineer in het roode <rhyme label="a" type="m" corresp="#s15:l1">meir</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s15:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Die syn volk, in <rhyme label="a" type="m">veiligheit</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Door woestynen, heeft <rhyme label="a" type="m" corresp="#s16:l1">geleidt</rhyme>,</l>
        <l id="s16:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s16:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Die, na hyse lang <rhyme label="a" type="m">verdroeg</rhyme>,</l>
        <l id="s17:l2" met="4" type="+-">Groote kooningen <rhyme label="a" type="m" corresp="#s17:l1">versloeg</rhyme>,</l>
        <l id="s17:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s17:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">En, al waaren sy seer <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s18:l2" met="4" type="+-">Kooningen doch heeft <rhyme label="a" type="m" corresp="#s18:l1">gedoodt</rhyme>,</l>
        <l id="s18:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s18:l3">eeuwigheit</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Hy trof Sihon, in syn <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s19:l2" met="4" type="+-">Kooning van den <rhyme label="a" type="m" corresp="#s19:l1">Amoryt</rhyme>,</l>
        <l id="s19:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s19:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">En vernielde doemaals <rhyme label="a" type="m">noch</rhyme></l>
        <l id="s20:l2" met="4" type="+-">Basans reus, den kooning <rhyme label="a" type="m" corresp="#s20:l1">Og</rhyme>,</l>
        <l id="s20:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s20:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s20:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">En vergaf het land, en <rhyme label="a" type="m">ryk</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">Hun ontnoomen, <rhyme label="a" type="m" corresp="#s21:l1">erffelyk</rhyme>,</l>
        <l id="s21:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s21:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s21:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Jaa, hy gaf 't, met erflik <rhyme label="a" type="m">recht</rhyme>,</l>
        <l id="s22:l2" met="4" type="+-">Israël, syn trouwen <rhyme label="a" type="m" corresp="#s22:l1">knecht</rhyme>,</l>
        <l id="s22:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s22:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s22:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Hy is 't, die aan ons <rhyme label="a" type="m">geslacht</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Lang verneedert, gunstig <rhyme label="a" type="m" corresp="#s23:l1">dacht</rhyme>,</l>
        <l id="s23:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s23:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s23:l3">eenwigheit</rhyme>:</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Hy heeft ons, als heel <rhyme label="a" type="m">verdrukt</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">Onse weerparty <rhyme label="a" type="m" corresp="#s24:l1">ontrukt</rhyme>,</l>
        <l id="s24:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s24:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s24:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">Hy is 't, die al 't vleesch, dat <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s25:l2" met="4" type="+-">Noodig brood, en spyse <rhyme label="a" type="m" corresp="#s25:l1">geeft</rhyme>,</l>
        <l id="s25:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s25:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s25:l3">eeuwigheit</rhyme>:</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">Elk bely dan 's Heeren <rhyme label="a" type="m">deugd</rhyme></l>
        <l id="s26:l2" met="4" type="+-">Voor dien Heemelgod, <rhyme label="a" type="m" corresp="#s26:l1">verheugt</rhyme>,</l>
        <l id="s26:l3" met="4" type="+-">Want syn gunst, en <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s26:l4" met="4" type="+-">Duuren in der <rhyme label="b" type="m" corresp="#s26:l3">eeuwigheit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
