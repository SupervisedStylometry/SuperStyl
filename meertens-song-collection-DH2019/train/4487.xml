      <category translation="religion">godsdienst</category>
    </categories>
  <author>Johan Fruytiers</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MOyses een seer heylich <rhyme label="a" type="m">man</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Is van tvoorseyde volck <rhyme label="b" type="f">ghesproten</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Des werelts liefde hy <rhyme label="a" type="m" corresp="#s1:l1">ghewan</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Godt hadt hem lief waer door hy <rhyme label="a" type="m" corresp="#s1:l1">dan</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Prijs ende gunste heeft <rhyme label="b" type="f" corresp="#s1:l2">ghenoten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Heere heeft hem oock <rhyme label="a" type="m">gheeert</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Ghelijck de vaderen seer <rhyme label="b" type="f">heylich</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Hy verhief hem, sijn vrees <rhyme label="a" type="m" corresp="#s2:l1">vermeert</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Sijn vianden sijn <rhyme label="a" type="m" corresp="#s2:l1">omghekeert</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Godt maecte door hem den wech <rhyme label="b" type="f" corresp="#s2:l2">veylich</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Veel teekenen dede hy <rhyme label="a" type="m">snel</rhyme></l>
        <l id="s3:l2" met="4" type="-+">En wert voor de princen hooch <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Godt gaf hem ouer tvolck <rhyme label="a" type="m" corresp="#s3:l1">beuel</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">De welcke hy regeerde <rhyme label="a" type="m" corresp="#s3:l1">wel</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Godts heerlijcheyt is hem <rhyme label="b" type="f" corresp="#s3:l2">bewesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Tot eenen seer heylighen <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Soo was hy van Godt <rhyme label="b" type="f">wtvercooren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En dat om sijne trouwe <rhyme label="a" type="m" corresp="#s4:l1">daet</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Saechtmoedich was hy van <rhyme label="a" type="m" corresp="#s4:l1">ghelaet</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Tot ouricheyt was hy <rhyme label="b" type="f" corresp="#s4:l2">gheboren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Godt die vercoos hem bouen <rhyme label="a" type="m">al</rhyme></l>
        <l id="s5:l2" met="4" type="-+">De ander menichte van <rhyme label="b" type="f">volcken</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Hy liet hem hooren t'soet <rhyme label="a" type="m" corresp="#s5:l1">gheschal</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Sijnder stemmen in dit <rhyme label="a" type="m" corresp="#s5:l1">dal</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En voerde hem in duyster <rhyme label="b" type="f" corresp="#s5:l2">wolcken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy gaf hem teghenwoordich <rhyme label="a" type="m">daer</rhyme></l>
        <l id="s6:l2" met="4" type="-+">De wet voor alle sijnen <rhyme label="b" type="f">knechten</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Des leuens en wijsheyt <rhyme label="a" type="m" corresp="#s6:l1">voorwaer</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dat Jacob leerde tverbont <rhyme label="a" type="m" corresp="#s6:l1">claer</rhyme></l>
        <l id="s6:l5" met="4" type="-+">En Israel oock sijne <rhyme label="b" type="f" corresp="#s6:l2">rechten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
