  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">VUt swaer <rhyme label="a" type="f">benouwen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Met vast <rhyme label="a" type="f" corresp="#s1:l1">betrouwen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Roep ick tot v Heer sonder <rhyme label="b" type="f">vermijden</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Aensiet ons vianden nv ten <rhyme label="b" type="f" corresp="#s1:l3">tijden</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Hoe sy raet <rhyme label="a" type="f" corresp="#s1:l1">houwen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">sonder <rhyme label="a" type="f" corresp="#s1:l1">verflouwen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">En stellen stricken op alle <rhyme label="b" type="f" corresp="#s1:l3">zijden</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Om ons te brenghen in druck en <rhyme label="b" type="f" corresp="#s1:l3">lijden</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Wilt ditte <rhyme label="a" type="f" corresp="#s1:l1">schouwen</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Met uwen ooghen wt shemels <rhyme label="c" type="f">throone</rhyme></l>
        <l id="s1:l11" met="2" type="+-">En ons van <rhyme label="a" type="f" corresp="#s1:l1">rouwen</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Uerlossen door v liefde <rhyme label="c" type="f" corresp="#s1:l10">schoone</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Gheeft ons v rijck te <rhyme label="c" type="f" corresp="#s1:l10">loone</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Sy legghen <rhyme label="a" type="f">laghen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Uan daghe te <rhyme label="a" type="f" corresp="#s2:l1">daghen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">So wy bemercken in dese <rhyme label="b" type="f">stonden</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Ghelijck een hert die van den <rhyme label="b" type="f" corresp="#s2:l3">honden</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Uermoeyt doort <rhyme label="a" type="f" corresp="#s2:l1">iaghen</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Om dorsts <rhyme label="a" type="f" corresp="#s2:l1">verslaghen</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Haect na een water seer diep van <rhyme label="b" type="f" corresp="#s2:l3">gronden</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Also zijn dese oock nv <rhyme label="b" type="f" corresp="#s2:l3">beuonden</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Om wt te <rhyme label="a" type="f" corresp="#s2:l1">vaghen</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Gods wtuercoren in alle <rhyme label="c" type="f">ghehuchten</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Sy roepen en <rhyme label="a" type="f" corresp="#s2:l1">claghen</rhyme></l>
        <l id="s2:l12" met="4" type="-+">Maer God sal in blijschap keeren <rhyme label="c" type="f" corresp="#s2:l10">tsuchten</rhyme></l>
        <l id="s2:l13" met="3" type="-+">Als wy tot hem <rhyme label="c" type="f" corresp="#s2:l10">vluchten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Tis om <rhyme label="a" type="f">beweenen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Dat sy als <rhyme label="a" type="f" corresp="#s3:l1">beenen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Uerhart met nijde so zijn <rhyme label="b" type="f">ontsteken</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Want diemen nv van Gods woort hoort <rhyme label="b" type="f" corresp="#s3:l3">spreken</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Wt oprecht <rhyme label="a" type="f" corresp="#s3:l1">meenen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Dien salmen <rhyme label="a" type="f" corresp="#s3:l1">vercleenen</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Soot openbaer nv is <rhyme label="b" type="f" corresp="#s3:l3">ghebleken</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Maer Heer ghy weet al haer loose <rhyme label="b" type="f" corresp="#s3:l3">treken</rhyme></l>
        <l id="s3:l9" met="2" type="-+">En anders <rhyme label="a" type="f" corresp="#s3:l1">gheenen</rhyme></l>
        <l id="s3:l10" met="4" type="-+">Ghy moecht haer voornemen haest <rhyme label="c" type="f">verpletten</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Certeyn niet <rhyme label="a" type="f" corresp="#s3:l1">eenen</rhyme></l>
        <l id="s3:l12" met="4" type="-+">En isser die ons mach schaen of <rhyme label="c" type="f" corresp="#s3:l10">letten</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Wilt God hem voor ons <rhyme label="c" type="f" corresp="#s3:l10">setten</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Heere <rhyme label="a" type="f">verheuen</rhyme></l>
        <l id="s4:l2" met="2" type="-+">U wil ick <rhyme label="a" type="f" corresp="#s4:l1">aencleuen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">En mijn betrouwen alleen op v <rhyme label="b" type="f">stellen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Al zijnder veel die ons nv <rhyme label="b" type="f" corresp="#s4:l3">quellen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Willet <rhyme label="a" type="f" corresp="#s4:l1">vergheuen</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Die nv ons <rhyme label="a" type="f" corresp="#s4:l1">leuen</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Met ghewelt ter neder willen <rhyme label="b" type="f" corresp="#s4:l3">vellen</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Al hoortmen haer nv veel wonders <rhyme label="b" type="f" corresp="#s4:l3">rellen</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Tis al <rhyme label="a" type="f" corresp="#s4:l1">bedreuen</rhyme></l>
        <l id="s4:l10" met="4" type="-+">Door onwetentheyt Heere der <rhyme label="c" type="f">Heeren</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Lacen sy <rhyme label="a" type="f" corresp="#s4:l1">sneuen</rhyme></l>
        <l id="s4:l12" met="4" type="-+">O ghenadich God wiltse <rhyme label="c" type="f" corresp="#s4:l10">bekeeren</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Ende haer v weghen <rhyme label="c" type="f" corresp="#s4:l10">leeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">God der <rhyme label="a" type="f">heyrscharen</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Die v <rhyme label="a" type="f" corresp="#s5:l1">dienaren</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Noyt in haren noot en hebt <rhyme label="b" type="f">verlaten</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Coemt ons o God nv oock ter <rhyme label="b" type="f" corresp="#s5:l3">baten</rhyme></l>
        <l id="s5:l5" met="2" type="-+">En wilt ons <rhyme label="a" type="f" corresp="#s5:l1">sparen</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Maer <rhyme label="a" type="f" corresp="#s5:l1">openbaren</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Uwen wille seer soet bouen <rhyme label="b" type="f" corresp="#s5:l3">maten</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Op dat wy v woorden moghen <rhyme label="b" type="f" corresp="#s5:l3">vaten</rhyme></l>
        <l id="s5:l9" met="2" type="-+">En voort <rhyme label="a" type="f" corresp="#s5:l1">verclaren</rhyme></l>
        <l id="s5:l10" met="4" type="-+">U goetheyt die ghy ons quaemt <rhyme label="c" type="f">bewijsen</rhyme></l>
        <l id="s5:l11" met="2" type="+-">Die ons <rhyme label="a" type="f" corresp="#s5:l1">sondaren</rhyme></l>
        <l id="s5:l12" met="4" type="-+">Wter doot in dat leuen hebt doen <rhyme label="c" type="f" corresp="#s5:l10">rijsen</rhyme></l>
        <l id="s5:l13" met="3" type="-+">Dus wilt God altijt <rhyme label="c" type="f" corresp="#s5:l10">prijsen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Prince God <rhyme label="a" type="f">machtich</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Die ons <rhyme label="a" type="f" corresp="#s6:l1">erfachtich</rhyme></l>
        <l id="s6:l3" met="4" type="-+">In v rijcke door Christum wildet <rhyme label="b" type="f">maken</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Op v so wil ick al mijn hope <rhyme label="b" type="f" corresp="#s6:l3">staken</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Maect ons <rhyme label="a" type="f" corresp="#s6:l1">deelachtich</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Uwen gheest <rhyme label="a" type="f" corresp="#s6:l1">crachtich</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Want na v goetheyt Heere wy <rhyme label="b" type="f" corresp="#s6:l3">haken</rhyme></l>
        <l id="s6:l8" met="4" type="-+">U liefde die doet alle droefheyt <rhyme label="b" type="f" corresp="#s6:l3">slaken</rhyme></l>
        <l id="s6:l9" met="2" type="-+">Sijt doch <rhyme label="a" type="f" corresp="#s6:l1">ghedachtich</rhyme></l>
        <l id="s6:l10" met="4" type="-+">U creaturen in haer <rhyme label="c" type="f">allenden</rhyme></l>
        <l id="s6:l11" met="2" type="+-">Wy roepen <rhyme label="a" type="f" corresp="#s6:l1">clachtich</rhyme></l>
        <l id="s6:l12" met="4" type="-+">Tot v om hulpe, Heer wilt tot ons <rhyme label="c" type="f" corresp="#s6:l10">wenden</rhyme></l>
        <l id="s6:l13" met="3" type="-+">Ende ons uwen gheest <rhyme label="c" type="f" corresp="#s6:l10">senden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
