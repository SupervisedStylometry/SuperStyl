  <author>Jacob Rosant</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NIemant en kan twee <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Gelijke dienen <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Den eenen hy wel <rhyme label="a" type="f" corresp="#s1:l1">eeren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met liefde dragen <rhyme label="b" type="m" corresp="#s1:l2">sel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Niet anders kan hy <rhyme label="a" type="f">ramen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dan dat hy d'ander <rhyme label="b" type="m">haet</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Gy kundt niet dienen <rhyme label="a" type="f" corresp="#s2:l1">t'samen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Godt en den Mammon <rhyme label="b" type="m" corresp="#s2:l2">quaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daerom ik u-lie <rhyme label="a" type="f">leere</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Weest niet met sorgh <rhyme label="b" type="m">belaen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Voor eeten of voor <rhyme label="a" type="f" corresp="#s3:l1">kleere</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wat dat gy sult doen <rhyme label="b" type="m" corresp="#s3:l2">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">En is uw siel niet <rhyme label="a" type="f">meerder</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dan 't eeten dat gy <rhyme label="b" type="m">eet</rhyme>?</l>
        <l id="s4:l3" met="3" type="-+">En 't lichaem is 't niet <rhyme label="a" type="f" corresp="#s4:l1">weerder</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dan 't kleet daer g' u mee <rhyme label="b" type="m" corresp="#s4:l2">kleet</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Vogels die niet <rhyme label="a" type="f">Saeyen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met sinnen eens <rhyme label="b" type="m">aenschouwt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hoe wel se niet en <rhyme label="a" type="f" corresp="#s5:l1">maeyen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Uw Vader <rhyme label="b" type="m" corresp="#s5:l2">s'onderhouwt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">En sijt gy niet veel <rhyme label="a" type="f">meerder</rhyme>?</l>
        <l id="s6:l2" met="3" type="-+">Wie of van u-lie <rhyme label="b" type="m">kan</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Aen sijne langhte <rhyme label="a" type="f" corresp="#s6:l1">meerder</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Een Cubitus doen <rhyme label="b" type="m" corresp="#s6:l2">an</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wat sorght ghy voor u <rhyme label="a" type="f">kleeden</rhyme>?</l>
        <l id="s7:l2" met="3" type="-+">De LeeliÃ«n <rhyme label="b" type="m">besiet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hoe dat se niet en <rhyme label="a" type="f" corresp="#s7:l1">reeden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Of sy en spinnen <rhyme label="b" type="m" corresp="#s7:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sy sijn noghtans soo <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Bekleet met glori <rhyme label="b" type="m">rijk</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Den Salomonschen <rhyme label="a" type="f" corresp="#s8:l1">Troone</rhyme></l>
        <l id="s8:l4" met="3" type="-+">En is haer niet <rhyme label="b" type="m" corresp="#s8:l2">gelijk</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">'t Hoy dat moet in den <rhyme label="a" type="f">Oove</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Soo Godt dat soo <rhyme label="b" type="m">bekleed'</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Sal hy, kleyn van <rhyme label="a" type="f" corresp="#s9:l1">geloove</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">U niet bekleeden <rhyme label="b" type="m" corresp="#s9:l2">meed'</rhyme>?</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wilt niet in sorge <rhyme label="a" type="f">treeden</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Om eeten of om <rhyme label="b" type="m">drank</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Nogh ook niet om te <rhyme label="a" type="f" corresp="#s10:l1">kleeden</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dit 's Heydenen <rhyme label="b" type="m" corresp="#s10:l2">verlangk</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wat dat u is van <rhyme label="a" type="f">noode</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Heel wel uw Vader <rhyme label="b" type="m">weet</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Soekt eerst het Rijk van <rhyme label="a" type="f" corresp="#s11:l1">Goode</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En sijn <rhyme label="b" type="m" corresp="#s11:l2">reghtveerdigheet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Dan sal voort al de <rhyme label="a" type="f">reste</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Den Hemelsch Vader <rhyme label="b" type="m">goedt</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">U geeven al ten <rhyme label="a" type="f" corresp="#s12:l1">beste</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Toe-smacken <rhyme label="b" type="m" corresp="#s12:l2">overvloedt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Daerom en sorght van <rhyme label="a" type="f">dage</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Niet voor d'aenstaenden <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Want die wel sorge <rhyme label="a" type="f" corresp="#s13:l1">dragen</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Ook voor sijn selven <rhyme label="b" type="m" corresp="#s13:l2">magh</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">d'Een dagh genogh <rhyme label="a" type="f">beladen</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Is met sijn eygen <rhyme label="b" type="m">quaet</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Dus is het niet <rhyme label="a" type="f" corresp="#s14:l1">geraden</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Dat men meer soeken <rhyme label="b" type="m" corresp="#s14:l2">gaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
