  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">BRoeders hebt doch <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Al soectmen hier v <rhyme label="a" type="m" corresp="#s1:l1">bloet</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Te storten om Gods woort <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Al zijn wy hier gheacht, dul, sot en <rhyme label="a" type="m" corresp="#s1:l1">onuroet</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Ia ghetorden onder die <rhyme label="a" type="m" corresp="#s1:l1">voet</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Al zijn die Godvreesende nv <rhyme label="b" type="m">gheplaecht</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Uanden Goddeloosen <rhyme label="b" type="m" corresp="#s1:l6">onuersaecht</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Dat spel sal saen omme <rhyme label="c" type="f">keeren</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Want also de wijse man <rhyme label="b" type="m" corresp="#s1:l6">ghewaecht</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Sy sullen sien staen die sy hebben <rhyme label="b" type="m" corresp="#s1:l6">veriaecht</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Met betrouwen byden <rhyme label="c" type="f" corresp="#s1:l8">Heere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Dan sullen sy <rhyme label="a" type="m">eerst</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Uan benautheyt des <rhyme label="a" type="m" corresp="#s2:l1">gheest</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Beuen, en zijn <rhyme label="a" type="m" corresp="#s2:l1">beureest</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Segghende, siet die wy met swaer <rhyme label="a" type="m" corresp="#s2:l1">tempeest</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Hebben veruolghet <rhyme label="a" type="m" corresp="#s2:l1">meest</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Nv staen sy by Gods kinders <rhyme label="b" type="m">ghetelt</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Die wy hier doodden met <rhyme label="b" type="m" corresp="#s2:l6">ghewelt</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Ia achten haer leuen <rhyme label="c" type="f">raserien</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Dese zijnt die wy hebben <rhyme label="b" type="m" corresp="#s2:l6">ghequelt</rhyme></l>
        <l id="s2:l10" met="4" type="+-">En met nijde ter neder <rhyme label="b" type="m" corresp="#s2:l6">gheuelt</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Nv sietmense <rhyme label="c" type="f" corresp="#s2:l8">verblien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Het blijct <rhyme label="a" type="m">voorwaer</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Dat wy weghen <rhyme label="a" type="m" corresp="#s3:l1">swaer</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Hebben ghewandelt, <rhyme label="a" type="m" corresp="#s3:l1">maer</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Wy zijn ghedwaelt vanden wech <rhyme label="a" type="m" corresp="#s3:l1">eenpaer</rhyme></l>
        <l id="s3:l5" met="4" type="+-">De Sonne der waerheyt <rhyme label="a" type="m" corresp="#s3:l1">claer</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Die en is ons niet op <rhyme label="b" type="m">ghegaen</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Wy zijn moe gheworden, hoort dit <rhyme label="b" type="m" corresp="#s3:l6">vermaen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Inden wech der <rhyme label="c" type="f">verdoemenissen</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Die waerheyt en hebben wy noyt <rhyme label="b" type="m" corresp="#s3:l6">verstaen</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Dus sullen wy deewighe doot <rhyme label="b" type="m" corresp="#s3:l6">ontfaen</rhyme></l>
        <l id="s3:l11" met="3" type="+-">En Gods blijschap <rhyme label="c" type="f" corresp="#s3:l8">missen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Wat hebben ter <rhyme label="a" type="m">noot</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Die rijckdommen <rhyme label="a" type="m" corresp="#s4:l1">groot</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Ghebaet inder <rhyme label="a" type="m" corresp="#s4:l1">doot</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Want gelijc de pijl die een sterc schutter <rhyme label="a" type="m" corresp="#s4:l1">schoot</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Snel vliecht door die lucht <rhyme label="a" type="m" corresp="#s4:l1">bloot</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Ia subbijter dan den roock <rhyme label="b" type="m">vergaet</rhyme></l>
        <l id="s4:l7" met="4" type="+-">So is al onse eere, macht ende <rhyme label="b" type="m" corresp="#s4:l6">staet</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Uergaen ghelijck der <rhyme label="c" type="f">schouwen</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Wy wtsinnighe menschen <rhyme label="b" type="m" corresp="#s4:l6">quaet</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Hebben gheuolcht des duyuels <rhyme label="b" type="m" corresp="#s4:l6">raet</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Nv blijuen wy in <rhyme label="c" type="f" corresp="#s4:l8">rouwen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Sulcke woorden <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Sullen op dat <rhyme label="a" type="m" corresp="#s5:l1">termijn</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Spreken die inder hellen <rhyme label="a" type="m" corresp="#s5:l1">zijn</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Wy zijn vergaen snelder dan den <rhyme label="a" type="m" corresp="#s5:l1">rijn</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Nv blijuen wy inder <rhyme label="a" type="m" corresp="#s5:l1">pijn</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Maer die wy hilden voor dwase <rhyme label="b" type="m">lien</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Moeten wy nv by den Heere <rhyme label="b" type="m" corresp="#s5:l6">sien</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Als die Sonne staen <rhyme label="c" type="f">blincken</rhyme></l>
        <l id="s5:l9" met="4" type="+-">Haer en can oock gheen droefheyt <rhyme label="b" type="m" corresp="#s5:l6">gheschien</rhyme></l>
        <l id="s5:l10" met="4" type="+-">Want God heeft haer vianden wech doen <rhyme label="b" type="m" corresp="#s5:l6">vlien</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Niemant en machse <rhyme label="c" type="f" corresp="#s5:l8">crincken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">O oppersten <rhyme label="a" type="m">Princier</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Die de uwe <rhyme label="a" type="m" corresp="#s6:l1">hier</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Beproeuen wilt int <rhyme label="a" type="m" corresp="#s6:l1">vier</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Als wy zijn benout, oft in eenich <rhyme label="a" type="m" corresp="#s6:l1">dangier</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Dan roepen wy tot v <rhyme label="a" type="m" corresp="#s6:l1">schier</rhyme></l>
        <l id="s6:l6" met="4" type="+-">En ghy helpt ons in corter <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s6:l7" met="4" type="+-">Want ghy doet recht die hier onrecht <rhyme label="b" type="m" corresp="#s6:l6">lijt</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Dus en vreest gheen <rhyme label="c" type="f">tribuleren</rhyme></l>
        <l id="s6:l9" met="4" type="+-">Hy heeft ons ghestelt hier inden <rhyme label="b" type="m" corresp="#s6:l6">strijt</rhyme></l>
        <l id="s6:l10" met="4" type="+-">Wie dat al verwint, hy sal <rhyme label="b" type="m" corresp="#s6:l6">subijt</rhyme></l>
        <l id="s6:l11" met="3" type="+-">Metten Heere <rhyme label="c" type="f" corresp="#s6:l8">triumpheren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
