  <author>Jacob Rosant</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">HEmel en <rhyme label="a" type="m">Aerdt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wilt groote blyschap <rhyme label="b" type="f">toonen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Jaeght alle droefheyt <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">'t Is heel wel <rhyme label="a" type="m" corresp="#s1:l1">waert</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Maekt soet-ruykende <rhyme label="b" type="f" corresp="#s1:l2">Kroonen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van Bloemen en van <rhyme label="c" type="m" corresp="#s1:l3">Kruyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En laet ook klinken <rhyme label="c" type="m" corresp="#s1:l3">luyt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Fioolen, ende <rhyme label="d" type="f">Veelen</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Laet ons van harten blijde <rhyme label="e" type="m">sijn</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En singen metter <rhyme label="d" type="f" corresp="#s1:l8">keelen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">In 's Heemels <rhyme label="a" type="m">Troon</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Maria wordt <rhyme label="b" type="f">verheeven</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En wordt aldaer <rhyme label="c" type="m">gekroont</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Van haren <rhyme label="a" type="m" corresp="#s2:l1">Soon</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Tot 't eeuwigh Saligh <rhyme label="b" type="f" corresp="#s2:l2">leeven</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Sijn goetheyt hy haer <rhyme label="c" type="m" corresp="#s2:l3">toont</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En seydt, nae sijn <rhyme label="c" type="m" corresp="#s2:l3">gewoont</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Komt mijn Lief, mijn <rhyme label="d" type="f">Vriendinne</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Mijn Bruydt, mijn Sus, mijn Duyfken <rhyme label="e" type="m">schoon</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Wiens schoonheydt is van <rhyme label="d" type="f" corresp="#s2:l8">binne</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Komt Moeder <rhyme label="a" type="m">lief</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vergeet uw droeve <rhyme label="b" type="f">dagen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die sijn nu <rhyme label="c" type="m">gepasseert</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Tot uw <rhyme label="a" type="m" corresp="#s3:l1">gerief</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Soo sal ik u <rhyme label="b" type="f" corresp="#s3:l2">op-dragen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Al wat dat gy <rhyme label="c" type="m" corresp="#s3:l3">begeert</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Nu sult gy sijn <rhyme label="c" type="m" corresp="#s3:l3">ge-eert</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En weesen <rhyme label="d" type="f">Koninginne</rhyme></l>
        <l id="s3:l9" met="4" type="-+">Van Aerdtrijk, en van <rhyme label="e" type="m">Heemel-rijk</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Boven de <rhyme label="d" type="f" corresp="#s3:l8">Seraphinne</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Met wat <rhyme label="a" type="m">jolijt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Door-klinkt des Heemels <rhyme label="b" type="f">troone</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'t Is al sijt <rhyme label="c" type="m">wellekom</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Welkom gy <rhyme label="a" type="m" corresp="#s4:l1">sijt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">O Moeder van Goodts <rhyme label="b" type="f" corresp="#s4:l2">Soone</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En niemant isser <rhyme label="c" type="m" corresp="#s4:l3">stom</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Den Heemel klinkt <rhyme label="c" type="m" corresp="#s4:l3">rontom</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Vol geestelijke <rhyme label="d" type="f">sangen</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Der Heyligen, en <rhyme label="e" type="m">Engelen</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Die haer met vreught <rhyme label="d" type="f" corresp="#s4:l8">ontfangen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Op 't Aerdtsche <rhyme label="a" type="m">pleyn</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Sijn bly wy <rhyme label="b" type="f">Catholijken</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die t'samen sijn <rhyme label="c" type="m">vergaert</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Om in 't <rhyme label="a" type="m" corresp="#s5:l1">gemeyn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Met vreughden te doen <rhyme label="b" type="f" corresp="#s5:l2">blijken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ons Christelijken <rhyme label="c" type="m" corresp="#s5:l3">aert</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Die soo publijk <rhyme label="c" type="m" corresp="#s5:l3">verklaert</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Hoe wy met gantscher <rhyme label="d" type="f">sinnen</rhyme>,</l>
        <l id="s5:l9" met="4" type="-+">Haer soeken ook wat eer te <rhyme label="e" type="m">doen</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Want wy haer hartigh <rhyme label="d" type="f" corresp="#s5:l8">minnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Heylige <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En souw men u niet <rhyme label="b" type="f">eeren</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Na ons vermogen <rhyme label="c" type="m">krank</rhyme>?</l>
        <l id="s6:l4" met="2" type="-+">Want 't Godt <rhyme label="a" type="m" corresp="#s6:l1">behaeght</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Als David ons wil <rhyme label="b" type="f" corresp="#s6:l2">leeren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Aenbiddet sijn <rhyme label="c" type="m" corresp="#s6:l3">voet-bank</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Seydt hy in 't <rhyme label="c" type="m" corresp="#s6:l3">Psalter-sank</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Gy moet sijn <rhyme label="d" type="f">aengebeeden</rhyme>,</l>
        <l id="s6:l9" met="4" type="-+">O ware Arke des <rhyme label="e" type="m">verbondts</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Om uwe <rhyme label="d" type="f" corresp="#s6:l8">heyligheeden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Trouwe <rhyme label="a" type="m">voorspraek</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Voor droevige <rhyme label="b" type="f">sondaren</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">By uw' en Goodes <rhyme label="c" type="m">Kindt</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">'t Is reght uw <rhyme label="a" type="m" corresp="#s7:l1">saek</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Ons altijt te <rhyme label="b" type="f" corresp="#s7:l2">bewaren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Gy sijt daer toe <rhyme label="c" type="m" corresp="#s7:l3">gesindt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Want gy ons seer <rhyme label="c" type="m" corresp="#s7:l3">bemindt</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Wilt ons van quaet <rhyme label="d" type="f">bevryden</rhyme>,</l>
        <l id="s7:l9" met="4" type="-+">Op dat wy in des Heemels <rhyme label="e" type="m">vreught</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Met u in Godt <rhyme label="d" type="f" corresp="#s7:l8">verblyden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
