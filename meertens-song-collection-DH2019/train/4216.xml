<song>
  <meta>
    <id>4216</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">KOmt hier ô <rhyme label="a" type="m">Vrouwelijk-geslacht</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met Trommelen, en <rhyme label="b" type="f">Velen</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Versaamd een rey, in u <rhyme label="a" type="m" corresp="#s1:l1">gedacht</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">En singt (met held <rhyme label="b" type="f" corresp="#s1:l2">re-kelen</rhyme>)</l>
        <l id="s1:l5" met="4" type="-+">Den lof van d' <rhyme label="c" type="m">Alder-Helden-Held</rhyme>:</l>
        <l id="s1:l6" met="4" type="-+">Die 't hoofd der Dap'ren heeft <rhyme label="c" type="m" corresp="#s1:l5">geveld</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Daar-om sijn Roem <rhyme label="c" type="m" corresp="#s1:l5">verteld'</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Bly, onder 't <rhyme label="b" type="f" corresp="#s1:l2">spelen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dit's David Isai jonger <rhyme label="a" type="m">Soon</rhyme>:</l>
        <l id="s2:l2" met="3" type="-+">Een schrik der <rhyme label="b" type="f">Philisténan</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Den Hémel reyk hem Sauls <rhyme label="a" type="m" corresp="#s2:l1">Kroon</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Sijn Sonne (eerst <rhyme label="b" type="f" corresp="#s2:l2">verschénen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">In het Bou-vallig <rhyme label="c" type="m">Israel</rhyme>)</l>
        <l id="s2:l6" met="4" type="-+">Verlicht de duyst're-nachten, <rhyme label="c" type="m" corresp="#s2:l5">wel</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Verdrijft diens dampen, <rhyme label="c" type="m" corresp="#s2:l5">snel</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En mist met <rhyme label="b" type="f" corresp="#s2:l2">énen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Siet Iacobs-Helden (in den <rhyme label="a" type="m">strijd</rhyme>)</l>
        <l id="s3:l2" met="3" type="-+">Bekranst met <rhyme label="b" type="f">Lauwer-bladen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Die maken Vrou, en Kind <rhyme label="a" type="m" corresp="#s3:l1">verblijd</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Door <rhyme label="b" type="f" corresp="#s3:l2">mannelijke-daden</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Van 't Heyr der Heydenen (dat <rhyme label="c" type="m">vlood</rhyme>)</l>
        <l id="s3:l6" met="4" type="-+">Heeft Saul Duysenden <rhyme label="c" type="m" corresp="#s3:l5">Gedood</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Die met sijn Boge <rhyme label="c" type="m" corresp="#s3:l5">schoot</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">En wreekten 't <rhyme label="b" type="f" corresp="#s3:l2">smaden</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">So Zegend God het Huys van <rhyme label="a" type="m">Kis</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">In Isrels <rhyme label="b" type="f">eerste-Koning</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Die (onder 't volk) de langste <rhyme label="a" type="m" corresp="#s4:l1">is</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Een <rhyme label="b" type="f" corresp="#s4:l2">aartige-vertoning'</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Van 't Goddelijk gesproken <rhyme label="c" type="m">woord</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dat Benjamin den Staf <rhyme label="c" type="m" corresp="#s4:l5">behoord</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">'t Geen Juda (half <rhyme label="c" type="m" corresp="#s4:l5">verstoort</rhyme>)</l>
        <l id="s4:l8" met="2" type="-+">Neem tot <rhyme label="b" type="f" corresp="#s4:l2">verschoning'</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maar David heeft (met <rhyme label="a" type="m">kloeker-hand</rhyme>)</l>
        <l id="s5:l2" met="3" type="-+">Tien-duysenden <rhyme label="b" type="f">verslagen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">De grootste zegen van ons <rhyme label="a" type="m" corresp="#s5:l1">Land</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Is van hem wech <rhyme label="b" type="f" corresp="#s5:l2">gedragen</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Gelijk de Leeuw, en <rhyme label="c" type="m">wrede-Beer</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Versloeg hy 't puyk van 's <rhyme label="c" type="m" corresp="#s5:l5">Vyands-Heyr</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">(Gewapend met <rhyme label="c" type="m" corresp="#s5:l5">Geweer</rhyme>.)</l>
        <l id="s5:l8" met="2" type="-+">Hy dorst het <rhyme label="b" type="f" corresp="#s5:l2">wagen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dees' sware val van <rhyme label="a" type="m">Goliath</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">(Gevolgd van so veel <rhyme label="b" type="f">vallen</rhyme>)</l>
        <l id="s6:l3" met="4" type="-+">Die schreyd de straten door te <rhyme label="a" type="m" corresp="#s6:l1">Gath</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En binnen <rhyme label="b" type="f" corresp="#s6:l2">Ekrons-wallen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Dies is 't Gerucht van hem noyt <rhyme label="c" type="m">moe</rhyme>:</l>
        <l id="s6:l6" met="4" type="-+">Den Staf die held na Juda <rhyme label="c" type="m" corresp="#s6:l5">toe</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">En 's Harders <rhyme label="c" type="m" corresp="#s6:l5">straffe-roe</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Rijst, boven <rhyme label="b" type="f" corresp="#s6:l2">allen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dit stemd de God van <rhyme label="a" type="m">Abraham</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">De God, <rhyme label="b" type="f">Isaacs-vertrouwen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">De hoop van Iacob en sijn <rhyme label="a" type="m" corresp="#s7:l1">Stam</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En al die op hem <rhyme label="b" type="f" corresp="#s7:l2">bouwen</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Hy neemt een Harder van het <rhyme label="c" type="m">Vee</rhyme></l>
        <l id="s7:l6" met="4" type="-+">En steld hem in een <rhyme label="c" type="m" corresp="#s7:l5">Konings-stee</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Dit doet hy David <rhyme label="c" type="m" corresp="#s7:l5">mee</rhyme>.</l>
        <l id="s7:l8" met="2" type="-+">Dies juygd ô! <rhyme label="b" type="f" corresp="#s7:l2">Vrouwen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
