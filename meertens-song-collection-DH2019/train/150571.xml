      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">GOdt erstijght ten <rhyme label="a" type="m">throon</rhyme></l>
        <l id="s1:l2" met="3" type="+-">En aenvaerdt de <rhyme label="a" type="m" corresp="#s1:l1">kroon</rhyme>:</l>
        <l id="s1:l3" met="3" type="+-">Dies den volcken <rhyme label="b" type="m">schroom</rhyme></l>
        <l id="s1:l4" met="3" type="+-">En vertsaging <rhyme label="b" type="m" corresp="#s1:l3">koom</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Heyl’ge <rhyme label="c" type="m">Cherubijns</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Zijn de zetel <rhyme label="c" type="m" corresp="#s1:l5">sijns</rhyme>.</l>
        <l id="s1:l7" met="3" type="+-">Die op aerden <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Moeten voor hem <rhyme label="d" type="f" corresp="#s1:l7">beven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Groot in Sion <rhyme label="a" type="m">d’Heer</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">En verheven <rhyme label="a" type="m" corresp="#s2:l1">veer</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Boven alle <rhyme label="b" type="m">volck</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Onder het <rhyme label="b" type="m" corresp="#s2:l3">gewolck</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">(Loff sal uwen <rhyme label="c" type="m">naem</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Groot en schricklijck <rhyme label="c" type="m" corresp="#s2:l5">t’saem</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Van elcks lippen <rhyme label="d" type="f">strijcken</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En vol-heyligh <rhyme label="d" type="f" corresp="#s2:l7">blijcken</rhyme>.)</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Dese Heldt, vol <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Lieft alleen en <rhyme label="a" type="m" corresp="#s3:l1">acht</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Heeft alleen sijn <rhyme label="b" type="m">vreuchdt</rhyme></l>
        <l id="s3:l4" met="3" type="+-">In de suyv’re <rhyme label="b" type="m" corresp="#s3:l3">deuchdt</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Van <rhyme label="c" type="m">gerechticheydt</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Door hem selfs <rhyme label="c" type="m" corresp="#s3:l5">bereydt</rhyme></l>
        <l id="s3:l7" met="3" type="+-">En, ten baeck van <rhyme label="d" type="f">leven</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Israël <rhyme label="d" type="f" corresp="#s3:l7">gegeven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Roemt hem <rhyme label="a" type="m">over-seer</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Heft ten top van <rhyme label="a" type="m" corresp="#s4:l1">eer</rhyme></l>
        <l id="s4:l3" met="3" type="+-">’tLoff ons’s Gods; en <rhyme label="b" type="m">kniel</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Met door-schrickte <rhyme label="b" type="m" corresp="#s4:l3">ziel</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Voor sijn <rhyme label="c" type="m">voet-schabel</rhyme>.</l>
        <l id="s4:l6" met="3" type="+-">Billijck is’t <rhyme label="c" type="m" corresp="#s4:l5">bevel</rhyme>:</l>
        <l id="s4:l7" met="3" type="+-">Sijn’ <rhyme label="d" type="f">groot-heylicheden</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Zijn ons des wel <rhyme label="d" type="f" corresp="#s4:l7">reden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Moses en <rhyme label="a" type="m">A’ron</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Dien hy selfs <rhyme label="a" type="m" corresp="#s5:l1">vergon</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Den soo-hoogen <rhyme label="b" type="m">trap</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Van sijn <rhyme label="b" type="m" corresp="#s5:l3">Priesterschap</rhyme>;</l>
        <l id="s5:l5" met="3" type="+-">En oock <rhyme label="c" type="m">Samuel</rhyme>:</l>
        <l id="s5:l6" met="3" type="+-">Alsse <rhyme label="c" type="m" corresp="#s5:l5">(Israel</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Ten gevall’) hem <rhyme label="d" type="f">baden</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Hoord’ hy in <rhyme label="d" type="f" corresp="#s5:l7">genaden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">’tRoepen van haer <rhyme label="a" type="m">stem</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Steegh, en neyghden <rhyme label="a" type="m" corresp="#s6:l1">hem</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Dat hy Godt-spraeck <rhyme label="b" type="m">gaff</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Van den hemel <rhyme label="b" type="m" corresp="#s6:l3">aff</rhyme></l>
        <l id="s6:l5" met="3" type="+-">uyt een <rhyme label="c" type="m">wolck-kalom</rhyme>:</l>
        <l id="s6:l6" met="3" type="+-">Want sy <rhyme label="c" type="m" corresp="#s6:l5">’theylighdom</rhyme></l>
        <l id="s6:l7" met="3" type="+-">Sijns gesets, vol <rhyme label="d" type="f">waerden</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Trouwelijck <rhyme label="d" type="f" corresp="#s6:l7">bewaerden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Ghy, Heer, onse <rhyme label="a" type="m">Godt</rhyme>.</l>
        <l id="s7:l2" met="3" type="+-">Ghy gaeft ooren <rhyme label="a" type="m" corresp="#s7:l1">tot</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Hares mondts <rhyme label="b" type="m">gebedt</rhyme>.</l>
        <l id="s7:l4" met="3" type="+-">’tFeylen in de <rhyme label="b" type="m" corresp="#s7:l3">Wet</rhyme></l>
        <l id="s7:l5" met="3" type="+-">Hebt ghy in den <rhyme label="c" type="m">lien</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Over ’thoofdt <rhyme label="c" type="m" corresp="#s7:l5">gesien</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Maer ’tmoedtwilligh <rhyme label="d" type="f">woelen</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Vwe wraeck doen <rhyme label="d" type="f" corresp="#s7:l7">voelen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Dies nu yeder <rhyme label="a" type="m">poogh</rhyme>’</l>
        <l id="s8:l2" met="3" type="+-">Dat hy groot en <rhyme label="a" type="m" corresp="#s8:l1">hoogh</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Van den Heere <rhyme label="b" type="m">roem</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">En in demoedt <rhyme label="b" type="m" corresp="#s8:l3">ko’m</rhyme></l>
        <l id="s8:l5" met="3" type="+-">Om ten bergh te <rhyme label="c" type="m">tre’en</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Daer der <rhyme label="c" type="m" corresp="#s8:l5">Godlijckhe’en</rhyme></l>
        <l id="s8:l7" met="3" type="+-">Glans is sijns <rhyme label="d" type="f">met-deyligh</rhyme>:</l>
        <l id="s8:l8" met="3" type="+-">Want de Heer is <rhyme label="d" type="f" corresp="#s8:l7">heyligh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
