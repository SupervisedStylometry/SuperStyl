  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Naeste gebuyrtje ick brengh het <rhyme label="a" type="m">uw</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">’t Is hier al qui beut, qui <rhyme label="b" type="m">boit</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vyt goeder herten gun ick het <rhyme label="a" type="m" corresp="#s1:l1">uw</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Want het Wijntje smaeckt seer <rhyme label="b" type="m" corresp="#s1:l2">moy</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Chocquons &amp; vuidons les <rhyme label="c" type="f">verres</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Faisons la bonne <rhyme label="c" type="f" corresp="#s1:l5">chere</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Laet ons singen in ‘t <rhyme label="d" type="m">generael</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">Vivent de vrienden <rhyme label="d" type="m" corresp="#s1:l7">altemael</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Soo neemt het al in u <rhyme label="a" type="m">handeken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">’t Is hier al que beut, que <rhyme label="b" type="m">boit</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En drinckt het uyt tot het <rhyme label="a" type="m" corresp="#s2:l1">randeken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Want de Wijn die smaeckt seer <rhyme label="b" type="m" corresp="#s2:l2">moy</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Chocquons &amp; vuidons les <rhyme label="c" type="f">verres</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Faisons la bonne <rhyme label="c" type="f" corresp="#s2:l5">chere</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Laet ons singen in ‘t <rhyme label="d" type="m">generael</rhyme>.</l>
        <l id="s2:l8" met="4" type="-+">Vivent de vrienden <rhyme label="d" type="m" corresp="#s2:l7">altemael</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Nu zet het doch aen uw <rhyme label="a" type="m">mondeken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">’t Is hier al qui beut, qui <rhyme label="b" type="m">boit</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En drinckt het uyt tot het <rhyme label="a" type="m" corresp="#s3:l1">grondeken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Want de Wijn die smaeckt seer <rhyme label="b" type="m" corresp="#s3:l2">moy</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Chocquons &amp; vuidons les <rhyme label="c" type="f">verres</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Faisons la bonne <rhyme label="c" type="f" corresp="#s3:l5">chere</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Laet ons singen in ‘t <rhyme label="d" type="m">generael</rhyme>.</l>
        <l id="s3:l8" met="4" type="-+">Vivent de vrienden <rhyme label="d" type="m" corresp="#s3:l7">altemael</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Nu heb ick het glaesje sijn recht <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">’t Is hier al qui beut, qui <rhyme label="b" type="m">boit</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat onder ons is sal boven <rhyme label="a" type="m" corresp="#s4:l1">staen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Want het glaesje is see <rhyme label="b" type="m" corresp="#s4:l2">moy</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Chocquons &amp; vuidons les <rhyme label="c" type="f">verres</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Faisons la bonne <rhyme label="c" type="f" corresp="#s4:l5">chere</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Laet ons singen in ‘t <rhyme label="d" type="m">generael</rhyme>.</l>
        <l id="s4:l8" met="4" type="-+">Vivent de vrienden <rhyme label="d" type="m" corresp="#s4:l7">altemael</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gebuurtje ten mach alsoo niet <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">’t Is hier al qui beut, qui <rhyme label="b" type="m">boit</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Ghy moet mijn geven u <rhyme label="a" type="m" corresp="#s5:l1">mondelijn</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Kust nu al kuste ghy <rhyme label="b" type="m" corresp="#s5:l2">noyt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Chocquons &amp; vuidons les <rhyme label="c" type="f">verres</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Faisons la bonne <rhyme label="c" type="f" corresp="#s5:l5">chere</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Laet ons singen in ‘t <rhyme label="d" type="m">generael</rhyme>.</l>
        <l id="s5:l8" met="4" type="-+">Vivent de vrienden <rhyme label="d" type="m" corresp="#s5:l7">altemael</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
