<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182576</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Schep vreugde in het leven</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Schep vreugd, <rhyme label="a" type="f">Bataaven</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Nu de lieven VreÃª <rhyme label="b" type="m">verschynd</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Dat twist en <rhyme label="c" type="f">tweedragt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Onder uw <rhyme label="b" type="m" corresp="#s1:l2">verdwynt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wie kent den braven Franschman <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">die ons de lieven Vreede <rhyme label="d" type="m" corresp="#s1:l5">bied</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Kom laat ons bly vereend van <rhyme label="e" type="m">hart</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Zyn gloorie roem <rhyme label="f" type="f">verheffen</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Heil zy <rhyme label="a" type="f">Buonaparte</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Die ons deze Zeegen <rhyme label="b" type="m">bied</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Wie zingt van <rhyme label="c" type="f">harte</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Niet een vrolyk <rhyme label="b" type="m" corresp="#s2:l2">lied</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Wy noemen op het hoogts <rhyme label="d" type="m">verheugd</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dees altoos Braven zoon der <rhyme label="d" type="m" corresp="#s2:l5">deugd</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Graveeren vergenoegd en <rhyme label="e" type="m">bly</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Zyn naam in onze <rhyme label="f" type="f">harten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Zyn naam blyf <rhyme label="a" type="f">duuren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Zo lang 't Waereld rond <rhyme label="b" type="m">bestaat</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Zelfs moet hem <rhyme label="c" type="f">agten</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Hy die Vryheid <rhyme label="b" type="m" corresp="#s3:l2">haat</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Bataven koom nu bly ten <rhyme label="d" type="m">dans</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Koom vlegten wy den <rhyme label="d" type="m" corresp="#s3:l5">Lauwerkrans</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Om eens het waardig hoofd des <rhyme label="e" type="m">Helds</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Der Franken te <rhyme label="f" type="f">versieren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Daar hy als <rhyme label="a" type="f">Krygsheld</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">In het Veld der eer <rhyme label="b" type="m">verscheen</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Vloog als den <rhyme label="c" type="f">Blixem</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Elken Vyand <rhyme label="b" type="m" corresp="#s4:l2">heen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">De Alphen melden nog zyn <rhyme label="d" type="m">roem</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dat men uit de oudheid helden <rhyme label="d" type="m" corresp="#s4:l5">noem</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Die hem in eer in <rhyme label="e" type="m">Heldenmoed</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En grootheid <rhyme label="f" type="f">overtroffen</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Dat doch het <rhyme label="a" type="f">Noodlot</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Hem nog lang het Leeven <rhyme label="b" type="m">schenk</rhyme></l>
        <l id="s5:l3" met="2" type="+-">En Verjaarde <rhyme label="c" type="f">gryshyd</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">By het sterven <rhyme label="b" type="m" corresp="#s5:l2">wenk</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Zo kan het jong en teer <rhyme label="d" type="m">geslagt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">By de aankomst van heur grootste <rhyme label="d" type="m" corresp="#s5:l5">kragt</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Nog eens den roem des Helds der <rhyme label="e" type="m">Nyl</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met dierbre toonen <rhyme label="f" type="f">zingen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Hyl zy <rhyme label="a" type="f">Buonaparte</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Die ons deeze Vreede <rhyme label="b" type="m">bied</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Wie zingd van <rhyme label="c" type="f">harte</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Niet een vrolyk <rhyme label="b" type="m" corresp="#s6:l2">lied</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Bataaf en Franschman noem zyn <rhyme label="d" type="m">naam</rhyme></l>
        <l id="s6:l6" met="4" type="-+">En blyven altoos Durend <rhyme label="d" type="m" corresp="#s6:l5">Zaam</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">In Bondgenoot en <rhyme label="e" type="m">Broederschap</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">door alle Eeuwen <rhyme label="f" type="f">leeven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
