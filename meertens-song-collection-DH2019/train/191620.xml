<song>
  <meta>
    <id>191620</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zullen dan mijn droeve klachten</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SChoone Venus, Ã´ <rhyme label="a" type="f">Godinne</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">'k Buyg ootmoedig voor u <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die een ieder helpt in 't <rhyme label="a" type="f" corresp="#s1:l1">Minnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als men doet naar uw <rhyme label="b" type="m" corresp="#s1:l2">begeer</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat men voor u Altaar <rhyme label="c" type="f">buyge</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En met open hert <rhyme label="c" type="f" corresp="#s1:l5">betuyge</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Hoord dan ook myn smeken <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Op dat 'k <rhyme label="e" type="f">meuge</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">My <rhyme label="e" type="f" corresp="#s1:l8">verheuge</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">En uw hulp van nu <rhyme label="d" type="m" corresp="#s1:l7">voortaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Offer hert, ziel en het <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Waarde Venus u te <rhyme label="b" type="m">zaam</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">'k Hoop gy zult my nooit <rhyme label="a" type="f" corresp="#s2:l1">begeven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Want gy zyt my <rhyme label="b" type="m" corresp="#s2:l2">aangenaam</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Die my altyd zal <rhyme label="c" type="f">vermaken</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En ik eeuwig zal na <rhyme label="c" type="f" corresp="#s2:l5">haken</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Blyft gy met uw raad my <rhyme label="d" type="m">by</rhyme>!</l>
        <l id="s2:l8" met="2" type="+-">Ik de <rhyme label="e" type="f">Vryheid</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">En haar <rhyme label="e" type="f" corresp="#s2:l8">blyheid</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Van my dan gantsch helder <rhyme label="d" type="m" corresp="#s2:l7">zy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laat die Vryheids dwaze <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Die haar zelfs achten het <rhyme label="b" type="m">meest</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En wilt haar niet <rhyme label="a" type="f" corresp="#s3:l1">openbaren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Wat nut ons het Huuw'lyk <rhyme label="b" type="m" corresp="#s3:l2">geeft</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Als men t'zaam kan al zyn <rhyme label="c" type="f">dagen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dan eens vreugt, dan droefheid <rhyme label="c" type="f" corresp="#s3:l5">dragen</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Daar de stille <rhyme label="d" type="m">eenzaamheid</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Altyd <rhyme label="e" type="f">zugten</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Geen <rhyme label="e" type="f" corresp="#s3:l8">genugten</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Immermeer voor ons <rhyme label="d" type="m" corresp="#s3:l7">bereid</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Gaat het al niet na <rhyme label="a" type="f">begeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Men draagt zyn leed dan niet <rhyme label="b" type="m">alleen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Men kan 'er ook al weer na <rhyme label="a" type="f" corresp="#s4:l1">teeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Is 't niet groot, zo is het <rhyme label="b" type="m" corresp="#s4:l2">kleen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Daar die eenig is zit <rhyme label="c" type="f">t'kniezen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Als hy eens raakt aan 't <rhyme label="c" type="f" corresp="#s4:l5">verliezen</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">En hem niemand helpen <rhyme label="d" type="m">kan</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Daar men <rhyme label="e" type="f">beyde</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Zig <rhyme label="e" type="f" corresp="#s4:l8">verbleyde</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Als de Vrouw eens helpt de <rhyme label="d" type="m" corresp="#s4:l7">Man</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Of schoon men slegts enkel brood <rhyme label="a" type="f">heeft</rhyme></l>
        <l id="s5:l2" met="4" type="+-">En een dronkje water <rhyme label="b" type="m">toe</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Evenwel men nog geen nood <rhyme label="a" type="f" corresp="#s5:l1">heeft</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Zyn te vreede in <rhyme label="b" type="m" corresp="#s5:l2">arremoe</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Als men 'er zig wil na <rhyme label="c" type="f">voegen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En zig zelfs kan <rhyme label="c" type="f" corresp="#s5:l5">vergenoegen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">'t Zyn de slagen van 't <rhyme label="d" type="m">geval</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">'t Geen die Vry <rhyme label="e" type="f">is</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Of gehuuwd <rhyme label="e" type="f" corresp="#s5:l8">is</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Even ligt gebeuren <rhyme label="d" type="m" corresp="#s5:l7">zal</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Daarom bid ik schoon <rhyme label="a" type="f">Godinne</rhyme>!</l>
        <l id="s6:l2" met="4" type="+-">Die ik 't allerwaardigste <rhyme label="b" type="m">schat</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat gy altyd myne <rhyme label="a" type="f" corresp="#s6:l1">zinnen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Met dat oordeel steeds <rhyme label="b" type="m" corresp="#s6:l2">omvat</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">'k Zal 'er my ook op <rhyme label="c" type="f">verlaten</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">d'Eenzaamheid ook altoos <rhyme label="c" type="f" corresp="#s6:l5">haten</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Maar agten 't Huwelyks <rhyme label="d" type="m">Verbond</rhyme>:</l>
        <l id="s6:l8" met="2" type="+-">Want dat 's 't <rhyme label="e" type="f">eerste</rhyme></l>
        <l id="s6:l9" met="2" type="+-">En het <rhyme label="e" type="f" corresp="#s6:l8">teerste</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Dat men onder Menschen <rhyme label="d" type="m" corresp="#s6:l7">vond</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
