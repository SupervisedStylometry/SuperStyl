    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AL soo mijn dit geschoncken // <rhyme label="a" type="m">is</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Breng ick't mijn lief met <rhyme label="b" type="m">sanck</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Alsoo my dit gheschoncken // <rhyme label="a" type="m" corresp="#s1:l1">is</rhyme></l>
        <l id="s1:l4" met="4" type="+-">En haer voor ghedroncken // <rhyme label="a" type="m" corresp="#s1:l1">is</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wacht ick bescheyt eer <rhyme label="b" type="m" corresp="#s1:l2">lanck</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Dringt, clinckt, schinckt, En weder <rhyme label="c" type="m">bringt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Met v lief loeroocht en <rhyme label="c" type="m" corresp="#s1:l6">winckt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Een cusken toe <rhyme label="c" type="m" corresp="#s1:l6">bedingt</rhyme>.</l>
      </lg>
