    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Geen heil is in den Echt te <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Als 't goud alleen het oog <rhyme label="b" type="m">bekoord</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Dies wil ik my aen hem <rhyme label="a" type="f" corresp="#s1:l1">verbinden</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Wiens Min my heeft het hart <rhyme label="b" type="m" corresp="#s1:l2">doorboord</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Want zuivre vergenoeging <rhyme label="c" type="m">groeyt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Alleen daer ware Liefde <rhyme label="c" type="m" corresp="#s1:l5">bloeyt</rhyme>.</l>
      </lg>
    </lg>
