<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14620</id>
    <date>
      <year-start>1638</year-start>
      <year-end>1638</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Rozemond waar gij vliedt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Aegidius Haeffacker</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IEsu soet, hooghste <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">O schoon wonder blinckent <rhyme label="b" type="m">diamant</rhyme>!</l>
        <l id="s1:l3" met="3" type="+-">Uwe gaven <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Sijn in onse <rhyme label="c" type="m" corresp="#s1:l3">noot</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Tegen strijt des duyvels <rhyme label="b" type="m" corresp="#s1:l2">valiant</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Maer eylacy <rhyme label="d" type="m">quaet</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Ben ick inder <rhyme label="d" type="m" corresp="#s1:l6">daet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Nochtans en laet ick <rhyme label="e" type="m">niet</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">U mijnen grooten <rhyme label="f" type="m">Godt</rhyme></l>
        <l id="s1:l10" met="5" type="+-">Aen te roeren met mijn suchten, <rhyme label="g" type="f">tranen</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Droefheyt is mijn meeste <rhyme label="f" type="m" corresp="#s1:l9">lodt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ach mijn hert is vol <rhyme label="a" type="m">smert</rhyme></l>
        <l id="s2:l2" met="5" type="+-">Als ick wel bedenck mijn edel <rhyme label="b" type="m">pont</rhyme>:</l>
        <l id="s2:l3" met="3" type="+-">Gaen ick door het <rhyme label="c" type="m">velt</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Mijn geest is <rhyme label="c" type="m" corresp="#s2:l3">ontstelt</rhyme>,</l>
        <l id="s2:l5" met="5" type="+-">Door u liefden wonderlick <rhyme label="b" type="m" corresp="#s2:l2">afgront</rhyme>:</l>
        <l id="s2:l6" met="3" type="+-">Merck ick dan op u <rhyme label="d" type="m">pijn</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Crachtighe <rhyme label="d" type="m" corresp="#s2:l6">medecijn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Waer door u dierbaer <rhyme label="e" type="m">bloet</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Is tot <rhyme label="f" type="m">overvloet</rhyme>,</l>
        <l id="s2:l10" met="5" type="+-">uytgegoten, <rhyme label="g" type="f">uytgeput</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Veracht, vertre'en, o Jesu <rhyme label="f" type="m" corresp="#s2:l9">soet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Keert dan weer, neemt een <rhyme label="a" type="m">keer</rhyme>,</l>
        <l id="s3:l2" met="5" type="+-">Van de sonde tot de ware <rhyme label="b" type="m">deught</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Siet de werelt <rhyme label="c" type="m">boos</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Vals en <rhyme label="c" type="m" corresp="#s3:l3">goddeloos</rhyme>,</l>
        <l id="s3:l5" met="5" type="+-">Die de zielen wegh-neemt, 's Hemels <rhyme label="b" type="m" corresp="#s3:l2">vreught</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Slaet voor u bost met <rhyme label="d" type="m">rouw</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Blijft uwen Godt <rhyme label="d" type="m" corresp="#s3:l6">getrouw</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Soo suldy zijn hier <rhyme label="e" type="m">naer</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Eeuwigh sonnen <rhyme label="f" type="m">claer</rhyme>,</l>
        <l id="s3:l10" met="5" type="+-">Met de Cherubijnen, <rhyme label="g" type="f">Throonen</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Seraphijnen <rhyme label="f" type="m" corresp="#s3:l9">wonderbaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
