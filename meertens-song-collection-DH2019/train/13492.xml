  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IK dank U lieve <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat Gy my hebt <rhyme label="b" type="m">bewaert</rhyme></l>
        <l id="s1:l3" met="3" type="-+">In deesenacht, hoe <rhyme label="a" type="f" corresp="#s1:l1">seere</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ben ik geweest <rhyme label="b" type="m" corresp="#s1:l2">beswaert</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Met duisternis <rhyme label="c" type="f">omvangen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In grooten nood en <rhyme label="d" type="m">strydt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Waer uit ik met <rhyme label="c" type="f" corresp="#s1:l5">verlangen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ben door Uw hulp <rhyme label="d" type="m" corresp="#s1:l6">bevrydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
