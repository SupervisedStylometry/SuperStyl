  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LAet niet toe myn <rhyme label="a" type="f">Vroolykheedjes</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Zeilsteen van myn volgzaam <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Dat het streng'len onzer <rhyme label="a" type="f" corresp="#s1:l1">leedjes</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Van het Veil verwonnen <rhyme label="b" type="m" corresp="#s1:l2">werd</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Vat my liever, <rhyme label="c" type="f">zielsverlangen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Vaster, dan de <rhyme label="d" type="m">Klimop-rank</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Pleeg d'ongaave Abeel te <rhyme label="c" type="f" corresp="#s1:l5">prangen</rhyme>:</l>
        <l id="s1:l8" met="4" type="+-">Druk my zo myn leven <rhyme label="d" type="m" corresp="#s1:l6">lank</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Veil sal sterven, wil men 't <rhyme label="a" type="f">scheuren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Van d'Abeel waar aan het <rhyme label="b" type="m">staat</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Liefje, 't zal ons ook <rhyme label="a" type="f" corresp="#s2:l1">gebeuren</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Zo ge u armpjes slippen <rhyme label="b" type="m" corresp="#s2:l2">laat</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Handtjes, Kusjes, Oogjes, <rhyme label="c" type="f">Kaakjes</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Hebben haakjes als de <rhyme label="d" type="m">rank</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">roosje Lief, gebruik die <rhyme label="c" type="f" corresp="#s2:l5">haakjes</rhyme></l>
        <l id="s2:l8" met="4" type="+-">En omhels me uw leven <rhyme label="d" type="m" corresp="#s2:l6">lank</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
