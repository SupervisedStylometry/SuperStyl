<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>162368</id>
    <date>
      <year-start>1700</year-start>
      <year-end>1700</year-end>
    </date>
    <tags>
      <tag standard-name="vastenavondlied" translation="Shrovetide Carnival song">vastenavondlied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Pieter de Vos</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Staet 'er jou Wijn en Bier al <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">'k Wenschje veel heyl in 't nieuwe <rhyme label="a" type="m" corresp="#s1:l1">jaer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wilje meer <rhyme label="b" type="f">weten</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Soo noodme ten <rhyme label="a" type="m" corresp="#s1:l1">eten</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Al hadje maer Bokkende Flensjes <rhyme label="a" type="m" corresp="#s1:l1">gaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Soo je wat aers te schaffen <rhyme label="a" type="m">weet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Over al ben ik toe <rhyme label="a" type="m" corresp="#s2:l1">gereet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hoenders en <rhyme label="b" type="f">duyven</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">En bouten te <rhyme label="a" type="m" corresp="#s2:l1">kluyven</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">'t Js my al evenveel wat ik <rhyme label="a" type="m" corresp="#s2:l1">eet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Weet je wel alderliefste <rhyme label="a" type="m">Mensch</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Wat ik in Jannuary <rhyme label="a" type="m" corresp="#s3:l1">wensch</rhyme>?</l>
        <l id="s3:l3" met="4" type="-+">Dat in jou <rhyme label="b" type="f">stooven</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Noyt kolen uyt <rhyme label="a" type="m" corresp="#s3:l1">dooven</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Al goot j'er een emmer nat op <rhyme label="a" type="m" corresp="#s3:l1">lens</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Js het in February <rhyme label="a" type="m">kout</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dat menje digt in de dekens <rhyme label="a" type="m" corresp="#s4:l1">dout</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Jk salje <rhyme label="b" type="f">besoeken</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">op Wafels en <rhyme label="a" type="m" corresp="#s4:l1">Koeken</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Jndienje maer Vasten avont <rhyme label="a" type="m" corresp="#s4:l1">hout</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Datje de guerige buyjege <rhyme label="a" type="m">Maert</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Nimmermeer stekelt met zijn <rhyme label="a" type="m" corresp="#s5:l1">staert</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En zijn <rhyme label="b" type="f">buyen</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Niet leggen te <rhyme label="a" type="m" corresp="#s5:l1">bruyen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Alsje gaet wandelen, rijd, of <rhyme label="a" type="m" corresp="#s5:l1">vaert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De eerste dag van de groene <rhyme label="a" type="m">April</rhyme>;</l>
        <l id="s6:l2" met="4" type="-+">Scheert men de gek met wie dat men <rhyme label="a" type="m" corresp="#s6:l1">wil</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maer ik wil <rhyme label="b" type="f">zweeren</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Komt ymant jou <rhyme label="a" type="m" corresp="#s6:l1">scheren</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Jk sny hem uyt joks wel in zijn <rhyme label="a" type="m" corresp="#s6:l1">bil</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Als 'er de soete Mey <rhyme label="a" type="m">genaekt</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Dat je dan ook in Liefde <rhyme label="a" type="m" corresp="#s7:l1">blaekt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En je laet <rhyme label="b" type="f">stippen</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Soo mal als de <rhyme label="a" type="m" corresp="#s7:l1">kippen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Als 'er het haentje maer jagt op <rhyme label="a" type="m" corresp="#s7:l1">maekt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Datje jou in Juny <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Branden moet als een roode <rhyme label="a" type="m" corresp="#s8:l1">biet</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">En jou geen <rhyme label="b" type="f">vliegen</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Of muggen <rhyme label="a" type="m" corresp="#s8:l1">bedriegen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Met steken en bijten al eer men 't <rhyme label="a" type="m" corresp="#s8:l1">ziet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Js het in July heel mooy <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Dat ikje leg in 't Hooy ter <rhyme label="a" type="m" corresp="#s9:l1">neer</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En jou mag <rhyme label="b" type="f">leyden</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Aen grazige <rhyme label="a" type="m" corresp="#s9:l1">weyden</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">En drukken en kussen jou keer op <rhyme label="a" type="m" corresp="#s9:l1">keer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Alsje in Agustus legt te <rhyme label="a" type="m">bed</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Dat 'er geen vloo jou rust <rhyme label="a" type="m" corresp="#s10:l1">belet</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Soose jou <rhyme label="b" type="f">plagen</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Ey komt het my <rhyme label="a" type="m" corresp="#s10:l1">klagen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Jk salse versuypen in 't paerde <rhyme label="a" type="m" corresp="#s10:l1">Wet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Dat je in de Kermis maent om <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Al wat Hansbeuling door <rhyme label="a" type="m" corresp="#s11:l1">beziet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Schafje dan <rhyme label="b" type="f">harsten</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Jk eet me te <rhyme label="a" type="m" corresp="#s11:l1">barsten</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">En ik sal komen so dra gy 't <rhyme label="a" type="m" corresp="#s11:l1">gebiet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Heb je in October Wijn te <rhyme label="a" type="m">veel</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Dat je bedenkt mijn drooge <rhyme label="a" type="m" corresp="#s12:l1">keel</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Wiltse niet <rhyme label="b" type="f">sparen</rhyme>,</l>
        <l id="s12:l4" met="2" type="-+">Jk magse so <rhyme label="a" type="m" corresp="#s12:l1">garen</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Al wasse met suyker en <rhyme label="a" type="m" corresp="#s12:l1">Pijp-Caneel</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Alsje in November worsten <rhyme label="a" type="m">stopt</rhyme>;</l>
        <l id="s13:l2" met="4" type="-+">Datjer niet een te barsten <rhyme label="a" type="m" corresp="#s13:l1">propt</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">En daer geen <rhyme label="b" type="f">katten</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">Ten huyse uyt <rhyme label="a" type="m" corresp="#s13:l1">spatten</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Met beuling, dat daerom niet wort <rhyme label="a" type="m" corresp="#s13:l1">gemopt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Eer ik de Winter maent <rhyme label="a" type="m">besluyt</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Datje dan wesen meugt de <rhyme label="a" type="m" corresp="#s14:l1">bruyd</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Dan zal ik <rhyme label="b" type="f">zingen</rhyme>,</l>
        <l id="s14:l4" met="2" type="-+">Van koddige <rhyme label="a" type="m" corresp="#s14:l1">dingen</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">En scheyden 'er nou maer uyt, uyt, <rhyme label="a" type="m" corresp="#s14:l1">uyt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Sagjes ik eyndige al te <rhyme label="a" type="m">rad</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Weetje wel wat ik bedongen <rhyme label="a" type="m" corresp="#s15:l1">had</rhyme>?</l>
        <l id="s15:l3" met="4" type="-+">Kannen te <rhyme label="b" type="f">vegen</rhyme>,</l>
        <l id="s15:l4" met="2" type="-+">En Kruyken te <rhyme label="a" type="m" corresp="#s15:l1">legen</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Mijn hoopje, mijn troosje, mijn liefje, mijn <rhyme label="a" type="m" corresp="#s15:l1">schat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
