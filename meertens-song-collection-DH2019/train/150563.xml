    <melody>Psalm 092 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEt zijn gegronde <rhyme label="a" type="f">dingen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Te uyten Godes <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En u, den hooghsten <rhyme label="b" type="m" corresp="#s1:l2">Heer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Het loff uws naems te <rhyme label="a" type="f" corresp="#s1:l1">singen</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Te singen van de <rhyme label="c" type="f">soetheydt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die ’tGodlijck hert <rhyme label="d" type="m">bewoont</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En sich soo heerlijck <rhyme label="d" type="m" corresp="#s1:l6">toont</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In uw’ getrouwe <rhyme label="c" type="f" corresp="#s1:l5">goedtheydt</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Te singen als de <rhyme label="a" type="f">Sonne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">De lucht door-purpert <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s2:l3" met="3" type="-+">En d’aerd’ ontluycking <rhyme label="b" type="m" corresp="#s2:l2">geeft</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Door’t nieuwe lichts <rhyme label="a" type="f" corresp="#s2:l1">vergonnen</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Te singen in de <rhyme label="c" type="f">stilheydt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van d’aengevangen <rhyme label="d" type="m">nacht</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als ’tbeeldeloos <rhyme label="d" type="m" corresp="#s2:l6">gedacht</rhyme></l>
        <l id="s2:l8" met="3" type="-+">De sinnen bet in <rhyme label="c" type="f" corresp="#s2:l5">wilheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Te singen op het <rhyme label="a" type="f">galmen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En dreunende <rhyme label="b" type="m">geluydt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van cither, harp, en <rhyme label="b" type="m" corresp="#s3:l2">fluyt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">By ’tmaet-gedicht der <rhyme label="a" type="f" corresp="#s3:l1">psalmen</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Vws goedicheydts <rhyme label="c" type="f">aenmercken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Deur-vrolijckt, Heer, mijn <rhyme label="d" type="m">geest</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Dies houd ick <rhyme label="d" type="m" corresp="#s3:l6">zielen-feest</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In uwer handen <rhyme label="c" type="f" corresp="#s3:l5">wercken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wat zijn, wat zijn uw’ <rhyme label="a" type="f">daden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Vw’ daden, grootste <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Vol achtbaerheydt en <rhyme label="b" type="m" corresp="#s4:l2">eer</rhyme>!</l>
        <l id="s4:l4" met="3" type="-+">Hoe diep uws herten <rhyme label="a" type="f" corresp="#s4:l1">raden</rhyme>!</l>
        <l id="s4:l5" met="3" type="-+">Een dwaes kan’t niet <rhyme label="c" type="f">bevroeden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Een door door-grondt het <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Als hy den wel-standt <rhyme label="d" type="m" corresp="#s4:l6">siet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Van quaden boven <rhyme label="c" type="f" corresp="#s4:l5">goeden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Vast bloeyt hier alle <rhyme label="a" type="f">dader</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Der <rhyme label="b" type="m">ongerechticheydt</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">’tHeeft schijn van <rhyme label="b" type="m" corresp="#s5:l2">onbescheydt</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Maer brengt uw’ ooge <rhyme label="a" type="f" corresp="#s5:l1">nader</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">’tIs waer, de boose <rhyme label="c" type="f">bloeyen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Maer bloeyen als het <rhyme label="d" type="m">kruydt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Dat schielijck hoogh <rhyme label="d" type="m" corresp="#s5:l6">op-spruyt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En datmen uyt gaet <rhyme label="c" type="f" corresp="#s5:l5">roeyen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghy, Heere, blijft <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s6:l2" met="3" type="-+">In aller <rhyme label="b" type="m">eeuwicheydt</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Verderff is hun <rhyme label="b" type="m" corresp="#s6:l2">bereydt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die met uw’ Godtheydt <rhyme label="a" type="f" corresp="#s6:l1">streven</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Tot uwe <rhyme label="c" type="f">salicheden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Wordt allen die der <rhyme label="d" type="m">sond</rhyme>’</l>
        <l id="s6:l7" met="3" type="-+">Sijn hert en handen <rhyme label="d" type="m" corresp="#s6:l6">gont</rhyme></l>
        <l id="s6:l8" met="3" type="-+">De toe-gang <rhyme label="c" type="f" corresp="#s6:l5">afgesneden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maer my sult ghy <rhyme label="a" type="f">verhoogen</rhyme>:</l>
        <l id="s7:l2" met="3" type="-+">Soo dat ick, <rhyme label="b" type="m">onbeducht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Als d’Eenhoorn, in de <rhyme label="b" type="m" corresp="#s7:l2">lucht</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Het hoofdt sal richten <rhyme label="a" type="f" corresp="#s7:l1">mogen</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Ghy hebt my <rhyme label="c" type="f">over-streken</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Met balssem-vocht; wiens <rhyme label="d" type="m">deuchdt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">De frische groent der <rhyme label="d" type="m" corresp="#s7:l6">jeuchdt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Ten aenschijn-uyt doet <rhyme label="c" type="f" corresp="#s7:l5">spreken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Mijn ooge sal <rhyme label="a" type="f">verblijden</rhyme></l>
        <l id="s8:l2" met="3" type="-+">In’t ongeluckigh <rhyme label="b" type="m">endt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">En wel-verschuld’ <rhyme label="b" type="m" corresp="#s8:l2">ellendt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Van haer die my <rhyme label="a" type="f" corresp="#s8:l1">benijden</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Mijn oor sal sich <rhyme label="c" type="f">verlusten</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In’t hooren van den <rhyme label="d" type="m">val</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Die hun toe-komen <rhyme label="d" type="m" corresp="#s8:l6">sal</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Die tegen my sich <rhyme label="c" type="f" corresp="#s8:l5">rusten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Gelijck de Palm, off <rhyme label="a" type="f">Ceder</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Op Libanon <rhyme label="b" type="m">geplant</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Schoon bloeyt, en houdt sijn <rhyme label="b" type="m" corresp="#s9:l2">standt</rhyme></l>
        <l id="s9:l4" met="3" type="-+">In allerhande <rhyme label="a" type="f" corresp="#s9:l1">weder</rhyme>;</l>
        <l id="s9:l5" met="3" type="-+">Schoon bloeyt en ciert de <rhyme label="c" type="f">aerde</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Rondsom met sijn <rhyme label="d" type="m">geslacht</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">Soo groenen oock in <rhyme label="d" type="m" corresp="#s9:l6">kracht</rhyme></l>
        <l id="s9:l8" met="3" type="-+">De vroom’ en <rhyme label="c" type="f" corresp="#s9:l5">Gode-waerde</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">O! die maer in Godts <rhyme label="a" type="f">wooning</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Een vasten wortel <rhyme label="b" type="m">heeft</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Geveste Deuchde <rhyme label="b" type="m" corresp="#s10:l2">geeft</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Gewisse <rhyme label="a" type="f" corresp="#s10:l1">Deuchd-belooning</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Hy sal in vollen <rhyme label="c" type="f">bloeysel</rhyme></l>
        <l id="s10:l6" met="3" type="-+">In Godes voor-hoff <rhyme label="d" type="m">staen</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">De grijse tijdt koom <rhyme label="d" type="m" corresp="#s10:l6">aen</rhyme>:</l>
        <l id="s10:l8" met="3" type="-+">Noch duert sijn jeuchdigh <rhyme label="c" type="f" corresp="#s10:l5">groeysel</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">’sMans weelden zijn <rhyme label="a" type="f">trompetten</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Van Godts <rhyme label="b" type="m">gerechtiche’en</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">Hier kan, en sal met <rhyme label="b" type="m" corresp="#s11:l2">re’en</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Het vrome hert sich <rhyme label="a" type="f" corresp="#s11:l1">setten</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">In Godt wil ick my <rhyme label="c" type="f">gronden</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Wiens geenderhande <rhyme label="d" type="m">daedt</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Vermaeghschapt met het <rhyme label="d" type="m" corresp="#s11:l6">quaedt</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Steeds suyver wordt <rhyme label="c" type="f" corresp="#s11:l5">gevonden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
