<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19351</id>
    <date>
      <year-start>1552</year-start>
      <year-end>1554</year-end>
    </date>
    <location>
      <orginal-name>antwerpen]</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Onze vader in hemelrijk</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Godt vader inden <rhyme label="a" type="m">Hemelrijck</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Godt soen, godt heylige gheest <rhyme label="a" type="m" corresp="#s1:l1">ghelijc</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Ghy heylighe <rhyme label="b" type="m">driuuldicheyt</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Een eenich Godt in <rhyme label="b" type="m" corresp="#s1:l3">eewicheyt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Op v toesegghinghe soo bidde <rhyme label="c" type="m">ick</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Wilt ons verhooren <rhyme label="c" type="m" corresp="#s1:l5">ghenadelijck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Och lieue Heer Godt spaert ons <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Wilt ons na onse verdienst niet <rhyme label="a" type="m" corresp="#s2:l1">loonen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Ontfermt v ons duer v <rhyme label="b" type="m">ghenade</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Lijf eer en goet behouwet van <rhyme label="b" type="m" corresp="#s2:l3">schade</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Weest ons ghenadich en helpt ons <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s2:l6" met="4" type="-+">En verlaet ons nv noch <rhyme label="c" type="m" corresp="#s2:l5">nemmermeer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Bewaert ons van des duyuels <rhyme label="a" type="m">list</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Uan zijn bedroch en van allen <rhyme label="a" type="m" corresp="#s3:l1">twist</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Keert van ons zijn vierich <rhyme label="b" type="m">huysghesin</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Helpt dat hy ons niet en <rhyme label="b" type="m" corresp="#s3:l3">verwin</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Och lief Heer Godt en verlaet ons <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Als ons die boose gheest <rhyme label="c" type="m" corresp="#s3:l5">aensiet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Behuet ons Godt tot allen <rhyme label="a" type="m">stonden</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Uan dwalende seckten schande en <rhyme label="a" type="m" corresp="#s4:l1">sonden</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Uan crijch, vyantschap, haet en <rhyme label="b" type="m">nijt</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Uan water vier en van dieren <rhyme label="b" type="m" corresp="#s4:l3">tijt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Uan die Pestelency ende crancheyt <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Uan onweer, ende van den eewyghen <rhyme label="c" type="m" corresp="#s4:l5">doot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Behoet ons Heer tot alder <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Maeckt ons al onse sonden <rhyme label="a" type="m" corresp="#s5:l1">quijt</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Och lieue Heer en verlaet ons <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Als wy comen voort tleste <rhyme label="b" type="m" corresp="#s5:l3">ghericht</rhyme></l>
        <l id="s5:l5" met="4" type="-+">En van dees eewyghen doots <rhyme label="c" type="m">veruaert</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Och help Heer Godt ons dan <rhyme label="c" type="m" corresp="#s5:l5">bewaert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wy arme sondaers bidden <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Helpt ons Ihesus Christus lieue <rhyme label="a" type="m" corresp="#s6:l1">Heer</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Helpt ons duer v heylighe <rhyme label="b" type="m">ghebot</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Duer v verrijsenisse en <rhyme label="b" type="m" corresp="#s6:l3">hemeluaert</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Duer uwen strijt en dierbaer <rhyme label="c" type="m">bloet</rhyme></l>
        <l id="s6:l6" met="4" type="-+">U heylich cruys en v bitter <rhyme label="c" type="m" corresp="#s6:l5">doot</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">U heylighe cristelijcke kercke o <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Bewaert die in v woort met oprechte <rhyme label="a" type="m" corresp="#s7:l1">leer</rhyme></l>
        <l id="s7:l3" met="4" type="-+">En in een heylich leuen die dienaers <rhyme label="b" type="m">dijn</rhyme></l>
        <l id="s7:l4" met="4" type="-+">En die Christum soecken en belijden <rhyme label="b" type="m" corresp="#s7:l3">reijn</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Duer twoort en gheest gheeft ons die <rhyme label="c" type="m">cracht</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Want het staet al in uwer <rhyme label="c" type="m" corresp="#s7:l5">macht</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">In v lant seynt doch ghetrouwe arbeyders <rhyme label="a" type="m">seere</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Die alle valsche seckten en ketterijen af <rhyme label="a" type="m" corresp="#s8:l1">keeren</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Ende allen ergernissen <rhyme label="b" type="m">verweeren</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Alle die noch dwalen Heer brenckt te <rhyme label="b" type="m" corresp="#s8:l3">recht</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Ende leydtse duer v woort inder waerheyt <rhyme label="c" type="m">wech</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Op dat si kennen dwoort als een getrouwe <rhyme label="c" type="m" corresp="#s8:l5">knecht</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Keert o Heer duer v machtighe <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Al die v woort noch doen <rhyme label="a" type="m" corresp="#s9:l1">wederstant</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Uan des grouwelijcke Turers ende wolfs <rhyme label="b" type="m">geslacht</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Och Christus v vrome christenen <rhyme label="b" type="m" corresp="#s9:l3">wacht</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Alle valsche leeraers keert en worpt in deeuwich <rhyme label="c" type="m">vuer</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Maer zijt ons een sterck Casteel en <rhyme label="c" type="m" corresp="#s9:l5">muer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Beschermpt o Heer ons lijf en <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Uerlost ons van oploop crijch en <rhyme label="a" type="m" corresp="#s10:l1">bloet</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Den Keyser ende die Cueninghen die Uorsten ende <rhyme label="b" type="m">Heeren</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Gheeft huer v ghenade dat si huer volc te recht <rhyme label="b" type="m" corresp="#s10:l3">regeren</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Gheeft Heer dat si mueghen houden vrede <rhyme label="c" type="m">altijt</rhyme></l>
        <l id="s10:l6" met="4" type="-+">En bewaertse van tweedracht en <rhyme label="c" type="m" corresp="#s10:l5">strijt</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Gheeft Heer ons Keyser een goet <rhyme label="a" type="m">gheluck</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Helpt dat hy v woort niet en <rhyme label="a" type="m" corresp="#s11:l1">verdruck</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Beschermt Heer alle ons <rhyme label="b" type="m">Lantsheeren</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Met uwen heylighen geest so wiltse doch <rhyme label="b" type="m" corresp="#s11:l3">leeren</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Onser stadt raet en gantsche <rhyme label="c" type="m">ghemeynt</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Die laet v o Heere beuolen <rhyme label="c" type="m" corresp="#s11:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Allen v Christen helpt wt allen <rhyme label="a" type="m">varen</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Wilt die met uwen gheest <rhyme label="a" type="m" corresp="#s12:l1">bewaren</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Sterckt en behout die herten <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Die noch zijn in ancxsten <rhyme label="b" type="m" corresp="#s12:l3">swaer</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Die naesten neempt in uwer <rhyme label="c" type="m">handt</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Die weeuwen troost in huer <rhyme label="c" type="m" corresp="#s12:l5">ellendt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">De bevruchte vrouwe ende suyghende <rhyme label="a" type="m">al</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Bewaert doch die kinderkens van <rhyme label="a" type="m" corresp="#s13:l1">ongheual</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Helpt oock alle die daer zijn <rhyme label="b" type="m">cranck</rhyme></l>
        <l id="s13:l4" met="4" type="-+">Dat huer ghelooft aen v niet en zij <rhyme label="b" type="m" corresp="#s13:l3">wanck</rhyme></l>
        <l id="s13:l5" met="4" type="-+">Ende maecktse aen huer siel <rhyme label="c" type="m">ghesont</rhyme></l>
        <l id="s13:l6" met="4" type="-+">Ende troostse al inder lester <rhyme label="c" type="m" corresp="#s13:l5">stont</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Och lieue Heer gheeft oock <rhyme label="a" type="m">ghedult</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Alle die noch lijden sonder <rhyme label="a" type="m" corresp="#s14:l1">schult</rhyme></l>
        <l id="s14:l3" met="4" type="-+">En verlostse wt der vyanden <rhyme label="b" type="m">handt</rhyme></l>
        <l id="s14:l4" met="4" type="-+">Huer ellende ende gheuanckenis <rhyme label="b" type="m" corresp="#s14:l3">went</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Troost die oock inder lester <rhyme label="c" type="m">noot</rhyme></l>
        <l id="s14:l6" met="4" type="-+">Die daer moeten lijden de <rhyme label="c" type="m" corresp="#s14:l5">doot</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Oock alle die ons vyandt <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Uergheeft haer huer misdaet ende <rhyme label="a" type="m" corresp="#s15:l1">sonden</rhyme></l>
        <l id="s15:l3" met="4" type="-+">Gheeft dat wy huer oock gheerne <rhyme label="b" type="m">vergheuen</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Met alle menschen in vrede <rhyme label="b" type="m" corresp="#s15:l3">leuen</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Helpt dat alle sondaers huer <rhyme label="c" type="m">bekeeren</rhyme></l>
        <l id="s15:l6" met="4" type="-+">Wt herten gront v ghenade <rhyme label="c" type="m" corresp="#s15:l5">begheeren</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">O Heere die vruchten op dat <rhyme label="a" type="m">lant</rhyme></l>
        <l id="s16:l2" met="4" type="-+">Die wilt ons gheuen dijn milde <rhyme label="a" type="m" corresp="#s16:l1">handt</rhyme></l>
        <l id="s16:l3" met="4" type="-+">En wiltse bewaren van haghel en <rhyme label="b" type="m">schade</rhyme></l>
        <l id="s16:l4" met="4" type="-+">Dat wijse te recht gebruycken gheeft die <rhyme label="b" type="m" corresp="#s16:l3">genade</rhyme></l>
        <l id="s16:l5" met="4" type="-+">Dat si ons lichaem niet en <rhyme label="c" type="m">beswaren</rhyme></l>
        <l id="s16:l6" met="4" type="-+">Noch ons herte van v en <rhyme label="c" type="m" corresp="#s16:l5">keeren</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">O Iesus Cristus warachtich Godts <rhyme label="a" type="m">soen</rhyme></l>
        <l id="s17:l2" met="4" type="-+">O Iesus Christis der ghenaden <rhyme label="a" type="m" corresp="#s17:l1">troon</rhyme></l>
        <l id="s17:l3" met="4" type="-+">O Iesus Christus ghy Godts <rhyme label="b" type="m">Lam</rhyme></l>
        <l id="s17:l4" met="4" type="-+">Dat der weerelt sonden droech en wech <rhyme label="b" type="m" corresp="#s17:l3">nam</rhyme></l>
        <l id="s17:l5" met="4" type="-+">O ghy ghebenedide <rhyme label="c" type="m">saet</rhyme></l>
        <l id="s17:l6" met="4" type="-+">Uerlost ons doch van alle <rhyme label="c" type="m" corresp="#s17:l5">quaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
