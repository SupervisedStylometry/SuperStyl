    <melody>Rozemond die lag gedoken</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>anoniem</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DOen Apol sijn heete <rhyme label="a" type="f">stralen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hadt verschooten van den <rhyme label="b" type="m">dach</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Gingh ick eensaem droevigh <rhyme label="a" type="f" corresp="#s1:l1">dwalen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Door de boomen daer ick <rhyme label="b" type="m" corresp="#s1:l2">sagh</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat een Rey van Nimphjens <rhyme label="c" type="m">stondt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Onder die was <rhyme label="c" type="m" corresp="#s1:l5">Rosemont</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Rosemont verliet het <rhyme label="a" type="f">reyen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En sy gingh haer wat <rhyme label="b" type="m">vertreen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Nae de groen begraesde <rhyme label="a" type="f" corresp="#s2:l1">Weyen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Op haer trots en vlugge <rhyme label="b" type="m" corresp="#s2:l2">leen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En met Hemels soet <rhyme label="c" type="m">gheluyt</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Songh aldaer dit Liedtje <rhyme label="c" type="m" corresp="#s2:l5">uyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">D’Hemel wil hem rijcklijck <rhyme label="a" type="f">loonen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Die van herten meendt, en <rhyme label="b" type="m">mint</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Iae sijn hooft met Laurier <rhyme label="a" type="f" corresp="#s3:l1">kroonen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Die sijn lusten toomd’ en <rhyme label="b" type="m" corresp="#s3:l2">bindt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En bewaert sijn lent’ en <rhyme label="c" type="m">jeughd</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Voor de trouw en Echte <rhyme label="c" type="m" corresp="#s3:l5">vreughd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die sijn lijf, sijn lust en <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Tot een offer voor de <rhyme label="b" type="m">geen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wil gewilligh over <rhyme label="a" type="f" corresp="#s4:l1">geven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Wien begaeft en soete <rhyme label="b" type="m" corresp="#s4:l2">reen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Iae wiens deughd en <rhyme label="c" type="m">deftigheydt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Kroon en Scepter by hen <rhyme label="c" type="m" corresp="#s4:l5">sweydt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sulcke liefd sal altijt <rhyme label="a" type="f">branden</rhyme>.</l>
        <l id="s5:l2" met="4" type="+-">Soo dat oock den <rhyme label="b" type="m">Oceaen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Die op onse stercke <rhyme label="a" type="f" corresp="#s5:l1">stranden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Soo veel stormen heeft <rhyme label="b" type="m" corresp="#s5:l2">gedaen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Door haer blasen en <rhyme label="c" type="m">getier</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Noyt sal blusschen ‘t Echte <rhyme label="c" type="m" corresp="#s5:l5">vier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wat kan Keysers Rijck dan <rhyme label="a" type="f">baten</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Alsmen vreed’ noch vreught en <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Want men moet het hier al <rhyme label="a" type="f" corresp="#s6:l1">laten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Maer die vroom en vredigh <rhyme label="b" type="m" corresp="#s6:l2">leeft</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Wert met Hemels Nectar <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Hier en oock hier nae <rhyme label="c" type="m" corresp="#s6:l5">gevoedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Nae ‘t gesangh gingh ick haer <rhyme label="a" type="f">groeten</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">En met groot <rhyme label="b" type="m">eerbiedigheyt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Boogh ick neder voor haer <rhyme label="a" type="f" corresp="#s7:l1">voeten</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Maer sy hield my op en <rhyme label="b" type="m" corresp="#s7:l2">seydt</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Waer toe dese <rhyme label="c" type="m">Kortesy</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Herder hier betoondt aen <rhyme label="c" type="m" corresp="#s7:l5">my</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Nimph seyd ick u sangh en <rhyme label="a" type="f">stemme</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Konnen yeders hert en <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Konnen al mijn lusten <rhyme label="a" type="f" corresp="#s8:l1">temme</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">En bewegen tot u u <rhyme label="b" type="m" corresp="#s8:l2">Min</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Ghy alleen mijn <rhyme label="c" type="m">Rosemondt</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Hebt my door u Liefd’ <rhyme label="c" type="m" corresp="#s8:l5">verwondt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">’t Sijn u Bruyn-geooghde <rhyme label="a" type="f">lontkjes</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">‘t Is u Deught en gaeuw <rhyme label="b" type="m">verstandt</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Die door soete <rhyme label="a" type="f" corresp="#s9:l1">Minne-vonckjes</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Hebben my geheel <rhyme label="b" type="m" corresp="#s9:l2">vermand</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Kusjes van u <rhyme label="c" type="m">Maeghde-bloos</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Sijn my soeter als <rhyme label="c" type="m" corresp="#s9:l5">Ambroos</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Herder seyse laet u <rhyme label="a" type="f">vleyen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Want ghy soud een <rhyme label="b" type="m">Maeghden-hert</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Licht verlocken en <rhyme label="a" type="f" corresp="#s10:l1">verleyen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Neen mijn Nimph seydt ick dats <rhyme label="b" type="m" corresp="#s10:l2">vardt</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Dat ick yets versoecken <rhyme label="c" type="m">souw</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Aen een Maeghd’ als Liefd’ en <rhyme label="c" type="m" corresp="#s10:l5">Trouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
