    <tags>
      <tag standard-name="danklied" translation="gratitude song">danklied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Psalm 061 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Uilenbroek</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Groot zijn Heer! uw <rhyme label="a" type="f">goedigheden</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Die gy <rhyme label="a" type="f" corresp="#s1:l1">heden</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En ook eertijds hebt <rhyme label="b" type="m">gedaan</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Nieuwlijks doet gy uw <rhyme label="c" type="f">genaden</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s1:l4">weldaaden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Weder voor onz’ ogen <rhyme label="b" type="m" corresp="#s1:l3">staan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Gy voldoet nu Heer der <rhyme label="a" type="f">Heeren</rhyme>!</l>
        <l id="s2:l2" met="2" type="+-">Ons <rhyme label="a" type="f" corresp="#s2:l1">begeeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Gy verhoort der zielen <rhyme label="b" type="m">klacht</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">Onze droefheit komt gy <rhyme label="c" type="f">wenden</rhyme></l>
        <l id="s2:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s2:l4">vollenden</rhyme></l>
        <l id="s2:l6" met="4" type="+-">’t Geen zoo lange was <rhyme label="b" type="m" corresp="#s2:l3">verwacht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Als de ziele was <rhyme label="a" type="f">beladen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Onder <rhyme label="a" type="f" corresp="#s3:l1">quaden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Uwe handt deë ons <rhyme label="b" type="m">bestaan</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Uwe gunst, quam ons <rhyme label="c" type="f">beschijnen</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Onder ’t <rhyme label="c" type="f" corresp="#s3:l4">quijnen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En deë ’t quaadt als rook <rhyme label="b" type="m" corresp="#s3:l3">vergaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Heer! gy wilde noch wat <rhyme label="a" type="f">sparen</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s4:l1">bewaren</rhyme></l>
        <l id="s4:l3" met="4" type="+-">’t Leven; van die tot uw <rhyme label="b" type="m">riep</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Gy verhoorden ’t klagen <rhyme label="c" type="f">zuchten</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Van die <rhyme label="c" type="f" corresp="#s4:l4">vluchten</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Tot uw, in het <rhyme label="b" type="m" corresp="#s4:l3">elends-diep</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">En wilt aller toevlucht <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Die u <rhyme label="a" type="f" corresp="#s5:l1">vrezen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">In de zeer benauwde <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Is uw naam een sterke <rhyme label="c" type="f">Toren</rhyme>!</l>
        <l id="s5:l5" met="2" type="+-"><rhyme label="c" type="f" corresp="#s5:l4">d’Uitverkoren</rhyme>’</l>
        <l id="s5:l6" met="4" type="+-">Vliedt daar heen, en wordt <rhyme label="b" type="m" corresp="#s5:l3">bevrijdt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Gy toont in benauwtheits <rhyme label="a" type="f">tijden</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s6:l1">Medelijden</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">In de vreeze van de <rhyme label="b" type="m">doodt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Komt gy weer verlossing <rhyme label="c" type="f">geven</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Aan die <rhyme label="c" type="f" corresp="#s6:l4">leven</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">In de allerdiepste <rhyme label="b" type="m" corresp="#s6:l3">noodt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Want al liep ons alles <rhyme label="a" type="f">tegen</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Uwe <rhyme label="a" type="f" corresp="#s7:l1">wegen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Gaan ver’ boven ons <rhyme label="b" type="m">verstandt</rhyme>;</l>
        <l id="s7:l4" met="4" type="+-">Gy kondt tegen al ’t <rhyme label="c" type="f">vermoeden</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Ons <rhyme label="c" type="f" corresp="#s7:l4">behoeden</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Door uw wijze sterke <rhyme label="b" type="m" corresp="#s7:l3">handt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Gy doet de benauwtheyts <rhyme label="a" type="f">baaren</rhyme>!</l>
        <l id="s8:l2" met="2" type="+-">Hene <rhyme label="a" type="f" corresp="#s8:l1">varen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Die ons dreigden groot <rhyme label="b" type="m">elend</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">En wilt die in nooden <rhyme label="c" type="f">zwerven</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Niet <rhyme label="c" type="f" corresp="#s8:l4">verderven</rhyme>:</l>
        <l id="s8:l6" met="4" type="+-">Want gy haar uw hulpe <rhyme label="b" type="m" corresp="#s8:l3">zend</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Gy geeft Heer! op onze <rhyme label="a" type="f">wegen</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Noch uw <rhyme label="a" type="f" corresp="#s9:l1">zeen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Dag aan dag in <rhyme label="b" type="m">overvloedt</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Wien ziel kan uw goedheits <rhyme label="c" type="f">schatten</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Recht <rhyme label="c" type="f" corresp="#s9:l4">bevatten</rhyme>?</l>
        <l id="s9:l6" met="4" type="+-">Die gy aan uw’ kindren <rhyme label="b" type="m" corresp="#s9:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Hoe gy ’t dan met onze <rhyme label="a" type="f">zaken</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-">Voort zult <rhyme label="a" type="f" corresp="#s10:l1">maken</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Geven wy in uwe <rhyme label="b" type="m">handt</rhyme>;</l>
        <l id="s10:l4" met="4" type="+-">Geeft gy? of wilt gy <rhyme label="c" type="f">beroven</rhyme>?</l>
        <l id="s10:l5" met="2" type="+-">Wy <rhyme label="c" type="f" corresp="#s10:l4">beloven</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Ons te voegen naa dien <rhyme label="b" type="m" corresp="#s10:l3">standt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Wy verplichten onze <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s11:l2" met="2" type="+-">Op te <rhyme label="a" type="f" corresp="#s11:l1">dragen</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">d’Offeren van <rhyme label="b" type="m">dankbaarheit</rhyme>;</l>
        <l id="s11:l4" met="4" type="+-">Voor zoo veele gunsts <rhyme label="c" type="f">bewijzen</rhyme>,</l>
        <l id="s11:l5" met="2" type="+-">U te <rhyme label="c" type="f" corresp="#s11:l4">prijzen</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Vriendelijke <rhyme label="b" type="m" corresp="#s11:l3">Majesteyt</rhyme>!</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Laaten wy ons nu ten <rhyme label="a" type="f">tijden</rhyme>,</l>
        <l id="s12:l2" met="2" type="+-">Dan <rhyme label="a" type="f" corresp="#s12:l1">verblijden</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Loven Godt ons leven <rhyme label="b" type="m">lang</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Komt dan ’s Heeren <rhyme label="c" type="f">lievelingen</rhyme>!</l>
        <l id="s12:l5" met="2" type="+-">Helpt ons <rhyme label="c" type="f" corresp="#s12:l4">zingen</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Vroolijk uit, dit <rhyme label="b" type="m" corresp="#s12:l3">bly-gezang</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">O mijn Ziele! rijst naa <rhyme label="a" type="f">boven</rhyme>,</l>
        <l id="s13:l2" met="2" type="+-">Godt te <rhyme label="a" type="f" corresp="#s13:l1">loven</rhyme></l>
        <l id="s13:l3" met="4" type="+-">Is uw werk, in deze <rhyme label="b" type="m">tijt</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Komt met alle binnen <rhyme label="c" type="f">krachten</rhyme>,</l>
        <l id="s13:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s13:l4">gedachten</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Toont dat gy recht dankbaar <rhyme label="b" type="m" corresp="#s13:l3">zijt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Gy verloste! looft den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s14:l2" met="2" type="+-">Doet hem <rhyme label="a" type="f" corresp="#s14:l1">Eere</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Noemt nu zijn <rhyme label="b" type="m">weldadigheyt</rhyme>;</l>
        <l id="s14:l4" met="4" type="+-">Gaat hem zoele, lijf, en <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s14:l5" met="2" type="+-">Over <rhyme label="c" type="f" corresp="#s14:l4">geven</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Looft hem in der <rhyme label="b" type="m" corresp="#s14:l3">eeuwigheit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
