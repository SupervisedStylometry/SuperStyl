<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14599</id>
    <date>
      <year-start>1638</year-start>
      <year-end>1638</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb de groene straten</melody>
  <author>Aegidius Haeffacker</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHebenedijt, o <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ons spijs en onsen <rhyme label="b" type="m">dranck</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Welck 'wy tot uwer <rhyme label="a" type="f" corresp="#s1:l1">eere</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghenieten al' in <rhyme label="b" type="m" corresp="#s1:l2">danck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Indien dat gh' al de <rhyme label="a" type="f">dieren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Gheeft spijse, naer hun <rhyme label="b" type="m">wensch</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ja met de kleynste <rhyme label="a" type="f" corresp="#s2:l1">mieren</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Wat sult ghy doen den <rhyme label="b" type="m" corresp="#s2:l2">mensch</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wie soude kunnen <rhyme label="a" type="f">prijsen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">O Heer', uw' milde <rhyme label="b" type="m">handt</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Ghy komt ons allen <rhyme label="a" type="f" corresp="#s3:l1">spijsen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In dit bedruckte <rhyme label="b" type="m" corresp="#s3:l2">lant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Loff sy u, Godt <rhyme label="a" type="f">gepresen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Door wien ons lichaem <rhyme label="b" type="m">groeyt</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Laet ons nu matigh <rhyme label="a" type="f" corresp="#s4:l1">wesen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In eerbaerheyt ons <rhyme label="b" type="m" corresp="#s4:l2">voeyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ons' zielen wilt <rhyme label="a" type="f">vervullen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met 't levend' Hemels <rhyme label="b" type="m">broot</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Waer toe wy voortaen <rhyme label="a" type="f" corresp="#s5:l1">sullen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Versterckt sijn tot de <rhyme label="b" type="m" corresp="#s5:l2">doot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gheeft ons in 't eeuwigh <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Den dranck daer elck na <rhyme label="b" type="m">haeckt</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Het broodt wilt ons daer <rhyme label="a" type="f" corresp="#s6:l1">geven</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat noyt tongh' heeft <rhyme label="b" type="m" corresp="#s6:l2">gesmaeckt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghebenedijt <rhyme label="a" type="f">midtsgaders</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dees' spijse minst en <rhyme label="b" type="m">meest</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Al in den Naem des <rhyme label="a" type="f" corresp="#s7:l1">Vaders</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Des Soons, des heylighs <rhyme label="b" type="m" corresp="#s7:l2">Gheest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
