<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21437</id>
    <date>
      <year-start>1558</year-start>
      <year-end>1558</year-end>
    </date>
    <location>
      <orginal-name>emden]</orginal-name>
      <standard-name>Emden, Germany</standard-name>
      <coordinates>
        <lon>53.3670442</lon>
        <lat>7.2058004</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Het lacht nu al het schone jaar is vol vreugden</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HOe moghen wy doch o Heere <rhyme label="a" type="f">almachtich</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Also totter ydelheyt zijn <rhyme label="b" type="f">gheresen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Dat wy vergheten v goetheyt <rhyme label="a" type="f" corresp="#s1:l1">crachtich</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Die ghy ons ghetoocht hebt, Heere <rhyme label="b" type="f" corresp="#s1:l2">ghepresen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Want also wy <rhyme label="b" type="f" corresp="#s1:l2">lesen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Doen wylien inder <rhyme label="c" type="m">doot</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Waren <rhyme label="b" type="f" corresp="#s1:l2">verwesen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Door onse sonden <rhyme label="c" type="m" corresp="#s1:l6">snoot</rhyme></l>
        <l id="s1:l9" met="4" type="-+">So verlostet ghy ons, o Heere <rhyme label="c" type="m" corresp="#s1:l6">groot</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Met v bloet <rhyme label="c" type="m" corresp="#s1:l6">root</rhyme></l>
        <l id="s1:l11" met="3" type="+-">En liet v goetheyt op ons <rhyme label="d" type="f">schijnen</rhyme></l>
        <l id="s1:l12" met="4" type="-+">Maer nv so doet die ydelheyt <rhyme label="c" type="m" corresp="#s1:l6">bloot</rhyme></l>
        <l id="s1:l13" met="2" type="-+">Altijts <rhyme label="c" type="m" corresp="#s1:l6">exploot</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Den gheest, want tis groote <rhyme label="d" type="f" corresp="#s1:l11">pijne</rhyme></l>
        <l id="s1:l15" met="4" type="-+">Der ydelheyt onderworpen <rhyme label="d" type="f" corresp="#s1:l11">tzijne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dus helpt ons, o God, en wilt nv <rhyme label="a" type="f">vermeeren</rhyme></l>
        <l id="s2:l2" met="4" type="-+">U goetheyt die groot is bouen <rhyme label="b" type="f">maten</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Op ons v schepsel, o Heere der <rhyme label="a" type="f" corresp="#s2:l1">heeren</rhyme></l>
        <l id="s2:l4" met="4" type="-+">En coemt ons crancheyt nv oock ter <rhyme label="b" type="f" corresp="#s2:l2">baten</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Want hoe groot van <rhyme label="b" type="f" corresp="#s2:l2">staten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sy en vermoghen <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Wilt ditte <rhyme label="b" type="f" corresp="#s2:l2">vaten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Sonder v hulpe <rhyme label="c" type="m" corresp="#s2:l6">siet</rhyme></l>
        <l id="s2:l9" met="4" type="-+">Helpt ghy ons niet, in ons <rhyme label="c" type="m" corresp="#s2:l6">verdriet</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Hoort dit <rhyme label="c" type="m" corresp="#s2:l6">bediet</rhyme></l>
        <l id="s2:l11" met="3" type="+-">So moeten wy gansch <rhyme label="d" type="f">verdwijnen</rhyme></l>
        <l id="s2:l12" met="4" type="-+">Wy zijn die Adam in sonden <rhyme label="c" type="m" corresp="#s2:l6">liet</rhyme></l>
        <l id="s2:l13" met="2" type="-+">Cranck als een <rhyme label="c" type="m" corresp="#s2:l6">riet</rhyme></l>
        <l id="s2:l14" met="3" type="-+">O God, want tis groote <rhyme label="d" type="f" corresp="#s2:l11">pijne</rhyme></l>
        <l id="s2:l15" met="4" type="-+">Der ydelheyt onderworpen <rhyme label="d" type="f" corresp="#s2:l11">tzijne</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy bidden v o Heere <rhyme label="a" type="f">verheuen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Laet v gracie op ons <rhyme label="b" type="f">douwen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Want wy zijn doch onderworpen <rhyme label="a" type="f" corresp="#s3:l1">bleuen</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Der ydelheyt beyde mannen en <rhyme label="b" type="f" corresp="#s3:l2">vrouwen</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Aldus int <rhyme label="b" type="f" corresp="#s3:l2">benouwen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Sijn wy <rhyme label="c" type="m">ghewent</rhyme></l>
        <l id="s3:l7" met="2" type="-+">O God vol <rhyme label="b" type="f" corresp="#s3:l2">trouwen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Door tfenijnich <rhyme label="c" type="m" corresp="#s3:l6">serpent</rhyme></l>
        <l id="s3:l9" met="4" type="-+">Dus bidden wy v o hoochsten <rhyme label="c" type="m" corresp="#s3:l6">regent</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Sijt ons <rhyme label="c" type="m" corresp="#s3:l6">ontrent</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Uerlost ons van sduyuels <rhyme label="d" type="f">lijne</rhyme></l>
        <l id="s3:l12" met="4" type="-+">Maect uwen naem <rhyme label="c" type="m" corresp="#s3:l6">excellent</rhyme></l>
        <l id="s3:l13" met="2" type="-+">In ons <rhyme label="c" type="m" corresp="#s3:l6">bekent</rhyme></l>
        <l id="s3:l14" met="3" type="-+">O God want tis groote <rhyme label="d" type="f" corresp="#s3:l11">pijne</rhyme></l>
        <l id="s3:l15" met="4" type="-+">Der ydelheyt onderworpen <rhyme label="d" type="f" corresp="#s3:l11">tzijne</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wy bidden v God, zijt onser <rhyme label="a" type="f">ghenadich</rhyme></l>
        <l id="s4:l2" met="4" type="-+">En helpt v dienaers nv ten <rhyme label="b" type="f">tijden</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Want wy zijn ghequelt vant vlees <rhyme label="a" type="f" corresp="#s4:l1">mordadich</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Ons sinnen van binnen inwendich <rhyme label="b" type="f" corresp="#s4:l2">strijden</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Den duyuel met <rhyme label="b" type="f" corresp="#s4:l2">nijden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Als eenen Leeu seer <rhyme label="c" type="m">fel</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Uan allen <rhyme label="b" type="f" corresp="#s4:l2">zijden</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Omgaet hy ons seer <rhyme label="c" type="m" corresp="#s4:l6">snel</rhyme></l>
        <l id="s4:l9" met="4" type="-+">Om de siele te bringhen int <rhyme label="c" type="m" corresp="#s4:l6">ghequel</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Tvlees valt <rhyme label="c" type="m" corresp="#s4:l6">rebel</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Den gheest tot elcken <rhyme label="d" type="f">termijne</rhyme></l>
        <l id="s4:l12" met="4" type="-+">Ghy moecht ons helpen, en niemant <rhyme label="c" type="m" corresp="#s4:l6">el</rhyme></l>
        <l id="s4:l13" met="2" type="-+">Door v <rhyme label="c" type="m" corresp="#s4:l6">beuel</rhyme></l>
        <l id="s4:l14" met="3" type="-+">O God, want tis groote <rhyme label="d" type="f" corresp="#s4:l11">pijne</rhyme></l>
        <l id="s4:l15" met="4" type="-+">Der ydelheyt onderworpen <rhyme label="d" type="f" corresp="#s4:l11">tzijne</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Prince o God schept een nieu <rhyme label="a" type="f">creatuere</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Uan in dees vre, na v <rhyme label="b" type="f">behaghen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Sent uwen gheest, die ons suyuer en <rhyme label="a" type="f" corresp="#s5:l1">puere</rhyme></l>
        <l id="s5:l4" met="4" type="-+">En voorts verstercke van daghe te <rhyme label="b" type="f" corresp="#s5:l2">daghen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">En sullen wy <rhyme label="b" type="f" corresp="#s5:l2">verdraghen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Allen <rhyme label="c" type="m">teghenspoet</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Ia sduyuels <rhyme label="b" type="f" corresp="#s5:l2">laghen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met al tsondich <rhyme label="c" type="m" corresp="#s5:l6">ghebroet</rhyme></l>
        <l id="s5:l9" met="4" type="-+">Uertreden door v hulpe <rhyme label="c" type="m" corresp="#s5:l6">goet</rhyme></l>
        <l id="s5:l10" met="2" type="-+">Gheheel onder de <rhyme label="c" type="m" corresp="#s5:l6">voet</rhyme></l>
        <l id="s5:l11" met="3" type="+-">En bringhen svleeschs lust te <rhyme label="d" type="f">ruijne</rhyme></l>
        <l id="s5:l12" met="4" type="-+">Door v hulpe o Heere <rhyme label="c" type="m" corresp="#s5:l6">vroet</rhyme></l>
        <l id="s5:l13" met="2" type="-+">Die ons bystant <rhyme label="c" type="m" corresp="#s5:l6">doet</rhyme></l>
        <l id="s5:l14" met="3" type="-+">Want anders o God waert groote <rhyme label="d" type="f" corresp="#s5:l11">pijne</rhyme>.</l>
        <l id="s5:l15" met="4" type="-+">Der ydelheyt onderworpen <rhyme label="d" type="f" corresp="#s5:l11">tzijne</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
