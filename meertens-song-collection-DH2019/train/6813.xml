      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Die geboorte des <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">dâ€™Engelen nu <rhyme label="a" type="f" corresp="#s1:l1">vereeren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ende singen uyt herten <rhyme label="b" type="m">slot</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Gloria sy eenen <rhyme label="b" type="m" corresp="#s1:l3">Godt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Een Ionckfrou van Godt <rhyme label="c" type="m">gespaert</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Heeft nu Godt Christum <rhyme label="c" type="m" corresp="#s1:l5">gebaert</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Altijt onbefleckt <rhyme label="c" type="m" corresp="#s1:l5">bewaert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die Engel troost die Herders <rhyme label="a" type="f">seer</rhyme></l>
        <l id="s2:l2" met="4" type="+-">En verkondigde haer die <rhyme label="a" type="f" corresp="#s2:l1">meer</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Christus geboorte <rhyme label="b" type="m">wonder</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Met een blijtschap <rhyme label="b" type="m" corresp="#s2:l3">besonder</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Een Ionckfrou van Godt <rhyme label="c" type="m">gespaert</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Heeft nu Godt Christum <rhyme label="c" type="m" corresp="#s2:l5">gebaert</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Altijt onbefleckt <rhyme label="c" type="m" corresp="#s2:l5">bewaert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Weest gegroet maget <rhyme label="a" type="f">vruchtbare</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Van Davids stamme <rhyme label="a" type="f" corresp="#s3:l1">klare</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Godes woonstede wel <rhyme label="b" type="m">bekent</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Nu en altijt <rhyme label="b" type="m" corresp="#s3:l3">ongeschent</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Een Ionckfrou van Godt <rhyme label="c" type="m">gespaert</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Heeft nu Godt Christum <rhyme label="c" type="m" corresp="#s3:l5">gebaert</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Altijt onbefleckt <rhyme label="c" type="m" corresp="#s3:l5">bewaert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">In dat kribbeken lach <rhyme label="a" type="f">hy</rhyme></l>
        <l id="s4:l2" met="4" type="+-">En in doecxkens gewonden <rhyme label="a" type="f" corresp="#s4:l1">bly</rhyme></l>
        <l id="s4:l3" met="4" type="+-">En die Konigen, <rhyme label="b" type="m">bemerckt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Aenbaden dat kint, <rhyme label="b" type="m" corresp="#s4:l3">versterckt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Een Ionckfrou van Godt <rhyme label="c" type="m">gespaert</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Heeft nu Godt Christum <rhyme label="c" type="m" corresp="#s4:l5">gebaert</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Altijt onbefleckt <rhyme label="c" type="m" corresp="#s4:l5">bewaert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sy aenbaden hem <rhyme label="a" type="f">gelijck</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Met hare gaven <rhyme label="a" type="f" corresp="#s5:l1">mildelijck</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Den Koning der <rhyme label="b" type="m">Koningen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ende eenen Godt laet lof <rhyme label="b" type="m" corresp="#s5:l3">singen</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Een Ionckfrou van Godt <rhyme label="c" type="m">gespaert</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Heeft nu Godt Christum <rhyme label="c" type="m" corresp="#s5:l5">gebaert</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Altijt onbefleckt <rhyme label="c" type="m" corresp="#s5:l5">bewaert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
