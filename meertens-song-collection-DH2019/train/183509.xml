<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183509</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <tags/>
    <melody>Mars van Dessau</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+">Elk <rhyme label="a" type="m">wil</rhyme></l>
        <l id="s1:l2" met="1" type="-+">Met <rhyme label="b" type="m">my</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Verheugd slaan blyde <rhyme label="c" type="m">toonen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En de Helden <rhyme label="c" type="m" corresp="#s1:l3">kroonen</rhyme></l>
        <l id="s1:l5" met="2" type="-+">In spyt den <rhyme label="d" type="m">Dwang</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Zy verdien' onz' <rhyme label="e" type="f">achting</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Die stout en <rhyme label="f" type="m">onverzaagd</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Volgens elks <rhyme label="e" type="f" corresp="#s1:l6">verwagting</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Heeft den Pruis <rhyme label="f" type="m" corresp="#s1:l7">verjaagd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="1" type="-+">Hoe <rhyme label="a" type="m">bly</rhyme></l>
        <l id="s2:l2" met="1" type="-+">Trok <rhyme label="b" type="m">gy</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Naar Amstels <rhyme label="c" type="m">Buiten-Posten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al mogt 't u leven <rhyme label="c" type="m" corresp="#s2:l3">kosten</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Uw plicht gy <rhyme label="d" type="m">betrachtt'</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Wyl Pruissen's Rovers <rhyme label="e" type="f">benden</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Door Heerschzugt daar <rhyme label="f" type="m">gebragt</rhyme>;</l>
        <l id="s2:l8" met="3" type="-+">Ras uwe Moed <rhyme label="e" type="f" corresp="#s2:l6">bekenden</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Spyt hun <rhyme label="f" type="m" corresp="#s2:l7">overmagt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="1" type="-+">Hoe <rhyme label="a" type="m">vloog</rhyme>.</l>
        <l id="s3:l2" met="1" type="-+">Voor 't <rhyme label="b" type="m">oog</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van u, die bende <rhyme label="c" type="m">Slaven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">'T zag ras door u ô <rhyme label="c" type="m" corresp="#s3:l3">braven</rhyme>!</l>
        <l id="s3:l5" met="2" type="-+">Hun moed <rhyme label="d" type="m">verplet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En vreesden voor de <rhyme label="e" type="f">blikken</rhyme></l>
        <l id="s3:l7" met="3" type="-+">By elke <rhyme label="f" type="m">zet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Uit uwe <rhyme label="e" type="f" corresp="#s3:l6">Blixem-stukken</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">En 't wel gelaen <rhyme label="f" type="m" corresp="#s3:l7">Musket</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="1" type="-+">ô <rhyme label="a" type="m">Stoet</rhyme>!</l>
        <l id="s4:l2" met="1" type="-+">Uw <rhyme label="b" type="m">Moed</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Zal 't Nageslagt steeds <rhyme label="c" type="m">roemen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En u Helden <rhyme label="c" type="m" corresp="#s4:l3">noemen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Die durfden <rhyme label="d" type="m">staan</rhyme>. -</l>
        <l id="s4:l6" met="3" type="-+">Dat m' uw Gedacht'nis <rhyme label="e" type="f">viere</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Tot eer van Neerlands <rhyme label="f" type="m">Vaen</rhyme>!</l>
        <l id="s4:l8" met="3" type="-+">En dat uw Naam <rhyme label="e" type="f" corresp="#s4:l6">versiere</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Onz' Historie - <rhyme label="f" type="m" corresp="#s4:l7">blaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
