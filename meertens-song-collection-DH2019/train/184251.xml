  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Edel Annaatje myn <rhyme label="a" type="f">Buurmeisje</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wat ben ik bly dat ik u <rhyme label="b" type="m">zie</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ik heb gedaan een Oost-Indies <rhyme label="a" type="f" corresp="#s1:l1">reisje</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ik heb gewonne schoone <rhyme label="c" type="m">splint</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Van Sits en Catoen, Die heb gy van <rhyme label="d" type="m">doen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">En pas die maar, Das ander <rhyme label="e" type="m">Waar</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Die heb ik heede, Gebragt <rhyme label="f" type="f">meede</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Die zal ik u geven <rhyme label="g" type="m">altemaal</rhyme>.</l>
      </lg>
    </lg>
