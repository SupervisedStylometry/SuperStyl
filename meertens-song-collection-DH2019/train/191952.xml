<song>
  <meta>
    <id>191952</id>
    <date>
      <year-start>1753</year-start>
      <year-end>1753</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Is hier de rust zo zoet</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh! Clorimeen, wat 's <rhyme label="a" type="m">dit</rhyme>?</l>
        <l id="s1:l2" met="3" type="-+">Waarom my dus <rhyme label="b" type="f">verlaeten</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Staat toe dat 'k u <rhyme label="a" type="m" corresp="#s1:l1">verbid</rhyme>.</l>
        <l id="s1:l4" met="2" type="+-">Zeg my <rhyme label="c" type="m">nu</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Waarom <rhyme label="c" type="m" corresp="#s1:l4">schuw</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Voor myn <rhyme label="d" type="m">min</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die weleer bezat uw <rhyme label="d" type="m" corresp="#s1:l6">zin</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Waarom gaat gy <rhyme label="e" type="m">verstoort</rhyme></l>
        <l id="s1:l9" met="3" type="-+">My dus afkeerig <rhyme label="b" type="f" corresp="#s1:l2">haeten</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Daar 'k u eertyds heb <rhyme label="e" type="m" corresp="#s1:l8">bekoort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Denk aan dien zoeten <rhyme label="a" type="m">tydt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Toen gy my pleeg te <rhyme label="b" type="f">schenken</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Uw min, vol <rhyme label="a" type="m" corresp="#s2:l1">zoetigheid</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Ach! wel <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Kreeg ik <rhyme label="c" type="m" corresp="#s2:l4">meer</rhyme>.</l>
        <l id="s2:l6" met="2" type="+-">Wyl myn <rhyme label="d" type="m">gloed</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Zoo vernoegde uw <rhyme label="d" type="m" corresp="#s2:l6">gemoed</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Waar scheelt het <rhyme label="e" type="m">Clorimeen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">'t Geen onze min komt <rhyme label="b" type="f" corresp="#s2:l2">krenken</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Die wy deelden onder <rhyme label="e" type="m" corresp="#s2:l8">een</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Keer weer! ei keer <rhyme label="a" type="m">weerom</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Laat ons die vreugd weer <rhyme label="b" type="f">voelen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Alderschoonste <rhyme label="a" type="m" corresp="#s3:l1">lenteblom</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Laat myn <rhyme label="c" type="m">vlam</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Die u <rhyme label="c" type="m" corresp="#s3:l4">nam</rhyme></l>
        <l id="s3:l6" met="2" type="+-">In 't <rhyme label="d" type="m">bezit</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Eens weer werden al ons <rhyme label="d" type="m" corresp="#s3:l6">wit</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Dan zal 't gevoel van <rhyme label="e" type="m">vreugd</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Daar wy gestaag naar <rhyme label="b" type="f" corresp="#s3:l2">doelen</rhyme></l>
        <l id="s3:l10" met="4" type="+-">Weer verfrisschen onze <rhyme label="e" type="m" corresp="#s3:l8">jeugd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
