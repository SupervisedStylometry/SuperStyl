<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>102963</id>
    <date>
      <year-start>1656</year-start>
      <year-end>1656</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb de bras van het luizige vrijen</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HEer hoe dril komt Fytje <rhyme label="a" type="f">Floris</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Met heur sulver tuig op <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s1:l3" met="4" type="+-">'T schynt huer hartje is soo <rhyme label="b" type="m" corresp="#s1:l2">bly</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Nou sy gaet met Bartel <rhyme label="a" type="f" corresp="#s1:l1">Joris</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Stroken Moy den emster <rhyme label="c" type="m">neer</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Naer de schulp of <rhyme label="c" type="m" corresp="#s1:l5">diemermeer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">t'Heeft wel zin in sulken <rhyme label="a" type="f">Vryer</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Die op steets wat is <rhyme label="b" type="m">vestrickt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En wiens riem is net <rhyme label="b" type="m" corresp="#s2:l2">bestickt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Euvol letteren k'wed dat <rhyme label="a" type="f" corresp="#s2:l1">syen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Liever langs de straet mee <rhyme label="c" type="m">gaet</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Als dat syer stil mee <rhyme label="c" type="m" corresp="#s2:l5">praet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">t'Moet hier ergens kormes <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Gunter komt noch twee  drie <rhyme label="b" type="m">paer</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Kick dat Meysien dat is <rhyme label="b" type="m" corresp="#s3:l2">klaer</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Datser ien dat <rhyme label="a" type="f" corresp="#s3:l1">uytgelesen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Tryntjen let op luier <rhyme label="c" type="m">gewaet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Kik hoe zinlick dat huer <rhyme label="c" type="m" corresp="#s3:l5">staet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sie een borstrock van <rhyme label="a" type="f">scharlaken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En een rock met fulpe <rhyme label="b" type="m">lijst</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ja huer klickers <rhyme label="b" type="m" corresp="#s4:l2">gepolijst</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Meeuwes durffe niet aen <rhyme label="a" type="f" corresp="#s4:l1">raken</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Want sy seyt die smodder <rhyme label="c" type="m">muil</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Macktme strack mijn neusdoek <rhyme label="c" type="m" corresp="#s4:l5">vuil</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">K'souie raden Meeuwis <rhyme label="a" type="f">Ianse</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Datie die plankette <rhyme label="b" type="m">pop</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Liever geven soud de <rhyme label="b" type="m" corresp="#s5:l2">schop</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">K'ging noch eer by Teuntje <rhyme label="a" type="f" corresp="#s5:l1">Fransje</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Die al gaanse moey <rhyme label="c" type="m">gekliet</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Sy en zijn so keurig <rhyme label="c" type="m" corresp="#s5:l5">niet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dat zijn Dochters die met <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Meugen kueren met een <rhyme label="b" type="m">Knecht</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En als alles dient <rhyme label="b" type="m" corresp="#s6:l2">gesegt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Van heur is wat meer te <rhyme label="a" type="f" corresp="#s6:l1">teren</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Als van Aefje Dieuwers <rhyme label="c" type="m">Kindt</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En 's is oock van jou <rhyme label="c" type="m" corresp="#s6:l5">gesint</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
