<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185428</id>
    <date>
      <year-start>1795</year-start>
      <year-end>1795</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zullen dan mijn droeve klachten</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Moet Ik dan nog langer <rhyme label="a" type="f">wagten</rhyme> /</l>
        <l id="s1:l2" met="4" type="+-">Zeg myn schoone <rhyme label="b" type="m">engelin</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Filis! Filis! hoord myn <rhyme label="a" type="f" corresp="#s1:l1">klagten</rhyme> /</l>
        <l id="s1:l4" met="4" type="+-">Ach! myn lieve <rhyme label="b" type="m" corresp="#s1:l2">herderin</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">'k Zal Uw nimmermeer <rhyme label="c" type="f">verlaaten</rhyme> /</l>
        <l id="s1:l6" met="4" type="+-">Maar staag blyve ik uw <rhyme label="d" type="m">getrouw</rhyme> /</l>
        <l id="s1:l7" met="4" type="+-">Daar myn hart u nooit zal <rhyme label="c" type="f" corresp="#s1:l5">haten</rhyme> /</l>
        <l id="s1:l8" met="4" type="+-">Ach! wierd gy myn lieve <rhyme label="d" type="m" corresp="#s1:l6">vrouw</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Coridon / ik heb uw <rhyme label="a" type="f">klagten</rhyme>-</l>
        <l id="s2:l2" met="4" type="+-">Meer dan eens met smert <rhyme label="b" type="m">gehoord</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Dan / gy moet de tyd <rhyme label="a" type="f" corresp="#s2:l1">afwagten</rhyme> /</l>
        <l id="s2:l4" met="4" type="+-">Eer ik aan uw' geeft myn <rhyme label="b" type="m" corresp="#s2:l2">woord</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Ik ben nog te jong van <rhyme label="c" type="f">Jaaren</rhyme> /</l>
        <l id="s2:l6" met="4" type="+-">En het is nog tyd <rhyme label="d" type="m">genoeg</rhyme> /</l>
        <l id="s2:l7" met="4" type="+-">Om met uw in de echt te <rhyme label="c" type="f" corresp="#s2:l5">paren</rhyme> /</l>
        <l id="s2:l8" met="4" type="+-">Coridon het is te <rhyme label="d" type="m" corresp="#s2:l6">vroeg</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laat my nu niet langer <rhyme label="a" type="f">klagen</rhyme> /</l>
        <l id="s3:l2" met="4" type="+-">Schenkt / myn 't Ja; het gunstig <rhyme label="b" type="m">woord</rhyme>?</l>
        <l id="s3:l3" met="4" type="+-">Laat het myn niet meerder <rhyme label="a" type="f" corresp="#s3:l1">vraagen</rhyme> /</l>
        <l id="s3:l4" met="4" type="+-">Wyl uw schoonheid my <rhyme label="b" type="m" corresp="#s3:l2">bekoord</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Laaten we ons door de echt <rhyme label="c" type="f">verbinden</rhyme> /</l>
        <l id="s3:l6" met="4" type="+-">En te zaam als Man en <rhyme label="d" type="m">Vrouw</rhyme> /</l>
        <l id="s3:l7" met="4" type="+-">(Tot de Dood ons zal <rhyme label="c" type="f" corresp="#s3:l5">verslinden</rhyme> /)</l>
        <l id="s3:l8" met="4" type="+-">Blyven aan elkaar <rhyme label="d" type="m" corresp="#s3:l6">getrouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ik zal u myn ja-woord <rhyme label="a" type="f">geeven</rhyme>;</l>
        <l id="s4:l2" met="4" type="+-">En aan uw verbonden <rhyme label="b" type="m">zyn</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Om als man en vrouw te <rhyme label="a" type="f" corresp="#s4:l1">leeven</rhyme> /</l>
        <l id="s4:l4" met="4" type="+-">Schenkt ons nu een roemer <rhyme label="b" type="m" corresp="#s4:l2">wyn</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">'k Zal voor uw een kransje <rhyme label="c" type="f">vlechten</rhyme> /</l>
        <l id="s4:l6" met="4" type="+-">Daar ik voor uw roozen <rhyme label="d" type="m">spreij</rhyme>.</l>
        <l id="s4:l7" met="4" type="+-">En die om uw schedel <rhyme label="c" type="f" corresp="#s4:l5">hechten</rhyme> /</l>
        <l id="s4:l8" met="4" type="+-">Dat de Dood ons nimmer <rhyme label="d" type="m" corresp="#s4:l6">scheij</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
