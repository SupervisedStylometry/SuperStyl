<song>
  <meta>
    <id>183277</id>
    <date>
      <year-start>1752</year-start>
      <year-end>1752</year-end>
    </date>
    <location>
      <orginal-name>gorinchem</orginal-name>
      <standard-name>Gorinchem, The Netherlands</standard-name>
      <coordinates>
        <lon>51.8296133</lon>
        <lat>4.9738743</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Petit Bordeaux</melody>
  <author>Jacob Groenewegen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">’K ben met rijkdom <rhyme label="a" type="f">overladen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wereldling ik heb een <rhyme label="b" type="m">schat</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">’k Mag my in de weelde <rhyme label="a" type="f" corresp="#s1:l1">baden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die geen wereldling <rhyme label="b" type="m" corresp="#s1:l2">bezat</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ik ben vol van goed en <rhyme label="c" type="f">eere</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Ik bezit een hooge <rhyme label="d" type="m">staat</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En ik draag de beste <rhyme label="c" type="f" corresp="#s1:l5">kleere</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Zyd’ en kostelijk <rhyme label="d" type="m" corresp="#s1:l6">gewaat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maar gy zult my niet <rhyme label="a" type="f">bevatte</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Wereldling en dus aan <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Vraage waar dog deze <rhyme label="a" type="f" corresp="#s2:l1">schatte</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En die hooge staate <rhyme label="b" type="m" corresp="#s2:l2">zijn</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Ia verwondert zult gy <rhyme label="c" type="f">vragen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Wel wat is tog dat gy <rhyme label="d" type="m">praat</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Wie is die u ooyt zag <rhyme label="c" type="f" corresp="#s2:l5">dragen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Zyd’ of kostelijk <rhyme label="d" type="m" corresp="#s2:l6">gewaat</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">’t Is zoo arme <rhyme label="a" type="f">wereldlinge</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Neemt gy ’t hier op aard en <rhyme label="b" type="m">slijk</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En vergankelyke <rhyme label="a" type="f" corresp="#s3:l1">dingen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">’k Geef volkomen u <rhyme label="b" type="m" corresp="#s3:l2">gelijk</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">’k Heb geen schatte van de <rhyme label="c" type="f">wereld</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">’k Heb geen eer geen goed nog <rhyme label="d" type="m">geld</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">’k Ben hier met geen glans <rhyme label="c" type="f" corresp="#s3:l5">bepereld</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Maar in lagen staat <rhyme label="d" type="m" corresp="#s3:l6">gesteld</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ik bewoon hier geen <rhyme label="a" type="f">paleysen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Maar een hutje en wiens <rhyme label="b" type="m">zin</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Is om met my mee te <rhyme label="a" type="f" corresp="#s4:l1">reysen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Komt ’er vry maar binnen <rhyme label="b" type="m" corresp="#s4:l2">in</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">En ik derf vrymoedig <rhyme label="c" type="f">zegge</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat ik alles hier <rhyme label="d" type="m">bezit</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Kunt gy ’t wereldling <rhyme label="c" type="f" corresp="#s4:l5">uytlegge</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Denkt gy niet wat taal is <rhyme label="d" type="m" corresp="#s4:l6">dit</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">’K zal u zegge ’k ben <rhyme label="a" type="f">gebooren</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Hooger dan de sterre <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Want ik was al <rhyme label="a" type="f" corresp="#s5:l1">uytverkooren</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Eer ik ’t ligt nog schouwen <rhyme label="b" type="m" corresp="#s5:l2">aan</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">God van <rhyme label="c" type="f">algenoegzaamheeden</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Is mijn vader en mijn <rhyme label="d" type="m">God</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Met dat deel ben ik te <rhyme label="c" type="f" corresp="#s5:l5">vreden</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">En met dat zoo zalig <rhyme label="d" type="m" corresp="#s5:l6">lot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Want hy doet zijn volk <rhyme label="a" type="f">beërven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Zulk een vast bestendig <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat in leeven en in <rhyme label="a" type="f" corresp="#s6:l1">sterven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Eeuwig en volmaakt <rhyme label="b" type="m" corresp="#s6:l2">voldoet</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">God het algenoegzaam <rhyme label="c" type="f">weeze</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Is mijn overvloedig <rhyme label="d" type="m">goud</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">En mijn zilver <rhyme label="c" type="f" corresp="#s6:l5">uytgeleeze</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Daar mijn ziele zig op <rhyme label="d" type="m" corresp="#s6:l6">voud</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">’K ben zijn kint hy is mijn <rhyme label="a" type="f">vader</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Des zijn rijkdom is de <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Hier een deel en <rhyme label="a" type="f" corresp="#s7:l1">altegader</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Zal ’t my haast geschonken <rhyme label="b" type="m" corresp="#s7:l2">zijn</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Hier geniet ik <rhyme label="c" type="f">d’eerstelingen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En die maake my zoo <rhyme label="d" type="m">rijk</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Dat ik juyge kan en <rhyme label="c" type="f" corresp="#s7:l5">zingen</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En veragten al u <rhyme label="d" type="m" corresp="#s7:l6">slijk</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Iezus is mijn oudste <rhyme label="a" type="f">broeder</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Ia mijn maker is mijn <rhyme label="b" type="m">man</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Iezus is mijn borg en <rhyme label="a" type="f" corresp="#s8:l1">hoeder</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Die mijn ziel vervullen <rhyme label="b" type="m" corresp="#s8:l2">kan</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Met zijn volheid en <rhyme label="c" type="f">genade</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Met zijn rijkdom en zijn <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">En verworve heyl <rhyme label="c" type="f" corresp="#s8:l5">weldade</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Is ’t nog weynig ’k zal nog <rhyme label="d" type="m" corresp="#s8:l6">meer</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Van mijn rijkdom uw <rhyme label="a" type="f">verkonden</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Iezus mijn volzalig <rhyme label="b" type="m">goed</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Heeft in my zijn geest <rhyme label="a" type="f" corresp="#s9:l1">gezonden</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Die my Abba roepe <rhyme label="b" type="m" corresp="#s9:l2">doet</rhyme>;</l>
        <l id="s9:l5" met="4" type="+-">Hy bewoon my als zijn <rhyme label="c" type="f">tempel</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Hy bezit mijn ziele, <rhyme label="d" type="m">en</rhyme></l>
        <l id="s9:l7" met="4" type="+-">Drukt op my zijn Godheid <rhyme label="c" type="f" corresp="#s9:l5">stempel</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Dat ik Godes eyge <rhyme label="d" type="m" corresp="#s9:l6">ben</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">’t Is een geest aan my <rhyme label="a" type="f">gegeven</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Die niet ledig in my <rhyme label="b" type="m">woont</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Maar met Hemels ligt en <rhyme label="a" type="f" corresp="#s10:l1">leeven</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">En met heyligheid my <rhyme label="b" type="m" corresp="#s10:l2">kroont</rhyme>;</l>
        <l id="s10:l5" met="4" type="+-">Als een kleed dat voor den <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Waardig is na myne <rhyme label="d" type="m">staat</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">Dat alleen dat zijn de <rhyme label="c" type="f" corresp="#s10:l5">kleere</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Zyd’ of kostelijk <rhyme label="d" type="m" corresp="#s10:l6">gewaad</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">’t Is een geest aan my <rhyme label="a" type="f">gegeven</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Tot een zeker <rhyme label="b" type="m">onderpand</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Van een eeuwig zalig <rhyme label="a" type="f" corresp="#s11:l1">leeven</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">In ’t gezegent <rhyme label="b" type="m" corresp="#s11:l2">vaderland</rhyme>:</l>
        <l id="s11:l5" met="4" type="+-">Vry van alle druk en <rhyme label="c" type="f">rouwen</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Met wat dunkt u van mijn <rhyme label="d" type="m">goed</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Kunt gy nu niet haast <rhyme label="c" type="f" corresp="#s11:l5">beschouwen</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">Mijn gebrek en <rhyme label="d" type="m" corresp="#s11:l6">overvloed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
