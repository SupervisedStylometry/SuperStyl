<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20395</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ballet Bronkhorst</melody>
  <author>B. Targier, Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">LIeve <rhyme label="a" type="f">schoonheydt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghy houdt mijn swang're <rhyme label="b" type="f">sinnen</rhyme></l>
        <l id="s1:l3" met="2" type="-+">In Liefdes <rhyme label="c" type="m">hoop'</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Die ten toon <rhyme label="a" type="f" corresp="#s1:l1">leydt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Van buyten en van <rhyme label="b" type="f" corresp="#s1:l2">binnen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Tot <rhyme label="c" type="m" corresp="#s1:l3">overloop'</rhyme>,</l>
        <l id="s1:l7" met="5" type="+-">Nymph u oogh, dat tart den <rhyme label="d" type="m">Diamant</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">V brave <rhyme label="e" type="f">lonckjes</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">En soete <rhyme label="e" type="f" corresp="#s1:l8">flonckjes</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Die stichten <rhyme label="e" type="f" corresp="#s1:l8">vonckjes</rhyme></l>
        <l id="s1:l11" met="5" type="-+">In mijn verliefde Ziel, mijn Ziel, mijn <rhyme label="f" type="m">Ziel</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Ach! mijn Zieltje staet in <rhyme label="d" type="m" corresp="#s1:l7">brandt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Soete <rhyme label="a" type="f">bloosjes</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hoe kleurt en suyver <rhyme label="b" type="f">ciertje</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Haer heldre <rhyme label="c" type="m">wangh'</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Trots de <rhyme label="a" type="f" corresp="#s2:l1">Roosjes</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ha! nobel <rhyme label="b" type="f" corresp="#s2:l2">Maeghde-diertje</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Ach! ick <rhyme label="c" type="m" corresp="#s2:l3">verlangh</rhyme>,</l>
        <l id="s2:l7" met="5" type="+-">Om die geur te slurpen gretigh <rhyme label="d" type="m">in</rhyme>;</l>
        <l id="s2:l8" met="2" type="-+">Ha! perll' der <rhyme label="e" type="f">perlen</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Mijn sinnen <rhyme label="e" type="f" corresp="#s2:l8">dwerlen</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Mijn harssen <rhyme label="e" type="f" corresp="#s2:l8">werlen</rhyme></l>
        <l id="s2:l11" met="5" type="-+">En smeulen in de lust, de lust, de <rhyme label="f" type="m">lust</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Die'er sweeft om u <rhyme label="d" type="m" corresp="#s2:l7">Vriendin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Goede <rhyme label="a" type="f">seden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ziel-queeckster, <rhyme label="b" type="f">hert-ophefster</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Ghy pronckt haer <rhyme label="c" type="m">jeught</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Brave <rhyme label="a" type="f" corresp="#s3:l1">reden</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Sin-voetster, <rhyme label="b" type="f" corresp="#s3:l2">Waerheyt-trefster</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Vermenght met <rhyme label="c" type="m" corresp="#s3:l3">Deught</rhyme>,</l>
        <l id="s3:l7" met="5" type="+-">Woont in haer, in volle volheydt <rhyme label="d" type="m">soet</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Soo dat'er <rhyme label="e" type="f">krachten</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">En stercke <rhyme label="e" type="f" corresp="#s3:l8">machten</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Mijne <rhyme label="e" type="f" corresp="#s3:l8">ghedachten</rhyme>,</l>
        <l id="s3:l11" met="5" type="-+">Met Liefde stadigh vo'en, en vo'en, en <rhyme label="f" type="m">vo'en</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Gen'er Deught, en hoogh <rhyme label="d" type="m" corresp="#s3:l7">ghemoet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Rare <rhyme label="a" type="f">luchjes</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hart-vo'ende noble <rhyme label="b" type="f">waessem</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Ghy quickt mijn <rhyme label="c" type="m">Ziel</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Dat u <rhyme label="a" type="f" corresp="#s4:l1">suchjes</rhyme></l>
        <l id="s4:l5" met="3" type="-+">u vol-op soete <rhyme label="b" type="f" corresp="#s4:l2">aessem</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Mijn niet <rhyme label="c" type="m" corresp="#s4:l3">behiel</rhyme>,</l>
        <l id="s4:l7" met="5" type="+-">'k Most dan haest, wel licht in Charons <rhyme label="d" type="m">boot</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Ay laet u <rhyme label="e" type="f">blasen</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Mijn Zieltje <rhyme label="e" type="f" corresp="#s4:l8">asen</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">En sta'egh <rhyme label="e" type="f" corresp="#s4:l8">begrasen</rhyme></l>
        <l id="s4:l11" met="5" type="-+">En douwen mijn gemoedt, gemoedt, <rhyme label="f" type="m">gemoedt</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">Ach! ick raeck wis anders <rhyme label="d" type="m" corresp="#s4:l7">doot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
