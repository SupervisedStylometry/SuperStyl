  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IK stel mijn hier ten <rhyme label="a" type="m">toon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Myn Winkel met Bloemen <rhyme label="a" type="m" corresp="#s1:l1">schoon</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Gy kond Dames en <rhyme label="b" type="m">Heeren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Kiesen na uw <rhyme label="b" type="m" corresp="#s1:l3">begeeren</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Gy kond hier vinden by <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Alle goed en zuper <rhyme label="c" type="m" corresp="#s1:l5">rijn</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Ook van <rhyme label="d" type="m">Liefhebbers</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De Zaden ook daer <rhyme label="d" type="m" corresp="#s1:l7">by</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Koop Bloem bollen <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ik zet se hier ten <rhyme label="a" type="m" corresp="#s2:l1">toon</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Koopt se uyt <rhyme label="b" type="m">Liefhebbery</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Met de Glazen ook daer <rhyme label="b" type="m" corresp="#s2:l3">by</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Een Roosje seer soet van <rhyme label="c" type="m">geur</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Staet in zyn vollen <rhyme label="c" type="m" corresp="#s2:l5">fleur</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Met zijn volle <rhyme label="d" type="m">knopen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Wilt dog iets van mijn <rhyme label="d" type="m" corresp="#s2:l7">koopen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een Tulp in der <rhyme label="a" type="m">daed</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Staet hier voor een <rhyme label="a" type="m" corresp="#s3:l1">Cieraed</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Sjonklelies de <rhyme label="b" type="m">Anjelier</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Die ruikt men met <rhyme label="b" type="m" corresp="#s3:l3">pleisier</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Koopt Bloem bollen <rhyme label="c" type="m">schoon</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Ik zet se hier ten <rhyme label="c" type="m" corresp="#s3:l5">toon</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Koopt se uyt <rhyme label="d" type="m">Liefhebbery</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Met de Glazen ook daer <rhyme label="d" type="m" corresp="#s3:l7">by</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ura van Persika <rhyme label="a" type="m">klijn</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ook Nageltakken <rhyme label="a" type="m" corresp="#s4:l1">rijn</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Leely, Oranje-bloessem <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Ook de mooye <rhyme label="b" type="m" corresp="#s4:l3">Zonne-blom</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Van Nockels en de <rhyme label="c" type="m">Afferca</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Ook de dubbelde <rhyme label="c" type="m" corresp="#s4:l5">Hane-kam</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Riedjes en stokjes <rhyme label="d" type="m">net</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Daerje de Blomme by vast <rhyme label="d" type="m" corresp="#s4:l7">zet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Koopt Bloem bollen <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ik zetse hier ten <rhyme label="a" type="m" corresp="#s5:l1">toon</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Koopt se uit <rhyme label="b" type="m">Liefhebbery</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Met de Glasen ook daer <rhyme label="b" type="m" corresp="#s5:l3">by</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Hier hebt gy nu alles <rhyme label="c" type="m">gesien</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Wie Koopt wat van U <rhyme label="c" type="m" corresp="#s5:l5">lien</rhyme>?</l>
        <l id="s5:l7" met="4" type="+-">Bloem, Zaden wit en <rhyme label="d" type="m">rood</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Bloem en bollen klijn en <rhyme label="d" type="m" corresp="#s5:l7">groot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Voor een Cyviele <rhyme label="a" type="m">prys</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Om dat ik niet meer <rhyme label="a" type="m" corresp="#s6:l1">eys</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Ik sluyt haest mijn <rhyme label="b" type="m">Deur</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En blyf u <rhyme label="b" type="m" corresp="#s6:l3">Serviteur</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Een Bloem gebruiken by <rhyme label="c" type="m">dag</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">By Nagt heeft die geen <rhyme label="c" type="m" corresp="#s6:l5">kragt</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Pielo soo is zijn <rhyme label="d" type="m">Naem</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En hoort dit Wonder <rhyme label="d" type="m" corresp="#s6:l7">aen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wilt gy iets <rhyme label="a" type="m">gebruiken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ik sluyt nu haest myn <rhyme label="a" type="m" corresp="#s7:l1">luiken</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Ik sluyt myn Raemtjes <rhyme label="b" type="m">digt</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Soo gy ziet is mijn <rhyme label="b" type="m" corresp="#s7:l3">pligt</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Daerom wilt u voordeel <rhyme label="c" type="m">doen</rhyme>?</l>
        <l id="s7:l6" met="3" type="+-">Koop van my met goed <rhyme label="c" type="m" corresp="#s7:l5">fatsoen</rhyme>;</l>
        <l id="s7:l7" met="4" type="+-">Ik Verkoop myn Bloemen <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En maek hier een <rhyme label="d" type="m" corresp="#s7:l7">besluyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
