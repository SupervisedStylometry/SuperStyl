<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181895</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O Holland schoon gij leeft in vrede</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Men roeme vry Heer Goverts <rhyme label="a" type="m">Stad</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Geboud voor <rhyme label="b" type="f">Delvenaeren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Zoo als Heer Blyswyks Schrift <rhyme label="a" type="m" corresp="#s1:l1">bevat</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Een roem der <rhyme label="b" type="f" corresp="#s1:l2">Burgerschaeren</rhyme>!</l>
        <l id="s1:l5" met="4" type="-+">'t Geslagt van hem daer <rhyme label="c" type="m">triomfeert</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">De Grooten zyn met hem <rhyme label="c" type="m" corresp="#s1:l5">vereert</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">En hy blyft aldaer <rhyme label="d" type="f">leven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Door 't Boek van hem <rhyme label="d" type="f" corresp="#s1:l7">geschreeven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Haer oudheid over lang <rhyme label="a" type="m">bekent</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">In alle <rhyme label="b" type="f">Wereld-streeken</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Wel waerdig in 't Uerstand <rhyme label="a" type="m" corresp="#s2:l1">geprent</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Als 't yder is <rhyme label="b" type="f" corresp="#s2:l2">gebleeken</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ik zal op haelen van haer <rhyme label="c" type="m">lof</rhyme>;</l>
        <l id="s2:l6" met="4" type="-+">Die klinkt aen 't hooge Sterren <rhyme label="c" type="m" corresp="#s2:l5">hof</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En zal de tyd <rhyme label="d" type="f">verduuren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Beroemd om Uest en <rhyme label="d" type="f" corresp="#s2:l7">Muuren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Kerken aldaer wys <rhyme label="a" type="m">geboud</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Doen duyzend derwaerts <rhyme label="b" type="f">streeven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En 't geen myn zang daer in <rhyme label="a" type="m" corresp="#s3:l1">ontvout</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Moet lof en glorie <rhyme label="b" type="f" corresp="#s3:l2">geven</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Daer leggen Helden in haer <rhyme label="c" type="m">Graf</rhyme>;</l>
        <l id="s3:l6" met="4" type="-+">De tyd veegt nooit haer glorie <rhyme label="c" type="m" corresp="#s3:l5">af</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Piet Heyn en and're <rhyme label="d" type="f">Helden</rhyme>!</l>
        <l id="s3:l8" met="3" type="-+">Mag men haer Glorie <rhyme label="d" type="f" corresp="#s3:l7">melden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Marnix zyn Dochter legt <rhyme label="a" type="m">alhier</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Een Raed van Prins <rhyme label="b" type="f">Oranje</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die nevens Hem vol moed en <rhyme label="a" type="m" corresp="#s4:l1">vier</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Zig kanten tegen <rhyme label="b" type="f" corresp="#s4:l2">Spanje</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Een Man die met zyn Pen en <rhyme label="c" type="m">Hand</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ten Lof Schreef van de <rhyme label="c" type="m" corresp="#s4:l5">Protestand</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Zyn Bykorf wys <rhyme label="d" type="f">Geschreven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zal eeuwig blyven <rhyme label="d" type="f" corresp="#s4:l7">Leven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer meerder maekt deez' Stad <rhyme label="a" type="m">vermaerd</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Door der Nassouwer <rhyme label="b" type="f">Helden</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">Prins Willem van een Ed'len <rhyme label="a" type="m" corresp="#s5:l1">Aerd</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Wiens deugt men moet <rhyme label="b" type="f" corresp="#s5:l2">vermelden</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Om dat Hy Leven Goed en <rhyme label="c" type="m">Bloed</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Met eene wakk're <rhyme label="c" type="m" corresp="#s5:l5">Heldenmoed</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Uoor Godsdienst en <rhyme label="d" type="f">Uryheden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Den Landen kwam <rhyme label="d" type="f" corresp="#s5:l7">besteden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Uervloekte Gerards die hem <rhyme label="a" type="m">schoot</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Door het Pistool <rhyme label="b" type="f">baldadig</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Met hy in 't nypen van dien <rhyme label="a" type="m" corresp="#s6:l1">noot</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Riep hy voor 't laest <rhyme label="b" type="f" corresp="#s6:l2">Genadig</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Bewaerd Ã´ Heer! het arme <rhyme label="c" type="m">Uolk</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Goedhals daer naer Gods <rhyme label="c" type="m" corresp="#s6:l5">Hemel-tolk</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Zal 't in zyn Boek <rhyme label="d" type="f">getuygen</rhyme>!</l>
        <l id="s6:l8" met="3" type="-+">Waer uit wy honing <rhyme label="d" type="f" corresp="#s6:l7">zuigen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Benevens Willem leggen <rhyme label="a" type="m">hier</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Zyn Nazaten <rhyme label="b" type="f">begraven</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die op het Oorlogs-Paerd zeer <rhyme label="a" type="m" corresp="#s7:l1">sier</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Hun Glorie deden <rhyme label="b" type="f" corresp="#s7:l2">draven</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Held Maurits regt der Spaenzen <rhyme label="c" type="m">schrik</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Zyn dapp're broeder <rhyme label="c" type="m" corresp="#s7:l5">Frederik</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Te vroeg voor 't Land <rhyme label="d" type="f">gesloten</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Moet men haer Naem <rhyme label="d" type="f" corresp="#s7:l7">vergrooten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De Trompen Mannen groot in <rhyme label="a" type="m">Naem</rhyme>:</l>
        <l id="s8:l2" met="3" type="-+">Hier rusten onder <rhyme label="b" type="f">zerken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Sy Leven nog door hunne <rhyme label="a" type="m" corresp="#s8:l1">Faem</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Op uit Gespreyde <rhyme label="b" type="f" corresp="#s8:l2">Ulerken</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Nooit wist men uit het Uroom <rhyme label="c" type="m">Gemoed</rhyme>;</l>
        <l id="s8:l6" met="4" type="-+">Zoo Tromp als het Oranje <rhyme label="c" type="m" corresp="#s8:l5">bloed</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">De roem doet het <rhyme label="d" type="f">ontvouwen</rhyme>:</l>
        <l id="s8:l8" met="3" type="-+">Om 't Graf van haer te <rhyme label="d" type="f" corresp="#s8:l7">schouwen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Men roem u Delftsche <rhyme label="a" type="m">Burgery</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Op het Ge-eert <rhyme label="b" type="f">onthalen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Uan onsen Willem: die men <rhyme label="a" type="m" corresp="#s9:l1">bly</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Zag in uw Uesten <rhyme label="b" type="f" corresp="#s9:l2">pralen</rhyme>:</l>
        <l id="s9:l5" met="4" type="-+">Men roem uw Min uw <rhyme label="c" type="m">Liefdeblyk</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Uerdiend vereeuwigt in <rhyme label="c" type="m" corresp="#s9:l5">Kronyk</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">Uw Lof werd op de <rhyme label="d" type="f">Tongen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Uan duyzende <rhyme label="d" type="f" corresp="#s9:l7">gesongen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Wat stond gy zaem niet trouw <rhyme label="a" type="m">geschaerd</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Om Willem te <rhyme label="b" type="f">ontfangen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Met zyne Uoogt t' zaem <rhyme label="a" type="m" corresp="#s10:l1">Gepaert</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Elk spoorde 's Princen <rhyme label="b" type="f" corresp="#s10:l2">gangen</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Het was viva lang leeft de <rhyme label="c" type="m">Prins</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Elk was in liefde heel van <rhyme label="c" type="m" corresp="#s10:l5">zins</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Om hem te <rhyme label="d" type="f">Wellekommen</rhyme>?</l>
        <l id="s10:l8" met="3" type="-+">Met Klok en <rhyme label="d" type="f" corresp="#s10:l7">Keteltrommen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Een elk regtmatig <rhyme label="a" type="m">Onderdaen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Die kwam den Hemel <rhyme label="b" type="f">smeken</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Dat lang Oranjes <rhyme label="a" type="m" corresp="#s11:l1">Ureeden-Uaem</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Het Zegel mote <rhyme label="b" type="f" corresp="#s11:l2">steken</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Door Gods Genaderyke <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Tot Heil van Kerk en <rhyme label="c" type="m" corresp="#s11:l5">Uaderland</rhyme>:</l>
        <l id="s11:l7" met="3" type="-+">Zoo wensten de <rhyme label="d" type="f">Gemeene</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">En Groot en Kleyn met <rhyme label="d" type="f" corresp="#s11:l7">eene</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
