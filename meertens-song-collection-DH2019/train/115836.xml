  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Gy Minnaars die den Hemel <rhyme label="a" type="m">geeft</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat gy gelukkig moogt <rhyme label="b" type="f">beminnen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dat gy syn gunst niet <rhyme label="a" type="m" corresp="#s1:l1">wederstreeft</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Maar mint, maar mint oprecht met zuiv're <rhyme label="b" type="f" corresp="#s1:l2">zinnen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Bemint terwyl de Min en <rhyme label="c" type="m">jeugd</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">U nooden tot de <rhyme label="c" type="m" corresp="#s1:l5">Huw'lyksvreugd</rhyme>.</l>
      </lg>
    </lg>
  </text>
