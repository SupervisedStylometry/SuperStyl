  <author>Philips van Marnix</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Heer, mijn hert' en is niet <rhyme label="a" type="m">hoogh'</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ick heb oock gheen hooveerdich <rhyme label="a" type="m" corresp="#s1:l1">oogh'</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">End' stae niet hoogh' noch <rhyme label="b" type="m">onverschaemt</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Na grooter  saecken dan m'en <rhyme label="b" type="m" corresp="#s1:l3">taemt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick houd' voorwaer mijn sinnen <rhyme label="a" type="m">stil</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">End' speense van haer lust end' <rhyme label="a" type="m" corresp="#s2:l1">wil</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">End' houd' mijn hert' alsoo <rhyme label="b" type="m">vercleendt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Als t'jonck kindt dat de moeder <rhyme label="b" type="m" corresp="#s2:l3">speent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">* Ben ick niet als een kindt, het <rhyme label="a" type="m">welck</rhyme></l>
        <l id="s3:l2" met="4" type="-+">De voester speent van hare <rhyme label="a" type="m" corresp="#s3:l1">melck</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Soo moet ick van nu rechte <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">In mijn ghebet zijn <rhyme label="b" type="m" corresp="#s3:l3">onverhoort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">* Dat Israel vry hop' end' <rhyme label="a" type="m">wacht</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Op Gods des Heeren groote <rhyme label="a" type="m" corresp="#s4:l1">macht</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">End' op zijn <rhyme label="b" type="m">ontfermherticheyt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Van nu tot inder <rhyme label="b" type="m" corresp="#s4:l3">eewicheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
