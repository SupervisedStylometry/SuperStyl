<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>1367</id>
    <date>
      <year-start>1613</year-start>
      <year-end>1613</year-end>
    </date>
    <location>
      <orginal-name>amsterdam?</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Een Venus' dier houdt mij in het bestier</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EEn Venus <rhyme label="a" type="m">dier</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hout my in haer <rhyme label="a" type="m" corresp="#s1:l1">bestier</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Gheen schoonder is daer <rhyme label="b" type="f">ghebooren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Haer Amoreusheyt <rhyme label="a" type="m" corresp="#s1:l1">fier</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Brenght my in swaer <rhyme label="a" type="m" corresp="#s1:l1">dangier</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Mijn sinnen hebben haer <rhyme label="b" type="f" corresp="#s1:l3">verkooren</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Als Aurora sietmen haer <rhyme label="c" type="f">vertoghen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Gelijck als den <rhyme label="d" type="m">Gout-draet</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Is haer hayrken <rhyme label="d" type="m" corresp="#s1:l8">delicaet</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Diamantich zijn haer <rhyme label="c" type="f" corresp="#s1:l7">Ooghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als Venus <rhyme label="a" type="m">Amoreus</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En Pallas <rhyme label="a" type="m" corresp="#s2:l1">gratieus</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En Iuno can sy haer <rhyme label="b" type="f">vercieren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Eerbaer en <rhyme label="a" type="m" corresp="#s2:l1">heus</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Seer <rhyme label="a" type="m" corresp="#s2:l1">familieus</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Als Diana zijn haer <rhyme label="b" type="f" corresp="#s2:l3">manieren</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Eloquent is sy ter <rhyme label="c" type="f">spraken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Root als een <rhyme label="d" type="m">Robijn</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Haer Lipkens <rhyme label="d" type="m" corresp="#s2:l8">fijn</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Sang vijn zijn haer <rhyme label="c" type="f" corresp="#s2:l7">kaecken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Voorsichtigh ende <rhyme label="a" type="m">wijs</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met een goet <rhyme label="a" type="m" corresp="#s3:l1">advijs</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Soo is mijn Lieveken <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Noch is haer den <rhyme label="a" type="m" corresp="#s3:l1">prijs</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Dat bloeyende <rhyme label="a" type="m" corresp="#s3:l1">rijs</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Van Sesto <rhyme label="b" type="f" corresp="#s3:l3">toegheschreven</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Eerbaer zijn haer <rhyme label="c" type="f">versetten</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ghelijck een <rhyme label="d" type="m">Albast</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Zijn haer tandekens <rhyme label="d" type="m" corresp="#s3:l8">ghepast</rhyme></l>
        <l id="s3:l10" met="4" type="-+">Haer Asem rieckt als <rhyme label="c" type="f" corresp="#s3:l7">Violetten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Noch heeft sy <rhyme label="a" type="m">voorwaer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Een lichaem dat is <rhyme label="a" type="m" corresp="#s4:l1">klaer</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Deel witter als den Sneeu <rhyme label="b" type="f">deurvloghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ick en weet aen <rhyme label="a" type="m" corresp="#s4:l1">haer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Toch gheen <rhyme label="a" type="m" corresp="#s4:l1">maer</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Venus Borsten heeft sy <rhyme label="b" type="f" corresp="#s4:l3">gesoghen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Haer tonghe spreeckt <rhyme label="c" type="f">Rethorijcken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Tot lachen staet haer <rhyme label="d" type="m">mont</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Met haer borstkens <rhyme label="d" type="m" corresp="#s4:l8">ront</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Haer keelken singht <rhyme label="c" type="f" corresp="#s4:l7">musijcke</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Prince <rhyme label="a" type="m">vaeleyant</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Als Helena <rhyme label="a" type="m" corresp="#s5:l1">playsant</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Soo is mijn Lieveken van <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Door haerder Liefden <rhyme label="a" type="m" corresp="#s5:l1">brant</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ende seer <rhyme label="a" type="m" corresp="#s5:l1">abondant</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Heeft Cupido mijn <rhyme label="b" type="f" corresp="#s5:l3">bestreden</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Sal ickse moeten <rhyme label="c" type="f">derven</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Als Leander <rhyme label="d" type="m">certeyn</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Om Hero <rhyme label="d" type="m" corresp="#s5:l8">reyn</rhyme></l>
        <l id="s5:l10" met="4" type="-+">Sal ickt moeten <rhyme label="c" type="f" corresp="#s5:l7">besterven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
