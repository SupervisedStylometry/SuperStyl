      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ave Maria gratia <rhyme label="a" type="f">plena</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ave Maria gratis <rhyme label="a" type="f" corresp="#s1:l1">plena</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">So groeten die Engelen die Ionckfrouw <rhyme label="b" type="f">Maria</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Daer sy in hare ghebeden <rhyme label="c" type="m">sat</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">So groeten die Engelen die Ionckfrouw <rhyme label="b" type="f" corresp="#s1:l3">Maria</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Daer sy in hare ghebeden <rhyme label="c" type="m" corresp="#s1:l4">sat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maria ghy sult een Soon <rhyme label="a" type="f">ontfangen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Maria ghy sult een Soon <rhyme label="a" type="f" corresp="#s2:l1">ontfangen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer na hemmel en aerde <rhyme label="b" type="f">verlangen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Dat gy een Moeder des Heeren sout <rhyme label="c" type="m">sijn</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Daer na hemmel en aerde <rhyme label="b" type="f" corresp="#s2:l3">verlangen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Dat gy een Moeder des Heeren sout <rhyme label="c" type="m" corresp="#s2:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O Engel hoe soud dat <rhyme label="a" type="f">gescheyen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">O Engel hoe soud dat <rhyme label="a" type="f" corresp="#s3:l1">gescheyen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Mijn hert kan genen Man <rhyme label="b" type="f">erkennen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">In deser wijde werelt <rhyme label="c" type="m">breet</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Mijn hert kan genen Man <rhyme label="b" type="f" corresp="#s3:l3">erkennen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">In deser wijde werelt <rhyme label="c" type="m" corresp="#s3:l4">breet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den heyligen Geest sal over u <rhyme label="a" type="f">kommen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Den heyligen Geest sal over u <rhyme label="a" type="f" corresp="#s4:l1">kommen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gelijck den dow rijst over die <rhyme label="b" type="f">blommen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Also moet God geboren <rhyme label="c" type="m">sijn</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Gelijck den dow rijst over die <rhyme label="b" type="f" corresp="#s4:l3">blommen</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Also moet God geboren <rhyme label="c" type="m" corresp="#s4:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maria sy hoorden alsulckes <rhyme label="a" type="f">geerne</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Maria sy hoorden alsulckes <rhyme label="a" type="f" corresp="#s5:l1">geerne</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Sy sprack ick ben een dienstmaegt des <rhyme label="b" type="f">Heeren</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Na uwen woort geschye <rhyme label="c" type="m">my</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Sy sprack ick ben een dienstmaegt des <rhyme label="b" type="f" corresp="#s5:l3">Heeren</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Na uwen woort geschye <rhyme label="c" type="m" corresp="#s5:l4">my</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die Engelen vielen al op hare <rhyme label="a" type="f">knyen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Die Engelen vielen al op hare <rhyme label="a" type="f" corresp="#s6:l1">knyen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Sy songen so uyde Sancte, <rhyme label="b" type="f">Sancte</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Den Loffgesanck van <rhyme label="c" type="m">Maria</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Sy songen so uyde Sancte, <rhyme label="b" type="f" corresp="#s6:l3">Sancte</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Den Loffgesanck van <rhyme label="c" type="m" corresp="#s6:l4">Maria</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die Engelen vlogen al hoger en <rhyme label="a" type="f">hoger</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Die Engelen vlogen al hoger en <rhyme label="a" type="f" corresp="#s7:l1">hoger</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Sijt willekom gy hemelsche <rhyme label="b" type="f">Boden</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Dat u Maria heeft wel <rhyme label="c" type="m">ontfangen</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Sijt willekom gy hemelsche <rhyme label="b" type="f" corresp="#s7:l3">Boden</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Dat u Maria heeft wel <rhyme label="c" type="m" corresp="#s7:l4">ontfangen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maria heeft ons wel <rhyme label="a" type="f">ontfangen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Maria heeft ons wel <rhyme label="a" type="f" corresp="#s8:l1">ontfangen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Daer nae stondt hemel en aerde <rhyme label="b" type="f">verlangen</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Sy is een uytvercoren <rhyme label="c" type="m">Bruyt</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Daer nae stondt hemel en aerde <rhyme label="b" type="f" corresp="#s8:l3">verlangen</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Sy is een uytvercoren <rhyme label="c" type="m" corresp="#s8:l4">Bruyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maria gy moegt wel vrolijck <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Maria gy moegt wel vrolijck <rhyme label="a" type="f" corresp="#s9:l1">wesen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat gy alleen sijt <rhyme label="b" type="f">uytghelesen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Een moeder des alderhoogsten te <rhyme label="c" type="m">sijn</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Dat gy alleen sijt <rhyme label="b" type="f" corresp="#s9:l3">uytghelesen</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Een moeder des alderhoogsten te <rhyme label="c" type="m" corresp="#s9:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Maria gy wilt Godt voor ons <rhyme label="a" type="f">bidden</rhyme>.</l>
        <l id="s10:l2" met="4" type="-+">Maria gy wilt Godt voor ons <rhyme label="a" type="f" corresp="#s10:l1">bidden</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Op dat wy kommen inde hemelsche <rhyme label="b" type="f">steden</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Dat wy met u verblijt mogen <rhyme label="c" type="m">sijn</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Op dat wy kommen inde hemelsche <rhyme label="b" type="f" corresp="#s10:l3">steden</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Dat wy met u verblijt mogen <rhyme label="c" type="m" corresp="#s10:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Der Heer sal sulckes niet <rhyme label="a" type="f">versagen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Der Heer sal sulckes niet <rhyme label="a" type="f" corresp="#s11:l1">versagen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Hy sal aenhooren ons weenen en <rhyme label="b" type="f">klagen</rhyme></l>
        <l id="s11:l4" met="4" type="-+">En voeren ons int <rhyme label="c" type="m">Hemelrijck</rhyme>.</l>
        <l id="s11:l5" met="4" type="-+">Hy sal aenhooren ons weenen en <rhyme label="b" type="f" corresp="#s11:l3">klagen</rhyme></l>
        <l id="s11:l6" met="4" type="-+">En voeren ons int <rhyme label="c" type="m" corresp="#s11:l4">Hemelrijck</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Nu willen wy prijsen, dancken, en <rhyme label="a" type="f">laven</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Nu willen wy prijsen, dancken, en <rhyme label="a" type="f" corresp="#s12:l1">laven</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Den Heer inden Hemel hooch daer <rhyme label="b" type="f">boven</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Dat ons der Heer verlosset <rhyme label="c" type="m">heeft</rhyme>.</l>
        <l id="s12:l5" met="4" type="-+">Den Heer inden Hemel hooch daer <rhyme label="b" type="f" corresp="#s12:l3">boven</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Dat ons der Heer verlosset <rhyme label="c" type="m" corresp="#s12:l4">heeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
