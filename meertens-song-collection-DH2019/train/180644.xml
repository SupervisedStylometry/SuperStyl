<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180644</id>
    <date>
      <year-start>1688</year-start>
      <year-end>1688</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 006 Datheen</melody>
  <author>Philippus van Sorgen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ick heb nu eens <rhyme label="a" type="f">gevonden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Een Balsem voor mijn <rhyme label="a" type="f" corresp="#s1:l1">Wonden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Een middel voor mijn <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Godt door sijn Soon <rhyme label="c" type="f">bewogen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die slaet op my sijn <rhyme label="c" type="f" corresp="#s1:l4">oogen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En treft my in het <rhyme label="b" type="m" corresp="#s1:l3">hert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sijn Licht komt my <rhyme label="a" type="f">beschijnen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En doet de nacht <rhyme label="a" type="f" corresp="#s2:l1">verdwijnen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hy geeft my vollen <rhyme label="b" type="m">wensch</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy komt om mijn te <rhyme label="c" type="f">helpen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En mijn gantsch <rhyme label="c" type="f" corresp="#s2:l4">overstelpen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ia Godt woont by den <rhyme label="b" type="m" corresp="#s2:l3">Mensch</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick sie dien hoogh <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Nu in sijn eygen <rhyme label="a" type="f" corresp="#s3:l1">wesen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ach! wat een soet <rhyme label="b" type="m">vermaeck</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Dewijl ick selfs op <rhyme label="c" type="f">heden</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Nu sijn <rhyme label="c" type="f" corresp="#s3:l4">bevalligheden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En sie, en voel, en <rhyme label="b" type="m" corresp="#s3:l3">smaeck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den opslag van sijn <rhyme label="a" type="f">Oogen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Heeft stracks mijn Hert <rhyme label="a" type="f" corresp="#s4:l1">bewoogen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">O Goddelijcke <rhyme label="b" type="m">strael</rhyme>!</l>
        <l id="s4:l4" met="3" type="-+">De Son met al haer <rhyme label="c" type="f">luyster</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Is by U klaerheyt <rhyme label="c" type="f" corresp="#s4:l4">duyster</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Gy schoonder <rhyme label="b" type="m" corresp="#s4:l3">duysentmael</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gy steelt mijn hert en <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Gy dwinght m’uw te <rhyme label="a" type="f" corresp="#s5:l1">beminnen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gy doet mijn soet <rhyme label="b" type="m">gewelt</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">U Oogen zijn als <rhyme label="c" type="f">schichten</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ay mijn! ‘k sal moeten <rhyme label="c" type="f" corresp="#s5:l4">swichten</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van U ter neer <rhyme label="b" type="m" corresp="#s5:l3">gevelt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Al wat u kon <rhyme label="a" type="f">verbeelden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Seer wijt van u oock <rhyme label="a" type="f" corresp="#s6:l1">scheelde</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ten was niet als <rhyme label="b" type="m">copy</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Dit kan ons niet <rhyme label="c" type="f">ontdecken</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Die overschoone <rhyme label="c" type="f" corresp="#s6:l4">trecken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ten hadde daer niet <rhyme label="b" type="m" corresp="#s6:l3">by</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hier boven in de <rhyme label="a" type="f">Wolcken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hier in de diepe <rhyme label="a" type="f" corresp="#s7:l1">Kolcken</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sie ick sijn soet <rhyme label="b" type="m">Aenschijn</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Maer ‘t moet oock voor hem <rhyme label="c" type="f">wijcken</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">‘t En kan naer hem niet <rhyme label="c" type="f" corresp="#s7:l4">lijcken</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Haer schoonheyt valt te <rhyme label="b" type="m" corresp="#s7:l3">kleyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ick hoor ‘t Gevogelt <rhyme label="a" type="f">queelen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Met duysenden van <rhyme label="a" type="f" corresp="#s8:l1">keelen</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En prijsen die haer <rhyme label="b" type="m">schiep</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Ick sie in haer sijn <rhyme label="c" type="f">goetheyt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Sijn uytgestorte <rhyme label="c" type="f" corresp="#s8:l4">soetheyt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">In haer oneyndigh <rhyme label="b" type="m" corresp="#s8:l3">diep</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">En al wat in der <rhyme label="a" type="f">Aerden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Moght sijn van grooter <rhyme label="a" type="f" corresp="#s9:l1">waerden</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Haer kostelijck <rhyme label="b" type="m">Metael</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Haer planten, kruyden, <rhyme label="c" type="f">bloemen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Hoor ick haer Schepper <rhyme label="c" type="f" corresp="#s9:l4">roemen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En spreecken in haer <rhyme label="b" type="m" corresp="#s9:l3">tael</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Hoe wijs en hoe <rhyme label="a" type="f">lanckmoedigh</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Hoe vol, hoe <rhyme label="a" type="f" corresp="#s10:l1">overvloedigh</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hoe heerlijck, hoe <rhyme label="b" type="m">volmaeckt</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Hy heeft het mijn <rhyme label="c" type="f">bewesen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Hy heeft mijn Ziel <rhyme label="c" type="f" corresp="#s10:l4">genesen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Hy heeft mijn <rhyme label="b" type="m" corresp="#s10:l3">aengeraeckt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Mijn Ziel die wort schier <rhyme label="a" type="f">droncken</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Hy heeft mijn <rhyme label="a" type="f" corresp="#s11:l1">ingeschoncken</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Sijn Wijn en oock sijn <rhyme label="b" type="m">Melck</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">‘t Is Nectar op mijn <rhyme label="c" type="f">tongen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Mijn hert als <rhyme label="c" type="f" corresp="#s11:l4">onbedwongen</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Roept wat een soeter <rhyme label="b" type="m" corresp="#s11:l3">Kelck</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ick ben oock niet <rhyme label="a" type="f">bedrogen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ick sie hem met mijn <rhyme label="a" type="f" corresp="#s12:l1">oogen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Ick rust nu in sijn <rhyme label="b" type="m">schoot</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Ick kan mijn selfs niet <rhyme label="c" type="f">vinden</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Ick laet mijn maer <rhyme label="c" type="f" corresp="#s12:l4">verslinden</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Van dese soete <rhyme label="b" type="m" corresp="#s12:l3">doot</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ick kan niets meer <rhyme label="a" type="f">begeeren</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">En niets sal my <rhyme label="a" type="f" corresp="#s13:l1">ontbeeren</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Want ick besit het <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Dit sal mijn niet <rhyme label="c" type="f">begeven</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Noch in ‘t toekomend’ <rhyme label="c" type="f" corresp="#s13:l4">leven</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Noch in dit <rhyme label="b" type="m" corresp="#s13:l3">Tranendal</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Wie kan die Liefde <rhyme label="a" type="f">blussen</rhyme>?</l>
        <l id="s14:l2" met="3" type="-+">Hy geeft mijn kus op <rhyme label="a" type="f" corresp="#s14:l1">kussen</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">‘t Is Oly in de <rhyme label="b" type="m">vlam</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Hy sagh niet op mijn <rhyme label="c" type="f">sonden</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Doen hy mijn Hert quam <rhyme label="c" type="f" corresp="#s14:l4">wonden</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">En daer sijn woon-plaets <rhyme label="b" type="m" corresp="#s14:l3">nam</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Ick houd my nu gantsch <rhyme label="a" type="f">stille</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Ick buygh my naer sijn <rhyme label="a" type="f" corresp="#s15:l1">wille</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">En al wat hem <rhyme label="b" type="m">behaeght</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Dewijl ick niets kan <rhyme label="c" type="f">wercken</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Ten zy hy mijn komt <rhyme label="c" type="f" corresp="#s15:l4">stercken</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Dat doet hy <rhyme label="b" type="m" corresp="#s15:l3">ongevraeght</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Den Afgront van mijn <rhyme label="a" type="f">sonden</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Die had mijn schier <rhyme label="a" type="f" corresp="#s16:l1">verslonden</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Den afgront van sijn <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Die trock mijn uyt de <rhyme label="c" type="f">poelen</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Nu leef ick by <rhyme label="c" type="f" corresp="#s16:l4">gevoelen</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Hy treckt mijn tot sigh <rhyme label="b" type="m" corresp="#s16:l3">in</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Ick sterf daer, en ick leef <rhyme label="a" type="f">daer</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Ick vliegh daer, en ick sweef <rhyme label="a" type="f" corresp="#s17:l1">daer</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">O wonderlijck <rhyme label="b" type="m">playsier</rhyme>!</l>
        <l id="s17:l4" met="3" type="-+">Wie kan dit togh <rhyme label="c" type="f">bevatten</rhyme>?</l>
        <l id="s17:l5" met="3" type="-+">Ia al des Werelts <rhyme label="c" type="f" corresp="#s17:l4">schatten</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Die acht ick niet een <rhyme label="b" type="m" corresp="#s17:l3">sier</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Ick kan geen woorden <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Mijn Schoone, mijn <rhyme label="a" type="f" corresp="#s18:l1">Beminden</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Hoe soet als Hemels <rhyme label="b" type="m">Man</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Ghy waert het die mijn <rhyme label="c" type="f">schaeckten</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Mijn Duyve, mijn <rhyme label="c" type="f" corresp="#s18:l4">Volmaeckten</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Mijn Bruydegom en <rhyme label="b" type="m" corresp="#s18:l3">Man</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Ghy kust en ghy om-armt <rhyme label="a" type="f">my</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">Ghy koestert, en verwarmt <rhyme label="a" type="f" corresp="#s19:l1">my</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Ick magh nu by u <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">‘k Ben in u Beelt <rhyme label="c" type="f">verborgen</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Ick rust nu vry van <rhyme label="c" type="f" corresp="#s19:l4">sorgen</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Want Iesus is oock <rhyme label="b" type="m" corresp="#s19:l3">mijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
