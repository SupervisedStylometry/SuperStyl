<song>
  <meta>
    <id>34526</id>
    <date>
      <year-start>1658</year-start>
      <year-end>1658</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">TOen 't snel gevlerkt <rhyme label="a" type="m">gerucht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Laerst sweefde door de <rhyme label="a" type="m" corresp="#s1:l1">lucht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En blies den roem der <rhyme label="b" type="f">Helden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hief op een schelle <rhyme label="c" type="m">toon</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die meer was als <rhyme label="c" type="m" corresp="#s1:l4">gewoon</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ick dacht wat sal het <rhyme label="b" type="f" corresp="#s1:l3">melden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Midts hoorden ick haar <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Uitgalmen dat <rhyme label="a" type="m" corresp="#s2:l1">betrof</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Geen mannelijcke <rhyme label="b" type="f">daden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Maar d'eer van een <rhyme label="c" type="m">heldin</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Een Juffrouw die met <rhyme label="c" type="m" corresp="#s2:l4">min</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En liefde was <rhyme label="b" type="f" corresp="#s2:l3">beladen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy liefde 't schoonste <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En klom na d'elpen <rhyme label="a" type="m" corresp="#s3:l1">throon</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van nutte <rhyme label="b" type="f">wetenschappen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hier vindse end, noch <rhyme label="c" type="m">maet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Doorsoeckt het al, ja <rhyme label="c" type="m" corresp="#s3:l4">gaet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Na d'alderhoogste <rhyme label="b" type="f" corresp="#s3:l3">trappen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">En vint hier nut, en <rhyme label="a" type="m">zoet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Waer uyt zy quaet en <rhyme label="a" type="m" corresp="#s4:l1">goet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Leert kennen recht van <rhyme label="b" type="f">passen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Vaer voort o eel <rhyme label="c" type="m">gemoet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dus zult ghy haest de <rhyme label="c" type="m" corresp="#s4:l4">stoet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van Helicon <rhyme label="b" type="f" corresp="#s4:l3">verrassen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dit blies de schelle <rhyme label="a" type="m">faem</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En schakelde de <rhyme label="a" type="m" corresp="#s5:l1">naem</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Van Iuffrouw <rhyme label="b" type="f">Catharina</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Aen 't snoer van <rhyme label="c" type="m">Eeuwigheyt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En heeft al om <rhyme label="c" type="m" corresp="#s5:l4">verbreyt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Haer kloeckheyt zonder <rhyme label="b" type="f" corresp="#s5:l3">weerga</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dus komt nu ider <rhyme label="a" type="m">zien</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En zyne gunste <rhyme label="a" type="m" corresp="#s6:l1">bien</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En roemt van hare <rhyme label="b" type="f">daden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Haer liefd tot nutte <rhyme label="c" type="m">konst</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Treckt yder tot haer <rhyme label="c" type="m" corresp="#s6:l4">gonst</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Geen Momus kan haer <rhyme label="b" type="f" corresp="#s6:l3">schaden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Soo ras zy tijt te <rhyme label="a" type="m">werck</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En bijt met <rhyme label="a" type="m" corresp="#s7:l1">water-sterck</rhyme></l>
        <l id="s7:l3" met="3" type="-+">In Koper of <rhyme label="b" type="f">Metalen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dan wort haer geestich <rhyme label="c" type="m">dicht</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Met held'ren glants <rhyme label="c" type="m" corresp="#s7:l4">verlicht</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Dat daer niet schijnt te <rhyme label="b" type="f" corresp="#s7:l3">falen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Plautil haelt hier geen <rhyme label="a" type="m">prijs</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Geen Sappho schijnt soo <rhyme label="a" type="m" corresp="#s8:l1">wijs</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wie sagh gepaart te <rhyme label="b" type="f">samen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">De Dicht, en <rhyme label="c" type="m">Schilder-konst</rhyme>!</l>
        <l id="s8:l5" met="3" type="-+">En brocht geen dobble <rhyme label="c" type="m" corresp="#s8:l4">jonst</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Daar deez twee samen <rhyme label="b" type="f" corresp="#s8:l3">quamen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dies eer ick hier met <rhyme label="a" type="m">reen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">U konst en <rhyme label="a" type="m" corresp="#s9:l1">aardigheen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">U lust, en grooten <rhyme label="b" type="f">yver</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Maer u oprecht <rhyme label="c" type="m">gemoet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">U onbeveynsden <rhyme label="c" type="m" corresp="#s9:l4">groet</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Die treft al vry wat <rhyme label="b" type="f" corresp="#s9:l3">stijver</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ja dreycht my als met <rhyme label="a" type="m">straf</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Soo ick geen lof <rhyme label="a" type="m" corresp="#s10:l1">verschaf</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Of danckbaerheden <rhyme label="b" type="f">thone</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Van de genooten <rhyme label="c" type="m">deucht</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Die sticht, en oock <rhyme label="c" type="m" corresp="#s10:l4">verheucht</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Daer ick niet voor kan <rhyme label="b" type="f" corresp="#s10:l3">lone</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
