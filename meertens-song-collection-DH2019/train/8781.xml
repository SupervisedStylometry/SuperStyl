<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8781</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ach Gott vom Himmel</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Myn woort end' myn geschrey <rhyme label="a" type="m">vernem</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn Coning' Godt end' <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vroech coemt voor u mijn claghe <rhyme label="a" type="m" corresp="#s1:l1">stem</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verhoort vroech mijn <rhyme label="b" type="f" corresp="#s1:l2">begheere</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Vroech stuyr ick tot u mijn <rhyme label="c" type="m">ghebedt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Myn herte neerstich daer op <rhyme label="c" type="m" corresp="#s1:l5">let</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Met aendacht end' <rhyme label="d" type="f">verstande</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want geensins Godt bent ghy <rhyme label="a" type="m">sodaen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dien Godloosheyt <rhyme label="b" type="f">behaghet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die Boose mach by u niet <rhyme label="a" type="m" corresp="#s2:l1">staen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Gheen Stouten ghy <rhyme label="b" type="f" corresp="#s2:l2">verdraghet</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Die Leugen end' <rhyme label="c" type="m">Bloetgiericheyt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">All Arghelist end' quade <rhyme label="c" type="m" corresp="#s2:l5">Feyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Het is den HEER een <rhyme label="d" type="f">grouwel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maer gaen wil ick ten Huyse <rhyme label="a" type="m">dijn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Nae dijn goetheyt <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Mijn Beed-huys sal dijn Tempel <rhyme label="a" type="m" corresp="#s3:l1">syn</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wil in dijn vreese <rhyme label="b" type="f" corresp="#s3:l2">leven</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Ach HEER in dijn <rhyme label="c" type="m">Gerechticheyt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Leyd my, dijn Wech voor my <rhyme label="c" type="m" corresp="#s3:l5">bereydt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Om mijner Vyand' <rhyme label="d" type="f">wille</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Want geen Waerheyt haer mont en <rhyme label="a" type="m">gaff</rhyme></l>
        <l id="s4:l2" met="3" type="-+">T'is herten leedt van <rhyme label="b" type="f">binnen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Haer keel is als een open <rhyme label="a" type="m" corresp="#s4:l1">graff</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">T is stanck en Doodt daer <rhyme label="b" type="f" corresp="#s4:l2">innen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Met haren Tonghen smeecken <rhyme label="c" type="m">sy</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Vercierent al met <rhyme label="c" type="m" corresp="#s4:l5">huychely</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">End draeyent hoe sy <rhyme label="d" type="f">willen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Verschamen wilt doch haer <rhyme label="a" type="m">propoost</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Mijn Godt wiltse <rhyme label="b" type="f">verstooten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Want boven maten zijn sy <rhyme label="a" type="m" corresp="#s5:l1">boos</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Stout teghen dy sy <rhyme label="b" type="f" corresp="#s5:l2">woeden</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Vermaen tot boet en batet <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Het wort al argher soo men <rhyme label="c" type="m" corresp="#s5:l5">siet</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dat moet den HEERE <rhyme label="d" type="f">spijten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maer vrolijc (Heer) end' eewich <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Laet roemen wel te <rhyme label="b" type="f">samen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">All' die betrouwen vast op <rhyme label="a" type="m" corresp="#s6:l1">dy</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">End' hebben lieff dijn <rhyme label="b" type="f" corresp="#s6:l2">Naeme</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Ghy seghent die Gherechten <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">En croontse met ghenaden <rhyme label="c" type="m" corresp="#s6:l5">HEER</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Ghelijck als met den <rhyme label="d" type="f">Schilde</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
