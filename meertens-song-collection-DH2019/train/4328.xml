    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Kasteel van Rijpermonde</melody>
  <author>Pieter Dubbels</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn trony, hangt 'er een mat <rhyme label="a" type="f">veur</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Iij zint het die ik mien, ja <rhyme label="b" type="m">mien</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Iij zint het die ik schat, <rhyme label="a" type="f" corresp="#s1:l1">veur</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Men aigen vlais in bien, ja <rhyme label="b" type="m" corresp="#s1:l2">bien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">As ik op jouw mijn <rhyme label="a" type="f">blikke</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Laet wayen mit bescheit, ja <rhyme label="b" type="m">scheit</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Val ik in duizent <rhyme label="a" type="f" corresp="#s2:l1">strikke</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van Kuip'ind'o gedreit, ja <rhyme label="b" type="m" corresp="#s2:l2">dreit</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn trony hangt 'er een mat <rhyme label="a" type="f">veur</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Iij zint het die ik mien, ja <rhyme label="b" type="m">mien</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En houw, en douw, en schat, <rhyme label="a" type="f" corresp="#s3:l1">veur</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Men aigen vlais in bien, ja <rhyme label="b" type="m" corresp="#s3:l2">bien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De stroop die uit het <rhyme label="a" type="f">purper</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Van uwe lippen druipt, ja <rhyme label="b" type="m">druipt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Heeft mij (onnoosle <rhyme label="a" type="f" corresp="#s4:l1">slurper</rhyme>!)</l>
        <l id="s4:l4" met="4" type="-+">In Venus kuip ekuipt, ja <rhyme label="b" type="m" corresp="#s4:l2">kuipt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn trony, hangt 'er een mat <rhyme label="a" type="f">veur</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Iij zint het die ik mien, ja <rhyme label="b" type="m">mien</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Iij zint het die ik schat, <rhyme label="a" type="f" corresp="#s5:l1">veur</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Men aigen vlais in bien, ja <rhyme label="b" type="m" corresp="#s5:l2">bien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
