<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>162100</id>
    <date>
      <year-start>1749</year-start>
      <year-end>1749</year-end>
    </date>
    <location>
      <orginal-name>'s-gravenhage</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>F. Pitton</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Myn Lust en Leven, is in <rhyme label="a" type="m">Holland</rhyme>.</l>
        <l id="s1:l2" met="4" type="-+">Myn Lust en Leven, is in <rhyme label="a" type="m" corresp="#s1:l1">Holland</rhyme>.</l>
        <l id="s1:l3" met="2" type="-+">Het 's <rhyme label="b" type="f">Gravenhage</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">is myn <rhyme label="b" type="f" corresp="#s1:l3">behagen</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s1:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s1:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">In 't kort de Vrede daer wort <rhyme label="a" type="m">geviert</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">In 't kort de Vrede daer wort <rhyme label="a" type="m" corresp="#s2:l1">geviert</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Gulde Vuur <rhyme label="b" type="f">pylen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Ten Hemel <rhyme label="b" type="f" corresp="#s2:l3">zylen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s2:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s2:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Haegse Vyver voert een <rhyme label="a" type="m">Paleis</rhyme>:</l>
        <l id="s3:l2" met="4" type="-+">De Haegse Vyver voert een <rhyme label="a" type="m" corresp="#s3:l1">Paleis</rhyme>:</l>
        <l id="s3:l3" met="2" type="-+">Met schoone <rhyme label="b" type="f">Tempels</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">En Marm're <rhyme label="b" type="f" corresp="#s3:l3">Drempels</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s3:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s3:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daer zullen Vuuren van groote <rhyme label="a" type="m">konst</rhyme>:</l>
        <l id="s4:l2" met="4" type="-+">Daer zullen Vuuren van groote <rhyme label="a" type="m" corresp="#s4:l1">konst</rhyme>:</l>
        <l id="s4:l3" met="2" type="-+">Met haere <rhyme label="b" type="f">straelen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Den nagt doen <rhyme label="b" type="f" corresp="#s4:l3">praelen</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s4:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s4:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Prins ter Eere staen daer <rhyme label="a" type="m">geplant</rhyme>:</l>
        <l id="s5:l2" met="4" type="-+">Den Prins ter Eere staen daer <rhyme label="a" type="m" corresp="#s5:l1">geplant</rhyme>:</l>
        <l id="s5:l3" met="2" type="-+">Veel' schoone <rhyme label="b" type="f">Meijen</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Die 't Oog <rhyme label="b" type="f" corresp="#s5:l3">verbleijen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s5:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s5:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">'t Oranje Vaendel der <rhyme label="a" type="m">Schuttery</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">'t Oranje Vaendel der <rhyme label="a" type="m" corresp="#s6:l1">Schuttery</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Heeft dit <rhyme label="b" type="f">bedreven</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">'t Eerst van zyn <rhyme label="b" type="f" corresp="#s6:l3">leven</rhyme>;</l>
        <l id="s6:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s6:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s6:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Deez' brave Helden zullen ook <rhyme label="a" type="m">saem</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">Deez' brave Helden zullen ook <rhyme label="a" type="m" corresp="#s7:l1">saem</rhyme>:</l>
        <l id="s7:l3" met="2" type="-+">Als <rhyme label="b" type="f">Batavieren</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Door 't Haegje <rhyme label="b" type="f" corresp="#s7:l3">zwieren</rhyme>;</l>
        <l id="s7:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s7:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s7:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ses Compagnien all' even <rhyme label="a" type="m">schoon</rhyme>:</l>
        <l id="s8:l2" met="4" type="-+">Ses Compagnien all' even <rhyme label="a" type="m" corresp="#s8:l1">schoon</rhyme>:</l>
        <l id="s8:l3" met="2" type="-+">In vol <rhyme label="b" type="f">Geweere</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Met pragt van <rhyme label="b" type="f" corresp="#s8:l3">Kleere</rhyme>;</l>
        <l id="s8:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s8:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s8:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Voor Prins en Staeten zal deeze <rhyme label="a" type="m">Schaer</rhyme>;</l>
        <l id="s9:l2" met="4" type="-+">Voor Prins en Staeten zal deeze <rhyme label="a" type="m" corresp="#s9:l1">Schaer</rhyme>;</l>
        <l id="s9:l3" met="2" type="-+">Als <rhyme label="b" type="f">Marscialen</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">In 't Wapen <rhyme label="b" type="f" corresp="#s9:l3">pralen</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s9:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s9:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dit zyn al vrugten der Vrede <rhyme label="a" type="m">zoet</rhyme>:</l>
        <l id="s10:l2" met="4" type="-+">Dit zyn al vrugten der Vrede <rhyme label="a" type="m" corresp="#s10:l1">zoet</rhyme>:</l>
        <l id="s10:l3" met="2" type="-+">Lang mag zy <rhyme label="b" type="f">duuren</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Zonder <rhyme label="b" type="f" corresp="#s10:l3">bezuuren</rhyme>!</l>
        <l id="s10:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s10:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s10:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Dank zy den Hemel die door den <rhyme label="a" type="m">Prins</rhyme>:</l>
        <l id="s11:l2" met="4" type="-+">Dank zy den Hemel die door den <rhyme label="a" type="m" corresp="#s11:l1">Prins</rhyme>:</l>
        <l id="s11:l3" met="2" type="-+">By Hem <rhyme label="b" type="f">verkoren</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Heeft Vrêe <rhyme label="b" type="f" corresp="#s11:l3">beschoren</rhyme>;</l>
        <l id="s11:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s11:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s11:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Lof zy den Hemel voor <rhyme label="a" type="m">d'Erf-Zoon</rhyme>?</l>
        <l id="s12:l2" met="4" type="-+">Lof zy den Hemel voor <rhyme label="a" type="m" corresp="#s12:l1">d'Erf-Zoon</rhyme>?</l>
        <l id="s12:l3" met="2" type="-+">Zyn Naem lang <rhyme label="b" type="f">leve</rhyme>!</l>
        <l id="s12:l4" met="2" type="-+">Ten Hemel <rhyme label="b" type="f" corresp="#s12:l3">zweve</rhyme>!</l>
        <l id="s12:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s12:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s12:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Oorlof gy Lieden van 's <rhyme label="a" type="m">Gravenhaeg</rhyme>:</l>
        <l id="s13:l2" met="4" type="-+">Oorlof gy Lieden van 's <rhyme label="a" type="m" corresp="#s13:l1">Gravenhaeg</rhyme>:</l>
        <l id="s13:l3" met="2" type="-+">Wilt u <rhyme label="b" type="f">verblyden</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">Dees' <rhyme label="b" type="f" corresp="#s13:l3">Kermis-tyden</rhyme>;</l>
        <l id="s13:l5" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m">aen</rhyme>…</l>
        <l id="s13:l6" met="2" type="-+">'t Staet my wel <rhyme label="c" type="m" corresp="#s13:l5">aen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
