  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'k Wil van daag geen vreugde <rhyme label="a" type="f">sparen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Op myne <rhyme label="a" type="f" corresp="#s1:l1">snaren</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">'k Wil van daag geen vreugde <rhyme label="a" type="f" corresp="#s1:l1">sparen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">ô <rhyme label="b" type="m">Harderin</rhyme>!</l>
        <l id="s1:l5" met="4" type="-+">'k Zal u met een Hoen <rhyme label="a" type="f" corresp="#s1:l1">verjaren</rhyme></l>
        <l id="s1:l6" met="4" type="-+">En een Appeltaart <rhyme label="b" type="m" corresp="#s1:l4">Vrindin</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">'k Zal u met een Hoen <rhyme label="a" type="f" corresp="#s1:l1">verjaren</rhyme></l>
        <l id="s1:l8" met="4" type="-+">En een Appeltaart <rhyme label="b" type="m" corresp="#s1:l4">Vrindin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'k Zal u boven dien <rhyme label="a" type="f">trakteren</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Op lekkre <rhyme label="a" type="f" corresp="#s2:l1">peren</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">'k Zal u boven dien <rhyme label="a" type="f" corresp="#s2:l1">trakteren</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Op <rhyme label="b" type="m">Schotzeneel</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">'k Schenk, uw jaargety' ter <rhyme label="a" type="f" corresp="#s2:l1">eeren</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Fransche wynen puik en <rhyme label="b" type="m" corresp="#s2:l4">eêl</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">'k Schenk, uw jaargety' ter <rhyme label="a" type="f" corresp="#s2:l1">eeren</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Fransche wynen puik en <rhyme label="b" type="m" corresp="#s2:l4">eêl</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laat my, om aan my te <rhyme label="a" type="f">denken</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">U noch iets <rhyme label="a" type="f" corresp="#s3:l1">schenken</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Laat my, om aan my te <rhyme label="a" type="f" corresp="#s3:l1">denken</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">(En tot een <rhyme label="b" type="m">blyk</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Dat niets kan myn blydschap <rhyme label="a" type="f" corresp="#s3:l1">krenken</rhyme>)</l>
        <l id="s3:l6" met="4" type="-+">Toe, dat ik u iets <rhyme label="b" type="m" corresp="#s3:l4">verryk</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Dat niets kan myn blydschap <rhyme label="a" type="f" corresp="#s3:l1">krenken</rhyme>)</l>
        <l id="s3:l8" met="4" type="-+">Toe, dat ik u iets <rhyme label="b" type="m" corresp="#s3:l4">verryk</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Neem deez' blyde gift, <rhyme label="a" type="f">Celinde</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Myn <rhyme label="a" type="f" corresp="#s4:l1">hartsbeminde</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Neem deez' blyde gift, <rhyme label="a" type="f" corresp="#s4:l1">Celinde</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Gulhartig <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Opdat Cloris <rhyme label="a" type="f" corresp="#s4:l1">onderwinde</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Weêr uw Jaar feest gaê te <rhyme label="b" type="m" corresp="#s4:l4">slaan</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Opdat Cloris <rhyme label="a" type="f" corresp="#s4:l1">onderwinde</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Weêr uw Jaar feest gaê te <rhyme label="b" type="m" corresp="#s4:l4">slaan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Leef lang en vernoegd van <rhyme label="a" type="f">harte</rhyme>;</l>
        <l id="s5:l2" met="2" type="-+">Leef vry van <rhyme label="a" type="f" corresp="#s5:l1">smarte</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Leef lang en vernoegd van <rhyme label="a" type="f" corresp="#s5:l1">harte</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Hier op deez' <rhyme label="b" type="m">aard</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Wilt de nyd en haat steeds <rhyme label="a" type="f" corresp="#s5:l1">tarten</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Tot uw Ziel reist <rhyme label="b" type="m" corresp="#s5:l4">Hemelwaard</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Wilt de nyd en haat steeds <rhyme label="a" type="f" corresp="#s5:l1">tarten</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Tot uw Ziel reist <rhyme label="b" type="m" corresp="#s5:l4">Hemelwaard</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
