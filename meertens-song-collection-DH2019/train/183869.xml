  <author>Jan Steven van Esveldt Holtrop, Willem Holtrop</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">U, aardsche godheid, <rhyme label="a" type="m">vaderland</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">U zij dees zang <rhyme label="b" type="m">gewijd</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En, ook dees zang aan 't <rhyme label="a" type="m" corresp="#s1:l1">vaderland</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">U, die 's lands steunsels <rhyme label="b" type="m" corresp="#s1:l2">zijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Waar men op uwe glorij <rhyme label="a" type="m">staar'</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Uw eerdienst word' <rhyme label="b" type="m">verricht</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Waar, op 't aan u gewijd <rhyme label="a" type="m" corresp="#s2:l1">altaar</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">'t U eerende offer <rhyme label="b" type="m" corresp="#s2:l2">ligt</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waar uwer zoonen forsche <rhyme label="a" type="m">vuist</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">In 't vuur der eer <rhyme label="b" type="m">verltaald</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Elk u vijandig heir <rhyme label="a" type="m" corresp="#s3:l1">vergruist</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Den lauwer grootsch <rhyme label="b" type="m" corresp="#s3:l2">behaalt</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Waar, als uw teedre stem, in <rhyme label="a" type="m">nood</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Den naaren angstkreet <rhyme label="b" type="m">uit</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Uw kroost den vorst des schriks, den <rhyme label="a" type="m" corresp="#s4:l1">dood</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In kluisters hegt <rhyme label="b" type="m" corresp="#s4:l2">besluit</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ook hier en ginds en heinde en <rhyme label="a" type="m">daar</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Prijst, dient, verdeedigt <rhyme label="b" type="m">u</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Elk waar, oprecht <rhyme label="a" type="m" corresp="#s5:l1">Vrij-Metselaar</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Getrouw aan Gode en <rhyme label="b" type="m" corresp="#s5:l2">u</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Getrouw aan reden, wet en <rhyme label="a" type="m">pligt</rhyme>:</l>
        <l id="s6:l2" met="3" type="-+">Door Wijsheid, Schoonheid, <rhyme label="b" type="m">Kracht</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Gevormd, gesterkt, bestuurd, <rhyme label="a" type="m" corresp="#s6:l1">verlicht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Zij nedrigheid zijn <rhyme label="b" type="m" corresp="#s6:l2">magt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">ô Aardsche godheid, <rhyme label="a" type="m">vaderland</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">U zingen wij dit <rhyme label="b" type="m">lied</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">ô Aardsche godheid, <rhyme label="a" type="m" corresp="#s7:l1">vaderland</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">De zang mishaage u <rhyme label="b" type="m" corresp="#s7:l2">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
