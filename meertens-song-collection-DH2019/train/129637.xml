    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Is liefde zoet lijden</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Klorinde, mijn <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Die ik er <rhyme label="b" type="m">bemin</rhyme></l>
        <l id="s1:l3" met="2" type="-+">En nooit sal <rhyme label="a" type="f" corresp="#s1:l1">begeeven</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Maar eeuwig <rhyme label="b" type="m" corresp="#s1:l2">bezin</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Gy gaat voor my <rhyme label="c" type="f">vlugten</rhyme></l>
        <l id="s1:l6" met="2" type="-+">En lagt met mijn <rhyme label="d" type="m">smart</rhyme></l>
        <l id="s1:l7" met="2" type="-+">En laat my mijn <rhyme label="c" type="f" corresp="#s1:l5">zugten</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Nog smooren in ’t <rhyme label="d" type="m" corresp="#s1:l6">hart</rhyme>.</l>
        <l id="s1:l9" met="2" type="-+">Dus tegen de <rhyme label="e" type="f">reden</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Ontfonkt gy mijn <rhyme label="f" type="m">hart</rhyme></l>
        <l id="s1:l11" met="2" type="+-">En ’t blijft nog op <rhyme label="e" type="f" corresp="#s1:l9">heden</rhyme></l>
        <l id="s1:l12" met="2" type="-+">In strikken <rhyme label="f" type="m" corresp="#s1:l10">verwart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Door liefde <rhyme label="a" type="f">gedreven</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Klaag ik u mijn <rhyme label="b" type="m">pijn</rhyme>.</l>
        <l id="s2:l3" met="2" type="-+">Wil dan, lieve <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Medogende <rhyme label="b" type="m" corresp="#s2:l2">zijn</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Maar zo gy mijn <rhyme label="c" type="f">klagten</rhyme></l>
        <l id="s2:l6" met="2" type="-+">En zugten <rhyme label="d" type="m">verdooft</rhyme></l>
        <l id="s2:l7" met="2" type="-+">En Kupidoos <rhyme label="c" type="f" corresp="#s2:l5">kragten</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Niet agt of <rhyme label="d" type="m" corresp="#s2:l6">gelooft</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Zal hy eens <rhyme label="e" type="f">ontsteeken</rhyme></l>
        <l id="s2:l10" met="2" type="-+">In u eene <rhyme label="f" type="m">vlam</rhyme>:</l>
        <l id="s2:l11" met="2" type="+-">Dog zig van u <rhyme label="e" type="f" corresp="#s2:l9">wreeken</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Die zijn eer <rhyme label="f" type="m" corresp="#s2:l10">ontnam</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Hy ziet Zelfs op <rhyme label="a" type="f">Goden</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Nog menschen <rhyme label="b" type="m">magt</rhyme>:</l>
        <l id="s3:l3" met="2" type="-+">Apollo mogt <rhyme label="a" type="f" corresp="#s3:l1">dooden</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Draak Pijthon met <rhyme label="b" type="m" corresp="#s3:l2">kragt</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Maar moest ook <rhyme label="c" type="f">gevoelen</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Een pijl van dit <rhyme label="d" type="m">wigt</rhyme></l>
        <l id="s3:l7" met="2" type="-+">En mogt zig niet <rhyme label="c" type="f" corresp="#s3:l5">koelen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">In Dafnes <rhyme label="d" type="m" corresp="#s3:l6">gezigt</rhyme>.</l>
        <l id="s3:l9" met="2" type="-+">Verander uw <rhyme label="e" type="f">zinnen</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Mijn lief, neem dit <rhyme label="f" type="m">aan</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Op dat u het <rhyme label="e" type="f" corresp="#s3:l9">minnen</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">Niet duur koom te <rhyme label="f" type="m" corresp="#s3:l10">staan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Eer hy   u leer <rhyme label="a" type="f">lonken</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Door wraake daar <rhyme label="b" type="m">van</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Op een die u <rhyme label="a" type="f" corresp="#s4:l1">vonken</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Nooit uitblusschen <rhyme label="b" type="m" corresp="#s4:l2">kan</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Dan moet gy de <rhyme label="c" type="f">smarten</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">- Zo gy hem niet <rhyme label="d" type="m">eert</rhyme> -</l>
        <l id="s4:l7" met="2" type="-+">Der leydende <rhyme label="c" type="f" corresp="#s4:l5">harten</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Zien op u   <rhyme label="d" type="m" corresp="#s4:l6">gekeert</rhyme></l>
        <l id="s4:l9" met="2" type="-+">En zult het <rhyme label="e" type="f">beklaagen</rhyme></l>
        <l id="s4:l10" met="2" type="-+">En voelen te <rhyme label="f" type="m">laat</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Hoe zwaar ’t is te <rhyme label="e" type="f" corresp="#s4:l9">dragen</rhyme>,</l>
        <l id="s4:l12" met="2" type="-+">Te lijden die <rhyme label="f" type="m" corresp="#s4:l10">smaad</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
