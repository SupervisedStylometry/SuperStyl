      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Hoort eens soete <rhyme label="a" type="f">Filledaetje</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Loopt soo haestigh niet <rhyme label="b" type="m">verby</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Maer verleent my een <rhyme label="a" type="f" corresp="#s1:l1">praetje</rhyme>,</l>
        <l id="s1:l4" met="1" type="-+">Een <rhyme label="a" type="f" corresp="#s1:l1">praetje</rhyme></l>
        <l id="s1:l5" met="1" type="-+">Een <rhyme label="a" type="f" corresp="#s1:l1">praetje</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Van de soete <rhyme label="b" type="m" corresp="#s1:l2">vryery</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Tyter ick weet van geen <rhyme label="a" type="f">vrye</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Noch ick moey mijn daer niet <rhyme label="b" type="m">mee</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Maer ick gae hier daghelijcx <rhyme label="a" type="f" corresp="#s2:l1">weye</rhyme>,</l>
        <l id="s2:l4" met="1" type="-+">Hier <rhyme label="a" type="f" corresp="#s2:l1">weye</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">Hier <rhyme label="a" type="f" corresp="#s2:l1">weye</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Met mijn schaepjes en mijn <rhyme label="b" type="m" corresp="#s2:l2">Vee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Waerom veynst ghy <rhyme label="a" type="f">Harderinne</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Voor mijn seght alwaerde <rhyme label="b" type="m">vrouw</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Ist niet Thirsis die u toont <rhyme label="a" type="f" corresp="#s3:l1">minne</rhyme>,</l>
        <l id="s3:l4" met="1" type="-+">Toont <rhyme label="a" type="f" corresp="#s3:l1">minne</rhyme>,</l>
        <l id="s3:l5" met="1" type="-+">Toont <rhyme label="a" type="f" corresp="#s3:l1">minne</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Ende biedt u staegh sijn <rhyme label="b" type="m" corresp="#s3:l2">trouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Thirsis kan my niet <rhyme label="a" type="f">vermaken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Daerom ick sijn min niet <rhyme label="b" type="m">acht</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Sijn liefd' mach hy wel <rhyme label="a" type="f" corresp="#s4:l1">staecke</rhyme>,</l>
        <l id="s4:l4" met="1" type="-+">Wel <rhyme label="a" type="f" corresp="#s4:l1">stake</rhyme>,</l>
        <l id="s4:l5" met="1" type="-+">Wel <rhyme label="a" type="f" corresp="#s4:l1">stake</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Want ghy geen min van my <rhyme label="b" type="m" corresp="#s4:l2">verwacht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Nochtans soete <rhyme label="a" type="f">Phillidaetje</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Sijt ghy t'samen op het <rhyme label="b" type="m">woort</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Men seyt dit soete <rhyme label="a" type="f" corresp="#s5:l1">Maetje</rhyme>,</l>
        <l id="s5:l4" met="1" type="-+">Dit <rhyme label="a" type="f" corresp="#s5:l1">Maetje</rhyme></l>
        <l id="s5:l5" met="1" type="-+">Dit <rhyme label="a" type="f" corresp="#s5:l1">Maetje</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Gaet met Philidaetje <rhyme label="b" type="m" corresp="#s5:l2">voort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Tyter soeckt my maer te <rhyme label="a" type="f">quellen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">'k Sie het is om tijt <rhyme label="b" type="m">verdrijf</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Vaert wel en laet dit <rhyme label="a" type="f" corresp="#s6:l1">lellen</rhyme></l>
        <l id="s6:l4" met="1" type="-+">Dit <rhyme label="a" type="f" corresp="#s6:l1">lelle</rhyme>,</l>
        <l id="s6:l5" met="1" type="-+">Dit <rhyme label="a" type="f" corresp="#s6:l1">lelle</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Langer ick by u niet <rhyme label="b" type="m" corresp="#s6:l2">blijf</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
