    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Ballet Dorothée</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Philis, ag, ik kwijn door <rhyme label="a" type="f">Liefde</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Sedert gy my ’t hart <rhyme label="a" type="f" corresp="#s1:l1">door-griefde</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Met schigt op <rhyme label="b" type="m">schigt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Uit u seer schoon <rhyme label="b" type="m" corresp="#s1:l3">gezigt</rhyme>.</l>
        <l id="s1:l5" met="1" type="-+">Wist <rhyme label="c" type="m">gy</rhyme></l>
        <l id="s1:l6" met="1" type="-+">Hoe <rhyme label="c" type="m" corresp="#s1:l5">my</rhyme></l>
        <l id="s1:l7" met="3" type="-+">’t Hart sprong door u t’ <rhyme label="d" type="f">aenschouwen</rhyme>!</l>
        <l id="s1:l8" met="2" type="+-">’t Is uw <rhyme label="e" type="m">deugdt</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Die mijn <rhyme label="e" type="m" corresp="#s1:l8">Jeugd</rhyme></l>
        <l id="s1:l10" met="4" type="+-">Boeit met sterke liefdens <rhyme label="d" type="f" corresp="#s1:l7">touwen</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Schoone <rhyme label="f" type="m">Maagd</rhyme>,</l>
        <l id="s1:l12" met="2" type="+-">En <rhyme label="f" type="m" corresp="#s1:l11">gewaegt</rhyme></l>
        <l id="s1:l13" met="4" type="+-">Van uw schoon dat my <rhyme label="f" type="m" corresp="#s1:l11">behaagd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maer wat helpt mijn teeder <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Gy laet u dog noit <rhyme label="a" type="f" corresp="#s2:l1">verwinnen</rhyme>!</l>
        <l id="s2:l3" met="2" type="-+">Maer hoe gy <rhyme label="b" type="m">lagt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met de Liefde en met haer <rhyme label="b" type="m" corresp="#s2:l3">kragt</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">Gy <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s2:l6" met="1" type="-+">Dat <rhyme label="c" type="m" corresp="#s2:l5">soet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Trekken,   want <rhyme label="d" type="f">Cupidootje</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Die gy <rhyme label="e" type="m">tard</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Sal uw <rhyme label="e" type="m" corresp="#s2:l8">hart</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Nog eens raken met een <rhyme label="d" type="f" corresp="#s2:l7">schootje</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Eer gy ’t <rhyme label="f" type="m">gist</rhyme>,</l>
        <l id="s2:l12" met="2" type="+-">Want hy <rhyme label="f" type="m" corresp="#s2:l11">mist</rhyme></l>
        <l id="s2:l13" met="4" type="+-">Noit, want hy verwint door <rhyme label="f" type="m" corresp="#s2:l11">list</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Cupido, ey, hebt <rhyme label="a" type="f">meedogen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Met mijn smert. Toond u <rhyme label="a" type="f" corresp="#s3:l1">vermogen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">O Minne <rhyme label="b" type="m">God</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Aen haer die met u <rhyme label="b" type="m" corresp="#s3:l3">spot</rhyme></l>
        <l id="s3:l5" met="1" type="-+">En <rhyme label="c" type="m">lagt</rhyme>.</l>
        <l id="s3:l6" met="1" type="-+">Uw <rhyme label="c" type="m" corresp="#s3:l5">kragt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Daer het alles voor moet <rhyme label="d" type="f">buigen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Zelf de <rhyme label="e" type="m">Goon</rhyme></l>
        <l id="s3:l9" met="2" type="+-">- O, wat <rhyme label="e" type="m" corresp="#s3:l8">hoon</rhyme> -</l>
        <l id="s3:l10" met="4" type="+-">Valt die nu geheel in <rhyme label="d" type="f" corresp="#s3:l7">duigen</rhyme>?</l>
        <l id="s3:l11" met="2" type="+-">O, dan <rhyme label="f" type="m">sal</rhyme></l>
        <l id="s3:l12" met="2" type="+-">Haest   den <rhyme label="f" type="m" corresp="#s3:l11">val</rhyme>,</l>
        <l id="s3:l13" met="4" type="+-">Naken van het groot heel <rhyme label="f" type="m" corresp="#s3:l11">al</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Trekt uit uw gevulde <rhyme label="a" type="f">Koker</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Eens een sterke <rhyme label="a" type="f" corresp="#s4:l1">Minne-stooker</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Treft haer en <rhyme label="b" type="m">toond</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hoe dat gy zijt <rhyme label="b" type="m" corresp="#s4:l3">gehoond</rhyme>!</l>
        <l id="s4:l5" met="1" type="-+">De <rhyme label="c" type="m">Min</rhyme></l>
        <l id="s4:l6" met="1" type="-+">Moet <rhyme label="c" type="m" corresp="#s4:l5">in</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dat kille hart dog <rhyme label="d" type="f">blaken</rhyme>;</l>
        <l id="s4:l8" met="2" type="+-">Vrouw <rhyme label="e" type="m">natuur</rhyme></l>
        <l id="s4:l9" met="2" type="+-">Sal dat <rhyme label="e" type="m" corresp="#s4:l8">vuur</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Meede aen ’t ontfonken <rhyme label="d" type="f" corresp="#s4:l7">maken</rhyme>.</l>
        <l id="s4:l11" met="2" type="+-">Toef maer <rhyme label="f" type="m">niet</rhyme></l>
        <l id="s4:l12" met="2" type="+-">Lang, maer <rhyme label="f" type="m" corresp="#s4:l11">schiet</rhyme></l>
        <l id="s4:l13" met="4" type="+-">Haer, soo eindigt mijn <rhyme label="f" type="m" corresp="#s4:l11">verdriet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
