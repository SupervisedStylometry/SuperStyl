<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24565</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Conditor alme siderum</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">UAn vrouden ons die kynder <rhyme label="a" type="f">syngen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">des auendes doe sy heymwert <rhyme label="a" type="f" corresp="#s1:l1">gyngen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">dat god onse here gebaren <rhyme label="b" type="m">waert</rhyme></l>
        <l id="s1:l4" met="4" type="-+">op enen wal hiligen kerstes <rhyme label="b" type="m" corresp="#s1:l3">nacht</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat was die alre coldeste <rhyme label="a" type="f">nacht</rhyme></l>
        <l id="s2:l2" met="4" type="-+">die god der werlt ye <rhyme label="a" type="f" corresp="#s2:l1">gegaf</rhyme></l>
        <l id="s2:l3" met="4" type="-+">die god der werlt ye <rhyme label="b" type="m">gesciep</rhyme></l>
        <l id="s2:l4" met="4" type="-+">in eenre crybben waert hi <rhyme label="b" type="m" corresp="#s2:l3">gelecht</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Toe betlahem waert hi <rhyme label="a" type="f">gebaren</rhyme></l>
        <l id="s3:l2" met="4" type="-+">in eenre crybben waert  hi <rhyme label="a" type="f" corresp="#s3:l1">vercaren</rhyme></l>
        <l id="s3:l3" met="4" type="-+">daer en was nyemant nae noch <rhyme label="b" type="m">by</rhyme></l>
        <l id="s3:l4" met="4" type="-+">dan Josep ende die maget <rhyme label="b" type="m" corresp="#s3:l3">maria</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maria en hadde hoer niet <rhyme label="a" type="f">bereyt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">tot enigerhande wiegen <rhyme label="a" type="f" corresp="#s4:l1">cleeyt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">daer si hoer lieue kynt in <rhyme label="b" type="m">want</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Josep die toech alte <rhyme label="b" type="m" corresp="#s4:l3">hant</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die hasen vanden benen <rhyme label="a" type="f">syn</rhyme></l>
        <l id="s5:l2" met="4" type="-+">diemen ons noch taken latet <rhyme label="a" type="f" corresp="#s5:l1">sien</rhyme></l>
        <l id="s5:l3" met="4" type="-+">ende daertoe dat wal hilige <rhyme label="b" type="m">cleet</rhyme></l>
        <l id="s5:l4" met="4" type="-+">daer god syne menscheit in <rhyme label="b" type="m" corresp="#s5:l3">ontfenck</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die Conynck herodes die toernden hem <rhyme label="a" type="f">seer</rhyme></l>
        <l id="s6:l2" met="4" type="-+">hi sprack hoe solde een arme <rhyme label="a" type="f" corresp="#s6:l1">deerne</rhyme></l>
        <l id="s6:l3" met="4" type="-+">een  kynt gewynnen sonder <rhyme label="b" type="m">man</rhyme></l>
        <l id="s6:l4" met="4" type="-+">dat heer waer auer die werlt <rhyme label="b" type="m" corresp="#s6:l3">al</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Conynck herodes des doet ons <rhyme label="a" type="f">noet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Nv slaet ons alle die kynder <rhyme label="a" type="f" corresp="#s7:l1">doet</rhyme></l>
        <l id="s7:l3" met="4" type="-+">en spaert noch conynck noch keysers <rhyme label="b" type="m">kynt</rhyme></l>
        <l id="s7:l4" met="4" type="-+">soe waer gyse inden lande <rhyme label="b" type="m" corresp="#s7:l3">vynt</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Sy slogen der elf dusent <rhyme label="a" type="f">doet</rhyme></l>
        <l id="s8:l2" met="4" type="-+">sy storten al oer Jonge <rhyme label="a" type="f" corresp="#s8:l1">bloet</rhyme></l>
        <l id="s8:l3" met="4" type="-+">hoe diep dat sy daer ynne <rhyme label="b" type="m">woet</rhyme></l>
        <l id="s8:l4" met="4" type="-+">al in der Jonger kynder <rhyme label="b" type="m" corresp="#s8:l3">bloet</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Een stemme al vanden hemel <rhyme label="a" type="f">quam</rhyme></l>
        <l id="s9:l2" met="4" type="-+">tot Josep den wal hilige <rhyme label="a" type="f" corresp="#s9:l1">man</rhyme></l>
        <l id="s9:l3" met="4" type="-+">scaffet v van hen der doet ons <rhyme label="b" type="m">noet</rhyme></l>
        <l id="s9:l4" met="4" type="-+">of Jhesus christus  blyuet hier <rhyme label="b" type="m" corresp="#s9:l3">doet</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Josep die liep ende <rhyme label="a" type="f">ran</rhyme></l>
        <l id="s10:l2" met="4" type="-+">dat hi een eselkyn <rhyme label="a" type="f" corresp="#s10:l1">gewan</rhyme></l>
        <l id="s10:l3" met="4" type="-+">daer op setten hi dat <rhyme label="b" type="m">maegdekyn</rhyme></l>
        <l id="s10:l4" met="4" type="-+">mit oren wal lieuen <rhyme label="b" type="m" corresp="#s10:l3">kyndekyn</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Maria doe vaste <rhyme label="a" type="f">voertgereet</rhyme></l>
        <l id="s11:l2" met="4" type="-+">den langen wech den smaelen <rhyme label="a" type="f" corresp="#s11:l1">pat</rhyme></l>
        <l id="s11:l3" met="4" type="-+">soe weer al in dat egipten <rhyme label="b" type="m">lant</rhyme></l>
        <l id="s11:l4" met="4" type="-+">daer waren sy al <rhyme label="b" type="m" corresp="#s11:l3">onbecant</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Daer nae auer driendertich <rhyme label="a" type="f">Jaren</rhyme></l>
        <l id="s12:l2" met="4" type="-+">waert Jhesus aen dat cruce <rhyme label="a" type="f" corresp="#s12:l1">geslagen</rhyme></l>
        <l id="s12:l3" met="4" type="-+">dat cruce was xv voeten <rhyme label="b" type="m">lanck</rhyme></l>
        <l id="s12:l4" met="4" type="-+">och Jhesus christus was soe <rhyme label="b" type="m" corresp="#s12:l3">cranck</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Van geyselen ende van swaren <rhyme label="a" type="f">slagen</rhyme></l>
        <l id="s13:l2" met="4" type="-+">dat hi dat cruce niet en conde <rhyme label="a" type="f" corresp="#s13:l1">gedragen</rhyme></l>
        <l id="s13:l3" met="4" type="-+">daer toe soldewi ons <rhyme label="b" type="m">voegen</rhyme></l>
        <l id="s13:l4" met="4" type="-+">dat wi tcruce mit hem <rhyme label="b" type="m" corresp="#s13:l3">droegen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
