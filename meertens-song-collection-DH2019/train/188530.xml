  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Als men eenmaal d'oogen <rhyme label="a" type="m">slaat</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Op al de geschapen <rhyme label="b" type="f">dingen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En hun <rhyme label="b" type="f" corresp="#s1:l2">beurtverwisselingen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Alles komt en alles <rhyme label="a" type="m" corresp="#s1:l1">gaat</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Alles komt en alles <rhyme label="a" type="m" corresp="#s1:l1">gaat</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">'t Bosch strooit zyne bladers <rhyme label="c" type="m">nêer</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En 't staat vol van dorre <rhyme label="d" type="f">boomen</rhyme>;</l>
        <l id="s1:l8" met="4" type="+-">Doch naauw is de lent <rhyme label="d" type="f" corresp="#s1:l7">gekomen</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Of het krygt zyn' groente <rhyme label="c" type="m" corresp="#s1:l6">weer</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Of het krygt zyn groente <rhyme label="c" type="m" corresp="#s1:l6">wêer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Doch het schoon 't geen 't <rhyme label="a" type="m">Godendom</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Gaf aan jonge <rhyme label="b" type="f">herderinnen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die de jonge knaapen <rhyme label="b" type="f" corresp="#s2:l2">minnen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Eens vergaan, keert nooit <rhyme label="a" type="m" corresp="#s2:l1">weerom</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Eens vergaan, keert nooit <rhyme label="a" type="m" corresp="#s2:l1">weerom</rhyme>.</l>
        <l id="s2:l6" met="4" type="+-">Frische jeugd, en schoon <rhyme label="c" type="m">vergaat</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Elpen voorhoofd, blonde <rhyme label="d" type="f">haaren</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Zwichten eind'lyk voor de <rhyme label="d" type="f" corresp="#s2:l7">jaaren</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">En dan is 't berouw te <rhyme label="c" type="m" corresp="#s2:l6">laat</rhyme>.</l>
        <l id="s2:l10" met="4" type="+-">En dan is 't berouw te <rhyme label="c" type="m" corresp="#s2:l6">laat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Fiere meisjes, die uw <rhyme label="a" type="m">jeugd</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Laat voorby - gaan zonder <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Laat de reden U <rhyme label="b" type="f" corresp="#s3:l2">verwinnen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Mint, terwyl gy minnen <rhyme label="a" type="m" corresp="#s3:l1">meugt</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Mint, terwyl gy minnen <rhyme label="a" type="m" corresp="#s3:l1">meugt</rhyme>.</l>
        <l id="s3:l6" met="4" type="+-">Zet uw fierheid aan een <rhyme label="c" type="m">kant</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Laat in tyds de vrugten <rhyme label="d" type="f">plukken</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Wilt tog voor de liefde <rhyme label="d" type="f" corresp="#s3:l7">bukken</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Kiest, die om uw schoonheid <rhyme label="c" type="m" corresp="#s3:l6">brandt</rhyme>.</l>
        <l id="s3:l10" met="4" type="+-">Kiest, die om uw schoonheid <rhyme label="c" type="m" corresp="#s3:l6">brandt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
