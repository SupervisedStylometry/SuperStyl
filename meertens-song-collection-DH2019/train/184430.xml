    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GY die Mariaas <rhyme label="a" type="m">zyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zegt, eer uw' herten <rhyme label="b" type="f">brand'den</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Was niet de liefde <rhyme label="a" type="m" corresp="#s1:l1">wyd</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Een woord sprak Hy U <rhyme label="c" type="m">toe</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Hoe was dat woordje? <rhyme label="c" type="m" corresp="#s1:l4">hoe</rhyme>?</l>
        <l id="s1:l6" met="3" type="-+">Neem, Zondaress <rhyme label="d" type="m">vervloekt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Den Zeegen, dien Gy <rhyme label="d" type="m" corresp="#s1:l6">zoekt</rhyme>.</l>
      </lg>
    </lg>
