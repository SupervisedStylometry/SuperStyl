  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SPaar nu geen Snaar noch lustigh <rhyme label="a" type="f">quelen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Maar trouw 't Musicq an d' Edle <rhyme label="b" type="m">Luyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">De Sangh ghevlochte met het <rhyme label="a" type="f" corresp="#s1:l1">Spelen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Beeld af hoe Struckel met syn <rhyme label="b" type="m" corresp="#s1:l2">Bruydt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Een-stemmigh, lieff lijk, eens van <rhyme label="c" type="f">sinne</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Beleven sullen haere <rhyme label="c" type="f" corresp="#s1:l5">minne</rhyme>.</l>
      </lg>
    </lg>
