<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9985</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laat de zinneloze vrijen</melody>
  <author>Isaak A. van Vaerlen en Jan Jansz. van Asten</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LAet de zinneloose <rhyme label="a" type="f">vryen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">So veel als hy immers <rhyme label="b" type="m">magh</rhyme></l>
        <l id="s1:l3" met="4" type="+-">En by 't Winter-koren <rhyme label="a" type="f" corresp="#s1:l1">lijen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Ick segh sulcke goeden <rhyme label="b" type="m" corresp="#s1:l2">dagh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Om een lonckje, om een <rhyme label="a" type="f">groetje</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Om een op-ghetooyde <rhyme label="b" type="m">niet</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Mijn onnoosel lompe <rhyme label="a" type="f" corresp="#s2:l1">Bloetje</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Schepje vreught in dit <rhyme label="b" type="m" corresp="#s2:l2">verdriet</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Is veel beter wel te <rhyme label="a" type="f">slapen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Op de sachte Pluymen <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">As by Nacht verkleumt staen <rhyme label="a" type="f" corresp="#s3:l1">gapen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Voor een deur, in Mottigh <rhyme label="b" type="m" corresp="#s3:l2">weer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wilje  u tot rust <rhyme label="a" type="f">begheven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Droomen isser gheen <rhyme label="b" type="m">ghebreck</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Och die soo bekommert <rhyme label="a" type="f" corresp="#s4:l1">leven</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Sijn wel dubbelt over <rhyme label="b" type="m" corresp="#s4:l2">geck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Komt met mijn dit vaersjen <rhyme label="a" type="f">quelen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Minnaers van de witte <rhyme label="b" type="m">doeck</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Soos al al u quellingh <rhyme label="a" type="f" corresp="#s5:l1">helen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En verschuyven in een <rhyme label="b" type="m" corresp="#s5:l2">hoeck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
