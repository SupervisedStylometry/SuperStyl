<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180705</id>
    <date>
      <year-start>1688</year-start>
      <year-end>1688</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nadien uw goddelijkheid</melody>
  <author>Philippus van Sorgen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoe wort mijn herte <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van alle hand soo <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Door soo veel list <rhyme label="b" type="f">besprongen</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Hoe komt die Helsche <rhyme label="c" type="m">slang</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Op my <rhyme label="b" type="f" corresp="#s1:l3">aangedrongen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En maakt mijn Ziel soo <rhyme label="c" type="m" corresp="#s1:l4">bang</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">En mijn gants snoode <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hoe maakt het my <rhyme label="a" type="m" corresp="#s2:l1">verwert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat ik niet door kan <rhyme label="b" type="f">breken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Waer dat ik sit of <rhyme label="c" type="m">sta</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Ah! hoe veel <rhyme label="b" type="f" corresp="#s2:l3">gebreken</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sleepen my agter <rhyme label="c" type="m" corresp="#s2:l4">na</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ook bid de Wereld <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat ik dog hares <rhyme label="a" type="m" corresp="#s3:l1">zy</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dan komt dien <rhyme label="b" type="f">Ziel-verleyder</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Met listen op my <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Op dat hy my <rhyme label="b" type="f" corresp="#s3:l3">weyder</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Mogt in sijn net doen <rhyme label="c" type="m" corresp="#s3:l4">gaan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ach Iesu! ô mijn <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Moet ik dan nog soo <rhyme label="a" type="m" corresp="#s4:l1">seer</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Van u zijn <rhyme label="b" type="f">afgedreven</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">Sal in die wreede <rhyme label="c" type="m">magt</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Mijn Ziel langer <rhyme label="b" type="f" corresp="#s4:l3">leven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dit nu is sonder <rhyme label="c" type="m" corresp="#s4:l4">kragt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">U bid ik, ô mijn <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Laet my dog <rhyme label="a" type="m" corresp="#s5:l1">nimmermeer</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Van uwe wegen <rhyme label="b" type="f">wijken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Blijf my dog altijt <rhyme label="c" type="m">by</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">Want ik moet <rhyme label="b" type="f" corresp="#s5:l3">beswijken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Als ik niet by u <rhyme label="c" type="m" corresp="#s5:l4">zy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Kom woont dog in mijn <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Eer dat het sterft van <rhyme label="a" type="m" corresp="#s6:l1">smert</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">U met u Geest <rhyme label="b" type="f">alleene</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soekt het; ja ‘t is u <rhyme label="c" type="m">deel</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">U deel, anders <rhyme label="b" type="f" corresp="#s6:l3">geene</rhyme>:</l>
        <l id="s6:l6" met="3" type="-+">Neemt gy het dog <rhyme label="c" type="m" corresp="#s6:l4">geheel</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O Iesu, kom dog <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Doet gy het tot u <rhyme label="a" type="m" corresp="#s7:l1">gaan</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Iaagt gy die vreemde <rhyme label="b" type="f">Heeren</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Met al haar magt daar <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Doet het tot u <rhyme label="b" type="f" corresp="#s7:l3">keeren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Dit is dog u <rhyme label="c" type="m" corresp="#s7:l4">besluyt</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O Wereld, snoode <rhyme label="a" type="m">vlees</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Waer toe dog al u <rhyme label="a" type="m" corresp="#s8:l1">vrees</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Dat ik u sal <rhyme label="b" type="f">begeven</rhyme>?</l>
        <l id="s8:l4" met="3" type="-+">Hebt gy dan niet <rhyme label="c" type="m">beschout</rhyme></l>
        <l id="s8:l5" met="3" type="+-">Wat Iesu most <rhyme label="b" type="f" corresp="#s8:l3">geven</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Eer hy my heeft <rhyme label="c" type="m" corresp="#s8:l4">getrout</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Heeft het u wel een <rhyme label="a" type="m">traan</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Doen uyt u oogen <rhyme label="a" type="m" corresp="#s9:l1">gaan</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Kostend’ u soo veel <rhyme label="b" type="f">sugten</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">(Op dat ik <rhyme label="c" type="m">wederom</rhyme></l>
        <l id="s9:l5" met="3" type="+-">Sou den doot <rhyme label="b" type="f" corresp="#s9:l3">ontvlugten</rhyme>,)</l>
        <l id="s9:l6" met="3" type="-+">Als mijnen <rhyme label="c" type="m" corresp="#s9:l4">Bruydegom</rhyme>?</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wat wilje my <rhyme label="a" type="m">geheel</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Nog hebben tot u <rhyme label="a" type="m" corresp="#s10:l1">deel</rhyme>?</l>
        <l id="s10:l3" met="3" type="-+">Zijt gy soo rijk als <rhyme label="b" type="f">Iesus</rhyme>?</l>
        <l id="s10:l4" met="3" type="-+">Gy bent een <rhyme label="c" type="m">bedelaar</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Als gy staet by <rhyme label="b" type="f" corresp="#s10:l3">Iesus</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Weg dan met u <rhyme label="c" type="m" corresp="#s10:l4">gebaar</rhyme>!</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Of denkje datje <rhyme label="a" type="m">hier</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Door al u loos <rhyme label="a" type="m" corresp="#s11:l1">getier</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">En door u slimme <rhyme label="b" type="f">treken</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">My sult bewegen <rhyme label="c" type="m">nog</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">En tot u dienst <rhyme label="b" type="f" corresp="#s11:l3">preken</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Door al u slim <rhyme label="c" type="m" corresp="#s11:l4">bedrog</rhyme>?</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Denkje dat ik niet <rhyme label="a" type="m">ken</rhyme></l>
        <l id="s12:l2" met="3" type="-+">En soo vergetig <rhyme label="a" type="m" corresp="#s12:l1">ben</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Hoeje my had <rhyme label="b" type="f">gebonden</rhyme>?</l>
        <l id="s12:l4" met="3" type="-+">Dat heeft te veel <rhyme label="c" type="m">gekost</rhyme></l>
        <l id="s12:l5" met="3" type="+-">Eer ik van mijn <rhyme label="b" type="f" corresp="#s12:l3">sonden</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Ben door veel bloet <rhyme label="c" type="m" corresp="#s12:l4">verlost</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">13.Iesus u Heylig <rhyme label="a" type="m">Hert</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Heeft het te seer <rhyme label="a" type="m" corresp="#s13:l1">gesmert</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">En ‘t heeft u boven <rhyme label="b" type="f">maten</rhyme></l>
        <l id="s13:l4" met="3" type="-+">In uwe Ziel <rhyme label="c" type="m">geraakt</rhyme>,</l>
        <l id="s13:l5" met="3" type="+-">God most u <rhyme label="b" type="f" corresp="#s13:l3">verlaten</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Eer ik wierd vry <rhyme label="c" type="m" corresp="#s13:l4">gemaakt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Meent gy ô Wereld <rhyme label="a" type="m">nu</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Dat ‘er nog iet voor <rhyme label="a" type="m" corresp="#s14:l1">u</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Overig is <rhyme label="b" type="f">gebleven</rhyme>?</l>
        <l id="s14:l4" met="3" type="-+">Neen, die voor my <rhyme label="c" type="m">geheel</rhyme></l>
        <l id="s14:l5" met="3" type="+-">Sig gink <rhyme label="b" type="f" corresp="#s14:l3">overgeven</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Die kies ik tot mijn <rhyme label="c" type="m" corresp="#s14:l4">deel</rhyme>!</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Weest my dog niet meer <rhyme label="a" type="m">aan</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Om u ten dienst te <rhyme label="a" type="m" corresp="#s15:l1">staan</rhyme>:</l>
        <l id="s15:l3" met="3" type="-+">Want dog: wat sal ‘t u <rhyme label="b" type="f">baten</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Soo ik een kleynen <rhyme label="c" type="m">tijt</rhyme>,</l>
        <l id="s15:l5" met="3" type="+-">Die u moet <rhyme label="b" type="f" corresp="#s15:l3">verlaten</rhyme>;</l>
        <l id="s15:l6" met="3" type="-+">Was tot u dienst <rhyme label="c" type="m" corresp="#s15:l4">bereyt</rhyme>?</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Hebt gy dan dienst van <rhyme label="a" type="m">doen</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Laat het u and’ren <rhyme label="a" type="m" corresp="#s16:l1">doen</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Die hier u vreugt <rhyme label="b" type="f">genieten</rhyme>:</l>
        <l id="s16:l4" met="3" type="-+">Gy zijt haar Hooft <rhyme label="c" type="m">gestelt</rhyme></l>
        <l id="s16:l5" met="3" type="+-">‘t Kan haar niet <rhyme label="b" type="f" corresp="#s16:l3">verdrieten</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Hoe gyse bind en <rhyme label="c" type="m" corresp="#s16:l4">quelt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Ik heb een ander <rhyme label="a" type="m">God</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Verkregen tot mijn <rhyme label="a" type="m" corresp="#s17:l1">lod</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Die kan ik niet <rhyme label="b" type="f">verlaten</rhyme>:</l>
        <l id="s17:l4" met="3" type="-+">Die wil voor u niet <rhyme label="c" type="m">meer</rhyme></l>
        <l id="s17:l5" met="3" type="+-">Over in my <rhyme label="b" type="f" corresp="#s17:l3">laten</rhyme>:</l>
        <l id="s17:l6" met="3" type="-+">Alles is voor dien <rhyme label="c" type="m" corresp="#s17:l4">Heer</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">U Iesu kleef ik <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Laet my niet van u <rhyme label="a" type="m" corresp="#s18:l1">gaan</rhyme>:</l>
        <l id="s18:l3" met="3" type="-+">Uw’ dienst is regte <rhyme label="b" type="f">vryheyt</rhyme>:</l>
        <l id="s18:l4" met="3" type="-+">Wat slaafschen dienst ooit <rhyme label="c" type="m">gaf</rhyme>,</l>
        <l id="s18:l5" met="3" type="+-">Acht ik by u <rhyme label="b" type="f" corresp="#s18:l3">blyheyt</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Niet meer als enkel <rhyme label="c" type="m" corresp="#s18:l4">draf</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">O Koning, gy <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Gy Iesu, anders <rhyme label="a" type="m" corresp="#s19:l1">geen</rhyme>;</l>
        <l id="s19:l3" met="3" type="-+">Gy zijt al mijn <rhyme label="b" type="f">begeeren</rhyme>:</l>
        <l id="s19:l4" met="3" type="-+">Als ik u maar <rhyme label="c" type="m">eerlang</rhyme>,</l>
        <l id="s19:l5" met="3" type="+-">Sal het my niet <rhyme label="b" type="f" corresp="#s19:l3">deeren</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Of het my valt wat <rhyme label="c" type="m" corresp="#s19:l4">bang</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Als ik u Geestes <rhyme label="a" type="m">gloet</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Nog soo wat missen <rhyme label="a" type="m" corresp="#s20:l1">moet</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">En in mijn Ziel niet <rhyme label="b" type="f">smaken</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">U, Iesus blijf ik <rhyme label="c" type="m">by</rhyme>:</l>
        <l id="s20:l5" met="3" type="+-">Wilt my los <rhyme label="b" type="f" corresp="#s20:l3">maken</rhyme>:</l>
        <l id="s20:l6" met="3" type="-+">Ei weest versoent met <rhyme label="c" type="m" corresp="#s20:l4">my</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Agt gy my dan niet <rhyme label="a" type="m">weert</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">Dat gy mijn saak <rhyme label="a" type="m" corresp="#s21:l1">aanveert</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">Doet het dan om die <rhyme label="b" type="f">liefde</rhyme></l>
        <l id="s21:l4" met="3" type="-+">Die uwe teere <rhyme label="c" type="m">Ziel</rhyme></l>
        <l id="s21:l5" met="3" type="+-">Soo kragtig <rhyme label="b" type="f" corresp="#s21:l3">doorgriefde</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">Om uyt dien tuymel <rhyme label="c" type="m" corresp="#s21:l4">wiel</rhyme></l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Waar in ik soo <rhyme label="a" type="m">vermoort</rhyme></l>
        <l id="s22:l2" met="3" type="-+">Lag door mijn bloet <rhyme label="a" type="m" corresp="#s22:l1">gesmoort</rhyme>,</l>
        <l id="s22:l3" met="3" type="-+">My weder op te <rhyme label="b" type="f">heffen</rhyme></l>
        <l id="s22:l4" met="3" type="-+">(O liefde <rhyme label="c" type="m">overgroot</rhyme>!</l>
        <l id="s22:l5" met="3" type="+-">Wie kan u <rhyme label="b" type="f" corresp="#s22:l3">beseffen</rhyme>!)</l>
        <l id="s22:l6" met="3" type="-+">Door sulken bitt’ren <rhyme label="c" type="m" corresp="#s22:l4">doot</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">O mijnen <rhyme label="a" type="m">Bruydegom</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Siet gy dog na my <rhyme label="a" type="m" corresp="#s23:l1">om</rhyme>,</l>
        <l id="s23:l3" met="3" type="-+">O mijnen lieven <rhyme label="b" type="f">Koning</rhyme>,</l>
        <l id="s23:l4" met="3" type="-+">Hoe kan ik van u <rhyme label="c" type="m">gaan</rhyme>?</l>
        <l id="s23:l5" met="3" type="+-">Mijn Heer is u <rhyme label="b" type="f" corresp="#s23:l3">wooning</rhyme>,</l>
        <l id="s23:l6" met="3" type="-+">‘t Moet voor u open <rhyme label="c" type="m" corresp="#s23:l4">staan</rhyme>?</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Vaar wel o snoode <rhyme label="a" type="m">vlees</rhyme>,</l>
        <l id="s24:l2" met="3" type="-+">Gy oorsaak van mijn <rhyme label="a" type="m" corresp="#s24:l1">vrees</rhyme>,</l>
        <l id="s24:l3" met="3" type="-+">Dat ’s voor u wreed <rhyme label="b" type="f">verkragten</rhyme>:</l>
        <l id="s24:l4" met="3" type="-+">Siet niet meer na my <rhyme label="c" type="m">om</rhyme>:</l>
        <l id="s24:l5" met="3" type="+-">Wat ik ga <rhyme label="b" type="f" corresp="#s24:l3">betraften</rhyme></l>
        <l id="s24:l6" met="3" type="-+">Is voor mijn <rhyme label="c" type="m" corresp="#s24:l4">Bruydegom</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">Daar Iesu, daar ’s mijn <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s25:l2" met="3" type="-+">Dat soo geheelijck <rhyme label="a" type="m" corresp="#s25:l1">wert</rhyme></l>
        <l id="s25:l3" met="3" type="-+">Om u, tot u <rhyme label="b" type="f">gedreven</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">O Iesu! daar is ‘t <rhyme label="c" type="m">heel</rhyme>,</l>
        <l id="s25:l5" met="3" type="+-">Gy deed ‘t weder <rhyme label="b" type="f" corresp="#s25:l3">leven</rhyme>,</l>
        <l id="s25:l6" met="3" type="-+">U kiest het tot sijn <rhyme label="c" type="m" corresp="#s25:l4">deel</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">Daar is het heel end’ <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s26:l2" met="3" type="-+">Nu het u dienen <rhyme label="a" type="m" corresp="#s26:l1">sal</rhyme>,</l>
        <l id="s26:l3" met="3" type="-+">Laat het geen vreemde <rhyme label="b" type="f">Heeren</rhyme>:</l>
        <l id="s26:l4" met="3" type="-+">Gy hebt het dier <rhyme label="c" type="m">gekogt</rhyme>:</l>
        <l id="s26:l5" met="3" type="+-">Laat het nooit van u <rhyme label="b" type="f" corresp="#s26:l3">keeren</rhyme>,</l>
        <l id="s26:l6" met="3" type="-+">‘t Is u magt <rhyme label="c" type="m" corresp="#s26:l4">gebrogt</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
