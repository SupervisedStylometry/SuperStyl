    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 107 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Philips van Marnix</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LOoft end' prijst God <rhyme label="a" type="f">ghenadich</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Want syne <rhyme label="b" type="m">goedicheyt</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Is groot end' duert <rhyme label="a" type="f" corresp="#s1:l1">gestadich</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Van nu in <rhyme label="b" type="m" corresp="#s1:l2">eewicheyt</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">2. Sulcx moghen seggen <rhyme label="c" type="m">bly'</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">All' die God uyt <rhyme label="d" type="f">ghenaden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ghemaect heeft los end' <rhyme label="c" type="m" corresp="#s1:l5">vry</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Van ancxt end' boose <rhyme label="d" type="f" corresp="#s1:l6">schaden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">* Die hy uyt all' <rhyme label="a" type="f">ghevesten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Mit wonderbaere <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Van Oosten end' van <rhyme label="a" type="f" corresp="#s2:l1">Westen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Suydt end' Noort heeft <rhyme label="b" type="m" corresp="#s2:l2">ghebracht</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">4. Doe sy verd' uyt de <rhyme label="c" type="m">weeg</rhyme></l>
        <l id="s2:l6" met="3" type="-+">In wildernisse <rhyme label="d" type="f">dwaelden</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">End' vonden stap noch <rhyme label="c" type="m" corresp="#s2:l5">steeg</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Maer alle steden <rhyme label="d" type="f" corresp="#s2:l6">faelden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">* Oock die schier zijn <rhyme label="a" type="f">besweken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wt dorst end' honghers <rhyme label="b" type="m">noot</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">End' in benauwtheyt <rhyme label="a" type="f" corresp="#s3:l1">steken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Cranck zijnde totter <rhyme label="b" type="m" corresp="#s3:l2">doot</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">6. Alss' in sulck <rhyme label="c" type="m">ongheluck</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Tot God den Heere <rhyme label="d" type="f">schreyen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Hy comts' uyt haren <rhyme label="c" type="m" corresp="#s3:l5">druck</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In voorspoet vrolijck <rhyme label="d" type="f" corresp="#s3:l6">ley'en</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">* Hy laetse wed'rom <rhyme label="a" type="f">treden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Op de bekende <rhyme label="b" type="m">baen</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">End' tot bewoonde <rhyme label="a" type="f" corresp="#s4:l1">steden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Brengtse van stonden <rhyme label="b" type="m" corresp="#s4:l2">aen</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">8. Dan louen sy met <rhyme label="c" type="m">vreugt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Den Heer om sijn <rhyme label="d" type="f">ghenaden</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">End' roemen seer <rhyme label="c" type="m" corresp="#s4:l5">verheugt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Den volcken sijn <rhyme label="d" type="f" corresp="#s4:l6">grootdaden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">* Als dat hy laeft het <rhyme label="a" type="f">herte</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat van dorst was <rhyme label="b" type="m">versmacht</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">End' neemt wech, s'honghers <rhyme label="a" type="f" corresp="#s5:l1">smerte</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met spys' end' nieuwe <rhyme label="b" type="m" corresp="#s5:l2">cracht</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">10. Die duyster inden <rhyme label="c" type="m">stock</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Bevaen met s'doodes <rhyme label="d" type="f">banden</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Sijn vast in eenen <rhyme label="c" type="m" corresp="#s5:l5">block</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Gheboeyet voet' end' <rhyme label="d" type="f" corresp="#s5:l6">handen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">* Om dat sy Gods woort <rhyme label="a" type="f">weerdich</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hielen in spot end' <rhyme label="b" type="m">smaet</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">End' van ghemoet <rhyme label="a" type="f" corresp="#s6:l1">hooveerdich</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Verstieten synen <rhyme label="b" type="m" corresp="#s6:l2">raet</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">12. Als nu haer droef <rhyme label="c" type="m">ghemoet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ghestraft met vele <rhyme label="d" type="f">plaghen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">In sulcken <rhyme label="c" type="m" corresp="#s6:l5">teghenspoet</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Is hulp'loos ne'er <rhyme label="d" type="f" corresp="#s6:l6">gheslaghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">* End' sy tot God den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Droef schreyen in <rhyme label="b" type="m">aenstoot</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hy helpts' uyt haer' hert <rhyme label="a" type="f" corresp="#s7:l1">seere</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">End' vrijdts' uyt ancxt end' <rhyme label="b" type="m" corresp="#s7:l2">noot</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">14. Hy brengts' uyt t'duyster <rhyme label="c" type="m">kot</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daers' inne zijn <rhyme label="d" type="f">gheseten</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Van armoey schier <rhyme label="c" type="m" corresp="#s7:l5">verrot</rhyme>:</l>
        <l id="s7:l8" met="3" type="-+">End' breeck't haer bandt end' <rhyme label="d" type="f" corresp="#s7:l6">keten</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">* Dies sy voorts sijn <rhyme label="a" type="f">ghenaden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Steed's voeren inden <rhyme label="b" type="m">mondt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">End' groote <rhyme label="a" type="f" corresp="#s8:l1">wonderdaden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Den menschen maken <rhyme label="b" type="m" corresp="#s8:l2">kondt</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">16. Die poorten van <rhyme label="c" type="m">metael</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Wt d'hencxsels kan <rhyme label="d" type="f">verrucken</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">End' grendels sterck kan <rhyme label="c" type="m" corresp="#s8:l5">stael</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">End' yser breeckt in <rhyme label="d" type="f" corresp="#s8:l6">stucken</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">* Die dwaes end' <rhyme label="a" type="f">onghebonden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">God' wederspannigh <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">End' vallen om haer <rhyme label="a" type="f" corresp="#s9:l1">sonden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">In cranckheyt ende <rhyme label="b" type="m" corresp="#s9:l2">pijn</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">18. Soo dat van spys' end' <rhyme label="c" type="m">dranck</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Verleedt, hun walgt de <rhyme label="d" type="f">maghe</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">End' zijn ter doot toe <rhyme label="c" type="m" corresp="#s9:l5">cranck</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Van sucht end' boose <rhyme label="d" type="f" corresp="#s9:l6">plaghe</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">* Soo sy dan tot Godt <rhyme label="a" type="f">schreyen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Met een ootmoedich <rhyme label="b" type="m">hert'</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hy comt sonder <rhyme label="a" type="f" corresp="#s10:l1">verbeyen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">End' helpts' uyt ancxt end' <rhyme label="b" type="m" corresp="#s10:l2">smert</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">20. Hy schickt hun <rhyme label="c" type="m">opperstondt</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Het woort syner <rhyme label="d" type="f">ghenaden</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">End' maecktse we'er <rhyme label="c" type="m" corresp="#s10:l5">ghesondt</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">End' vry van alle <rhyme label="d" type="f" corresp="#s10:l6">schaden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">* Dan louen sy den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">End' zijn ghenade <rhyme label="b" type="m">goet</rhyme>:</l>
        <l id="s11:l3" met="3" type="-+">Sy maken ruchtbaer <rhyme label="a" type="f" corresp="#s11:l1">seere</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">De deugt die hy hun <rhyme label="b" type="m" corresp="#s11:l2">doet</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">22. Sy schencken loff end' <rhyme label="c" type="m">danck</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">In ste' van <rhyme label="d" type="f">offerhanden</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Vertellend' in haer' <rhyme label="c" type="m" corresp="#s11:l5">sanck</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Het werck van synen <rhyme label="d" type="f" corresp="#s11:l6">handen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">* De ghene die in <rhyme label="a" type="f">schepen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Te water doen haer <rhyme label="b" type="m">vaert</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">End' hebben <rhyme label="a" type="f" corresp="#s12:l1">aenghegrepen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Haer handel ter <rhyme label="b" type="m" corresp="#s12:l2">zeewaert</rhyme>.</l>
        <l id="s12:l5" met="3" type="-+">24. Die moghen vry <rhyme label="c" type="m">aldaer</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Des Heeren daden <rhyme label="d" type="f">mercken</rhyme>:</l>
        <l id="s12:l7" met="3" type="-+">End' op het water <rhyme label="c" type="m" corresp="#s12:l5">claer</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Aensien zijn <rhyme label="d" type="f" corresp="#s12:l6">wonderwercken</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">* Want als hy t'woort wil <rhyme label="a" type="f">spreken</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Hy doet van stonden <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Storm ende wint <rhyme label="a" type="f" corresp="#s13:l1">opsteken</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">End' baren om hoogh' <rhyme label="b" type="m" corresp="#s13:l2">slaen</rhyme>.</l>
        <l id="s13:l5" met="3" type="-+">26. Dan klimmens' inde <rhyme label="c" type="m">locht</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Dan wordens' ind' <rhyme label="d" type="f">afgronden</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Met een baer we'er <rhyme label="c" type="m" corresp="#s13:l5">ghebrocht</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">Haer gheest is schier <rhyme label="d" type="f" corresp="#s13:l6">verswonden</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">* De moet is hun <rhyme label="a" type="f">ghesoncken</rhyme>:</l>
        <l id="s14:l2" met="3" type="-+">T'hooft suyselt ende <rhyme label="b" type="m">draeyt</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Als een die vol end' <rhyme label="a" type="f" corresp="#s14:l1">droncken</rhyme></l>
        <l id="s14:l4" met="3" type="-+">T'verstandt heeft heel <rhyme label="b" type="m" corresp="#s14:l2">verwaeyt</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">28. Dan roepen sy God <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Met een ootmoedich <rhyme label="d" type="f">herte</rhyme>:</l>
        <l id="s14:l7" met="3" type="-+">Die verlosts' alsoo <rhyme label="c" type="m" corresp="#s14:l5">saen</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Van haeren druck end' <rhyme label="d" type="f" corresp="#s14:l6">smerte</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">De storm end' all' <rhyme label="a" type="f">onweder</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Wort stracx van hem <rhyme label="b" type="m">ghestilt</rhyme>:</l>
        <l id="s15:l3" met="3" type="-+">Met een woort legt hy <rhyme label="a" type="f" corresp="#s15:l1">neder</rhyme></l>
        <l id="s15:l4" met="3" type="-+">De baren stuer end' <rhyme label="b" type="m" corresp="#s15:l2">wilt</rhyme>.</l>
        <l id="s15:l5" met="3" type="-+">30. Dies wort haer hert' <rhyme label="c" type="m">verblijdt</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Van Godes goede <rhyme label="d" type="f">gauen</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Die t'schip van storm <rhyme label="c" type="m" corresp="#s15:l5">bevrijdt</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">Brengt in behouden <rhyme label="d" type="f" corresp="#s15:l6">hauen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">* Dan hoort mense <rhyme label="a" type="f">verhalen</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Godts goetheyt <rhyme label="b" type="m">openbaer</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">End' allen mensch <rhyme label="a" type="f" corresp="#s16:l1">afmalen</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Sijn wonderwercken <rhyme label="b" type="m" corresp="#s16:l2">claer</rhyme>.</l>
        <l id="s16:l5" met="3" type="-+">32. Hy wort van hun <rhyme label="c" type="m">vermelt</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">By groote volcker <rhyme label="d" type="f">scharen</rhyme>:</l>
        <l id="s16:l7" met="3" type="-+">End' syne lof <rhyme label="c" type="m" corresp="#s16:l5">vertelt</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">Daer d'oude lie'n <rhyme label="d" type="f" corresp="#s16:l6">vergaren</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">* Die stracx verkeert <rhyme label="a" type="f">riuieren</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">In een dorr' end' woest <rhyme label="b" type="m">landt</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">End' waters doet <rhyme label="a" type="f" corresp="#s17:l1">vertieren</rhyme></l>
        <l id="s17:l4" met="3" type="-+">In een onvruchtbaer <rhyme label="b" type="m" corresp="#s17:l2">sandt</rhyme>.</l>
        <l id="s17:l5" met="3" type="-+">34. Van een vruchtbaren <rhyme label="c" type="m">grondt</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Maeckt soutt' end' dorre <rhyme label="d" type="f">heyden</rhyme>,</l>
        <l id="s17:l7" met="3" type="-+">Om der inwoonders <rhyme label="c" type="m" corresp="#s17:l5">sond'</rhyme>,</l>
        <l id="s17:l8" met="3" type="-+">End' t'leuen dat sy <rhyme label="d" type="f" corresp="#s17:l6">leyden</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">* Daer teghen de <rhyme label="a" type="f">woestynen</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Met watervloeden <rhyme label="b" type="m">vocht</rhyme>:</l>
        <l id="s18:l3" met="3" type="-+">End' met schoon claer <rhyme label="a" type="f" corresp="#s18:l1">fonteynen</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Het dorre sandt <rhyme label="b" type="m" corresp="#s18:l2">verlocht</rhyme>.</l>
        <l id="s18:l5" met="3" type="-+">36. Daer hy dan woonen <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Deghen' die hongher <rhyme label="d" type="f">leden</rhyme>:</l>
        <l id="s18:l7" met="3" type="-+">Die daer met blyden <rhyme label="c" type="m" corresp="#s18:l5">moet</rhyme>,</l>
        <l id="s18:l8" met="3" type="-+">Opbouwen schoone <rhyme label="d" type="f" corresp="#s18:l6">steden</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">* Besaeyen oock den <rhyme label="a" type="f">acker</rhyme></l>
        <l id="s19:l2" met="3" type="-+">End' planten <rhyme label="b" type="m">onbeducht</rhyme></l>
        <l id="s19:l3" met="3" type="-+">Den Wijnstock, die seer <rhyme label="a" type="f" corresp="#s19:l1">wacker</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Brengt jaerlijck syne <rhyme label="b" type="m" corresp="#s19:l2">vrucht</rhyme>.</l>
        <l id="s19:l5" met="3" type="-+">38. God seghent all' haer <rhyme label="c" type="m">werck</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">End' gheeft een goey <rhyme label="d" type="f">ghedyen</rhyme>:</l>
        <l id="s19:l7" met="3" type="-+">Haer vee ghesondt end' <rhyme label="c" type="m" corresp="#s19:l5">sterck</rhyme></l>
        <l id="s19:l8" met="3" type="-+">Neemt toe aen alle <rhyme label="d" type="f" corresp="#s19:l6">syen</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">* Doch worden daernae <rhyme label="a" type="f">minder</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">End' comen schier te <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Door kommer ende <rhyme label="a" type="f" corresp="#s20:l1">hinder</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">End' velerley <rhyme label="b" type="m" corresp="#s20:l2">verdriet</rhyme>.</l>
        <l id="s20:l5" met="3" type="-+">40. Want God stort't schand' end' <rhyme label="c" type="m">smaet</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">Op vorsten ende <rhyme label="d" type="f">Heeren</rhyme>,</l>
        <l id="s20:l7" met="3" type="-+">Die hy verr' uyt de <rhyme label="c" type="m" corresp="#s20:l5">straet</rhyme>,</l>
        <l id="s20:l8" met="3" type="-+">Int wilde, doet <rhyme label="d" type="f" corresp="#s20:l6">verkeeren</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">* Maer hy verheft den <rhyme label="a" type="f">ermen</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">Wt kommer end' <rhyme label="b" type="m">ellendt</rhyme>:</l>
        <l id="s21:l3" met="3" type="-+">End' doet sijn saet wel <rhyme label="a" type="f" corresp="#s21:l1">termen</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Als een kudd' sonder <rhyme label="b" type="m" corresp="#s21:l2">end'</rhyme>.</l>
        <l id="s21:l5" met="3" type="-+">42. Den vromen die dit <rhyme label="c" type="m">sien</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">Sal t'hert van vreugden <rhyme label="d" type="f">kloppen</rhyme>:</l>
        <l id="s21:l7" met="3" type="-+">Maer goddeloose <rhyme label="c" type="m" corresp="#s21:l5">lie'n</rhyme>,</l>
        <l id="s21:l8" met="3" type="-+">Sullen den mont <rhyme label="d" type="f" corresp="#s21:l6">toestoppen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
