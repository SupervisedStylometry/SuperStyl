  <author>Jan van Hoogstraten</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wyd den Heer uw <rhyme label="a" type="f">lofgezangen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die het alverwinnend <rhyme label="b" type="m">zwaard</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat zijn vyanden <rhyme label="b" type="m" corresp="#s1:l2">vervaart</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Heeft op uwe zy <rhyme label="a" type="f" corresp="#s1:l1">gehangen</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Wyd den Heer uw <rhyme label="a" type="f" corresp="#s1:l1">lofgezangen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Die u agt zijn hulpe <rhyme label="b" type="m" corresp="#s1:l2">waard</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hoe de Roofzugt scheen <rhyme label="a" type="f">gebeten</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Op ons vredig <rhyme label="b" type="m">Nederland</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daar de Wet Gods wijngaart <rhyme label="b" type="m" corresp="#s2:l2">plant</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Hy heeft haar ter neer <rhyme label="a" type="f" corresp="#s2:l1">gesmeten</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Hare magt van een <rhyme label="a" type="f" corresp="#s2:l1">gereten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En verstroyt met zijne <rhyme label="b" type="m" corresp="#s2:l2">hand</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hoe de Lelyvorst mogt <rhyme label="a" type="f">stoffen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Op zijn onbepaalde <rhyme label="b" type="m">magt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Hy in 't midden van zijn <rhyme label="b" type="m" corresp="#s3:l2">kragt</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Van Gods sterke hand <rhyme label="a" type="f" corresp="#s3:l1">getroffen</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Zag zijn heir ter aarden <rhyme label="a" type="f" corresp="#s3:l1">ploffen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">En zijn glori heel <rhyme label="b" type="m" corresp="#s3:l2">veragt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Sluyt nu met een <rhyme label="a" type="f">wapenketen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Hollands vrygevogten <rhyme label="b" type="m">hek</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Treed den Godsdienst op den <rhyme label="b" type="m" corresp="#s4:l2">nek</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Op uw Rijkszon te <rhyme label="a" type="f" corresp="#s4:l1">vermeten</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">God in 't eewig ligt <rhyme label="a" type="f" corresp="#s4:l1">gezeten</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Werpt haar luister in den <rhyme label="b" type="m" corresp="#s4:l2">drek</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">God, voor wien het al moet <rhyme label="a" type="f">buigen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Doet het vredig <rhyme label="b" type="m">Nederlant</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Steeds gered van zijne <rhyme label="b" type="m" corresp="#s5:l2">hand</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Van zijn bystant weer <rhyme label="a" type="f" corresp="#s5:l1">getuigen</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Nu het alles stort in <rhyme label="a" type="f" corresp="#s5:l1">duigen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dat haar vest had <rhyme label="b" type="m" corresp="#s5:l2">aangerand</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zoo vertreed, op zijn <rhyme label="a" type="f">begeeren</rhyme></l>
        <l id="s6:l2" met="4" type="+-">De Geregtigheyt den <rhyme label="b" type="m">Nyt</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Bondgenooten wijd, en <rhyme label="b" type="m" corresp="#s6:l2">zijd</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Zingt, en looft den Heer der <rhyme label="a" type="f" corresp="#s6:l1">Heeren</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Die u gunt te <rhyme label="a" type="f" corresp="#s6:l1">triomferen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En voor Hollands welstant <rhyme label="b" type="m" corresp="#s6:l2">strijd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
