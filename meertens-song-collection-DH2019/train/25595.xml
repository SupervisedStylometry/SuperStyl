    <melody>O Venus der minne godin</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Leenaert Clock</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Almachtich Godt, comt my te <rhyme label="a" type="f">baten</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Gheeft my in mijn herte te <rhyme label="a" type="f" corresp="#s1:l1">vaten</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Wat u // doch nu // heer doet <rhyme label="b" type="f">behaghen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">In mijne <rhyme label="b" type="f" corresp="#s1:l3">daghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En hout my doch in uwer <rhyme label="a" type="f">vresen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Wat mijn doen en laten sal <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat gheeft // beleeft // my doch nu <rhyme label="b" type="f">spoedich</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">O Heere <rhyme label="b" type="f" corresp="#s2:l3">goedich</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Christus u lieve kindt sal <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Te rechten die boose en <rhyme label="a" type="f" corresp="#s3:l1">vromen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Voorwaer // aldaer // sal hy <rhyme label="b" type="f">verclaren</rhyme></l>
        <l id="s3:l4" met="2" type="-+">En <rhyme label="b" type="f" corresp="#s3:l3">openbaren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het ghen’ dat hem eer doet <rhyme label="a" type="f">bewijsen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Is die hongherighe te <rhyme label="a" type="f" corresp="#s4:l1">spijsen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Met broot // in noot // om soo te <rhyme label="b" type="f">wercken</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Wilt my <rhyme label="b" type="f" corresp="#s4:l3">verstercken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Troulijck den Armen te <rhyme label="a" type="f">bedincken</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">In dorst te laven, hem te <rhyme label="a" type="f" corresp="#s5:l1">schincken</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Met troost // altoost // sijn hert te <rhyme label="b" type="f">laven</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Gheeft my die <rhyme label="b" type="f" corresp="#s5:l3">gaven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Blijd’lijck die gasten te <rhyme label="a" type="f">logieren</rhyme></l>
        <l id="s6:l2" met="4" type="-+">In mijn huys, Heere <rhyme label="a" type="f" corresp="#s6:l1">goedertieren</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Arm, rijck, ghelijck, tot elcker <rhyme label="b" type="f">uren</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Al sonder <rhyme label="b" type="f" corresp="#s6:l3">trueren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">En wilt mijn herte soo <rhyme label="a" type="f">bereyden</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Om nu den naeckten te <rhyme label="a" type="f" corresp="#s7:l1">becleyden</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Met moet // en spoet // hem te <rhyme label="b" type="f">verhoghen</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Nae mijn <rhyme label="b" type="f" corresp="#s7:l3">vermoghen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Noch meer die kranck ligghen in <rhyme label="a" type="f">hoecken</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Gheeft my lief Heere te <rhyme label="a" type="f" corresp="#s8:l1">besoecken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Met raet // en daet // soo’t is <rhyme label="b" type="f">gheleghen</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Haer vriend’lijck <rhyme label="b" type="f" corresp="#s8:l3">pleghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">En stiert mijn herte met <rhyme label="a" type="f">verlanghen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Tot hem die swaerlijck is <rhyme label="a" type="f" corresp="#s9:l1">ghevanghen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat mijn // const fijn // hem mach <rhyme label="b" type="f">verblijden</rhyme></l>
        <l id="s9:l4" met="2" type="-+">In al sijn <rhyme label="b" type="f" corresp="#s9:l3">lijden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dat ick hier bidde iin dit <rhyme label="a" type="f">singhen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Laet aen mijn ziele oock <rhyme label="a" type="f" corresp="#s10:l1">volbringhen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Spijst, drinckt // en schinckjt // haer cleeder <rhyme label="b" type="f">schoone</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Wt uwen <rhyme label="b" type="f" corresp="#s10:l3">Throone</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">In u herberghe wiltse <rhyme label="a" type="f">leyden</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Om d’eeuwighe rust ye <rhyme label="a" type="f" corresp="#s11:l1">verbeyden</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Mijn zeel // gheheel // met desen <rhyme label="b" type="f">saken</rhyme></l>
        <l id="s11:l4" met="2" type="-+">Wilt soo <rhyme label="b" type="f" corresp="#s11:l3">vermaken</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Kranck leyt mijn ziel, wilt tot haer <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s12:l2" met="4" type="-+">O lieve Heere <rhyme label="a" type="f" corresp="#s12:l1">uytghenomen</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Ghesont // terstont // maecktse <rhyme label="b" type="f">gherade</rhyme></l>
        <l id="s12:l4" met="2" type="-+">Door u <rhyme label="b" type="f" corresp="#s12:l3">ghenade</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">T’ghevanckenis wilt Heere <rhyme label="a" type="f">breken</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Daerin mijn ziele schier <rhyme label="a" type="f" corresp="#s13:l1">besweken</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Leyt vast // den last // wilt my <rhyme label="b" type="f">verlichten</rhyme>,</l>
        <l id="s13:l4" met="2" type="-+">Hier uyt <rhyme label="b" type="f" corresp="#s13:l3">oprichten</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Van vreuchden sal mijn hert <rhyme label="a" type="f">opspringen</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Als ick bevind’, lief Heer, dees <rhyme label="a" type="f" corresp="#s14:l1">dinghen</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Voor al // ick sal // u Godt hier <rhyme label="b" type="f">boven</rhyme></l>
        <l id="s14:l4" met="2" type="-+">Dancken en <rhyme label="b" type="f" corresp="#s14:l3">loven</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Soo wil ick desen sanck <rhyme label="a" type="f">besluyten</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Crachtighe Godt, groot van <rhyme label="a" type="f" corresp="#s15:l1">virtuyten</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Staet by // gheeft my // dit al te <rhyme label="b" type="f">samen</rhyme>,</l>
        <l id="s15:l4" met="2" type="-+">Lief Heere, <rhyme label="b" type="f" corresp="#s15:l3">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
