    <melody>Grijp een moed gij edele jongelingen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="occasions">gelegenheden</category>
    </categories>
  <author>G.A. Bredero</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Als d'ooghen <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die myne siel <rhyme label="b" type="f">besworen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Mijn steel-wijs <rhyme label="a" type="m" corresp="#s1:l1">noon</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Met anminnich <rhyme label="b" type="f" corresp="#s1:l2">bekoren</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Soo moet ick <rhyme label="a" type="m" corresp="#s1:l1">doon</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Soo moet ick <rhyme label="a" type="m" corresp="#s1:l1">doon</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Mijn wrevle moet, <rhyme label="b" type="f" corresp="#s1:l2">gheboren</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Wt spijt en <rhyme label="a" type="m" corresp="#s1:l1">hoon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Soo veel <rhyme label="a" type="m">vermach</rhyme></l>
        <l id="s2:l2" met="3" type="-+">De waertste Vrou der <rhyme label="b" type="f">Vrouwen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Dien ick <rhyme label="a" type="m" corresp="#s2:l1">aensach</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met al te groot <rhyme label="b" type="f" corresp="#s2:l2">betrouwen</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">O hart <rhyme label="a" type="m" corresp="#s2:l1">ghelach</rhyme>!</l>
        <l id="s2:l6" met="2" type="-+">O hart <rhyme label="a" type="m" corresp="#s2:l1">ghelach</rhyme>!</l>
        <l id="s2:l7" met="3" type="-+">Vervloeckt, soo moet ick <rhyme label="b" type="f" corresp="#s2:l2">houwen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Die layde <rhyme label="a" type="m" corresp="#s2:l1">dach</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Doen ick <rhyme label="a" type="m">verkoor</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Om eeuwelijck te <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">De glans en <rhyme label="a" type="m" corresp="#s3:l1">gloor</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Der Amstelsche <rhyme label="b" type="f" corresp="#s3:l2">godinnen</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Nu is te <rhyme label="a" type="m" corresp="#s3:l1">loor</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Nu is te <rhyme label="a" type="m" corresp="#s3:l1">loor</rhyme></l>
        <l id="s3:l7" met="3" type="-+">'tGoetduncken van mijn <rhyme label="b" type="f" corresp="#s3:l2">sinnen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Dat mijn quam <rhyme label="a" type="m" corresp="#s3:l1">voor</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Want doen quam <rhyme label="a" type="m">my</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met holle hoopen <rhyme label="b" type="f">asen</rhyme></l>
        <l id="s4:l3" met="2" type="-+"><rhyme label="a" type="m" corresp="#s4:l1">Pluymstrijckery</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Vol wints en <rhyme label="b" type="f" corresp="#s4:l2">opgheblasen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Van <rhyme label="a" type="m" corresp="#s4:l1">Hovaerdy</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Van <rhyme label="a" type="m" corresp="#s4:l1">Hovaerdy</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En licht gheloof, der <rhyme label="b" type="f" corresp="#s4:l2">dwasen</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Aarts <rhyme label="a" type="m" corresp="#s4:l1">Kettery</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Dit is de <rhyme label="a" type="m">seckt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En dwaling van de <rhyme label="b" type="f">wysen</rhyme>:</l>
        <l id="s5:l3" met="2" type="-+">Die 't breyn <rhyme label="a" type="m" corresp="#s5:l1">vergeckt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Van jonghe en van <rhyme label="b" type="f" corresp="#s5:l2">grysen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Dien sy <rhyme label="a" type="m" corresp="#s5:l1">bedeckt</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Dien sy <rhyme label="a" type="m" corresp="#s5:l1">bedeckt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Beveynst, weet aen te <rhyme label="b" type="f" corresp="#s5:l2">prysen</rhyme>.</l>
        <l id="s5:l8" met="2" type="-+">Daar 'thert na <rhyme label="a" type="m" corresp="#s5:l1">treckt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ach <rhyme label="a" type="m">ydelheydt</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">Ach nietich <rhyme label="b" type="f">overmoghen</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Door't <rhyme label="a" type="m" corresp="#s6:l1">onderscheyt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van waerheyt en van <rhyme label="b" type="f" corresp="#s6:l2">loghen</rhyme>:</l>
        <l id="s6:l5" met="2" type="-+">Ben ick <rhyme label="a" type="m" corresp="#s6:l1">bekeyt</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Ben ick <rhyme label="a" type="m" corresp="#s6:l1">bekeyt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">En vind my selfs <rhyme label="b" type="f" corresp="#s6:l2">bedroghen</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Door 't quaet <rhyme label="a" type="m" corresp="#s6:l1">beleyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wel die met <rhyme label="a" type="m">raet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">De saken <rhyme label="b" type="f">overweghen</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">En voor de <rhyme label="a" type="m" corresp="#s7:l1">daet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Afmercken eens te <rhyme label="b" type="f" corresp="#s7:l2">deghen</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Het goet en <rhyme label="a" type="m" corresp="#s7:l1">quaet</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Het goet en <rhyme label="a" type="m" corresp="#s7:l1">quaet</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En watter is <rhyme label="b" type="f" corresp="#s7:l2">gheleghen</rhyme></l>
        <l id="s7:l8" met="2" type="-+">An't schoon <rhyme label="a" type="m" corresp="#s7:l1">ghelaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Dat was dat <rhyme label="a" type="m">mijn</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Soo licht'lijck kon <rhyme label="b" type="f">verleyen</rhyme>:</l>
        <l id="s8:l3" met="2" type="-+">Ach schoone <rhyme label="a" type="m" corresp="#s8:l1">schijn</rhyme>!</l>
        <l id="s8:l4" met="3" type="-+">Ach troetelen! ach <rhyme label="b" type="f" corresp="#s8:l2">vleyen</rhyme>!</l>
        <l id="s8:l5" met="2" type="-+">Ach <rhyme label="a" type="m" corresp="#s8:l1">Maechdekijn</rhyme>!</l>
        <l id="s8:l6" met="2" type="-+">Ach <rhyme label="a" type="m" corresp="#s8:l1">Maechdekijn</rhyme>!</l>
        <l id="s8:l7" met="3" type="-+">Hoe bitter doet ghy <rhyme label="b" type="f" corresp="#s8:l2">scheyen</rhyme></l>
        <l id="s8:l8" met="2" type="-+">Den Dienaer <rhyme label="a" type="m" corresp="#s8:l1">dijn</rhyme>?</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Al waer <rhyme label="a" type="m">verwoedt</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Van nijt mijn hart <rhyme label="b" type="f">ontsteken</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Al soodt mijn <rhyme label="a" type="m" corresp="#s9:l1">bloedt</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Wt lust, om my te <rhyme label="b" type="f" corresp="#s9:l2">wreken</rhyme>:</l>
        <l id="s9:l5" met="2" type="-+">Een lonckje <rhyme label="a" type="m" corresp="#s9:l1">soet</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Een lonckje <rhyme label="a" type="m" corresp="#s9:l1">soet</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Sou al de gramschap <rhyme label="b" type="f" corresp="#s9:l2">breken</rhyme></l>
        <l id="s9:l8" met="2" type="-+">Van mijn <rhyme label="a" type="m" corresp="#s9:l1">ghemoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
