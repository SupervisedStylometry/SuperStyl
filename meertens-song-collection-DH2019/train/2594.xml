      <category translation="love and sex">liefde en seks</category>
      <category translation="emotions">emoties</category>
    </categories>
  <author>G.A. Bredero</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">O soete tyt ay schoone Soomer <rhyme label="a" type="f">vlaaghen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe haast verkeert u <rhyme label="b" type="m">blat</rhyme>?</l>
        <l id="s1:l3" met="5" type="-+">Want met jolyt leefden ick eens dry <rhyme label="a" type="f" corresp="#s1:l1">daaghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat ick mijn lusten <rhyme label="b" type="m" corresp="#s1:l2">hadt</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Doen ick by haar <rhyme label="b" type="m" corresp="#s1:l2">sadt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Maar laas, t'is te <rhyme label="a" type="f" corresp="#s1:l1">beclaaghen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ghy reeckent nu mijn <rhyme label="b" type="m" corresp="#s1:l2">dat</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Soet wel voor duysent <rhyme label="a" type="f" corresp="#s1:l1">plaaghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Mijn sinnen vyf souden niet connen <rhyme label="a" type="f">vinden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Gheen liever tijdt noch <rhyme label="b" type="m">stont</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Of tyt verdrijf als doen mijn <rhyme label="a" type="f" corresp="#s2:l1">beminde</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Haar jonst my heeft <rhyme label="b" type="m" corresp="#s2:l2">ghejont</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Wt een goede <rhyme label="b" type="m" corresp="#s2:l2">gront</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Docht ick sy my <rhyme label="a" type="f" corresp="#s2:l1">besinde</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Nu thoonse haar goet <rhyme label="b" type="m" corresp="#s2:l2">ront</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Of sy my niet en <rhyme label="a" type="f" corresp="#s2:l1">kinde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">t'Gesicht vant Bos of wel geswierde <rhyme label="a" type="f">Mayen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Daart vlietend' Water <rhyme label="b" type="m">ruyst</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Geen wilde Mos of aenghenaam <rhyme label="a" type="f" corresp="#s3:l1">vallayen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van Berghen <rhyme label="b" type="m" corresp="#s3:l2">onbesuyst</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Is niet ick ben <rhyme label="b" type="m" corresp="#s3:l2">verkuyst</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">t'Ooch na haar te <rhyme label="a" type="f" corresp="#s3:l1">drayen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Die my met weenen <rhyme label="b" type="m" corresp="#s3:l2">kruyst</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Doch als sy wilt kan <rhyme label="a" type="f" corresp="#s3:l1">payen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Geen woest ghetier, van blye <rhyme label="a" type="f">Vogelskeelen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hoe vrolijck wel <rhyme label="b" type="m">ghebeckt</rhyme></l>
        <l id="s4:l3" met="5" type="-+">Jaa Harp of Lier, Bondol Luyt noch <rhyme label="a" type="f" corresp="#s4:l1">Veelen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hoe wel ghespeelt <rhyme label="b" type="m" corresp="#s4:l2">coreckt</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">My sulcken Vreucht <rhyme label="b" type="m" corresp="#s4:l2">verweckt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Als dit vermakelijck <rhyme label="a" type="f" corresp="#s4:l1">queelen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat meer reeden <rhyme label="b" type="m" corresp="#s4:l2">ontdeckt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Als het ghewayt en <rhyme label="a" type="f" corresp="#s4:l1">speelen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Wat reucken secht is t'liefelycxt en hoe <rhyme label="a" type="f">men</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die aygentlijck <rhyme label="b" type="m">beduyt</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Ist wel gevlecht een crans van schoone <rhyme label="a" type="f" corresp="#s5:l1">bloemen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ghesochte ruyckers <rhyme label="b" type="m" corresp="#s5:l2">uyt</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Muskus Noot of <rhyme label="b" type="m" corresp="#s5:l2">Cruyt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Is soo waart niet om <rhyme label="a" type="f" corresp="#s5:l1">roemen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Als d'asem van een <rhyme label="b" type="m" corresp="#s5:l2">spruyt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die ick niet darf <rhyme label="a" type="f" corresp="#s5:l1">noemen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Geen wyn hoe eel, geen leckere <rhyme label="a" type="f">bancquetten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Hoe groot van <rhyme label="b" type="m">overdaat</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Die Dienaers veel ordentelijck op <rhyme label="a" type="f" corresp="#s6:l1">setten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Gout Silver tot <rhyme label="b" type="m" corresp="#s6:l2">cieraet</rhyme></l>
        <l id="s6:l5" met="3" type="+-">'tGraach begheer <rhyme label="b" type="m" corresp="#s6:l2">versaat</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Van d'inwendighe <rhyme label="a" type="f" corresp="#s6:l1">hetten</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Als haar goeden <rhyme label="b" type="m" corresp="#s6:l2">raat</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Voor dees strenghe <rhyme label="a" type="f" corresp="#s6:l1">wetten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Wie sou hier voor onder 'tVee willen <rhyme label="a" type="f">woelen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hoe sacht van Wol of <rhyme label="b" type="m">Pluym</rhyme></l>
        <l id="s7:l3" met="5" type="-+">Had' ick mijn keur ick koos schoone <rhyme label="a" type="f" corresp="#s7:l1">boelen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">In plaats van sulck <rhyme label="b" type="m" corresp="#s7:l2">schuym</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Die naar <rhyme label="b" type="m" corresp="#s7:l2">kostuym</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Ons haer sachtheyt doen <rhyme label="a" type="f" corresp="#s7:l1">voelen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Met soete kusjens <rhyme label="b" type="m" corresp="#s7:l2">ruym</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Die nochtans niet en <rhyme label="a" type="f" corresp="#s7:l1">koelen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Prinses ick Sie u over schoone <rhyme label="a" type="f">gaven</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ick Hoor u grooten <rhyme label="b" type="m">Lof</rhyme>,</l>
        <l id="s8:l3" met="5" type="-+">Ick Smaack troost die my een weynig can <rhyme label="a" type="f" corresp="#s8:l1">laven</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ick Ruyck u Deuchtlijck <rhyme label="b" type="m" corresp="#s8:l2">Hoff</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Ick Voel slechts en <rhyme label="b" type="m" corresp="#s8:l2">groff</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Mijn eyghen kleynheyt <rhyme label="a" type="f" corresp="#s8:l1">draven</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Al sterft ghy mijn <rhyme label="b" type="m" corresp="#s8:l2">off</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">'tHart leyt by u <rhyme label="a" type="f" corresp="#s8:l1">begraaven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
