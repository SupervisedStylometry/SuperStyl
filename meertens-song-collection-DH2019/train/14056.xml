    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Het gouddradige haar</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Jan Jansz. Starter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">'t Gouddradigh <rhyme label="a" type="f">hayr</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Dat dees Goddin om 't hoofd <rhyme label="b" type="f">sweefd</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Phoebo <rhyme label="a" type="f" corresp="#s1:l1">voorwaer</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Wat van syn glans <rhyme label="c" type="m">ontrooft</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">De stralen <rhyme label="d" type="f">prachtigh</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Die hy uyt zijn tent <rhyme label="e" type="f">schiet</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Syn voorwaer so <rhyme label="d" type="f" corresp="#s1:l5">krachtigh</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Nergens by <rhyme label="e" type="f" corresp="#s1:l6">omtrent</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">(so ick blijcklijck <rhyme label="f" type="m">sie</rhyme>)</l>
        <l id="s1:l10" met="3" type="+-">Als de stralen, <rhyme label="f" type="m" corresp="#s1:l9">die</rhyme></l>
        <l id="s1:l11" met="5" type="+-">Sy uyt haer bruyn-oogjens schieten <rhyme label="g" type="m">kan</rhyme>;</l>
        <l id="s1:l12" met="3" type="+-">Want sy 't hart daer <rhyme label="h" type="m">met</rhyme>,</l>
        <l id="s1:l13" met="3" type="+-">Tegens al 't <rhyme label="h" type="m" corresp="#s1:l12">verset</rhyme>,</l>
        <l id="s1:l14" met="5" type="+-">Sou doorwonden vanden kloecksten <rhyme label="g" type="m" corresp="#s1:l11">Man</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Met twee roode <rhyme label="a" type="f">bloosjes</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Zijn ver√ßierd haer <rhyme label="b" type="f">wangen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Recht gelijck twee <rhyme label="a" type="f" corresp="#s2:l1">roosjes</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Die de douw <rhyme label="c" type="m">bevangt</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Haer Yvore <rhyme label="d" type="f">tanden</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Dicht by een <rhyme label="e" type="f">gedrongen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Schynen silvre <rhyme label="d" type="f" corresp="#s2:l5">randen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Om haer roode <rhyme label="e" type="f" corresp="#s2:l6">tong</rhyme>.</l>
        <l id="s2:l9" met="3" type="+-">Haer gedraeyden <rhyme label="f" type="m">hals</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Is veel blancker <rhyme label="f" type="m" corresp="#s2:l9">als</rhyme></l>
        <l id="s2:l11" met="5" type="+-">Hagel, Sneeuw, Albaster, of <rhyme label="g" type="m">Yvoor</rhyme>,</l>
        <l id="s2:l12" met="3" type="+-">Met een held're <rhyme label="h" type="m">glans</rhyme></l>
        <l id="s2:l13" met="3" type="+-">Om end' om de <rhyme label="h" type="m" corresp="#s2:l12">krans</rhyme></l>
        <l id="s2:l14" met="5" type="+-">Schynen daer de blauwe ad'ren <rhyme label="g" type="m" corresp="#s2:l11">door</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Bruygom ghy mooght <rhyme label="a" type="f">iuygen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Die van hare <rhyme label="b" type="f">lippen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Sult de Nectar <rhyme label="a" type="f" corresp="#s3:l1">suygen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Die den Go'on <rhyme label="c" type="m">ontslipt</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Iae in gulden <rhyme label="d" type="f">Iaren</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Sult door haere <rhyme label="e" type="f">deughden</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Grysen sien u <rhyme label="d" type="f" corresp="#s3:l5">hayren</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Opgehoopt in <rhyme label="e" type="f" corresp="#s3:l6">vreughd</rhyme>.</l>
        <l id="s3:l9" met="3" type="+-">Dies segh ick: dat <rhyme label="f" type="m">ghy</rhyme></l>
        <l id="s3:l10" met="3" type="+-">U meughd roemen <rhyme label="f" type="m" corresp="#s3:l9">vry</rhyme>,</l>
        <l id="s3:l11" met="5" type="+-">Tot u min te hebben nu <rhyme label="g" type="m">bekoord</rhyme></l>
        <l id="s3:l12" met="3" type="+-">De Godinne, <rhyme label="h" type="m">die</rhyme></l>
        <l id="s3:l13" met="3" type="+-">D'eere voor de <rhyme label="h" type="m" corresp="#s3:l12">drie</rhyme></l>
        <l id="s3:l14" met="5" type="+-">Hemelsche Godinnen <rhyme label="g" type="m" corresp="#s3:l11">toebehoord</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
