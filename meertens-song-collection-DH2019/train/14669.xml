<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14669</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter van Gelre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">HEer wilt ons <rhyme label="a" type="f">ontfermen</rhyme>!</l>
        <l id="s1:l2" met="3" type="+-">Christi in u <rhyme label="a" type="f" corresp="#s1:l1">armen</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Wilt ons doch <rhyme label="a" type="f" corresp="#s1:l1">beschermen</rhyme>.</l>
        <l id="s1:l4" met="3" type="+-">Heyl'ghen Gheest verlicht // <rhyme label="b" type="f">ons</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Eene Godt <rhyme label="a" type="f">drievuldigh</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Die den mensch <rhyme label="a" type="f" corresp="#s2:l1">sorchvuldich</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Steedts bewaert <rhyme label="a" type="f" corresp="#s2:l1">geduldich</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Tot u Tempel sticht // <rhyme label="b" type="f">ons</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Iesu Godes <rhyme label="a" type="f">Sone</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">'s Vaders aerdtsche <rhyme label="a" type="f" corresp="#s3:l1">throone</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Die by ons quaemt <rhyme label="a" type="f" corresp="#s3:l1">woone</rhyme>.</l>
        <l id="s3:l4" met="3" type="+-">Onser wilt <rhyme label="b" type="f">ontfermen</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Iesu <rhyme label="a" type="f">furieuste</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Bly <rhyme label="a" type="f" corresp="#s4:l1">victorieuste</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">Alder <rhyme label="a" type="f" corresp="#s4:l1">glorieuste</rhyme>.</l>
        <l id="s4:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Iesu <rhyme label="a" type="f">wonderlijckste</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Eeuwich <rhyme label="a" type="f" corresp="#s5:l1">alderrijckste</rhyme>.</l>
        <l id="s5:l3" met="3" type="+-">En <rhyme label="a" type="f" corresp="#s5:l1">vermaeckelijckste</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Iesu <rhyme label="a" type="f">alderbloedichst'</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Ootmoedich, en <rhyme label="a" type="f" corresp="#s6:l1">goedichst'</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Heer <rhyme label="a" type="f" corresp="#s6:l1">aldersachtmoedichst'</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Iesu, ons <rhyme label="a" type="f">voornaemste</rhyme>.</l>
        <l id="s7:l2" met="3" type="+-">Tot de liefd' <rhyme label="a" type="f" corresp="#s7:l1">bequaemste</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-"><rhyme label="a" type="f" corresp="#s7:l1">Alderghehoorsaemste</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Iesu langh <rhyme label="a" type="f">verduldichst'</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Tot de doodt <rhyme label="a" type="f" corresp="#s8:l1">onschuldichst'</rhyme>.</l>
        <l id="s8:l3" met="3" type="+-"><rhyme label="a" type="f" corresp="#s8:l1">Aldersorchvuldichst'</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Klaer-blinckende <rhyme label="a" type="f">Sterre</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Iesu, die van <rhyme label="a" type="f" corresp="#s9:l1">verre</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Ons leyt uyt <rhyme label="a" type="f" corresp="#s9:l1">'tverwerre</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Liefd' en vrede <rhyme label="a" type="f">kusser</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Iesu tweedrachts <rhyme label="a" type="f" corresp="#s10:l1">susser</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Helsche vlamme <rhyme label="a" type="f" corresp="#s10:l1">blusser</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Voorbeelt aller <rhyme label="a" type="f">deughden</rhyme>,</l>
        <l id="s11:l2" met="3" type="+-">Iesu, die met <rhyme label="a" type="f" corresp="#s11:l1">vreughden</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">Ons, door u <rhyme label="a" type="f" corresp="#s11:l1">verheughden</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Toevlucht der <rhyme label="a" type="f">bedruckten</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">Iesu, die ons <rhyme label="a" type="f" corresp="#s12:l1">ruckten</rhyme></l>
        <l id="s12:l3" met="3" type="+-">Wt, dat ons <rhyme label="a" type="f" corresp="#s12:l1">misluckten</rhyme>.</l>
        <l id="s12:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">Trouwe Herder <rhyme label="a" type="f">goedich</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">Iesu, die <rhyme label="a" type="f" corresp="#s13:l1">kloeckmoedich</rhyme>,</l>
        <l id="s13:l3" met="3" type="+-">Keert des Wolfs muyl <rhyme label="a" type="f" corresp="#s13:l1">bloedich</rhyme>,</l>
        <l id="s13:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="+-">'s Vaders eeuwich <rhyme label="a" type="f">wijsheyt</rhyme>.</l>
        <l id="s14:l2" met="3" type="+-">Iesu, die <rhyme label="a" type="f" corresp="#s14:l1">d'afgrijsheyt</rhyme>,</l>
        <l id="s14:l3" met="3" type="+-">Van ons siel, met prijs <rhyme label="a" type="f" corresp="#s14:l1">leyt</rhyme>,</l>
        <l id="s14:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="+-">Vreucht, in d'Engelen <rhyme label="a" type="f">wooningh</rhyme></l>
        <l id="s15:l2" met="3" type="+-">Patriarchen <rhyme label="a" type="f" corresp="#s15:l1">Coningh</rhyme></l>
        <l id="s15:l3" met="3" type="+-">Klaer Propheets <rhyme label="a" type="f" corresp="#s15:l1">vertooningh</rhyme></l>
        <l id="s15:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="+-">Meester die noyt <rhyme label="a" type="f">miste</rhyme>,</l>
        <l id="s16:l2" met="3" type="+-">Iesu, boeck, schat <rhyme label="a" type="f" corresp="#s16:l1">kiste</rhyme></l>
        <l id="s16:l3" met="3" type="+-">Der <rhyme label="a" type="f" corresp="#s16:l1">Euangelisten</rhyme>,</l>
        <l id="s16:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="+-">Der Astost'len <rhyme label="a" type="f">wijser</rhyme>,</l>
        <l id="s17:l2" met="3" type="+-">Iesu, deuchden <rhyme label="a" type="f" corresp="#s17:l1">prijser</rhyme>,</l>
        <l id="s17:l3" met="3" type="+-">Vuyle sonds <rhyme label="a" type="f" corresp="#s17:l1">afgrijser</rhyme>,</l>
        <l id="s17:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="+-">Tracht der <rhyme label="a" type="f">martelaren</rhyme>,</l>
        <l id="s18:l2" met="3" type="+-">Iesu, als veel <rhyme label="a" type="f" corresp="#s18:l1">scharen</rhyme></l>
        <l id="s18:l3" met="3" type="+-">haer ter doodt <rhyme label="a" type="f" corresp="#s18:l1">beswaren</rhyme>,</l>
        <l id="s18:l4" met="3" type="+-"><rhyme label="b" type="f">Onser</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="+-">Licht der <rhyme label="a" type="f">Confessoren</rhyme></l>
        <l id="s19:l2" met="3" type="+-">Iesu, als sy <rhyme label="a" type="f" corresp="#s19:l1">hooren</rhyme>,</l>
        <l id="s19:l3" met="3" type="+-">Dat haer mocht <rhyme label="a" type="f" corresp="#s19:l1">bekooren</rhyme>,</l>
        <l id="s19:l4" met="3" type="+-">Onser <rhyme label="b" type="f">wilt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="+-">Heyl'ghe Troone <rhyme label="a" type="f">Iesu</rhyme>!</l>
        <l id="s20:l2" met="3" type="+-">Alle die met vrees // <rhyme label="a" type="f" corresp="#s20:l1">u</rhyme></l>
        <l id="s20:l3" met="3" type="+-">Hier aenroepen; dees // <rhyme label="a" type="f" corresp="#s20:l1">u</rhyme></l>
        <l id="s20:l4" met="3" type="+-">Wilt ghenadich, <rhyme label="b" type="f">sparen</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="+-">Ons van alle <rhyme label="a" type="f">quaeden</rhyme></l>
        <l id="s21:l2" met="3" type="+-">Sondaers boos <rhyme label="a" type="f" corresp="#s21:l1">aenraden</rhyme></l>
        <l id="s21:l3" met="3" type="+-">En van vuyle <rhyme label="a" type="f" corresp="#s21:l1">daden</rhyme>,</l>
        <l id="s21:l4" met="3" type="+-">Wilt ghenadich <rhyme label="b" type="f">sparen</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="+-">'Tvrees'lijck <rhyme label="a" type="f">overtreden</rhyme>,</l>
        <l id="s22:l2" met="3" type="+-">Van de heyl'ghe <rhyme label="a" type="f" corresp="#s22:l1">reden</rhyme></l>
        <l id="s22:l3" met="3" type="+-">Vw's gheboden <rhyme label="a" type="f" corresp="#s22:l1">mede</rhyme>,</l>
        <l id="s22:l4" met="3" type="+-">Soo verlost ons <rhyme label="b" type="f">Iesu</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="+-">Van 't onsalich <rhyme label="a" type="f">sterven</rhyme></l>
        <l id="s23:l2" met="3" type="+-">En het eeuwich <rhyme label="a" type="f" corresp="#s23:l1">derven</rhyme>,</l>
        <l id="s23:l3" met="3" type="+-">Van u Hemels <rhyme label="a" type="f" corresp="#s23:l1">erve</rhyme>,</l>
        <l id="s23:l4" met="3" type="+-">Soo verlost ons <rhyme label="b" type="f">Iesu</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="+-">Door u neder <rhyme label="a" type="f">dale</rhyme>,</l>
        <l id="s24:l2" met="3" type="+-">In een stals stroo <rhyme label="a" type="f" corresp="#s24:l1">sale</rhyme>,</l>
        <l id="s24:l3" met="3" type="+-">Achtste daeghs naem <rhyme label="a" type="f" corresp="#s24:l1">hale</rhyme>.</l>
        <l id="s24:l4" met="3" type="+-">Soo <rhyme label="b" type="f">verlost</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="+-">Door u geessel <rhyme label="a" type="f">roeden</rhyme>:</l>
        <l id="s25:l2" met="3" type="+-">'Tkroonen vande <rhyme label="a" type="f" corresp="#s25:l1">woeden</rhyme>:</l>
        <l id="s25:l3" met="3" type="+-">En het deerlijck <rhyme label="a" type="f" corresp="#s25:l1">bloeden</rhyme>,</l>
        <l id="s25:l4" met="3" type="+-">Soo <rhyme label="b" type="f">verlost</rhyme>,</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="+-">Door u bitter <rhyme label="a" type="f">dooden</rhyme>:</l>
        <l id="s26:l2" met="3" type="+-">Helsche macht uyt <rhyme label="a" type="f" corresp="#s26:l1">roden</rhyme>,</l>
        <l id="s26:l3" met="3" type="+-">Opstaen, hemels <rhyme label="a" type="f" corresp="#s26:l1">nooden</rhyme>,</l>
        <l id="s26:l4" met="3" type="+-">Soo <rhyme label="b" type="f">verlost</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="3" type="+-">Lam dat onse <rhyme label="a" type="f">sonden</rhyme>,</l>
        <l id="s27:l2" met="3" type="+-">Inde aertsche <rhyme label="a" type="f" corresp="#s27:l1">monden</rhyme>,</l>
        <l id="s27:l3" met="3" type="+-">Weder hebt <rhyme label="a" type="f" corresp="#s27:l1">ghebonden</rhyme>,</l>
        <l id="s27:l4" met="3" type="+-">Spaert ons siele, <rhyme label="b" type="f">Iesu</rhyme>!</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="3" type="+-">Lam dat onse <rhyme label="a" type="f">packen</rhyme></l>
        <l id="s28:l2" met="3" type="+-">Inde Dooren <rhyme label="a" type="f" corresp="#s28:l1">tacken</rhyme></l>
        <l id="s28:l3" met="3" type="+-">Van u hooft liet <rhyme label="a" type="f" corresp="#s28:l1">sacken</rhyme>.</l>
        <l id="s28:l4" met="3" type="+-">Wilt ons doch <rhyme label="b" type="f">verhooren</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="3" type="+-">Lam dat onse <rhyme label="a" type="f">smetten</rhyme>,</l>
        <l id="s29:l2" met="3" type="+-">Aen u cruys liet <rhyme label="a" type="f" corresp="#s29:l1">setten</rhyme>,</l>
        <l id="s29:l3" met="3" type="+-">Om ons niet te <rhyme label="a" type="f" corresp="#s29:l1">letten</rhyme>.</l>
        <l id="s29:l4" met="3" type="+-">Wilt ons voort <rhyme label="b" type="f">ontfermen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
