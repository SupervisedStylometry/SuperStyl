  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh Heer v moet ick <rhyme label="a" type="f">klaghen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tghene dat ick sie <rhyme label="b" type="f">ghebueren</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Hoe men in dese <rhyme label="a" type="f" corresp="#s1:l1">daghen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dijn schaepkens siet <rhyme label="b" type="f" corresp="#s1:l2">verschueren</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Door Spaus <rhyme label="b" type="f" corresp="#s1:l2">errueren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">So loopen sy <rhyme label="c" type="m">verdwaelt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Haer herders die haer <rhyme label="b" type="f" corresp="#s1:l2">stueren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Hebben den wech <rhyme label="c" type="m" corresp="#s1:l6">ghefalt</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s1:l9">schapen</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s1:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">So sy in Daniels <rhyme label="a" type="f">tijden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Uersierden grouwel <rhyme label="b" type="f">leerden</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Door menschen <rhyme label="a" type="f" corresp="#s2:l1">fantasien</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Het simpel volck <rhyme label="b" type="f" corresp="#s2:l2">verkeerden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">So sietmen ons <rhyme label="b" type="f" corresp="#s2:l2">gheleerden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dat selue werck <rhyme label="c" type="m">bestaen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Maer so sy doen <rhyme label="b" type="f" corresp="#s2:l2">vermeerden</rhyme></l>
        <l id="s2:l8" met="3" type="-+">So salter noch <rhyme label="c" type="m" corresp="#s2:l6">vergaen</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s2:l9">schapen</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s2:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy maken haer <rhyme label="a" type="f">afgoden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die doen sy tvolck <rhyme label="b" type="f">aenbeden</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dwelck God klaer heeft <rhyme label="a" type="f" corresp="#s3:l1">verboden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Int woort na Moses <rhyme label="b" type="f" corresp="#s3:l2">reden</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Sy bruycken vreemde <rhyme label="b" type="f" corresp="#s3:l2">seden</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Rechts na haer droomen <rhyme label="c" type="m">vrij</rhyme></l>
        <l id="s3:l7" met="3" type="-+">So haer voorvaders <rhyme label="b" type="f" corresp="#s3:l2">deden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Twoort Gods verlaten <rhyme label="c" type="m" corresp="#s3:l6">sy</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s3:l9">schapen</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s3:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sy drijuen tvolck met <rhyme label="a" type="f">hoopen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Op eyghen duechtsaem <rhyme label="b" type="f">wercken</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En stellen te <rhyme label="a" type="f" corresp="#s4:l1">verkoopen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den hemel so wy <rhyme label="b" type="f" corresp="#s4:l2">mercken</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dit zijn Spaus <rhyme label="b" type="f" corresp="#s4:l2">klercken</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Die tvolck so leyden <rhyme label="c" type="m">blent</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En Christus dienstich <rhyme label="b" type="f" corresp="#s4:l2">stercken</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Wort onder haer <rhyme label="c" type="m" corresp="#s4:l6">gheschent</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s4:l9">schapen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s4:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Lof God tis beter <rhyme label="a" type="f">komen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dwoort kan haer wel <rhyme label="b" type="f">regieren</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Haer doodelicke <rhyme label="a" type="f" corresp="#s5:l1">verdomen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat sietmen gantsch <rhyme label="b" type="f" corresp="#s5:l2">mineren</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Nv is haer <rhyme label="b" type="f" corresp="#s5:l2">tlamenteren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En ons door Christum <rhyme label="c" type="m">vruecht</rhyme></l>
        <l id="s5:l7" met="3" type="-+">O kinders wilt <rhyme label="b" type="f" corresp="#s5:l2">gaudieren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Int lustich licht <rhyme label="c" type="m" corresp="#s5:l6">verhuecht</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s5:l9">schapen</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s5:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Al haer vermoort <rhyme label="a" type="f">versieren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Is nv int <rhyme label="b" type="f">openbare</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Den Prince der <rhyme label="a" type="f" corresp="#s6:l1">Princhieren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Stelt haer boeuerie int <rhyme label="b" type="f" corresp="#s6:l2">klare</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Haer dierbaer <rhyme label="b" type="f" corresp="#s6:l2">ware</rhyme></l>
        <l id="s6:l6" met="3" type="-+">En wert niet meer <rhyme label="c" type="m">ghesocht</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Coemt wech vliet Babels <rhyme label="b" type="f" corresp="#s6:l2">schare</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Trecht is ons <rhyme label="c" type="m" corresp="#s6:l6">wederbrocht</rhyme></l>
        <l id="s6:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s6:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s6:l9">schapen</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s6:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wee v ghy blinde <rhyme label="a" type="f">ghuylen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Baals valsche <rhyme label="b" type="f">Propheten</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Waer onder wilt ghy <rhyme label="a" type="f" corresp="#s7:l1">schuylen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dijn decksel wert <rhyme label="b" type="f" corresp="#s7:l2">versleten</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Dijn mate is v <rhyme label="b" type="f" corresp="#s7:l2">ghemeten</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Ghetelt wort uwen <rhyme label="c" type="m">loon</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Gods woort heeft by na <rhyme label="b" type="f" corresp="#s7:l2">gheten</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Tsyraet van Babels <rhyme label="c" type="m" corresp="#s7:l6">kroon</rhyme></l>
        <l id="s7:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s7:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s7:l9">schapen</rhyme></l>
        <l id="s7:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s7:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Stelt v sduyuels <rhyme label="a" type="f">practijcken</rhyme></l>
        <l id="s8:l2" met="3" type="-+">By dwoort der waerheyt <rhyme label="b" type="f">reene</rhyme></l>
        <l id="s8:l3" met="3" type="-+">En siet of sy <rhyme label="a" type="f" corresp="#s8:l1">ghelijcken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Oordeelt na dwoort <rhyme label="b" type="f" corresp="#s8:l2">alleene</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Neen, daer sydy te <rhyme label="b" type="f" corresp="#s8:l2">kleene</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Twort goet ghemaect met <rhyme label="c" type="m">kracht</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Dus loopt ghy dan als <rhyme label="b" type="f" corresp="#s8:l2">eene</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Moetwillich in de <rhyme label="c" type="m" corresp="#s8:l6">gracht</rhyme></l>
        <l id="s8:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s8:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s8:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s8:l9">schapen</rhyme></l>
        <l id="s8:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s8:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ueel meer had ick <rhyme label="a" type="f">gheschreuen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Uan haer ghedroomde <rhyme label="b" type="f">loghen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Maer elck heeft wel <rhyme label="a" type="f" corresp="#s9:l1">beseuen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Hoe dat sy weynich <rhyme label="b" type="f" corresp="#s9:l2">doghen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Schout nv na dijn <rhyme label="b" type="f" corresp="#s9:l2">vemoghen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Tfenijn bout op Gods <rhyme label="c" type="m">woort</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Want dwoort, en Babels <rhyme label="b" type="f" corresp="#s9:l2">loghen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">En draghens gheen <rhyme label="c" type="m" corresp="#s9:l6">accoort</rhyme></l>
        <l id="s9:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s9:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s9:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s9:l9">schapen</rhyme></l>
        <l id="s9:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s9:l10">voen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Prince die so <rhyme label="a" type="f">gheuoelen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">En dwoort der waerheyt <rhyme label="b" type="f">prijsen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Wilt haren dreck <rhyme label="a" type="f" corresp="#s10:l1">afspoelen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Laet v van Christo <rhyme label="b" type="f" corresp="#s10:l2">wijsen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Op dat ghy int <rhyme label="b" type="f" corresp="#s10:l2">verrijsen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Met Christo <rhyme label="c" type="m">onbeuleckt</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Eewich moecht <rhyme label="b" type="f" corresp="#s10:l2">veriolijsen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">In heerlickheyt <rhyme label="c" type="m" corresp="#s10:l6">prefect</rhyme></l>
        <l id="s10:l9" met="2" type="-+">Uliet Baals <rhyme label="d" type="f">papen</rhyme></l>
        <l id="s10:l10" met="3" type="-+">Babels <rhyme label="e" type="m">religioen</rhyme></l>
        <l id="s10:l11" met="3" type="+-">Die Christus arme <rhyme label="d" type="f" corresp="#s10:l9">schapen</rhyme></l>
        <l id="s10:l12" met="3" type="-+">Met dreck van menschen <rhyme label="e" type="m" corresp="#s10:l10">voen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
