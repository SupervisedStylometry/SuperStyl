  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HAer Oogjes blinken als de <rhyme label="a" type="m">Son</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ik hiet die schoonste <rhyme label="a" type="m" corresp="#s1:l1">wellekom</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Haer Borsje rond, met Melk en <rhyme label="b" type="m">hoon</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Twee tepeltjes kouralen <rhyme label="b" type="m" corresp="#s1:l3">Rood</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Haer Tanden witter als <rhyme label="a" type="m">Yvoor</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Haer halsje draegt een Parle <rhyme label="a" type="m" corresp="#s2:l1">snoer</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Haer voeten teer, maer vlug en <rhyme label="b" type="m">snel</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die schoonste die behaegt my <rhyme label="b" type="m" corresp="#s2:l3">wel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Haer Roosen rood kouralen <rhyme label="a" type="m">Mond</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Als ik die kus maekt mijn <rhyme label="a" type="m" corresp="#s3:l1">gezond</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Haer handen zagter als <rhyme label="b" type="m">Fluweel</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Myn alderschoonste <rhyme label="b" type="m" corresp="#s3:l3">Pronk-Juweel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer ach mijn zugten is om <rhyme label="a" type="m">niet</rhyme>!</l>
        <l id="s4:l2" met="4" type="-+">Die schoonste acht mijn klagten <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Zy vlugt voor mijn het Boswaerd <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Mijn alderschoonste <rhyme label="b" type="m" corresp="#s4:l3">ziels-vriendin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ik volg haer op staende <rhyme label="a" type="m">voet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ik voel ontsteken mijn <rhyme label="a" type="m" corresp="#s5:l1">minne-gloet</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Daer zit zy aen een klaer <rhyme label="b" type="m">fontein</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Al met haer Schaepjes soo <rhyme label="b" type="m" corresp="#s5:l3">allein</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ik groet u overschoon <rhyme label="a" type="m">godin</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ey toond u Minnaer <rhyme label="a" type="m" corresp="#s6:l1">weder-min</rhyme>!</l>
        <l id="s6:l3" met="4" type="-+">Daer is op Trouw een <rhyme label="b" type="m">Diamant</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Kom rykt mijn maer u <rhyme label="b" type="m" corresp="#s6:l3">Regterhand</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ik neem die Trouw van u dan <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Wy zullen zamen ter Bruiloft <rhyme label="a" type="m" corresp="#s7:l1">gaen</rhyme>;</l>
        <l id="s7:l3" met="4" type="-+">En dat al by die Herders <rhyme label="b" type="m">zael</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Hier in dit geurig groene <rhyme label="b" type="m" corresp="#s7:l3">dael</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
