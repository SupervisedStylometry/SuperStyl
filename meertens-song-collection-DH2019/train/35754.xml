<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35754</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Wees nimf gegroet</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">VAn hare <rhyme label="a" type="m">jeught</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Sint Anna de <rhyme label="a" type="m" corresp="#s1:l1">deught</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Heeft hooger geacht, en dieper <rhyme label="b" type="m">bemint</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Dan al dat men in de werelt <rhyme label="b" type="m" corresp="#s1:l3">vint</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Haer nedrigh <rhyme label="c" type="m">kleedt</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Niet onnut <rhyme label="c" type="m" corresp="#s1:l5">besteedt</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Al is sy gesproten uyt Conincx <rhyme label="d" type="m">bloet</rhyme>;</l>
        <l id="s1:l8" met="4" type="-+">Geen hoogheydt laet komen in 't <rhyme label="d" type="m" corresp="#s1:l7">gemoet</rhyme>.</l>
        <l id="s1:l9" met="2" type="-+">De menschen quaedt <rhyme label="e" type="f">vluchten</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Met al dat is <rhyme label="f" type="m">quaedt</rhyme>;</l>
        <l id="s1:l11" met="2" type="+-">Al 's werelts <rhyme label="e" type="f" corresp="#s1:l9">genuchten</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Met d'ydelheydt <rhyme label="f" type="m" corresp="#s1:l10">haet</rhyme>.</l>
        <l id="s1:l13" met="3" type="-+">Met 't hart, ziel en <rhyme label="g" type="f">zinnen</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Betrachte Goods <rhyme label="h" type="m">eer</rhyme>;</l>
        <l id="s1:l15" met="2" type="-+">'t Goddelijck van <rhyme label="g" type="f" corresp="#s1:l13">binnen</rhyme></l>
        <l id="s1:l16" met="2" type="-+">Plaets gaf, en niet <rhyme label="h" type="m" corresp="#s1:l14">meer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Om Lant, noch <rhyme label="a" type="m">gout</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Sint Ioachim <rhyme label="a" type="m" corresp="#s2:l1">Trouwt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Noch om de quade lusten van het <rhyme label="b" type="m">vleysch</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Maer tot Godts eer, na d' oude Wets <rhyme label="b" type="m" corresp="#s2:l3">eysch</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">In dese <rhyme label="c" type="m">staet</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Geen deughden <rhyme label="c" type="m" corresp="#s2:l5">verlaet</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Sy onderhouden Godts Wetten <rhyme label="d" type="m">geheel</rhyme>.</l>
        <l id="s2:l8" met="4" type="-+">Sy met haer man leeft sonder <rhyme label="d" type="m" corresp="#s2:l7">verscheel</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">D' Heylighste der <rhyme label="e" type="f">aerde</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Was in hare <rhyme label="f" type="m">tijdt</rhyme>.</l>
        <l id="s2:l11" met="2" type="+-">Nedrigheydt <rhyme label="e" type="f" corresp="#s2:l9">bewaerde</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">Sy raeckt die noyt <rhyme label="f" type="m" corresp="#s2:l10">quijt</rhyme>.</l>
        <l id="s2:l13" met="3" type="-+">De'es Sonne-bloem <rhyme label="g" type="f">voeghden</rhyme>,</l>
        <l id="s2:l14" met="3" type="-+">Drayde, na Goodts <rhyme label="h" type="m">wil</rhyme>;</l>
        <l id="s2:l15" met="2" type="-+">Godts wercken <rhyme label="g" type="f" corresp="#s2:l13">genoeghden</rhyme></l>
        <l id="s2:l16" met="2" type="-+">Haer sonder <rhyme label="h" type="m" corresp="#s2:l14">verschil</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Sy is soo <rhyme label="a" type="m">vroom</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Gelijck de <rhyme label="a" type="m" corresp="#s3:l1">palm-boom</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Haer ziel is vervult met d'Heylige <rhyme label="b" type="m">Geest</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Voor 't minste gebreck, en sonden <rhyme label="b" type="m" corresp="#s3:l3">vreest</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Als een Son <rhyme label="c" type="m">licht</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">De staten al <rhyme label="c" type="m" corresp="#s3:l5">sticht</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Als David sy is na Godts hart en <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">In liefde brant als een <rhyme label="d" type="m" corresp="#s3:l7">Seraphin</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Staet als de <rhyme label="e" type="f">Laurieren</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Altijdt schoon, en <rhyme label="f" type="m">groen</rhyme>.</l>
        <l id="s3:l11" met="2" type="+-">Haer tongh, noch <rhyme label="e" type="f" corresp="#s3:l9">manieren</rhyme>.</l>
        <l id="s3:l12" met="2" type="-+">Quaet spreken, noch <rhyme label="f" type="m" corresp="#s3:l10">doen</rhyme>.</l>
        <l id="s3:l13" met="3" type="-+">In alle haer <rhyme label="g" type="f">paden</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Voor oogen Godt <rhyme label="h" type="m">houdt</rhyme>:</l>
        <l id="s3:l15" met="2" type="-+">Vol Godes <rhyme label="g" type="f" corresp="#s3:l13">genaden</rhyme></l>
        <l id="s3:l16" met="2" type="-+">Op haer niet <rhyme label="h" type="m" corresp="#s3:l14">betrout</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Als Iob is <rhyme label="a" type="m">slecht</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Simpel en <rhyme label="a" type="m" corresp="#s4:l1">oprecht</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Godt noch geen menschen haer berispen <rhyme label="b" type="m">sal</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ia! In haer besluyt de deughden <rhyme label="b" type="m" corresp="#s4:l3">al</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">D'Armen milt <rhyme label="c" type="m">geeft</rhyme>;</l>
        <l id="s4:l6" met="2" type="-+">Als een Engel <rhyme label="c" type="m" corresp="#s4:l5">leeft</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Als Ioachim was oudt, zy <rhyme label="d" type="m">onvruchtbaer</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Getrouwt ruym zijnde wel twintigh <rhyme label="d" type="m" corresp="#s4:l7">Iaer</rhyme>:</l>
        <l id="s4:l9" met="2" type="-+">Een Engel des <rhyme label="e" type="f">Heeren</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Met vreught <rhyme label="f" type="m">openbaert</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Sint Ioachim <rhyme label="e" type="f" corresp="#s4:l9">leeren</rhyme></l>
        <l id="s4:l12" met="2" type="-+">Komt, ende <rhyme label="f" type="m" corresp="#s4:l10">verklaert</rhyme>:</l>
        <l id="s4:l13" met="3" type="-+">U Vrouw sal <rhyme label="g" type="f">ontfangen</rhyme></l>
        <l id="s4:l14" met="3" type="-+">In haer dorre <rhyme label="h" type="m">schoot</rhyme>,</l>
        <l id="s4:l15" met="2" type="-+">Na yders <rhyme label="g" type="f" corresp="#s4:l13">verlangen</rhyme>,</l>
        <l id="s4:l16" met="2" type="-+">d'Eelste Iesse <rhyme label="h" type="m" corresp="#s4:l14">loot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Dees Wijngaert <rhyme label="a" type="m">bloeyt</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">D'heylighste vrucht <rhyme label="a" type="m" corresp="#s5:l1">groeyt</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Sy baert Mariam, d'aldersoetste <rhyme label="b" type="m">Druyf</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">'s Hemels Coninghin, de reyne <rhyme label="b" type="m" corresp="#s5:l3">Duyf</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Godts waerdtste <rhyme label="c" type="m">Stadt</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">De Parel en <rhyme label="c" type="m" corresp="#s5:l5">schat</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Die gesocht, en begraven is <rhyme label="d" type="m">geweest</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Die 't hooft most breken van 't helsche <rhyme label="d" type="m" corresp="#s5:l7">beest</rhyme>:</l>
        <l id="s5:l9" met="2" type="-+">Uyt dese ranck <rhyme label="e" type="f">groeyen</rhyme></l>
        <l id="s5:l10" met="2" type="-+">Moet, 't opperste <rhyme label="f" type="m">goedt</rhyme></l>
        <l id="s5:l11" met="2" type="+-">Uyt wiens borst sal <rhyme label="e" type="f" corresp="#s5:l9">vloeyen</rhyme></l>
        <l id="s5:l12" met="2" type="-+">Ons voldoende <rhyme label="f" type="m" corresp="#s5:l10">bloedt</rhyme>:</l>
        <l id="s5:l13" met="3" type="-+">'t Eenigh kindt <rhyme label="g" type="f">op-dragen</rhyme></l>
        <l id="s5:l14" met="3" type="-+">Tot Goods dienst, en <rhyme label="h" type="m">eer</rhyme>,</l>
        <l id="s5:l15" met="2" type="-+">Dat sijn <rhyme label="g" type="f" corresp="#s5:l13">wel-behagen</rhyme></l>
        <l id="s5:l16" met="2" type="-+">Sou doen met haer <rhyme label="h" type="m" corresp="#s5:l14">d'Heer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Na haer Mans <rhyme label="a" type="m">doodt</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Kleyn, edel noch <rhyme label="a" type="m" corresp="#s6:l1">groot</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Haer bepraten, ofte verleyde <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dat sy sou trouwe de tweede <rhyme label="b" type="m" corresp="#s6:l3">Man</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Godt mint <rhyme label="c" type="m">alleen</rhyme>:</l>
        <l id="s6:l6" met="2" type="-+">Ende anders <rhyme label="c" type="m" corresp="#s6:l5">geen</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Haer zuyverheydt Godt geeft: leeft <rhyme label="d" type="m">onbesmet</rhyme>.</l>
        <l id="s6:l8" met="4" type="-+">Als Iudith by 't vasten voeght 't <rhyme label="d" type="m" corresp="#s6:l7">gebedt</rhyme>.</l>
        <l id="s6:l9" met="2" type="-+">D'Armen en Goods <rhyme label="e" type="f">Tempel</rhyme></l>
        <l id="s6:l10" met="2" type="-+">Helpt, en <rhyme label="f" type="m">onderhoudt</rhyme>.</l>
        <l id="s6:l11" met="2" type="+-">Sy is een <rhyme label="e" type="f" corresp="#s6:l9">exempel</rhyme></l>
        <l id="s6:l12" met="2" type="-+">Voor jongen en <rhyme label="f" type="m" corresp="#s6:l10">oudt</rhyme>.</l>
        <l id="s6:l13" met="3" type="-+">Niemandt sal <rhyme label="g" type="f">verhalen</rhyme></l>
        <l id="s6:l14" met="3" type="-+">Haer lof, eer en <rhyme label="h" type="m">loon</rhyme>,</l>
        <l id="s6:l15" met="2" type="-+">Dies' in 's Hemels <rhyme label="g" type="f" corresp="#s6:l13">zalen</rhyme></l>
        <l id="s6:l16" met="2" type="-+">Heeft van Godes <rhyme label="h" type="m" corresp="#s6:l14">Soon</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Met lijf en <rhyme label="a" type="m">ziel</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Sint Anna, ick <rhyme label="a" type="m" corresp="#s7:l1">kniel</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Voor u neer: ghy zijt d'ader waer uyt <rhyme label="b" type="m">vloeyt</rhyme></l>
        <l id="s7:l4" met="4" type="-+">De Put des levens, die 't al <rhyme label="b" type="m" corresp="#s7:l3">besproeyt</rhyme>:</l>
        <l id="s7:l5" met="2" type="-+">Wilt met my <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s7:l6" met="2" type="-+">In strijden en <rhyme label="c" type="m" corresp="#s7:l5">pijn</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Versoent my aen Iesu, u Dochters <rhyme label="d" type="m">kindt</rhyme>.</l>
        <l id="s7:l8" met="4" type="-+">'t Is mijn leet dat ick 't quaedt heb <rhyme label="d" type="m" corresp="#s7:l7">bemint</rhyme>.</l>
        <l id="s7:l9" met="2" type="-+">Krijght door uw' <rhyme label="e" type="f">ghebeden</rhyme>:</l>
        <l id="s7:l10" met="2" type="-+">Dat ick ziel en <rhyme label="f" type="m">lijf</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">En goedt mach <rhyme label="e" type="f" corresp="#s7:l9">besteden</rhyme></l>
        <l id="s7:l12" met="2" type="-+">Tot Godes <rhyme label="f" type="m" corresp="#s7:l10">gerijf</rhyme>:</l>
        <l id="s7:l13" met="3" type="-+">En dat ick van <rhyme label="g" type="f">mijne</rhyme></l>
        <l id="s7:l14" met="3" type="-+">Staet, noch Godt <rhyme label="h" type="m">afwijck</rhyme>:</l>
        <l id="s7:l15" met="2" type="-+">Dat mijn ziel <rhyme label="g" type="f" corresp="#s7:l13">verschijne</rhyme></l>
        <l id="s7:l16" met="2" type="-+">Mach, in 't <rhyme label="h" type="m" corresp="#s7:l14">hemelrijck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
