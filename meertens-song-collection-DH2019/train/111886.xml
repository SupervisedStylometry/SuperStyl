  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'t KAn Phillis my niet <rhyme label="a" type="f">schelen</rhyme>,</l>
        <l id="s1:l2" met="6" type="+-">Heb ik uw Ongena, <rhyme label="b" type="m">a.a.a</rhyme>.</l>
        <l id="s1:l3" met="3" type="+-">Het zal my niet <rhyme label="a" type="f" corresp="#s1:l1">vervelen</rhyme>,</l>
        <l id="s1:l4" met="6" type="+-">Wyl 'k naer een ander ga, <rhyme label="b" type="m" corresp="#s1:l2">a.a.a</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Ik lach eens met het <rhyme label="c" type="f">Minnen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die Kleyne <rhyme label="d" type="m">Guit</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Die ban ik <rhyme label="d" type="m" corresp="#s1:l6">uit</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Myn Zinnen, Myn <rhyme label="c" type="f" corresp="#s1:l5">Zinnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Schoon gy my niet wilt <rhyme label="a" type="f">kussen</rhyme>,</l>
        <l id="s2:l2" met="6" type="+-">'k Ben daerom wel te vree, <rhyme label="b" type="m">e.e.e</rhyme>.</l>
        <l id="s2:l3" met="3" type="+-">Ik kan myn Vlam wel <rhyme label="a" type="f" corresp="#s2:l1">blussen</rhyme>,</l>
        <l id="s2:l4" met="6" type="+-">By Roosje of Galathe, <rhyme label="b" type="m" corresp="#s2:l2">e.e.e</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Ik lach eens met het <rhyme label="c" type="f">Minnen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Die Kleyne <rhyme label="d" type="m">Guit</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Die ban ik <rhyme label="d" type="m" corresp="#s2:l6">uit</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Myn Zinnen, Myn <rhyme label="c" type="f" corresp="#s2:l5">Zinnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gy dagt dat ik zou <rhyme label="a" type="f">treuren</rhyme>,</l>
        <l id="s3:l2" met="6" type="+-">Om u naer dat ik zie, <rhyme label="b" type="m">i.i.i</rhyme>.</l>
        <l id="s3:l3" met="3" type="+-">Maer 't zal u niet <rhyme label="a" type="f" corresp="#s3:l1">gebeuren</rhyme>,</l>
        <l id="s3:l4" met="6" type="+-">Wyl ik de smert ontvlie, <rhyme label="b" type="m" corresp="#s3:l2">i.i.i</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Ik lach eens met het <rhyme label="c" type="f">Minnen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Die Kleyne <rhyme label="d" type="m">Guit</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Die ban ik <rhyme label="d" type="m" corresp="#s3:l6">uit</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Myn Zinnen, Myn <rhyme label="c" type="f" corresp="#s3:l5">Zinnen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maer laet ik dog eens <rhyme label="a" type="f">horen</rhyme>,</l>
        <l id="s4:l2" met="6" type="+-">Phillis hoe komt het zoo, <rhyme label="b" type="m">o.o.o</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">Dat gy dus zyt in <rhyme label="a" type="f" corresp="#s4:l1">toren</rhyme>;</l>
        <l id="s4:l4" met="6" type="+-">Of was ik u te bloo? <rhyme label="b" type="m" corresp="#s4:l2">o.o.o</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Ik lach eens met het <rhyme label="c" type="f">Minnen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Die Kleyne <rhyme label="d" type="m">Guit</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Die ban ik <rhyme label="d" type="m" corresp="#s4:l6">uit</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Myn Zinnen, Myn <rhyme label="c" type="f" corresp="#s4:l5">Zinnen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Of dagt gy my te <rhyme label="a" type="f">Trouwen</rhyme>,</l>
        <l id="s5:l2" met="6" type="+-">Verlaet dees Sustenu, <rhyme label="b" type="m">u.u.u</rhyme>.</l>
        <l id="s5:l3" met="3" type="+-">Ik hou my van de <rhyme label="a" type="f" corresp="#s5:l1">Vrouwen</rhyme>,</l>
        <l id="s5:l4" met="6" type="+-">En ben van 't Huwelyk schu, <rhyme label="b" type="m" corresp="#s5:l2">u.u.u</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Ik lach eens met het <rhyme label="c" type="f">Minnen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Die Kleyne <rhyme label="d" type="m">Guit</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Die ban ik <rhyme label="d" type="m" corresp="#s5:l6">uyt</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Myn Zinnen, Myn <rhyme label="c" type="f" corresp="#s5:l5">Zinnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
