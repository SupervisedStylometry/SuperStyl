<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20886</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Boksvoetje</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">GEkomen // <rhyme label="a" type="f">vernomen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Is heden in 't <rhyme label="b" type="m">stal</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Den Hoeder // den <rhyme label="c" type="f">Voeder</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Den Schepper van <rhyme label="b" type="m" corresp="#s1:l2">all'</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">De Knapen // der <rhyme label="d" type="f">schapen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Verlaten hun <rhyme label="e" type="m">Vee</rhyme>;</l>
        <l id="s1:l7" met="2" type="+-">En soecken // in <rhyme label="f" type="f">doecken</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Blymoedigh Hem <rhyme label="e" type="m" corresp="#s1:l6">dree</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Haer keelen // en <rhyme label="a" type="f">Veelen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Die worden <rhyme label="b" type="m">gestelt</rhyme>;</l>
        <l id="s2:l3" met="2" type="-+">Sy singen // en <rhyme label="c" type="f">springen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Van vreughden langhs <rhyme label="b" type="m" corresp="#s2:l2">veld</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Sy rasen // en <rhyme label="d" type="f">blasen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">En spelen op 't <rhyme label="e" type="m">Riet</rhyme>:</l>
        <l id="s2:l7" met="2" type="+-">'t Is wonder // dat <rhyme label="f" type="f">onder</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Hunlieden <rhyme label="e" type="m" corresp="#s2:l6">geschiet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Sy loopen, sy <rhyme label="a" type="f">hopen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Het Kindtje te <rhyme label="b" type="m">sien</rhyme>;</l>
        <l id="s3:l3" met="2" type="-+">Sy willen // het <rhyme label="c" type="f">stillen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Sy willen 't <rhyme label="b" type="m" corresp="#s3:l2">verblien</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Met kussen // met <rhyme label="d" type="f">sussen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Met <rhyme label="e" type="m">vrindelijckheyd</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Met kallen // met <rhyme label="f" type="f">rallen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Met <rhyme label="e" type="m" corresp="#s3:l6">Herders-beleyd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Heus Cloris // en <rhyme label="a" type="f">Doris</rhyme>.</l>
        <l id="s4:l2" met="2" type="-+">Waer loopt ghy dus <rhyme label="b" type="m">ras</rhyme>?</l>
        <l id="s4:l3" met="2" type="-+">Laet soetjes // uw' <rhyme label="c" type="f">voetjes</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Volbrengen haer <rhyme label="b" type="m" corresp="#s4:l2">pas</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Wy moeten // gaen <rhyme label="d" type="f">groeten</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Te samen den <rhyme label="e" type="m">Heer</rhyme>;</l>
        <l id="s4:l7" met="2" type="+-">'t Is reden // 'k wil <rhyme label="f" type="f">mede</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Bewijsen Hem <rhyme label="e" type="m" corresp="#s4:l6">eer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">O Tyter ,, wat <rhyme label="a" type="f">Cyther</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Wat <rhyme label="b" type="m">snaare-gespel</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Wat Orgel // wat <rhyme label="c" type="f">gorgel</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Behouwen wy <rhyme label="b" type="m" corresp="#s5:l2">wel</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Om d' eere // des <rhyme label="d" type="f">Heere</rhyme></l>
        <l id="s5:l6" met="2" type="-+">(Die 't herte <rhyme label="e" type="m">verheughdt</rhyme>)</l>
        <l id="s5:l7" met="2" type="+-">Te spelen // te <rhyme label="f" type="f">quelen</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Met 's herten <rhyme label="e" type="m" corresp="#s5:l6">geneughd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Trompetten // <rhyme label="a" type="f">Kornetten</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Behouven wy <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Noch Bullen: // wy <rhyme label="c" type="f">sullen</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Van buyten een <rhyme label="b" type="m" corresp="#s6:l2">Lied</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Beginne // uyt <rhyme label="d" type="f">minne</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Van 't Opperste <rhyme label="e" type="m">Goed</rhyme>;</l>
        <l id="s6:l7" met="2" type="+-">Waer heenen // ons' <rhyme label="f" type="f">beenen</rhyme></l>
        <l id="s6:l8" met="2" type="-+">Nu loopen met <rhyme label="e" type="m" corresp="#s6:l6">spoed</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">O Maetjens // ons <rhyme label="a" type="f">praetjens</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Die dienen <rhyme label="b" type="m">gestaeckt</rhyme>;</l>
        <l id="s7:l3" met="2" type="-+">Houdt Herder! // nier <rhyme label="c" type="f">varder</rhyme>:</l>
        <l id="s7:l4" met="2" type="-+">Het Stal'ken <rhyme label="b" type="m" corresp="#s7:l2">genaeckt</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+">Laet vallen // ons <rhyme label="d" type="f">allen</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Ter aerde ter <rhyme label="e" type="m">neêr</rhyme>.</l>
        <l id="s7:l7" met="2" type="+-">En groete // dit <rhyme label="f" type="f">soete</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Kleyn Kindeken <rhyme label="e" type="m" corresp="#s7:l6">teer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Zijt welkoom, zijt <rhyme label="a" type="f">welkoom</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Zijt welkoom <rhyme label="b" type="m">weerom</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Bestierder // <rhyme label="c" type="f">Vercierder</rhyme></l>
        <l id="s8:l4" met="2" type="-+">Van Sterren // en <rhyme label="b" type="m" corresp="#s8:l2">blom</rhyme>;</l>
        <l id="s8:l5" met="2" type="-+">Wien baerde // op d' <rhyme label="d" type="f">aerde</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Dit Maeghdeken <rhyme label="e" type="m">fier</rhyme>,</l>
        <l id="s8:l7" met="2" type="+-">Wiens glory // <rhyme label="f" type="f">memory</rhyme></l>
        <l id="s8:l8" met="2" type="-+">Wy vieren nu <rhyme label="e" type="m" corresp="#s8:l6">hier</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Komt Machten ,, komt <rhyme label="a" type="f">Krachten</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Van 't hemelsche <rhyme label="b" type="m">hof</rhyme>;</l>
        <l id="s9:l3" met="2" type="-+">Komt Eng'len ,, <rhyme label="c" type="f">vermeng'len</rhyme></l>
        <l id="s9:l4" met="2" type="-+">Uw' stemme tot <rhyme label="b" type="m" corresp="#s9:l2">lof</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Ons' Broeders ,, ons' <rhyme label="d" type="f">Hoeders</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Met hemelsch' <rhyme label="e" type="m">jolijt</rhyme>:</l>
        <l id="s9:l7" met="2" type="+-">Seght Amen: ,, weest t' <rhyme label="f" type="f">samen</rhyme></l>
        <l id="s9:l8" met="2" type="-+">Met d' Herders <rhyme label="e" type="m" corresp="#s9:l6">verblijdt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Messia ,, <rhyme label="a" type="f">Maria</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Wy groeten U t' <rhyme label="b" type="m">saem</rhyme>;</l>
        <l id="s10:l3" met="2" type="-+">Verspreyden // <rhyme label="c" type="f">verbreyden</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Wy sullen uw' <rhyme label="b" type="m" corresp="#s10:l2">Naem</rhyme>:</l>
        <l id="s10:l5" met="2" type="-+">Hier leggen // ons' <rhyme label="d" type="f">Weggen</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Hier laten w' ons' <rhyme label="e" type="m">Melck</rhyme>;</l>
        <l id="s10:l7" met="2" type="+-">Wy jachten // wy <rhyme label="f" type="f">trachten</rhyme></l>
        <l id="s10:l8" met="2" type="-+">Nae huys toe weêr <rhyme label="e" type="m" corresp="#s10:l6">elck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
