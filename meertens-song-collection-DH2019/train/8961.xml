  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VReucht u ghy lief <rhyme label="a" type="m">Christenheyt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Christ heeft u die steede <rhyme label="a" type="m" corresp="#s1:l1">bereyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat ghy syt, waer hy <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Dies zydy lof geseyt Heer Jesu <rhyme label="b" type="m" corresp="#s1:l3">Christ</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Christ een Coninck in <rhyme label="a" type="m">hemel</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die ghy syt in Maria <rhyme label="a" type="m" corresp="#s2:l1">ontfangen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En syt van dood' <rhyme label="b" type="m">erstanden</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Christ bid' t Godt voor <rhyme label="b" type="m" corresp="#s2:l3">ons</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
    </lg>
  </text>
