<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185227</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatstmaal ging ik ter jacht</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Gods wonderen <rhyme label="a" type="m">aensiet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">‘t Geen hy heeft voor <rhyme label="b" type="f">genomen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Terstont en geeft hy ‘t <rhyme label="a" type="m" corresp="#s1:l1">niet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">‘t Contrary laet hy <rhyme label="b" type="f" corresp="#s1:l2">comen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dit doet hy al met goet <rhyme label="c" type="m">voordacht</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Om het menschelick <rhyme label="c" type="m" corresp="#s1:l5">gheslacht</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Te toonen zijne <rhyme label="c" type="m" corresp="#s1:l5">macht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want daer en scheen niet <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ter tijt van Iosephs <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat Godt had in den <rhyme label="a" type="m" corresp="#s2:l1">sin</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Om het het rijck te <rhyme label="b" type="f" corresp="#s2:l2">geven</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Als hy in die <rhyme label="c" type="m">gevangenis</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Sonder schult geworpen <rhyme label="c" type="m" corresp="#s2:l5">is</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Sijn droomen scheenen <rhyme label="c" type="m" corresp="#s2:l5">mis</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Oock als zijn broeders <rhyme label="a" type="m">fel</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Met een grammoedich <rhyme label="b" type="f">poghen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hem wilden dooden <rhyme label="a" type="m" corresp="#s3:l1">snel</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">lck een had zijn <rhyme label="b" type="f" corresp="#s3:l2">be-ooghen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">D’een wild hem worpen in den <rhyme label="c" type="m">put</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">D’ander die heeft dat <rhyme label="c" type="m" corresp="#s3:l5">beschut</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">‘t Verkoopen was hem <rhyme label="c" type="m" corresp="#s3:l5">nut</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al sonder haer <rhyme label="a" type="m">beleyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Sy die Cooplieden <rhyme label="b" type="f">saghen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Terstont was elck <rhyme label="a" type="m" corresp="#s4:l1">bereyt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om van den koop te <rhyme label="b" type="f" corresp="#s4:l2">vraghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Godts schickingh sy en misten <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">‘t Geen sy wilden keeren <rhyme label="c" type="m" corresp="#s4:l5">siet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Dat is door haer <rhyme label="c" type="m" corresp="#s4:l5">gheschiet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het menschelick <rhyme label="a" type="m">verstant</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat werckt op zijn <rhyme label="b" type="f">manieren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Maer Gods verborghen <rhyme label="a" type="m" corresp="#s5:l1">hant</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Heeft daer in zijn <rhyme label="b" type="f" corresp="#s5:l2">bestieren</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Hy doet te niet al haren <rhyme label="c" type="m">raet</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Dat haer voorgenomen <rhyme label="c" type="m" corresp="#s5:l5">quaet</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Noch al ten besten <rhyme label="c" type="m" corresp="#s5:l5">staet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">-Hy laetse voor een <rhyme label="a" type="m">tijt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Haer eyghen wil <rhyme label="b" type="f">bedrijven</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Maer ‘t dient al tot <rhyme label="a" type="m" corresp="#s6:l1">profijt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Gods raet moet staende <rhyme label="b" type="f" corresp="#s6:l2">bluven</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Hoe het den mensche keert of <rhyme label="c" type="m">wendt</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">‘t Comt al tot den goeden <rhyme label="c" type="m" corresp="#s6:l5">endt</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Al ist hem <rhyme label="c" type="m" corresp="#s6:l5">onbekent</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die Godt den Heer <rhyme label="a" type="m">bemint</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Vyt aller zielen <rhyme label="b" type="f">krachten</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sijn gunst hy wel <rhyme label="a" type="m" corresp="#s7:l1">bevint</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">In alle zijn <rhyme label="b" type="f" corresp="#s7:l2">betrachten</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Al schijnt dat hy geen oogh en <rhyme label="c" type="m">hout</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Sijn oordeel is menich <rhyme label="c" type="m" corresp="#s7:l5">vout</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Wel dien die hem <rhyme label="c" type="m" corresp="#s7:l5">betrout</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
