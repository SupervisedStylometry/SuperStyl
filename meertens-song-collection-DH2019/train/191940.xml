<song>
  <meta>
    <id>191940</id>
    <date>
      <year-start>1753</year-start>
      <year-end>1753</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zie de roosjes nu eens bloeien</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VEnus man had eens de <rhyme label="a" type="f">pylen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Voor de minnegoon <rhyme label="b" type="m">gesmeed</rhyme>.</l>
        <l id="s1:l3" met="2" type="+-">Gloejend <rhyme label="b" type="m" corresp="#s1:l2">heet</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Waar van Lemnos vuur, by <rhyme label="a" type="f" corresp="#s1:l1">wylen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">In het smitshol gloeit en <rhyme label="c" type="m">blaekt</rhyme>.</l>
        <l id="s1:l6" met="2" type="+-">Als hy 't <rhyme label="d" type="f">yzer</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Als hy 't yzer eerst <rhyme label="c" type="m" corresp="#s1:l5">aanraekt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Venus, die op weermin <rhyme label="a" type="f">hooptte</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Kwam 'er by, en heeft elks <rhyme label="b" type="m">punt</rhyme></l>
        <l id="s2:l3" met="2" type="+-">Wat <rhyme label="b" type="m" corresp="#s2:l2">verdunt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'t Geen ze in zoeten honing <rhyme label="a" type="f" corresp="#s2:l1">doopte</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daar cupido gal by <rhyme label="c" type="m">mengd</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Als hy 't <rhyme label="d" type="f">yzer</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Als hy 't yzer gloejend <rhyme label="c" type="m" corresp="#s2:l5">brengd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Mars, uit 't oorlogshuis <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">[Daar hy spies en <rhyme label="b" type="m">oorlogsschilt</rhyme></l>
        <l id="s3:l3" met="2" type="+-">Zwaeit en <rhyme label="b" type="m" corresp="#s3:l2">drilt</rhyme>]</l>
        <l id="s3:l4" met="4" type="+-">Lachte met dees <rhyme label="a" type="f" corresp="#s3:l1">waependroomen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Spotte, en hield cupidoos <rhyme label="c" type="m">schigt</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">En het <rhyme label="d" type="f">yzer</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En het yzer veel te <rhyme label="c" type="m" corresp="#s3:l5">ligt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maar cupido, straks aen 't <rhyme label="a" type="f">woelen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Blaekte, en zei: dees schigt, zie <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Is wel <rhyme label="b" type="m" corresp="#s4:l2">zwaar</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Zoo ge hem raekt, zoo zult ge 't <rhyme label="a" type="f" corresp="#s4:l1">voelen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Mavors greep hem, maar <rhyme label="c" type="m">terstond</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Gaf het <rhyme label="d" type="f">yzer</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Gaf het yzer hem een <rhyme label="c" type="m" corresp="#s4:l5">wond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Venus lachtte. Mars <rhyme label="a" type="f">verzuchtte</rhyme>.</l>
        <l id="s5:l2" met="4" type="+-">Och! dees pyl, zegt Hy, <rhyme label="b" type="m">voorwaar</rhyme></l>
        <l id="s5:l3" met="2" type="+-">Is te <rhyme label="b" type="m" corresp="#s5:l2">zwaar</rhyme>!</l>
        <l id="s5:l4" met="4" type="+-">Ruk hem uit eer 'k slimmer <rhyme label="a" type="f" corresp="#s5:l1">duchte</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Maar cupido sprak; wel <rhyme label="c" type="m">nu</rhyme>!</l>
        <l id="s5:l6" met="2" type="+-">Hou dat <rhyme label="d" type="f">yzer</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Hou dat yzer slegts voor <rhyme label="c" type="m" corresp="#s5:l5">u</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
