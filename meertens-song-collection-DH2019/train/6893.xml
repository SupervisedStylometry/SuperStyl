  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Nv lovet Godt in Hemels <rhyme label="a" type="m">throon</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ghy heyden aller <rhyme label="a" type="m" corresp="#s1:l1">natioon</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Prijst hem hooch met vreugden <rhyme label="b" type="f">schalle</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Ghy volcker op eerden <rhyme label="b" type="f" corresp="#s1:l3">alle</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dan sijne gnaed en trouwe <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Aen ons sich altijt meeren <rhyme label="a" type="m" corresp="#s2:l1">doet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En sal die waerheyt des <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">In eewigheyt altijt <rhyme label="b" type="f" corresp="#s2:l3">weeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Lof sy den Vader en den <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Met beyder Geest in hogen <rhyme label="a" type="m" corresp="#s3:l1">throon</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">In een wesen Godt en een <rhyme label="b" type="f">Heer</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">In dry persoonen geeft hem <rhyme label="b" type="f" corresp="#s3:l3">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
