      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DEn Heere alle Heydens <rhyme label="a" type="m">prijst</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Hem alle volken eer <rhyme label="a" type="m" corresp="#s1:l1">bewijst</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Want sijn genaed' op ons <rhyme label="b" type="m">beklijft</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En 's Heeren Waerheydt eeuwigh <rhyme label="b" type="m" corresp="#s1:l3">blijft</rhyme>.</l>
      </lg>
