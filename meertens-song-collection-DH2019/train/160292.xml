    <melody>Gevulde zwijnendarm</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Lijden o bitter <rhyme label="a" type="f">lijden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hoe comt ghy my dus <rhyme label="a" type="f" corresp="#s1:l1">bestrijden</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Och eylacy, sal de <rhyme label="b" type="f">graty</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Nimmer my <rhyme label="a" type="f" corresp="#s1:l1">verblyden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick dencker om een <rhyme label="a" type="f">Goddinne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sy steelter mijn hert end <rhyme label="a" type="f" corresp="#s2:l1">sinnen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Want dit meisien, menich <rhyme label="b" type="f">reisien</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Brant de ziel van <rhyme label="a" type="f" corresp="#s2:l1">binnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zy doeter my deerlijck <rhyme label="a" type="f">swoelen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">End heefter nochtans <rhyme label="a" type="f" corresp="#s3:l1">ghevoelen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Dat sy boven, al te <rhyme label="b" type="f">loven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Moet dees hitte <rhyme label="a" type="f" corresp="#s3:l1">koelen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Want alle mijn swaere <rhyme label="a" type="f">wonden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Een goe medecijn <rhyme label="a" type="f" corresp="#s4:l1">gevonden</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Can zy toghen, uyt <rhyme label="b" type="f">medooghen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En heeft haest <rhyme label="a" type="f" corresp="#s4:l1">verbonden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O groote beroemde <rhyme label="a" type="f">Goden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dien icker noyt ben <rhyme label="a" type="f" corresp="#s5:l1">ontvloden</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Laet dat meysien, toch een <rhyme label="b" type="f">reysien</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Staen in mijn <rhyme label="a" type="f" corresp="#s5:l1">gheboden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
