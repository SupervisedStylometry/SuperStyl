  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'t Is ten lesten eens <rhyme label="a" type="m">geluckt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat wy met <rhyme label="b" type="f">avontagie</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hebben 't Schut te Boort <rhyme label="a" type="m" corresp="#s1:l1">geruckt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En den Engels-man <rhyme label="a" type="m" corresp="#s1:l1">verdruckt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Couragie, Cauragie, <rhyme label="b" type="f" corresp="#s1:l2">couragie</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Onse Vloot die lagh in <rhyme label="a" type="m">ly</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Jn 't voordeel van <rhyme label="b" type="f">Zeylagie</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat bracht ons den Vyandt <rhyme label="a" type="m" corresp="#s2:l1">by</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die wy siende, riepen <rhyme label="a" type="m" corresp="#s2:l1">wy</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Couragie, Cauragie, <rhyme label="b" type="f" corresp="#s2:l2">couragie</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Elck toonde sigh gelijck een <rhyme label="a" type="m">Leeuw</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">By 't Wilt in een <rhyme label="b" type="f">Bosschagie</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Daer d'Hollanders, Vries en <rhyme label="a" type="m" corresp="#s3:l1">Zeeuw'</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">t'Samen maeckten een <rhyme label="a" type="m" corresp="#s3:l1">geschreeuw</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Couragie, Cauragie, <rhyme label="b" type="f" corresp="#s3:l2">couragie</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Tromp die hier de voor-tocht <rhyme label="a" type="m">hadt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Diende sigh van <rhyme label="b" type="f">d'occasie</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Roepen: Mannen raeckt maer <rhyme label="a" type="m" corresp="#s4:l1">wat</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Peur de Staertman an sijn <rhyme label="a" type="m" corresp="#s4:l1">gat</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Couragie, Cauragie, <rhyme label="b" type="f" corresp="#s4:l2">couragie</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">d'Andre volgen al-te <rhyme label="a" type="m">mael</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En maeckten een <rhyme label="b" type="f">ravagie</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Voerder al de selve <rhyme label="a" type="m" corresp="#s5:l1">Tael</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Met haer stucken van <rhyme label="a" type="m" corresp="#s5:l1">Metael</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Couragie, Cauragie, <rhyme label="b" type="f" corresp="#s5:l2">couragie</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Klamp den Admirael aen <rhyme label="a" type="m">boort</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die met sijn Lacquay en <rhyme label="b" type="f">Pagie</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Hoogh van moedt en heel <rhyme label="a" type="m" corresp="#s6:l1">verstoort</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Niet wil luyst'ren nae ons <rhyme label="a" type="m" corresp="#s6:l1">woort</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Couragie, Cauragie, <rhyme label="b" type="f" corresp="#s6:l2">couragie</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">So geseyd was, soo <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Daer 's Buyt en goe <rhyme label="b" type="f">Packagie</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ruyter, Tromp en Sweers en <rhyme label="a" type="m" corresp="#s7:l1">Haen</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Drongen dapper <rhyme label="a" type="m" corresp="#s7:l1">aen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Couragie, Cauragie, <rhyme label="b" type="f" corresp="#s7:l2">couragie</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ascus Silvere <rhyme label="a" type="m">Servies</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat maeckte ons <rhyme label="b" type="f">volagie</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Die wy door een <rhyme label="a" type="m" corresp="#s8:l1">Enter-pries</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Dwongen tot sijn Schips <rhyme label="a" type="m" corresp="#s8:l1">verlies</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Couragie, Cauragie, <rhyme label="b" type="f" corresp="#s8:l2">couragie</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dit brocht d'andren op de <rhyme label="a" type="m">vlucht</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Daer Monck vol <rhyme label="b" type="f">desperacv</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Mee verlaten most de <rhyme label="a" type="m" corresp="#s9:l1">lucht</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Daerom nu den Coningh <rhyme label="a" type="m" corresp="#s9:l1">sucht</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Och lacy! Och lacy! Och <rhyme label="b" type="f" corresp="#s9:l2">lacy</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">'t Gantsche Landt is nu vol <rhyme label="a" type="m">vreught</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Danckt Godt voor dese <rhyme label="b" type="f">glory</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Die ons harten weer <rhyme label="a" type="m" corresp="#s10:l1">verheught</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Singht nu watje singhen <rhyme label="a" type="m" corresp="#s10:l1">meught</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Victory, Victory, <rhyme label="b" type="f" corresp="#s10:l2">Victory</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
