  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn hert is, Heer, verheven <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Mijn ooge niet na hoocheyt <rhyme label="a" type="m" corresp="#s1:l1">siet</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Geen dingen derf ick <rhyme label="b" type="m">onderstaen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Die mijn begrijp te boven <rhyme label="b" type="m" corresp="#s1:l3">gaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En heb ick mijne lusten <rhyme label="a" type="m">wilt</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Niet ingetoomet en <rhyme label="a" type="m" corresp="#s2:l1">gestilt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En my gelijck een kint <rhyme label="b" type="m">vercleent</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Dat van syn moeder wert <rhyme label="b" type="m" corresp="#s2:l3">gespeent</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mijn ziele hout haer soo <rhyme label="a" type="m">gering'</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Gelijck een teder <rhyme label="a" type="m" corresp="#s3:l1">suygeling'</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Die vande melck wert <rhyme label="b" type="m">afgeweert</rhyme></l>
        <l id="s3:l4" met="4" type="-+">En die geduldelijck <rhyme label="b" type="m" corresp="#s3:l3">ontbeert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat IsraÃ«l lanckmoedich <rhyme label="a" type="m">richt'</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Op God al syne <rhyme label="a" type="m" corresp="#s4:l1">toeversicht</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En ruste op syn <rhyme label="b" type="m">goedicheyt</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Van nu tot inder <rhyme label="b" type="m" corresp="#s4:l3">eeuwicheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
