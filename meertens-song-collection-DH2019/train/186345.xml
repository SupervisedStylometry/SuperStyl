<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186345</id>
    <date>
      <year-start>1725</year-start>
      <year-end>1725</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wel mijn hert en ziel <rhyme label="a" type="m">Vorstin</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Vreugde-leven van de <rhyme label="a" type="m" corresp="#s1:l1">Min</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Als ik maar u schoon sie <rhyme label="b" type="f">blinken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Moet ik in een zee <rhyme label="b" type="f" corresp="#s1:l3">versinken</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Van een diep <rhyme label="a" type="m" corresp="#s1:l1">verwondering</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dat gy zijt het puyk der <rhyme label="a" type="m">Maagden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Want my niemand meer <rhyme label="a" type="m" corresp="#s2:l1">behaagden</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Boven uw mijn Philis <rhyme label="b" type="f">lief</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Gy zijt staag mijn <rhyme label="b" type="f" corresp="#s2:l3">herte-dief</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Schoon dat gy met Thirs aan <rhyme label="a" type="m" corresp="#s2:l1">laagden</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">’’k Dogt dat ik soo lief ter <rhyme label="a" type="m">been</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Met u en met <rhyme label="a" type="m" corresp="#s3:l1">Clorimeen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zoo in vrolijkheid soud <rhyme label="b" type="f">wand’’len</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En van soetheyd aardig <rhyme label="b" type="f" corresp="#s3:l3">hand’’len</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Gy waart voor my so kout als <rhyme label="a" type="m" corresp="#s3:l1">steen</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Thirsis lag in ’’t hert <rhyme label="a" type="m">begraven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En die kon u soetheid <rhyme label="a" type="m" corresp="#s4:l1">laven</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Clorimeen die sag het <rhyme label="b" type="f">ook</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">’’t Scheen wel sotheid of een <rhyme label="b" type="f" corresp="#s4:l3">spook</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dat ik Thirs die vryheid <rhyme label="a" type="m" corresp="#s4:l1">gaven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sout u vreemt zijn dat gy <rhyme label="a" type="m">mee</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Met my gang? en dat gy <rhyme label="a" type="m" corresp="#s5:l1">lee</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Dat ik U soo lief <rhyme label="b" type="f">omarmde</rhyme>?</l>
        <l id="s5:l4" met="4" type="+-">Als een Thirs die gy <rhyme label="b" type="f" corresp="#s5:l3">ontfarmde</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">die U brogt van stee op <rhyme label="a" type="m" corresp="#s5:l1">stee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Neen mijn Philis, Thirs in ’’t <rhyme label="a" type="m">herte</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Gy bragt doen mijn vreugd ten <rhyme label="a" type="m" corresp="#s6:l1">smerte</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Clorimeen was niet soo <rhyme label="b" type="f">schuw</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Schoon zy wist dat ik voor <rhyme label="b" type="f" corresp="#s6:l3">uw</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Meest was als voor haar in ’’t <rhyme label="a" type="m" corresp="#s6:l1">herte</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Als wy nog eens daar gaan <rhyme label="a" type="m">spelen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Zal dan Thirs weer met u <rhyme label="a" type="m" corresp="#s7:l1">quelen</rhyme>?</l>
        <l id="s7:l3" met="4" type="+-">’’k Zal dan Philis nooyt soo <rhyme label="b" type="f">sagt</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Met Thirs handelen als ik <rhyme label="b" type="f" corresp="#s7:l3">plagt</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Maar u schoonheyd selver <rhyme label="a" type="m" corresp="#s7:l1">streelen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Of Thirs mogt door groodsen <rhyme label="a" type="m">aart</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Hem verbeelden hy was ’’t <rhyme label="a" type="m" corresp="#s8:l1">waart</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Zal dan Philis Thirs <rhyme label="b" type="f">aanklampen</rhyme>?</l>
        <l id="s8:l4" met="4" type="+-">Aan hem geven menig <rhyme label="b" type="f" corresp="#s8:l3">rampen</rhyme>?</l>
        <l id="s8:l5" met="4" type="+-">Die al zijn voor hem <rhyme label="a" type="m" corresp="#s8:l1">vergaart</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Soet mijn Phiel! laet Thirs <rhyme label="a" type="m">beginnen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Trekken vry u hert en <rhyme label="a" type="m" corresp="#s9:l1">sinnen</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">’’k Blijf die ik ben, mijn soete <rhyme label="b" type="f">Phil</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">’’t Hert gerust buygt in u <rhyme label="b" type="f" corresp="#s9:l3">wil</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Al soud Thirs u eeuwig <rhyme label="a" type="m" corresp="#s9:l1">minnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
