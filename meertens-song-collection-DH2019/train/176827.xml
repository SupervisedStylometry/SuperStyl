<song>
  <meta>
    <id>176827</id>
    <date>
      <year-start>1683</year-start>
      <year-end>1683</year-end>
    </date>
    <location>
      <orginal-name>leeuwarden</orginal-name>
      <standard-name>Leeuwarden, The Netherlands</standard-name>
      <coordinates>
        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>O Gott wir danken deiner Güt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Godt Vader in <rhyme label="a" type="m">eeuwigheyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wy loven u <rhyme label="b" type="f">bequame</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ons ziel en ons <rhyme label="a" type="m" corresp="#s1:l1">inwendigheyt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Prijst uwen heyl’gen <rhyme label="b" type="f" corresp="#s1:l2">Name</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ghy hebt ons zielen lieve <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ghespijst met uwe Woort en <rhyme label="c" type="m" corresp="#s1:l5">Leer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Des loven wy u <rhyme label="b" type="f" corresp="#s1:l2">t’same</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nu ghy, o Heer, u Godlijck <rhyme label="a" type="m">woort</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ons hebt laten <rhyme label="b" type="f">verkonden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Sterkt ons gheloove, helpt ons <rhyme label="a" type="m" corresp="#s2:l1">voort</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ghetrou te zijn <rhyme label="b" type="f" corresp="#s2:l2">bevonden</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ons nietigheyt is u <rhyme label="c" type="m">bekent</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">U met genaden tot ons <rhyme label="c" type="m" corresp="#s2:l5">went</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Sterkt ons tot alle <rhyme label="b" type="f" corresp="#s2:l2">stonden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gheeft ons een hert en eenen <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een nieu gemoet daer <rhyme label="b" type="f">neven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Daer uwe Wet wert vruchtbaer <rhyme label="a" type="m" corresp="#s3:l1">in</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Om dat al te <rhyme label="b" type="f" corresp="#s3:l2">beleven</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dat ons te vooren is <rhyme label="c" type="m">gestelt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Op dat wy moghen met <rhyme label="c" type="m" corresp="#s3:l5">ghewelt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Na ‘t eeuwige goed <rhyme label="b" type="f" corresp="#s3:l2">streven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den Trooster uwen heyl’ghen <rhyme label="a" type="m">Gheest</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wilt ons van boven <rhyme label="b" type="f">senden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Op dat wy moghen <rhyme label="a" type="m" corresp="#s4:l1">onbevreest</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Onsen loop recht <rhyme label="b" type="f" corresp="#s4:l2">vol-enden</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En recht in uwe weghen <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">In dees tijt op de smalle <rhyme label="c" type="m" corresp="#s4:l5">baen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wilt u niet van ons <rhyme label="b" type="f" corresp="#s4:l2">wenden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Heer wilt dat maken <rhyme label="a" type="m">vrugtbaer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat ghy in ons laet <rhyme label="b" type="f">zaeyen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Op dat het elck een mach hier <rhyme label="a" type="m" corresp="#s5:l1">naer</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met grooter vreuchden <rhyme label="b" type="f" corresp="#s5:l2">maeyen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En uwen name werd’ <rhyme label="c" type="m">ghe-eert</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Oock uwe lof altijt <rhyme label="c" type="m" corresp="#s5:l5">vermeert</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Hoe dat de winden <rhyme label="b" type="f" corresp="#s5:l2">waeyen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wy dancken u van herten <rhyme label="a" type="m">gront</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ghy die woont in den <rhyme label="b" type="f">Throone</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ghy heelt en maket ons <rhyme label="a" type="m" corresp="#s6:l1">ghesont</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Door Christum uwen <rhyme label="b" type="f" corresp="#s6:l2">Soone</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Daerom loven, u Heere <rhyme label="c" type="m">goet</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Al uwe werken die ghy <rhyme label="c" type="m" corresp="#s6:l5">doet</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">In den Hemel seer <rhyme label="b" type="f" corresp="#s6:l2">schoone</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Lof, eer, en prijs, en grooten <rhyme label="a" type="m">danck</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Zijt ghy weerdich <rhyme label="b" type="f">t’ontfanghen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Van ons, o Heer, ons leven <rhyme label="a" type="m" corresp="#s7:l1">lanck</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Gheeft ons een groot <rhyme label="b" type="f" corresp="#s7:l2">verlanghen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">En kracht uws heyl’ghen Gheester <rhyme label="c" type="m">fijn</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Om Heer te doen den wille <rhyme label="c" type="m" corresp="#s7:l5">dijn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Door Iesum Christum, <rhyme label="b" type="f" corresp="#s7:l2">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
