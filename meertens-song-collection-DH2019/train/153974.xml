  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs Jerihos hooghe <rhyme label="a" type="f">wallen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Waren onder voet <rhyme label="a" type="f" corresp="#s1:l1">ghevallen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Godts volck door de Bresse <rhyme label="b" type="m">drongh</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En verworghde Out en <rhyme label="b" type="m" corresp="#s1:l3">Jongh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maer doe Rahab uyt haer <rhyme label="a" type="f">venster</rhyme></l>
        <l id="s2:l2" met="4" type="+-">'t Roode Snoer, tot eenen <rhyme label="a" type="f" corresp="#s2:l1">kenster</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van haer woonplaets hanghen <rhyme label="b" type="m">liet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Bleef zy vry in dat <rhyme label="b" type="m" corresp="#s2:l3">verdriet</rhyme>"</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wanneer Godt sijn snelle <rhyme label="a" type="f">Pijlen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En sijn blinckendt Swaert gaet <rhyme label="a" type="f" corresp="#s3:l1">vijlen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Om de sondaers te <rhyme label="b" type="m">verslaen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Siet hy het gheloove <rhyme label="b" type="m" corresp="#s3:l3">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Waer dat blijckt door eenigh <rhyme label="a" type="f">teecken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Laet hy na in toorn te <rhyme label="a" type="f" corresp="#s4:l1">wreecken</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Maer doet daer sijn goetheydt <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Als een <rhyme label="b" type="m" corresp="#s4:l3">Middagh-zonne-schijn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
