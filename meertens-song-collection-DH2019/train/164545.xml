  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Sie daar de <rhyme label="a" type="m">Lentetijd</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die alles doet <rhyme label="b" type="f">herleven</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Met lust en <rhyme label="a" type="m" corresp="#s1:l1">vrolijkheid</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ons op een nieuw <rhyme label="b" type="f" corresp="#s1:l2">gegeven</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Sie het pluymgedierte <rhyme label="b" type="f" corresp="#s1:l2">sweven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Wiens gesang het veld <rhyme label="a" type="m" corresp="#s1:l1">verblijd</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Hoor vrouw Flora hare <rhyme label="c" type="f">bloemen</rhyme></l>
        <l id="s1:l8" met="4" type="+-">So vol geur en kleuren <rhyme label="c" type="f" corresp="#s1:l7">roemen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Hoor hoe <rhyme label="d" type="m">Pan</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Hoort hoe Pan vol vreugd in 't <rhyme label="e" type="m">veld</rhyme></l>
        <l id="s1:l11" met="3" type="-+">Sijn schelle riedpijp <rhyme label="e" type="m" corresp="#s1:l10">steld</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sie hoe elk een vol <rhyme label="a" type="m">vreugd</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het Stads gewoel <rhyme label="b" type="f">ontweken</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sig op het veld <rhyme label="a" type="m" corresp="#s2:l1">verheugd</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">By boomen en aan <rhyme label="b" type="f" corresp="#s2:l2">beeken</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Door die zoete vreugd <rhyme label="b" type="f" corresp="#s2:l2">ontsteken</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Streelen hare teere <rhyme label="a" type="m" corresp="#s2:l1">jeugd</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ja, een zoet vermaak <rhyme label="c" type="f">ontfangen</rhyme></l>
        <l id="s2:l8" met="4" type="+-">En met veld of <rhyme label="c" type="f" corresp="#s2:l7">minne-sangen</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Of <rhyme label="d" type="m">gevry</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Of gevry, bevrijd van <rhyme label="e" type="m">smert</rhyme>,</l>
        <l id="s2:l11" met="3" type="-+">Vermaken 't jeugdig <rhyme label="e" type="m" corresp="#s2:l10">hert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wel aan, mijn Fillis, <rhyme label="a" type="m">laat</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die vreugd u geest <rhyme label="b" type="f">bekoren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wild vry na mijnen <rhyme label="a" type="m" corresp="#s3:l1">raad</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Mijn ziels beminde, <rhyme label="b" type="f" corresp="#s3:l2">hooren</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Eer u lentetijd <rhyme label="b" type="f" corresp="#s3:l2">verloren</rhyme></l>
        <l id="s3:l6" met="4" type="+-">En u schoonheid <rhyme label="a" type="m" corresp="#s3:l1">ondergaat</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Kan de lente elk <rhyme label="c" type="f">vermaken</rhyme>.</l>
        <l id="s3:l8" met="4" type="+-">Denk, het bloosen uwer <rhyme label="c" type="f" corresp="#s3:l7">kaken</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Is <rhyme label="d" type="m">gelijk</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Is gelijk een lente <rhyme label="e" type="m">bloem</rhyme>,</l>
        <l id="s3:l11" met="3" type="-+">De jeugd is hare <rhyme label="e" type="m" corresp="#s3:l10">roem</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maar als de Noorde <rhyme label="a" type="m">wind</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In guere <rhyme label="b" type="f">winterdagen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Het frisse groen <rhyme label="a" type="m" corresp="#s4:l1">verslind</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat yder kan <rhyme label="b" type="f" corresp="#s4:l2">behagen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Hoort men niemand meer <rhyme label="b" type="f" corresp="#s4:l2">gewagen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Dat men buyten vreugde <rhyme label="a" type="m" corresp="#s4:l1">vind</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">Soo sal ook u roem <rhyme label="c" type="f">verteeren</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Niemand sal u schoonheid <rhyme label="c" type="f" corresp="#s4:l7">eeren</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Als u <rhyme label="d" type="m">jeugd</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Als u jeugd, die ras <rhyme label="e" type="m">vergaat</rhyme>,</l>
        <l id="s4:l11" met="3" type="-+">U door de tijd <rhyme label="e" type="m" corresp="#s4:l10">verlaat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laat dan u <rhyme label="a" type="m">lentetijd</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Niet vrugteloos <rhyme label="b" type="f">verloopen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Eer u schoonheid <rhyme label="a" type="m" corresp="#s5:l1">slyt</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Wat kund gy namaals <rhyme label="b" type="f" corresp="#s5:l2">hopen</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Als met naberouw <rhyme label="b" type="f" corresp="#s5:l2">bekopen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dat gy hebt uw zelf <rhyme label="a" type="m" corresp="#s5:l1">misleid</rhyme>.</l>
        <l id="s5:l7" met="4" type="+-">Wel, laat dan u koele <rhyme label="c" type="f">zinnen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Buygen voor het soete <rhyme label="c" type="f" corresp="#s5:l7">minnen</rhyme></l>
        <l id="s5:l9" met="2" type="+-">En <rhyme label="d" type="m">besteed</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">En besteed in hare <rhyme label="e" type="m">vreugd</rhyme>,</l>
        <l id="s5:l11" met="3" type="-+">Mijn Fillis, uwe <rhyme label="e" type="m" corresp="#s5:l10">jeugd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
