    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">VOlcken, <rhyme label="a" type="m">over-al</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Maeckt triumph <rhyme label="a" type="m" corresp="#s1:l1">geschal</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Stort met bly <rhyme label="b" type="m">geluyt</rhyme></l>
        <l id="s1:l4" met="3" type="+-">God’ uw’ herten <rhyme label="b" type="m" corresp="#s1:l3">uyt</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">D’opper <rhyme label="c" type="m">Godlijckheydt</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Sijner <rhyme label="c" type="m" corresp="#s1:l5">Majesteyt</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Is <rhyme label="d" type="m">verschrickelijck</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Over gantsch <rhyme label="d" type="m" corresp="#s1:l7">aerd-rijck</rhyme>.</l>
        <l id="s1:l9" met="3" type="+-">Alles watmen <rhyme label="e" type="m">siet</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Heeft hy in <rhyme label="e" type="m" corresp="#s1:l9">gebiet</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">’tBuyght voor sijnen <rhyme label="f" type="m">throon</rhyme></l>
        <l id="s1:l12" met="3" type="+-">En erkent sijn <rhyme label="f" type="m" corresp="#s1:l11">kroon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Hy is’t die ons <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Hooger heeft <rhyme label="a" type="m" corresp="#s2:l1">gebracht</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">En de volcken <rhyme label="b" type="m">stelt</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Onder ons <rhyme label="b" type="m" corresp="#s2:l3">geweldt</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Dat haer trotsen <rhyme label="c" type="m">neck</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Ons ten voet-banck <rhyme label="c" type="m" corresp="#s2:l5">streck</rhyme>.</l>
        <l id="s2:l7" met="3" type="+-">Hy is die’t <rhyme label="d" type="m">genot</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Van ’tgewenschte <rhyme label="d" type="m" corresp="#s2:l7">lot</rhyme></l>
        <l id="s2:l9" met="3" type="+-">(Iacobs <rhyme label="e" type="m">heerlijckheydt</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Daer sijn liefd’ op <rhyme label="e" type="m" corresp="#s2:l9">leyt</rhyme>)</l>
        <l id="s2:l11" met="3" type="+-">Uyt een hert soo <rhyme label="f" type="m">mildt</rhyme></l>
        <l id="s2:l12" met="3" type="+-">Ons heeft <rhyme label="f" type="m" corresp="#s2:l11">toegewilt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Schouwt Gods grootheyt <rhyme label="a" type="m">aen</rhyme>.</l>
        <l id="s3:l2" met="3" type="+-">Hy vaert op, <rhyme label="a" type="m" corresp="#s3:l1">omvaen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Met <rhyme label="b" type="m">triumph-gesanck</rhyme></l>
        <l id="s3:l4" met="3" type="+-">En <rhyme label="b" type="m" corresp="#s3:l3">trompet-geklanck</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Hier, met keel en <rhyme label="c" type="m">stem</rhyme>:</l>
        <l id="s3:l6" met="3" type="+-">Singt en we’er-singt <rhyme label="c" type="m" corresp="#s3:l5">hem</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Singt hem loff en <rhyme label="d" type="m">eer</rhyme>:</l>
        <l id="s3:l8" met="3" type="+-">Hem, ons Godt en <rhyme label="d" type="m" corresp="#s3:l7">Heer</rhyme>.</l>
        <l id="s3:l9" met="3" type="+-">Die den scepter <rhyme label="e" type="m">heeft</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Over all’ wat <rhyme label="e" type="m" corresp="#s3:l9">leeft</rhyme>.</l>
        <l id="s3:l11" met="3" type="+-">Singt, en maeckt uw’ <rhyme label="f" type="m">loff</rhyme></l>
        <l id="s3:l12" met="3" type="+-">Nutte leerings <rhyme label="f" type="m" corresp="#s3:l11">stoff</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Vreemde Vorsten <rhyme label="a" type="m">slaen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Met Gods kind’ren <rhyme label="a" type="m" corresp="#s4:l1">aen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Willen een met <rhyme label="b" type="m">haer</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Hopen by die <rhyme label="b" type="m" corresp="#s4:l3">schaer</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Deel en <rhyme label="c" type="m">mede-lot</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Aen Abrahams <rhyme label="c" type="m" corresp="#s4:l5">Godt</rhyme>.</l>
        <l id="s4:l7" met="3" type="+-">Wiens <rhyme label="d" type="m">ontsachlijckheydt</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Over al <rhyme label="d" type="m" corresp="#s4:l7">verspreydt</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">(Want hy heeft op <rhyme label="e" type="m">aerdt</rhyme></l>
        <l id="s4:l10" met="3" type="+-">Beyde schildt en <rhyme label="e" type="m" corresp="#s4:l9">swaerdt</rhyme>)</l>
        <l id="s4:l11" met="3" type="+-">Stadigh aenwas <rhyme label="f" type="m">krijght</rhyme></l>
        <l id="s4:l12" met="3" type="+-">En op’t hooghste <rhyme label="f" type="m" corresp="#s4:l11">stijght</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
