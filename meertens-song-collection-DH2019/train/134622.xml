<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>134622</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>alkmaar</orginal-name>
      <standard-name>Alkmaar, The Netherlands</standard-name>
      <coordinates>
        <lon>52.6312592</lon>
        <lat>4.7503798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="gezongen onze vader" translation="sung Lord's prayer">gezongen onze vader</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vader ons in <rhyme label="a" type="m">Hemelrijck</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Maeckt ons gebed <rhyme label="b" type="m">bequaem</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Wy roepen tot u al <rhyme label="a" type="m" corresp="#s1:l1">gelijck</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Geheylight zy uwen <rhyme label="b" type="m" corresp="#s1:l2">Naem</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">U Rijcke laet ons <rhyme label="a" type="m">gewerden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">U glorie laet ons <rhyme label="b" type="m">sien</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">In Hemel en oock op <rhyme label="a" type="m" corresp="#s2:l1">Aerden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Moet uwen wille <rhyme label="b" type="m" corresp="#s2:l2">geschien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Geeft ons huyden ons daegâ€™licx <rhyme label="a" type="m">broodt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Schelt ons van sonden <rhyme label="b" type="m">quijt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Gelijc wy doen ons <rhyme label="a" type="m" corresp="#s3:l1">mede-genoot</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Alhier op deser <rhyme label="b" type="m" corresp="#s3:l2">tijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Leydt ons in geen <rhyme label="a" type="m">tentatie</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Van quaden maeckt ons <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">In onser <rhyme label="a" type="m" corresp="#s4:l1">tribulatie</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Soo staet ons allegaer <rhyme label="b" type="m" corresp="#s4:l2">by</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want u alleen behoort dat <rhyme label="a" type="m">rijck</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die kracht en <rhyme label="b" type="m">heerlijckheydt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dijn uytverkoren <rhyme label="a" type="m" corresp="#s5:l1">sekerlijck</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Hebt ghy daer toe <rhyme label="b" type="m" corresp="#s5:l2">bereydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maria vol van <rhyme label="a" type="m">gratie</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">U heerlijckheydt groeten <rhyme label="b" type="m">wy</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Met grooter <rhyme label="a" type="m" corresp="#s6:l1">jubilatien</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soo is de Heere met <rhyme label="b" type="m" corresp="#s6:l2">dy</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Boven alle de <rhyme label="a" type="m">Vrouwen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Zijt ghy <rhyme label="b" type="m">gebenedijt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Godts Geest sal by u <rhyme label="a" type="m" corresp="#s7:l1">blijven</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Van nu tot aller <rhyme label="b" type="m" corresp="#s7:l2">tijdt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Gebenedijt soo moet oock <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Jesus u liefste <rhyme label="b" type="m">Soon</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ghy baerden hem al sonder <rhyme label="a" type="m" corresp="#s8:l1">pijn</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ende bleeft een Maget <rhyme label="b" type="m" corresp="#s8:l2">schoon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
