      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter van Gelre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">KOmt tot de <rhyme label="a" type="f">Wat'ren</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Van blijtschap, <rhyme label="b" type="m">o</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ghy Heydensche <rhyme label="c" type="m">geslacht</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Wilt u <rhyme label="a" type="f" corresp="#s1:l1">Tiatren</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Af breecken, <rhyme label="b" type="m" corresp="#s1:l2">soo</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Vervalt u Satans <rhyme label="c" type="m" corresp="#s1:l3">macht</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">en treedt // <rhyme label="d" type="m">ghereedt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Met Wieroock in u <rhyme label="e" type="m">handt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Opwaerts nae 't Iootsche <rhyme label="e" type="m" corresp="#s1:l8">lant</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Om te doen <rhyme label="e" type="m" corresp="#s1:l8">offerhandt</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Aen den nieu ghebooren <rhyme label="f" type="m">Prins</rhyme></l>
        <l id="s1:l12" met="4" type="+-">Die u ballinghachap <rhyme label="f" type="m" corresp="#s1:l11">geensins</rhyme></l>
        <l id="s1:l13" met="3" type="-+">Begeert, maer vreeds <rhyme label="e" type="m" corresp="#s1:l8">bestand</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Siet selfs u <rhyme label="a" type="f">Coningh</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">O Saba, <rhyme label="b" type="m">rijdt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Op Kem'len, om te <rhyme label="c" type="m">sien</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Een Hemels <rhyme label="a" type="f" corresp="#s2:l1">crooningh</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Die nu niet <rhyme label="b" type="m" corresp="#s2:l2">wijdt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van Bethlehem sal <rhyme label="c" type="m" corresp="#s2:l3">gheschien</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">De Thars // van <rhyme label="d" type="m">vars</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Comt oock met yver <rhyme label="e" type="m">aen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Om voor den Heer te <rhyme label="e" type="m" corresp="#s2:l8">staen</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Als een goedt <rhyme label="e" type="m" corresp="#s2:l8">onderdaen</rhyme>.</l>
        <l id="s2:l11" met="4" type="+-">En der Moren Coningh <rhyme label="f" type="m">wijs</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Wil oock dat dees Heer veel <rhyme label="f" type="m" corresp="#s2:l11">prijs</rhyme></l>
        <l id="s2:l13" met="3" type="-+">En eer wordt <rhyme label="e" type="m" corresp="#s2:l8">aenghedaen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Heerlijcke <rhyme label="a" type="f">dinghen</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Sijn dan <rhyme label="b" type="m">gheseyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van u, ghy <rhyme label="c" type="m">Bethelem</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Want in u <rhyme label="a" type="f" corresp="#s3:l1">singhen</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Met <rhyme label="b" type="m" corresp="#s3:l2">soeticheydt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Veel Enghels soete <rhyme label="c" type="m" corresp="#s3:l3">stem</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">'Tghewelf // heeft <rhyme label="d" type="m">self</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Hier van u lof <rhyme label="e" type="m">ghemelt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Aen die schier <rhyme label="e" type="m" corresp="#s3:l8">onghetelt</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Laghen, op 'swerelts <rhyme label="e" type="m" corresp="#s3:l8">veldt</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Want haer Princen vallen <rhyme label="f" type="m">neer</rhyme></l>
        <l id="s3:l12" met="4" type="+-">Off'ren Wieroock aenden <rhyme label="f" type="m" corresp="#s3:l11">Heer</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Met Goudt, en Mirh' <rhyme label="e" type="m" corresp="#s3:l8">verselt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Een nieuwe <rhyme label="a" type="f">Sterre</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Heeft haer tot <rhyme label="b" type="m">u</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Heel haestigh dus <rhyme label="c" type="m">gheport</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Want sy van <rhyme label="a" type="f" corresp="#s4:l1">verre</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Saghen, dat <rhyme label="b" type="m" corresp="#s4:l2">nu</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Godts goedtheyt was <rhyme label="c" type="m" corresp="#s4:l3">ghestort</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Soo dat // het <rhyme label="d" type="m">vat</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Van droeve siels <rhyme label="e" type="m">verdriet</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Elck haestich omme <rhyme label="e" type="m" corresp="#s4:l8">stiet</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Want sy voor-saghen <rhyme label="e" type="m" corresp="#s4:l8">yet</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Dat haer beter soud' <rhyme label="f" type="m">versaen</rhyme></l>
        <l id="s4:l12" met="4" type="+-">Dus sijn sy tot u <rhyme label="f" type="m" corresp="#s4:l11">ghegaen</rhyme></l>
        <l id="s4:l13" met="3" type="-+">O Beth'lem, met <rhyme label="e" type="m" corresp="#s4:l8">eerbiedt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Och of wy <rhyme label="a" type="f">saghen</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Hoe dese <rhyme label="b" type="m">drie</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als Coninghen <rhyme label="c" type="m">bedaert</rhyme></l>
        <l id="s5:l4" met="2" type="-+">haer gift <rhyme label="a" type="f" corresp="#s5:l1">op-draghen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Aen Iesus <rhyme label="b" type="m" corresp="#s5:l2">die</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Een Maghet heeft <rhyme label="c" type="m" corresp="#s5:l3">ghebaert</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Elck souw' // met <rhyme label="d" type="m">rouw'</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Het vuyle sondigh <rhyme label="e" type="m">caf</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Voort-aen wel veghen <rhyme label="e" type="m" corresp="#s5:l8">af</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Door penitenci <rhyme label="e" type="m" corresp="#s5:l8">straf</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">En roepen aen Iesus <rhyme label="f" type="m">mee</rhyme></l>
        <l id="s5:l12" met="4" type="+-">Spaert ons sondigh <rhyme label="f" type="m" corresp="#s5:l11">Ninive</rhyme></l>
        <l id="s5:l13" met="3" type="-+">Door u barmhert'ghe <rhyme label="e" type="m" corresp="#s5:l8">staf</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
