<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>176135</id>
    <date>
      <year-start>1640</year-start>
      <year-end>1640</year-end>
    </date>
    <location>
      <orginal-name>deventer</orginal-name>
      <standard-name>Deventer, The Netherlands</standard-name>
      <coordinates>
        <lon>52.2532272</lon>
        <lat>6.1584725</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 136 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DAnckt den Heere <rhyme label="a" type="m">al-gelijck</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Want hy is seer <rhyme label="a" type="m" corresp="#s1:l1">vriendelijck</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s1:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Prijset, en van herten <rhyme label="a" type="m">looft</rhyme></l>
        <l id="s2:l2" met="4" type="+-">God der Goden <rhyme label="a" type="m" corresp="#s2:l1">overhooft</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s2:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Gevet hem de hoochste <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Hy is aller Heeren <rhyme label="a" type="m" corresp="#s3:l1">Heer</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s3:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die alleen door syne <rhyme label="a" type="m">sterckt'</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Onbegrijplijck wonder <rhyme label="a" type="m" corresp="#s4:l1">werckt</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s4:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Die het schoone <rhyme label="a" type="m">hemelrijck</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Heeft gebout <rhyme label="a" type="m" corresp="#s5:l1">verstandelijck</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s5:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die het onbeweegde <rhyme label="a" type="m">lant</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Op de woeste baren <rhyme label="a" type="m" corresp="#s6:l1">spant</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s6:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die de groote lichten <rhyme label="a" type="m">beyd'</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Aen den hemel heeft <rhyme label="a" type="m" corresp="#s7:l1">bereyt</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s7:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Die de sonne gaf den <rhyme label="a" type="m">schijn</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Om des dages voocht te <rhyme label="a" type="m" corresp="#s8:l1">zijn</rhyme>.</l>
        <l id="s8:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s8:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Die de sterren en de <rhyme label="a" type="m">maen</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Doet des nachtes <rhyme label="a" type="m" corresp="#s9:l1">omme-gaen</rhyme>.</l>
        <l id="s9:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s9:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Die Egyptens eerste <rhyme label="a" type="m">cracht</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Haestelijck heeft <rhyme label="a" type="m" corresp="#s10:l1">omgebracht</rhyme>.</l>
        <l id="s10:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s10:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Die het dienstbaer <rhyme label="a" type="m">Israël</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Heeft verlosset uyt <rhyme label="a" type="m" corresp="#s11:l1">gequel</rhyme>.</l>
        <l id="s11:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s11:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s11:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Syne hant heeft haer <rhyme label="a" type="m">bevrijt</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Synen arm behielt den <rhyme label="a" type="m" corresp="#s12:l1">strijt</rhyme>.</l>
        <l id="s12:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s12:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Die de ongestuyme <rhyme label="a" type="m">zee</rhyme></l>
        <l id="s13:l2" met="4" type="+-">Van malcander scheyden <rhyme label="a" type="m" corresp="#s13:l1">dee</rhyme>.</l>
        <l id="s13:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s13:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s13:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Die voor Israël een <rhyme label="a" type="m">baen</rhyme></l>
        <l id="s14:l2" met="4" type="+-">Maeckte om daer door te <rhyme label="a" type="m" corresp="#s14:l1">gaen</rhyme>.</l>
        <l id="s14:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s14:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s14:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Pharao daer in <rhyme label="a" type="m">versonck</rhyme></l>
        <l id="s15:l2" met="4" type="+-">En syn heele heyr <rhyme label="a" type="m" corresp="#s15:l1">verdronck</rhyme>.</l>
        <l id="s15:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s15:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s15:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Die syn knechten cloeck en <rhyme label="a" type="m">fris</rhyme></l>
        <l id="s16:l2" met="4" type="+-">Leyde door de <rhyme label="a" type="m" corresp="#s16:l1">wildernis</rhyme>.</l>
        <l id="s16:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s16:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s16:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Die der machtigen <rhyme label="a" type="m">gewelt</rhyme></l>
        <l id="s17:l2" met="4" type="+-">Heeft geweldichlijck <rhyme label="a" type="m" corresp="#s17:l1">gevelt</rhyme>.</l>
        <l id="s17:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">godertierenheyt</rhyme></l>
        <l id="s17:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s17:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Die de coningen vol <rhyme label="a" type="m">eer</rhyme></l>
        <l id="s18:l2" met="4" type="+-">Schielijck heeft geslagen <rhyme label="a" type="m" corresp="#s18:l1">neer</rhyme>.</l>
        <l id="s18:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goederterenheyt</rhyme></l>
        <l id="s18:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s18:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Sihon, onder wiens <rhyme label="a" type="m">gebot</rhyme></l>
        <l id="s19:l2" met="4" type="+-">Lach der Amoriten <rhyme label="a" type="m" corresp="#s19:l1">rot</rhyme>.</l>
        <l id="s19:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s19:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s19:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Og, die onder synen <rhyme label="a" type="m">voet</rhyme></l>
        <l id="s20:l2" met="4" type="+-">Hadde Basans <rhyme label="a" type="m" corresp="#s20:l1">overvloet</rhyme>.</l>
        <l id="s20:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s20:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s20:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">En hy heeft haer beyder <rhyme label="a" type="m">rijck</rhyme></l>
        <l id="s21:l2" met="4" type="+-">Wtgedeylet <rhyme label="a" type="m" corresp="#s21:l1">erfelijck</rhyme>.</l>
        <l id="s21:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s21:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s21:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Erfelijck hy't geven <rhyme label="a" type="m">wou</rhyme></l>
        <l id="s22:l2" met="4" type="+-">Israël syn dienaer <rhyme label="a" type="m" corresp="#s22:l1">trou</rhyme>.</l>
        <l id="s22:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s22:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s22:l3">eeuwicheyt</rhyme></l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Die alsdan aen ons <rhyme label="a" type="m">gedenckt</rhyme></l>
        <l id="s23:l2" met="4" type="+-">Als wy werden meest <rhyme label="a" type="m" corresp="#s23:l1">gecrenckt</rhyme>.</l>
        <l id="s23:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s23:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s23:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Die ons heeft gemaket <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s24:l2" met="4" type="+-">Van ons harde wee'r <rhyme label="a" type="m" corresp="#s24:l1">party</rhyme>.</l>
        <l id="s24:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s24:l4" met="4" type="+-">Dueret inder <rhyme label="b" type="m" corresp="#s24:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">Die het noodich voetsel <rhyme label="a" type="m">geeft</rhyme></l>
        <l id="s25:l2" met="4" type="+-">Alles wat ter werelt <rhyme label="a" type="m" corresp="#s25:l1">leeft</rhyme>.</l>
        <l id="s25:l3" met="4" type="+-">Want syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s25:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s25:l3">eeuwicheyt</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">Looft den Heere <rhyme label="a" type="m">al-gelijck</rhyme>,</l>
        <l id="s26:l2" met="4" type="+-">Hy is Heer van <rhyme label="a" type="m" corresp="#s26:l1">hemelrijck</rhyme>.</l>
        <l id="s26:l3" met="4" type="+-">En syn <rhyme label="b" type="m">goedertierenheyt</rhyme></l>
        <l id="s26:l4" met="4" type="+-">Dueret in der <rhyme label="b" type="m" corresp="#s26:l3">eeuwicheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
