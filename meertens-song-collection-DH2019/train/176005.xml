    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 047 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">VOlcken over <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Maeckt een bly <rhyme label="a" type="m" corresp="#s1:l1">geschal</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Slaet de handen <rhyme label="b" type="m">tsaem</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Lovet Godes <rhyme label="b" type="m" corresp="#s1:l3">naem</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Met een helle <rhyme label="c" type="m">stem</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Vrolijck juychet <rhyme label="c" type="m" corresp="#s1:l5">hem</rhyme>.</l>
        <l id="s1:l7" met="3" type="+-">Want hy is de <rhyme label="d" type="m">Heer</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Hooch van macht en <rhyme label="d" type="m" corresp="#s1:l7">eer</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">En <rhyme label="e" type="m">verschrickelijck</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Die het gants <rhyme label="e" type="m" corresp="#s1:l9">aertrijck</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Als een Coninck <rhyme label="f" type="m">stelt</rhyme></l>
        <l id="s1:l12" met="3" type="+-">Onder syn <rhyme label="f" type="m" corresp="#s1:l11">gewelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Onder onse <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Heeft de Heer <rhyme label="a" type="m" corresp="#s2:l1">gebracht</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Volcken, die wy <rhyme label="b" type="m">sien</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Datse hare <rhyme label="b" type="m" corresp="#s2:l3">knien</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Buygen met <rhyme label="c" type="m">ootmoet</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Vallend' ons te <rhyme label="c" type="m" corresp="#s2:l5">voet</rhyme>.</l>
        <l id="s2:l7" met="3" type="+-">Hy ist die ons <rhyme label="d" type="m">jont</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Onse erf en <rhyme label="d" type="m" corresp="#s2:l7">gront</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">En ons <rhyme label="e" type="m">toebereydt</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Iacobs <rhyme label="e" type="m" corresp="#s2:l9">heerlijckheyt</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Dien hy als zijn <rhyme label="f" type="m">kint</rhyme></l>
        <l id="s2:l12" met="3" type="+-">Hertelijck <rhyme label="f" type="m" corresp="#s2:l11">bemint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Onse God vaert <rhyme label="a" type="m">op</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Met een <rhyme label="a" type="m" corresp="#s3:l1">hant-geclop</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Met <rhyme label="b" type="m">triumph-gesanck</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Met <rhyme label="b" type="m" corresp="#s3:l3">basuynen-clanck</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Diemen spelen <rhyme label="c" type="m">hoort</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Met een soet <rhyme label="c" type="m" corresp="#s3:l5">accoort</rhyme>.</l>
        <l id="s3:l7" met="3" type="+-">Singet, singt den <rhyme label="d" type="m">Heer</rhyme>.</l>
        <l id="s3:l8" met="3" type="+-">Singet, singt syn <rhyme label="d" type="m" corresp="#s3:l7">eer</rhyme>.</l>
        <l id="s3:l9" met="3" type="+-">Maeckt syn lof <rhyme label="e" type="m">bekent</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Want tot swerelts <rhyme label="e" type="m" corresp="#s3:l9">end</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Streckt hem het <rhyme label="f" type="m">gebot</rhyme></l>
        <l id="s3:l12" met="3" type="+-">Van den grooten <rhyme label="f" type="m" corresp="#s3:l11">God</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Singt met <rhyme label="a" type="m">onderwijs</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Tot des Heeren <rhyme label="a" type="m" corresp="#s4:l1">prijs</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Die regeert <rhyme label="b" type="m">ront-om</rhyme></l>
        <l id="s4:l4" met="3" type="+-">In het <rhyme label="b" type="m" corresp="#s4:l3">heyden-dom</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Sittend' op syn <rhyme label="c" type="m">throon</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Heylich ende <rhyme label="c" type="m" corresp="#s4:l5">schoon</rhyme>.</l>
        <l id="s4:l7" met="3" type="+-">d' Edelen der <rhyme label="d" type="m">aerd'</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Hebben haer <rhyme label="d" type="m" corresp="#s4:l7">vergaert</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Voor zijn claer <rhyme label="e" type="m">aenschijn</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Om te mogen <rhyme label="e" type="m" corresp="#s4:l9">syn</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Onder het <rhyme label="f" type="m">geslacht</rhyme></l>
        <l id="s4:l12" met="3" type="+-">Abrahams <rhyme label="f" type="m" corresp="#s4:l11">geacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
