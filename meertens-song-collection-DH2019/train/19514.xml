  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LIgte Klaartje streelde <rhyme label="a" type="f">Pieter</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Naar de mode van het <rhyme label="b" type="m">Hof</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Dats geseit, met weinig <rhyme label="b" type="m" corresp="#s1:l2">lof</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">'t Meisje malde, 't Knegje stiet <rhyme label="a" type="f" corresp="#s1:l1">'er</rhyme></l>
        <l id="s1:l5" met="4" type="+-">After over in 'et <rhyme label="c" type="m">Hooi</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Juist komt Besje uit de <rhyme label="c" type="m" corresp="#s1:l5">Kooi</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Daarse lagen met haar <rhyme label="a" type="f">beien</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En se roept, met groot <rhyme label="b" type="m">geluit</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Buren, Buren, ogh kom <rhyme label="b" type="m" corresp="#s2:l2">uit</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">Helme dese Mense <rhyme label="a" type="f" corresp="#s2:l1">scheien</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Buren, Buren kom, ogh! <rhyme label="c" type="m">ogh</rhyme>!</l>
        <l id="s2:l6" met="4" type="+-">Klaar vermoort Hy, helpme <rhyme label="c" type="m" corresp="#s2:l5">togh</rhyme>.</l>
      </lg>
    </lg>
  </text>
