<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4124</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Comtesse de Corsol</melody>
  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE <rhyme label="a" type="m">Koninglijke-Dans</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">(Vol <rhyme label="a" type="m" corresp="#s1:l1">heymelijke-glans</rhyme>)</l>
        <l id="s1:l3" met="3" type="-+">Ver-nuwd in ons <rhyme label="b" type="f">gedachten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">De <rhyme label="c" type="m">Vorstelijke-vreugd</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">In Israël <rhyme label="c" type="m" corresp="#s1:l4">verheugd</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door 't <rhyme label="b" type="f" corresp="#s1:l3">Goddelijk-betrachten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De <rhyme label="a" type="m">Priesterlijke-Staf</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die veel <rhyme label="a" type="m" corresp="#s2:l1">Amand'len-gaf</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En Jacobs-Saad <rhyme label="b" type="f">vermaande</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">De Bond-Kist die haar <rhyme label="c" type="m">wees</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Gods by-zijn: en hem <rhyme label="c" type="m" corresp="#s2:l4">prees</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Maakt onse sinnen <rhyme label="b" type="f" corresp="#s2:l3">gaande</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Na het <rhyme label="a" type="m">waarachtig-Beeld</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">De Vaders <rhyme label="a" type="m" corresp="#s3:l1">voor-gespeeld</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Door schaduwen, der <rhyme label="b" type="f">dingen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die namaals souden <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s3:l5" met="3" type="-+">In waarheyt: boven <rhyme label="c" type="m" corresp="#s3:l4">schijn</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Daar d' Eng'len nu van <rhyme label="b" type="f" corresp="#s3:l3">singen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'t Waarachtig <rhyme label="a" type="m">Vrouwen.Saad</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat (willig) op sich <rhyme label="a" type="m" corresp="#s4:l1">laad</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Den last der <rhyme label="b" type="f">uyt-verkoren</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Dat ons uyt <rhyme label="c" type="m">Abraham</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Iuda, en Davids <rhyme label="c" type="m" corresp="#s4:l4">Stam</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In Bethlem, is <rhyme label="b" type="f" corresp="#s4:l3">geboren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Een Maagd heeft hem <rhyme label="a" type="m">gebaard</rhyme>:</l>
        <l id="s5:l2" met="3" type="-+">Dies is hy (uyt den <rhyme label="a" type="m" corresp="#s5:l1">aart</rhyme>)</l>
        <l id="s5:l3" met="3" type="-+">Een Mensch, een Soon des <rhyme label="b" type="f">Menschen</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Wiens <rhyme label="c" type="m">an-gename-dach</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Den vriend des Heeren <rhyme label="c" type="m" corresp="#s5:l4">sach</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Na sijn begeert, en <rhyme label="b" type="f" corresp="#s5:l3">wenschen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Siet: heden is dit <rhyme label="a" type="m">Heyl</rhyme></l>
        <l id="s6:l2" met="3" type="-+">De Arme-sondaars <rhyme label="a" type="m" corresp="#s6:l1">veyl</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">In Adams-val <rhyme label="b" type="f">verloren</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Om dat dien <rhyme label="c" type="m">Vrede-Vorst</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Des Werelds sonden <rhyme label="c" type="m" corresp="#s6:l4">torst</rhyme>:</l>
        <l id="s6:l6" met="3" type="-+">En stut sijns Vaders <rhyme label="b" type="f" corresp="#s6:l3">toren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Al 't geen oyt is <rhyme label="a" type="m">voorseyd</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Voor-beeld, en <rhyme label="a" type="m" corresp="#s7:l1">uyt-geleyd</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Beloofd, en <rhyme label="b" type="f">toe-gesworen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Van Gods <rhyme label="c" type="m">Gesalfde-Knect</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Is nu, in hem, te <rhyme label="c" type="m" corresp="#s7:l4">recht</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Vervuld: voor yders <rhyme label="b" type="f" corresp="#s7:l3">oren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hy is des <rhyme label="a" type="m">Levens-Boom</rhyme>:</l>
        <l id="s8:l2" met="3" type="-+">De <rhyme label="a" type="m" corresp="#s8:l1">levendige-Stroom</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">De Son, het Licht vol <rhyme label="b" type="f">klaarheyd</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">De Star, die luchtig <rhyme label="c" type="m">schijnt</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">So dat de nacht <rhyme label="c" type="m" corresp="#s8:l4">verdwijnd</rhyme>:</l>
        <l id="s8:l6" met="3" type="-+">De Wech, de Deur, de <rhyme label="b" type="f" corresp="#s8:l3">Waarheyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wanneer ik in 't <rhyme label="a" type="m">geheel</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En ook in yder <rhyme label="a" type="m" corresp="#s9:l1">deel</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Betrachte sijn <rhyme label="b" type="f">Mensch-Warding</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Om d' uytverkoor'nen <rhyme label="c" type="m">Gods</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Te vesten, op de <rhyme label="c" type="m" corresp="#s9:l4">rots</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Van haar geloofs <rhyme label="b" type="f" corresp="#s9:l3">vol-harding</rhyme>:</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">So spreekt mijn harte <rhyme label="a" type="m">dan</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Dat dit ziel-voedend <rhyme label="a" type="m" corresp="#s10:l1">Man</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Is (wenschelijk) <rhyme label="b" type="f">geregend</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Op 't <rhyme label="c" type="m">heerlijk-Palastijn</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Waar door de Heyd'nen <rhyme label="c" type="m" corresp="#s10:l4">zijn</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Na Ziel, en Lijf <rhyme label="b" type="f" corresp="#s10:l3">gezegend</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Treed-toe (ô <rhyme label="a" type="m">Christen-schaar</rhyme>)</l>
        <l id="s11:l2" met="3" type="-+">Tot desen <rhyme label="a" type="m" corresp="#s11:l1">Middelaar</rhyme>:</l>
        <l id="s11:l3" met="3" type="-+">En rust op sijn <rhyme label="b" type="f">voldoening</rhyme>.</l>
        <l id="s11:l4" met="3" type="-+">Die in sich heeft <rhyme label="c" type="m">gewrocht</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">En yder <rhyme label="c" type="m" corresp="#s11:l4">toe-gebrocht</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Een <rhyme label="b" type="f" corresp="#s11:l3">eeuwige-versoening</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
