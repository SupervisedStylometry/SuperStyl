    <tags/>
    <melody>Nu wees gegroet Maria zoet</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy gheestelicke Maechden <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wilt met Lofsanghen <rhyme label="b" type="f">prijsen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Maria die reyn maghet <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt haer doch eer’ <rhyme label="b" type="f" corresp="#s1:l2">bewijsen</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Maria die reyn maghet <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wilt haer doch eer’ <rhyme label="b" type="f" corresp="#s1:l2">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt haer altijdt naer mijn <rhyme label="a" type="m">bedien</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als maeght en moeder <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die van t’beghinsel was <rhyme label="a" type="m" corresp="#s2:l1">voor-sien</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Tot een Moeder des <rhyme label="b" type="f" corresp="#s2:l2">Heeren</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Die van t’beghinsel was <rhyme label="a" type="m" corresp="#s2:l1">voor-sien</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Tot een Moeder des <rhyme label="b" type="f" corresp="#s2:l2">Heeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy is die maghet sonder <rhyme label="a" type="m">smet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een Vrouw’ van groote <rhyme label="b" type="f">krachten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">T’hooft des serpents heeft sy <rhyme label="a" type="m" corresp="#s3:l1">verplet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Eert haer alle <rhyme label="b" type="f" corresp="#s3:l2">gheslachten</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">T’hooft des serpents heeft sy <rhyme label="a" type="m" corresp="#s3:l1">verplet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Eert haer alle <rhyme label="b" type="f" corresp="#s3:l2">gheslachten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Esaias die heeft van <rhyme label="a" type="m">haer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghepropheteert <rhyme label="b" type="f">waerachtich</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat sy ons baren soud’ <rhyme label="a" type="m" corresp="#s4:l1">voorwaer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den Sone Godts <rhyme label="b" type="f" corresp="#s4:l2">almachtich</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Dat sy ons baren soud’ <rhyme label="a" type="m" corresp="#s4:l1">voorwaer</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Den Sone Godts <rhyme label="b" type="f" corresp="#s4:l2">almachtich</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sy was verciert die suyuer <rhyme label="a" type="m">maecht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met deuchden <rhyme label="b" type="f">overvloedich</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Maer sonderlingh’ heeft Godt <rhyme label="a" type="m" corresp="#s5:l1">behaecht</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Om dat sy was <rhyme label="b" type="f" corresp="#s5:l2">ootmoedich</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Maer sonderlingh’ heeft Godt <rhyme label="a" type="m" corresp="#s5:l1">behaecht</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Om dat sy was <rhyme label="b" type="f" corresp="#s5:l2">ootmoedich</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Gheen tonghe kan uytspreken, <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Haer deucht, en heylich <rhyme label="b" type="f">leuen</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Een herte kan’t bedencken <rhyme label="a" type="m" corresp="#s6:l1">niet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hoe sy nu is <rhyme label="b" type="f" corresp="#s6:l2">verheuen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Een herte kan’t bedencken <rhyme label="a" type="m" corresp="#s6:l1">niet</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Hoe sy nu is <rhyme label="b" type="f" corresp="#s6:l2">verheuen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dus prijst haer maechden <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Haer deuchden wilt <rhyme label="b" type="f">aenmercken</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Wilt ghy met haer in vreuchden <rhyme label="a" type="m" corresp="#s7:l1">zijn</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Volght naer haer goede <rhyme label="b" type="f" corresp="#s7:l2">wercken</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Wilt ghy met haer in vreuchden <rhyme label="a" type="m" corresp="#s7:l1">zijn</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Volght naer haer goede <rhyme label="b" type="f" corresp="#s7:l2">wercken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
