<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19894</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="danklied" translation="gratitude song">danklied</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Johan van Born</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LAet ons met hel <rhyme label="a" type="m">geklangh</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Godt singen een <rhyme label="a" type="m" corresp="#s1:l1">gesangh</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Voor sijnen milden <rhyme label="b" type="f">seghen</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Die alles watter <rhyme label="c" type="m">leeft</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Haer spijs en voedsel <rhyme label="c" type="m" corresp="#s1:l4">geeft</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Den soeten douw en <rhyme label="b" type="f" corresp="#s1:l3">regen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want al (om onsent <rhyme label="a" type="m">wil</rhyme>)</l>
        <l id="s2:l2" met="3" type="-+">Geeft Godt gerust en <rhyme label="a" type="m" corresp="#s2:l1">stil</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Het geen wy konnen <rhyme label="b" type="f">wenschen</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Al wat de rijcke <rhyme label="c" type="m">zee</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Voor leckerny brengt <rhyme label="c" type="m" corresp="#s2:l4">mee</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">'t Is alles voor de <rhyme label="b" type="f" corresp="#s2:l3">menschen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo haest de jonge <rhyme label="a" type="m">vrucht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Des Moeder schept de <rhyme label="a" type="m" corresp="#s3:l1">lucht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Is hem zijn voedsel <rhyme label="b" type="f">open</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Terstont komt Godes <rhyme label="c" type="m">hand</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Hem leyden in een <rhyme label="c" type="m" corresp="#s3:l4">Land</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Daer alles is te <rhyme label="b" type="f" corresp="#s3:l3">koopen</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ons Moeder, d'Aerden <rhyme label="a" type="m">schoot</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Al watse voed en <rhyme label="a" type="m" corresp="#s4:l1">poot</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Al watse schat voor <rhyme label="b" type="f">vruchten</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">'t Is alles voor de <rhyme label="c" type="m">mensch</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Of tot sijn lust of <rhyme label="c" type="m" corresp="#s4:l4">wensch</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Tot nut of tot <rhyme label="b" type="f" corresp="#s4:l3">genuchten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Jae wat de Locht <rhyme label="a" type="m">doorsnijt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En op sijn wiecken <rhyme label="a" type="m" corresp="#s5:l1">rijt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Is voor den Mensch ten <rhyme label="b" type="f">besten</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Self 't ongetemde <rhyme label="c" type="m">Vee</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">In dal en Bergen <rhyme label="c" type="m" corresp="#s5:l4">mee</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">In Bosschen of <rhyme label="b" type="f" corresp="#s5:l3">gewesten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Het Coren dient tot <rhyme label="a" type="m">spijs</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Tot dranck gelijcker <rhyme label="a" type="m" corresp="#s6:l1">wijs</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">De wijn-ranck is sijn <rhyme label="b" type="f">jeughde</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">De Os, het Schaep, en 't <rhyme label="c" type="m">Swijn</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">De Haes en 't vet <rhyme label="c" type="m" corresp="#s6:l4">Conijn</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dient alles tot sijn <rhyme label="b" type="f" corresp="#s6:l3">vreughde</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">'t Ondanckbaer Adams <rhyme label="a" type="m">saet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dit niet eens gade <rhyme label="a" type="m" corresp="#s7:l1">slaet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Maer blijft in 't aerdsche <rhyme label="b" type="f">hangen</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">En woelt hier alsoo <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Jae 't kent naeuw sijnen <rhyme label="c" type="m" corresp="#s7:l4">Heer</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">Van wien hy 't heeft <rhyme label="b" type="f" corresp="#s7:l3">ontfangen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Maer een herboren <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">De God den Heere <rhyme label="a" type="m" corresp="#s8:l1">vreest</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Brenght op de <rhyme label="b" type="f">Reuck-Altaren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Der offer van 't <rhyme label="c" type="m">gemoed</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En danckt Godt die hem <rhyme label="c" type="m" corresp="#s8:l4">voed</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En eeuwigh sal <rhyme label="b" type="f" corresp="#s8:l3">bewaren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
