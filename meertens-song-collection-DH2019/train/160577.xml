<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160577</id>
    <date>
      <year-start>1769</year-start>
      <year-end>1769</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Moet ik nu nog langer <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Cupido in deeze <rhyme label="b" type="m">stryd</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wild myn verligtenis <rhyme label="a" type="f" corresp="#s1:l1">geven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In myne Ionge <rhyme label="b" type="m" corresp="#s1:l2">tyd</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ik zal 't den Hemel <rhyme label="c" type="f">klagen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En ook het <rhyme label="d" type="m">Firmament</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat ik myne jonge <rhyme label="c" type="f" corresp="#s1:l5">Dagen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Moet verslyte in <rhyme label="d" type="m" corresp="#s1:l6">elend</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den Herder dien ik <rhyme label="a" type="f">minde</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En dien ik zwoer myn <rhyme label="b" type="m">Trouw</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Onder deez' groene <rhyme label="a" type="f" corresp="#s2:l1">Linde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maer eylaas verlaet myn <rhyme label="b" type="m" corresp="#s2:l2">nouw</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hy die myn Schaepjes <rhyme label="c" type="f">hoede</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met vreugd zoo menig <rhyme label="d" type="m">nagt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Terwyl dat ik in <rhyme label="c" type="f" corresp="#s2:l5">ruste</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Met vreugde by hem <rhyme label="d" type="m" corresp="#s2:l6">lag</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maer na veel zoete <rhyme label="a" type="f">reeden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zoo blyft hy van myn <rhyme label="b" type="m">af</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dat zal myn nog doen <rhyme label="a" type="f" corresp="#s3:l1">treeden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Tot in het duyster <rhyme label="b" type="m" corresp="#s3:l2">Graf</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">ô Boomen zoete <rhyme label="c" type="f">Vrugten</rhyme>!</l>
        <l id="s3:l6" met="3" type="-+">ô Waterstroomen <rhyme label="d" type="m">zoet</rhyme>!</l>
        <l id="s3:l7" met="3" type="-+">Voldoet dog myne <rhyme label="c" type="f" corresp="#s3:l5">lusten</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Eer dat ik sterven <rhyme label="d" type="m" corresp="#s3:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ach Herder dien ik <rhyme label="a" type="f">minne</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gy hebt myn hert <rhyme label="b" type="m">doorwond</rhyme>!</l>
        <l id="s4:l3" met="3" type="-+">Gy noemde myn <rhyme label="a" type="f" corresp="#s4:l1">Vriendinne</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gy kuste myn <rhyme label="b" type="m" corresp="#s4:l2">Mond</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En uwe zoete <rhyme label="c" type="f">woorden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Bekoorden myne <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En uwe <rhyme label="c" type="f" corresp="#s4:l5">Minnelonken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">dwongen my tot de <rhyme label="d" type="m" corresp="#s4:l6">Min</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Zult gy dan nog langer <rhyme label="a" type="f">toeven</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Te komen eens by <rhyme label="b" type="m">myn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat zal myn nog <rhyme label="a" type="f" corresp="#s5:l1">bedroeven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hebt deernis met <rhyme label="b" type="m" corresp="#s5:l2">pyn</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Stuur al uw <rhyme label="c" type="f">Minneklagten</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Naer uwe <rhyme label="d" type="m">Minnares</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Die uw hier zal <rhyme label="c" type="f" corresp="#s5:l5">verwagten</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">By deeze <rhyme label="d" type="m" corresp="#s5:l6">Waterples</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De zon verligt zyn <rhyme label="a" type="f">Straalen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dus blyft zy in haar <rhyme label="b" type="m">rust</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Onder deez' Groene <rhyme label="a" type="f" corresp="#s6:l1">hagen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Daer neem ik myne <rhyme label="b" type="m" corresp="#s6:l2">rust</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En dat hy myn <rhyme label="c" type="f">ontwaekte</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Wat vreugd was dat voor <rhyme label="d" type="m">myn</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat hy myn wakker <rhyme label="c" type="f" corresp="#s6:l5">maakte</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Gedaen was al myn <rhyme label="d" type="m" corresp="#s6:l6">pyn</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ras op dan <rhyme label="a" type="f">Engelinne</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">'k heb uw zoo lang <rhyme label="b" type="m">gezogt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Gy weet dat ik u <rhyme label="a" type="f" corresp="#s7:l1">minde</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wat hebt gy dus <rhyme label="b" type="m" corresp="#s7:l2">gedogt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Dat ik niet ben <rhyme label="c" type="f">gekoomen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Op de bestemden <rhyme label="d" type="m">tyd</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Ik heb u zoo lang <rhyme label="c" type="f" corresp="#s7:l5">vernoomen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Schelt dog al myn schulden <rhyme label="d" type="m" corresp="#s7:l6">quyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ik heb u zoo lang <rhyme label="a" type="f">verkooren</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Lief voor myn Egte <rhyme label="b" type="m">Vrouw</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wy zyn voor een <rhyme label="a" type="f" corresp="#s8:l1">gebooren</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Ziet ik blyf u <rhyme label="b" type="m" corresp="#s8:l2">getrouw</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Laet droefhied ende <rhyme label="c" type="f">zugten</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Maer vaaren 't is nog <rhyme label="d" type="m">tyd</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Wy zullen met <rhyme label="c" type="f" corresp="#s8:l5">genugten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Leven in <rhyme label="d" type="m" corresp="#s8:l6">Eerbaerheid</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Komt Herders by <rhyme label="a" type="f">malkanden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Op deeze Bruylofts <rhyme label="b" type="m">feest</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Van daeg zoo zal <rhyme label="a" type="f" corresp="#s9:l1">Leander</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Weest vrolyk om het <rhyme label="b" type="m" corresp="#s9:l2">meest</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Wilt spelen op de <rhyme label="c" type="f">Fluyten</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Brengt Bas, Fioolen <rhyme label="d" type="m">by</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Komt Coridon met <rhyme label="c" type="f" corresp="#s9:l5">Luyten</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">En speel een <rhyme label="d" type="m" corresp="#s9:l6">Melody</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
