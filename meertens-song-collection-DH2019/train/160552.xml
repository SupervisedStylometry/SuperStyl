<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160552</id>
    <date>
      <year-start>1690</year-start>
      <year-end>1690</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Lofzang van Maria</melody>
  <author>Barent Pietersz. Kompas</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn oorspronck is den <rhyme label="a" type="m">Heer</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">Dies jaeght mijn herte <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die Heyl-fonteyn te <rhyme label="b" type="f">vinden</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Die levend-water <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Waer door men eeuwigh <rhyme label="c" type="m" corresp="#s1:l4">leeft</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat 's Jesus, mijn <rhyme label="b" type="f" corresp="#s1:l3">Beminden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ach was ick maer soo <rhyme label="a" type="m">vert</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Dat mijn hartjagent <rhyme label="a" type="m" corresp="#s2:l1">hert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dien Heylandt mocht <rhyme label="b" type="f">beoogen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoe soud' ick dagh en <rhyme label="c" type="m">nacht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Uyt aller zielen <rhyme label="c" type="m" corresp="#s2:l4">kracht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">My derwaerts heenen <rhyme label="b" type="f" corresp="#s2:l3">poogen</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Elck die beooght hier <rhyme label="a" type="m">iet</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">Daer hy op doeld, en <rhyme label="a" type="m" corresp="#s3:l1">siet</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Ick star-oogh door 't <rhyme label="b" type="f">Geloove</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Op iets van meerder <rhyme label="c" type="m">waerd'</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Als d'Hemel en de <rhyme label="c" type="m" corresp="#s3:l4">Aerd'</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat 's op mijn Godt, hier <rhyme label="b" type="f" corresp="#s3:l3">boven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In welck volmaeckste <rhyme label="a" type="m">Goedt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Mijn ziele rusten <rhyme label="a" type="m" corresp="#s4:l1">moet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Naer al 't verdriet'lijck <rhyme label="b" type="f">slaaven</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Dat hier op d' Aerd' <rhyme label="c" type="m">geschiedt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Daer niemandt recht <rhyme label="c" type="m" corresp="#s4:l4">geniet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">De rust van 's Hemels <rhyme label="b" type="f" corresp="#s4:l3">Haaven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Kleyn is al 't groot, (hoe <rhyme label="a" type="m">groot</rhyme>)</l>
        <l id="s5:l2" met="3" type="-+">Dat hier den Mensch ter <rhyme label="a" type="m" corresp="#s5:l1">noodt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Ter vreughde mach <rhyme label="b" type="f">gebeuren</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Daer nochtans al wat <rhyme label="c" type="m">leeft</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Sich vierigh toe <rhyme label="c" type="m" corresp="#s5:l4">begeeft</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Dat nietigh naer te <rhyme label="b" type="f" corresp="#s5:l3">speuren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">En daerom heeft het <rhyme label="a" type="m">reen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat ick het noodighst <rhyme label="a" type="m" corresp="#s6:l1">een</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Met hert en ziel <rhyme label="b" type="f">aenkleeve</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Dat ick my derwaerts <rhyme label="c" type="m">wend'</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Naer mijn ziels <rhyme label="c" type="m" corresp="#s6:l4">Element</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Om eeuwigh in te <rhyme label="b" type="f" corresp="#s6:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Fijn goudt, hoe goedt van <rhyme label="a" type="m">stof</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hoe dierbaer, hoogh in <rhyme label="a" type="m" corresp="#s7:l1">lof</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Is dreck, by die <rhyme label="b" type="f">Iuweelen</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Die mijn ziels <rhyme label="c" type="m">Bruydegom</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Soo haest ick tot hem <rhyme label="c" type="m" corresp="#s7:l4">kom</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Uyt Liefd' my mee sal <rhyme label="b" type="f" corresp="#s7:l3">deelen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Rijckhalsend' soeck ick <rhyme label="a" type="m">dan</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Soo veel ick immers <rhyme label="a" type="m" corresp="#s8:l1">kan</rhyme></l>
        <l id="s8:l3" met="3" type="-+">My derwaerts heen te <rhyme label="b" type="f">spoeden</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Daer 't wesentlijcke <rhyme label="c" type="m">goedt</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Ia 't volmaeckt Hemels <rhyme label="c" type="m" corresp="#s8:l4">soet</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Soo stroomt, met volle <rhyme label="b" type="f" corresp="#s8:l3">vloeden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Al waer in <rhyme label="a" type="m">eeuwigheydt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Geen hertseer, druck, noch <rhyme label="a" type="m" corresp="#s9:l1">strijdt</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Sich meer sal <rhyme label="b" type="f">openbaaren</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Maer daer in <rhyme label="c" type="m">tegendeel</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">'t Volmaeckte all', <rhyme label="c" type="m" corresp="#s9:l4">geheel</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Sal sonne-klaer <rhyme label="b" type="f" corresp="#s9:l3">verklaaren</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Naer dien 't soo waerlijck <rhyme label="a" type="m">is</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Met 's Hemels <rhyme label="a" type="m" corresp="#s10:l1">ervenis</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En 't zaligh deel der <rhyme label="b" type="f">vroomen</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">Soo dient de wegh <rhyme label="c" type="m">gesocht</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En 't noodighst werck <rhyme label="c" type="m" corresp="#s10:l4">volbrocht</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Van die daer wille <rhyme label="b" type="f" corresp="#s10:l3">komen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Soo vierigh als den <rhyme label="a" type="m">Mensch</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Sijns herten lust en <rhyme label="a" type="m" corresp="#s11:l1">wensch</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">In 't Aertsch soeckt naer te <rhyme label="b" type="f">jagen</rhyme>:</l>
        <l id="s11:l4" met="3" type="-+">Soo moet den <rhyme label="c" type="m">Hemelingh</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Af sien van al 't <rhyme label="c" type="m" corresp="#s11:l4">geringh</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En meest naer 't waerdighst <rhyme label="b" type="f" corresp="#s11:l3">vragen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Elck proef dan zijn <rhyme label="a" type="m">gemoedt</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Waer 't meest door werdt <rhyme label="a" type="m" corresp="#s12:l1">gevoedt</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Door 't min, of 't meer van <rhyme label="b" type="f">waerde</rhyme>?</l>
        <l id="s12:l4" met="3" type="-+">Of 't Hemels boven <rhyme label="c" type="m">drijft</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Dan, of men altijdt <rhyme label="c" type="m" corresp="#s12:l4">blijft</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Geanckert aen de <rhyme label="b" type="f" corresp="#s12:l3">Aerde</rhyme>?</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Van al te groot <rhyme label="a" type="m">gewicht</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Is 't Hemels, om 't soo <rhyme label="a" type="m" corresp="#s13:l1">licht</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Als 't Aertsch, te late <rhyme label="b" type="f">weegen</rhyme>.</l>
        <l id="s13:l4" met="3" type="-+">Ach wierdt dit recht <rhyme label="c" type="m">bedacht</rhyme>!</l>
        <l id="s13:l5" met="3" type="-+">En 't waerdighst meest <rhyme label="c" type="m" corresp="#s13:l4">betracht</rhyme>!</l>
        <l id="s13:l6" met="3" type="-+">Den Hemel droop van <rhyme label="b" type="f" corresp="#s13:l3">zeegen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Als een volkomen <rhyme label="a" type="m">Zee</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Sou Godes gunst en <rhyme label="a" type="m" corresp="#s14:l1">vree</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Geduurigh blijven <rhyme label="b" type="f">stroomen</rhyme>:</l>
        <l id="s14:l4" met="3" type="-+">In tijdt en <rhyme label="c" type="m">eeuwigheydt</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">O wie sich wel <rhyme label="c" type="m" corresp="#s14:l4">bereydt</rhyme>!</l>
        <l id="s14:l6" met="3" type="-+">Sal Godt en 't heyl <rhyme label="b" type="f" corresp="#s14:l3">bekoomen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Niets Heer! bedroeft my <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Als dat ick noch soo <rhyme label="a" type="m" corresp="#s15:l1">veer</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Van u blijf <rhyme label="b" type="f">afgescheyden</rhyme>;</l>
        <l id="s15:l4" met="3" type="-+">Dat mijn verdorven <rhyme label="c" type="m">vleys</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">My in mijn Hemels <rhyme label="c" type="m" corresp="#s15:l4">reys</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Geduurigh wil <rhyme label="b" type="f" corresp="#s15:l3">verleyden</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Daer ick my door 't <rhyme label="a" type="m">Gebedt</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Wel dickmaels tegen <rhyme label="a" type="m" corresp="#s16:l1">set</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Met waecken, worst'len, <rhyme label="b" type="f">strijden</rhyme>:</l>
        <l id="s16:l4" met="3" type="-+">Maer d' vyandt blijft te <rhyme label="c" type="m">sterck</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">En ick in 't Hemels <rhyme label="c" type="m" corresp="#s16:l4">werck</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Te swack, om veel te <rhyme label="b" type="f" corresp="#s16:l3">lijden</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">En schoon ich neem al <rhyme label="a" type="m">voor</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Om recht te breecke <rhyme label="a" type="m" corresp="#s17:l1">door</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Om 't Hemels spoor te <rhyme label="b" type="f">houwen</rhyme>.</l>
        <l id="s17:l4" met="3" type="-+">O 't is al niet met <rhyme label="c" type="m">al</rhyme>!</l>
        <l id="s17:l5" met="3" type="-+">Ick struyckel, of ick <rhyme label="c" type="m" corresp="#s17:l4">val</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">Men kan op my niet <rhyme label="b" type="f" corresp="#s17:l3">bouwen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Naer dien ick dan <rhyme label="a" type="m">bevind'</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Hoe kreupel, lam en <rhyme label="a" type="m" corresp="#s18:l1">blindt</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Ick geest'lijck werdt <rhyme label="b" type="f">bevonden</rhyme>.</l>
        <l id="s18:l4" met="3" type="-+">Soo kom ick, Heer, tot <rhyme label="c" type="m">u</rhyme>!</l>
        <l id="s18:l5" met="3" type="-+">Ick bidde helpt my <rhyme label="c" type="m" corresp="#s18:l4">nu</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Van 't lastigh pack der <rhyme label="b" type="f" corresp="#s18:l3">sonden</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Bestraelt my met u <rhyme label="a" type="m">geest</rhyme>:</l>
        <l id="s19:l2" met="3" type="-+">Die alle quael <rhyme label="a" type="m" corresp="#s19:l1">geneest</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Der arme <rhyme label="b" type="f">sondaressen</rhyme>.</l>
        <l id="s19:l4" met="3" type="-+">Wilt Heer, door u Soons <rhyme label="c" type="m">Bloedt</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">(Dat sulck Miraeckel <rhyme label="c" type="m" corresp="#s19:l4">doet</rhyme>)</l>
        <l id="s19:l6" met="3" type="-+">Mijn arme ziele <rhyme label="b" type="f" corresp="#s19:l3">lessen</rhyme>!</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Reyckt my u rechter <rhyme label="a" type="m">handt</rhyme>!</l>
        <l id="s20:l2" met="3" type="-+">Terwijl ick geen <rhyme label="a" type="m" corresp="#s20:l1">bestant</rhyme></l>
        <l id="s20:l3" met="3" type="-+">In mijne beenders <rhyme label="b" type="f">vinden</rhyme>.</l>
        <l id="s20:l4" met="3" type="-+">Wie is soo swack als <rhyme label="c" type="m">ick</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">Die schier elck <rhyme label="c" type="m" corresp="#s20:l4">oogenblick</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">Weer val, als d' <rhyme label="b" type="f" corresp="#s20:l3">onbesinde</rhyme>?</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">O Vader! siet my <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">Als een die heb <rhyme label="a" type="m" corresp="#s21:l1">misdaen</rhyme>:</l>
        <l id="s21:l3" met="3" type="-+">Doch weder soeck te <rhyme label="b" type="f">keeren</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Met den verlooren <rhyme label="c" type="m">Soon</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Alsoo ick uwen <rhyme label="c" type="m" corresp="#s21:l4">Throon</rhyme></l>
        <l id="s21:l6" met="3" type="-+">Niet eeuwigh mach <rhyme label="b" type="f" corresp="#s21:l3">ontbeeren</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Ey! leght u Ringh, en 't <rhyme label="a" type="m">Kleedt</rhyme></l>
        <l id="s22:l2" met="3" type="-+">O Vader! weer <rhyme label="a" type="m" corresp="#s22:l1">gereedt</rhyme>.</l>
        <l id="s22:l3" met="3" type="-+">Wilt het gemest doen <rhyme label="b" type="f">slachten</rhyme>,</l>
        <l id="s22:l4" met="3" type="-+">'k Verlaet d' Aerdts <rhyme label="c" type="m">Verckens-trogh</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Ick soeck dijn Hemel, <rhyme label="c" type="m" corresp="#s22:l4">och</rhyme>!</l>
        <l id="s22:l6" met="3" type="-+">Wilt mijn daer in <rhyme label="b" type="f" corresp="#s22:l3">verwachten</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">Christus, mijns levens <rhyme label="a" type="m">Bron</rhyme>,</l>
        <l id="s23:l2" met="3" type="-+">Mijn Heylandt, mijn siels <rhyme label="a" type="m" corresp="#s23:l1">Son</rhyme>,</l>
        <l id="s23:l3" met="3" type="-+">Mijn Heer, mijn Godt, mijn <rhyme label="b" type="f">Herder</rhyme>!</l>
        <l id="s23:l4" met="3" type="-+">Soeckt my u Schaepken <rhyme label="c" type="m">teer</rhyme>,</l>
        <l id="s23:l5" met="3" type="-+">Vanght my u Duyfjen <rhyme label="c" type="m" corresp="#s23:l4">weer</rhyme>,</l>
        <l id="s23:l6" met="3" type="-+">Laet my nooyt doolen <rhyme label="b" type="f" corresp="#s23:l3">verder</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Kijck uyt u Hemels <rhyme label="a" type="m">Erck</rhyme>;</l>
        <l id="s24:l2" met="3" type="-+">Neemt steets op mijn <rhyme label="a" type="m" corresp="#s24:l1">ooghmerck</rhyme>;</l>
        <l id="s24:l3" met="3" type="-+">Laet my doch nooyt <rhyme label="b" type="f">verdwalen</rhyme></l>
        <l id="s24:l4" met="3" type="-+">Op mijne Hemels <rhyme label="c" type="m">Rijs</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">Naer 't eeuwigh <rhyme label="c" type="m" corresp="#s24:l4">Paradijs</rhyme>:</l>
        <l id="s24:l6" met="3" type="-+">Maer houd my in dijn <rhyme label="b" type="f" corresp="#s24:l3">palen</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">Ey laet ons door <rhyme label="a" type="m">gena</rhyme>,</l>
        <l id="s25:l2" met="3" type="-+">De voorsmaeck vroegh en <rhyme label="a" type="m" corresp="#s25:l1">spa</rhyme></l>
        <l id="s25:l3" met="3" type="-+">Van 't Hemels soet recht <rhyme label="b" type="f">smaecken</rhyme>!</l>
        <l id="s25:l4" met="3" type="-+">Of ons dat soo <rhyme label="c" type="m">geviel</rhyme>,</l>
        <l id="s25:l5" met="3" type="-+">Dat wy met hert en <rhyme label="c" type="m" corresp="#s25:l4">ziel</rhyme>,</l>
        <l id="s25:l6" met="3" type="-+">Daer steets naer mochten <rhyme label="b" type="f" corresp="#s25:l3">haecken</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">Niet noodiger als <rhyme label="a" type="m">dat</rhyme>:</l>
        <l id="s26:l2" met="3" type="-+">Dat ons de waertste <rhyme label="a" type="m" corresp="#s26:l1">schat</rhyme></l>
        <l id="s26:l3" met="3" type="-+">Doet lieven, en doet <rhyme label="b" type="f">koopen</rhyme>:</l>
        <l id="s26:l4" met="3" type="-+">Dat ons tot Gode <rhyme label="c" type="m">brenght</rhyme>,</l>
        <l id="s26:l5" met="3" type="-+">Die 't eeuwigh Leven <rhyme label="c" type="m" corresp="#s26:l4">schenckt</rhyme>.</l>
        <l id="s26:l6" met="3" type="-+">Die door 't Geloof dus <rhyme label="b" type="f" corresp="#s26:l3">loopen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
