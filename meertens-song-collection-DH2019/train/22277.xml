    <melody>Psalm 068 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ghy sHeeren knechten dient doch <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Een yeglijck synen naem loue <rhyme label="a" type="m" corresp="#s1:l1">snel</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ghy knechten in thuys des <rhyme label="b" type="f">Heeren</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Ghy die daer dient in de houen <rhyme label="c" type="m">ghemeen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Des Tempels onsers Godts <rhyme label="c" type="m" corresp="#s1:l4">alleen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Doeghedt doch ghewillich en <rhyme label="b" type="f" corresp="#s1:l3">gheeren</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Loeft den Heer want hy vriendelijck <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Loeft synen naem tallen tijde <rhyme label="d" type="m" corresp="#s1:l7">ghewis</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Want hy is lieflijck, en <rhyme label="e" type="f">goede</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">De Heer hem Jacob vercoren <rhyme label="f" type="m">heeft</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">En Israels eygendom niet en <rhyme label="f" type="m" corresp="#s1:l10">sneeft</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Dat sal hy seer wel <rhyme label="e" type="f" corresp="#s1:l9">verhoeden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Heer is groot, want hy te bouen <rhyme label="a" type="m">gaet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Alle de Goden ver bouen <rhyme label="a" type="m" corresp="#s2:l1">maet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hy doeghedt al nae synen <rhyme label="b" type="f">luste</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Inden Hemel, en op het <rhyme label="c" type="m">eerdtrijck</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Inde Zee, en in alle diepten <rhyme label="c" type="m" corresp="#s2:l4">diesglijck</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die Wolcken die can hy <rhyme label="b" type="f" corresp="#s2:l3">toerusten</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">Hy brengtse van deynd der eerden <rhyme label="d" type="m">voort</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Hy maeckt den Blixem, en Reghen <rhyme label="d" type="m" corresp="#s2:l7">groot</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">De Wint comt wt heymlijcke <rhyme label="e" type="f">steden</rhyme>:</l>
        <l id="s2:l10" met="4" type="-+">Hy heeft oock met zijnder macht <rhyme label="f" type="m">vermoort</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">In Egipten, alle eerste <rhyme label="f" type="m" corresp="#s2:l10">Gheboort</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Vande Menschen, en Beesten <rhyme label="e" type="f" corresp="#s2:l9">mede</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sijn Teecknen deed hy in Egipten <rhyme label="a" type="m">landt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Aen Pharo, en zijn knechten tallen <rhyme label="a" type="m" corresp="#s3:l1">cant</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hunder Coninghen doode hy <rhyme label="b" type="f">veele</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Der Amoriter Coninck <rhyme label="c" type="m">Sihon</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En oock den Coninck te <rhyme label="c" type="m" corresp="#s3:l4">Basan</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In Canan oock den <rhyme label="b" type="f" corresp="#s3:l3">meestendeele</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Hy gaff hun Landt tot een Erffgoet <rhyme label="d" type="m">wel</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Sijnen volcke, den Kindren van <rhyme label="d" type="m" corresp="#s3:l7">Israel</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Heer uwen naem duert <rhyme label="e" type="f">eewelijcke</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">De Heer sal syn volck ordeelen <rhyme label="f" type="m">fyn</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">En zijnen knechten ghenadich <rhyme label="f" type="m" corresp="#s3:l10">syn</rhyme>:</l>
        <l id="s3:l12" met="3" type="-+">Want der Heyden Goden <rhyme label="e" type="f" corresp="#s3:l9">allglijcke</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die zijn gemaect van Gout, en Siluer <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dat sy hebben monden maer en spreken <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Sy en sien niet met haren <rhyme label="b" type="f">ooghen</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Sy en hooren niet met hun <rhyme label="c" type="m">ooren</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Hun mondt die heeft den asem <rhyme label="c" type="m" corresp="#s4:l4">verloren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hun maekers sal Godt plaghen <rhyme label="b" type="f" corresp="#s4:l3">hooghe</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Maer Israels huys, en oock <rhyme label="d" type="m">Aaroon</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">En thuys Leui louen den Heere <rhyme label="d" type="m" corresp="#s4:l7">schoon</rhyme>:</l>
        <l id="s4:l9" met="3" type="-+">Ghy Godtureesende loeft den <rhyme label="e" type="f">Heere</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">De Heer sy oock gheloeft wt <rhyme label="f" type="m">Zyoon</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Die te Jherusalem woont <rhyme label="f" type="m" corresp="#s4:l10">schoon</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Den Heer soudmen louen <rhyme label="e" type="f" corresp="#s4:l9">seere</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
