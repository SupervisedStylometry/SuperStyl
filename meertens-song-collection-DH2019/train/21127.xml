    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">BRuyd'gom die den <rhyme label="a" type="m">Echten-staet</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Nu <rhyme label="a" type="m" corresp="#s1:l1">aen-vaet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Door natures kracht <rhyme label="b" type="f">gedreven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Om door liefd' en vreeds <rhyme label="c" type="m">besluyt</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Met u <rhyme label="c" type="m" corresp="#s1:l4">Bruydt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Uwe dagen af te <rhyme label="b" type="f" corresp="#s1:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Desen heyl'gen <rhyme label="a" type="m">Echten-bandt</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Eyscht <rhyme label="a" type="m" corresp="#s2:l1">verstant</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Trouwe liefd' en Godts-vrees <rhyme label="b" type="f">beyde</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Op dat men in 't soet, of <rhyme label="c" type="m">roet</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">'t Quaet uyt 't <rhyme label="c" type="m" corresp="#s2:l4">goet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Weet te schiften en te <rhyme label="b" type="f" corresp="#s2:l3">scheyden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ghy moet tot u Bruydt en <rhyme label="a" type="m">lief</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Haer <rhyme label="a" type="m" corresp="#s3:l1">gerief</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Staegh met voorsicht voor haer <rhyme label="b" type="f">waecken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ghy moet houden 't oogh in 't <rhyme label="c" type="m">zeyl</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Tot haer <rhyme label="c" type="m" corresp="#s3:l4">heyl</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat geen onheyl haer <rhyme label="b" type="f" corresp="#s3:l3">genaecke</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ghy moet staegh voorganger <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Naeckt u <rhyme label="a" type="m" corresp="#s4:l1">pijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Neemt u toe-vlucht tot den <rhyme label="b" type="f">Heere</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Stelt u hart in hem <rhyme label="c" type="m">gerust</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Hy heeft <rhyme label="c" type="m" corresp="#s4:l4">lust</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">'t Quaet, van die hem soeckt, te <rhyme label="b" type="f" corresp="#s4:l3">weeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">V, o Bruydt! past met <rhyme label="a" type="m">bescheydt</rhyme></l>
        <l id="s5:l2" met="2" type="+-"><rhyme label="a" type="m" corresp="#s5:l1">G'hoorsaemheydt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Zedigh weest altijdt de <rhyme label="b" type="f">minste</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Soo sal by u liefd' en <rhyme label="c" type="m">vree</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Houden <rhyme label="c" type="m" corresp="#s5:l4">stee</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Acht dit voor een rijcke <rhyme label="b" type="f" corresp="#s5:l3">winste</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Houdt u Man voor Vooght en <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">
          <rhyme label="a" type="m" corresp="#s6:l1">Nimmermeer</rhyme>
        </l>
        <l id="s6:l3" met="4" type="+-">Laet door twist u vree <rhyme label="b" type="f">verjagen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En in liefde <rhyme label="c" type="m">wederom</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s6:l4">Bruydegom</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Wilt u Bruydtjes swackheydt <rhyme label="b" type="f" corresp="#s6:l3">dragen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">En als ('t welck men veel <rhyme label="a" type="m">bespeurt</rhyme>)</l>
        <l id="s7:l2" met="2" type="+-">V <rhyme label="a" type="m" corresp="#s7:l1">gebeurt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Dat Godt u Echts-vrucht doet <rhyme label="b" type="f">krijgen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Soo moet ghy die van der <rhyme label="c" type="m">jeught</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Tot de <rhyme label="c" type="m" corresp="#s7:l4">deught</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En ten Hemel leeren <rhyme label="b" type="f" corresp="#s7:l3">stijgen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Bruygom, sal dit in der <rhyme label="a" type="m">kracht</rhyme></l>
        <l id="s8:l2" met="2" type="+-">Sijn <rhyme label="a" type="m" corresp="#s8:l1">volbracht</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dat vereyscht verstant en <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Daerom bidt dat Godt u <rhyme label="c" type="m">Echt</rhyme></l>
        <l id="s8:l5" met="2" type="+-">Knoopt en <rhyme label="c" type="m" corresp="#s8:l4">vlecht</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Met trouw', liefd', eendracht en <rhyme label="b" type="f" corresp="#s8:l3">vreede</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Daerom jongh vereenight <rhyme label="a" type="m">paer</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Wilt <rhyme label="a" type="m" corresp="#s9:l1">malkaer</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Recht in 't hart voor Gode <rhyme label="b" type="f">trouwen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Soo sal hy met Zegens <rhyme label="c" type="m">vloet</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">V <rhyme label="c" type="m" corresp="#s9:l4">gemoet</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Met vernoegingh staegh <rhyme label="b" type="f" corresp="#s9:l3">bedouwen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
