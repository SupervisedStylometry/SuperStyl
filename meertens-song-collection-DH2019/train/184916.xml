    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mars van de graaf van Egten</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">JEught, die van dees Leering' <rhyme label="a" type="m">syt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Dat om het <rhyme label="b" type="m">meest</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Op dese <rhyme label="b" type="m" corresp="#s1:l2">feest</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Godts glori' u <rhyme label="a" type="m" corresp="#s1:l1">verblydt</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">t'Is heden, dat men viert diën <rhyme label="c" type="m">heldt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die door een Goddelyck <rhyme label="c" type="m" corresp="#s1:l5">geweldt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Godts leering' heeft <rhyme label="c" type="m" corresp="#s1:l5">herstelt</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Godts leering' is <rhyme label="d" type="m">versterckt</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Ignatius heeft <rhyme label="d" type="m" corresp="#s1:l8">t'uyt-gewerckt</rhyme>:</l>
        <l id="s1:l10" met="2" type="+-">Danckt, dat van syn <rhyme label="e" type="m">Sociëteyt</rhyme></l>
        <l id="s1:l11" met="5" type="+-">U de waere leer wordt <rhyme label="e" type="m" corresp="#s1:l10">uyt-geleydt</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Door desen heldt <rhyme label="e" type="m" corresp="#s1:l10">verbreydt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hy heeft tot ons vyandts <rhyme label="a" type="m">spyt</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Door s'Hemels <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Van ketters <rhyme label="b" type="m" corresp="#s2:l2">macht</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Europa meest <rhyme label="a" type="m" corresp="#s2:l1">bevrydt</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Den iever tot een <rhyme label="c" type="m">Sacrament</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Was aen de wereldt <rhyme label="c" type="m" corresp="#s2:l5">ongewent</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Godts dienst als <rhyme label="c" type="m" corresp="#s2:l5">onbekent</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Godts dienst is nu <rhyme label="d" type="m">versterckt</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Ignatius heeft <rhyme label="d" type="m" corresp="#s2:l8">t'uyt-gewerckt</rhyme>:</l>
        <l id="s2:l10" met="2" type="+-">Danckt, dat van syn <rhyme label="e" type="m">Sociëteyt</rhyme></l>
        <l id="s2:l11" met="5" type="+-">U de waere leer wordt <rhyme label="e" type="m" corresp="#s2:l10">uyt-geleydt</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Door desen heldt <rhyme label="e" type="m" corresp="#s2:l10">verbreydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hy heeft tot in t'leste <rhyme label="a" type="m">landt</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Door <rhyme label="b" type="m">Asia</rhyme></l>
        <l id="s3:l3" met="2" type="-+">
          <rhyme label="b" type="m" corresp="#s3:l2">America</rhyme>
        </l>
        <l id="s3:l4" met="3" type="-+">Het waer geloof <rhyme label="a" type="m" corresp="#s3:l1">geplant</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">De jonckheydt wist hy meer en <rhyme label="c" type="m">meer</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Te trecken naer de Christi <rhyme label="c" type="m" corresp="#s3:l5">leer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Tot glori' van den <rhyme label="c" type="m" corresp="#s3:l5">Heer</rhyme>:</l>
        <l id="s3:l8" met="3" type="-+">Godts glori' is <rhyme label="d" type="m">versterckt</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Ignatius heeft t' <rhyme label="d" type="m" corresp="#s3:l8">uyt-gewerckt</rhyme>:</l>
        <l id="s3:l10" met="2" type="+-">Danckt, dat van syn <rhyme label="e" type="m">Sociëteyt</rhyme></l>
        <l id="s3:l11" met="5" type="+-">U de waere leer wordt <rhyme label="e" type="m" corresp="#s3:l10">uyt-geleydt</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Door desen heldt <rhyme label="e" type="m" corresp="#s3:l10">verbreydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wat heeft hy al <rhyme label="a" type="m">uyt-gestaen</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Om door veel <rhyme label="b" type="m">strydt</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Tot s'vyandts <rhyme label="b" type="m" corresp="#s4:l2">nydt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Godts Kercke <rhyme label="a" type="m" corresp="#s4:l1">voor-te-staen</rhyme>!</l>
        <l id="s4:l5" met="4" type="-+">De Sociëteyt is <rhyme label="c" type="m">op-gerecht</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">En blyft aen Jesus <rhyme label="c" type="m" corresp="#s4:l5">vast-gehecht</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Voor welckers naem sy <rhyme label="c" type="m" corresp="#s4:l5">vecht</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Godts naem is nu <rhyme label="d" type="m">versterckt</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Ignatius heeft t'uyt <rhyme label="d" type="m" corresp="#s4:l8">gewerckt</rhyme>:</l>
        <l id="s4:l10" met="2" type="+-">Verheft, ô jeught, viert u <rhyme label="e" type="m">Patroon</rhyme>,</l>
        <l id="s4:l11" met="5" type="+-">Die in Godts leer u stelt ten <rhyme label="e" type="m" corresp="#s4:l10">thoon</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Voor prys des Hemels <rhyme label="e" type="m" corresp="#s4:l10">Kroon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
