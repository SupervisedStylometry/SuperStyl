<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186299</id>
    <date>
      <year-start>1672</year-start>
      <year-end>1672</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Courante monsieur</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Philis mijn <rhyme label="a" type="m">Herderin</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Princes van mijn <rhyme label="b" type="m">gemoed</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Ghy zijt <rhyme label="a" type="m" corresp="#s1:l1">Goddin</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die my de smert <rhyme label="b" type="m" corresp="#s1:l2">aen-doet</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">So dat mijn <rhyme label="c" type="m">Ziel</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Door u in Min <rhyme label="c" type="m" corresp="#s1:l5">beviel</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Ach Philis en weest niet soo <rhyme label="d" type="m">wreed</rhyme>,</l>
        <l id="s1:l8" met="5" type="-+">U Echte Slaef Amintas is <rhyme label="d" type="m" corresp="#s1:l7">bereet</rhyme></l>
        <l id="s1:l9" met="5" type="-+">Om u te dienen, kostelijck <rhyme label="e" type="m">Robijn</rhyme>,</l>
        <l id="s1:l10" met="5" type="-+">Wilt doch u Dienaer eens genadigh <rhyme label="e" type="m" corresp="#s1:l9">zijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ach alderschoonste <rhyme label="a" type="m">Beelt</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">U blondigh krol <rhyme label="b" type="m">gevlucht</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Mijn hert ghy <rhyme label="a" type="m" corresp="#s2:l1">steelt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al met u soet <rhyme label="b" type="m" corresp="#s2:l2">gesicht</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">U Traentjens <rhyme label="c" type="m">wit</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">'t Is immers niet een <rhyme label="c" type="m" corresp="#s2:l5">lit</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Of 't schijnt te zijn een <rhyme label="d" type="m">Albast</rhyme>,</l>
        <l id="s2:l8" met="5" type="-+">En let doch scherp hoe dit Kleet haer <rhyme label="d" type="m" corresp="#s2:l7">past</rhyme>,</l>
        <l id="s2:l9" met="5" type="-+">Wie had gedacht dat onder desen <rhyme label="e" type="m">schijn</rhyme>,</l>
        <l id="s2:l10" met="5" type="-+">Sulcken wreedheyd sou gemenght <rhyme label="e" type="m" corresp="#s2:l9">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Philis waer wilt ghy <rhyme label="a" type="m">heen</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Isser dan geenen <rhyme label="b" type="m">troost</rhyme>?</l>
        <l id="s3:l3" met="2" type="-+">Voor mijn <rhyme label="a" type="m" corresp="#s3:l1">geween</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">In Westen noch in <rhyme label="b" type="m" corresp="#s3:l2">Oost</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Gunt docht <rhyme label="c" type="m">genaed'</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Lief eer het word te <rhyme label="c" type="m" corresp="#s3:l5">laet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">En stelt u ooren na mijn <rhyme label="d" type="m">klacht</rhyme>,</l>
        <l id="s3:l8" met="5" type="-+">Die ick gestadigh doe by dagh en <rhyme label="d" type="m" corresp="#s3:l7">nacht</rhyme>,</l>
        <l id="s3:l9" met="5" type="-+">Verkeert u eens och Liefste <rhyme label="e" type="m">soet</rhyme>,</l>
        <l id="s3:l10" met="5" type="-+">Want ghy zijt die my dees smert <rhyme label="e" type="m" corresp="#s3:l9">aen-doet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een Herder die in 't <rhyme label="a" type="m">Wout</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Gestadigh doolt <rhyme label="b" type="m">eylaes</rhyme>!</l>
        <l id="s4:l3" met="2" type="-+">Is heel <rhyme label="a" type="m" corresp="#s4:l1">benout</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En sonder het <rhyme label="b" type="m" corresp="#s4:l2">solaes</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Daerom mijn <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Is vol van <rhyme label="c" type="m" corresp="#s4:l5">smert</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Ick sucht en klaegh door Bos en <rhyme label="d" type="m">Velt</rhyme>,</l>
        <l id="s4:l8" met="5" type="-+">En dat om Philis die my doet <rhyme label="d" type="m" corresp="#s4:l7">gewelt</rhyme>,</l>
        <l id="s4:l9" met="5" type="-+">Al door u schoonheyd en u helder <rhyme label="e" type="m">glans</rhyme>,</l>
        <l id="s4:l10" met="5" type="-+">Die weert bekroont is met een <rhyme label="e" type="m" corresp="#s4:l9">Bloeme-Krans</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick schuur den Moor om <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Amintas blijft in <rhyme label="b" type="m">druck</rhyme></l>
        <l id="s5:l3" met="2" type="-+">En in <rhyme label="a" type="m" corresp="#s5:l1">verdriet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En meeste <rhyme label="b" type="m" corresp="#s5:l2">ongeluck</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Tuygt Bos en <rhyme label="c" type="m">Boom</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En klare <rhyme label="c" type="m" corresp="#s5:l5">Water-stroom</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Getuyght ghy Linden die hier <rhyme label="d" type="m">staen</rhyme>,</l>
        <l id="s5:l8" met="5" type="-+">Hoe dickwils dat ick heb mijn klacht <rhyme label="d" type="m" corresp="#s5:l7">gedaen</rhyme>,</l>
        <l id="s5:l9" met="5" type="-+">Met droevigh suchten hoe dat Philis <rhyme label="e" type="m">blijft</rhyme></l>
        <l id="s5:l10" met="5" type="-+">In al mijn kermen als een Rots <rhyme label="e" type="m" corresp="#s5:l9">verstijft</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick val noch eens te <rhyme label="a" type="m">voet</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">U Philis mijn <rhyme label="b" type="m">Goddin</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Met al <rhyme label="a" type="m" corresp="#s6:l1">ootmoet</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ey keert doch uwen <rhyme label="b" type="m" corresp="#s6:l2">sin</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">En gunt u <rhyme label="c" type="m">slaef</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Doch eens een <rhyme label="c" type="m" corresp="#s6:l5">Minne-gaef</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Schiet maer een strael met u <rhyme label="d" type="m">gesicht</rhyme>,</l>
        <l id="s6:l8" met="5" type="-+">Dat my de Ziel van dese druck <rhyme label="d" type="m" corresp="#s6:l7">verlicht</rhyme>,</l>
        <l id="s6:l9" met="5" type="-+">Dat ick magh roepen, o gewenschte <rhyme label="e" type="m">uur</rhyme>,</l>
        <l id="s6:l10" met="5" type="-+">Dat Philis is geworden mijn <rhyme label="e" type="m" corresp="#s6:l9">Portuur</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maer neen 't is al om <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Mijn Philis blijft <rhyme label="b" type="m">gestoort</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Sy stadigh <rhyme label="a" type="m" corresp="#s7:l1">vliet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Waer door sy my <rhyme label="b" type="m" corresp="#s7:l2">vermoort</rhyme>:</l>
        <l id="s7:l5" met="2" type="-+">Ick tree in 't <rhyme label="c" type="m">Graf</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En schey de Werelt <rhyme label="c" type="m" corresp="#s7:l5">af</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Ey komt dan spoedigh wreede <rhyme label="d" type="m">Dood</rhyme>,</l>
        <l id="s7:l8" met="5" type="-+">En druckt my in uwen droeven <rhyme label="d" type="m" corresp="#s7:l7">schoot</rhyme>,</l>
        <l id="s7:l9" met="5" type="-+">Ick vlucht genuchten, Cyters ende <rhyme label="e" type="m">Fluyt</rhyme>,</l>
        <l id="s7:l10" met="5" type="-+">'k Verlaet de Werelt, en ick scheyder <rhyme label="e" type="m" corresp="#s7:l9">uyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Adieu Princes, <rhyme label="a" type="m">adieu</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Adieu Philis ick <rhyme label="b" type="m">ben</rhyme></l>
        <l id="s8:l3" met="2" type="-+">Mijn Leven <rhyme label="a" type="m" corresp="#s8:l1">mu</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Adieu nu die ick <rhyme label="b" type="m" corresp="#s8:l2">ken</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">'k Segh tot <rhyme label="c" type="m">verwijt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat ghy de oorsaeck <rhyme label="c" type="m" corresp="#s8:l5">zijt</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Dat ick moet sterven vol <rhyme label="d" type="m">torment</rhyme>,</l>
        <l id="s8:l8" met="5" type="-+">Ach Hemel is dit dan mijn droevigh <rhyme label="d" type="m" corresp="#s8:l7">end</rhyme>,</l>
        <l id="s8:l9" met="5" type="-+">En stelt op mijnen Serck, want het is <rhyme label="e" type="m">nood</rhyme>,</l>
        <l id="s8:l10" met="5" type="-+">Dat hier Amintas leyd door Philis <rhyme label="e" type="m" corresp="#s8:l9">dood</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
