    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Het daagt in de oosten</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Pieter Elsevier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAer baet geen <rhyme label="a" type="f">tegen-streven</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">Daer Venus Septer <rhyme label="b" type="m">praelt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Al soeck j'een beter <rhyme label="a" type="f" corresp="#s1:l1">leven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">De min u <rhyme label="b" type="m" corresp="#s1:l2">achter-haelt</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Al soek j'een beter leven, beter <rhyme label="a" type="f" corresp="#s1:l1">leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mercuer de boo der <rhyme label="a" type="f">Gooden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Den snooden Kiecken <rhyme label="b" type="m">dief</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ginck onlangs gasten <rhyme label="a" type="f" corresp="#s2:l1">nooden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die toonden hy een <rhyme label="b" type="m" corresp="#s2:l2">brief</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Gink onlangs gasten nooden, Gasten <rhyme label="a" type="f" corresp="#s2:l1">nooden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy prees de <rhyme label="a" type="f">Guyterye</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Het rooven gaf hy <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Mids quam Bacchus <rhyme label="a" type="f" corresp="#s3:l1">verleye</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Bracht volckjen in sijn <rhyme label="b" type="m" corresp="#s3:l2">Hof</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Mids quam Bacchus verleye, Quam <rhyme label="a" type="f" corresp="#s3:l1">verleye</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een yder wert <rhyme label="a" type="f">beschoncke</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En prees de snoode <rhyme label="b" type="m">vont</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Oock Paphos vier'ge <rhyme label="a" type="f" corresp="#s4:l1">loncke</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En soete nekter <rhyme label="b" type="m" corresp="#s4:l2">mont</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">Ook Paphos vier'ge longe, vierige <rhyme label="a" type="f" corresp="#s4:l1">loncke</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Werelt is <rhyme label="a" type="f">bedrogen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Door dese valschen <rhyme label="b" type="m">schijn</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Van 't rechten <rhyme label="a" type="f" corresp="#s5:l1">af-getogen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ter plaets daer rampen <rhyme label="b" type="m" corresp="#s5:l2">zijn</rhyme>,</l>
        <l id="s5:l5" met="5" type="-+">Van 't rechten af-getogen, <rhyme label="a" type="f" corresp="#s5:l1">afgetogen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hoe wel dat snoode <rhyme label="a" type="f">Guytje</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Vrouw Venus zoete <rhyme label="b" type="m">Kint</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Als Apoll speelt op 't <rhyme label="a" type="f" corresp="#s6:l1">fluytje</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ons herten al door <rhyme label="b" type="m" corresp="#s6:l2">tint</rhyme>,</l>
        <l id="s6:l5" met="5" type="-+">Als Apoll speelt op 't fluytje, speelt op 't  <rhyme label="a" type="f" corresp="#s6:l1">fluytje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wanneer Diaen haer <rhyme label="a" type="f">Broeder</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ten oosten rijst by <rhyme label="b" type="m">dagh</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Soo ley de <rhyme label="a" type="f" corresp="#s7:l1">Minne-moeder</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Ten Boswaert in en <rhyme label="b" type="m" corresp="#s7:l2">sagh</rhyme>,</l>
        <l id="s7:l5" met="5" type="-+">Soo ley de Minne-moeder, <rhyme label="a" type="f" corresp="#s7:l1">Minne-moeder</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dan in de <rhyme label="a" type="f">Klaver-weyen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En in het eensaem <rhyme label="b" type="m">Woudt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Daer de Gelieven <rhyme label="a" type="f" corresp="#s8:l1">vryen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Sy staeg haer Zeetel <rhyme label="b" type="m" corresp="#s8:l2">houdt</rhyme>,</l>
        <l id="s8:l5" met="5" type="-+">Daer de Gelieven vryen, lieven <rhyme label="a" type="f" corresp="#s8:l1">vryen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Haer Ry heeft veel <rhyme label="a" type="f">vermaecken</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Doch geeft in 't eynd maer <rhyme label="b" type="m">smart</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Oock die tot Dief-stal <rhyme label="a" type="f" corresp="#s9:l1">raecken</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Krijcht licht in 't eynd' een <rhyme label="b" type="m" corresp="#s9:l2">gart</rhyme>,</l>
        <l id="s9:l5" met="5" type="-+">Oock die tot Dief-stal raecken, Dief stal <rhyme label="a" type="f" corresp="#s9:l1">raecken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
