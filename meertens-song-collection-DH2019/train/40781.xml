<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>40781</id>
    <date>
      <year-start>1681</year-start>
      <year-end>1681</year-end>
    </date>
    <location>
      <orginal-name>leeuwarden</orginal-name>
      <standard-name>Leeuwarden, The Netherlands</standard-name>
      <coordinates>
        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 033 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Gysbert Japicx</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MEy siel-wille', herts-njue, muwlle' in <rhyme label="a" type="f">tonge</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Glier taens-rijp Hijmmelsch'lof-luwd <rhyme label="b" type="m">fol</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Môt de oprjuecht' de HEER' God Lof-Psalm' <rhyme label="a" type="f" corresp="#s1:l1">sjonge</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Goads lof betammet jerm' rjuecht <rhyme label="b" type="m" corresp="#s1:l2">wol</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">To den: d'Gaest <rhyme label="c" type="f">op-scherpje</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">In de' HEER' Goads lof <rhyme label="c" type="f" corresp="#s1:l5">herpje</rhyme>;</l>
        <l id="s1:l7" met="3" type="+-">Litt' de yn-swiete <rhyme label="d" type="m">Luwtt'</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Litt' 't klien' Tjien-snaere' <rhyme label="e" type="f">heare</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Dat se' uwz 't hert, trogge' <rhyme label="e" type="f" corresp="#s1:l8">eare</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Tockje', yn Goad' bly, <rhyme label="d" type="m" corresp="#s1:l7">uwt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sjongje' (yn Godsill'ge' uwt-litt' ne liette', <rhyme label="a" type="f">ô</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Mingje' yn-borsts yn uwt-wind'ge <rhyme label="b" type="m">freugd</rhyme>)</l>
        <l id="s2:l3" met="4" type="-+">d'HEER' yen steez-oon-ny' lof-sjong liet <rhyme label="a" type="f" corresp="#s2:l1">to</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Spijllje' op, meyts' sill'ge wille, yn <rhyme label="b" type="m" corresp="#s2:l2">deugd</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Want uwz HEER' Goads <rhyme label="c" type="f">sizzen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Iz, naet om <rhyme label="c" type="f" corresp="#s2:l5">weer-lizzen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Inck'le rjuecht-uwt <rhyme label="d" type="m">rjuecht</rhyme>:</l>
        <l id="s2:l8" met="3" type="+-">Yn fijn wirckens <rhyme label="e" type="f">wond're</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Great, brie, boppe' in <rhyme label="e" type="f" corresp="#s2:l8">ond're</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Trouw', wiss' wierheyt' <rhyme label="d" type="m" corresp="#s2:l7">ljuecht't</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy ljeaft, hy minn't, hy hadt <rhyme label="a" type="f">heag-wirddigh</rhyme></l>
        <l id="s3:l2" met="4" type="-+">'t Rjuecht, hy ljeav't fromme' <rhyme label="b" type="m">oprjuechtigheyt</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">O sijn goertiern'heyt rint, <rhyme label="a" type="f" corresp="#s3:l1">firdd'-firddig</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Seyn-fol, 't brie yerdtrijck oer, ynn' <rhyme label="b" type="m" corresp="#s3:l2">weyd'</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Trog sijn WIRD, fol <rhyme label="c" type="f">eere</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Macke' (ô great wirck!) de <rhyme label="c" type="f" corresp="#s3:l5">HEERE</rhyme></l>
        <l id="s3:l7" met="3" type="+-">d'Hijmm'len bocht om <rhyme label="d" type="m">bocht</rhyme>:</l>
        <l id="s3:l8" met="3" type="+-">Sijn muwll'-Gaests <rhyme label="e" type="f">uwt-rjuechten</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Ing'len scheppe, in <rhyme label="e" type="f" corresp="#s3:l8">ljuechten</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Mey al 't Hijmm'le' <rhyme label="d" type="m" corresp="#s3:l7">heer-tocht'</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy gearre', az op yen heap', 't <rhyme label="a" type="f">See-wetter</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">'t <rhyme label="b" type="m">Suwz'-ruwz-bruwz-hol-bol-hobb'le-tob</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Setse' igh, boerd', ran, stran, pealle'; in let <rhyme label="a" type="f" corresp="#s4:l1">her</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Az ynn' schat-keam're kolck' <rhyme label="b" type="m" corresp="#s4:l2">duwck-dobb'</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Litt' den de Yerde' hosck <rhyme label="c" type="f">buwgje</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">'s HEER'NE ontsjochtme <rhyme label="c" type="f" corresp="#s4:l5">uwt-tjuwgje</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Litte' all' wae 't dy <rhyme label="d" type="m">wrad</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Het ynn' bruwckma <rhyme label="e" type="f">schickje</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Dat hy for God <rhyme label="e" type="f" corresp="#s4:l8">schrickje</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Dy 't krefts hânz <rhyme label="d" type="m" corresp="#s4:l7">bewâdt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want eernet hy? sjea dear!, it iz <rhyme label="a" type="f">'er</rhyme>.</l>
        <l id="s5:l2" met="4" type="-+">Hy hiet't, in 't stiet 'er, so 's hy 't <rhyme label="b" type="m">wol</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Hy naett get de Heyd'ne rie: jae lizze <rhyme label="a" type="f" corresp="#s5:l1">'er</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Forbruwcke' ynn' djoey in tinck, gnod <rhyme label="b" type="m" corresp="#s5:l2">dol</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Mar, ô, d'rie uwz <rhyme label="c" type="f">HEER'NE</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Dy hy ae <rhyme label="c" type="f" corresp="#s5:l5">uwt-eerne</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Stiet ynne' <rhyme label="d" type="m">yvigheyt</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Ja sijn hert' <rhyme label="e" type="f">besletten</rhyme>-</l>
        <l id="s5:l9" met="3" type="+-">Tinck, wirt fest, az <rhyme label="e" type="f" corresp="#s5:l8">wetten</rhyme>,</l>
        <l id="s5:l10" met="3" type="+-">Teamm' ney teamme' <rhyme label="d" type="m" corresp="#s5:l7">yn-heyd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O wol-lock-sillig <rhyme label="a" type="f">oer-lock-sillig</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Iz 't foltsen waems God de HEER' salme' <rhyme label="b" type="m">iz</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dat de HEER him eyn-mijnnet, <rhyme label="a" type="f" corresp="#s6:l1">salm-willig</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Ja kippet uwt to'n eyne <rhyme label="b" type="m" corresp="#s6:l2">erv'niss'</rhyme></l>
        <l id="s6:l5" met="3" type="+-">d'HEERE', uwtte hijmm'le <rhyme label="c" type="f">heegte</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Schoaget oon, ynn' <rhyme label="c" type="f" corresp="#s6:l5">leegte</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">All' wradz minsche <rhyme label="d" type="m">bern</rhyme>.</l>
        <l id="s6:l8" met="3" type="+-">Uwt sijn feste <rhyme label="e" type="f">wengje</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Sjocht hy s', dy <rhyme label="e" type="f" corresp="#s6:l8">bewenje</rhyme></l>
        <l id="s6:l10" met="3" type="+-">d'Yerd, fen hern' to <rhyme label="d" type="m" corresp="#s6:l7">hern'</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Allinne hy stal-formjet de' <rhyme label="a" type="f">herten</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Hy aegt op all' ljue wirck in <rhyme label="b" type="m">dwaen</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Yen Kening wraegs'let naet uwtt' <rhyme label="a" type="f" corresp="#s7:l1">smerten</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Trog great heer-krefts gef juecht, yn 't <rhyme label="b" type="m" corresp="#s7:l2">slaen</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Yen great-diedig <rhyme label="c" type="f">fjuechter</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Wirt nin winst <rhyme label="c" type="f" corresp="#s7:l5">uwt-rjuechter</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Trog sijn kreg'le <rhyme label="d" type="m">fest</rhyme>.</l>
        <l id="s7:l8" met="3" type="+-">'t Hynzer jout nin <rhyme label="e" type="f">fjuecht-seyn'</rhyme></l>
        <l id="s7:l9" met="3" type="+-">Trog hoarss'-kreft. O <rhyme label="e" type="f" corresp="#s7:l8">sljuecht-eyn</rhyme>!</l>
        <l id="s7:l10" met="3" type="+-">Dy 't op trouw't, ynn' <rhyme label="d" type="m" corresp="#s7:l7">lest</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Mar loytsje' uwz ljeave' HEERE' <rhyme label="a" type="f">eag-dol-gloerjen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Oer dy him freezje' uwt <rhyme label="b" type="m">herte-gruwn'</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">In op sijn goertiern'heyt (yn 't <rhyme label="a" type="f" corresp="#s8:l1">doerjen</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Lang-stuwnig) hoopje' alle' uwre' in <rhyme label="b" type="m" corresp="#s8:l2">stuwn'</rhyme>.</l>
        <l id="s8:l5" met="3" type="+-">Om uwt deads <rhyme label="c" type="f">gryp-kloeren</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Herre siel to <rhyme label="c" type="f" corresp="#s8:l5">schoerren</rhyme>;</l>
        <l id="s8:l7" met="3" type="+-">Om, ynne' hong're <rhyme label="d" type="m">nea</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Herre to <rhyme label="e" type="f">bewadden</rhyme>,</l>
        <l id="s8:l9" met="3" type="+-">Herre <rhyme label="e" type="f" corresp="#s8:l8">t'onderhadden</rhyme>,</l>
        <l id="s8:l10" met="3" type="+-">Libb'ne lyv's, mey <rhyme label="d" type="m" corresp="#s8:l7">brea</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Uwz' sielen d'ljeave' HEER God <rhyme label="a" type="f">forwachtje</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Hy 's uwz haed, holp, honck, schuwlle' in <rhyme label="b" type="m">scherm</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Uwz herten bly, yn him <rhyme label="a" type="f" corresp="#s9:l1">fornachtgje</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">(:O hert-swiet' hymmelsch <rhyme label="b" type="m" corresp="#s9:l2">huynnig-swerm'</rhyme>!:)</l>
        <l id="s9:l5" met="3" type="+-">Om 't wy de HEER' <rhyme label="c" type="f">fortrouwje</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">In op d'Namme <rhyme label="c" type="f" corresp="#s9:l5">bouwje</rhyme></l>
        <l id="s9:l7" met="3" type="+-">Fen sijn <rhyme label="d" type="m">heyligheyt</rhyme>.</l>
        <l id="s9:l8" met="3" type="+-">HEER' jon goertiern' <rhyme label="e" type="f">njoeheyt</rhyme></l>
        <l id="s9:l9" met="3" type="+-">Wezze' oer uwz: ô <rhyme label="e" type="f" corresp="#s9:l8">goeheyt</rhyme>!</l>
        <l id="s9:l10" met="3" type="+-">Dear uwz hope op <rhyme label="d" type="m" corresp="#s9:l7">leyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
