    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn siel maeckt groot den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn geest den lof en <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Blymoedichlijck <rhyme label="b" type="f">gewaget</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mijns Heylants, die syn <rhyme label="a" type="m" corresp="#s1:l1">ooch</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Slaet van den hemel <rhyme label="a" type="m" corresp="#s1:l1">hooch</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Op syn geringe <rhyme label="b" type="f" corresp="#s1:l3">maget</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Nu sal ick zijn <rhyme label="a" type="m">geacht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">By yegelijck <rhyme label="a" type="m" corresp="#s2:l1">geslacht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Geluckich <rhyme label="b" type="f">boven-maten</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Want God heeft door syn <rhyme label="a" type="m" corresp="#s2:l1">macht</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Een daet aen my <rhyme label="a" type="m" corresp="#s2:l1">gewracht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die geen verstant can <rhyme label="b" type="f" corresp="#s2:l3">vaten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Syn name heylich <rhyme label="a" type="m">is</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">Onfeylbaer en <rhyme label="a" type="m" corresp="#s3:l1">gewis</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Syn goedicheyt <rhyme label="b" type="f">beclijvet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En gaet op <rhyme label="a" type="m" corresp="#s3:l1">kindes-kint</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van die syn woort <rhyme label="a" type="m" corresp="#s3:l1">bemint</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En in syn vreese <rhyme label="b" type="f" corresp="#s3:l3">blijvet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den arm des Heeren <rhyme label="a" type="m">sterck</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Heeft uytgevoert een <rhyme label="a" type="m" corresp="#s4:l1">werck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Twelck waerdich is te <rhyme label="b" type="f">loven</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">De trotze hy <rhyme label="a" type="m" corresp="#s4:l1">verslaet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Voor hem is haren <rhyme label="a" type="m" corresp="#s4:l1">raet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Verwaeyet en <rhyme label="b" type="f" corresp="#s4:l3">verstoven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die machtich zijn en <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Hy uyt den setel <rhyme label="a" type="m" corresp="#s5:l1">stoot</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Maer, die syn <rhyme label="b" type="f">neer-gebogen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In een verachten <rhyme label="a" type="m" corresp="#s5:l1">stant</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Verhoocht syn <rhyme label="a" type="m" corresp="#s5:l1">rechter-hant</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Voor aller menschen <rhyme label="b" type="f" corresp="#s5:l3">oogen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die honger lijden <rhyme label="a" type="m">most</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Besorget hy den <rhyme label="a" type="m" corresp="#s6:l1">cost</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Maer de verwaende <rhyme label="b" type="f">rijcken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die op haer gelt en <rhyme label="a" type="m" corresp="#s6:l1">goet</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Het herte stellen, <rhyme label="a" type="m" corresp="#s6:l1">doet</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Hy ledich van hem <rhyme label="b" type="f" corresp="#s6:l3">wijcken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Syn dienaer <rhyme label="a" type="m">IsraÃ«l</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Doet hy gestadich <rhyme label="a" type="m" corresp="#s7:l1">wel</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Syn gonst blijft <rhyme label="b" type="f">onvergeten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Die hy aen <rhyme label="a" type="m" corresp="#s7:l1">Abraham</rhyme></l>
        <l id="s7:l5" met="3" type="-+">En 'tvolck dat na hem <rhyme label="a" type="m" corresp="#s7:l1">quam</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Heeft voormaels laten <rhyme label="b" type="f" corresp="#s7:l3">weten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
