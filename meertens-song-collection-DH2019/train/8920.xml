      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy s'Heeren knechten dient hem <rhyme label="a" type="m">wel</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Een yeder Looft syn Name <rhyme label="a" type="m" corresp="#s1:l1">snel</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ghy Knecht' int Huys des <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ghy die dient in den Hoven <rhyme label="c" type="m">reyn</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Des Tempels onses Gods <rhyme label="c" type="m" corresp="#s1:l4">alleyn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Sulcx willich doet en <rhyme label="b" type="f" corresp="#s1:l3">geeren</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Loofft hem, want Vrendlijck is de <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Loofft synen Naeme altijdt <rhyme label="d" type="m" corresp="#s1:l7">meer</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Die Lieff'lijck is Al <rhyme label="e" type="f">omme</rhyme>.</l>
        <l id="s1:l10" met="4" type="-+">Israel ende Jacob <rhyme label="f" type="m">goet</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Heeft hy vercooren, end <rhyme label="f" type="m" corresp="#s1:l10">Behoedt</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Tot synen <rhyme label="e" type="f" corresp="#s1:l9">Eyghendomme</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick weet dat groot is Godt de <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Jae, boven alle Goden <rhyme label="a" type="m" corresp="#s2:l1">Veer'</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Al wat Godt wil end <rhyme label="b" type="f">Lustet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Doet hy in HEmel end <rhyme label="c" type="m">Aertrijck</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">In Zee en Diepten <rhyme label="c" type="m" corresp="#s2:l4">Insghelijck</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">De Wolcken hy <rhyme label="b" type="f" corresp="#s2:l3">Toerustet</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Laets' Op gaen van der Aerden <rhyme label="d" type="m">Endt</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Hy Blicxem maeckt, en Reghen <rhyme label="d" type="m" corresp="#s2:l7">sendt</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Windt wt heymlijcke <rhyme label="e" type="f">steeden</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Hy heeft oock met syn Macht <rhyme label="f" type="m">vermoort</rhyme></l>
        <l id="s2:l11" met="4" type="+-">In Egypten all' Eerst <rhyme label="f" type="m" corresp="#s2:l10">Gheboort'</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Van Menschen ende <rhyme label="e" type="f" corresp="#s2:l9">Veede</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy Wonder in Egypten <rhyme label="a" type="m">deed'</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Aen Pharo end syn Knechten <rhyme label="a" type="m" corresp="#s3:l1">meed'</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Veel Volcken bracht hy <rhyme label="b" type="f">omme</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Haer Coninghen van grooter <rhyme label="c" type="m">Macht</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Hy Doode ende Omme <rhyme label="c" type="m" corresp="#s3:l4">bracht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Int Landt Al om en <rhyme label="b" type="f" corresp="#s3:l3">tomme</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Der Amoriter Coninck <rhyme label="d" type="m">fel</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Sihon end Og te Basan <rhyme label="d" type="m" corresp="#s3:l7">snel</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">End oock die <rhyme label="e" type="f">Coninckrijcken</rhyme></l>
        <l id="s3:l10" met="4" type="-+">In Canaan van grooter <rhyme label="f" type="m">Eer</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Vernielde ende bracht ter <rhyme label="f" type="m" corresp="#s3:l10">Neer</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">De HEERE All' <rhyme label="e" type="f" corresp="#s3:l9">ghelijcke</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy gaff haer Landt ten Erve <rhyme label="a" type="m">heel</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Syn Volck die Kindren <rhyme label="a" type="m" corresp="#s4:l1">Israel</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Dijn Naem duert Eewich <rhyme label="b" type="f">HEERE</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">De Heer syn Volck sal Rechen <rhyme label="c" type="m">fijn</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">End syne Knechten alle <rhyme label="c" type="m" corresp="#s4:l4">zijn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ghenaedich <rhyme label="b" type="f" corresp="#s4:l3">Immermeere</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Der Heyden Goden syn <rhyme label="d" type="m">ghebout</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Al tsaem van silver ende <rhyme label="d" type="m" corresp="#s4:l7">gout</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Ghemaeckt van Menschen <rhyme label="e" type="f">Handen</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Sy hebben Monden, spreken <rhyme label="f" type="m">nicht</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">End Ooghen, maer gants gheen <rhyme label="f" type="m" corresp="#s4:l10">gesicht</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Sy blijven all' te <rhyme label="e" type="f" corresp="#s4:l9">schanden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sy hebben Ooren, hooren <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gheen Azem oock tot ghener <rhyme label="a" type="m" corresp="#s5:l1">Tijdt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En is in haren <rhyme label="b" type="f">Monde</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Die sulcke maecken, syn All' <rhyme label="c" type="m">gaer</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Als die op sulcke Hopen <rhyme label="c" type="m" corresp="#s5:l4">daer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">End gaen daer meed' te <rhyme label="b" type="f" corresp="#s5:l3">gronde</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Maer Isr'els Huys end <rhyme label="d" type="m">Aaroon</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">End Levi den HEER Loven <rhyme label="d" type="m" corresp="#s5:l7">schoon</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Syn Loff werdt hooch <rhyme label="e" type="f">verheven</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Wt Zyon sy gheloofft de <rhyme label="f" type="m">HEER</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Die te Jerus'lem woont met <rhyme label="f" type="m" corresp="#s5:l10">Eer</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Den HEER men Loff moet <rhyme label="e" type="f" corresp="#s5:l9">geven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
