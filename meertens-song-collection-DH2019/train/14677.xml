<song>
  <meta>
    <id>14677</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Maria schoon</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter van Gelre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ICk groet u <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Maria klaer <rhyme label="a" type="m" corresp="#s1:l1">fonteyn</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Beecke daer Godts genaed' door <rhyme label="b" type="m">vloeyt</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Ghy zijt <rhyme label="a" type="m" corresp="#s1:l1">alleyn</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Gelijck den <rhyme label="a" type="m" corresp="#s1:l1">Roosemareyn</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die altijdt stadich staet en <rhyme label="b" type="m" corresp="#s1:l3">bloeyt</rhyme>,</l>
        <l id="s1:l7" met="6" type="+-">Wy bidden, dat ons gebedt u niet <rhyme label="b" type="m" corresp="#s1:l3">vermoeyt</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Ghy zijt ons <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Als een Zee-sterre <rhyme label="c" type="m" corresp="#s1:l8">klaer</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Als dit aerdtsche <rhyme label="c" type="m" corresp="#s1:l8">ghevaer</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Ons benaut <rhyme label="c" type="m" corresp="#s1:l8">allegaer</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Dus o Maecht u tot ons <rhyme label="b" type="m" corresp="#s1:l3">spoeyt</rhyme>,</l>
        <l id="s1:l13" met="4" type="-+">En met ons tot Godts haven <rhyme label="b" type="m" corresp="#s1:l3">roeyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Noyt Vrouwe <rhyme label="a" type="m">tongh'</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Soo soet en lieffelijck <rhyme label="a" type="m" corresp="#s2:l1">songh'</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Als u wel deed, doen <rhyme label="b" type="m">Gabriel</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Tot u in <rhyme label="a" type="m" corresp="#s2:l1">gongh'</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En door de Hemelen <rhyme label="a" type="m" corresp="#s2:l1">krongh'</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dat ghy ontfinght <rhyme label="b" type="m" corresp="#s2:l3">Emanuel</rhyme>,</l>
        <l id="s2:l7" met="6" type="+-">Waer door dat ghy Satan die ons viel <rhyme label="b" type="m" corresp="#s2:l3">rebel</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Het hooft hebt <rhyme label="c" type="m">verplet</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Ia gebracht in het <rhyme label="c" type="m" corresp="#s2:l8">net</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Daer hy ons in <rhyme label="c" type="m" corresp="#s2:l8">gheset</rhyme></l>
        <l id="s2:l11" met="3" type="+-">had door Evas <rhyme label="c" type="m" corresp="#s2:l8">belet</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Dus soo langh wy u <rhyme label="b" type="m" corresp="#s2:l3">bevel</rhyme>,</l>
        <l id="s2:l13" met="4" type="-+">In alles doen verbaest de <rhyme label="b" type="m" corresp="#s2:l3">Hel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ontbindt den <rhyme label="a" type="m">bandt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van ons misdadighe <rhyme label="a" type="m" corresp="#s3:l1">handt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Brenght voort het licht den blinden <rhyme label="b" type="m">hier</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Verdrijft met <rhyme label="a" type="m" corresp="#s3:l1">schandt</rhyme></l>
        <l id="s3:l5" met="4" type="+-">De boosheden uyt het <rhyme label="a" type="m" corresp="#s3:l1">landt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Van onse sinnens qua <rhyme label="b" type="m" corresp="#s3:l3">manier</rhyme>,</l>
        <l id="s3:l7" met="6" type="+-">En geeft ons den breydel van u Soons <rhyme label="b" type="m" corresp="#s3:l3">bestier</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Toont dat ghy hier <rhyme label="c" type="m">zijt</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Onse Moeder <rhyme label="c" type="m" corresp="#s3:l8">altijdt</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Als de wereltse <rhyme label="c" type="m" corresp="#s3:l8">strijdt</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Ons bestormt met <rhyme label="c" type="m" corresp="#s3:l8">nijdt</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Op dat elck als een <rhyme label="b" type="m" corresp="#s3:l3">Angier</rhyme></l>
        <l id="s3:l13" met="4" type="-+">Bloeyt voor hem die ons kocht soo <rhyme label="b" type="m" corresp="#s3:l3">dier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Want waerde <rhyme label="a" type="m">Maecht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghy hebt sonderlingh' <rhyme label="a" type="m" corresp="#s4:l1">behaeght</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Door u sachtmoedigheyt den <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Als wy <rhyme label="a" type="m" corresp="#s4:l1">geplaecht</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">En deerelijck <rhyme label="a" type="m" corresp="#s4:l1">uytgejaecht</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Lagen, in doodts verachtingh <rhyme label="b" type="m" corresp="#s4:l3">neer</rhyme>,</l>
        <l id="s4:l7" met="6" type="+-">Doen baerden ghy 't hemelsche leven ons <rhyme label="b" type="m" corresp="#s4:l3">weer</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Soo dat ghy wel <rhyme label="c" type="m">meucht</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Eeuwelijck zijn <rhyme label="c" type="m" corresp="#s4:l8">verheucht</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Dat u snee-witte <rhyme label="c" type="m" corresp="#s4:l8">deucht</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Ons hier bracht sulcken <rhyme label="c" type="m" corresp="#s4:l8">vreucht</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">Maeckt ons grove sinnen <rhyme label="b" type="m" corresp="#s4:l3">teer</rhyme>,</l>
        <l id="s4:l13" met="4" type="-+">Om wel te doen u Soons <rhyme label="b" type="m" corresp="#s4:l3">begeer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Verleent ons <rhyme label="a" type="m">dan</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Voort een goedt leven <rhyme label="a" type="m" corresp="#s5:l1">van</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Alle gherusticheyt tot <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Dat den <rhyme label="a" type="m" corresp="#s5:l1">Tyran</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Der Hellen, ons niet <rhyme label="a" type="m" corresp="#s5:l1">kan</rhyme></l>
        <l id="s5:l6" met="4" type="-+">In eeuwigheyt maecken <rhyme label="b" type="m" corresp="#s5:l3">bespot</rhyme>,</l>
        <l id="s5:l7" met="6" type="+-">Maer dat wy het Lam Gods aensiende, voort <rhyme label="b" type="m" corresp="#s5:l3">tot</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Sijn troon mogen <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Daer hem eer wordt <rhyme label="c" type="m" corresp="#s5:l8">ghedaen</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Met een Engels <rhyme label="c" type="m" corresp="#s5:l8">vermaen</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Van die rondtom hem <rhyme label="c" type="m" corresp="#s5:l8">staen</rhyme>,</l>
        <l id="s5:l12" met="4" type="+-">Och hoe saligh is dat <rhyme label="b" type="m" corresp="#s5:l3">lot</rhyme></l>
        <l id="s5:l13" met="4" type="-+">Daermen singht Heyligh tot <rhyme label="b" type="m" corresp="#s5:l3">beslot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
