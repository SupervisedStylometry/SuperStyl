        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 006 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrick Jansz. Prins</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">STraf my, o Heer! mijn <rhyme label="a" type="f">hoorne</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Doch niet in uwen <rhyme label="a" type="f" corresp="#s1:l1">toorne</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Kastijdt my immer <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">In uwe <rhyme label="c" type="f">grimmigheden</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Want mijn gheboghen <rhyme label="c" type="f" corresp="#s1:l4">leden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Gaen swanger van <rhyme label="b" type="m" corresp="#s1:l3">verdriet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Zijt, Heer, zijt my <rhyme label="a" type="f">genadigh</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dewijl ghy zijt <rhyme label="a" type="f" corresp="#s2:l1">goedtdadigh</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Want ick ben gantsch <rhyme label="b" type="m">verswackt</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Wilt my genesingh <rhyme label="c" type="f">schicken</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Want al mijn beenen <rhyme label="c" type="f" corresp="#s2:l4">schricken</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En zijn gelijck <rhyme label="b" type="m" corresp="#s2:l3">ontrackt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn Ziele is door 't <rhyme label="a" type="f">plagen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Verschrickt, en seer <rhyme label="a" type="f" corresp="#s3:l1">verslagen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En quijnet nacht en <rhyme label="b" type="m">dach</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">En ghy, o Heer! hoe <rhyme label="c" type="f">lange</rhyme>?</l>
        <l id="s3:l5" met="3" type="-+">Want my is wonder <rhyme label="c" type="f" corresp="#s3:l4">bange</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Als die niet meer en <rhyme label="b" type="m" corresp="#s3:l3">mach</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Keer weder, Heer, keer <rhyme label="a" type="f">weder</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Slaet u gesichte <rhyme label="a" type="f" corresp="#s4:l1">neder</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En reddet mijne <rhyme label="b" type="m">Ziel</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om uwer goedtheyt <rhyme label="c" type="f">wille</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Verlos my van dit <rhyme label="c" type="f" corresp="#s4:l4">schrille</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En bijster angh <rhyme label="b" type="m" corresp="#s4:l3">gekriel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wat's in den doodt te <rhyme label="a" type="f">wachten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Van u? gantsch geen <rhyme label="a" type="f" corresp="#s5:l1">ghedachten</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Wie looft u in het <rhyme label="b" type="m">graf</rhyme>?</l>
        <l id="s5:l4" met="3" type="-+">Ick ben vermoeyt van <rhyme label="c" type="f">suchten</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En door mijn <rhyme label="c" type="f" corresp="#s5:l4">ongenuchten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Soo neemt mijn leven <rhyme label="b" type="m" corresp="#s5:l3">af</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hoe soud' ick niet <rhyme label="a" type="f">verlemmen</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Ick doe gestadigh <rhyme label="a" type="f" corresp="#s6:l1">swemmen</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Mijn bed', en mijn <rhyme label="b" type="m">bed'-stee</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Door-nat ick met mijn <rhyme label="c" type="f">tranen</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Ick piep ghelijck de <rhyme label="c" type="f" corresp="#s6:l4">Kranen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En hebbe rust noch <rhyme label="b" type="m" corresp="#s6:l3">vree</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn ooghen sich <rhyme label="a" type="f">door-knagen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van mijn verdrietigh <rhyme label="a" type="f" corresp="#s7:l1">klagen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dat nimmer op en <rhyme label="b" type="m">houdt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Van weghen mijn <rhyme label="c" type="f">benyers</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En wreede <rhyme label="c" type="f" corresp="#s7:l4">tegen-stryers</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">Mijn aengesicht <rhyme label="b" type="m" corresp="#s7:l3">veroudt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ghy werckers <rhyme label="a" type="f">onbesneden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Der <rhyme label="a" type="f" corresp="#s8:l1">ongerechtigheden</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Wijckt wegh van my nu <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">De Heere heeft mijn <rhyme label="c" type="f">reden</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Mijn stemme, mijn <rhyme label="c" type="f" corresp="#s8:l4">ghebeden</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En mijn geween <rhyme label="b" type="m" corresp="#s8:l3">gehoort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">De Heer, ick mach het <rhyme label="a" type="f">spreecken</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Heeft aengehoort mijn <rhyme label="a" type="f" corresp="#s9:l1">smeecken</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">De Heer sal mijn <rhyme label="b" type="m">ghebedt</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Aennemen, en <rhyme label="c" type="f">ontfangen</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Hy heeft, na mijn <rhyme label="c" type="f" corresp="#s9:l4">verlangen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Mijn Ziele <rhyme label="b" type="m" corresp="#s9:l3">uyt-geredt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Nu sullen mijn <rhyme label="a" type="f">vyanden</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Te samen staen met <rhyme label="a" type="f" corresp="#s10:l1">schanden</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Beschaemt, en seer <rhyme label="b" type="m">verbaest</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">Met schand' te rugghe <rhyme label="c" type="f">keeren</rhyme>;</l>
        <l id="s10:l5" met="3" type="-+">Met schaed' en schande <rhyme label="c" type="f" corresp="#s10:l4">leeren</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Dat Godt hem t'my-waers <rhyme label="b" type="m" corresp="#s10:l3">haest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
