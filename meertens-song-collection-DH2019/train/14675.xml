<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14675</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Het viel een hemelse dauw</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Pieter van Gelre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DOchters wilt <rhyme label="a" type="f">jubileren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van Syon <rhyme label="b" type="m">alghemeyn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Een nieuwe Coninck <rhyme label="a" type="f" corresp="#s1:l1">teere</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Is heden ons heel <rhyme label="b" type="m" corresp="#s1:l2">reyn</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Gheboren op dees <rhyme label="c" type="f">aerden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Man eene Maecht <rhyme label="d" type="m">eerbaer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Looft hem met groot <rhyme label="c" type="f" corresp="#s1:l5">eerwaerden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Al in dit Nieuwe <rhyme label="d" type="m" corresp="#s1:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Tot Bethelem der <rhyme label="a" type="f">Stede</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Daer is hy <rhyme label="b" type="m">voortgebracht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Daer die Voor-vaders <rhyme label="a" type="f" corresp="#s2:l1">mede</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Langh' hebben na <rhyme label="b" type="m" corresp="#s2:l2">gewacht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Een Maget is sijn <rhyme label="c" type="f">Moeder</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Hy is ons Godt <rhyme label="d" type="m">voorwaer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Oock mede onsen <rhyme label="c" type="f" corresp="#s2:l5">Broeder</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Al in dit Nieuwe <rhyme label="d" type="m" corresp="#s2:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Eng'len singen <rhyme label="a" type="f">schoone</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En geven goeden <rhyme label="b" type="m">wens</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Glori' sy Godt inden <rhyme label="a" type="f" corresp="#s3:l1">troone</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En vreed' hier met den <rhyme label="b" type="m" corresp="#s3:l2">mens</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die in goe' wille <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Want nu is <rhyme label="d" type="m">allegaer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Den Salighmaecker <rhyme label="c" type="f" corresp="#s3:l5">geven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Al in dit Nieuwe <rhyme label="d" type="m" corresp="#s3:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Het Woordt is vleesch <rhyme label="a" type="f">geworden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Verheucht u <rhyme label="b" type="m">Israel</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ras wilt u kleeders <rhyme label="a" type="f" corresp="#s4:l1">gorden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En gaet tot Beth'lem <rhyme label="b" type="m" corresp="#s4:l2">snel</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Daer leyt hy by de <rhyme label="c" type="f">beesten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">De Heere der <rhyme label="d" type="m">heyr-schaer</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Aenbidt hem reyn van <rhyme label="c" type="f" corresp="#s4:l5">geeste</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Al in dit Nieuwe <rhyme label="d" type="m" corresp="#s4:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daer leyt hy teer <rhyme label="a" type="f">gewonden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In arme boeckjes <rhyme label="b" type="m">snoot</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En wordt van beesten <rhyme label="a" type="f" corresp="#s5:l1">monden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ghewarmt: want 't is <rhyme label="b" type="m" corresp="#s5:l2">noodt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Sijn teere leetjes <rhyme label="c" type="f">beven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van groote koude <rhyme label="d" type="m">swaer</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Liefd' heeft hem dus <rhyme label="c" type="f" corresp="#s5:l5">ghedreven</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Al in dit Nieuwe <rhyme label="d" type="m" corresp="#s5:l6">Iaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Lof zy u Maget <rhyme label="a" type="f">reyne</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ghy die <rhyme label="b" type="m">Emanuel</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Dat edel suyver <rhyme label="a" type="f" corresp="#s6:l1">greyne</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Baerden voor <rhyme label="b" type="m" corresp="#s6:l2">Israel</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Waer van dat u nu <rhyme label="c" type="f">loven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">De Engelen seer <rhyme label="d" type="m">klaer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">In't schoon Syon hier <rhyme label="c" type="f" corresp="#s6:l5">boven</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Al in dit Nieuwe <rhyme label="d" type="m" corresp="#s6:l6">iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
