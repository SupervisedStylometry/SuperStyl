    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Es ist das Heil uns kommen her</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DEs HEEren goetheyt <rhyme label="a" type="m">Eewichlijc</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ick singh' wt Herten <rhyme label="b" type="f">gronde</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">End' syn Waerheyt <rhyme label="a" type="m" corresp="#s1:l1">ghestaedichlijck</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Vercond' met mijnen <rhyme label="b" type="f" corresp="#s1:l2">Monde</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En segg' een eeuwig' g'naed' op <rhyme label="c" type="m">gaet</rhyme></l>
        <l id="s1:l6" met="4" type="-+">End' ghy sult in des HEmels <rhyme label="c" type="m" corresp="#s1:l5">staet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dijn Waerheyt Trouw'lijck <rhyme label="d" type="f">houden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een vast Verbondt heb ick <rhyme label="a" type="m">ghesticht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met mijnen <rhyme label="b" type="f">Wtvercoren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Den David mijnen Vromen <rhyme label="a" type="m" corresp="#s2:l1">Knecht</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hebb' ick een Eedt <rhyme label="b" type="f" corresp="#s2:l2">gheswooren</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Verleenen dy een Eewich <rhyme label="c" type="m">Zaet</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">End' dijnen Stoel voor alle <rhyme label="c" type="m" corresp="#s2:l5">quaet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wil ick altijdt <rhyme label="d" type="f">Behouwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die HEmel prijsen sonder <rhyme label="a" type="m">Faut</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dijn Wonder (HEER) so <rhyme label="b" type="f">Reyne</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End' dijne Waerheyt <rhyme label="a" type="m" corresp="#s3:l1">Onbenaut</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">In dijn Heyligh' <rhyme label="b" type="f" corresp="#s3:l2">Ghemeyne</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Wie mach den HEERE syn <rhyme label="c" type="m">ghelijck</rhyme>?</l>
        <l id="s3:l6" met="4" type="-+">In Wolcken end' opt heel <rhyme label="c" type="m" corresp="#s3:l5">Aertrijck</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Onder der Goden <rhyme label="d" type="f">kinder</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Godt is heerlick van groote <rhyme label="a" type="m">Macht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In die Versaem der <rhyme label="b" type="f">Vromen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">End' Wonderbaerlijck hooch <rhyme label="a" type="m" corresp="#s4:l1">gheacht</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Boven die syn <rhyme label="b" type="f" corresp="#s4:l2">Rondtomme</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">HEER Zebaoth wie is <rhyme label="c" type="m">ghelijck</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Als ghy, een Machtich Godt <rhyme label="c" type="m" corresp="#s4:l5">Heerlijck</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dijn Waerheyt om end' <rhyme label="d" type="f">Omme</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy Heerst over de Zee <rhyme label="a" type="m">verwoet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">End' stillet syne <rhyme label="b" type="f">Baren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Egypten sloeghet ghy te <rhyme label="a" type="m" corresp="#s5:l1">Doot</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dijn sterckheyt ginckt niet <rhyme label="b" type="f" corresp="#s5:l2">sparen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Den HEmel end' die Aerd' is <rhyme label="c" type="m">dijn</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">De Werelt onderhoudt ghy <rhyme label="c" type="m" corresp="#s5:l5">fijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">End' al wat is <rhyme label="d" type="f">daerinne</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die tijden sijn in dijn <rhyme label="a" type="m">Bescherm</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dijn Naem is s'Lands <rhyme label="b" type="f">ghenoeghe</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Want ghy hebt een Almachtich <rhyme label="a" type="m" corresp="#s6:l1">Arm</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dijn Rechte Handt is <rhyme label="b" type="f" corresp="#s6:l2">hooghe</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Gherechticheyt end' Rechte <rhyme label="c" type="m">Leer</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Dat is dijn Stoel een vast <rhyme label="c" type="m" corresp="#s6:l5">gheweer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Sy moghen niet <rhyme label="d" type="f">Faelgieren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Genaed' end' waerheyt sijn all' <rhyme label="a" type="m">beyd</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Voor dijnen <rhyme label="b" type="f">Aenghesichte</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Hoe salich is dat Volck <rhyme label="a" type="m" corresp="#s7:l1">bereydt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dat Juychet in dijn <rhyme label="b" type="f" corresp="#s7:l2">Stichte</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Want (Heer) sy Wandlen al te <rhyme label="c" type="m">saem</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Int Licht dijns Aensichts, End' dijn <rhyme label="c" type="m" corresp="#s7:l5">Naem</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Daeg'lijcx sy vrolijck <rhyme label="d" type="f">prijsen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Heerlijck in dijn <rhyme label="a" type="m">Gherechticheyt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Sijn sy, want ghy gaet <rhyme label="b" type="f">stercken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">End' wilt na dijne <rhyme label="a" type="m" corresp="#s8:l1">Goedicheyt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Door ons so Roemlijck <rhyme label="b" type="f" corresp="#s8:l2">wercken</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Want onse Schilt dat is de <rhyme label="c" type="m">HEER</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Ons Coninck end' een goet <rhyme label="c" type="m" corresp="#s8:l5">Gheweer</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Die Heyligh' <rhyme label="d" type="f">Israelis</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Waerachtich hebt ghy Toe <rhyme label="a" type="m">geseyt</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dijn Heyl'ghen end' <rhyme label="b" type="f">Beminden</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Ick heb den Rechten Helt <rhyme label="a" type="m" corresp="#s9:l1">bereyt</rhyme></l>
        <l id="s9:l4" met="3" type="-+">By hem is Hulp te <rhyme label="b" type="f" corresp="#s9:l2">vinden</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Hooch wt den Volck is een <rhyme label="c" type="m">Vercoorn</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Dien heb ick mijnen Eedt <rhyme label="c" type="m" corresp="#s9:l5">gheswoorn</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Mijn Knecht David <rhyme label="d" type="f">ghevonden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Met Heyl'gen Oly heb ick <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Hem G'salfft tot wonder <rhyme label="b" type="f">Wercken</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Mijn Handt sal hem behouden <rhyme label="a" type="m" corresp="#s10:l1">vry</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">End' mijn Arm sal hem <rhyme label="b" type="f" corresp="#s10:l2">stercken</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Die Vyanden hem sullen <rhyme label="c" type="m">nicht</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Noch all' dat Ongherechte <rhyme label="c" type="m" corresp="#s10:l5">sticht</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Verwinnen noch <rhyme label="d" type="f">Vernielen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Verslaen wil ick al zijn <rhyme label="a" type="m">Party</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Sijn Haeters wil ick <rhyme label="b" type="f">Plaghen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Mijn Waerheyt end' Ghenade <rhyme label="a" type="m" corresp="#s11:l1">bly</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Sal by hem Oudt <rhyme label="b" type="f" corresp="#s11:l2">Bedaghen</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">End' syn Horn sal in mijnen <rhyme label="c" type="m">Naem</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Verheven syn met goede <rhyme label="c" type="m" corresp="#s11:l5">Faem</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">End' over al <rhyme label="d" type="f">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Sijn Rechte Hant sal Heyrschen <rhyme label="a" type="m">Vroe</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Int Meer end' groote <rhyme label="b" type="f">Water</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Hy sal my Noemen oock <rhyme label="a" type="m" corresp="#s12:l1">alsoe</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Ghy zijt mijn Rechte <rhyme label="b" type="f" corresp="#s12:l2">Vader</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Mijn Godt, mijn Troost, mijn Hulpe <rhyme label="c" type="m">schoon</rhyme></l>
        <l id="s12:l6" met="4" type="-+">End ick wil hem een Eersten <rhyme label="c" type="m" corresp="#s12:l5">Soon</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Hooch over al gaen <rhyme label="d" type="f">maecken</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Eewich so blijft hem mijn <rhyme label="a" type="m">Genaed'</rhyme></l>
        <l id="s13:l2" met="3" type="-+">End' mijn Verbondt sal <rhyme label="b" type="f">dueren</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">So lang' als duert des HEmels <rhyme label="a" type="m" corresp="#s13:l1">Staet</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Sijn Rijck wil ick <rhyme label="b" type="f" corresp="#s13:l2">volvueren</rhyme>.</l>
        <l id="s13:l5" met="4" type="-+">Maer so syn Kinder mijne <rhyme label="c" type="m">Wet</rhyme></l>
        <l id="s13:l6" met="4" type="-+">Verlaten, End' niet Wandlen <rhyme label="c" type="m" corresp="#s13:l5">Net</rhyme></l>
        <l id="s13:l7" met="3" type="-+">In mijnen Rechten <rhyme label="d" type="f">reyne</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Ontheyl'gen sy wat ick haer <rhyme label="a" type="m">sticht</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Versmaeden mijn <rhyme label="b" type="f">Gheboden</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">So wil ick oock nae mijn <rhyme label="a" type="m" corresp="#s14:l1">Ghericht</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Castijense met <rhyme label="b" type="f" corresp="#s14:l2">Roeden</rhyme>,</l>
        <l id="s14:l5" met="4" type="-+">Wel Plaghen wil ick haer <rhyme label="c" type="m">Misdaet</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">Maer nochtans wil ick mijn <rhyme label="c" type="m" corresp="#s14:l5">Ghenaed'</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Van haer niet heelicx <rhyme label="d" type="f">weynden</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Niet wil ick breken mijn <rhyme label="a" type="m">Verbondt</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Noch oock dat selff <rhyme label="b" type="f">verkeeren</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Wat is ghegaen wt mijnen <rhyme label="a" type="m" corresp="#s15:l1">Mont</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Ick heb eenmael gaen <rhyme label="b" type="f" corresp="#s15:l2">sweeren</rhyme></l>
        <l id="s15:l5" met="4" type="-+">By mijner eyghen <rhyme label="c" type="m">Heylicheet</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">Ick wil den David Lieghen <rhyme label="c" type="m" corresp="#s15:l5">niet</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Syn Zaedt sal Eewich <rhyme label="d" type="f">blijven</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Syn stoel als Son end' Maen voor <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Sal Eewich syn <rhyme label="b" type="f">behouwen</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Ghelijck de Regenboghe <rhyme label="a" type="m" corresp="#s16:l1">vry</rhyme></l>
        <l id="s16:l4" met="3" type="-+">In Wolcken is <rhyme label="b" type="f" corresp="#s16:l2">t'Aenschouwen</rhyme>.</l>
        <l id="s16:l5" met="4" type="-+">Maer nu verwerpt ghy end' <rhyme label="c" type="m">verstoot</rhyme></l>
        <l id="s16:l6" met="4" type="-+">Den G'salffden dijn in Thoorne <rhyme label="c" type="m" corresp="#s16:l5">groot</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">T'verbondt met dijnen <rhyme label="d" type="f">Knechte</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Syn Croone ghy ter Aerden <rhyme label="a" type="m">treet</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Verscheurt all' syne <rhyme label="b" type="f">Mueren</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Syn Vesten ghy al heel <rhyme label="a" type="m" corresp="#s17:l1">verplet</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Een Rooff is hy met <rhyme label="b" type="f" corresp="#s17:l2">treuren</rhyme>.</l>
        <l id="s17:l5" met="4" type="-+">T'beschimpen hem van alle <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s17:l6" met="4" type="-+">Want ghy verhoocht des Vyants <rhyme label="c" type="m" corresp="#s17:l5">handt</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Verblijdt all' syn <rhyme label="d" type="f">Bestrijders</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Victory mach hy Vechten <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Gheen hulp mach hem <rhyme label="b" type="f">Oorbooren</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Syn reynicheyt leyt int <rhyme label="a" type="m" corresp="#s18:l1">verdriet</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">T'is met syn Stoel <rhyme label="b" type="f" corresp="#s18:l2">verlooren</rhyme>,</l>
        <l id="s18:l5" met="4" type="-+">Syns Jonghen levens beste <rhyme label="c" type="m">sort</rhyme>,</l>
        <l id="s18:l6" met="4" type="-+">Die hebt ghy thoornich aff <rhyme label="c" type="m" corresp="#s18:l5">ghecort</rhyme>,</l>
        <l id="s18:l7" met="3" type="-+">Bedeckt hem met veel <rhyme label="d" type="f">smade</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Och Heer hoelang' wilt ghy noch <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Ons arme Sondaers <rhyme label="b" type="f">haten</rhyme>?</l>
        <l id="s19:l3" met="4" type="-+">Dijn grim noch als een Vuyr <rhyme label="a" type="m" corresp="#s19:l1">voortaen</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Verderff'lick branden <rhyme label="b" type="f" corresp="#s19:l2">laten</rhyme>?</l>
        <l id="s19:l5" met="4" type="-+">Bedenckt myns levens kleyne <rhyme label="c" type="m">Tal</rhyme>,</l>
        <l id="s19:l6" met="4" type="-+">Waerom wilt ghy de Menschen <rhyme label="c" type="m" corresp="#s19:l5">al</rhyme></l>
        <l id="s19:l7" met="3" type="-+">Doch maken so te <rhyme label="d" type="f">niete</rhyme>?</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Waer is yemant die levet <rhyme label="a" type="m">hier</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">End' hy den Doot niet <rhyme label="b" type="f">smaecke</rhyme>?</l>
        <l id="s20:l3" met="4" type="-+">Mach hy syn Siel bevrijen <rhyme label="a" type="m" corresp="#s20:l1">schier</rhyme></l>
        <l id="s20:l4" met="3" type="-+">Dat hem die Hell' niet <rhyme label="b" type="f" corresp="#s20:l2">raecke</rhyme>?</l>
        <l id="s20:l5" met="4" type="-+">HEER, waer is nu dijn G'nade <rhyme label="c" type="m">vroet</rhyme>,</l>
        <l id="s20:l6" met="4" type="-+">Die ghy beswoert den David <rhyme label="c" type="m" corresp="#s20:l5">goet</rhyme>,</l>
        <l id="s20:l7" met="3" type="-+">In dijner waerheyt <rhyme label="d" type="f">trouwe</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Gedenct Heer hoe dijn Knechten <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Versmaetheyt lijden <rhyme label="b" type="f">moeten</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">End' ick draecht al in mynen <rhyme label="a" type="m" corresp="#s21:l1">schoot</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Van Volcken kleyn en <rhyme label="b" type="f" corresp="#s21:l2">grooten</rhyme>,</l>
        <l id="s21:l5" met="4" type="-+">Want wat dijn Knechten wort <rhyme label="c" type="m">ghedaen</rhyme></l>
        <l id="s21:l6" met="4" type="-+">Dat gaet dy HEER, al selvest <rhyme label="c" type="m" corresp="#s21:l5">aen</rhyme>,</l>
        <l id="s21:l7" met="3" type="-+">Het is dijns Naemens <rhyme label="d" type="f">saecke</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Die Vyand', HEer, versmaden <rhyme label="a" type="m">dy</rhyme></l>
        <l id="s22:l2" met="3" type="-+">Als sy gaen ons <rhyme label="b" type="f">verfoeyen</rhyme>,</l>
        <l id="s22:l3" met="4" type="-+">Sy treden oock met Voeten <rhyme label="a" type="m" corresp="#s22:l1">vry</rhyme>,</l>
        <l id="s22:l4" met="3" type="-+">End' meynen wt te <rhyme label="b" type="f" corresp="#s22:l2">Roeyen</rhyme>,</l>
        <l id="s22:l5" met="4" type="-+">Dien ghy ghesalvet hebt O <rhyme label="c" type="m">HEER</rhyme>,</l>
        <l id="s22:l6" met="4" type="-+">Tot onsen Heyl en dijne <rhyme label="c" type="m" corresp="#s22:l5">Eer</rhyme>,</l>
        <l id="s22:l7" met="3" type="-+">HEER Eewich zijt <rhyme label="d" type="f">ghelovet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
