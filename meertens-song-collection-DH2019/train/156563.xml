    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Kom gij schone veldgodin</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GIlead was om <rhyme label="a" type="f">ghehanghen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Met een <rhyme label="b" type="m">Grassen-Rock</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Welckers loof was groen, en <rhyme label="a" type="f" corresp="#s1:l1">langhe</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Dat elcks ooghen <rhyme label="b" type="m" corresp="#s1:l2">trock</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Doen Ruben, ende <rhyme label="c" type="m">Gad</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">En Manass', haer <rhyme label="a" type="f" corresp="#s1:l1">gangen</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Streckten langhs dat <rhyme label="c" type="m" corresp="#s1:l5">padt</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vloogh de lust in door haer <rhyme label="a" type="f">ooghen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Tot in't <rhyme label="b" type="m">ingewant</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En heeft haer het hert <rhyme label="a" type="f" corresp="#s2:l1">ghebogen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Tot dat Gras-rijck <rhyme label="b" type="m" corresp="#s2:l2">Landt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">'t Welck was haer hooghste <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Daerom zy niet <rhyme label="a" type="f" corresp="#s2:l1">toghen</rhyme></l>
        <l id="s2:l7" met="3" type="+-">In Kan√°ans <rhyme label="c" type="m" corresp="#s2:l5">rust</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer de aertsche <rhyme label="a" type="f">Wereldtlinghen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Doen noch dwaser <rhyme label="b" type="m">saeck</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Want zy yd'le <rhyme label="a" type="f" corresp="#s3:l1">beuselinghen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Kiesen tot <rhyme label="b" type="m" corresp="#s3:l2">vermaeck</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">En 't Hemels waerdigh <rhyme label="c" type="m">goedt</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Om de Aerdtsche <rhyme label="a" type="f" corresp="#s3:l1">dinghen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Schoppen met de <rhyme label="c" type="m" corresp="#s3:l5">voet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
