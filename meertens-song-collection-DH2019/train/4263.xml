<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4263</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Precieus bepareld zaad</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">PRetieus bepeerelt // <rhyme label="a" type="m">saet</rhyme> //</l>
        <l id="s1:l2" met="3" type="-+">Die my verheugen <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Deur de gansche werelt // <rhyme label="a" type="m" corresp="#s1:l1">gaet</rhyme> //</l>
        <l id="s1:l4" met="3" type="-+">V lof met <rhyme label="b" type="m" corresp="#s1:l2">overvloet</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Want ghy hebt <rhyme label="b" type="m" corresp="#s1:l2">versoet</rhyme> //</l>
        <l id="s1:l6" met="3" type="-+">Mijn pijn, en grooten <rhyme label="c" type="f">rouwe</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Dies ick u lief <rhyme label="b" type="m" corresp="#s1:l2">moet</rhyme> //</l>
        <l id="s1:l8" met="3" type="-+">In liefden zijn getrou  <rhyme label="c" type="f" corresp="#s1:l6">we</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ieugdich, ende schoone // <rhyme label="a" type="m">maeght</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">In woorden heusch en <rhyme label="b" type="m">mildt</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Ghy die nu de croone // <rhyme label="a" type="m" corresp="#s2:l1">draeght</rhyme></l>
        <l id="s2:l4" met="3" type="-+">By Venus veel <rhyme label="b" type="m" corresp="#s2:l2">verschilt</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Want ghy hebt <rhyme label="b" type="m" corresp="#s2:l2">gestilt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Trumoer, dies ick <rhyme label="c" type="f">aenschouwe</rhyme></l>
        <l id="s2:l7" met="3" type="+-">Dat ghy u lief <rhyme label="b" type="m" corresp="#s2:l2">wilt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">In liefden zijn <rhyme label="c" type="f" corresp="#s2:l6">getrouwe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Eertijts mijn natuere // <rhyme label="a" type="m">weeck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Schier was ghemaect ter <rhyme label="b" type="m">doot</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Maer nu, mijn coleure // <rhyme label="a" type="m" corresp="#s3:l1">bleeck</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Verandert bloedich <rhyme label="b" type="m" corresp="#s3:l2">root</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Dies met reden <rhyme label="b" type="m" corresp="#s3:l2">groot</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ick u (O schoonste <rhyme label="c" type="f">vrouwe</rhyme>)</l>
        <l id="s3:l7" met="3" type="+-">Sal tot stervens <rhyme label="b" type="m" corresp="#s3:l2">noot</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In liefden zijn <rhyme label="c" type="f" corresp="#s3:l6">getrouwe</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Tis een groot verheugen // <rhyme label="a" type="m">bly</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als lief, lief vrientschap <rhyme label="b" type="m">iont</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">S'gelijcx hebt t'vermeugen // <rhyme label="a" type="m" corresp="#s4:l1">ghy</rhyme></l>
        <l id="s4:l4" met="3" type="-+">My te maken <rhyme label="b" type="m" corresp="#s4:l2">gesont</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Is my thert <rhyme label="b" type="m" corresp="#s4:l2">gewont</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Sluyt my den bant te <rhyme label="c" type="f">nouwe</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Ghy wilt my <rhyme label="b" type="m" corresp="#s4:l2">terstont</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In liefden zijn <rhyme label="c" type="f" corresp="#s4:l6">getrouwe</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Es my dees oorsake // <rhyme label="a" type="m">claer</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Tot liefden geen <rhyme label="b" type="m">gemerck</rhyme>:</l>
        <l id="s5:l3" met="3" type="+-">Crijgh ick, daer ick hake // <rhyme label="a" type="m" corresp="#s5:l1">naer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">T'is dijn genadich <rhyme label="b" type="m" corresp="#s5:l2">werck</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">Dies ick simpel <rhyme label="b" type="m" corresp="#s5:l2">clerck</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Onweerdich sulck <rhyme label="c" type="f">Kersouwe</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">V sal even <rhyme label="b" type="m" corresp="#s5:l2">sterck</rhyme></l>
        <l id="s5:l8" met="3" type="-+">In liefden zijn <rhyme label="c" type="f" corresp="#s5:l6">getrouwe</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Reynste sonder smette // <rhyme label="a" type="m">gans</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Princesse ionge <rhyme label="b" type="m">maeght</rhyme>:</l>
        <l id="s6:l3" met="3" type="+-">O mijn violette // <rhyme label="a" type="m" corresp="#s6:l1">crans</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Om u wert veel <rhyme label="b" type="m" corresp="#s6:l2">gewaeght</rhyme>:</l>
        <l id="s6:l5" met="3" type="+-">Oorlof wert <rhyme label="b" type="m" corresp="#s6:l2">gevraeght</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Aen u, met herten <rhyme label="c" type="f">flouwe</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Van my, die u <rhyme label="b" type="m" corresp="#s6:l2">draeght</rhyme></l>
        <l id="s6:l8" met="3" type="-+">In liefden vaste <rhyme label="c" type="f" corresp="#s6:l6">trouwe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
