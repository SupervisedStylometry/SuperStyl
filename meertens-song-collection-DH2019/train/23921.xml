  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DOen Maria ghehouwet <rhyme label="a" type="m">was</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En men de Schrifture <rhyme label="a" type="m" corresp="#s1:l1">las</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Wat seyde haren <rhyme label="b" type="m">man</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ick salt u <rhyme label="c" type="f">openbaren</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">soo ick <rhyme label="b" type="m" corresp="#s1:l3">can</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Joseph op onser Vrouwen <rhyme label="a" type="m">sach</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ende was seer <rhyme label="a" type="m" corresp="#s2:l1">vervaert</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Ende hy en seyde <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy peysde lieve <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Wat is my <rhyme label="b" type="m" corresp="#s2:l3">gheschiet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Joseph dacht in zijnen <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dede hy haer schande ten waer niet <rhyme label="a" type="m" corresp="#s3:l1">goet</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Hy peysde <rhyme label="b" type="m">dat</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hy haer ontgaen <rhyme label="c" type="f">woude</rhyme></l>
        <l id="s3:l5" met="2" type="-+">In een ander <rhyme label="b" type="m" corresp="#s3:l3">stadt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daer Joseph lach en <rhyme label="a" type="m">sliep</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Den Heylighen Gheest hem aen <rhyme label="a" type="m" corresp="#s4:l1">riep</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Dat was <rhyme label="b" type="m">alsoo</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Staet op Davids <rhyme label="c" type="f">Sone</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Ende weest <rhyme label="b" type="m" corresp="#s4:l3">vo</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Joseph weest niet <rhyme label="a" type="m">versaecht</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat kindt dat Maria <rhyme label="a" type="m" corresp="#s5:l1">draecht</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Dat is Godts <rhyme label="b" type="m">kint</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Den Heylighen Gheest is in haer <rhyme label="c" type="f">comen</rhyme></l>
        <l id="s5:l5" met="2" type="-+">En twijfelt <rhyme label="b" type="m" corresp="#s5:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">En waer dat kindt noyt <rhyme label="a" type="m">gheboren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Onse siele waer <rhyme label="a" type="m" corresp="#s6:l1">verloren</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Dat was <rhyme label="b" type="m">alsoo</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Staet op Davids <rhyme label="c" type="f">Sone</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Ende weest <rhyme label="b" type="m" corresp="#s6:l3">vro</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
