<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185829</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het was een maagd uitverkoren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Het was een Maget <rhyme label="a" type="f">wtvercoren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Daer Iesus af wilde zijn <rhyme label="a" type="f" corresp="#s1:l1">geboren</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s1:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s1:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al tot der stadt van <rhyme label="a" type="f">Nazareth</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Daer is een Maget <rhyme label="a" type="f" corresp="#s2:l1">onbesmet</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s2:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s2:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Groetse my met den name <rhyme label="a" type="f">mijn</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Segt dat ick haer kint wil <rhyme label="a" type="f" corresp="#s3:l1">zijn</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s3:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s3:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die Engel was een bode <rhyme label="a" type="f">goet</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hy voer derwaerts metter <rhyme label="a" type="f" corresp="#s4:l1">spoet</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s4:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s4:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy seyde: Godt groet u suyver <rhyme label="a" type="f">Maecht</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ghy zijt die Gode seer wel <rhyme label="a" type="f" corresp="#s5:l1">behaecht</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s5:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s5:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy wil van v geboren <rhyme label="a" type="f">zijn</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Iesus de lieve Meester <rhyme label="a" type="f" corresp="#s6:l1">mijn</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s6:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s6:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maria sprack: hoe mocht ick hem <rhyme label="a" type="f">gewinnen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Want ic noyt man en geerden om <rhyme label="a" type="f" corresp="#s7:l1">minnen</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s7:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s7:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De H. Geest sal in u <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s8:l2" met="4" type="-+">So den dauw valt op der <rhyme label="a" type="f" corresp="#s8:l1">bloemen</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s8:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s8:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maria weest <rhyme label="a" type="f">onversaecht</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Als den sone Gods die ghy <rhyme label="a" type="f" corresp="#s9:l1">draecht</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s9:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s9:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Hy heeft u daer toe <rhyme label="a" type="f">vercoren</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Hy wil verlossen dat was <rhyme label="a" type="f" corresp="#s10:l1">verloren</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s10:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s10:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Van alle sorgen ben ick <rhyme label="a" type="f">ontslegen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Die deerne Gods wil ick <rhyme label="a" type="f" corresp="#s11:l1">wesen</rhyme>,</l>
        <l id="s11:l3" met="2" type="-+">Ses ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s11:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s11:l3">benedicamus</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Maria viel neder op haer <rhyme label="a" type="f">knien</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Den wille Gods moet <rhyme label="a" type="f" corresp="#s12:l1">geschien</rhyme>,</l>
        <l id="s12:l3" met="2" type="-+">Des ben ick <rhyme label="b" type="m">vro</rhyme>.</l>
        <l id="s12:l4" met="6" type="-+">O, o, o, o, <rhyme label="b" type="m" corresp="#s12:l3">benedicamus</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
