<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19495</id>
    <date>
      <year-start>1677</year-start>
      <year-end>1677</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="spotlied" translation="mocking song">spotlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">HOe stoffen en <rhyme label="a" type="f">raasen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Hoe snoeven en <rhyme label="a" type="f" corresp="#s1:l1">blaasen</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hoe juychen en <rhyme label="a" type="f">toeten</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Die Moffen en <rhyme label="a" type="f" corresp="#s2:l1">Knoeten</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Noe se aan haer <rhyme label="a" type="f">Karke</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Soo vlitig sien <rhyme label="a" type="f" corresp="#s3:l1">warken</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Met koppigen <rhyme label="a" type="f">jver</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Joo langer joo <rhyme label="a" type="f" corresp="#s4:l1">stiver</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Dat men haest den <rhyme label="a" type="f">drempel</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Der pottigen <rhyme label="a" type="f" corresp="#s5:l1">Tempel</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">(Soo 't werrick soo voort <rhyme label="a" type="f">gaet</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">En d' jver het door <rhyme label="a" type="f" corresp="#s6:l1">staet</rhyme>)</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Van 't schuym der <rhyme label="a" type="f">kanalje</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">En 't oolickst <rhyme label="a" type="f" corresp="#s7:l1">rapalje</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Met lustige <rhyme label="a" type="f">schreden</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Sien sullen <rhyme label="a" type="f" corresp="#s8:l1">betreden</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">d'Onwrikbare <rhyme label="a" type="f">muuren</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Die konnen <rhyme label="a" type="f" corresp="#s9:l1">verduuren</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Het dond'ren van <rhyme label="a" type="f">HOPPE</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">En andere <rhyme label="a" type="f" corresp="#s10:l1">koppen</rhyme>,</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Die met reen noch <rhyme label="a" type="f">wetten</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Niet zijn te <rhyme label="a" type="f" corresp="#s11:l1">verzetten</rhyme>;</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">Soo hart als de <rhyme label="a" type="f">pylers</rhyme>,</l>
        <l id="s12:l2" met="2" type="-+">Soo dick als de <rhyme label="a" type="f" corresp="#s12:l1">stylers</rhyme>,</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="2" type="-+">In 't Nieuwe <rhyme label="a" type="f">getimmer</rhyme>,</l>
        <l id="s13:l2" met="2" type="-+">Daer niemant je <rhyme label="a" type="f" corresp="#s13:l1">nimmer</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="2" type="-+">Sal over <rhyme label="a" type="f">bedancken</rhyme>,</l>
        <l id="s14:l2" met="2" type="-+">Voor 't spillen van <rhyme label="a" type="f" corresp="#s14:l1">blancken</rhyme>,</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="2" type="-+">En aerde <rhyme label="a" type="f">vermetzel</rhyme>,</l>
        <l id="s15:l2" met="2" type="-+">Een onnut <rhyme label="a" type="f" corresp="#s15:l1">beletzel</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="2" type="-+">Voor Hanssen en <rhyme label="a" type="f">Lyzen</rhyme>,</l>
        <l id="s16:l2" met="2" type="-+">Die 't werrick <rhyme label="a" type="f" corresp="#s16:l1">verwijzen</rhyme>:</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="2" type="-+">Maer de Haagze <rhyme label="a" type="f">mode</rhyme>,</l>
        <l id="s17:l2" met="2" type="-+">Of nieuwer <rhyme label="a" type="f" corresp="#s17:l1">metode</rhyme>,</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="2" type="-+">Die moester <rhyme label="a" type="f">gehouwen</rhyme></l>
        <l id="s18:l2" met="2" type="-+">Al zoud't'er <rhyme label="a" type="f" corresp="#s18:l1">berouwen</rhyme>:</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="2" type="-+">Soo groots was de <rhyme label="a" type="f">zotheit</rhyme>,</l>
        <l id="s19:l2" met="2" type="-+">Soo trots was de <rhyme label="a" type="f" corresp="#s19:l1">botheit</rhyme></l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="2" type="-+">Van lompen en <rhyme label="a" type="f">lorren</rhyme>,</l>
        <l id="s20:l2" met="2" type="-+">Van Ezels met <rhyme label="a" type="f" corresp="#s20:l1">ooren</rhyme>,</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="2" type="-+">Die balde <rhyme label="a" type="f">besweeken</rhyme></l>
        <l id="s21:l2" met="2" type="-+">Om 't Ja-woordt te <rhyme label="a" type="f" corresp="#s21:l1">spreeken</rhyme>,</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="2" type="-+">Maakt nu maer goe <rhyme label="a" type="f">reck'ning</rhyme>,</l>
        <l id="s22:l2" met="2" type="-+">'t Was beter de <rhyme label="a" type="f" corresp="#s22:l1">teek'ning</rhyme></l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="2" type="-+">Gedaen bei je <rhyme label="a" type="f">Leden</rhyme>,</l>
        <l id="s23:l2" met="2" type="-+">Gingt nutter <rhyme label="a" type="f" corresp="#s23:l1">besteden</rhyme>;</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="2" type="-+">Dat is om t' <rhyme label="a" type="f">erbarmen</rhyme>,</l>
        <l id="s24:l2" met="2" type="-+">Jou arreme <rhyme label="a" type="f" corresp="#s24:l1">Armen</rhyme>,</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="2" type="-+">Die by Geus en <rhyme label="a" type="f">Papen</rhyme></l>
        <l id="s25:l2" met="2" type="-+">De kost moeten <rhyme label="a" type="f" corresp="#s25:l1">rapen</rhyme>;</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="2" type="-+">En bleef in je <rhyme label="a" type="f">Packhuys</rhyme>,</l>
        <l id="s26:l2" met="2" type="-+">En roerde jou <rhyme label="a" type="f" corresp="#s26:l1">backhuys</rhyme>,</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="2" type="-+">En schreeuwde en <rhyme label="a" type="f">snapte</rhyme></l>
        <l id="s27:l2" met="2" type="-+">Aldaer dat het <rhyme label="a" type="f" corresp="#s27:l1">klapte</rhyme>:</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="2" type="-+">Soo had je te <rhyme label="a" type="f">dencken</rhyme></l>
        <l id="s28:l2" met="2" type="-+">Veel beter <rhyme label="a" type="f" corresp="#s28:l1">geschencken</rhyme>,</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="2" type="-+">En fraeyer <rhyme label="a" type="f">belooningh</rhyme>,</l>
        <l id="s29:l2" met="2" type="-+">Van d'Opperste <rhyme label="a" type="f" corresp="#s29:l1">Koningh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
