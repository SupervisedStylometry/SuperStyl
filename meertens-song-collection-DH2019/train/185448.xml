<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185448</id>
    <date>
      <year-start>1720</year-start>
      <year-end>1720</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O filii et filiae</melody>
  <author>Johannes Stichter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Filij, &amp; <rhyme label="a" type="m">Filiae</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Rex caelestis, Rex <rhyme label="a" type="m" corresp="#s1:l1">gloriae</rhyme>,</l>
        <l id="s1:l3" met="6" type="-+">Morte surrexit hodie. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s1:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s1:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Et mane prima <rhyme label="a" type="m">Sabathi</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ad ostium <rhyme label="a" type="m" corresp="#s2:l1">monumenti</rhyme>,</l>
        <l id="s2:l3" met="6" type="-+">Accesserunt Discipuli. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s2:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s2:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Et Maria <rhyme label="a" type="m">Magdalene</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Et Jacobi, &amp; <rhyme label="a" type="m" corresp="#s3:l1">Salome</rhyme>,</l>
        <l id="s3:l3" met="6" type="-+">Venerunt Corpus ungere. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s3:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s3:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">In albis sedens <rhyme label="a" type="m">Angelus</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Praedixit <rhyme label="a" type="m" corresp="#s4:l1">mulieribus</rhyme>,</l>
        <l id="s4:l3" met="6" type="-+">In Galileja est Dominus. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s4:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s4:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Et Joannes <rhyme label="a" type="m">Apostolus</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Cucurrit Petio <rhyme label="a" type="m" corresp="#s5:l1">citius</rhyme>,</l>
        <l id="s5:l3" met="6" type="-+">Monumentum vidit prius. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s5:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s5:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Discipulis <rhyme label="a" type="m">adstantibus</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">In medio stetit <rhyme label="a" type="m" corresp="#s6:l1">Christus</rhyme>,</l>
        <l id="s6:l3" met="6" type="-+">Dicens: Pax Vobis omnibus. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s6:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s6:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ut intellexit <rhyme label="a" type="m">Didimus</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Quia surrexerat <rhyme label="a" type="m" corresp="#s7:l1">Jesus</rhyme>,</l>
        <l id="s7:l3" met="6" type="-+">Remansit fere dubius. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s7:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s7:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Vide Thomas, vide <rhyme label="a" type="m">latus</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Vide pedes, vide <rhyme label="a" type="m" corresp="#s8:l1">manus</rhyme>,</l>
        <l id="s8:l3" met="6" type="-+">Noli esse incredulus. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s8:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s8:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Quando Thomas vidit <rhyme label="a" type="m">Christum</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Pedes, manus, latus <rhyme label="a" type="m" corresp="#s9:l1">suum</rhyme>,</l>
        <l id="s9:l3" met="6" type="-+">Dixit: Tu es Deus meus. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s9:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s9:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Beati qui non <rhyme label="a" type="m">viderunt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Et firmiter <rhyme label="a" type="m" corresp="#s10:l1">crediderunt</rhyme>,</l>
        <l id="s10:l3" met="6" type="-+">Vitam aeternam habebunt. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s10:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s10:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">In hoc Festo <rhyme label="a" type="m">Sanctissimo</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Sit laus &amp; <rhyme label="a" type="m" corresp="#s11:l1">jubilatio</rhyme>,</l>
        <l id="s11:l3" met="6" type="-+">Benedicamus Domino. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s11:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s11:l3">alleluja</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ex quibus nos <rhyme label="a" type="m">Humillimas</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Devotas atque <rhyme label="a" type="m" corresp="#s12:l1">debitas</rhyme>,</l>
        <l id="s12:l3" met="6" type="-+">Deo dicamus gratias. <rhyme label="b" type="m">Alleluja</rhyme>.</l>
        <l id="s12:l4" met="6" type="-+">Alleluja, alleluja, <rhyme label="b" type="m" corresp="#s12:l3">alleluja</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
