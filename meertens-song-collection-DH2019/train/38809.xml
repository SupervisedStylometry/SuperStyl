<song>
  <meta>
    <id>38809</id>
    <date>
      <year-start>1616</year-start>
      <year-end>1616</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
