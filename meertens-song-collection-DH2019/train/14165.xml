    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="vermaanlied" translation="admonitory song">vermaanlied</tag>
    </tags>
    <melody>Psalm 128 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Frans vander Straten de jongere</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ghenade ende <rhyme label="a" type="f">vrede</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn lieue suster <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En s hemels <rhyme label="a" type="f" corresp="#s1:l1">ghesinthede</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wensche Ick u voor een <rhyme label="b" type="m" corresp="#s1:l2">groet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat ghy daer door meucht <rhyme label="c" type="f">smaken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Altijt wat hemels <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En u wel bereyt <rhyme label="c" type="f" corresp="#s1:l5">maken</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Om daer te comen <rhyme label="d" type="m" corresp="#s1:l6">fris</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Om daer eens te <rhyme label="a" type="f">gheraken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">So moeten wy met <rhyme label="b" type="m">vliet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Neerstich bidden en <rhyme label="a" type="f" corresp="#s2:l1">waken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Also Christus <rhyme label="b" type="m" corresp="#s2:l2">ghebiet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Den Heere van herten <rhyme label="c" type="f">vreesen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Mijden der sonden <rhyme label="d" type="m">stanck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ende goet doen <rhyme label="c" type="f" corresp="#s2:l5">ghepreesen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Alle ons leven <rhyme label="d" type="m" corresp="#s2:l6">lanck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Alsoo langhe als wy <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Moeten wy hier <rhyme label="b" type="m">devijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Den heere vast <rhyme label="a" type="f" corresp="#s3:l1">ancleven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En hem ghehoorsaem <rhyme label="b" type="m" corresp="#s3:l2">sijn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ons selven heel <rhyme label="c" type="f">versaken</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ende te buyten <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ons lichaem suyver <rhyme label="c" type="f" corresp="#s3:l5">maken</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Van al dat streckt tot <rhyme label="d" type="m" corresp="#s3:l6">quaen</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Reynicht u mijn <rhyme label="a" type="f">beminde</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Suster al in den <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Als die wijse <rhyme label="a" type="f" corresp="#s4:l1">ghesinde</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Al tijt meer ende <rhyme label="b" type="m" corresp="#s4:l2">meer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Neempt getrauwelick <rhyme label="c" type="f">waer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">V beroepinghe <rhyme label="d" type="m">jdoon</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Soo sult ghy hier <rhyme label="c" type="f" corresp="#s4:l5">naer</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Ontfaen d'eewighe <rhyme label="d" type="m" corresp="#s4:l6">Croon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Verciert u' O Bruyt <rhyme label="a" type="f">vercooren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Des Lams seer <rhyme label="b" type="m">excelent</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">In deucht na den <rhyme label="a" type="f" corresp="#s5:l1">behooren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wast op tot inden <rhyme label="b" type="m" corresp="#s5:l2">ent</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die liefde laet niet <rhyme label="c" type="f">vercauden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Maer wilt vierich voort <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Op dat ghy meucht <rhyme label="c" type="f" corresp="#s5:l5">behauden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">t'Gheen dat ghy hebt <rhyme label="d" type="m" corresp="#s5:l6">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wilt ghetrauwe <rhyme label="a" type="f">bewaren</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dien costelicken <rhyme label="b" type="m">schat</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die ghy (hoort mijn <rhyme label="a" type="f" corresp="#s6:l1">verclaren</rhyme>)</l>
        <l id="s6:l4" met="3" type="-+">Hebt in het aerden <rhyme label="b" type="m" corresp="#s6:l2">Vat</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ontfanghen vanden <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Door zijn liefde <rhyme label="d" type="m">minioot</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Houdt hem in grooter <rhyme label="c" type="f" corresp="#s6:l5">eere</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Al tijts tot inde <rhyme label="d" type="m" corresp="#s6:l6">doot</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">In dien ghy hebt <rhyme label="a" type="f">beginnen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dus vierich inden <rhyme label="b" type="m">gheest</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wilt met herten en <rhyme label="a" type="f" corresp="#s7:l1">sinnen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Daer in wandelen <rhyme label="b" type="m" corresp="#s7:l2">meest</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Hebt Godt altijt voor <rhyme label="c" type="f">ooghen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Ende int herte <rhyme label="d" type="m">me</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En wilt u neerstich <rhyme label="c" type="f" corresp="#s7:l5">pooghen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Nae die hemelsche <rhyme label="d" type="m" corresp="#s7:l6">Ste</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Int voor nemen wilt niet <rhyme label="a" type="f">verflauwen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Inden gheest vierich <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Int gebet wilt niet op <rhyme label="a" type="f" corresp="#s8:l1">hauwen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Schickt u al na den <rhyme label="b" type="m" corresp="#s8:l2">tijt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Weest vroolick inder <rhyme label="c" type="f">hoopen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Verduldich in druck en <rhyme label="d" type="m">arbeyt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">En wilt gheduldich <rhyme label="c" type="f" corresp="#s8:l5">loopen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Inden strijt die ons vooren <rhyme label="d" type="m" corresp="#s8:l6">leyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Treckt cloeckelick ten <rhyme label="a" type="f">strijde</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Al met het Sweert des <rhyme label="b" type="m">geest</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Verwint tot allen <rhyme label="a" type="f" corresp="#s9:l1">tijden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En weest doch niet <rhyme label="b" type="m" corresp="#s9:l2">bevreest</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Neempt Gods Harnas vol <rhyme label="c" type="f">eeren</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Haut alsoo goede <rhyme label="d" type="m">wacht</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Om t'quaet van t'Huys te <rhyme label="c" type="f" corresp="#s9:l5">weeren</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Neerstelick sorghe <rhyme label="d" type="m" corresp="#s9:l6">draecht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Den Tempele <rhyme label="a" type="f">jdoone</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Soo die Schrift <rhyme label="b" type="m">allciert</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Moet suyver zijn en <rhyme label="a" type="f" corresp="#s10:l1">schoone</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Daer Godts gheest in <rhyme label="b" type="m" corresp="#s10:l2">logiert</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Laet ons om ons <rhyme label="c" type="f">beclyven</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Ons selven nemen <rhyme label="d" type="m">waer</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Dat Godts gheest in ons <rhyme label="c" type="f" corresp="#s10:l5">blyven</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Mach en woonen al <rhyme label="d" type="m" corresp="#s10:l6">daer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Des Heeren gheest <rhyme label="a" type="f">bequame</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Wensch ick u beyde <rhyme label="b" type="m">hier</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Mijn suster Seyntghen met <rhyme label="a" type="f" corresp="#s11:l1">name</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Met uwen man <rhyme label="b" type="m" corresp="#s11:l2">goedertier</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Wilt mijn simpel <rhyme label="c" type="f">ghedichte</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Wt liefden doch <rhyme label="d" type="m">ghedaen</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">En beghere al ist <rhyme label="c" type="f" corresp="#s11:l5">slichte</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Bidt ick in danck <rhyme label="d" type="m" corresp="#s11:l6">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Hier mede wil ick <rhyme label="a" type="f">enden</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Mijn simpel <rhyme label="b" type="m">ghesanck</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Willet int goede <rhyme label="a" type="f" corresp="#s12:l1">wenden</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Reyne liefde my <rhyme label="b" type="m" corresp="#s12:l2">dwanck</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Wilt my altijt <rhyme label="c" type="f">ghedencken</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">In u ghebet tot <rhyme label="d" type="m">Godt</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">My dat tot hulpe <rhyme label="c" type="f" corresp="#s12:l5">schencken</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Bidt ick voor een <rhyme label="d" type="m" corresp="#s12:l6">slodt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Adieu Suster <rhyme label="a" type="f">bequame</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Adieu myn vrinden <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Wandelt int licht <rhyme label="a" type="f" corresp="#s13:l1">eersame</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Hier in dit duyster <rhyme label="b" type="m" corresp="#s13:l2">dal</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Hier mede wil ick <rhyme label="c" type="f">scheyden</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Wt dit simpel <rhyme label="d" type="m">ghesanck</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Godts Geest wil u <rhyme label="c" type="f" corresp="#s13:l5">gheleyden</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">En stieren uwe <rhyme label="d" type="m" corresp="#s13:l6">ganck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
