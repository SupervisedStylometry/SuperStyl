    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="gezongen onze vader" translation="sung Lord's prayer">gezongen Onze Vader</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">BRoeders laet ons <rhyme label="a" type="f">aenbeden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Den Uader inden <rhyme label="b" type="m">gheest</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En inder waerheyt <rhyme label="a" type="f" corresp="#s1:l1">meden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Sulck ghebet verhoort hy <rhyme label="b" type="m" corresp="#s1:l2">meest</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Laet ydel woorden <rhyme label="c" type="f">schouwen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat Heydens ydel <rhyme label="d" type="m">gheschal</rhyme></l>
        <l id="s1:l7" met="3" type="-+">En vast op God <rhyme label="c" type="f" corresp="#s1:l5">betrouwen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dat hy ons verhooren <rhyme label="d" type="m" corresp="#s1:l6">sal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ons Uader in <rhyme label="a" type="f">hemelrijcke</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Aensiet v kinderen <rhyme label="b" type="m">al</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die wonen op <rhyme label="a" type="f" corresp="#s2:l1">eerderijcke</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In dit droeue iammer <rhyme label="b" type="m" corresp="#s2:l2">dal</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Ghy zijt alleene ons <rhyme label="c" type="f">hope</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ons troost, ons <rhyme label="d" type="m">toeuerlaet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Willen wy Babylon <rhyme label="c" type="f" corresp="#s2:l5">ontlopen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Wy moeten doen uwen <rhyme label="d" type="m" corresp="#s2:l6">raet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O Uader uwen <rhyme label="a" type="f">name</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die moet gheheylicht <rhyme label="b" type="m">zijn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Sonder lachter ofte <rhyme label="a" type="f" corresp="#s3:l1">blame</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Door Christus leeringhen <rhyme label="b" type="m" corresp="#s3:l2">fijn</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Dat wy v alle <rhyme label="c" type="f">louen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dancken, ende gheuen <rhyme label="d" type="m">prijs</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Uwen name gaet verre te <rhyme label="c" type="f" corresp="#s3:l5">bouen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Alle andere namen <rhyme label="d" type="m" corresp="#s3:l6">wijs</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Toecomende sy ons v <rhyme label="a" type="f">Rijcke</rhyme></l>
        <l id="s4:l2" met="3" type="-+">O milde Uader <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Daer door wy Babylon <rhyme label="a" type="f" corresp="#s4:l1">bezwijcken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met haren hooghen <rhyme label="b" type="m" corresp="#s4:l2">moet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Sterct ons met uwen <rhyme label="c" type="f">gheeste</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Uerlicht ons door v <rhyme label="d" type="m">woort</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Teghen die Babylonsche <rhyme label="c" type="f" corresp="#s4:l5">beeste</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Weest onser sielen <rhyme label="d" type="m" corresp="#s4:l6">confoort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Uwen heylighen wille o <rhyme label="a" type="f">Uadere</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Gheschiede op <rhyme label="b" type="m">eerderijcke</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Onder ons alle <rhyme label="a" type="f" corresp="#s5:l1">gadere</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ghelijck in <rhyme label="b" type="m" corresp="#s5:l2">hemelrijcke</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Al wilt ons vleeschs <rhyme label="c" type="f">partien</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Soecken zijn eyghen <rhyme label="d" type="m">lust</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Doet onsen gheest <rhyme label="c" type="f" corresp="#s5:l5">verblijen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">In uwen wille <rhyme label="d" type="m" corresp="#s5:l6">gherust</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O Uader wilt ons <rhyme label="a" type="f">gheuen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Heden ons daghelicx <rhyme label="b" type="m">broot</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Op dat wy moghen <rhyme label="a" type="f" corresp="#s6:l1">leuen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En weeren tvleesch den <rhyme label="b" type="m" corresp="#s6:l2">noot</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Wilt meest ons siele <rhyme label="c" type="f">spijsen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Met twoort van uwen <rhyme label="d" type="m">mont</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Op dat wy <rhyme label="c" type="f" corresp="#s6:l5">veriolijsen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Wt al onser herten <rhyme label="d" type="m" corresp="#s6:l6">gront</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wilt ons misdaet <rhyme label="a" type="f">vergheuen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Barmhertich Uader <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Op dat wy niet en <rhyme label="a" type="f" corresp="#s7:l1">beuen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Uoor deewighe helsche <rhyme label="b" type="m" corresp="#s7:l2">gloet</rhyme></l>
        <l id="s7:l5" met="3" type="-+">En laet ons oock niet <rhyme label="c" type="f">wreken</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Uan dat ons is <rhyme label="d" type="m">misdaen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Maer ander lieden <rhyme label="c" type="f" corresp="#s7:l5">ghebreken</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Tuwer eeren ten besten <rhyme label="d" type="m" corresp="#s7:l6">slaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ghetrouwe lieue <rhyme label="a" type="f">Uader</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En wilt van ons niet <rhyme label="b" type="m">scheen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Weest altijt ons <rhyme label="a" type="f" corresp="#s8:l1">berader</rhyme></l>
        <l id="s8:l4" met="3" type="-+">En laet ons niet <rhyme label="b" type="m" corresp="#s8:l2">verleen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Het Babylonsche <rhyme label="c" type="f">rijcke</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Bestormt ons ouer <rhyme label="d" type="m">al</rhyme></l>
        <l id="s8:l7" met="3" type="-+">De duyuelsche <rhyme label="c" type="f" corresp="#s8:l5">practijcke</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Meent ons te bringhen int heltsche <rhyme label="d" type="m" corresp="#s8:l6">dal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Uerlost ons vanden <rhyme label="a" type="f">quaden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dat ons ront om <rhyme label="b" type="m">beuanct</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Want by des duyuels <rhyme label="a" type="f" corresp="#s9:l1">raden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Tghelooue wert seer <rhyme label="b" type="m" corresp="#s9:l2">ghecranct</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Men siet de liefde <rhyme label="c" type="f">flouwen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">De broosheyt neemt <rhyme label="d" type="m">douerhant</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Die werelt is vol <rhyme label="c" type="f" corresp="#s9:l5">ontrouwen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Dus noot is aen elcken <rhyme label="d" type="m" corresp="#s9:l6">cant</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Nv laet ons al te <rhyme label="a" type="f">samen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Den Uader gheuen <rhyme label="b" type="m">prijs</rhyme></l>
        <l id="s10:l3" met="3" type="-+">En singhen hem danckelick <rhyme label="a" type="f" corresp="#s10:l1">Amen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Hy is almachtich ende <rhyme label="b" type="m" corresp="#s10:l2">wijs</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Hy is alleene ons <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Ons Keyser, ende ons <rhyme label="d" type="m">Coninck</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Sy zijn al buyten <rhyme label="c" type="f" corresp="#s10:l5">keere</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Die hopen op ander <rhyme label="d" type="m" corresp="#s10:l6">dinck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
