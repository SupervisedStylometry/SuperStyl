  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+">Zie <rhyme label="a" type="m">daer</rhyme>,</l>
        <l id="s1:l2" met="1" type="-+">Hoe <rhyme label="a" type="m" corresp="#s1:l1">klaer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Blinkt deze <rhyme label="a" type="m" corresp="#s1:l1">moezelaer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In 't vol geschonken <rhyme label="b" type="f">glaesje</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">Ik geef 't u nobel <rhyme label="b" type="f" corresp="#s1:l4">baesje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hy heugt wel dertig <rhyme label="a" type="m" corresp="#s1:l1">jaer</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Hy 's <rhyme label="c" type="f">geurig</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Hy 's <rhyme label="c" type="f" corresp="#s1:l7">kleurig</rhyme>,</l>
        <l id="s1:l9" met="1" type="-+">Hy 's <rhyme label="d" type="m">fris</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Als wyn in myne kelder <rhyme label="d" type="m" corresp="#s1:l9">is</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Voor dezen ouden <rhyme label="e" type="m">traen</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Moet Venus achter <rhyme label="e" type="m" corresp="#s1:l11">staen</rhyme>.</l>
      </lg>
    </lg>
  </text>
