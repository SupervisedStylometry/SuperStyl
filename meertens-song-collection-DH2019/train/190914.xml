<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190914</id>
    <date>
      <year-start>1845</year-start>
      <year-end>1845</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Gijsbertus van Sandwijk</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Kampers op deze <rhyme label="a" type="m">eerebaan</rhyme>!</l>
        <l id="s1:l2" met="3" type="+-">Komt, ten strijd <rhyme label="b" type="f">getogen</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">'t Lauwerkransje lacht ons <rhyme label="a" type="m" corresp="#s1:l1">aan</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Schittert ons in de <rhyme label="b" type="f" corresp="#s1:l2">oogen</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Treden wij, vol moed, in 't <rhyme label="c" type="m">perk</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Als bedachte <rhyme label="d" type="f">kindren</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">Komt dan, vlijtig aan het <rhyme label="c" type="m" corresp="#s1:l5">werk</rhyme>;</l>
        <l id="s1:l8" met="3" type="+-">Niets, niets moge ons <rhyme label="d" type="f" corresp="#s1:l6">hindren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Kampers op deze <rhyme label="a" type="m">eereban</rhyme>!</l>
        <l id="s2:l2" met="3" type="+-">Komt, ten strijd <rhyme label="b" type="f">getogen</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">'t Lauverkransje lacht ons <rhyme label="a" type="m" corresp="#s2:l1">aan</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Schittert ons in de <rhyme label="b" type="f" corresp="#s2:l2">oogen</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Treden wij, vol moed, in 't <rhyme label="c" type="m">perk</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Als bedachte <rhyme label="d" type="f">kindren</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Komt dan, vlijtig aan het <rhyme label="c" type="m" corresp="#s2:l5">werk</rhyme>;</l>
        <l id="s2:l8" met="3" type="+-">Niets, niets moge ons <rhyme label="d" type="f" corresp="#s2:l6">hindren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Blijken zal het, wie hier <rhyme label="a" type="m">steeds</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Vlijt met lust mogt <rhyme label="b" type="f">paren</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Blijken zal het, wie hier <rhyme label="a" type="m" corresp="#s3:l1">reeds</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Kennis mogt <rhyme label="b" type="f" corresp="#s3:l2">verg√¢ren</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Hij, die woekert met zijn' <rhyme label="c" type="m">tijd</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Kan ook vordring <rhyme label="d" type="f">toonen</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Ziet zijn naarstigheid en <rhyme label="c" type="m" corresp="#s3:l5">vlijt</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Luisterrijk <rhyme label="d" type="f" corresp="#s3:l6">bekroonen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
