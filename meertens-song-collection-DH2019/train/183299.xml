<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183299</id>
    <date>
      <year-start>1752</year-start>
      <year-end>1752</year-end>
    </date>
    <location>
      <orginal-name>gorinchem</orginal-name>
      <standard-name>Gorinchem, The Netherlands</standard-name>
      <coordinates>
        <lon>51.8296133</lon>
        <lat>4.9738743</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoonste nimf van het woud</melody>
  <author>Jacob Groenewegen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Rots des heyls o levens <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Stelpt mijn <rhyme label="a" type="m" corresp="#s1:l1">dorst</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En mijn hertelijk ziels <rhyme label="b" type="f">verlangen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hoor het zugten van mijn <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Dat <rhyme label="c" type="m" corresp="#s1:l4">verward</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Steeds in ongeloof blijft <rhyme label="b" type="f" corresp="#s1:l3">hangen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Schenk mijn ziel geloof en <rhyme label="a" type="m">ligt</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">En <rhyme label="a" type="m" corresp="#s2:l1">gezigt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Van u geest aan my <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Leer my kenne ’s Heeren <rhyme label="c" type="m">werk</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">En uw <rhyme label="c" type="m" corresp="#s2:l4">merk</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Op dat ik in blijdschap <rhyme label="b" type="f" corresp="#s2:l3">leeven</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Roept met ligt en kragt my <rhyme label="a" type="m">toe</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Ziele <rhyme label="a" type="m" corresp="#s3:l1">hoe</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Leeft gy zoo vol angst en <rhyme label="b" type="f">vreezen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">’K ben u God u deel en <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Hoe <rhyme label="c" type="m" corresp="#s3:l4">gerust</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En vol blydschap zou ik <rhyme label="b" type="f" corresp="#s3:l3">weezen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hebt gy niet o Heer mijn <rhyme label="a" type="m">hert</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Doet ’t met <rhyme label="a" type="m" corresp="#s4:l1">smert</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Over zonde was <rhyme label="b" type="f">verbrooken</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Door u geest op ’t <rhyme label="c" type="m">onverzienst</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Tot u <rhyme label="c" type="m" corresp="#s4:l4">dienst</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Zuyverlijk in lust <rhyme label="b" type="f" corresp="#s4:l3">ontstoken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Heb ik my niet Godes <rhyme label="a" type="m">Zoon</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Voor u <rhyme label="a" type="m" corresp="#s5:l1">troon</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Gans veroordeelt als <rhyme label="b" type="f">onwaardig</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En my zondig Adams <rhyme label="c" type="m">kint</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Naakt en <rhyme label="c" type="m" corresp="#s5:l4">blint</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Regt gezien en God <rhyme label="b" type="f" corresp="#s5:l3">regtvaardig</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dreef my dat niet na u <rhyme label="a" type="m">heen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Met <rhyme label="a" type="m" corresp="#s6:l1">geween</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Kwam ik niet met al mijn <rhyme label="b" type="f">zonden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Tot u vlugte gans <rhyme label="c" type="m">verbaast</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Om met <rhyme label="c" type="m" corresp="#s6:l4">haast</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">My te zien in u <rhyme label="b" type="f" corresp="#s6:l3">gevonden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ben ik niet met al mijn <rhyme label="a" type="m">schuld</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Zoo <rhyme label="a" type="m" corresp="#s7:l1">vervult</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En omringt met ziels <rhyme label="b" type="f">elende</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">My aan u geworden <rhyme label="c" type="m">kwijt</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Toen ter <rhyme label="c" type="m" corresp="#s7:l4">tijd</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Kwam gy u niet tot my <rhyme label="b" type="f" corresp="#s7:l3">wende</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Zey mijn hert niet vroeg en <rhyme label="a" type="m">spa</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Vrolijk <rhyme label="a" type="m" corresp="#s8:l1">ja</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Op u aanbod van <rhyme label="b" type="f">genade</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Zelfs door kruys en <rhyme label="c" type="m">tegenspoed</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Goed en <rhyme label="c" type="m" corresp="#s8:l4">bloed</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Kost ik dat niet al <rhyme label="b" type="f" corresp="#s8:l3">versmade</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">En is dat niet en nog <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Toets dan <rhyme label="a" type="m" corresp="#s9:l1">Heer</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">’t Werk van uwe eyge <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Wast mijn eyge herts <rhyme label="c" type="m">bedrog</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">’k Wil dan <rhyme label="c" type="m" corresp="#s9:l4">nog</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">My geheel aan u <rhyme label="b" type="f" corresp="#s9:l3">verpanden</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Was mijn herte niet <rhyme label="a" type="m">verheugt</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-">en met <rhyme label="a" type="m" corresp="#s10:l1">vreugt</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Ia vervult met zoete <rhyme label="b" type="f">vrede</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Doen mijn ziel met u o <rhyme label="c" type="m">God</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">Als haar <rhyme label="c" type="m" corresp="#s10:l4">lot</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Roeme kost in <rhyme label="b" type="f" corresp="#s10:l3">eeuwigheden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Was die vreugt dan maar <rhyme label="a" type="m">bedrog</rhyme>,</l>
        <l id="s11:l2" met="2" type="+-">Proef my <rhyme label="a" type="m" corresp="#s11:l1">nog</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">’K wil niet voor den Heere <rhyme label="b" type="f">liegen</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Geef mijn ziele nu nog <rhyme label="c" type="m">ligt</rhyme>,</l>
        <l id="s11:l5" met="2" type="+-">En <rhyme label="c" type="m" corresp="#s11:l4">gezigt</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Dat ik my niet mogt <rhyme label="b" type="f" corresp="#s11:l3">bedriegen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Is niet nog mijn hert o <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s12:l2" met="2" type="+-">Voor u <rhyme label="a" type="m" corresp="#s12:l1">teer</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Met hoogagting <rhyme label="b" type="f">ingenomen</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">U gemeynschap en <rhyme label="c" type="m">gebied</rhyme>,</l>
        <l id="s12:l5" met="2" type="+-">Is dat <rhyme label="c" type="m" corresp="#s12:l4">niet</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Mijn vermaak met al de <rhyme label="b" type="f" corresp="#s12:l3">vroomen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Is niet Heer mijn hert <rhyme label="a" type="m">gezet</rhyme>,</l>
        <l id="s13:l2" met="2" type="+-">Om u <rhyme label="a" type="m" corresp="#s13:l1">wet</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">En u wege te <rhyme label="b" type="f">bewaren</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">En dat ik mijn herte <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s13:l5" met="2" type="+-">Langs hoe <rhyme label="c" type="m" corresp="#s13:l4">meer</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Daar toe aan u mogt <rhyme label="b" type="f" corresp="#s13:l3">verklaren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Doet my steeds gelovig <rhyme label="a" type="m">staan</rhyme>,</l>
        <l id="s14:l2" met="2" type="+-">In u <rhyme label="a" type="m" corresp="#s14:l1">paan</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Leer my wandelen zonder <rhyme label="b" type="f">vrezen</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Laat het zondig <rhyme label="c" type="m">ongeloof</rhyme>,</l>
        <l id="s14:l5" met="2" type="+-">Nooyt een <rhyme label="c" type="m" corresp="#s14:l4">roof</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Van mijn ziele blijdschap <rhyme label="b" type="f" corresp="#s14:l3">wezen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Dan zal regt de ziele <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s15:l2" met="2" type="+-">Bezig <rhyme label="a" type="m" corresp="#s15:l1">zijn</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">In u eere te <rhyme label="b" type="f">bewyzen</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Tot dat ik eeuwig boven <rhyme label="c" type="m">stof</rhyme>,</l>
        <l id="s15:l5" met="2" type="+-">Uwen <rhyme label="c" type="m" corresp="#s15:l4">lof</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">En u heerlijkheid zal <rhyme label="b" type="f" corresp="#s15:l3">pryzen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
