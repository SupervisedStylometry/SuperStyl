    <melody>Psalm 023 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Frans vander Straten de jongere</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Hantreycking' en ghenade wilt my <rhyme label="a" type="f">gheven</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">God in goetheyt en macht seer hoch <rhyme label="a" type="f" corresp="#s1:l1">verheven</rhyme></l>
        <l id="s1:l3" met="5" type="-+">V maysteyt dat ic te recht mach <rhyme label="b" type="f">prijsen</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">En metter daet goe danckbaerheyt <rhyme label="b" type="f" corresp="#s1:l3">bewijsen</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Voor al die schoon en heerlicke <rhyme label="c" type="f">weldaden</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Van u ontfaen o fonteyn der <rhyme label="c" type="f" corresp="#s1:l5">genaden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Nacht ende dach dat ic mach <rhyme label="a" type="f">overweghen</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Met een ghemoet ganschlick daer toe, <rhyme label="a" type="f" corresp="#s2:l1">genegen</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">V goetheyt groot, en die louen te <rhyme label="b" type="f">deghen</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Dat ick van u de kennis heb <rhyme label="b" type="f" corresp="#s2:l3">ghecreghen</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Om nae u wil goet ende <rhyme label="c" type="f">welehaghen</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Te dijenen u al mijnes levens <rhyme label="c" type="f" corresp="#s2:l5">daghen</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Reyn onbelet van des waerheyts <rhyme label="a" type="f">vyanden</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Doet dat ick u hier diene in dees <rhyme label="a" type="f" corresp="#s3:l1">landen</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Alwaer ick nu in vryheyt bin <rhyme label="b" type="f">ghecomen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Soo dat ick geen vervolch behoef te <rhyme label="b" type="f" corresp="#s3:l3">schromen</rhyme></l>
        <l id="s3:l5" met="5" type="-+">Gaer van ghy my genadelijc hebt gaen <rhyme label="c" type="f">vrijen</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Doet doch dat dit ten besten mach <rhyme label="c" type="f" corresp="#s3:l5">gedien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Out ende Jonck van dijne uyt <rhyme label="a" type="f">ghelesen</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Behoeven nu niet alsoo seer te <rhyme label="a" type="f" corresp="#s4:l1">vreesen</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Vyanden quaet, die t'Lichaem willen <rhyme label="b" type="f">quellen</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Maer veel meer die den mensch' brengen ter <rhyme label="b" type="f" corresp="#s4:l3">hellen</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">Als duyvel en werelt met vleesche <rhyme label="c" type="f">lusten</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Daer teghen elck Christen hen wel moet <rhyme label="c" type="f" corresp="#s4:l5">rusten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Soo wanneer als s'lichaems vyanden <rhyme label="a" type="f">swijgen</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Gemeenlic dan d'andre victori <rhyme label="a" type="f" corresp="#s5:l1">crijgen</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Als d'mensch is vry van des lichaems <rhyme label="b" type="f">benauwen</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Sietmen hem veel tijts in het goet <rhyme label="b" type="f" corresp="#s5:l3">verflauwen</rhyme>,</l>
        <l id="s5:l5" met="5" type="-+">Dus bidt ick Heer met ootmoediger <rhyme label="c" type="f">stemme</rhyme>,</l>
        <l id="s5:l6" met="5" type="-+">Doet dat ic dees vyanden te recht <rhyme label="c" type="f" corresp="#s5:l5">temme</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Doch doet my nu daerom seer wacker <rhyme label="a" type="f">waken</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Dat dees vryheyt my niet sorchloos gae <rhyme label="a" type="f" corresp="#s6:l1">maken</rhyme></l>
        <l id="s6:l3" met="5" type="-+">Dat met het Vleesch deur s'werelts schoon <rhyme label="b" type="f">aenschauwen</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Werd' lustich, en doe de liefde <rhyme label="b" type="f" corresp="#s6:l3">berauwen</rhyme>,</l>
        <l id="s6:l5" met="5" type="-+">Dat ic den Leeuw die my soo gaet <rhyme label="c" type="f">bespijeden</rhyme>,</l>
        <l id="s6:l6" met="5" type="-+">Soo wedersta, dat hy moet van my <rhyme label="c" type="f" corresp="#s6:l5">vlijeden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Ter rechter noch ter slincker doet my <rhyme label="a" type="f">wijcken</rhyme></l>
        <l id="s7:l2" met="5" type="-+">Maer t'aller tijt mijn stantvasticheyt <rhyme label="a" type="f" corresp="#s7:l1">blijcken</rhyme>,</l>
        <l id="s7:l3" met="5" type="-+">Dat ick my niet verheffe in <rhyme label="b" type="f">voorspoede</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">In tegenspoet oock niet en worde <rhyme label="b" type="f" corresp="#s7:l3">moede</rhyme>,</l>
        <l id="s7:l5" met="5" type="-+">Dan doet voorspoet in my dancbaerheyt <rhyme label="c" type="f">bauwen</rhyme>,</l>
        <l id="s7:l6" met="5" type="-+">En tegenspoet op u een vast <rhyme label="c" type="f" corresp="#s7:l5">betrauwen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Wt alle dinc op dat my voorcom <rhyme label="a" type="f">t'goede</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">En ick in my de deuchden heerlick <rhyme label="a" type="f" corresp="#s8:l1">voede</rhyme>,</l>
        <l id="s8:l3" met="5" type="-+">Dat die soo seer toenemen ende <rhyme label="b" type="f">wassen</rhyme>,</l>
        <l id="s8:l4" met="5" type="-+">Dat geen vyant my meer en can <rhyme label="b" type="f" corresp="#s8:l3">verrassen</rhyme>,</l>
        <l id="s8:l5" met="5" type="-+">En mijnen lust sy boven alle <rhyme label="c" type="f">saken</rhyme>,</l>
        <l id="s8:l6" met="5" type="-+">V heyligen naem groot en heerlick te <rhyme label="c" type="f" corresp="#s8:l5">maken</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">En ghelick in de groote <rhyme label="a" type="f">huysghesinnen</rhyme>,</l>
        <l id="s9:l2" met="5" type="-+">Veel vaten zijn, daer van de sommige <rhyme label="a" type="f" corresp="#s9:l1">binnen</rhyme>,</l>
        <l id="s9:l3" met="5" type="-+">Ghebruyckelick tot oneerlicke <rhyme label="b" type="f">dingen</rhyme></l>
        <l id="s9:l4" met="5" type="-+">En d'andren weer eerlicken dienst <rhyme label="b" type="f" corresp="#s9:l3">volbringen</rhyme>,</l>
        <l id="s9:l5" met="5" type="-+">Doet dat ick bin voor u een vat ter <rhyme label="c" type="f">Eeren</rhyme>,</l>
        <l id="s9:l6" met="5" type="-+">Datmen tot een goet gebruyck mach <rhyme label="c" type="f" corresp="#s9:l5">bekeeren</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="-+">Anders niet doe soo lang ick leef op <rhyme label="a" type="f">aerde</rhyme>,</l>
        <l id="s10:l2" met="5" type="-+">Dan dat ic in deuchts toeneeming' <rhyme label="a" type="f" corresp="#s10:l1">volheerde</rhyme>,</l>
        <l id="s10:l3" met="5" type="-+">Op dat ghy haer van my recht wart <rhyme label="b" type="f">gepresen</rhyme>,</l>
        <l id="s10:l4" met="5" type="-+">Mijn naeste oock van t'quaet tot goet <rhyme label="b" type="f" corresp="#s10:l3">gewesen</rhyme>,</l>
        <l id="s10:l5" met="5" type="-+">En ic hier nae met vreuchden mach <rhyme label="c" type="f">versamen</rhyme>,</l>
        <l id="s10:l6" met="5" type="-+">Met u mijn Heer, en allen heyligen <rhyme label="c" type="f" corresp="#s10:l5">Amen</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
