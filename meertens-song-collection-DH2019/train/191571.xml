<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191571</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zijn mannelijk wezen jent</melody>
  <author>Marijn de Brauwer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HOe lieflijck sijn de voeten // <rhyme label="a" type="m">van</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Die boden op de bergen <rhyme label="b" type="m">claer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die vrede tot versoeten // <rhyme label="a" type="m" corresp="#s1:l1">can</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Vercondigen, <rhyme label="b" type="m" corresp="#s1:l2">eenpaer</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Naer // den wille des <rhyme label="c" type="f">Heeren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En tgoet op elck <rhyme label="d" type="m">termijn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Prediken ende <rhyme label="c" type="f" corresp="#s1:l5">leeren</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Ons die wellecomm' <rhyme label="d" type="m" corresp="#s1:l6">sijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wellecomme vrienden eer // <rhyme label="a" type="m">baer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Lijt ghy ons al, vry <rhyme label="b" type="m">ongemist</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wy hebben lange tijt seer // <rhyme label="a" type="m" corresp="#s2:l1">naer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">U lieder comst <rhyme label="b" type="m" corresp="#s2:l2">geweest</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Men leest // d' <rhyme label="c" type="f">Ephesianen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Hoe sy met goet <rhyme label="d" type="m">accoort</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Paulus hoorden <rhyme label="c" type="f" corresp="#s2:l5">vermanen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Al uyt des Heeren <rhyme label="d" type="m" corresp="#s2:l6">woort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Als een sent bode uyt goede // <rhyme label="a" type="m">noot</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Beval d' ouders de kudde <rhyme label="b" type="m">fier</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Die Christum met sijn bloede // <rhyme label="a" type="m" corresp="#s3:l1">root</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hadde verworven <rhyme label="b" type="m" corresp="#s3:l2">dier</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Want hier // naer mijn af <rhyme label="c" type="f">scheyden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Sprack hy, sullen <rhyme label="d" type="m">opstaen</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Mannen die daer <rhyme label="c" type="f" corresp="#s3:l5">verleyden</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Die schapen vande <rhyme label="d" type="m" corresp="#s3:l6">baen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">En so vele wel heugen // <rhyme label="a" type="m">mach</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Ist oock in onsen tijt <rhyme label="b" type="m">geschiet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dat deene dander teugen // <rhyme label="a" type="m" corresp="#s4:l1">lach</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Tot droefheyt en <rhyme label="b" type="m" corresp="#s4:l2">verdriet</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Niet // meer en moet <rhyme label="c" type="f">gebeuren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Voor swerelts eynde <rhyme label="d" type="m">wijt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dat ghy u dienst met <rhyme label="c" type="f" corresp="#s4:l5">treuren</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Sout doen, tis geen <rhyme label="d" type="m" corresp="#s4:l6">profijt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wy hopen al te blyven // <rhyme label="a" type="m">vast</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gebonden met den vreden <rhyme label="b" type="m">bant</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Dat tlichaem tot verstyven // <rhyme label="a" type="m" corresp="#s5:l1">wast</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Altijt int recht <rhyme label="b" type="m" corresp="#s5:l2">verstant</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Want // commer en <rhyme label="c" type="f">bezwaren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Hier niemant naer <rhyme label="d" type="m">verlanckt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Wy sijn met ons <rhyme label="c" type="f" corresp="#s5:l5">dienaeren</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Vreedsaem God sy <rhyme label="d" type="m" corresp="#s5:l6">gedanckt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">God moeten wy dancken // <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Die ons hier helpt behouwen <rhyme label="b" type="m">tveldt</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Want wy sijn arm cranck en // <rhyme label="a" type="m" corresp="#s6:l1">teer</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ons doen doch niet en <rhyme label="b" type="m" corresp="#s6:l2">gelt</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Al stelt // den mensch wat <rhyme label="c" type="f">vooren</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Hier in dit aertsche <rhyme label="d" type="m">dal</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Van God comt dat (wilt <rhyme label="c" type="f" corresp="#s6:l5">hooren</rhyme>)</l>
        <l id="s6:l8" met="3" type="+-">Die tonge spreken <rhyme label="d" type="m" corresp="#s6:l6">sal</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Met eenen mont te prijsen // <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Och is dat niet een groote <rhyme label="b" type="m">vreucht</rhyme></l>
        <l id="s7:l3" met="4" type="+-">En ons altijt bewijsen // <rhyme label="a" type="m" corresp="#s7:l1">tot</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Gehoorsaemheyt en <rhyme label="b" type="m" corresp="#s7:l2">deucht</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Verheucht // maket van <rhyme label="c" type="f">binnen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Om singen vroech en <rhyme label="d" type="m">spa</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Met hert gemoet en <rhyme label="c" type="f" corresp="#s7:l5">sinnen</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Lof God, <rhyme label="d" type="m" corresp="#s7:l6">Aleluia</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Deelt uyt den schat der <rhyme label="a" type="m">waerheyt</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Die selve can u maken <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s8:l3" met="4" type="+-">En dinckt dat al uwen <rhyme label="a" type="m" corresp="#s8:l1">aerbeyt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Niet te vergeefs en <rhyme label="b" type="m" corresp="#s8:l2">zy</rhyme></l>
        <l id="s8:l5" met="4" type="+-">By // den oppersten <rhyme label="c" type="f">herder</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Als hy hem <rhyme label="d" type="m">openbaert</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Een kroone al veel <rhyme label="c" type="f" corresp="#s8:l5">werder</rhyme></l>
        <l id="s8:l8" met="3" type="+-">Dan Gout, wort u <rhyme label="d" type="m" corresp="#s8:l6">bewaert</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Latet u niet verdrieten // <rhyme label="a" type="m">vrient</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Want tis naer des Heeren <rhyme label="b" type="m">bevel</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Dat ghy ons dus om nieten // <rhyme label="a" type="m" corresp="#s9:l1">dient</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Naer ons begeeren <rhyme label="b" type="m" corresp="#s9:l2">wel</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Daniel // doet <rhyme label="c" type="f">gedincken</rhyme></l>
        <l id="s9:l6" met="3" type="-+">In twaelfde hy daer <rhyme label="d" type="m">seyt</rhyme></l>
        <l id="s9:l7" met="3" type="-+">De Leeraers sullen <rhyme label="c" type="f" corresp="#s9:l5">blincken</rhyme></l>
        <l id="s9:l8" met="3" type="+-">Als des Hemels <rhyme label="d" type="m" corresp="#s9:l6">claerheyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Prince die Heere geven // <rhyme label="a" type="m">wil</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Altijt sijn vrede hooch <rhyme label="b" type="m">geacht</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Op dat wy mogen leven // <rhyme label="a" type="m" corresp="#s10:l1">stil</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Vry van twist en <rhyme label="b" type="m" corresp="#s10:l2">tweedracht</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Goe wacht // tot ons <rhyme label="c" type="f">versoeten</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Houdt elck hoe lang hy <rhyme label="d" type="m">leeft</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Van herten wilt ons <rhyme label="c" type="f" corresp="#s10:l5">groeten</rhyme></l>
        <l id="s10:l8" met="3" type="+-">Die u gesonden <rhyme label="d" type="m" corresp="#s10:l6">heeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
