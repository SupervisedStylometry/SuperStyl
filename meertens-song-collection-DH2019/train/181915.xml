<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181915</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Juich Batavieren</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Juig <rhyme label="a" type="f">Batavieren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Singt nu vrolyk bly van <rhyme label="b" type="m">toon</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En helpt nu <rhyme label="a" type="f" corresp="#s1:l1">vercieren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Fredriks <rhyme label="b" type="m" corresp="#s1:l2">zegen-kroon</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dien zeer roemrugtigen <rhyme label="c" type="m">Held</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Is verwinnaar in het <rhyme label="c" type="m" corresp="#s1:l5">Veld</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezee</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Viva de Koning Fredrik <rhyme label="d" type="m" corresp="#s1:l7">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Komt Sang <rhyme label="a" type="f">godinne</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met Parnas op den <rhyme label="b" type="m">Troon</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Al wie <rhyme label="a" type="f" corresp="#s2:l1">beminne</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Mars een Kryg der <rhyme label="b" type="m" corresp="#s2:l2">Goon</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Muza die heeft hier <rhyme label="c" type="m">stof</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Te zingen ’t roemwaardig <rhyme label="c" type="m" corresp="#s2:l5">Lof</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezee</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Viva de Koning Fredrik <rhyme label="d" type="m" corresp="#s2:l7">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">’t Is Pruize den <rhyme label="a" type="f">Koning</rhyme>:</l>
        <l id="s3:l2" met="3" type="-+">Die met zyn Leeger <rhyme label="b" type="m">magt</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">In ’t veld zyn <rhyme label="a" type="f" corresp="#s3:l1">wooning</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Houd bij dag en <rhyme label="b" type="m" corresp="#s3:l2">nagt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En met veel <rhyme label="c" type="m">dapperheid</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Als een Herculus <rhyme label="c" type="m" corresp="#s3:l5">streyd</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezee</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Viva de Koning Fredrik <rhyme label="d" type="m" corresp="#s3:l7">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Schoon dat veel <rhyme label="a" type="f">Magten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Tegen die Vorst trekken te <rhyme label="b" type="m">Veld</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Nooit vyands <rhyme label="a" type="f" corresp="#s4:l1">kragten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Vreesden dien <rhyme label="b" type="m" corresp="#s4:l2">Held</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hy volgt zyn vyand over <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En dryft haar uit de stad en <rhyme label="c" type="m" corresp="#s4:l5">wal</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezee</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Viva de Koning Fredrik <rhyme label="d" type="m" corresp="#s4:l7">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Hy gaat zyn <rhyme label="a" type="f">Schaapen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Als een getrouwe Herder <rhyme label="b" type="m">voor</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">God zegent zyn <rhyme label="a" type="f" corresp="#s5:l1">Wapen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Op het Helden <rhyme label="b" type="m" corresp="#s5:l2">spoor</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Die Godes gunste heeft te <rhyme label="c" type="m">baat</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Nooit voor zyn vyand vlugten <rhyme label="c" type="m" corresp="#s5:l5">gaat</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezee</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Viva de Koning Fredrik <rhyme label="d" type="m" corresp="#s5:l7">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ziet de <rhyme label="a" type="f">Officiere</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van de Pruysen al zyn <rhyme label="b" type="m">getrouw</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">In de Krygs <rhyme label="a" type="f" corresp="#s6:l1">maniere</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Zyn Zoldaaaten <rhyme label="b" type="m" corresp="#s6:l2">gouw</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Pandoer Hoezaaren <rhyme label="c" type="m">principaal</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die vreezen voor geen vuur nog <rhyme label="c" type="m" corresp="#s6:l5">staal</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezee</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Viva de Koning Fredrik <rhyme label="d" type="m" corresp="#s6:l7">Hoezee</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wat Storm <rhyme label="a" type="f">vlaegen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Heeft Pruissen gedaan op Stad en <rhyme label="b" type="m">Wal</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Bloedige <rhyme label="a" type="f" corresp="#s7:l1">Veldslagen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Meenig in <rhyme label="b" type="m" corresp="#s7:l2">getal</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Zyn heldenmoed is nooit <rhyme label="c" type="m">geblust</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Voor dat zijn vyand is <rhyme label="c" type="m" corresp="#s7:l5">gerust</rhyme>;</l>
        <l id="s7:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezee</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Viva de Koning Fredrik <rhyme label="d" type="m" corresp="#s7:l7">hoezee</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Leef lang <rhyme label="a" type="f">roemwaarde</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Fredrik Pruises <rhyme label="b" type="m">Majesteit</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Gy hebt lof op <rhyme label="a" type="f" corresp="#s8:l1">Aerde</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Door u <rhyme label="b" type="m" corresp="#s8:l2">dapperheid</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">De Faam die blaast u glorie <rhyme label="c" type="m">om</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Door Turken en ’t <rhyme label="c" type="m" corresp="#s8:l5">Christendom</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Viva <rhyme label="d" type="m">Hoezee</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Viva de Koning Fredrik <rhyme label="d" type="m" corresp="#s8:l7">Hoezee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
