    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Nun lob meine Seele den Herrn</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn GOdt ick wil dy <rhyme label="a" type="f">Roemen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Want ghy zijt Coninck vol van <rhyme label="b" type="m">Eer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">End Loven sonder <rhyme label="a" type="f" corresp="#s1:l1">schroomen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dijn Naem ymmer end Eewich <rhyme label="b" type="m" corresp="#s1:l2">meer</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Daeg'licx wil ick <rhyme label="c" type="f">verconden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dijn Loff end Naeme <rhyme label="d" type="m">vroet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Jaer Roement t'Allen <rhyme label="c" type="f" corresp="#s1:l5">stonden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De HEEr is wonder <rhyme label="d" type="m" corresp="#s1:l6">groot</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Seer Loff'lijck boven <rhyme label="e" type="f">Mate</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Niemandt wtspreken <rhyme label="f" type="m">mach</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Syn Heerlickheyt end <rhyme label="e" type="f" corresp="#s1:l9">State</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Kindts kindren alden <rhyme label="f" type="m" corresp="#s1:l10">dach</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Heer Weldadich <rhyme label="a" type="f">allen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Ontfermt hem over syne <rhyme label="b" type="m">Werck</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dies sullens met <rhyme label="a" type="f" corresp="#s2:l1">gheschalle</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dy (Heer) oock dancken alle <rhyme label="b" type="m" corresp="#s2:l2">sterck</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dijn Heyl'gen sullen <rhyme label="c" type="f">Prijsen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dijns Rijcks groote <rhyme label="d" type="m">Eer</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">End gaen met Roem <rhyme label="c" type="f" corresp="#s2:l5">bewijsen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dijn Almacht altijdt <rhyme label="d" type="m" corresp="#s2:l6">meer</rhyme></l>
        <l id="s2:l9" met="3" type="-+">End over al <rhyme label="e" type="f">verbreyden</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Dijns Rijckes <rhyme label="f" type="m">Heerlickheyt</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Die Nacoemling' <rhyme label="e" type="f" corresp="#s2:l9">bescheyden</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Van dijne <rhyme label="f" type="m" corresp="#s2:l10">Vromicheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">HEER, Eewich is dijn <rhyme label="a" type="f">Rijcke</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dijn Heerschappijen sonder <rhyme label="b" type="m">Eynd</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Des HEEren Hulp niet <rhyme label="a" type="f" corresp="#s3:l1">wijcke</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Van dien, die vallen in <rhyme label="b" type="m" corresp="#s3:l2">Elend</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Hy helpt weer op met <rhyme label="c" type="f">Machten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die Neer gheslaghen <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">HEEr, alle Ooghen <rhyme label="c" type="f" corresp="#s3:l5">wachten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van dy haer Spijse <rhyme label="d" type="m" corresp="#s3:l6">fijn</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Als dijne Handt dan <rhyme label="e" type="f">ghevet</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Yeg'lijck tot syner <rhyme label="f" type="m">Tijdt</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Versaedt is al wat <rhyme label="e" type="f" corresp="#s3:l9">Levet</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">End voelt hem wel <rhyme label="f" type="m" corresp="#s3:l10">verblijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Rechtveerdich is de <rhyme label="a" type="f">HEERE</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">In alle syne Weghen <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Jae Heylich end vol <rhyme label="a" type="f" corresp="#s4:l1">Eere</rhyme></l>
        <l id="s4:l4" met="4" type="-+">In all' syn Wercken oock <rhyme label="b" type="m" corresp="#s4:l2">Certeyn</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">De HEER is nae by <rhyme label="c" type="f">allen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die hem Aenroepen <rhyme label="d" type="m">recht</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Troostloos laet hy niet <rhyme label="c" type="f" corresp="#s4:l5">vallen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die hem betrouwen <rhyme label="d" type="m" corresp="#s4:l6">slecht</rhyme>.</l>
        <l id="s4:l9" met="3" type="-+">All' die den HEERE <rhyme label="e" type="f">vreesen</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Hy doet nae haer <rhyme label="f" type="m">begeer</rhyme></l>
        <l id="s4:l11" met="3" type="+-">End gaet hier wel <rhyme label="e" type="f" corresp="#s4:l9">ghenesen</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Haer Stem verhoort de <rhyme label="f" type="m" corresp="#s4:l10">HEER</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De HEEre gaet <rhyme label="a" type="f">Behoeden</rhyme></l>
        <l id="s5:l2" met="4" type="-+">All' die hem Lieven (tis haer <rhyme label="b" type="m">GOdt</rhyme>)</l>
        <l id="s5:l3" met="3" type="-+">Maer Eewich sal hy <rhyme label="a" type="f" corresp="#s5:l1">Dooden</rhyme></l>
        <l id="s5:l4" met="4" type="-+">All' die daer syn t'Godloose <rhyme label="b" type="m" corresp="#s5:l2">Rot</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">(Aenmerckt ghy menschen <rhyme label="c" type="f">even</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Het mach Faelgeeren <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ten HEER wilt u <rhyme label="c" type="f" corresp="#s5:l5">begheven</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Vermijden groot <rhyme label="d" type="m" corresp="#s5:l6">Verdriet</rhyme>)</l>
        <l id="s5:l9" met="3" type="-+">Mijn Mondt sal t'Loff des <rhyme label="e" type="f">HEEren</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Verconden <rhyme label="f" type="m">Openbaer</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">All' Vleesch syn Naem gae <rhyme label="e" type="f" corresp="#s5:l9">Eeren</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">End Heyl'gen Eewich <rhyme label="f" type="m" corresp="#s5:l10">daer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
