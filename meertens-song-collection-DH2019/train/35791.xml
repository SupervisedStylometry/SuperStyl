<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35791</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>O schepper fier</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Vader! <rhyme label="a" type="m">weest</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Van ons gegroet al <rhyme label="b" type="f">heden</rhyme>:</l>
        <l id="s1:l3" met="3" type="+-">Ghy Godes Zoon <rhyme label="b" type="f" corresp="#s1:l2">mede</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Weest, weest, daer by <rhyme label="c" type="m">gegroet</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Heyligen <rhyme label="a" type="m" corresp="#s1:l1">geest</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Gunt aen 't Landt, Zee en <rhyme label="b" type="f" corresp="#s1:l2">Steden</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Welvaert, liefde, <rhyme label="b" type="f" corresp="#s1:l2">vreden</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Noch bidden met <rhyme label="c" type="m" corresp="#s1:l4">ootmoet</rhyme></l>
        <l id="s1:l9" met="4" type="+-">Een ware Godt, en drie <rhyme label="d" type="f">Persoonen</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Ons quaet, en zonden wilt <rhyme label="d" type="f" corresp="#s1:l9">verschoonen</rhyme>.</l>
        <l id="s1:l11" met="3" type="+-">Sterckt ons' ziel en <rhyme label="e" type="m">hart</rhyme></l>
        <l id="s1:l12" met="3" type="-+">In plagen, droefheyt <rhyme label="e" type="m" corresp="#s1:l11">smart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Maria <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Der Vrouwen lof, en <rhyme label="b" type="f">kroone</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Poort van 's Hemels <rhyme label="b" type="f" corresp="#s2:l2">throone</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Van ons 't Helsche <rhyme label="c" type="m">Serpent</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Verbant en <rhyme label="a" type="m" corresp="#s2:l1">jaeght</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Verkrijght van uwe <rhyme label="b" type="f" corresp="#s2:l2">Zoone</rhyme>:</l>
        <l id="s2:l7" met="3" type="+-">Dat wy by u <rhyme label="b" type="f" corresp="#s2:l2">woone</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">En tot ons levens <rhyme label="c" type="m" corresp="#s2:l4">ent</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Ey! wilt altijt by ons doch <rhyme label="d" type="f">blijven</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Beware, troosten en <rhyme label="d" type="f" corresp="#s2:l9">verstijven</rhyme>.</l>
        <l id="s2:l11" met="3" type="+-">Van ons noyt en <rhyme label="e" type="m">gaet</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Noch Moeder, ons <rhyme label="e" type="m" corresp="#s2:l11">verlaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Krijght uyt Goods <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Brandende <rhyme label="b" type="f">Zeraphinnen</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Wijse <rhyme label="b" type="f" corresp="#s3:l2">Cherubinnen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Met al d' Engele <rhyme label="c" type="m">schaer</rhyme></l>
        <l id="s3:l5" met="2" type="-+">De liefde <rhyme label="a" type="m" corresp="#s3:l1">brandt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat wy boven al <rhyme label="b" type="f" corresp="#s3:l2">minnen</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Godt, met hart en <rhyme label="b" type="f" corresp="#s3:l2">zinnen</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Leert ons Goods wijsheyt <rhyme label="c" type="m" corresp="#s3:l4">klaer</rhyme>.</l>
        <l id="s3:l9" met="4" type="+-">Bewaert ons wel, Engel <rhyme label="d" type="f">bewaerder</rhyme>!</l>
        <l id="s3:l10" met="4" type="-+">In d'ure des doodts komt ons <rhyme label="d" type="f" corresp="#s3:l9">naerder</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Als onse ziel' <rhyme label="e" type="m">scheydt</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Tot 's Hemels vreught die <rhyme label="e" type="m" corresp="#s3:l11">leydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Apost'len <rhyme label="a" type="m">hoogh</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van Godt en Mensch <rhyme label="b" type="f">gepresen</rhyme>;</l>
        <l id="s4:l3" met="3" type="+-">Twalif uyt <rhyme label="b" type="f" corresp="#s4:l2">gelesen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Uyt 't heel Adams <rhyme label="c" type="m">zaet</rhyme>;</l>
        <l id="s4:l5" met="2" type="-+">Met 't hart en <rhyme label="a" type="m" corresp="#s4:l1">oogh</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wilt altijt by ons <rhyme label="b" type="f" corresp="#s4:l2">wesen</rhyme>;</l>
        <l id="s4:l7" met="3" type="+-">Maeckt dat wy Godt <rhyme label="b" type="f" corresp="#s4:l2">vresen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En leven na Goods <rhyme label="c" type="m" corresp="#s4:l4">raedt</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Euangelisten en <rhyme label="d" type="f">Propheten</rhyme></l>
        <l id="s4:l10" met="4" type="-+">Wilt ons nu, nu noch oyt <rhyme label="d" type="f" corresp="#s4:l9">vergeten</rhyme>.</l>
        <l id="s4:l11" met="3" type="+-">Van ons 't sondigh <rhyme label="e" type="m">sogh</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Iaeght, en Ketters <rhyme label="e" type="m" corresp="#s4:l11">bedrogh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ghy Pristers <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Heremijten, <rhyme label="b" type="f">Doctoren</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Vroome, <rhyme label="b" type="f" corresp="#s5:l2">Consessoren</rhyme>;</l>
        <l id="s5:l4" met="4" type="-+">En Maeghden hoog van <rhyme label="c" type="m">stam</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Veel in 't <rhyme label="a" type="m" corresp="#s5:l1">getal</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wilt ons gebedt <rhyme label="b" type="f" corresp="#s5:l2">verhooren</rhyme>;</l>
        <l id="s5:l7" met="3" type="+-">Maeckt dat wy <rhyme label="b" type="f" corresp="#s5:l2">verlooren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Noyt gaen, door <rhyme label="c" type="m" corresp="#s5:l4">Venus-vlam</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Noch Satans raedt, noch 's Werelts <rhyme label="d" type="f">lusten</rhyme>;</l>
        <l id="s5:l10" met="4" type="-+">Dat 't hart in Godt alleen mach <rhyme label="d" type="f" corresp="#s5:l9">rusten</rhyme>.</l>
        <l id="s5:l11" met="3" type="+-">Martelaren <rhyme label="e" type="m">vroom</rhyme>!</l>
        <l id="s5:l12" met="3" type="-+">Bevrijt ons hart van <rhyme label="e" type="m" corresp="#s5:l11">schroom</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ghy al die <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Patroonen van ons <rhyme label="b" type="f">Kercken</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Wilt leken en <rhyme label="b" type="f" corresp="#s6:l2">klercken</rhyme></l>
        <l id="s6:l4" met="4" type="-+">In droefheyt, druck en <rhyme label="c" type="m">scha</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">En inde <rhyme label="a" type="m" corresp="#s6:l1">strijt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Vertroosten en <rhyme label="b" type="f" corresp="#s6:l2">verstercken</rhyme>.</l>
        <l id="s6:l7" met="3" type="+-">Maeckt dat wy uw' <rhyme label="b" type="f" corresp="#s6:l2">wercken</rhyme></l>
        <l id="s6:l8" met="3" type="-+">En daden volgen <rhyme label="c" type="m" corresp="#s6:l4">na</rhyme>.</l>
        <l id="s6:l9" met="4" type="+-">Bevrijdt ons van al d'Helsche <rhyme label="d" type="f">klippen</rhyme>,</l>
        <l id="s6:l10" met="4" type="-+">Om met ziele, lijf, tongh en <rhyme label="d" type="f" corresp="#s6:l9">lippen</rhyme></l>
        <l id="s6:l11" met="3" type="+-">In 's Hemelsche <rhyme label="e" type="m">Hof</rhyme>.</l>
        <l id="s6:l12" met="3" type="-+">Te singen Godes <rhyme label="e" type="m" corresp="#s6:l11">lof</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Getrouwde <rhyme label="a" type="m">Mans</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En Vrouwe hoogh <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Maeckt dat wort <rhyme label="b" type="f" corresp="#s7:l2">verdreven</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Van 't Huwelijcke <rhyme label="c" type="m">juck</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Twist, tweedracht <rhyme label="a" type="m" corresp="#s7:l1">gans</rhyme>.</l>
        <l id="s7:l6" met="3" type="-+">Dat wy al heyligh <rhyme label="b" type="f" corresp="#s7:l2">leven</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">En Godt glory <rhyme label="b" type="f" corresp="#s7:l2">geven</rhyme></l>
        <l id="s7:l8" met="3" type="-+">In voorspoet ende <rhyme label="c" type="m" corresp="#s7:l4">druck</rhyme>.</l>
        <l id="s7:l9" met="4" type="+-">Weduwen en de <rhyme label="d" type="f">Wedenaren</rhyme>,</l>
        <l id="s7:l10" met="4" type="-+">Bit voor al de gescheyde <rhyme label="d" type="f" corresp="#s7:l9">paren</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Dat Godt sijn oogh <rhyme label="e" type="m">slaet</rhyme></l>
        <l id="s7:l12" met="3" type="-+">Op dese vroegh en <rhyme label="e" type="m" corresp="#s7:l11">laet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Ghy al die <rhyme label="a" type="m">zit</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Om Godt te zien, en <rhyme label="b" type="f">loven</rhyme></l>
        <l id="s8:l3" met="3" type="+-">In d' Hemelsche <rhyme label="b" type="f" corresp="#s8:l2">hoven</rhyme>;</l>
        <l id="s8:l4" met="4" type="-+">Verkrijght door uw' <rhyme label="c" type="m">gebedt</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Ons alle <rhyme label="a" type="m" corresp="#s8:l1">dit</rhyme>:</l>
        <l id="s8:l6" met="3" type="-+">Dat niet ons' ziel kan <rhyme label="b" type="f" corresp="#s8:l2">roven</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Nocht in 't goet <rhyme label="b" type="f" corresp="#s8:l2">verdoven</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">En leven na Goods <rhyme label="c" type="m" corresp="#s8:l4">wet</rhyme>.</l>
        <l id="s8:l9" met="4" type="+-">Voor 't laetst, ghy Santen en <rhyme label="d" type="f">Santinnen</rhyme></l>
        <l id="s8:l10" met="4" type="-+">Leydt ons in 's Hemels vreughde <rhyme label="d" type="f" corresp="#s8:l9">binnen</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">Toont ons Goods <rhyme label="e" type="m">aenschijn</rhyme></l>
        <l id="s8:l12" met="3" type="-+">Maeckt dat wy by u <rhyme label="e" type="m" corresp="#s8:l11">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
