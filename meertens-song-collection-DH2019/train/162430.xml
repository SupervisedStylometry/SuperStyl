<song>
  <meta>
    <id>162430</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Luz de mi alma</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">EEn statigh <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Wort seer <rhyme label="a" type="f" corresp="#s1:l1">gepresen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Geen hooger <rhyme label="b" type="f">deughde</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Noch geen meerder <rhyme label="b" type="f" corresp="#s1:l3">vreughde</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Voor een jonge <rhyme label="c" type="m">Maeght</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">O Nimphjes, <rhyme label="d" type="f">Nimphjes</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Die haer deuchtsaem <rhyme label="c" type="m" corresp="#s1:l5">draeght</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Houdt in <rhyme label="a" type="f">memory</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Dees roem vol <rhyme label="a" type="f" corresp="#s2:l1">glory</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">U soete <rhyme label="b" type="f">oogen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Zijn van hoogh <rhyme label="b" type="f" corresp="#s2:l3">vermogen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Kiest de ware <rhyme label="c" type="m">deught</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">O Nimphjes, <rhyme label="d" type="f">Nimphjes</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Soo leeft ghy in <rhyme label="c" type="m" corresp="#s2:l5">vreught</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Het soete <rhyme label="a" type="f">smeken</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Het vriend'lijck <rhyme label="a" type="f" corresp="#s3:l1">spreken</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Van <rhyme label="b" type="f">Iong-gesellen</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Die op hoop u <rhyme label="b" type="f" corresp="#s3:l3">quellen</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Niet geheel <rhyme label="c" type="m">versmaet</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">O Nimphjes, <rhyme label="d" type="f">Nimphjes</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Niet trots <rhyme label="c" type="m" corresp="#s3:l5">tegen-staet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Maer weest <rhyme label="a" type="f">voorsichtigh</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">De saeck is <rhyme label="a" type="f" corresp="#s4:l1">wichtigh</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Het soete <rhyme label="b" type="f">minne</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Eyst wel rijpe <rhyme label="b" type="f" corresp="#s4:l3">sinnen</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Gaet in 't rechte <rhyme label="c" type="m">spoor</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">O Nimphjes, <rhyme label="d" type="f">Nimphjes</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Geeft stracks geen <rhyme label="c" type="m" corresp="#s4:l5">gehoor</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Maer wilt met <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">U tijt <rhyme label="a" type="f" corresp="#s5:l1">besteden</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Niet trots <rhyme label="b" type="f">hoovaerdigh</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Acht niemant <rhyme label="b" type="f" corresp="#s5:l3">onwaerdigh</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Want den eene <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">O Nimphjes, <rhyme label="d" type="f">Nimphjes</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Brenght den ander <rhyme label="c" type="m" corresp="#s5:l5">in</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Geen straffe <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Betoont in 't <rhyme label="a" type="f" corresp="#s6:l1">minnen</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Wilt niemant <rhyme label="b" type="f">vergen</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Noch geen Vryers <rhyme label="b" type="f" corresp="#s6:l3">tergen</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Die met d' eene <rhyme label="c" type="m">paert</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">O Nimphjes, <rhyme label="d" type="f">Nimphjes</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Is den ander <rhyme label="c" type="m" corresp="#s6:l5">waert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
