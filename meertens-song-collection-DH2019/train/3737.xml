  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALLE ghy knechten des <rhyme label="a" type="f">Heeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wilt hem nu t'saemen <rhyme label="a" type="f" corresp="#s1:l1">vereeren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghy die in sijn huys staet end <rhyme label="b" type="m">waeckt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dient hem end sijnen naem groot <rhyme label="b" type="m" corresp="#s1:l3">maeckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Heft die handen in <rhyme label="a" type="f">heylicheit</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">In syn huys op met <rhyme label="a" type="f" corresp="#s2:l1">vlieticheit</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Om hem te louen t'aller <rhyme label="b" type="m">tydt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Pryst hem met hert' end mondt <rhyme label="b" type="m" corresp="#s2:l3">verblydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">V sal de Heer', die eewigh <rhyme label="a" type="f">leeft</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Die hemel end d'Ä™erd' ghemaeckt <rhyme label="a" type="f" corresp="#s3:l1">heeft</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wt Zion met groot <rhyme label="b" type="m">ouervloet</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Seghenen met allerley <rhyme label="b" type="m" corresp="#s3:l3">goet</rhyme>.</l>
      </lg>
    </lg>
  </text>
