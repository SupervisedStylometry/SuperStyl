<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191578</id>
    <date>
      <year-start>1614</year-start>
      <year-end>1614</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Marijn de Brauwer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VRienden wilt <rhyme label="a" type="f">onderscheyden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tlicht uyt de <rhyme label="b" type="m">duysternis</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En wilt u soo <rhyme label="a" type="f" corresp="#s1:l1">bereyden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Na Gods woorden <rhyme label="b" type="m" corresp="#s1:l2">gewis</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Want menich valsch Prophete <rhyme label="b" type="m" corresp="#s1:l2">fris</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Op gestaen <rhyme label="b" type="m" corresp="#s1:l2">is</rhyme></l>
        <l id="s1:l7" met="3" type="-+">En laet u niet <rhyme label="a" type="f" corresp="#s1:l1">verleyden</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Vanden <rhyme label="b" type="m" corresp="#s1:l2">Antechris</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer neemt doch een <rhyme label="a" type="f">exempel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Wat Israel seyt <rhyme label="b" type="m">voorwaer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Hier is des Heeren <rhyme label="a" type="f" corresp="#s2:l1">Tempel</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Des Heeren Tempel is <rhyme label="b" type="m" corresp="#s2:l2">daer</rhyme></l>
        <l id="s2:l5" met="4" type="-+">En volgen soo haer afgoden <rhyme label="b" type="m" corresp="#s2:l2">naer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Menschen jocken <rhyme label="b" type="m" corresp="#s2:l2">zwaer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Niet sonder vleck oft <rhyme label="a" type="f" corresp="#s2:l1">rempel</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Maer seer <rhyme label="b" type="m" corresp="#s2:l2">onklaer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zy vergaten God haer <rhyme label="a" type="f">Vader</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Haer hooft en <rhyme label="b" type="m">Capiteyn</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die haer gaf alle <rhyme label="a" type="f" corresp="#s3:l1">gader</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Schoon water uyt harde <rhyme label="b" type="m" corresp="#s3:l2">Steyn</rhyme></l>
        <l id="s3:l5" met="4" type="-+">En groeven haer stijnckende Putten <rhyme label="b" type="m" corresp="#s3:l2">ghemeyn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Sonder water <rhyme label="b" type="m" corresp="#s3:l2">reyn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">En verlieten de <rhyme label="a" type="f" corresp="#s3:l1">Fonteyn-ader</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Twelck is God <rhyme label="b" type="m" corresp="#s3:l2">alleyn</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Zy hebben haer <rhyme label="a" type="f">begeven</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Inde zeden der Heydenen <rhyme label="b" type="m">bot</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Den Prophete heeft <rhyme label="a" type="f" corresp="#s4:l1">gheschreven</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Elcke Stad hadde zijn <rhyme label="b" type="m" corresp="#s4:l2">God</rhyme></l>
        <l id="s4:l5" met="4" type="-+">En overtraden soo Gods <rhyme label="b" type="m" corresp="#s4:l2">ghebodt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Maer wierden <rhyme label="b" type="m" corresp="#s4:l2">bespot</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Als sot // uyt Lant <rhyme label="a" type="f" corresp="#s4:l1">gedreven</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Kiest een beter <rhyme label="b" type="m" corresp="#s4:l2">Lot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maer Adams boos <rhyme label="a" type="f">natuere</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Is altijdt tot quaet <rhyme label="b" type="m">ghesint</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Anders dan de <rhyme label="a" type="f" corresp="#s5:l1">Schriftuere</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Soo menichmael <rhyme label="b" type="m" corresp="#s5:l2">ontbint</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Mer wie dat quade vleesch <rhyme label="b" type="m" corresp="#s5:l2">verwint</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Worden als een <rhyme label="b" type="m" corresp="#s5:l2">kint</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Gaen in ter rechter <rhyme label="a" type="f" corresp="#s5:l1">deure</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Het eeuwich leven <rhyme label="b" type="m" corresp="#s5:l2">vint</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer desen wegh te <rhyme label="a" type="f">gane</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Is menich herte te <rhyme label="b" type="m">hoogh</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Blyven int duyster <rhyme label="a" type="f" corresp="#s6:l1">stane</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Tot licht en is geen <rhyme label="b" type="m" corresp="#s6:l2">poogh</rhyme></l>
        <l id="s6:l5" met="4" type="-+">want dat Serpent dat Eva <rhyme label="b" type="m" corresp="#s6:l2">bedrooch</rhyme></l>
        <l id="s6:l6" met="3" type="-+">En leugens <rhyme label="b" type="m" corresp="#s6:l2">loogh</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Dat is noch op de <rhyme label="a" type="f" corresp="#s6:l1">Bane</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Met veel schoon <rhyme label="b" type="m" corresp="#s6:l2">vertooch</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maer het sal haer wel <rhyme label="a" type="f">ghelucken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Die doen soo Pauwelus <rhyme label="b" type="m">seyt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Haer vleesch hier tonder <rhyme label="a" type="f" corresp="#s7:l1">drucken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">In Godts <rhyme label="b" type="m" corresp="#s7:l2">ghehoorsaemheyt</rhyme></l>
        <l id="s7:l5" met="4" type="-+">En verciert staen met haer Bruylofs <rhyme label="b" type="m" corresp="#s7:l2">kleyt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Als den Heere <rhyme label="b" type="m" corresp="#s7:l2">scheyt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">De Schapen uyt de <rhyme label="a" type="f" corresp="#s7:l1">bucken</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Voor zijn <rhyme label="b" type="m" corresp="#s7:l2">Majesteyt</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dat ghy sonder <rhyme label="a" type="f">verstooren</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Sult hooren zijn stemme <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Komt hier mijn <rhyme label="a" type="f" corresp="#s8:l1">uytvercoren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Besit mijn Rijcke <rhyme label="b" type="m" corresp="#s8:l2">goet</rhyme></l>
        <l id="s8:l5" met="4" type="-+">maer dan sal hy dat quade <rhyme label="b" type="m" corresp="#s8:l2">ghebroet</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In den Helschen <rhyme label="b" type="m" corresp="#s8:l2">gloet</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Smijten met grammigen <rhyme label="a" type="f" corresp="#s8:l1">tooren</rhyme></l>
        <l id="s8:l8" met="3" type="+-">Onder zijnen <rhyme label="b" type="m" corresp="#s8:l2">voet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Vrienden laet ons <rhyme label="a" type="f">mercken</rhyme></l>
        <l id="s9:l2" met="3" type="-+">En gaen door desen wegh <rhyme label="b" type="m">smal</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Dat wy tot ons <rhyme label="a" type="f" corresp="#s9:l1">verstercken</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Doch wesen vant vrome <rhyme label="b" type="m" corresp="#s9:l2">getal</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Want Christus sal comen met groot <rhyme label="b" type="m" corresp="#s9:l2">geschal</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Hier int aertsche <rhyme label="b" type="m" corresp="#s9:l2">dal</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Elck loon na zyne <rhyme label="a" type="f" corresp="#s9:l1">werken</rhyme></l>
        <l id="s9:l8" met="3" type="+-">Dan ontfangen <rhyme label="b" type="m" corresp="#s9:l2">sal</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Die dan wil gaen int <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Om eeuwich zalich te <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Moeten haer hier <rhyme label="a" type="f" corresp="#s10:l1">begheven</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Inde wegen des Heeren <rhyme label="b" type="m" corresp="#s10:l2">devijn</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Die werden als dan bevrijt van <rhyme label="b" type="m" corresp="#s10:l2">pijn</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Die den <rhyme label="b" type="m" corresp="#s10:l2">Medecijn</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Fijn // soecken hier <rhyme label="a" type="f" corresp="#s10:l1">beneven</rhyme></l>
        <l id="s10:l8" met="3" type="+-">sonder geveynsden <rhyme label="b" type="m" corresp="#s10:l2">schijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Op dat onse <rhyme label="a" type="f">voyagie</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Met coragie // worde <rhyme label="b" type="m">volent</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Hier doende ons <rhyme label="a" type="f" corresp="#s11:l1">pilgeremagie</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Om Gagie // in sHemels <rhyme label="b" type="m" corresp="#s11:l2">Tent</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Hier reysende door des Werelt <rhyme label="b" type="m" corresp="#s11:l2">blent</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Doch <rhyme label="b" type="m" corresp="#s11:l2">ongeschent</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Want in dees wilde <rhyme label="a" type="f" corresp="#s11:l1">Bosschagie</rhyme></l>
        <l id="s11:l8" met="3" type="+-">Is menich boos <rhyme label="b" type="m" corresp="#s11:l2">Serpent</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Serpenten ende <rhyme label="a" type="f">Slanghen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Vonden oock <rhyme label="b" type="m">Israel</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Die haer niet om <rhyme label="a" type="f" corresp="#s12:l1">verstrangen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Brochten in doots <rhyme label="b" type="m" corresp="#s12:l2">ghequel</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Om datse den Heer deur boos <rhyme label="b" type="m" corresp="#s12:l2">opstel</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Waren <rhyme label="b" type="m" corresp="#s12:l2">rebel</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Maer die den Heer aen <rhyme label="a" type="f" corresp="#s12:l1">hangen</rhyme></l>
        <l id="s12:l8" met="3" type="+-">Die bewaert hy <rhyme label="b" type="m" corresp="#s12:l2">wel</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Och vriendekens, <rhyme label="a" type="f">ghetrouwe</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Vreest Godt tot allen <rhyme label="b" type="m">tien</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Ghy sult verdriet noch <rhyme label="a" type="f" corresp="#s13:l1">rouwe</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Noch geen benouwen <rhyme label="b" type="m" corresp="#s13:l2">lien</rhyme></l>
        <l id="s13:l5" met="4" type="-+">Maer die weer na Egypten <rhyme label="b" type="m" corresp="#s13:l2">vlien</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Haer sal druck <rhyme label="b" type="m" corresp="#s13:l2">gheschien</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Maer gedinckt Loths <rhyme label="a" type="f" corresp="#s13:l1">Huysvrouwe</rhyme></l>
        <l id="s13:l8" met="3" type="+-">En wilt niet omme <rhyme label="b" type="m" corresp="#s13:l2">sien</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Maer als ghetrouwe <rhyme label="a" type="f">knechten</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Laet ons houden goe <rhyme label="b" type="m">wacht</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Dat Gods gebodt ten <rhyme label="a" type="f" corresp="#s14:l1">rechten</rhyme></l>
        <l id="s14:l4" met="3" type="-+">By ons is hooghe <rhyme label="b" type="m" corresp="#s14:l2">geacht</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Maer die verblint sijn vande <rhyme label="b" type="m" corresp="#s14:l2">nacht</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Vallen inde <rhyme label="b" type="m" corresp="#s14:l2">Gracht</rhyme></l>
        <l id="s14:l7" met="3" type="-+">God die sal teghen hem <rhyme label="a" type="f" corresp="#s14:l1">vechten</rhyme>,</l>
        <l id="s14:l8" met="3" type="+-">Met zijn Sweert <rhyme label="b" type="m" corresp="#s14:l2">onsacht</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Prince laet ons uyt <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Verwinnen de weerelt <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Met die vernieut van <rhyme label="a" type="f" corresp="#s15:l1">binnen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Bekinnen de waerheyt <rhyme label="b" type="m" corresp="#s15:l2">bloot</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Dat wy doch vry wesen ter <rhyme label="b" type="m" corresp="#s15:l2">noot</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Van de tweede <rhyme label="b" type="m" corresp="#s15:l2">doot</rhyme></l>
        <l id="s15:l7" met="3" type="-+">Godt die bewaer u <rhyme label="a" type="f" corresp="#s15:l1">sinnen</rhyme></l>
        <l id="s15:l8" met="3" type="+-">In Christo <rhyme label="b" type="m" corresp="#s15:l2">minjoot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
