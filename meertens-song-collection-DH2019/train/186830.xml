<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186830</id>
    <date>
      <year-start>1674</year-start>
      <year-end>1674</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Onze vader in hemelrijk</melody>
  <author>Joannes Six van Chandelier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O onse vaader, die <rhyme label="a" type="m">altyd</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Op sheemels troon geseeten <rhyme label="a" type="m" corresp="#s1:l1">zyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En van der aard, by dag, en <rhyme label="b" type="m">nacht</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">De noodbee van uw kindren <rhyme label="b" type="m" corresp="#s1:l3">wacht</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wy roepen u dan waarlik <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Zo laat het u ter harten <rhyme label="c" type="m" corresp="#s1:l5">gaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Geheiligt zy uw groote <rhyme label="a" type="m">naam</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Doch maak gy ons daar toe <rhyme label="a" type="m" corresp="#s2:l1">bequaam</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Uw goedheit, wysheit, trouw, en <rhyme label="b" type="m">magt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Zy heiliglyk van ons <rhyme label="b" type="m" corresp="#s2:l3">betracht</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Op dat daar door uw prys, en <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">By ons, en ieder een, <rhyme label="c" type="m" corresp="#s2:l5">vermeer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Uw kooningryke koom ons <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Neem binnen ons uw <rhyme label="a" type="m" corresp="#s3:l1">heerschappy</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Bestier ons door uw Geest, en <rhyme label="b" type="m">woord</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maak al des duivels werk <rhyme label="b" type="m" corresp="#s3:l3">verstoort</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Volbouw uw kerk, en slaase <rhyme label="c" type="m">gaa</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Tot dat uw ryk volmaakt <rhyme label="c" type="m" corresp="#s3:l5">bestaa</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Uw wil geschiede naa haar <rhyme label="a" type="m">eisch</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Zo buig ons hart, en alle <rhyme label="a" type="m" corresp="#s4:l1">vleisch</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Op dat, op aard, in vreugd, noch <rhyme label="b" type="m">leed</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Sich niemand teegen u <rhyme label="b" type="m" corresp="#s4:l3">vergeet</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Doe elk uw dienst zo gaade <rhyme label="c" type="m">slaan</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Als in den heemel werdt <rhyme label="c" type="m" corresp="#s4:l5">gedaan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Geef ons op huiden 't daagliks <rhyme label="a" type="m">brood</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Versorg ons lyf voor alle <rhyme label="a" type="m" corresp="#s5:l1">nood</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Op datwe, tot ons <rhyme label="b" type="m">onderstand</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Alleen op uwe milde <rhyme label="b" type="m" corresp="#s5:l3">hand</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Ons oog geduurig doende <rhyme label="c" type="m">gaan</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">In eerbaarheit, vernoegt <rhyme label="c" type="m" corresp="#s5:l5">bestaan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Vergeef ons onse schuld, als <rhyme label="a" type="m">wy</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Uit goeder deugd, met <rhyme label="a" type="m" corresp="#s6:l1">meedely</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Door eenen broederlyken <rhyme label="b" type="m">soen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Aan onse schuldenaaren <rhyme label="b" type="m" corresp="#s6:l3">doen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Dien wy het quaad, aan ons <rhyme label="c" type="m">begaan</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Vergeeven, en van schuld <rhyme label="c" type="m" corresp="#s6:l5">ontslaan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Verlos ons lyf, en ziel <rhyme label="a" type="m">altyd</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Van alle quaad, dat haar <rhyme label="a" type="m" corresp="#s7:l1">bestrydt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Gelyk daar zyn de holsche <rhyme label="b" type="m">leeuw</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Ons vleesch, en dees verdorven <rhyme label="b" type="m" corresp="#s7:l3">eeuw</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Doch help ons meest in onsen <rhyme label="c" type="m">dood</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Neem onse ziel dan in uw <rhyme label="c" type="m" corresp="#s7:l5">schoot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Want u, die hoog, en laag <rhyme label="a" type="m">regeert</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">En alles schikt, zo gy <rhyme label="a" type="m" corresp="#s8:l1">begeert</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Behoort alleen het <rhyme label="b" type="m">kooningryk</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">In kracht is niemand u <rhyme label="b" type="m" corresp="#s8:l3">gelyk</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">U is de hoogste <rhyme label="c" type="m">majesteit</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">En heerlikheit, in <rhyme label="c" type="m" corresp="#s8:l5">eeuwigheit</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">'t Geschiede zoo, en naa uw <rhyme label="a" type="m">woord</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Zy daagliks ons gebed <rhyme label="a" type="m" corresp="#s9:l1">verhoort</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">Doch niemand twyffelt iet daar <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Het werdt in Kristus naam <rhyme label="b" type="m" corresp="#s9:l3">gedaan</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Want die is onse <rhyme label="c" type="m">middelaar</rhyme>.</l>
        <l id="s9:l6" met="4" type="-+">Het zy dan amen, jaa, en <rhyme label="c" type="m" corresp="#s9:l5">waar</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
