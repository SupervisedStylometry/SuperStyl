    <melody>Da Israel aus Egypten</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">NIet ons, niet ons, O eewighe <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Maer uwen Naem gheeft alleen die <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Om dijn goetheyt en <rhyme label="b" type="f">trouwe</rhyme>.</l>
        <l id="s1:l4" met="5" type="+-">Waerom sullen zijn wy der Heyd'n <rhyme label="c" type="m">spot</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Dat sy segghen waer is nu haer <rhyme label="c" type="m" corresp="#s1:l4">Godt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat moest ons al <rhyme label="b" type="f" corresp="#s1:l3">berouwen</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Want onse Godt in Hemel <rhyme label="d" type="m">is</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Hy maeckt al wat hem lust <rhyme label="d" type="m" corresp="#s1:l7">ghewis</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Daer gheener Gode maecket <rhyme label="e" type="m">zijn</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Wt Silver, Gout, van Menschen <rhyme label="e" type="m" corresp="#s1:l9">fijn</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Sy hebben Monden, spreken <rhyme label="f" type="m">nicht</rhyme>,</l>
        <l id="s1:l12" met="4" type="-+">End Oogen, maer gantsch geen <rhyme label="f" type="m" corresp="#s1:l11">ghesicht</rhyme>,</l>
        <l id="s1:l13" met="4" type="-+">Alleluia, <rhyme label="g" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Sy hebben ooren end hooren <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s2:l2" met="5" type="+-">En Neusen, maer gheenen Reuck <rhyme label="a" type="m" corresp="#s2:l1">altijt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">T'is haer een Fout <rhyme label="b" type="f">gheheele</rhyme>.</l>
        <l id="s2:l4" met="5" type="+-">Sy hebben handen en grijpen <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s2:l5" met="5" type="+-">End Voeten, maer sy gaen doch <rhyme label="c" type="m" corresp="#s2:l4">niet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gheen spraeck coemt wt haer <rhyme label="b" type="f" corresp="#s2:l3">keele</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Die sulcke maecken syn soo <rhyme label="d" type="m">mee</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">End alle op haer <rhyme label="d" type="m" corresp="#s2:l7">hopende</rhyme>.</l>
        <l id="s2:l9" met="4" type="-+">Maer Aarons huys end <rhyme label="e" type="m">Israel</rhyme></l>
        <l id="s2:l10" met="4" type="-+">En die den Heere vreesen <rhyme label="e" type="m" corresp="#s2:l9">wel</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Die hoopen altijt op den <rhyme label="f" type="m">HEER</rhyme>,</l>
        <l id="s2:l12" met="4" type="-+">Die is haer hulp, troost end <rhyme label="f" type="m" corresp="#s2:l11">Gheweer</rhyme>,</l>
        <l id="s2:l13" met="4" type="-+">Alleluia, <rhyme label="g" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Genadelijck denckt aen ons de <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s3:l2" met="5" type="+-">En sal ons Eewelijck Seeg'nen <rhyme label="a" type="m" corresp="#s3:l1">meer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met Gaven schoon van <rhyme label="b" type="f">boven</rhyme>,</l>
        <l id="s3:l4" met="5" type="+-">Hy Seghent dat Huys van <rhyme label="c" type="m">Israel</rhyme>,</l>
        <l id="s3:l5" met="5" type="+-">Hy Seghent oock t'Huys Aarons <rhyme label="c" type="m" corresp="#s3:l4">wel</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al die aen hem <rhyme label="b" type="f" corresp="#s3:l3">Ghelooven</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Sy Seghents' alle <rhyme label="d" type="m">goedertier</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Die Godt den Heer recht Vreesen <rhyme label="d" type="m" corresp="#s3:l7">hier</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">De Heer tot desen Seghen <rhyme label="e" type="m">al</rhyme></l>
        <l id="s3:l10" met="4" type="-+">Noch meerder Seghen gheven <rhyme label="e" type="m" corresp="#s3:l9">sal</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Op u end uwe kinder <rhyme label="f" type="m">beyd</rhyme>,</l>
        <l id="s3:l12" met="4" type="-+">Nu altijdt end in <rhyme label="f" type="m" corresp="#s3:l11">Eewicheyt</rhyme>,</l>
        <l id="s3:l13" met="4" type="-+">Alleluia, <rhyme label="g" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Ghy zijt des Heeren <rhyme label="a" type="m">Ghesegende</rhyme>,</l>
        <l id="s4:l2" met="5" type="+-">Door welcken Hemel end Aerd staen <rhyme label="a" type="m" corresp="#s4:l1">Ree</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">End wat daer in is <rhyme label="b" type="f">meere</rhyme>.</l>
        <l id="s4:l4" met="5" type="+-">Den Hemel seer schoon All om vol <rhyme label="c" type="m">Eer</rhyme>,</l>
        <l id="s4:l5" met="5" type="+-">Is Eewelijck Onderdaen den <rhyme label="c" type="m" corresp="#s4:l4">Heer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">D'aerd is den Menschen kind'ren <rhyme label="b" type="f" corresp="#s4:l3">geven</rhyme></l>
        <l id="s4:l7" met="4" type="-+">O Heer die Dooden sullen <rhyme label="d" type="m">dy</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Niet Loven, noch tot gheenen <rhyme label="d" type="m" corresp="#s4:l7">Ty</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Die in der Stilheyt in de <rhyme label="e" type="m">Aerd</rhyme></l>
        <l id="s4:l10" met="4" type="-+">Begraven syn hier <rhyme label="e" type="m" corresp="#s4:l9">Onderwaert</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Maer wy te Loven syn <rhyme label="f" type="m">bereydt</rhyme></l>
        <l id="s4:l12" met="4" type="-+">Den Heer, van nu in <rhyme label="f" type="m" corresp="#s4:l11">Eewicheyt</rhyme>.</l>
        <l id="s4:l13" met="4" type="-+">Alleluia, <rhyme label="g" type="m">Alleluia</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
