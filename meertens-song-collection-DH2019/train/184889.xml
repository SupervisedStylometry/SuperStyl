  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIlt gy t'Hemels Hof <rhyme label="a" type="f">beschryven</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">En al dat ons daer <rhyme label="b" type="m">verheught</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Laet al uwe sinnen <rhyme label="a" type="f" corresp="#s1:l1">dryven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Gy en vat noch niet dees <rhyme label="b" type="m" corresp="#s1:l2">vreught</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Noyt en ist in d'oor <rhyme label="c" type="m">gedaelt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Noch t'en heeft t'gesicht <rhyme label="c" type="m" corresp="#s1:l5">bestraelt</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">t'Geen naer <rhyme label="d" type="f">t'leven</rhyme></l>
        <l id="s1:l8" met="2" type="+-">Godt sal <rhyme label="d" type="f" corresp="#s1:l7">geven</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Aen die <rhyme label="e" type="f">trachten</rhyme></l>
        <l id="s1:l10" met="2" type="+-">Uyt hun <rhyme label="e" type="f" corresp="#s1:l9">krachten</rhyme></l>
        <l id="s1:l11" met="2" type="+-">Naer syn <rhyme label="f" type="m">min</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Met hert, en <rhyme label="f" type="m" corresp="#s1:l11">sin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hemels poorten, Hemels <rhyme label="a" type="f">straeten</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Syn van Peerels <rhyme label="b" type="m">op-gebouwt</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">En daer blincken boven <rhyme label="a" type="f" corresp="#s2:l1">maeten</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Diamanten in fyn <rhyme label="b" type="m" corresp="#s2:l2">goudt</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Boven al sal Godts <rhyme label="c" type="m">aenschyn</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Blyder, schoonder, klaerder <rhyme label="c" type="m" corresp="#s2:l5">syn</rhyme>;</l>
        <l id="s2:l7" met="2" type="+-">d'Hemels <rhyme label="d" type="f">schaeren</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Door bly <rhyme label="d" type="f" corresp="#s2:l7">snaeren</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Sullen <rhyme label="e" type="f">loven</rhyme></l>
        <l id="s2:l10" met="2" type="+-">Godt hier <rhyme label="e" type="f" corresp="#s2:l9">boven</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Geven <rhyme label="f" type="m">danck</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Door soet <rhyme label="f" type="m" corresp="#s2:l11">gesanck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Alle goedt, dat is te <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Sal voor alle <rhyme label="b" type="m">eeuwigheydt</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">U met Godt te saemen <rhyme label="a" type="f" corresp="#s3:l1">binden</rhyme></l>
        <l id="s3:l4" met="4" type="+-">In die blyde <rhyme label="b" type="m" corresp="#s3:l2">saeligheydt</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Stiert maer, jonckheydt, van uw' <rhyme label="c" type="m">jeught</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Heel uw herte tot de <rhyme label="c" type="m" corresp="#s3:l5">deught</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Sult gy, <rhyme label="d" type="f">menschen</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Dan niet <rhyme label="d" type="f" corresp="#s3:l7">wenschen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">En niet <rhyme label="e" type="f">haeken</rhyme></l>
        <l id="s3:l10" met="2" type="+-">Om te <rhyme label="e" type="f" corresp="#s3:l9">smaeken</rhyme></l>
        <l id="s3:l11" met="2" type="+-">DiÃ«n <rhyme label="f" type="m">tydt</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">Die t'al <rhyme label="f" type="m" corresp="#s3:l11">verblydt</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
