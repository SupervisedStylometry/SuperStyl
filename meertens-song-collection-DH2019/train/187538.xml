<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187538</id>
    <date>
      <year-start>1693</year-start>
      <year-end>1693</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Je ni toi mon coeur</melody>
  <author>Frank A. Metaal</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">D E droeve <rhyme label="a" type="m">eensaemheyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Begin ick voor u te <rhyme label="b" type="f">vreesen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Want ick ben nu <rhyme label="a" type="m" corresp="#s1:l1">bereyt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Om met mijn t’ween te <rhyme label="b" type="f" corresp="#s1:l2">weesen</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Stiert gy by <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Een goet <rhyme label="c" type="m" corresp="#s1:l5">partuer</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Om u Heer te dienen <rhyme label="c" type="m" corresp="#s1:l5">bly</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick kom tot u om <rhyme label="a" type="m">raet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O Heer met knien <rhyme label="b" type="f">gebogen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ick soeck den <rhyme label="a" type="m" corresp="#s2:l1">Echten-staet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wilter my doch in <rhyme label="b" type="f" corresp="#s2:l2">vogen</rhyme>.</l>
        <l id="s2:l5" met="2" type="+-">Stiert gy by <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Een goet <rhyme label="c" type="m" corresp="#s2:l5">partuer</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Om u Heer te dienen <rhyme label="c" type="m" corresp="#s2:l5">bly</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">‘k Heb hier en daer <rhyme label="a" type="m">verkeert</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Om ’t Houwelijck te <rhyme label="b" type="f">vinde</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Gy hebt noch niet <rhyme label="a" type="m" corresp="#s3:l1">begeert</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dus ô Heer mijn <rhyme label="b" type="f" corresp="#s3:l2">beminde</rhyme>.</l>
        <l id="s3:l5" met="2" type="+-">Stiert gy by <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Een goet <rhyme label="c" type="m" corresp="#s3:l5">partuer</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Om u Heer te dienen <rhyme label="c" type="m" corresp="#s3:l5">bly</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Want sonder u is ’t <rhyme label="a" type="m">quaet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">’t Huwelijck aen te <rhyme label="b" type="f">vaerden</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dus ick niet af en <rhyme label="a" type="m" corresp="#s4:l1">laet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Te bidden Heer van <rhyme label="b" type="f" corresp="#s4:l2">waerden</rhyme>.</l>
        <l id="s4:l5" met="2" type="+-">Stiert gy by <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Een goet <rhyme label="c" type="m" corresp="#s4:l5">partuer</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Om u Heer te dienen <rhyme label="c" type="m" corresp="#s4:l5">bly</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Op dat het mach tot <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Glucksaligheyt <rhyme label="b" type="f">gedyen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dus Heer en <rhyme label="a" type="m" corresp="#s5:l1">Medecijn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Weest met my in het <rhyme label="b" type="f" corresp="#s5:l2">vryen</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Stiert gy by <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Een goet <rhyme label="c" type="m" corresp="#s5:l5">partuer</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Om u Heer te dienen <rhyme label="c" type="m" corresp="#s5:l5">bly</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Op dat ick mach ô <rhyme label="a" type="m">Heer</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">U lof en eere <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">In ’t goe meer ende <rhyme label="a" type="m" corresp="#s6:l1">meer</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Toe-nemer in dit <rhyme label="b" type="f" corresp="#s6:l2">leven</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Stiert gy by <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Een goet <rhyme label="c" type="m" corresp="#s6:l5">partuer</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Om u Heer te dienen <rhyme label="c" type="m" corresp="#s6:l5">bly</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Oorlof, ô Princen <rhyme label="a" type="m">God</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">Met u wil ick <rhyme label="b" type="f">beginnen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Geeft mijn doch een goet <rhyme label="a" type="m" corresp="#s7:l1">lot</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Regeert en stiert mijn <rhyme label="b" type="f" corresp="#s7:l2">sinnen</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Stiert gy by <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Een goet <rhyme label="c" type="m" corresp="#s7:l5">partuer</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Om u Heer te dienen <rhyme label="c" type="m" corresp="#s7:l5">bly</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
