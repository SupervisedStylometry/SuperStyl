  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Vis-jes, in dees heldre <rhyme label="a" type="m">vliet</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Schoon gy moogt in 't water <rhyme label="b" type="f">leven</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Zulks kan u geen voordeel <rhyme label="b" type="f" corresp="#s1:l2">geven</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Gy ontvlucht de liefde <rhyme label="a" type="m" corresp="#s1:l1">niet</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">'t Water kan geen min <rhyme label="c" type="f">verkoelen</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">Wat baet my dan 't tegen <rhyme label="c" type="f" corresp="#s1:l5">woelen</rhyme>?</l>
        <l id="s1:l7" met="4" type="+-">'k Volg o listig <rhyme label="d" type="m">minnekindt</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Uw geboden na als <rhyme label="d" type="m" corresp="#s1:l7">blind</rhyme>.</l>
      </lg>
    </lg>
  </text>
