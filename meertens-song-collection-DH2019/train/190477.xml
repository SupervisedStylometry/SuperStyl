<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190477</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Figaro</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Toomloos Volk, wilt eindlyk <rhyme label="a" type="f">staaken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Al uw haat en <rhyme label="b" type="m">plunderzucht</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Schoon de Keezen hun gal <rhyme label="a" type="f" corresp="#s1:l1">braaken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Zy zyn thans niet meer <rhyme label="b" type="m" corresp="#s1:l2">geducht</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Zints Oranje U kwam <rhyme label="a" type="f" corresp="#s1:l1">genaaken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Zyn wy als in de oude <rhyme label="c" type="m">tyd</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Vry voor 't woeden van de <rhyme label="c" type="m" corresp="#s1:l6">nyd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Laat u dan ten beste <rhyme label="a" type="f">raaden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Kiest voor wraakzucht, vreede en <rhyme label="b" type="m">rust</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Twist en opr{problem}er kan u <rhyme label="a" type="f" corresp="#s2:l1">schaaden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Maak van de Eendracht al uw <rhyme label="b" type="m" corresp="#s2:l2">lust</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Laat de wroeging aan de <rhyme label="a" type="f" corresp="#s2:l1">kwaaden</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dien is steeds de grootste <rhyme label="c" type="m">straf</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Voor hunne eereloos <rhyme label="c" type="m" corresp="#s2:l6">geblaf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Houd dan op met <rhyme label="a" type="f">Removeeren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Zulks verstrekt u maar tot <rhyme label="b" type="m">hoon</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zoek u roem staag te <rhyme label="a" type="f" corresp="#s3:l1">vermeeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Volg U Overheids <rhyme label="b" type="m" corresp="#s3:l2">gebo√¥n</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dan zal ieder u <rhyme label="a" type="f" corresp="#s3:l1">waardeeren</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Als het allerbeste <rhyme label="c" type="m">Pand</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Van het vrye <rhyme label="c" type="m" corresp="#s3:l6">Nederland</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wilt U steeds alom <rhyme label="a" type="f">verblyden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Gy getrouwe <rhyme label="b" type="m">Burgerschaar</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Nassouws vuist zal voor U <rhyme label="a" type="f" corresp="#s4:l1">stryden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En ontrukken aan 't <rhyme label="b" type="m" corresp="#s4:l2">gevaar</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Hy verloste U van U <rhyme label="a" type="f" corresp="#s4:l1">leiden</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Door des Heemels wondre <rhyme label="c" type="m">magt</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Die alleen Uw ramp <rhyme label="c" type="m" corresp="#s4:l6">vernacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
