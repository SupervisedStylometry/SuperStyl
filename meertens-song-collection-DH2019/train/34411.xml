<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>34411</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Jezus wees gegroet almachtig</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Godfried Bussé</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IESU weest ghegroet <rhyme label="a" type="f">Almachtigh</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Godt en Kindeken <rhyme label="a" type="f" corresp="#s1:l1">waerachtigh</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Godt die alle schatten <rhyme label="b" type="m">gheeft</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Kindeken hier niet en <rhyme label="b" type="m" corresp="#s1:l3">heeft</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s1:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s1:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s1:l7">bemint</rhyme>!</l>
        <l id="s1:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s1:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Schepper, Godt, van alle <rhyme label="a" type="f">dinghen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Daer de Enghelen om <rhyme label="a" type="f" corresp="#s2:l1">singhen</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Godt het woordt hier niet en <rhyme label="b" type="m">spreckt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En noch leyt hy groot <rhyme label="b" type="m" corresp="#s2:l3">ghebreck</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s2:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s2:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s2:l7">bemint</rhyme>!</l>
        <l id="s2:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s2:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Voor ons wormmekens der <rhyme label="a" type="f">eerden</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Komt ghy hier het vlees <rhyme label="a" type="f" corresp="#s3:l1">aenveerden</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Voor ons minder als den <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Leyt ghy Jseu groot <rhyme label="b" type="m" corresp="#s3:l3">verdriet</rhyme>!</l>
        <l id="s3:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s3:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s3:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s3:l7">bemint</rhyme>!</l>
        <l id="s3:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s3:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hebben niet tot hunnen <rhyme label="a" type="f">besten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Vossen hollen, Voghels <rhyme label="a" type="f" corresp="#s4:l1">nesten</rhyme>?</l>
        <l id="s4:l3" met="4" type="+-">Daer ghy, ô eeuwigh <rhyme label="b" type="m">wellust</rhyme>!</l>
        <l id="s4:l4" met="4" type="+-">Hebt niet daer u hooft <rhyme label="b" type="m" corresp="#s4:l3">op-rust</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s4:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s4:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s4:l7">bemint</rhyme>!</l>
        <l id="s4:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s4:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Siet den alderhooghsten <rhyme label="a" type="f">Koningh</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Heeft noch hof, noch huys, noch <rhyme label="a" type="f" corresp="#s5:l1">wooningh</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Siet den alderhooghsten <rhyme label="b" type="m">Godt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Leyt verworpen in een <rhyme label="b" type="m" corresp="#s5:l3">kot</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s5:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s5:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s5:l7">bemint</rhyme>!</l>
        <l id="s5:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s5:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Godt het vier verstijft van <rhyme label="a" type="f">kouwe</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">T'eeuwigh woordt dat schreyt van <rhyme label="a" type="f" corresp="#s6:l1">rouwe</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">Die de hemelen <rhyme label="b" type="m">verblijdt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Hongher, dorst, en droefheyt <rhyme label="b" type="m" corresp="#s6:l3">lijdt</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s6:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s6:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s6:l7">bemint</rhyme>!</l>
        <l id="s6:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s6:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Godt en mensch betaelt <rhyme label="a" type="f">ghenadigh</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Voor u sondaer seer <rhyme label="a" type="f" corresp="#s7:l1">misdadigh</rhyme>;</l>
        <l id="s7:l3" met="4" type="+-">Peyst hoe ghy verghelden <rhyme label="b" type="m">sult</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Die voldoet voor uwe <rhyme label="b" type="m" corresp="#s7:l3">schult</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s7:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s7:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s7:l7">bemint</rhyme>!</l>
        <l id="s7:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s7:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Peyst ô mensch wat ghy sijt <rhyme label="a" type="f">schuldigh</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Hem, die voor u is <rhyme label="a" type="f" corresp="#s8:l1">sorghvuldigh</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Hem, die altijdt op u <rhyme label="b" type="m">denckt</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">En sy selven aen u <rhyme label="b" type="m" corresp="#s8:l3">schenckt</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s8:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s8:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s8:l7">bemint</rhyme>!</l>
        <l id="s8:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s8:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Och oft ghy met <rhyme label="a" type="f">weder-liefde</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Dit soet Kindeken <rhyme label="a" type="f" corresp="#s9:l1">beliefde</rhyme>!</l>
        <l id="s9:l3" met="4" type="+-">En waert danckbaer voor het <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Dat hy t'uwer liefde <rhyme label="b" type="m" corresp="#s9:l3">doet</rhyme>.</l>
        <l id="s9:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s9:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s9:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s9:l7">bemint</rhyme>!</l>
        <l id="s9:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s9:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Liefsten Jesu wilt dan <rhyme label="a" type="f">gheven</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Dat wy in u liefde <rhyme label="a" type="f" corresp="#s10:l1">leven</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Liefde stercker als de <rhyme label="b" type="m">doodt</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Liefde toevlucht inden <rhyme label="b" type="m" corresp="#s10:l3">noodt</rhyme>.</l>
        <l id="s10:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s10:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s10:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s10:l7">bemint</rhyme>!</l>
        <l id="s10:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s10:l7">bemint</rhyme>!</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Want u liefde Godt, te <rhyme label="a" type="f">derven</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Js ons een ghedurigh <rhyme label="a" type="f" corresp="#s11:l1">sterven</rhyme>:</l>
        <l id="s11:l3" met="4" type="+-">Sterven door des liefdens <rhyme label="b" type="m">kracht</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Js Godt die mijn siel <rhyme label="b" type="m" corresp="#s11:l3">verwacht</rhyme>.</l>
        <l id="s11:l5" met="2" type="-+">O <rhyme label="c" type="m">goedtheydt</rhyme>!</l>
        <l id="s11:l6" met="2" type="-+">O <rhyme label="d" type="m">soetheydt</rhyme>!</l>
        <l id="s11:l7" met="3" type="-+">O Godt! ô <rhyme label="e" type="m">Kindt</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s11:l7">bemint</rhyme>!</l>
        <l id="s11:l9" met="3" type="-+">Hoe hebt ghy ons <rhyme label="e" type="m" corresp="#s11:l7">bemint</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
