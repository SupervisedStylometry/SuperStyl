<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185407</id>
    <date>
      <year-start>1720</year-start>
      <year-end>1720</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Graaf Willem zat op zolder</melody>
  <author>Johannes Stichter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALs yemant eens wel gaet <rhyme label="a" type="m">doorsien</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Den Staet van <rhyme label="b" type="m">Alleman</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Van Princen, en van <rhyme label="c" type="m">Koningen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">My dunkt hy zeggen <rhyme label="b" type="m" corresp="#s1:l2">kan</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Gerust in eenen <rhyme label="e" type="m">Stal</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Als hoog verheven eens te <rhyme label="f" type="m">zijn</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En wachten zwaren <rhyme label="e" type="m" corresp="#s1:l6">val</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al draegt den Koning op zijn <rhyme label="a" type="m">hooft</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Een Kroon van louter <rhyme label="b" type="m">Goud</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Al woont hy in een schoon <rhyme label="c" type="m">Palleys</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Seer kostelijk <rhyme label="b" type="m" corresp="#s2:l2">gebouwt</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met eenen slegten <rhyme label="e" type="m">hoed</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Als in soo grooten <rhyme label="f" type="m">zorge</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Te werden <rhyme label="e" type="m" corresp="#s2:l6">opgevoed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Al heeft den Bisschop, en <rhyme label="a" type="m">Prelaet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een Goude Koor-kap <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Al mag hy met den gulden <rhyme label="c" type="m">Staf</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En met den Mijter <rhyme label="b" type="m" corresp="#s3:l2">gaen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">De hand al aen den <rhyme label="e" type="m">Ploeg</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Altijt te moeten <rhyme label="f" type="m">sorgen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Voor Zielen, laet en <rhyme label="e" type="m" corresp="#s3:l6">vroeg</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Princen, en den <rhyme label="a" type="m">Edeldom</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Al maecken zy goet <rhyme label="b" type="m">cier</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met Rijnschen Wijn, en <rhyme label="c" type="m">Wilt-gebraed</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En ook met kostelijk <rhyme label="b" type="m" corresp="#s4:l2">Bier</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En eet Boek-weyte <rhyme label="e" type="m">bry</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Als met haer in den Krijg te <rhyme label="f" type="m">gaen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Den Deegen aen de <rhyme label="e" type="m" corresp="#s4:l6">zy</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Al is mijn Heer Pastoor <rhyme label="a" type="m">verzien</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van Torf, ende <rhyme label="b" type="m">brand</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Al heeft hy lange Rocken <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De Moffen aen de <rhyme label="b" type="m" corresp="#s5:l2">hand</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Al ley ik somtijts <rhyme label="e" type="m">kouw</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Als Preecken, en Studeren <rhyme label="f" type="m">soo</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En leven sonder <rhyme label="e" type="m" corresp="#s5:l6">Vrouw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Al wint een Procureur veel <rhyme label="a" type="m">gelt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En ook den <rhyme label="b" type="m">Advocaet</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Alleen met Pen, en zijnen <rhyme label="c" type="m">mont</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Te schrijven Goet en <rhyme label="b" type="m" corresp="#s6:l2">Quaet</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Met mijn Spae in ’t <rhyme label="e" type="m">Velt</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Als van een twistig <rhyme label="f" type="m">Ackerman</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Te winnen soo veel <rhyme label="e" type="m" corresp="#s6:l6">gelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Al woont de Koopman in de <rhyme label="a" type="m">Stadt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Seer magtig, ende <rhyme label="b" type="m">rijk</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Al gaet hy op zijn Muyltjes <rhyme label="c" type="m">sagt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Al treet hy niet in ’t <rhyme label="b" type="m" corresp="#s7:l2">slijk</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Hier buyten sonder <rhyme label="e" type="m">pragt</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Als wonderlijk te <rhyme label="f" type="m">zorgen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En schrijven bag en <rhyme label="e" type="m" corresp="#s7:l6">nagt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Al is de Winckelier <rhyme label="a" type="m">versien</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Van Waeren <rhyme label="b" type="m">abondant</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Al is de Neering nog soo <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Al wint hy voor de <rhyme label="b" type="m" corresp="#s8:l2">hand</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En bezig met mijn <rhyme label="e" type="m">Wan</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Als op te passen vroeg, en <rhyme label="f" type="m">laet</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Tot dienst van yeder <rhyme label="e" type="m" corresp="#s8:l6">Man</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Al heeft den Ruyter groot <rhyme label="a" type="m">pleysier</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">In Trommelen e <rhyme label="b" type="m">Trompet</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Al gaet hy met de Sluyer <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Op ’t hooft een <rhyme label="b" type="m" corresp="#s9:l2">Hellemet</rhyme>:</l>
        <l id="s9:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">En slape altijt <rhyme label="e" type="m">wel</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Als seven uren in den <rhyme label="f" type="m">nagt</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Te staen op <rhyme label="e" type="m" corresp="#s9:l6">Sinternel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Al heeft den Schipper goe <rhyme label="a" type="m">Fortuyn</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Als hy maer komen <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Geladen uyt een ander <rhyme label="c" type="m">deel</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Des Werelts, <rhyme label="b" type="m" corresp="#s10:l2">t’Amsterdam</rhyme>:</l>
        <l id="s10:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Met mijn Kar op de <rhyme label="e" type="m">Hey</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Als midden in de wilde <rhyme label="f" type="m">Zee</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Met een Schip oft <rhyme label="e" type="m" corresp="#s10:l6">Galey</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Met oorlof gy <rhyme label="a" type="m">Boerinnekens</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">En Boeren <rhyme label="b" type="m">altemael</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Acht u geluckig in u <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En segt in ’t <rhyme label="b" type="m" corresp="#s11:l2">generael</rhyme>:</l>
        <l id="s11:l5" met="4" type="-+">Nog liever ben ik eenen <rhyme label="d" type="m">Boer</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En leef gerust op ’t <rhyme label="e" type="m">Landt</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Als Koning, Prins, of <rhyme label="f" type="m">Edelman</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Vol zorg aen alle <rhyme label="e" type="m" corresp="#s11:l6">kant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
