  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-"><rhyme label="a" type="f">Veldgodinnen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Waard te <rhyme label="a" type="f" corresp="#s1:l1">minnen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Laat u niet <rhyme label="b" type="f">mishagen</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Dat ik <rhyme label="c" type="f">heden</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Hier kom <rhyme label="c" type="f" corresp="#s1:l4">treden</rhyme></l>
        <l id="s1:l6" met="2" type="+-"><rhyme label="d" type="m">Onverwacht</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Ik zocht u <rhyme label="e" type="m">over√§l</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Langs bergen, bosch en <rhyme label="e" type="m" corresp="#s1:l7">dal</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">En zal bly' <rhyme label="f" type="m">zyn</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Mag het vry <rhyme label="f" type="m" corresp="#s1:l9">zyn</rhyme></l>
        <l id="s1:l11" met="3" type="+-">U myn dienst op te <rhyme label="g" type="f">offren</rhyme>,</l>
        <l id="s1:l12" met="2" type="+-">En te <rhyme label="h" type="f">tonen</rhyme>,</l>
        <l id="s1:l13" met="2" type="+-"><rhyme label="h" type="f" corresp="#s1:l12">Overschonen</rhyme>,</l>
        <l id="s1:l14" met="2" type="+-">Hoe 'k u <rhyme label="d" type="m" corresp="#s1:l6">acht</rhyme>.</l>
      </lg>
