  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEf op u Stemmen, Hart en <rhyme label="a" type="f">Handen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Lok heyligheyt ten Hemel <rhyme label="b" type="m">af</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Smeek dat dees' beyder liefd' mach <rhyme label="a" type="f" corresp="#s1:l1">branden</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Soo lange tot haar scheyd' het <rhyme label="b" type="m" corresp="#s1:l2">Graf</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">En datse nevens <rhyme label="c" type="m">dien</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Kindts-kind'ren mogen <rhyme label="c" type="m" corresp="#s1:l5">sien</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">Met <rhyme label="d" type="m">lust</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Getrouwt, <rhyme label="d" type="m" corresp="#s1:l7">Gekust</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Tot haarder Zielen <rhyme label="d" type="m" corresp="#s1:l7">rust</rhyme>.</l>
      </lg>
    </lg>
  </text>
