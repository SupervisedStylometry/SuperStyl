<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8942</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Gelobt seist du Jesu Christ</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GElooft soo zijt ghy JEsu <rhyme label="a" type="m">CHRIst</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat ghy Mensche gheboren <rhyme label="a" type="m" corresp="#s1:l1">bist</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Van eener Maghet, dat is <rhyme label="b" type="m">waer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dies vrolick syn der Enghel <rhyme label="b" type="m" corresp="#s1:l3">schaer</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Al le lu <rhyme label="c" type="m">ya</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Des Eewighen Vaders eenich <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Nu men in der Kribben <rhyme label="a" type="m" corresp="#s2:l1">vindt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">In ons Catijvig' Vleesch end <rhyme label="b" type="m">Bloet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vercleedet hem dat Eewig' <rhyme label="b" type="m" corresp="#s2:l3">Goet</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dien alle Werelt niet <rhyme label="a" type="m">besloot</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die leydt in Marijen <rhyme label="a" type="m" corresp="#s3:l1">schoot</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hy is een kindlijn worden <rhyme label="b" type="m">cleyn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die alle Dinck Erhalt <rhyme label="b" type="m" corresp="#s3:l3">alleyn</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat Eewig' Licht gaet daer in <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Geeft der Werelt een Nieuwen <rhyme label="a" type="m" corresp="#s4:l1">schijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Het leucht wel midden in der <rhyme label="b" type="m">Nacht</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Heeft ons des Lichtes Kintschap <rhyme label="b" type="m" corresp="#s4:l3">bracht</rhyme></l>
        <l id="s4:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Soon des Vaders God van <rhyme label="a" type="m">aert</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Een gast hy in der Werrelt <rhyme label="a" type="m" corresp="#s5:l1">waert</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">End voert ons wt den <rhyme label="b" type="m">Jammerdael</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Maeckt ons Erven in synen <rhyme label="b" type="m" corresp="#s5:l3">Zael</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy is op Aerd' ghecomen <rhyme label="a" type="m">Arm</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat hy onser hem <rhyme label="a" type="m" corresp="#s6:l1">Ontfarm</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">End in den Hemel maecke <rhyme label="b" type="m">Rijck</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Jae synen lieven Eng'len <rhyme label="b" type="m" corresp="#s6:l3">ghelijck</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dat heeft hy alles ons <rhyme label="a" type="m">ghedaen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Syn groote Liefd' te Thoonen <rhyme label="a" type="m" corresp="#s7:l1">aen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dies Vreucht haer alle <rhyme label="b" type="m">Christenheyt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">En Dancken hem in <rhyme label="b" type="m" corresp="#s7:l3">Eewicheyt</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
