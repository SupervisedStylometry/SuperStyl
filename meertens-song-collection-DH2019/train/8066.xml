    <melody>Het was een klerkje dat ging ter school</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GAbriel was van Godt <rhyme label="a" type="f">ghesonden</rhyme></l>
        <l id="s1:l2" met="4" type="+-">In eene stadt van <rhyme label="b" type="m">Galileen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Nazareth geheten, na Lucas <rhyme label="a" type="f" corresp="#s1:l1">vermonden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Tot een suyver Maghet <rhyme label="b" type="m" corresp="#s1:l2">reen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die ondertrouwt was een man <rhyme label="a" type="f">ydoone</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Den welcken diemen Ioseph <rhyme label="b" type="m">hiet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Van Davids huys, en de maghet <rhyme label="a" type="f" corresp="#s2:l1">schoone</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Hiet Maria, vaet dit <rhyme label="b" type="m" corresp="#s2:l2">bediet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Den Engel quam tot haer met <rhyme label="a" type="f">vrede</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En sprack: Weest gegroet, vol gracy <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">De Heer is met u, gebenedijt <rhyme label="a" type="f" corresp="#s3:l1">mede</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Zijt ghy onder alle vrouwen <rhyme label="b" type="m" corresp="#s3:l2">soet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als Maria den Engel <rhyme label="a" type="f">aenschoude</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Verschricktese haer in sijn reden <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Maer op den Heere sy vroom <rhyme label="a" type="f" corresp="#s4:l1">betroude</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En dachte, wat een groete mach dit <rhyme label="b" type="m" corresp="#s4:l2">zijn</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">De Enghel sprack, en wilt u niet <rhyme label="a" type="f">vervaren</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Ghy hebt genade vonden by <rhyme label="b" type="m">Godt</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Ghy sult ontfangen en vrolijck <rhyme label="a" type="f" corresp="#s5:l1">baren</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Een Sone in u maeghdelijck <rhyme label="b" type="m" corresp="#s5:l2">slot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Iesus sult ghy heten sijnen <rhyme label="a" type="f">Name</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Hy sal wesen van machte <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Des alderhooghsten Sone <rhyme label="a" type="f" corresp="#s6:l1">bequame</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Wert hy geheeten, zijt des wel <rhyme label="b" type="m" corresp="#s6:l2">vroet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Godt de Heere die sal hem <rhyme label="a" type="f">geven</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Davids sijns Vaders stoel <rhyme label="b" type="m">excellent</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En hy sal over Iacobs huys <rhyme label="a" type="f" corresp="#s7:l1">verheven</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Regnieren met een Rijcke sonder <rhyme label="b" type="m" corresp="#s7:l2">endt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Maria sprack, hoe mach dit <rhyme label="a" type="f">wesen</rhyme>.</l>
        <l id="s8:l2" met="4" type="+-">Want ick en bekenne gheenen <rhyme label="b" type="m">man</rhyme>?</l>
        <l id="s8:l3" met="4" type="+-">De Enghel antwoorde na <rhyme label="a" type="f" corresp="#s8:l1">desen</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">De Heylige Gheest sal u comen <rhyme label="b" type="m" corresp="#s8:l2">an</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">De cracht des Hoochsten sal oock te <rhyme label="a" type="f">vooren</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Om schaduwen u, also 't <rhyme label="b" type="m">betaemt</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Daerom het Heyligh, d'welck wordt <rhyme label="a" type="f" corresp="#s9:l1">geboren</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Sal worden Godes Sone <rhyme label="b" type="m" corresp="#s9:l2">genaemt</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Onvruchtbaer was Elisabeth u <rhyme label="a" type="f">nichte</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">En sy heeft ontfangen een knapelijck <rhyme label="b" type="m">kint</rhyme></l>
        <l id="s10:l3" met="4" type="+-">In haer ouderdom, het is Godt al <rhyme label="a" type="f" corresp="#s10:l1">lichte</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Om doen, dies hy hem <rhyme label="b" type="m" corresp="#s10:l2">onderwint</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Maria liet haer van den Engel <rhyme label="a" type="f">leeren</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">En sy geloofde met een <rhyme label="b" type="m">verblien</rhyme></l>
        <l id="s11:l3" met="4" type="+-">En sprack, hier is de dienstmaecht des <rhyme label="a" type="f" corresp="#s11:l1">Heeren</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Na uwen woorden moet my <rhyme label="b" type="m" corresp="#s11:l2">gheschien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
