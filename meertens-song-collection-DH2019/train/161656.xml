<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161656</id>
    <date>
      <year-start>1730</year-start>
      <year-end>1730</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het vinnige stralen van de zon</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">RYst uyt den slaep schoone <rhyme label="a" type="m">herderin</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Aurora die komt ons <rhyme label="b" type="f">ontwecken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Rijst uyt den slaep mijn schoon <rhyme label="a" type="m" corresp="#s1:l1">vriendin</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Wy moeten samen in't velt gaen <rhyme label="b" type="f" corresp="#s1:l2">trecken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wel wie hoor ick voor mijn <rhyme label="a" type="m">deur</rhyme>?</l>
        <l id="s2:l2" met="4" type="-+">My roepen in der <rhyme label="b" type="f">midder-nachten</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Is't eenen herder of een <rhyme label="a" type="m" corresp="#s2:l1">monsieur</rhyme>?</l>
        <l id="s2:l4" met="4" type="-+">Die my so vroeg komt <rhyme label="b" type="f" corresp="#s2:l2">ontwaken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Och neen mijn schoone <rhyme label="a" type="m">Silvia</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">'t Is Coridon uwen <rhyme label="b" type="f">beminde</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Komt liefste want het wort te <rhyme label="a" type="m" corresp="#s3:l1">spa</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Laet ons gaen treeden onder de <rhyme label="b" type="f" corresp="#s3:l2">linde</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ach Coridon waer wilt ghy <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ick sie de lucht is noch vol <rhyme label="b" type="f">sterren</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">Wat maeckt gy so vroeg op de <rhyme label="a" type="m" corresp="#s4:l1">baen</rhyme>?</l>
        <l id="s4:l4" met="4" type="-+">Want de dagh is noch te <rhyme label="b" type="f" corresp="#s4:l2">verre</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wel hoe mijn schoone <rhyme label="a" type="m">Engelin</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Mijn schaepkens zijn al aen het <rhyme label="b" type="f">weyden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Sa spoet u wat mijn <rhyme label="a" type="m" corresp="#s5:l1">herderin</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Om uwent wille sal ick wat <rhyme label="b" type="f" corresp="#s5:l2">beyden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wel is het waer? ach <rhyme label="a" type="m">Coridon</rhyme>!</l>
        <l id="s6:l2" met="4" type="-+">Ick sal haest op de been wel <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Ia Lief mijn waerde <rhyme label="a" type="m" corresp="#s6:l1">blom</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">De Son is langh <rhyme label="b" type="f" corresp="#s6:l2">opgeresen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sa komt mijn schoone <rhyme label="a" type="m">herderin</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Ick sal u schaepkens gaen <rhyme label="b" type="f">ontsluyten</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ick salse drijven te velden <rhyme label="a" type="m" corresp="#s7:l1">in</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Om te gaen plucken de groene <rhyme label="b" type="f" corresp="#s7:l2">kruyden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Wel Coridon, ach! toeft noch <rhyme label="a" type="m">wat</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Ick moet mijn dan gereet gaen <rhyme label="b" type="f">maken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Dan sullen wy al langs dit <rhyme label="a" type="m" corresp="#s8:l1">pat</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ons schaepkens drijven en vreugde <rhyme label="b" type="f" corresp="#s8:l2">rapen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wel Lief ick bid u toef niet <rhyme label="a" type="m">lanck</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Den Nachtegael begint te <rhyme label="b" type="f">singen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Al door zijn schoon en soet <rhyme label="a" type="m" corresp="#s9:l1">geklanck</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Mijn hert van vreugt begint te <rhyme label="b" type="f" corresp="#s9:l2">springen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Sa Coridon laet ons met <rhyme label="a" type="m">vreugt</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Ons schaepjes in het velt gaen <rhyme label="b" type="f">leyden</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Ach laet ons nu treeden met <rhyme label="a" type="m" corresp="#s10:l1">deugt</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Al langs de groene klaver <rhyme label="b" type="f" corresp="#s10:l2">weyden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ach Lief mijn schoone <rhyme label="a" type="m">Silvia</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Sa komt mijn soetste <rhyme label="b" type="f">Engelinne</rhyme>;</l>
        <l id="s11:l3" met="4" type="-+">Och ontfangt my in u <rhyme label="a" type="m" corresp="#s11:l1">gena</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Laet ons gaen spreeken van de <rhyme label="b" type="f" corresp="#s11:l2">minne</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Coridon is dat u <rhyme label="a" type="m">gedacht</rhyme>?</l>
        <l id="s12:l2" met="4" type="-+">Mijn aen te spreeken van de <rhyme label="b" type="f">minne</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Gy zijt voorwaer niet wel <rhyme label="a" type="m" corresp="#s12:l1">bedacht</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Ick ben te jong een <rhyme label="b" type="f" corresp="#s12:l2">herderinne</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Wel Lief ick bid u weest <rhyme label="a" type="m">gerust</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Laet my u soetjes eens <rhyme label="b" type="f">genaken</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Dat ick u geve eenen <rhyme label="a" type="m" corresp="#s13:l1">kus</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">En dat al op u rooder <rhyme label="b" type="f" corresp="#s13:l2">kaken</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Acht wat sal 't zijn! ach <rhyme label="a" type="m">Coridon</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Als ghy een kusjen hebt <rhyme label="b" type="f">gegeven</rhyme>?</l>
        <l id="s14:l3" met="4" type="-+">Het sal verdwijnen als de <rhyme label="a" type="m" corresp="#s14:l1">Son</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Die over ons hier komt <rhyme label="b" type="f" corresp="#s14:l2">gescheenen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Oorlof gy Herders in het <rhyme label="a" type="m">groen</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Leef soetjes met uw' <rhyme label="b" type="f">Herderinne</rhyme>:</l>
        <l id="s15:l3" met="4" type="-+">Geeft met het aengenaem <rhyme label="a" type="m" corresp="#s15:l1">saysoen</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Malkander eenen kus van <rhyme label="b" type="f" corresp="#s15:l2">minne</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
