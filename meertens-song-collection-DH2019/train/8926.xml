        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ach Gott vom Himmel</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ACh Heer ick Roep Ancxtich tot <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Te helpen my, haest <rhyme label="b" type="f">seere</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Verhoort des Clagens stem van <rhyme label="a" type="m" corresp="#s1:l1">my</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Als ick dy Aenroep' <rhyme label="b" type="f" corresp="#s1:l2">Heere</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Laet mijn Ghebedt syn <rhyme label="c" type="m">Aenghenaem</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Ghelijck een Offerhand' <rhyme label="c" type="m" corresp="#s1:l5">bequaem</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Nae dijnen Wil <rhyme label="d" type="f">ghemaecket</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Als ick mijn Handen tot dy <rhyme label="a" type="m">vou</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat laet dy HEEr <rhyme label="b" type="f">behaghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Bewaer mijn Mondt en Lippen <rhyme label="a" type="m" corresp="#s2:l1">Trou</rhyme></l>
        <l id="s2:l4" met="3" type="-+">En laet mijn Hert niet <rhyme label="b" type="f" corresp="#s2:l2">waghen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Met den Godloosen godloos <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Noch Eten end Laudeeren <rhyme label="c" type="m" corresp="#s2:l5">fijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wat Quaetdoenders <rhyme label="d" type="f">beminnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die vroome my wel vriend'lijc <rhyme label="a" type="m">slae</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">End straffe mijn <rhyme label="b" type="f">ghebreken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Dat sal my wel doen, sonder <rhyme label="a" type="m" corresp="#s3:l1">smae</rhyme></l>
        <l id="s3:l4" met="3" type="-+">By Balssem goet <rhyme label="b" type="f" corresp="#s3:l2">gheleken</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Want altijdt bid' ick end' wensch <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Wilt my behoeden, lieve <rhyme label="c" type="m" corresp="#s3:l5">Heer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat sy my niet en <rhyme label="d" type="f">schaden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Och dat haer Leeraers moeten <rhyme label="a" type="m">doch</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Over een Steenrotz <rhyme label="b" type="f">sneven</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dan sal men mijner Leere <rhyme label="a" type="m" corresp="#s4:l1">noch</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Het beste Loff nae <rhyme label="b" type="f" corresp="#s4:l2">gheven</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Verstroyt is ons Ghebeen ter <rhyme label="c" type="m">Hel</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">End lijden so deerlijck <rhyme label="c" type="m" corresp="#s4:l5">ghequel</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Ghelijck een t'landt, om <rhyme label="d" type="f">keeret</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mijn Oogh' op dy Heer siet <rhyme label="a" type="m">alleyn</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Betrouw' op dy <rhyme label="b" type="f">ghestadich</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Verstoot mijn Ziele niet <rhyme label="a" type="m" corresp="#s5:l1">Vileyn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maer zijt mijn Godt <rhyme label="b" type="f" corresp="#s5:l2">ghenadich</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Bewaert my oock voor haren <rhyme label="c" type="m">strick</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Dien sy my legghen tot een <rhyme label="c" type="m" corresp="#s5:l5">schrick</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Om my al t' <rhyme label="d" type="f">Overvallen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">HEER voor die Valle my <rhyme label="a" type="m">behoed'</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die my die Quade <rhyme label="b" type="f">stellen</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Bestricken moet haer eyghen <rhyme label="a" type="m" corresp="#s6:l1">Voet</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Der Godlooser <rhyme label="b" type="f" corresp="#s6:l2">gesellen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">In haren Net te samen <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Maer ghy HEER my altijdt <rhyme label="c" type="m" corresp="#s6:l5">bewaer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">End laet my vry <rhyme label="d" type="f">Passeeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
