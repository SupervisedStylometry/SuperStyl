    <melody>La princesse</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">PHilida mijn troost en <rhyme label="a" type="f">leven</rhyme>;</l>
        <l id="s1:l2" met="4" type="+-">Ik heb u mijn hert <rhyme label="a" type="f" corresp="#s1:l1">gegeven</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">In mijn ziel bent gy <rhyme label="b" type="m">geplant</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Reykt mijn doch u rechter <rhyme label="b" type="m" corresp="#s1:l3">hant</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">U min ik met hert en <rhyme label="c" type="f">sinnen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Altijt sal ik u <rhyme label="c" type="f" corresp="#s1:l5">beminnen</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Want gy zijt mijn waartste <rhyme label="b" type="m" corresp="#s1:l3">pant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Heb aan u mijn troost <rhyme label="a" type="f">geswooren</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat gy blijft mijn <rhyme label="a" type="f" corresp="#s2:l1">uytverkoren</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Mijn beminde <rhyme label="b" type="m">Herderin</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Ey mijn soete <rhyme label="b" type="m" corresp="#s2:l3">afgodin</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Wilt mijn doch niet meer <rhyme label="c" type="f">versmade</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Want gy siet ik ben <rhyme label="c" type="f" corresp="#s2:l5">belade</rhyme></l>
        <l id="s2:l7" met="4" type="+-">En verga schier door de <rhyme label="b" type="m" corresp="#s2:l3">min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Lost mijn haastig uyt <rhyme label="a" type="f">benouwen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Of anders moet ik <rhyme label="a" type="f" corresp="#s3:l1">verflouwen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En vergaen door liefd' en <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ik beswijk door lang <rhyme label="b" type="m" corresp="#s3:l3">afwachten</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dat gy noyt beweegt en <rhyme label="c" type="f">wert</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">U wreetheyt doet mijn <rhyme label="c" type="f" corresp="#s3:l5">versmachten</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Ey laaft doch mijn dorstig <rhyme label="b" type="m" corresp="#s3:l3">hert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Nacht en dag doet gy mijn <rhyme label="a" type="f">klagen</rhyme>;</l>
        <l id="s4:l2" met="4" type="+-">Dat ik u noit kan <rhyme label="a" type="f" corresp="#s4:l1">behagen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Waerom soete <rhyme label="b" type="m">Koningin</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En kan ik u noit <rhyme label="b" type="m" corresp="#s4:l3">bekoren</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Heft in u geen Plaats de <rhyme label="c" type="f">min</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Zijt en blijft mijn <rhyme label="c" type="f" corresp="#s4:l5">uytverkoren</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Schenkt my doch u hert en <rhyme label="b" type="m" corresp="#s4:l3">sin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
