<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20695</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Guillemette</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">GRoet U, Ã´ <rhyme label="a" type="f">Engel</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Die mijn bewaerder <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Om Wien ick <rhyme label="a" type="f" corresp="#s1:l1">hengel</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Als Satan my <rhyme label="b" type="m" corresp="#s1:l2">bestrijdt</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Ick koom U <rhyme label="c" type="f">prijsen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ick koom U eer <rhyme label="c" type="f" corresp="#s1:l5">bewijsen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die my doet van sonden <rhyme label="c" type="f" corresp="#s1:l5">oprijsen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Die van quaet my <rhyme label="b" type="m" corresp="#s1:l2">vrijdt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Nae dat <rhyme label="a" type="f">gekomen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ick was op 's werelts <rhyme label="b" type="m">dal</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Heb ick <rhyme label="a" type="f" corresp="#s2:l1">vernomen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Uw' hulp in mijne <rhyme label="b" type="m" corresp="#s2:l2">val</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Ten allen <rhyme label="c" type="f">tijden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ghy mijne ziel <rhyme label="c" type="f" corresp="#s2:l5">bevrijden</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Van die my de deughden <rhyme label="c" type="f" corresp="#s2:l5">benijden</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En des Hemels <rhyme label="b" type="m" corresp="#s2:l2">wall'</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Door uwe <rhyme label="a" type="f">leydingh</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Heb ick den Doop <rhyme label="b" type="m">ontfaen</rhyme>;</l>
        <l id="s3:l3" met="2" type="-+">En die <rhyme label="a" type="f" corresp="#s3:l1">bereydingh</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Doet my ten hemel <rhyme label="b" type="m" corresp="#s3:l2">gaen</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Noch bidt Ghy <rhyme label="c" type="f">krachtigh</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Voor my den Heer <rhyme label="c" type="f" corresp="#s3:l5">Almachtigh</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Dat Hy mijnder altijd <rhyme label="c" type="f" corresp="#s3:l5">gedachtigh</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Brenght tot 's hemels <rhyme label="b" type="m" corresp="#s3:l2">baen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Als my de <rhyme label="a" type="f">Helle</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Het vleysch, en werelt <rhyme label="b" type="m">quelt</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Soo komt Gh' U <rhyme label="a" type="f" corresp="#s4:l1">stellen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Beneffens my in 't <rhyme label="b" type="m" corresp="#s4:l2">veld</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Ghy helpt my <rhyme label="c" type="f">vechten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ghy helpt de kroon my <rhyme label="c" type="f" corresp="#s4:l5">vlechten</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Waer mee worden gekroont Goods <rhyme label="c" type="f" corresp="#s4:l5">knechten</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Die haer doen <rhyme label="b" type="m" corresp="#s4:l2">geweld</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">In all' mijn <rhyme label="a" type="f">wegen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Staet Ghy my vroomigh <rhyme label="b" type="m">by</rhyme>;</l>
        <l id="s5:l3" met="2" type="-+">En Godes <rhyme label="a" type="f" corresp="#s5:l1">Zegen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Soeckt Ghy gestaegh voor <rhyme label="b" type="m" corresp="#s5:l2">my</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Ghy (sonder <rhyme label="c" type="f">gecken</rhyme>)</l>
        <l id="s5:l6" met="3" type="-+">Komt my tot deughden <rhyme label="c" type="f" corresp="#s5:l5">trecken</rhyme>;</l>
        <l id="s5:l7" met="4" type="+-">Ghy my staeghs tot goet komt <rhyme label="c" type="f" corresp="#s5:l5">verwecken</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Altijd even <rhyme label="b" type="m" corresp="#s5:l2">bly</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ghy doet my <rhyme label="a" type="f">keeren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van deses werelts <rhyme label="b" type="m">pa'en</rhyme>:</l>
        <l id="s6:l3" met="2" type="-+">Ghy komt my <rhyme label="a" type="f" corresp="#s6:l1">leeren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Den wegh des Heeren <rhyme label="b" type="m" corresp="#s6:l2">gaen</rhyme>:</l>
        <l id="s6:l5" met="2" type="-+">Ghy komt my <rhyme label="c" type="f">leyden</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Tot 's Hemels schoone <rhyme label="c" type="f" corresp="#s6:l5">weyden</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Waer na ick met yver <rhyme label="c" type="f" corresp="#s6:l5">verbeyde</rhyme>:</l>
        <l id="s6:l8" met="3" type="+-">'k Hoop by God te <rhyme label="b" type="m" corresp="#s6:l2">staen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Hoe sal ick <rhyme label="a" type="f">loonen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Aen U, dees <rhyme label="b" type="m">trouwigheyd</rhyme>?</l>
        <l id="s7:l3" met="2" type="-+">Ick sal my <rhyme label="a" type="f" corresp="#s7:l1">toonen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Tot uwen dienst <rhyme label="b" type="m" corresp="#s7:l2">bereydt</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+">'k Sal all' mijn <rhyme label="c" type="f">dagen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">(O Engel!) my soo <rhyme label="c" type="f" corresp="#s7:l5">dragen</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Dat Ghy niet sult hebben te <rhyme label="c" type="f" corresp="#s7:l5">klagen</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">'t Wort U <rhyme label="b" type="m" corresp="#s7:l2">toegeseyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Wilt my <rhyme label="a" type="f">vergeven</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Het geen ick heb <rhyme label="b" type="m">misdaen</rhyme>.</l>
        <l id="s8:l3" met="2" type="-+">In 't eeuwigh <rhyme label="a" type="f" corresp="#s8:l1">leven</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Wilt mijne ziel <rhyme label="b" type="m" corresp="#s8:l2">ontfaen</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">Daer sal ick <rhyme label="c" type="f">singen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Daer sal mijn geest <rhyme label="c" type="f" corresp="#s8:l5">opspringen</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Ende sien die wond're <rhyme label="c" type="f" corresp="#s8:l5">dingen</rhyme></l>
        <l id="s8:l8" met="3" type="+-">Die het hart <rhyme label="b" type="m" corresp="#s8:l2">versaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
