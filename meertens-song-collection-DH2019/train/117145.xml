  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DAer was een boerinnetje vroeg <rhyme label="a" type="m">opgestaen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Al om haer Koeytjes te gaen <rhyme label="b" type="f">melke</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Te gaen <rhyme label="b" type="f" corresp="#s1:l2">melke</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Sy ging al na de Soldaetjes <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Soldaetjes <rhyme label="c" type="m" corresp="#s1:l4">toe</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Verkoft haer melk met <rhyme label="d" type="f">stoope</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Ia met <rhyme label="d" type="f" corresp="#s1:l6">stoope</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Boer al na de Capiteyn <rhyme label="a" type="m">gong</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En sprak mijn Heer ick moet u <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Moet u <rhyme label="b" type="f" corresp="#s2:l2">klagen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">U Soldaten hebben mijn Wijvetje <rhyme label="c" type="m">mee</rhyme>.</l>
        <l id="s2:l5" met="2" type="+-">Wijvetje <rhyme label="c" type="m" corresp="#s2:l4">mee</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ik en durf’er niet na <rhyme label="d" type="f">vragen</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Niet na <rhyme label="d" type="f" corresp="#s2:l6">vragen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Soldaten houwen mijn Wijvetje <rhyme label="a" type="m">vast</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En gaender samen mee <rhyme label="b" type="f">spanceeren</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Mee <rhyme label="b" type="f" corresp="#s3:l2">spanceeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Al op een groene sootje <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Sootje <rhyme label="c" type="m" corresp="#s3:l4">klaer</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En gaender mee <rhyme label="d" type="f">boeleeren</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Mee <rhyme label="d" type="f" corresp="#s3:l6">boeleeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En wil den Boer zijn Wijf weer <rhyme label="a" type="m">hebben</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hy magse selver komen <rhyme label="b" type="f">halen</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">komen <rhyme label="b" type="f" corresp="#s4:l2">halen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Drie goude kroonen die waren <rhyme label="c" type="m">verteert</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Waren <rhyme label="c" type="m" corresp="#s4:l4">verteert</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Den Boer moest ’t al <rhyme label="d" type="f">betalen</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Al <rhyme label="d" type="f" corresp="#s4:l6">betalen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Boer al in zijn tasje <rhyme label="a" type="m">schoot</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En doen telden hy een twee <rhyme label="b" type="f">drieje</rhyme>.</l>
        <l id="s5:l3" met="2" type="+-">Een twee <rhyme label="b" type="f" corresp="#s5:l2">drieje</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Toen kreeg den Boer zijn wijvetje <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Sijn wijvetje <rhyme label="c" type="m" corresp="#s5:l4">weer</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Sonder vechten ofte <rhyme label="d" type="f">kijven</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Ofte <rhyme label="d" type="f" corresp="#s5:l6">kijven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De Boer al uyt de poorte <rhyme label="a" type="m">quam</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Begon so bitterlijk te <rhyme label="b" type="f">kermen</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Ia te <rhyme label="b" type="f" corresp="#s6:l2">kermen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Gy sult niet meer om soete melck <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Soete melck <rhyme label="c" type="m" corresp="#s6:l4">gaen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Al souse staen <rhyme label="d" type="f">bederven</rhyme>,</l>
        <l id="s6:l7" met="2" type="+-">Staen <rhyme label="d" type="f" corresp="#s6:l6">bederven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Oorlof gy Boeren <rhyme label="a" type="m">altemael</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En wilt u wijfjes wel <rhyme label="b" type="f">bewaren</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Wel <rhyme label="b" type="f" corresp="#s7:l2">bewaren</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">so salse niet meer na de Soldaetjes <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Soldaetjes <rhyme label="c" type="m" corresp="#s7:l4">gaen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En ik sal der niet na <rhyme label="d" type="f">vragen</rhyme>,</l>
        <l id="s7:l7" met="2" type="+-">Niet na <rhyme label="d" type="f" corresp="#s7:l6">vragen</rhyme>.</l>
      </lg>
    </lg>
  </text>
