  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom, singen wy het <rhyme label="a" type="f">vermogen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Der groote <rhyme label="b" type="m">Minnegod</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die Bachus heeft <rhyme label="a" type="f" corresp="#s1:l1">bewogen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Te eeren sijn <rhyme label="b" type="m" corresp="#s1:l2">genot</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Een yder <rhyme label="c" type="m">swigt</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Voor sijn <rhyme label="c" type="m" corresp="#s1:l5">schigt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die alles kan <rhyme label="d" type="f">ontsteken</rhyme>:</l>
        <l id="s1:l8" met="2" type="-+">Het woedende <rhyme label="e" type="m">vee</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Maakt hy self   <rhyme label="e" type="m" corresp="#s1:l8">gedwee</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Wat aard of water <rhyme label="f" type="m">voed</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Omhelsd sijn <rhyme label="f" type="m" corresp="#s1:l10">gloed</rhyme>.</l>
      </lg>
    </lg>
