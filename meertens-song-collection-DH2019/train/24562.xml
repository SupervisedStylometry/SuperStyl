<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24562</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Puer natus in Bethleem</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EEn kynt gebaren in <rhyme label="a" type="m">betlahem</rhyme></l>
        <l id="s1:l2" met="4" type="+-">verblydet alle <rhyme label="a" type="m" corresp="#s1:l1">Jherusalem</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s1:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s1:l3">Amor</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die sone die nam die menscheit <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">die bi den vader comen <rhyme label="a" type="m" corresp="#s2:l1">can</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s2:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s2:l3">Amor</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doe gabryel die engel <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s3:l2" met="4" type="+-">die Joncfrouwe doeden sone <rhyme label="a" type="m" corresp="#s3:l1">gewan</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s3:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s3:l3">Amor</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Een brudegom wt sinre <rhyme label="a" type="m">cameren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">gecoemen sonder <rhyme label="a" type="m" corresp="#s4:l1">Jameren</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s4:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s4:l3">Amor</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sy legden in een <rhyme label="a" type="m">crybbekyn</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Des ewigen vaders <rhyme label="a" type="m" corresp="#s5:l1">soenekyn</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s5:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s5:l3">Amor</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die os ende die esel die hebbent <rhyme label="a" type="m">geweten</rhyme></l>
        <l id="s6:l2" met="4" type="+-">mer dat dat kynt es Jhesus <rhyme label="a" type="m" corresp="#s6:l1">geheten</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s6:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s6:l3">Amor</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Drie Conyngen quamen wt oesten <rhyme label="a" type="m">lande</rhyme></l>
        <l id="s7:l2" met="4" type="+-">ende brachten mit hem oer <rhyme label="a" type="m" corresp="#s7:l1">offerhande</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s7:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s7:l3">Amor</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Sy gyngen daer te samen <rhyme label="a" type="m">in</rhyme></l>
        <l id="s8:l2" met="4" type="+-">ende grueten dat nye <rhyme label="a" type="m" corresp="#s8:l1">kyndekyn</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s8:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s8:l3">Amor</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Die enghelen songen doe ter <rhyme label="a" type="m">tyt</rhyme></l>
        <l id="s9:l2" met="4" type="+-">die hierdekens waren mede <rhyme label="a" type="m" corresp="#s9:l1">verblyt</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s9:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s9:l3">Amor</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Die menschen inder eerden <rhyme label="a" type="m">mede</rhyme></l>
        <l id="s10:l2" met="4" type="+-">ende die gelouigen cregen den <rhyme label="a" type="m" corresp="#s10:l1">vrede</rhyme></l>
        <l id="s10:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">Amor</rhyme></l>
        <l id="s10:l4" met="3" type="+-">quam dulcys est <rhyme label="b" type="f" corresp="#s10:l3">Amor</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Myt den enghelen wille wi <rhyme label="a" type="m">syngen</rhyme></l>
        <l id="s11:l2" met="4" type="+-">ende laten onse herten in vrouden <rhyme label="a" type="m" corresp="#s11:l1">spryngen</rhyme></l>
        <l id="s11:l3" met="3" type="+-">Amor amor <rhyme label="b" type="f">amor</rhyme></l>
        <l id="s11:l4" met="3" type="+-">quam dulcis est <rhyme label="b" type="f" corresp="#s11:l3">amor</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
