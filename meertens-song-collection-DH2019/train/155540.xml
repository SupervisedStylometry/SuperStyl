<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>155540</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Tien geboden</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DOen Abram 't Offer-Vlees <rhyme label="a" type="f">bereyde</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Door het bevel van 's Hemels <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Tot welckers eer hy het <rhyme label="a" type="f" corresp="#s1:l1">uytscheyde</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Viel daer op 't Roof-gevogelt' <rhyme label="b" type="m" corresp="#s1:l2">neer</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En pickten met haer bitse <rhyme label="a" type="f">Nebbe</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En hebben 't soetst daer uyt <rhyme label="b" type="m">geknaeght</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Maer Abram die dat niet woud' <rhyme label="a" type="f" corresp="#s2:l1">hebben</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Heeft al dat Roof-gespuys <rhyme label="b" type="m" corresp="#s2:l2">verjaeght</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als wy Godt Off ren ons <rhyme label="a" type="f">Gebeden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ontslopen uyt het Aerdts <rhyme label="b" type="m">gewoel</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Soo komen dan terstont oock <rhyme label="a" type="f" corresp="#s3:l1">mede</rhyme></l>
        <l id="s3:l4" met="4" type="-+">De Gieren uyt den <rhyme label="b" type="m" corresp="#s3:l2">Zulpher-poel</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En picken met haer Aertsche <rhyme label="a" type="f">sorgen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ons heyl'ghe aendacht in't <rhyme label="b" type="m">gesicht</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Om ons Gebedt soo te <rhyme label="a" type="f" corresp="#s4:l1">verworgen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Eer het ons Ziel ten Hemel <rhyme label="b" type="m" corresp="#s4:l2">licht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer soo wy haer de Prickel <rhyme label="a" type="f">bieden</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En seggen 't Eeuwigh-Oorlogh <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Gewis sy sullen eynd'lijck <rhyme label="a" type="f" corresp="#s5:l1">vlieden</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Om in den Afgront t'schuyl te <rhyme label="b" type="m" corresp="#s5:l2">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
