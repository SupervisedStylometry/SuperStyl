  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Gun, o waardste der <rhyme label="a" type="f">vrindinnen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die ik immer heb <rhyme label="b" type="m">vertrouwd</rhyme></l>
        <l id="s1:l3" met="4" type="+-">En 't geheim van mijne <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Openhertig heb <rhyme label="b" type="m" corresp="#s1:l2">ontfoud</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Iets dat ik van u <rhyme label="c" type="f">begere</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Dat u <rhyme label="d" type="m">tegenwoordigheyd</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Dit ons Egt-feest   mag <rhyme label="c" type="f" corresp="#s1:l5">vereren</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">En dat Damon u <rhyme label="d" type="m" corresp="#s1:l6">geleid</rhyme>.</l>
      </lg>
    </lg>
