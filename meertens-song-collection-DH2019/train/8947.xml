    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Puer nobis nascitur</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ONs is nu gheboor'n een <rhyme label="a" type="m">Kindt</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Aller Eng'len <rhyme label="b" type="f">Heere</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">In die Wer'lt syn voetsel <rhyme label="a" type="m" corresp="#s1:l1">t'vindt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Heer, aller Heeren <rhyme label="b" type="f" corresp="#s1:l2">veere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">In een cribbe was't <rhyme label="a" type="m">gheleyt</rhyme></l>
        <l id="s2:l2" met="3" type="+-">In doecken <rhyme label="b" type="f">ghewonden</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daer die Wijsen hem <rhyme label="a" type="m" corresp="#s2:l1">verbreyt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Voor haren Heylant <rhyme label="b" type="f" corresp="#s2:l2">vonden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doen Herodes dat <rhyme label="a" type="m">vernam</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Verschrickt' hy met <rhyme label="b" type="f">woeden</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Hy was toornich ende <rhyme label="a" type="m" corresp="#s3:l1">gram</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">End liet die Kind'ren <rhyme label="b" type="f" corresp="#s3:l2">dooden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer dit Kint ons huyden <rhyme label="a" type="m">bly</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Maria deed' <rhyme label="b" type="f">baeren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dat door syn ghenade <rhyme label="a" type="m" corresp="#s4:l1">wy</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ten Hemel mochten <rhyme label="b" type="f" corresp="#s4:l2">varen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Daerom laet ons op dit <rhyme label="a" type="m">Feest</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Juychen ende <rhyme label="b" type="f">loven</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Met Loffsanghen <rhyme label="a" type="m" corresp="#s5:l1">aldermeest</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Den Heere van hier <rhyme label="b" type="f" corresp="#s5:l2">boven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
