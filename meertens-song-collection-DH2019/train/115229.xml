  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MEt Juffer tot <rhyme label="a" type="m">gerijf</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Laet ons een <rhyme label="a" type="m" corresp="#s1:l1">Convertijf</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Aenrechten tot wat tijt <rhyme label="a" type="m" corresp="#s1:l1">verdrijf</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Alhier in dit <rhyme label="b" type="m">Salet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Speelen met het <rhyme label="b" type="m" corresp="#s1:l4">Raket</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Want ghy kont slaen den bal seer <rhyme label="b" type="m" corresp="#s1:l4">net</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wel aen, ick sal mijn <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als dan voor dese <rhyme label="a" type="m" corresp="#s2:l1">keer</rhyme></l>
        <l id="s2:l3" met="4" type="-+">mijn voege na uwe <rhyme label="a" type="m" corresp="#s2:l1">begeer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat gaet u voor; siet <rhyme label="b" type="m">toe</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ghy slaet niet reght, wel <rhyme label="b" type="m" corresp="#s2:l4">hoe</rhyme>?</l>
        <l id="s2:l6" met="4" type="-+">Ghy Speelt gelijck een domme <rhyme label="b" type="m" corresp="#s2:l4">Koe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Me Juffer ey? <rhyme label="a" type="m">beget</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghy slaet den bal seer <rhyme label="a" type="m" corresp="#s3:l1">net</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maer met u Reeden vry noch <rhyme label="a" type="m" corresp="#s3:l1">bet</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Ghy Kaest met Schempen <rhyme label="b" type="m">snel</rhyme></l>
        <l id="s3:l5" met="3" type="-+">De Jonckers toe soo <rhyme label="b" type="m" corresp="#s3:l4">Wel</rhyme>;</l>
        <l id="s3:l6" met="4" type="-+">Doch vaert maer voort, het wel gaen <rhyme label="b" type="m" corresp="#s3:l4">sel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick hou mijn Woorden <rhyme label="a" type="m">stant</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat menich Lossen <rhyme label="a" type="m" corresp="#s4:l1">quant</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Speelt de Verliefde tot sijn <rhyme label="a" type="m" corresp="#s4:l1">schant</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Door suchten, vleyen <rhyme label="b" type="m">braef</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Jae noemt Hem haere <rhyme label="b" type="m" corresp="#s4:l4">Slaef</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Door sijnen gladden Tongh seer <rhyme label="b" type="m" corresp="#s4:l4">gaef</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maer soo Ras hy van <rhyme label="a" type="m">daen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">De Juffer is <rhyme label="a" type="m" corresp="#s5:l1">gegaen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Voort Spreeckt hy weer een ander <rhyme label="a" type="m" corresp="#s5:l1">aen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Klaegt haer oock sijn <rhyme label="b" type="m">Gebreck</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En soo Rolt dese <rhyme label="b" type="m" corresp="#s5:l4">Geck</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Ghelijck de Bal op het <rhyme label="b" type="m" corresp="#s5:l4">Raket</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer Juffer met <rhyme label="a" type="m">verlof</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ghy maeckt het veel te <rhyme label="a" type="m" corresp="#s6:l1">Grof</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Ghy Kaets soo dat de bal mijn <rhyme label="a" type="m" corresp="#s6:l1">trof</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Het waer oock Jammer, <rhyme label="b" type="m">dat</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Al de Jonckmans <rhyme label="b" type="m" corresp="#s6:l4">beklat</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Waeren met ’t geen ghy hun <rhyme label="b" type="m" corresp="#s6:l4">bespat</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn heer tref u den <rhyme label="a" type="m">bal</rhyme>;</l>
        <l id="s7:l2" met="3" type="-+">Kaets hem Weerom; ick <rhyme label="a" type="m" corresp="#s7:l1">sal</rhyme></l>
        <l id="s7:l3" met="4" type="-+">In weerom Kaetsen blijven <rhyme label="a" type="m" corresp="#s7:l1">Pal</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Is u Neus schoon, ghy <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Dan van het wisse <rhyme label="b" type="m" corresp="#s7:l4">zy</rhyme>:</l>
        <l id="s7:l6" met="4" type="-+">Set u Voet aen het Stuck nu <rhyme label="b" type="m" corresp="#s7:l4">by</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Mijn Juffer, voor soo <rhyme label="a" type="m">Veel</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ghy dit toepast een <rhyme label="a" type="m" corresp="#s8:l1">deel</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Daer in hebt ghy ghelijck <rhyme label="a" type="m" corresp="#s8:l1">Geheel</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Maer Rekent ghy mijn <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Oock midden Onder <rhyme label="b" type="m" corresp="#s8:l4">haer</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Dat sou mijn Eer wat gaen te <rhyme label="b" type="m" corresp="#s8:l4">naer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wel of ick in dees <rhyme label="a" type="m">Treyn</rhyme></l>
        <l id="s9:l2" met="3" type="-+">U oock gong Stellen <rhyme label="a" type="m" corresp="#s9:l1">pleyn</rhyme>?</l>
        <l id="s9:l3" met="4" type="-+">Wie sou Contrary blijck’ aen <rhyme label="a" type="m" corresp="#s9:l1">mijn</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Gaen toonen? dat gy <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Waert van die geene <rhyme label="b" type="m" corresp="#s9:l4">siet</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Die maer ons tong schrapt, en dan <rhyme label="b" type="m" corresp="#s9:l4">vliet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dit quaet ick Nooyt en <rhyme label="a" type="m">docht</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Want ick Verfoey <rhyme label="a" type="m" corresp="#s10:l1">bedroch</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Daer door blijf ick u <rhyme label="a" type="m" corresp="#s10:l1">Borrigh-tocht</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">Maer of gy dit <rhyme label="b" type="m">Vergat</rhyme>?</l>
        <l id="s10:l5" met="3" type="-+">Wegh was mijn Borrigh <rhyme label="b" type="m" corresp="#s10:l4">plat</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Daerom vertrou ick niet soo <rhyme label="b" type="m" corresp="#s10:l4">Rat</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Veel Achterdocht is <rhyme label="a" type="m">quaet</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Licht geloven dat <rhyme label="a" type="m" corresp="#s11:l1">Schaet</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Goet ondersoeck is beste <rhyme label="a" type="m" corresp="#s11:l1">Raet</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Ick laet soo veel daer <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Mijn niet gelegen <rhyme label="b" type="m" corresp="#s11:l4">staen</rhyme>:</l>
        <l id="s11:l6" met="4" type="-+">Gy kaets al weer buyte de <rhyme label="b" type="m" corresp="#s11:l4">baen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Dit Kaetsen is <rhyme label="a" type="m">genoch</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ick Kaets sonder <rhyme label="a" type="m" corresp="#s12:l1">bedroch</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Het Kaetsen is u niet <rhyme label="a" type="m" corresp="#s12:l1">bedocht</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Ick Kaets met Errints <rhyme label="b" type="m">Lief</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Gaet daer men u <rhyme label="b" type="m" corresp="#s12:l4">Berieft</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Voor my het gants niet meer <rhyme label="b" type="m" corresp="#s12:l4">Belieft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
