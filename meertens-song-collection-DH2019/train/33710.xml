      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacob Rosant</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EEn Vrouw met Koorts <rhyme label="a" type="f">bevange</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Het Petrus Schoon-moÃªr <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hadt krank geleegen <rhyme label="a" type="f" corresp="#s1:l1">lange</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maer nu geneesden <rhyme label="b" type="m" corresp="#s1:l2">ras</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want Iesus quam <rhyme label="a" type="f">intreeden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met sijn Discipels <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die hebben hem <rhyme label="a" type="f" corresp="#s2:l1">gebeeden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">O Heer, geneest togh <rhyme label="b" type="m" corresp="#s2:l2">haer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En hy begost te <rhyme label="a" type="f">naken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En gingh dicht by haer <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Haer handt gingh hy <rhyme label="a" type="f" corresp="#s3:l1">aenraken</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">De Koorts is wegh <rhyme label="b" type="m" corresp="#s3:l2">gegaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dus is sy <rhyme label="a" type="f">op-gereesen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">En dienden met 'er <rhyme label="b" type="m">vlijt</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Want sy was heel <rhyme label="a" type="f" corresp="#s4:l1">geneesen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En was de Koortse <rhyme label="b" type="m" corresp="#s4:l2">quijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gesontheyt wilt ons <rhyme label="a" type="f">geeven</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Aen siele en aen <rhyme label="b" type="m">lijf</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Op dat wy al ons <rhyme label="a" type="f" corresp="#s5:l1">leeven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">O Heer, u dienen <rhyme label="b" type="m" corresp="#s5:l2">stijf</rhyme>.</l>
      </lg>
    </lg>
  </text>
