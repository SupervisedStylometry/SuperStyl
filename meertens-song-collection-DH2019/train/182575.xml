<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182575</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Al mijn geluk</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Al myn geluk en vreugd is nu <rhyme label="a" type="f">gebooren</rhyme> /</l>
        <l id="s1:l2" met="5" type="-+">Mijn Minnaar kan myn wederom <rhyme label="a" type="f" corresp="#s1:l1">bekooren</rhyme> /</l>
        <l id="s1:l3" met="2" type="-+">Myn waarde <rhyme label="b" type="m">vriend</rhyme> /</l>
        <l id="s1:l4" met="2" type="-+">Myn waarde <rhyme label="b" type="m" corresp="#s1:l3">vriend</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Laat myn zyn stem weer <rhyme label="a" type="f" corresp="#s1:l1">hooren</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">Ik zugt niet of ik <rhyme label="c" type="m">schrey</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">Maar ik ben verheugd en <rhyme label="c" type="m" corresp="#s1:l6">bly</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Toen ik was jonk liet ik myn minnaar <rhyme label="a" type="f">varen</rhyme> /</l>
        <l id="s2:l2" met="5" type="-+">Schoon hy myn mind / dat baard hem groot <rhyme label="a" type="f" corresp="#s2:l1">bezwaaren</rhyme> /</l>
        <l id="s2:l3" met="2" type="-+">Hy minde <rhyme label="b" type="m">myn</rhyme> /</l>
        <l id="s2:l4" met="2" type="-+">Hy minde <rhyme label="b" type="m" corresp="#s2:l3">myn</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Doch echter na twee <rhyme label="a" type="f" corresp="#s2:l1">jaaren</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">Toen kuste ik <rhyme label="c" type="m">verheugd</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Myn Minnaar vol van <rhyme label="c" type="m" corresp="#s2:l6">vreugd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Ik was verheugd toen hy was weer <rhyme label="a" type="f">gekoomen</rhyme> /</l>
        <l id="s3:l2" met="5" type="-+">Hy heeft terstond myn hart weer <rhyme label="a" type="f" corresp="#s3:l1">ingenoomen</rhyme> /</l>
        <l id="s3:l3" met="2" type="-+">Dat baard myn <rhyme label="b" type="m">vreugd</rhyme> /</l>
        <l id="s3:l4" met="2" type="-+">Dat baard myn <rhyme label="b" type="m" corresp="#s3:l3">vrengd</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Ik dagt toen om geen <rhyme label="a" type="f" corresp="#s3:l1">schroomen</rhyme> /</l>
        <l id="s3:l6" met="3" type="-+">Maar was verheugd van <rhyme label="c" type="m">zin</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">Betoonde hem myn <rhyme label="c" type="m" corresp="#s3:l6">min</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ik wist geen raad om hem weer te <rhyme label="a" type="f">beminnen</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Ik kwam by hem streelde ras zyn <rhyme label="a" type="f" corresp="#s4:l1">zinnen</rhyme> /</l>
        <l id="s4:l3" met="2" type="-+">Hy kuste <rhyme label="b" type="m">my</rhyme> /</l>
        <l id="s4:l4" met="2" type="-+">Hy kuste <rhyme label="b" type="m" corresp="#s4:l3">my</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">En voeld het vuur van <rhyme label="a" type="f" corresp="#s4:l1">binnen</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">Wyl hy kwam van de <rhyme label="c" type="m">Oost</rhyme> /</l>
        <l id="s4:l7" met="3" type="-+">En was zeer wel <rhyme label="c" type="m" corresp="#s4:l6">getroost</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Hij was de geen die nooit mijn eer zou <rhyme label="a" type="f">krenken</rhyme></l>
        <l id="s5:l2" met="5" type="-+">Maar zwoer dat hij altoos aan mijzou <rhyme label="a" type="f" corresp="#s5:l1">denke</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Als hij kwam <rhyme label="b" type="m">thuis</rhyme> /</l>
        <l id="s5:l4" met="2" type="-+">Als hij kwam <rhyme label="b" type="m" corresp="#s5:l3">thuis</rhyme> /</l>
        <l id="s5:l5" met="3" type="-+">Daarom zal ik hem <rhyme label="a" type="f" corresp="#s5:l1">schenken</rhyme> /</l>
        <l id="s5:l6" met="3" type="-+">Om zijn <rhyme label="c" type="m">standvastigheid</rhyme> /</l>
        <l id="s5:l7" met="3" type="-+">Mijn hart is steeds <rhyme label="c" type="m" corresp="#s5:l6">derbleid</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Geen broef elend zal nimmer mijn ziel <rhyme label="a" type="f">prangen</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Ik ben van hem nog van geen ander <rhyme label="a" type="f" corresp="#s6:l1">zwanger</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Ik leef <rhyme label="b" type="m">gerust</rhyme> /</l>
        <l id="s6:l4" met="2" type="-+">Ik leef <rhyme label="b" type="m" corresp="#s6:l3">gerust</rhyme> /</l>
        <l id="s6:l5" met="3" type="-+">En dat was mijn <rhyme label="a" type="f" corresp="#s6:l1">verlangen</rhyme> /</l>
        <l id="s6:l6" met="3" type="-+">Want smaad en groot <rhyme label="c" type="m">verdriet</rhyme> /</l>
        <l id="s6:l7" met="3" type="-+">Leid eerbaarheid toch <rhyme label="c" type="m" corresp="#s6:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Alschoon mij een ieder wil <rhyme label="a" type="f">veragten</rhyme> /</l>
        <l id="s7:l2" met="5" type="-+">Geen vreugd zo groot dat mij meer kan <rhyme label="a" type="f" corresp="#s7:l1">verzagten</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Als dat mijn <rhyme label="b" type="m">lief</rhyme> /</l>
        <l id="s7:l4" met="2" type="-+">Als dat mijn <rhyme label="b" type="m" corresp="#s7:l3">lief</rhyme> /</l>
        <l id="s7:l5" met="3" type="-+">Na mijn niet hoeft te <rhyme label="a" type="f" corresp="#s7:l1">wagten</rhyme> /</l>
        <l id="s7:l6" met="3" type="-+">Maar geeft mijn hart en <rhyme label="c" type="m">hand</rhyme> /</l>
        <l id="s7:l7" met="3" type="-+">Aan hem tot <rhyme label="c" type="m" corresp="#s7:l6">onderpand</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
