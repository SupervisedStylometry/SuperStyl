<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24504</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="mystiek lied" translation="mystical song">mystiek lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>O Venus' band o vurige brand</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Jhesus bant O vuerich <rhyme label="a" type="m">brant</rhyme></l>
        <l id="s1:l2" met="4" type="-+">hoe heeft v mynne in my <rhyme label="a" type="m" corresp="#s1:l1">bewant</rhyme></l>
        <l id="s1:l3" met="3" type="-+">myn hertken <rhyme label="b" type="f">onbedwongen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">dat doet v troestelic <rhyme label="a" type="m" corresp="#s1:l1">onderstant</rhyme></l>
        <l id="s1:l5" met="4" type="-+">wat lof wort v int hemelsche <rhyme label="a" type="m" corresp="#s1:l1">lant</rhyme></l>
        <l id="s1:l6" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s1:l3">gesongen</rhyme>
        </l>
        <l id="s1:l7" met="3" type="-+">al mitter engelscher <rhyme label="b" type="f" corresp="#s1:l3">tongen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O maget reyn o hemels <rhyme label="a" type="m">screyn</rhyme></l>
        <l id="s2:l2" met="4" type="-+">gy syt ons hape ons troest <rhyme label="a" type="m" corresp="#s2:l1">alleyn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">stady ons nv in <rhyme label="b" type="f">staden</rhyme></l>
        <l id="s2:l4" met="4" type="-+">gy syt den sunderen int <rhyme label="a" type="m" corresp="#s2:l1">gemeyn</rhyme></l>
        <l id="s2:l5" met="4" type="-+">ende een vol vat dat der is <rhyme label="a" type="m" corresp="#s2:l1">sarteyn</rhyme></l>
        <l id="s2:l6" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s2:l3">geladen</rhyme>
        </l>
        <l id="s2:l7" met="3" type="-+">mit godtelicker <rhyme label="b" type="f" corresp="#s2:l3">genaden</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">O reyne maget daert licht wt <rhyme label="a" type="m">daget</rhyme></l>
        <l id="s3:l2" met="4" type="-+">v groet oetmoet heeft god <rhyme label="a" type="m" corresp="#s3:l1">behaget</rhyme></l>
        <l id="s3:l3" met="3" type="-+">ende daer toe <rhyme label="b" type="f">wtuercaren</rhyme></l>
        <l id="s3:l4" met="4" type="-+">die jerste sunde heeft hi <rhyme label="a" type="m" corresp="#s3:l1">beclaget</rhyme></l>
        <l id="s3:l5" met="4" type="-+">ende waert van v jae <rhyme label="a" type="m" corresp="#s3:l1">onuersaget</rhyme></l>
        <l id="s3:l6" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s3:l3">gebaren</rhyme>
        </l>
        <l id="s3:l7" met="3" type="-+">hi ontsloet datter was <rhyme label="b" type="f" corresp="#s3:l3">verlaren</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O rosegart van god <rhyme label="a" type="m">bewaert</rhyme></l>
        <l id="s4:l2" met="4" type="-+">v gracie is <rhyme label="a" type="m" corresp="#s4:l1">geapenbaert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">in ons tot mengen <rhyme label="b" type="f">stonden</rhyme></l>
        <l id="s4:l4" met="4" type="-+">mer als die ziele ter pynen <rhyme label="a" type="m" corresp="#s4:l1">uaert</rhyme></l>
        <l id="s4:l5" met="4" type="-+">biddi daer voer sy wort <rhyme label="a" type="m" corresp="#s4:l1">gespaert</rhyme></l>
        <l id="s4:l6" met="1" type="-+">van <rhyme label="b" type="f" corresp="#s4:l3">sunden</rhyme></l>
        <l id="s4:l7" met="3" type="-+">des wil ic vry <rhyme label="b" type="f" corresp="#s4:l3">orconden</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O lieue here lof danck ende <rhyme label="a" type="m">eer</rhyme></l>
        <l id="s5:l2" met="4" type="-+">scryue ic tot <rhyme label="a" type="m" corresp="#s5:l1">v</rhyme></l>
        <l id="s5:l3" met="3" type="-+">mit groter begeerten wt <rhyme label="b" type="f">mynnen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">ic byn gewont here herde <rhyme label="a" type="m" corresp="#s5:l1">seer</rhyme></l>
        <l id="s5:l5" met="4" type="-+">mit groten sunden buten <rhyme label="a" type="m" corresp="#s5:l1">keer</rhyme></l>
        <l id="s5:l6" met="1" type="-+">van <rhyme label="b" type="f" corresp="#s5:l3">bynnen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">sent troest of wy <rhyme label="b" type="f" corresp="#s5:l3">ontsynnen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die viant fel hi is altyt <rhyme label="a" type="m">sneel</rhyme></l>
        <l id="s6:l2" met="4" type="-+">wy syn verloest dat weet hi <rhyme label="a" type="m" corresp="#s6:l1">wel</rhyme></l>
        <l id="s6:l3" met="3" type="-+">daer om lecht hi ons <rhyme label="b" type="f">lagen</rhyme></l>
        <l id="s6:l4" met="4" type="-+">hi smyt hi cloppet hi maket <rhyme label="a" type="m" corresp="#s6:l1">rebel</rhyme></l>
        <l id="s6:l5" met="4" type="-+">op dat hi ons mach in die <rhyme label="a" type="m" corresp="#s6:l1">helle</rhyme></l>
        <l id="s6:l6" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s6:l3">beknaghen</rhyme>
        </l>
        <l id="s6:l7" met="3" type="-+">ist vremde dat wy <rhyme label="b" type="f" corresp="#s6:l3">versaghen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hier om ist noet cleyn ende <rhyme label="a" type="m">groet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">mer dat wy dencken opten <rhyme label="a" type="m" corresp="#s7:l1">doet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">ende leren altyt <rhyme label="b" type="f">steruen</rhyme></l>
        <l id="s7:l4" met="4" type="-+">ten baet weer hals noch edel <rhyme label="a" type="m" corresp="#s7:l1">bloet</rhyme></l>
        <l id="s7:l5" met="4" type="-+">veer huys noch hof noch enich <rhyme label="a" type="m" corresp="#s7:l1">guet</rhyme></l>
        <l id="s7:l6" met="1" type="-+">of <rhyme label="b" type="f" corresp="#s7:l3">erue</rhyme></l>
        <l id="s7:l7" met="3" type="-+">wy moeten dan al <rhyme label="b" type="f" corresp="#s7:l3">steruen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Waer op segt my verlaety <rhyme label="a" type="m">di</rhyme></l>
        <l id="s8:l2" met="4" type="-+">geen scepen brieue wer olt noch <rhyme label="a" type="m" corresp="#s8:l1">nye</rhyme></l>
        <l id="s8:l3" met="3" type="-+">sy en moegen v dan niet <rhyme label="b" type="f">baten</rhyme></l>
        <l id="s8:l4" met="4" type="-+">daer om siet voer dat seg ic <rhyme label="a" type="m" corresp="#s8:l1">vry</rhyme></l>
        <l id="s8:l5" met="4" type="-+">v lyf ende v guet mer als een <rhyme label="a" type="m" corresp="#s8:l1">pry</rhyme></l>
        <l id="s8:l6" met="1" type="-+">te <rhyme label="b" type="f" corresp="#s8:l3">haten</rhyme></l>
        <l id="s8:l7" met="3" type="-+">gy moetet al hier <rhyme label="b" type="f" corresp="#s8:l3">laten</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dit ist slot van al dat god <rhyme label="a" type="m">beual</rhyme></l>
        <l id="s9:l2" met="4" type="-+">dient my oetmoedich sonder <rhyme label="a" type="m" corresp="#s9:l1">gal</rhyme></l>
        <l id="s9:l3" met="3" type="-+">staet vast in uwen <rhyme label="b" type="f">stryden</rhyme></l>
        <l id="s9:l4" met="4" type="-+">holt niet ant gras ende laet den <rhyme label="a" type="m" corresp="#s9:l1">val</rhyme></l>
        <l id="s9:l5" met="4" type="-+">mer doedy dat soe sy dy <rhyme label="a" type="m" corresp="#s9:l1">mael</rhyme></l>
        <l id="s9:l6" met="1" type="-+">int <rhyme label="b" type="f" corresp="#s9:l3">liden</rhyme></l>
        <l id="s9:l7" met="3" type="-+">stat vast in uwen <rhyme label="b" type="f" corresp="#s9:l3">stryden</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Hebt lief verstaen die v <rhyme label="a" type="m">versmaen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">recht als francyscus heeft <rhyme label="a" type="m" corresp="#s10:l1">gedaen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">hi is daer by <rhyme label="b" type="f">verheuen</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Soe moegdi mit hem sonder <rhyme label="a" type="m" corresp="#s10:l1">vaen</rhyme></l>
        <l id="s10:l5" met="4" type="-+">dat blyde woert van god <rhyme label="a" type="m" corresp="#s10:l1">ontfaen</rhyme></l>
        <l id="s10:l6" met="1" type="-+">
          <rhyme label="b" type="f" corresp="#s10:l3">bescreuen</rhyme>
        </l>
        <l id="s10:l7" met="3" type="-+">comt hier int ewige <rhyme label="b" type="f" corresp="#s10:l3">leuen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
