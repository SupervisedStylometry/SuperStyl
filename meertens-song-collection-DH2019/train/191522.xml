<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191522</id>
    <date>
      <year-start>1736</year-start>
      <year-end>1736</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La douce flamme de l'amour</melody>
  <author>Joannes de Lixbona</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Nacht veel klaerder als den <rhyme label="a" type="m">dagh</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Waer in een suyver Maeght <rhyme label="a" type="m" corresp="#s1:l1">gelagh</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Een Maeght waer door Godt door zijn <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Nu is gebooren lanck van te <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">O nacht <rhyme label="b" type="m" corresp="#s1:l3">verwacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Een Maeght Godt baert wort niet <rhyme label="a" type="m">ontgort</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">De duysternis verdreven <rhyme label="a" type="m" corresp="#s2:l1">wordt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Door licht dat ons door eygen <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Nu is geboren lanck van te <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">O licht <rhyme label="b" type="m" corresp="#s2:l3">verwacht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een poorte waer door noyt man en <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Geopent wierdt voor't soete <rhyme label="a" type="m" corresp="#s3:l1">Lam</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">t'Onser soeningh in desen <rhyme label="b" type="m">nacht</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Nu is geboren lanck van te <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">O Lam <rhyme label="b" type="m" corresp="#s3:l3">verwacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O oude Vaders u <rhyme label="a" type="m">verheught</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Want gy u al verheugen <rhyme label="a" type="m" corresp="#s4:l1">meught</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">U verlossingh in desen <rhyme label="b" type="m">nacht</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Nu is gebooren lanck van te <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">By u <rhyme label="b" type="m" corresp="#s4:l3">verwacht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O menschen wilt in desen <rhyme label="a" type="m">tijdt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Nu altemael ook zijn <rhyme label="a" type="m" corresp="#s5:l1">verblijdt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Uwen Salighmaker in desen <rhyme label="b" type="m">nacht</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Nu is gebooren lanck van te <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">O lanck <rhyme label="b" type="m" corresp="#s5:l3">verwacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
