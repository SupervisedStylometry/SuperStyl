    <melody>Psalm 068 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">RIist uyt den slaep, ghy s Heeren <rhyme label="a" type="m">knecht</rhyme></l>
        <l id="s1:l2" met="4" type="-+">U rekeninghe <rhyme label="a" type="m" corresp="#s1:l1">ouerleght</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Wilt neerstich <rhyme label="b" type="f">ouersommen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Of all' uwe saken staen <rhyme label="a" type="m" corresp="#s1:l1">recht</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Maer in u herte niet en <rhyme label="a" type="m" corresp="#s1:l1">seght</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn Heere vertreckt te <rhyme label="b" type="f" corresp="#s1:l3">commen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Wilt so niet commen, als <rhyme label="c" type="m">versteent</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Zijn comste alsment niet en <rhyme label="c" type="m" corresp="#s1:l7">meent</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Sal haestelijck <rhyme label="d" type="f">gheschieden</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Neemt waer den tijdt u hier <rhyme label="c" type="m" corresp="#s1:l7">verleent</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Op dat ghy namaels niet en <rhyme label="c" type="m" corresp="#s1:l7">weent</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Maer weerdich sijt t' <rhyme label="d" type="f" corresp="#s1:l9">ontvlieden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den Heere comt die eeuwich <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Daer 't al voren verschrickt en <rhyme label="a" type="m" corresp="#s2:l1">beeft</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Om te oordeelen <rhyme label="b" type="f">crachtich</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Elck sie dat hy goe reden <rhyme label="a" type="m" corresp="#s2:l1">gheeft</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Van elcken woorde dat hy <rhyme label="a" type="m" corresp="#s2:l1">heeft</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ghesproken <rhyme label="b" type="f" corresp="#s2:l3">onvoordachtich</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Bidt eendrachtich na zijn <rhyme label="c" type="m">vermaen</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Dat ghy dan voor hem meucht <rhyme label="c" type="m" corresp="#s2:l7">bestaen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Als hy elcken sal <rhyme label="d" type="f">loonen</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Nae dat hy sal hebben <rhyme label="c" type="m" corresp="#s2:l7">ghedaen</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Tsy goet oft quaet, ende niet <rhyme label="c" type="m" corresp="#s2:l7">aen</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Sien eenighe <rhyme label="d" type="f" corresp="#s2:l9">persoonen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Siet oft ghy hebt ghedaen <rhyme label="a" type="m">profijt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Met t' pont dijns Heeren nu ter <rhyme label="a" type="m" corresp="#s3:l1">tijdt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En of ghy als <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Vruchtbaer in Godts kennisse <rhyme label="a" type="m" corresp="#s3:l1">sijt</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En oft ghy hebt met al u <rhyme label="a" type="m" corresp="#s3:l1">vlijt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In 't Gheloof deucht <rhyme label="b" type="f" corresp="#s3:l3">bewesen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">In desen bescheydenheyt <rhyme label="c" type="m">vroet</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Maticheyt, en lijdtsaemheyt <rhyme label="c" type="m" corresp="#s3:l7">goet</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Godtsalicheyt <rhyme label="d" type="f">eersame</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">En broederlijcke liefde <rhyme label="c" type="m" corresp="#s3:l7">soet</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Met der ghemeener liefden <rhyme label="c" type="m" corresp="#s3:l7">gloet</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Overvloedich <rhyme label="d" type="f" corresp="#s3:l9">bequame</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ist so u werdt seer <rhyme label="a" type="m">autentijck</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Ontsloten <rhyme label="a" type="m" corresp="#s4:l1">overvloedelijck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Den inganck sonder <rhyme label="b" type="f">falen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Tot des Heeren schoon lustich <rhyme label="a" type="m" corresp="#s4:l1">Rijck</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Daer niet besmeurt met sondich <rhyme label="a" type="m" corresp="#s4:l1">slijck</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In comt, na Schrifts <rhyme label="b" type="f" corresp="#s4:l3">verhalen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">En wilt niet dwalen, Broeders, <rhyme label="c" type="m">want</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">In dat eeuwich beloofde <rhyme label="c" type="m" corresp="#s4:l7">Landt</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En comt gheen <rhyme label="d" type="f">onbesneden</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Welck besnyden in reyn <rhyme label="c" type="m" corresp="#s4:l7">verstandt</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Gheschiet door Godts Gheest sonder <rhyme label="c" type="m" corresp="#s4:l7">handt</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Aen d' inwendighe <rhyme label="d" type="f" corresp="#s4:l9">leden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Anders t' besnyden niet en <rhyme label="a" type="m">ghelt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Daer moet sijn (so de Schrift <rhyme label="a" type="m" corresp="#s5:l1">vertelt</rhyme>)</l>
        <l id="s5:l3" met="3" type="-+">Een nieuwe <rhyme label="b" type="f">Creatuere</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Dus strijdt dat ghy behoudt het <rhyme label="a" type="m" corresp="#s5:l1">velt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En t' Rijck in nemet met <rhyme label="a" type="m" corresp="#s5:l1">ghewelt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Als Christus u <rhyme label="b" type="f" corresp="#s5:l3">Figuere</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Zijn puere voetstappen <rhyme label="c" type="m">eerbaer</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Cruysliefdich ghesint volghet <rhyme label="c" type="m" corresp="#s5:l7">naer</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">In woorden en in <rhyme label="d" type="f">wercken</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Door veel verdriet en lyden <rhyme label="c" type="m" corresp="#s5:l7">swaer</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Gaetmen in 't Hemelrijcke <rhyme label="c" type="m" corresp="#s5:l7">claer</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">So wy aen Christum <rhyme label="d" type="f" corresp="#s5:l9">mercken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Princelijck Knecht weest wel <rhyme label="a" type="m">bedacht</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Houdt t' alder ure goede <rhyme label="a" type="m" corresp="#s6:l1">wacht</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Want uwen Heere <rhyme label="b" type="f">ydoone</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Sal comen met al zijn <rhyme label="a" type="m" corresp="#s6:l1">Heyrcracht</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Ghelijck eenen dief in der <rhyme label="a" type="m" corresp="#s6:l1">nacht</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Waeckt ghy, tot uwen <rhyme label="b" type="f" corresp="#s6:l3">loone</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">De Croone sal wesen u <rhyme label="c" type="m">deel</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Een onverganckelijck <rhyme label="c" type="m" corresp="#s6:l7">Juweel</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Dat eeuwich leven <rhyme label="d" type="f">jeuchdich</rhyme>,</l>
        <l id="s6:l10" met="4" type="-+">Volcomen blijtschap al <rhyme label="c" type="m" corresp="#s6:l7">gheheel</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Al in dat lustich schoon <rhyme label="c" type="m" corresp="#s6:l7">prieel</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Wert u ghegheven <rhyme label="d" type="f" corresp="#s6:l9">vreuchdich</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
