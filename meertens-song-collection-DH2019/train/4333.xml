    <tags/>
    <melody>Verweerloos geveld</melody>
  <author>Pieter Dubbels</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VErwereloos <rhyme label="a" type="m">gevelt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Door Cypriaas <rhyme label="a" type="m" corresp="#s1:l1">gewelt</rhyme>,</l>
        <l id="s1:l3" met="6" type="-+">Ontswachtel ik mijn ramp, op hope mijn <rhyme label="b" type="m">gesmeek</rhyme>,</l>
        <l id="s1:l4" met="6" type="-+">De diamante schellen van uw' zinnen <rhyme label="b" type="m" corresp="#s1:l3">week</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gelijk een <rhyme label="a" type="m">roze-stronk</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ontsteken door 't <rhyme label="a" type="m" corresp="#s2:l1">gelonk</rhyme></l>
        <l id="s2:l3" met="6" type="-+">Van Peans vuurige oogen, welkt, en slenst, en <rhyme label="b" type="m">dort</rhyme>,</l>
        <l id="s2:l4" met="6" type="-+">Ten zij haar droge kruin met daauw gekoestert <rhyme label="b" type="m" corresp="#s2:l3">wort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zo jeukt mijn <rhyme label="a" type="m">ingewant</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ontsteken door de <rhyme label="a" type="m" corresp="#s3:l1">brant</rhyme></l>
        <l id="s3:l3" met="6" type="-+">Van uwe starren, naar het delven van mijn <rhyme label="b" type="m">graft</rhyme>,</l>
        <l id="s3:l4" met="6" type="-+">Ten zij uw' weder-min mijn Minne-brant <rhyme label="b" type="m" corresp="#s3:l3">verzaft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
