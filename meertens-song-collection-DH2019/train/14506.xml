<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14506</id>
    <date>
      <year-start>1638</year-start>
      <year-end>1638</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Het vinnige stralen van de zon</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Aegidius Haeffacker</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">KOmt <rhyme label="a" type="m">alle-gaer</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Met stem, met <rhyme label="a" type="m" corresp="#s1:l1">snaer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Eendrachtelijcken <rhyme label="b" type="f">prijsen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">De reyne <rhyme label="c" type="m">Claer</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Het <rhyme label="c" type="m" corresp="#s1:l4">exemplaer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Der Maeghden van <rhyme label="b" type="f" corresp="#s1:l3">Assisen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Franciscus <rhyme label="a" type="m">had</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Het enghe <rhyme label="a" type="m" corresp="#s2:l1">padt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Noch nauwelijcks <rhyme label="b" type="f">begonnen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Of siet, de <rhyme label="c" type="m">drift</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Van sijn <rhyme label="c" type="m" corresp="#s2:l4">voor-schrift</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Heeft Claraes hert <rhyme label="b" type="f" corresp="#s2:l3">verwonnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Des wereldts <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Weeck voor de <rhyme label="a" type="m" corresp="#s3:l1">min</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van IESVS haer <rhyme label="b" type="f">Behoeder</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Die door sijn <rhyme label="c" type="m">Cruys</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Haer 't lieve <rhyme label="c" type="m" corresp="#s3:l4">huys</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dee laten van haer <rhyme label="b" type="f" corresp="#s3:l3">moeder</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">De eele <rhyme label="a" type="m">ziel</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Koos 't rouwe <rhyme label="a" type="m" corresp="#s4:l1">Wiel</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Voor 't ciersel der <rhyme label="b" type="f">paruycken</rhyme>;</l>
        <l id="s4:l4" met="2" type="-+">Om dat Godts <rhyme label="c" type="m">Soon</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Van haer een <rhyme label="c" type="m" corresp="#s4:l4">kroon</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Van doornen wou <rhyme label="b" type="f" corresp="#s4:l3">ghebruycken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">En hoe sou 'k <rhyme label="a" type="m">my</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Met goud of <rhyme label="a" type="m" corresp="#s5:l1">sy'</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Sprack Clara, durven <rhyme label="b" type="f">kleden</rhyme>;</l>
        <l id="s5:l4" met="2" type="-+">Terwijl mijn <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Hem hier wel <rhyme label="c" type="m" corresp="#s5:l4">eer</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met schaeps-grau heeft <rhyme label="b" type="f" corresp="#s5:l3">gheleden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Neen, 't lichaem <rhyme label="a" type="m">stugh</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Moet op den <rhyme label="a" type="m" corresp="#s6:l1">rugh</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Een kleed van haersnoer <rhyme label="b" type="f">draghen</rhyme>.</l>
        <l id="s6:l4" met="2" type="-+">Wat hoeft een <rhyme label="c" type="m">Maeghd</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Te zijn <rhyme label="c" type="m" corresp="#s6:l4">ghekraeght</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die Christo wil <rhyme label="b" type="f" corresp="#s6:l3">behaghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">O Maghet <rhyme label="a" type="m">Claer</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Van gaven <rhyme label="a" type="m" corresp="#s7:l1">swaer</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Door 't zaedt van dese <rhyme label="b" type="f">tuchten</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Hebt ghy <rhyme label="c" type="m">ghebaerdt</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Met Godt <rhyme label="c" type="m" corresp="#s7:l4">ghepaerdt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Een menighte van <rhyme label="b" type="f" corresp="#s7:l3">vruchten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Maer <rhyme label="a" type="m">onghevleescht</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">En na den <rhyme label="a" type="m" corresp="#s8:l1">gheest</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Die sonder eynd' van <rhyme label="b" type="f">tijden</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Met haer <rhyme label="c" type="m">ghetal</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Bevrijdt van <rhyme label="c" type="m" corresp="#s8:l4">val</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">U sullen doen <rhyme label="b" type="f" corresp="#s8:l3">verblijden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Och of ick <rhyme label="a" type="m">mee</rhyme></l>
        <l id="s9:l2" met="2" type="-+">In d' eyghe <rhyme label="a" type="m" corresp="#s9:l1">stee</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Uw blijschaps waer te <rhyme label="b" type="f">samen</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">En op de <rhyme label="c" type="m">stof</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Van 't <rhyme label="c" type="m" corresp="#s9:l4">Maeghden-lof</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Van ver moght segghen: <rhyme label="b" type="f" corresp="#s9:l3">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
