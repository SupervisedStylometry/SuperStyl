  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Doen laatst Klimeene dwalen <rhyme label="a" type="f">ging</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Door ’t ruijsend lommer <rhyme label="b" type="m">heen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Koos zy de diepste <rhyme label="a" type="f" corresp="#s1:l1">daalen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">En be-gaf zig daar <rhyme label="b" type="m" corresp="#s1:l2">al-leen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Om haar bedroefde <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s1:l6" met="3" type="-+">’t Ontlasten van een <rhyme label="c" type="m" corresp="#s1:l5">smert</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die een verbor-ge <rhyme label="d" type="m">Min</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Gevat op ziel en <rhyme label="d" type="m" corresp="#s1:l7">zin</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Daar teeder had <rhyme label="e" type="f">ontstoke</rhyme></l>
        <l id="s1:l10" met="4" type="+-">En nog vloog ter boezem <rhyme label="d" type="m" corresp="#s1:l7">in</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haar oog ontsprong door <rhyme label="a" type="f">tranen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">En zy loosten zugt op <rhyme label="b" type="m">zugt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Om dus haer hert te <rhyme label="a" type="f" corresp="#s2:l1">banen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Een ververssing door de <rhyme label="b" type="m" corresp="#s2:l2">lugt</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">‘Ag’, zeize, ‘ô minne en <rhyme label="c" type="m">Deugt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wat baard gy leed en <rhyme label="c" type="m" corresp="#s2:l5">vreugd</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Als gy een hert <rhyme label="d" type="m">regeerd</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En gants volstrekt <rhyme label="d" type="m" corresp="#s2:l7">verheerd</rhyme>!</l>
        <l id="s2:l9" met="3" type="-+">De min beloofd meêr <rhyme label="e" type="f">vreugde</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Maar de deugd ons meerder <rhyme label="d" type="m" corresp="#s2:l7">eerd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn kunne is meêr <rhyme label="a" type="f">genegen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Tot der deugden <rhyme label="b" type="m">strengigheên</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maar zugt tot vreugde en <rhyme label="a" type="f" corresp="#s3:l1">zeegen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Doen ons tot het minnen <rhyme label="b" type="m" corresp="#s3:l2">treên</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Terwijl me   uitwendig <rhyme label="c" type="m">schijnd</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Heel fier, men zelv <rhyme label="c" type="m" corresp="#s3:l5">verkwijnd</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Van Liefde, door de <rhyme label="d" type="m">kragt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Des Liefs, die ’t oog <rhyme label="d" type="m" corresp="#s3:l7">veragt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Ja, zelve pord' de <rhyme label="e" type="f">Minne</rhyme></l>
        <l id="s3:l10" met="4" type="+-">tot ons schaa een grooter <rhyme label="d" type="m" corresp="#s3:l7">magt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nu staan ik in twee <rhyme label="a" type="f">kwaaden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Wijl my mind een waarde <rhyme label="b" type="m">ziel</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">De deugd wil me ook <rhyme label="a" type="f" corresp="#s4:l1">verraden</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En zy dreijgd met storm mijn <rhyme label="b" type="m" corresp="#s4:l2">kiel</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Zy werpt het hert te <rhyme label="c" type="m">grond</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat zy eerzugtig <rhyme label="c" type="m" corresp="#s4:l5">vond</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En zegt my: ‘Stelt uw’ <rhyme label="d" type="m">eer</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In ’t Minnen van dien <rhyme label="d" type="m" corresp="#s4:l7">Heer</rhyme>;</l>
        <l id="s4:l9" met="3" type="-+">Gy zult geen deugd <rhyme label="e" type="f">verzaken</rhyme></l>
        <l id="s4:l10" met="4" type="+-">Maar bevestigen die <rhyme label="d" type="m" corresp="#s4:l7">meer</rhyme>.’</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hier ben ik <rhyme label="a" type="f">toegekomen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Na   mijn Lief een wijl <rhyme label="b" type="m">geleên</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Heeft alles <rhyme label="a" type="f" corresp="#s5:l1">ondernomen</rhyme></l>
        <l id="s5:l4" met="4" type="+-">En met Min en Deugd <rhyme label="b" type="m" corresp="#s5:l2">gestreên</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Ik smeet de reeden <rhyme label="c" type="m">neêr</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Als kragteloos <rhyme label="c" type="m" corresp="#s5:l5">geweer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Met welk mijn ziele <rhyme label="d" type="m">streed</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">Als min in ’t herte <rhyme label="d" type="m" corresp="#s5:l7">treed</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Is ’t schaad’lijk zig te <rhyme label="e" type="f">weeren</rhyme>:</l>
        <l id="s5:l10" met="4" type="+-">hy verwekt een eeuwig <rhyme label="d" type="m" corresp="#s5:l7">leed</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Na ik begon te <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Heeft die God   mijn smert <rhyme label="b" type="m">vermeerd</rhyme>’:</l>
        <l id="s6:l3" met="3" type="-+">Hy keerd op wil en <rhyme label="a" type="f" corresp="#s6:l1">zinnen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">’t Wapen dat my heeft <rhyme label="b" type="m" corresp="#s6:l2">verweerd</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En leerd my nu te <rhyme label="c" type="m">laat</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dat wijsheid niet en <rhyme label="c" type="m" corresp="#s6:l5">baat</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Maar eeuwig’lijk <rhyme label="d" type="m">verwerd</rhyme></l>
        <l id="s6:l8" met="3" type="-+">De rust van een <rhyme label="d" type="m" corresp="#s6:l7">hert</rhyme>.</l>
        <l id="s6:l9" met="3" type="-+">Want ’t zwijgen van de <rhyme label="e" type="f">Liefde</rhyme></l>
        <l id="s6:l10" met="4" type="+-">baard ons d’allergrootste <rhyme label="d" type="m" corresp="#s6:l7">smert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O Goôn, doed <rhyme label="a" type="f">zeegepraalen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Van mijn hert mijn lief met <rhyme label="b" type="m">vreugd</rhyme>!</l>
        <l id="s7:l3" met="3" type="-+">Doed my te zaam <rhyme label="a" type="f" corresp="#s7:l1">behalen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">’t Zoet der Liefde, glants der <rhyme label="b" type="m" corresp="#s7:l2">deugd</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Op dat de kragt der <rhyme label="c" type="m">Min</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Vervul mijn hert en <rhyme label="c" type="m" corresp="#s7:l5">zin</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Terwijl het schoon der <rhyme label="d" type="m">deugd</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Mijn jonge ziel <rhyme label="d" type="m" corresp="#s7:l7">verheugd</rhyme></l>
        <l id="s7:l9" met="3" type="-+">En my op aard doe <rhyme label="e" type="f">leven</rhyme></l>
        <l id="s7:l10" met="4" type="+-">in een onbedenkb’re <rhyme label="d" type="m" corresp="#s7:l7">vreugd</rhyme>!’</l>
      </lg>
    </lg>
  </text>
</song>
