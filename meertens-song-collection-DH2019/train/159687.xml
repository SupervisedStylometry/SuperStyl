    <melody>Ach mijn schone mijn beminde</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Laat zig vry gelukkig <rhyme label="a" type="f">roemen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">'t Dertele en pragtig <rhyme label="b" type="m">hof</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">'t Lust ons 't geurig veld te <rhyme label="a" type="f" corresp="#s1:l1">roemen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat ons altoos ryke <rhyme label="b" type="m" corresp="#s1:l2">stof</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Schaft tot blijdschap en <rhyme label="c" type="f">vermaken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Daar wy, vrolijk en <rhyme label="d" type="m">verheugd</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">In elkanders liefden <rhyme label="c" type="f" corresp="#s1:l5">blaken</rhyme></l>
        <l id="s1:l8" met="4" type="+-">En besteden onse <rhyme label="d" type="m" corresp="#s1:l6">Jeugd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Lust ons by de klare <rhyme label="a" type="f">beeken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Waarde Kloris, <rhyme label="b" type="m">onbeschroomd</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van de zoete min te <rhyme label="a" type="f" corresp="#s2:l1">spreken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Of by 't Jeugdige <rhyme label="b" type="m" corresp="#s2:l2">geboomt</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Na het soet gesang te <rhyme label="c" type="f">hooren</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van 't gevogelt dat, <rhyme label="d" type="m">verblijd</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Elk kan met haar stem <rhyme label="c" type="f" corresp="#s2:l5">bekoren</rhyme></l>
        <l id="s2:l8" met="4" type="+-">In de groene <rhyme label="d" type="m" corresp="#s2:l6">lentetijd</rhyme>;</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Of te wandelen in 't <rhyme label="a" type="f">lommer</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Van de telgen,   <rhyme label="b" type="m">ongeplaagd</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Van de nijd en sorg en <rhyme label="a" type="f" corresp="#s3:l1">kommer</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Die het hof steeds met sig <rhyme label="b" type="m" corresp="#s3:l2">draagt</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Hier kan ons de nyd niet <rhyme label="c" type="f">plagen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Niemand sal hier onse <rhyme label="d" type="m">staat</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Door afgunstigheid <rhyme label="c" type="f" corresp="#s3:l5">belagen</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Nog vervolgen met sijn <rhyme label="d" type="m" corresp="#s3:l6">haat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'t Lof der schaduwrijke <rhyme label="a" type="f">Elsen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Sal ons niet benyden, <rhyme label="b" type="m">als</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Wy elkanderen <rhyme label="a" type="f" corresp="#s4:l1">omhelsen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">En ik, om u schoone <rhyme label="b" type="m" corresp="#s4:l2">hals</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Vast gestrengeld, uwe <rhyme label="c" type="f">lippen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Met de mijne vurig <rhyme label="d" type="m">druk</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">En het stranden op die <rhyme label="c" type="f" corresp="#s4:l5">klippen</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Aanmerk als mijn hoog <rhyme label="d" type="m" corresp="#s4:l6">geluk</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Weg met hoofze <rhyme label="a" type="f">minnerijen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Vol van snood bedrog en <rhyme label="b" type="m">list</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Weg met strelen en met <rhyme label="a" type="f" corresp="#s5:l1">vlijen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Door de schijn slegts <rhyme label="b" type="m" corresp="#s5:l2">gevernist</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Wy, door suyver vuur <rhyme label="c" type="f">gedreven</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Kiesen in het geurig <rhyme label="d" type="m">veld</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Vry van Nyd en sorg te <rhyme label="c" type="f" corresp="#s5:l5">leven</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Die al om het hof <rhyme label="d" type="m" corresp="#s5:l6">verseld</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
