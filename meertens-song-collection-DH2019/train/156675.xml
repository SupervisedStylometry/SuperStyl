  <author>Marijn de Brauwer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GOdt die Heere <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s1:l2" met="3" type="+-">heeft den mensche de <rhyme label="b" type="m">keur</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Hier op Aerden <rhyme label="a" type="f" corresp="#s1:l1">gegeven</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Wilt ghy komen ten <rhyme label="a" type="f" corresp="#s1:l1">leven</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Ghy moet hier <rhyme label="a" type="f" corresp="#s1:l1">beneven</rhyme></l>
        <l id="s1:l6" met="3" type="-+">U schicken naer die <rhyme label="b" type="m" corresp="#s1:l2">Schrifteur</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy heeft u gestelt <rhyme label="a" type="f">vooren</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Vyer en water <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Wilt ghy ontvlieden Godts <rhyme label="a" type="f" corresp="#s2:l1">tooren</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Ghy moet sijn <rhyme label="a" type="f" corresp="#s2:l1">herbooren</rhyme></l>
        <l id="s2:l5" met="3" type="+-">En zijn stemme <rhyme label="a" type="f" corresp="#s2:l1">hooren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En volgen hem altijt <rhyme label="b" type="m" corresp="#s2:l2">naer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wilt ghy hier naer <rhyme label="a" type="f">geraken</rhyme></l>
        <l id="s3:l2" met="3" type="+-">By Christum in zijn <rhyme label="b" type="m">accoort</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Ghy moetet hier zoo <rhyme label="a" type="f" corresp="#s3:l1">maken</rhyme></l>
        <l id="s3:l4" met="3" type="+-">U selven <rhyme label="a" type="f" corresp="#s3:l1">versaken</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Met bidden en <rhyme label="a" type="f" corresp="#s3:l1">waken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dringen door d' enge <rhyme label="b" type="m" corresp="#s3:l2">Poort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick weet geen ander <rhyme label="a" type="f">paden</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Om komen in die <rhyme label="b" type="m">Stadt</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Laet u van Christum <rhyme label="a" type="f" corresp="#s4:l1">raden</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Hy wil u <rhyme label="a" type="f" corresp="#s4:l1">ontladen</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Van al u <rhyme label="a" type="f" corresp="#s4:l1">misdaden</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En geern vergeven <rhyme label="b" type="m" corresp="#s4:l2">dat</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wilt ghy Christum <rhyme label="a" type="f">behagen</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Om te wesen zynen <rhyme label="b" type="m">vrient</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Ghy moet sonder <rhyme label="a" type="f" corresp="#s5:l1">vertragen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Hier u Kruyse <rhyme label="a" type="f" corresp="#s5:l1">dragen</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Want hem naect veel <rhyme label="a" type="f" corresp="#s5:l1">slagen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die Godt hier niet en <rhyme label="b" type="m" corresp="#s5:l2">dient</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wilt ghy Godt vreesen <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Soo latet u ernstig <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Soo zult ghy komen ter <rhyme label="a" type="f" corresp="#s6:l1">eeren</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Hem naect goot <rhyme label="a" type="f" corresp="#s6:l1">verseeren</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Die den Heer <rhyme label="a" type="f" corresp="#s6:l1">temteeren</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Door ydel geveynsde <rhyme label="b" type="m" corresp="#s6:l2">schijn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Oft ghy meest al u <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s7:l2" met="3" type="+-">In wellusten hat <rhyme label="b" type="m">geleeft</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Dat sal u noch <rhyme label="a" type="f" corresp="#s7:l1">bezwaren</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Ghy moet hene <rhyme label="a" type="f" corresp="#s7:l1">varen</rhyme></l>
        <l id="s7:l5" met="3" type="+-">Voor hem <rhyme label="a" type="f" corresp="#s7:l1">openbaren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Die loon naer wercken <rhyme label="b" type="m" corresp="#s7:l2">geeft</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Prince en wilt niet <rhyme label="a" type="f">hincken</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Als eenen krupelen, <rhyme label="b" type="m">maer</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Doet een loop sonder <rhyme label="a" type="f" corresp="#s8:l1">mijncken</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Ick most u <rhyme label="a" type="f" corresp="#s8:l1">gedijncken</rhyme></l>
        <l id="s8:l5" met="3" type="+-">Een Liedeken <rhyme label="a" type="f" corresp="#s8:l1">schijncken</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Voor een zalich nieu <rhyme label="b" type="m" corresp="#s8:l2">Jaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
