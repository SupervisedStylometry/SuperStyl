<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181921</id>
    <date>
      <year-start>1782</year-start>
      <year-end>1782</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Vrienden luister naar mijn rede</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zou ik thans niet welkom <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Na de zege van ‘sLands <rhyme label="b" type="m">vloot</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">By Anna myn <rhyme label="a" type="f" corresp="#s1:l1">uitgeleezen</rhyme>?...</l>
        <l id="s1:l4" met="4" type="+-">Zagjes wat jy zoent me <rhyme label="b" type="m" corresp="#s1:l2">dood</rhyme>:...</l>
        <l id="s1:l5" met="4" type="+-">Ja Zoutman die brave <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Heeft met ons ‘s Lands roem <rhyme label="c" type="m" corresp="#s1:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">De eerste Slag strekt ons tot <rhyme label="a" type="f">glorie</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Englands hoogmoed is <rhyme label="b" type="m">gestuit</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Parker vlugt, Hoezee! <rhyme label="a" type="f" corresp="#s2:l1">Victorie</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">Al dat snoeven heeft nu <rhyme label="b" type="m" corresp="#s2:l2">uit</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Want Zoutman, die dappre <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Heeft ‘s Lands ouden roem <rhyme label="c" type="m" corresp="#s2:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zeeuw en Maas-man niet te <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Stelt de hoop op buit te <rhyme label="b" type="m">loor</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Scheepen, volk, geschut en <rhyme label="a" type="f" corresp="#s3:l1">winden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Parker had in alles <rhyme label="b" type="m" corresp="#s3:l2">voor</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Nogtans heeft Zoutman, die <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Met Gods hulp s’Lands eer <rhyme label="c" type="m" corresp="#s3:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ja schoon Rusland ons mogt <rhyme label="a" type="f">foppen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Om ‘t getalm, met Sweed en <rhyme label="b" type="m">Deen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Konden wy den vyand <rhyme label="a" type="f" corresp="#s4:l1">kloppen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">De eer blyft nu voor ons <rhyme label="b" type="m" corresp="#s4:l2">alleen</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Want Zoutman, die dappre <rhyme label="c" type="m">Held</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Heeft ‘s Lands ouden roem <rhyme label="c" type="m" corresp="#s4:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Rood of Rinsch kan thans niet <rhyme label="a" type="f">baaten</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Neen, die grootste <rhyme label="b" type="m">zegepraal</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Eischt Bourgonje op ‘t heil der <rhyme label="a" type="f" corresp="#s5:l1">Staaten</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En voor d’Opper <rhyme label="b" type="m" corresp="#s5:l2">Admiraal</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Want Zoutman, die dappre <rhyme label="c" type="m">Held</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Heeft ‘s Lands ouden roem <rhyme label="c" type="m" corresp="#s5:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zoutman moet vooral een <rhyme label="a" type="f">glaasje</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Niemand deed meer dienst aan ‘t <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">‘t Is geen schreeuwer maar een <rhyme label="a" type="f" corresp="#s6:l1">baasje</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Dat voelt England tot zyn <rhyme label="b" type="m" corresp="#s6:l2">schand</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Want die ouderwetsche <rhyme label="c" type="m">Held</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Heeft zyn overmacht <rhyme label="c" type="m" corresp="#s6:l5">geveld</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">‘k Heb condities by <rhyme label="a" type="f">dozynen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Maar een zeeman weet zyn <rhyme label="b" type="m">maat</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Dit is voor de <rhyme label="a" type="f" corresp="#s7:l1">Capiteinen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Voor die zuilen van den <rhyme label="b" type="m" corresp="#s7:l2">Staat</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Door wier hulp en moed de <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Zoutman heeft ‘s Lands roem <rhyme label="c" type="m" corresp="#s7:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Nu voor d’Amsterdamsche <rhyme label="a" type="f">Vaders</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Zonder wien ‘t niet was <rhyme label="b" type="m">geschied</rhyme>;</l>
        <l id="s8:l3" met="4" type="+-">Die zyn ‘s Princen beste <rhyme label="a" type="f" corresp="#s8:l1">raders</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Raad van vreemden deugd dog <rhyme label="b" type="m" corresp="#s8:l2">niet</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Door hun dingtaal is de <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Uitgezeilt, ‘s Lands roem <rhyme label="c" type="m" corresp="#s8:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Siet hoe trouw zy de <rhyme label="a" type="f">gekwesten</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">In het gasthuis <rhyme label="b" type="m">gadeslaan</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Alles geeven zy ten <rhyme label="a" type="f" corresp="#s9:l1">besten</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Wie word niet door liefde <rhyme label="b" type="m" corresp="#s9:l2">ontdaan</rhyme>?</l>
        <l id="s9:l5" met="4" type="+-">Sy met Zoutman, onzen <rhyme label="c" type="m">Held</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Hebben de eer van ‘t Land <rhyme label="c" type="m" corresp="#s9:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Hadden wy niet fel <rhyme label="a" type="f">gevogten</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Amsterdam was <rhyme label="b" type="m">onderdrukt</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Maar in spyt van die dat <rhyme label="a" type="f" corresp="#s10:l1">zogten</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Is die snoode zet <rhyme label="b" type="m" corresp="#s10:l2">mislukt</rhyme>:</l>
        <l id="s10:l5" met="4" type="+-">Want haar schat is door den <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Trouw in veiligheid <rhyme label="c" type="m" corresp="#s10:l5">gesteld</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Zend de Prins nu ryklyk <rhyme label="a" type="f">scheepen</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Onder Zoutman, word de <rhyme label="b" type="m">Brit</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Als hy staan durft, zoo <rhyme label="a" type="f" corresp="#s11:l1">geneepen</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Dat hy ons om vreede <rhyme label="b" type="m" corresp="#s11:l2">bidt</rhyme>:</l>
        <l id="s11:l5" met="4" type="+-">Want de naam zelfs van die <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s11:l6" met="4" type="+-">Strekt ten schrik voor zyn <rhyme label="c" type="m" corresp="#s11:l5">geweld</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">‘t Is de zee doch die voor <rhyme label="a" type="f">deezen</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">‘t Land tot grootheid heeft <rhyme label="b" type="m">gebragt</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Sonder Graaf of Prins te <rhyme label="a" type="f" corresp="#s12:l1">weezen</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Toont een Burger daar zyn <rhyme label="b" type="m" corresp="#s12:l2">kragt</rhyme>:</l>
        <l id="s12:l5" met="4" type="+-">‘t Blykt aan Zoutman, want die <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Heeft op zee ‘s Lands eer <rhyme label="c" type="m" corresp="#s12:l5">hersteld</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Ja de zee is recht myn <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Morgen trek ik weêr naar <rhyme label="b" type="m">boord</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">‘k Sal je een lekker afscheid <rhyme label="a" type="f" corresp="#s13:l1">geven</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Meid lief, help my schielyk <rhyme label="b" type="m" corresp="#s13:l2">voort</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">En wacht buit, zo onze <rhyme label="c" type="m">Held</rhyme></l>
        <l id="s13:l6" met="4" type="+-">Word aan ‘t hoofd der vloot <rhyme label="c" type="m" corresp="#s13:l5">gesteld</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
