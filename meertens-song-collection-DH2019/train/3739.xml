    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 136 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DANCKET Godt nu <rhyme label="a" type="m">opentlick</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hy is doch seer <rhyme label="a" type="m" corresp="#s1:l1">vriendelick</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s1:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Singht Godt, want hy is seer <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Wydt bouen d'afgoden <rhyme label="a" type="m" corresp="#s2:l1">dood'</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s2:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wilt onsen Godt <rhyme label="a" type="m">vereeren</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Hy is een Heer der <rhyme label="a" type="m" corresp="#s3:l1">Heeren</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s3:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Pryst hem die wonderen <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Doer sijn kracht in <rhyme label="a" type="m" corresp="#s4:l1">ouervloet</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s4:l3">eewicheit</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Die den hemel ghemaeckt <rhyme label="a" type="m">heeft</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">End een heerlick cieraet <rhyme label="a" type="m" corresp="#s5:l1">gheeft</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s5:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die op t' water d' ęerde <rhyme label="a" type="m">swaer</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Wydt wtghestreckt heeft <rhyme label="a" type="m" corresp="#s6:l1">voorwaer</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s6:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die cierden des hemels <rhyme label="a" type="m">throon</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Met lichten seer groot end <rhyme label="a" type="m" corresp="#s7:l1">schoon</rhyme>.</l>
        <l id="s7:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s7:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Die de Maen' end sterren <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Laett herschen des nachts <rhyme label="a" type="m" corresp="#s8:l1">eenpaer</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s8:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Die de Sonne laet <rhyme label="a" type="m">wtgaen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Om den dagh so voor te <rhyme label="a" type="m" corresp="#s9:l1">staen</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s9:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Die in AEgypten met <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">D'eerst gheboren' heeft <rhyme label="a" type="m" corresp="#s10:l1">ombrachtt</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s10:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Die sijn volck van <rhyme label="a" type="m">Israel</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Heeft verlost wt angst seer <rhyme label="a" type="m" corresp="#s11:l1">fel</rhyme>:</l>
        <l id="s11:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s11:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Doer sijn handt seer sterck met <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">End doer sijnen aerm vol <rhyme label="a" type="m" corresp="#s12:l1">kracht</rhyme>:</l>
        <l id="s12:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s12:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">End die de baeren seer <rhyme label="a" type="m">wreedt</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Des Męers deelde wydt end <rhyme label="a" type="m" corresp="#s13:l1">breedt</rhyme>:</l>
        <l id="s13:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s13:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Die Israel liet voort <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Drooghsvoets doer t'roode Męer <rhyme label="a" type="m" corresp="#s14:l1">gaen</rhyme>:</l>
        <l id="s14:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s14:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Die Pharao met sijn <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s15:l2" met="4" type="+-">In't Męer haest heeft <rhyme label="a" type="m" corresp="#s15:l1">omghebracht</rhyme>:</l>
        <l id="s15:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s15:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Die oock ghaf den volcke <rhyme label="a" type="m">sijn</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Dat gheleydt doer die <rhyme label="a" type="m" corresp="#s16:l1">woestyn'</rhyme>:</l>
        <l id="s16:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s16:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Die de Koninghen <rhyme label="a" type="m">rebel</rhyme>,</l>
        <l id="s17:l2" met="4" type="+-">Versloegh in sijn gramschap <rhyme label="a" type="m" corresp="#s17:l1">snel</rhyme>:</l>
        <l id="s17:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s17:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Die ruesen sterck <rhyme label="a" type="m">onbesorght</rhyme>,</l>
        <l id="s18:l2" met="4" type="+-">Heeft de Heer' selue <rhyme label="a" type="m" corresp="#s18:l1">verworght</rhyme>:</l>
        <l id="s18:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s18:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Syn handt sloegh den <rhyme label="a" type="m">Amorryt</rhyme></l>
        <l id="s19:l2" met="4" type="+-">Sihon tot niet in den <rhyme label="a" type="m" corresp="#s19:l1">strydt</rhyme>:</l>
        <l id="s19:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s19:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Og den Koningh tot <rhyme label="a" type="m">Basan</rhyme>,</l>
        <l id="s20:l2" met="4" type="+-">Bracht Godt oock also daer <rhyme label="a" type="m" corresp="#s20:l1">van</rhyme>:</l>
        <l id="s20:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s20:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s20:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Dit landt vol met <rhyme label="a" type="m">ouervloet</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">Ghaf hy t'volck tot een <rhyme label="a" type="m" corresp="#s21:l1">erfgoet</rhyme>:</l>
        <l id="s21:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s21:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s21:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Dat verkreegh hy in den <rhyme label="a" type="m">strydt</rhyme>,</l>
        <l id="s22:l2" met="4" type="+-">Den sijnen t' eewigher <rhyme label="a" type="m" corresp="#s22:l1">tydt</rhyme>:</l>
        <l id="s22:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s22:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s22:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Hoe wy meer waeren <rhyme label="a" type="m">beswaert</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Hoe ons meer hielp Godt <rhyme label="a" type="m" corresp="#s23:l1">vermaert</rhyme>:</l>
        <l id="s23:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s23:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s23:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Hy bracht ons wt den <rhyme label="a" type="m">handen</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">Aller onser <rhyme label="a" type="m" corresp="#s24:l1">vianden</rhyme>:</l>
        <l id="s24:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s24:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s24:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">Hy is die de spyse <rhyme label="a" type="m">gheeft</rhyme>,</l>
        <l id="s25:l2" met="4" type="+-">Allen wat ter węerelt <rhyme label="a" type="m" corresp="#s25:l1">leeft</rhyme>:</l>
        <l id="s25:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s25:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s25:l3">eewicheit</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">Loeft den naeme Godts <rhyme label="a" type="m">ghewis</rhyme>,</l>
        <l id="s26:l2" met="4" type="+-">Die een Heer des hemels <rhyme label="a" type="m" corresp="#s26:l1">is</rhyme>:</l>
        <l id="s26:l3" met="4" type="+-">Want sijn groote <rhyme label="b" type="m">goedicheit</rhyme>,</l>
        <l id="s26:l4" met="4" type="+-">Gheduert in der <rhyme label="b" type="m" corresp="#s26:l3">eewicheit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
