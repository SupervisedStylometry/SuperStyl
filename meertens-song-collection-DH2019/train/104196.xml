    <melody>Als Atalante doden maakte</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">AL eer de Zon zijn blinde <rhyme label="a" type="f">Stralen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Uyt het Oost liet <rhyme label="a" type="f" corresp="#s1:l1">dalen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vervoerde, ick mijn Graage <rhyme label="b" type="f">Schaapjes</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Langs het <rhyme label="c" type="m">Claver-gras</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Daar ick doe von, Myn <rhyme label="d" type="m">Levens-Son</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Gantsch eentig <rhyme label="a" type="f" corresp="#s1:l1">dwalen</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Myn bly gesigt, Verschoot voor't <rhyme label="e" type="m">licht</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Vryheyt smolt als <rhyme label="c" type="m" corresp="#s1:l4">Was</rhyme>.</l>
      </lg>
    </lg>
  </text>
