<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100100</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zal ik nog langer in hete tranen</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE Doot en Cupido gingh <rhyme label="a" type="f">rusten</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Te samen op een leger <rhyme label="b" type="m">stee</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Om haer doorslapen te <rhyme label="a" type="f" corresp="#s1:l1">verlusten</rhyme></l>
        <l id="s1:l4" met="4" type="-+">En vry te syn van moeytens <rhyme label="b" type="m" corresp="#s1:l2">Wee</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Dies yder <rhyme label="c" type="m">seegh</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">en soetjes <rhyme label="c" type="m" corresp="#s1:l5">neegh</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">Ter aerden neer, en leyden samen <rhyme label="d" type="m">al</rhyme></l>
        <l id="s1:l8" met="5" type="-+">Hen schichtens tuyghjen onder een <rhyme label="d" type="m" corresp="#s1:l7">getal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Als nu de Doot had rust <rhyme label="a" type="f">genooten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Onwetent nam Cupidoos <rhyme label="b" type="m">boogh</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Hy heeft de outheyt weer <rhyme label="a" type="f" corresp="#s2:l1">geschooten</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Dat haer de min int Herte <rhyme label="b" type="m" corresp="#s2:l2">vloogh</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">In plaets dat <rhyme label="c" type="m">haer</rhyme> //</l>
        <l id="s2:l6" met="2" type="-+">des Doots <rhyme label="c" type="m" corresp="#s2:l5">gevaer</rhyme></l>
        <l id="s2:l7" met="5" type="-+">'t Leven ontnam, en in het duyster <rhyme label="d" type="m">graf</rhyme></l>
        <l id="s2:l8" met="5" type="-+">Haer Ziele boerde vande leden <rhyme label="d" type="m" corresp="#s2:l7">af</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Soo sagmen Beste vaer vol <rhyme label="a" type="f">vreugden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En Besje met een bly <rhyme label="b" type="m">gesicht</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Als of sy tot het spel noch <rhyme label="a" type="f" corresp="#s3:l1">deugden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">O! Goon dit doet de <rhyme label="b" type="m" corresp="#s3:l2">Minne-schigt</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">De oude <rhyme label="c" type="m">lien</rhyme> //</l>
        <l id="s3:l6" met="2" type="-+">die gingen <rhyme label="c" type="m" corresp="#s3:l5">bien</rhyme>,</l>
        <l id="s3:l7" met="5" type="-+">Haer stijve leen, op Venus welig <rhyme label="d" type="m">Bet</rhyme>;</l>
        <l id="s3:l8" met="5" type="-+">En wouden oock aen Cipres <rhyme label="d" type="m" corresp="#s3:l7">geset</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Cupidoo, oock terstont <rhyme label="a" type="f">ontwaeckten</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Vloogh op, en nam des Doodes <rhyme label="b" type="m">schicht</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Tot dat hy inde locht <rhyme label="a" type="f" corresp="#s4:l1">geraeckte</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">En na gewoonte 't soete <rhyme label="b" type="m" corresp="#s4:l2">VVigt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">De jongheyt <rhyme label="c" type="m">schoot</rhyme> //</l>
        <l id="s4:l6" met="2" type="-+">maer laes 't was <rhyme label="c" type="m" corresp="#s4:l5">Doot</rhyme>;</l>
        <l id="s4:l7" met="5" type="-+">Geen vreugt de pijl haer herten zieltje <rhyme label="d" type="m">gaf</rhyme></l>
        <l id="s4:l8" met="5" type="-+">Maer na de doot, het diep en duyster <rhyme label="d" type="m" corresp="#s4:l7">graf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ach laes, hoe deerlijck sagmen <rhyme label="a" type="f">sterven</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En derelijck wert doe <rhyme label="b" type="m">vernielt</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">De Ieugt, die Garons boot most <rhyme label="a" type="f" corresp="#s5:l1">erven</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">En pijnelijck hen leen <rhyme label="b" type="m" corresp="#s5:l2">ontzielt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Een plaets van <rhyme label="c" type="m">vreugt</rhyme> //</l>
        <l id="s5:l6" met="2" type="-+">ist <rhyme label="c" type="m" corresp="#s5:l5">ongeneugt</rhyme>,</l>
        <l id="s5:l7" met="5" type="-+">Dat haer omvat, geene Nectar dan nu <rhyme label="d" type="m">ach</rhyme>!</l>
        <l id="s5:l8" met="5" type="-+">Men nu, van hare lipjes suygen <rhyme label="d" type="m" corresp="#s5:l7">mach</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Cupidoo houd u rust doch <rhyme label="a" type="f">eenich</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Verselt u nimmer met de <rhyme label="b" type="m">Doodt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Zijn hert is koudt en dapper <rhyme label="a" type="f" corresp="#s6:l1">steenigh</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Ey rust doch in u Moeders <rhyme label="b" type="m" corresp="#s6:l2">schoot</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Soos sal u <rhyme label="c" type="m">pijl</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">niet inde <rhyme label="c" type="m" corresp="#s6:l5">wijl</rhyme></l>
        <l id="s6:l7" met="5" type="-+">Wanneer ghy slaept, en soetjes soeckt u <rhyme label="d" type="m">rust</rhyme></l>
        <l id="s6:l8" met="5" type="-+">Van oude lieden noyt meer zijn <rhyme label="d" type="m" corresp="#s6:l7">gekust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
