    <melody>Psalm 065 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Men looft u Heer, met stemmen <rhyme label="a" type="f">reyne</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Tot Zyon <rhyme label="b" type="m">openbaer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">De beloofde gaven <rhyme label="a" type="f" corresp="#s1:l1">ghemeyne</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Betaelt men u oock <rhyme label="b" type="m" corresp="#s1:l2">daer</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">End’ dewijl dat ghy daer wilt <rhyme label="c" type="f">hooren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">’t Ghebedt uwes volcks <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">So sullen daer u <rhyme label="c" type="f" corresp="#s1:l5">uyt-verkoren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">U vallen Heer te <rhyme label="d" type="m" corresp="#s1:l6">voet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al mijn misdaden end’ mijn <rhyme label="a" type="f">zonden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Waren seer groot en <rhyme label="b" type="m">swaer</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maer u goetheyt niet om <rhyme label="a" type="f" corresp="#s2:l1">doorgronden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Vergaf die al <rhyme label="b" type="m" corresp="#s2:l2">voorwaer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Wel hem dien ghy hebt <rhyme label="c" type="f">uyt-ghelesen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat hy by u zy, <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Die oock altijt by u mach <rhyme label="c" type="f" corresp="#s2:l5">wesen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Sonder te scheyden <rhyme label="d" type="m" corresp="#s2:l6">meer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy sullen met d’heerlijckheyt <rhyme label="a" type="f">schoone</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Uwes huys versaet <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">‘t Rijckdom, Heer, van ‘t huys uwer <rhyme label="a" type="f" corresp="#s3:l1">woone</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sal ons wel voeden <rhyme label="b" type="m" corresp="#s3:l2">fijn</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Ghy, die nae u goetheyt <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ons beschermt end’ <rhyme label="d" type="m">bewaert</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Sult ons een goet antwoorde <rhyme label="c" type="f" corresp="#s3:l5">geven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Nae u kracht seer <rhyme label="d" type="m" corresp="#s3:l6">vermaert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">‘t Geheel aertrijckl aen u, Heer <rhyme label="a" type="f">hanget</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met een vertrouwen <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Al die dat groote Meyr <rhyme label="a" type="f" corresp="#s4:l1">om-vanghet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Staen vast op u <rhyme label="b" type="m" corresp="#s4:l2">alleyn</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ghy maeckt door u schrick’lijcke <rhyme label="c" type="f">krachten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Seer vast de Bergen <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Van sterckheyt end’ van groote <rhyme label="c" type="f" corresp="#s4:l5">machten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Zijt ghy, o Heer niet <rhyme label="d" type="m" corresp="#s4:l6">bloot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy kondt oock dat brullen haest <rhyme label="a" type="f">stillen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Des Meyrs, alst is <rhyme label="b" type="m">beweeght</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">De menschen oock die woeden <rhyme label="a" type="f" corresp="#s5:l1">willen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ghy haest te stillen <rhyme label="b" type="m" corresp="#s5:l2">pleegt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Als de volckeren wel <rhyme label="c" type="f">bemercken</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die om her zijn o <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Dees groote wonderlijcke <rhyme label="c" type="f" corresp="#s5:l5">wercken</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Sy zijn verwondert <rhyme label="d" type="m" corresp="#s5:l6">seer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Heer, van den Oosten totten <rhyme label="a" type="f">Westen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Nae u goetheyt end’ <rhyme label="b" type="m">woordt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Laet ghy voortkomen ons ten <rhyme label="a" type="f" corresp="#s6:l1">besten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Allerley blijdtschap <rhyme label="b" type="m" corresp="#s6:l2">voort</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">So haest als droogh is ons <rhyme label="c" type="f">aerdtrijke</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ghy besoeckt dat <rhyme label="d" type="m">voorwaer</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Met Dauw end’ Reghen <rhyme label="c" type="f" corresp="#s6:l5">desghelijcke</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Maeckt gy ‘t rijck end’ <rhyme label="d" type="m" corresp="#s6:l6">vruchtbaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">‘t Water, springend’ uyt de <rhyme label="a" type="f">Fonteyne</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Maeckt dat aertrijcke <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Tot Tarw’ end’ ander vruchten <rhyme label="a" type="f" corresp="#s7:l1">reyne</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">In grooten <rhyme label="b" type="m" corresp="#s7:l2">over-vloet</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">De gheploeghde Voren <rhyme label="c" type="f">bequame</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Watert ghy t’sijner <rhyme label="d" type="m">tijdt</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Gy breekt d’aertrijck end’ sijn vreucht <rhyme label="c" type="f" corresp="#s7:l5">t’same</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Ghy seer <rhyme label="d" type="m" corresp="#s7:l6">ghebenedijdt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy verciert dat Iaer <rhyme label="a" type="f">overvloedigh</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Met u goederen <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">End’ laet ‘t’ vette regenen <rhyme label="a" type="f" corresp="#s8:l1">goedigh</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Boven uyt uwen <rhyme label="b" type="m" corresp="#s8:l2">throon</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">De Hutten selfs in de <rhyme label="c" type="f">Woestijne</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Zijn vol vruchten met <rhyme label="d" type="m">een</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">De Berghen seer vruchtbaer van <rhyme label="c" type="f" corresp="#s8:l5">Wijne</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Staen lustigh groot en <rhyme label="d" type="m" corresp="#s8:l6">kleen</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dan sietmen in Beemden en <rhyme label="a" type="f">Weyden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">’t Vee in groot ghetal <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">‘t Velt (van den Berghen <rhyme label="a" type="f" corresp="#s9:l1">af-ghescheyden</rhyme>)</l>
        <l id="s9:l4" met="3" type="-+">Siet men vol Koorne <rhyme label="b" type="m" corresp="#s9:l2">staen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Alsoo schijnt dat aertrijck <rhyme label="c" type="f">t’ontspringhen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">End’ te verblijden <rhyme label="d" type="m">seer</rhyme>:</l>
        <l id="s9:l7" met="4" type="-+">Dies moeten wy juychen end’ <rhyme label="c" type="f" corresp="#s9:l5">singhen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">In u van herten <rhyme label="d" type="m" corresp="#s9:l6">Heer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
