  <author>Johan Deknatel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DAt de borsten der <rhyme label="a" type="f">genaaden</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Niet <rhyme label="a" type="f" corresp="#s1:l1">verzaaden</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Is onmooglyk, of het <rhyme label="b" type="m">kind</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Moet niet laag zich neder <rhyme label="c" type="f">buigen</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Om te <rhyme label="c" type="f" corresp="#s1:l4">zuigen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Wyl het dan geen voedsel <rhyme label="b" type="m" corresp="#s1:l3">vindt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Jesus Liefde moet dog <rhyme label="a" type="f">vlieten</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Moet zich <rhyme label="a" type="f" corresp="#s2:l1">gieten</rhyme></l>
        <l id="s2:l3" met="4" type="+-">In des armen Zondaars <rhyme label="b" type="m">hert</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Maar daar moet Geloove <rhyme label="c" type="f">weezen</rhyme>;</l>
        <l id="s2:l5" met="2" type="+-">'t Moet <rhyme label="c" type="f" corresp="#s2:l4">geneezen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Als 't Geloove zuigend <rhyme label="b" type="m" corresp="#s2:l3">werd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Blyft dan eene ziele <rhyme label="a" type="f">smachtig</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Is 't <rhyme label="a" type="f" corresp="#s3:l1">verdachtig</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat zy Jesus half <rhyme label="b" type="m">begeert</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">By 't Geloof in jammer <rhyme label="c" type="f">dryven</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Steken <rhyme label="c" type="f" corresp="#s3:l4">blyven</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Is een Straf door eigen <rhyme label="b" type="m" corresp="#s3:l3">sweerd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zoekt dan grondig 't geen u <rhyme label="a" type="f">hindert</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">'t Geen <rhyme label="a" type="f" corresp="#s4:l1">vermindert</rhyme></l>
        <l id="s4:l3" met="4" type="+-">In u zyn genaade <rhyme label="b" type="m">vloed</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Weg daarmeé, die zich <rhyme label="c" type="f">ontledigt</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Word <rhyme label="c" type="f" corresp="#s4:l4">bevredigt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En ontfangt een <rhyme label="b" type="m" corresp="#s4:l3">overvloed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Houw dan op, van alle <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Dus te <rhyme label="a" type="f" corresp="#s5:l1">klaagen</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Ik verlange met <rhyme label="b" type="m">verdriet</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Ik ben hongerig en <rhyme label="c" type="f">smagte</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Leg en <rhyme label="c" type="f" corresp="#s5:l4">wagte</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Voor de deur, en kryg dog <rhyme label="b" type="m" corresp="#s5:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zeg 't u liever zelfs tot <rhyme label="a" type="f">schande</rhyme>:</l>
        <l id="s6:l2" met="2" type="+-">Myne <rhyme label="a" type="f" corresp="#s6:l1">banden</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Kluist'ren heim'lyk my aan d' <rhyme label="b" type="m">aard</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">Daarom komt uit all myn <rhyme label="c" type="f">trachten</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Niets tot <rhyme label="c" type="f" corresp="#s6:l4">krachten</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Niet dat God zyn Gaaven <rhyme label="b" type="m" corresp="#s6:l3">spaart</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ziet Gy geest het, Wy <rhyme label="a" type="f">ontfangen</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Ons <rhyme label="a" type="f" corresp="#s7:l1">verlangen</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Naar uw heil verschuift gy <rhyme label="b" type="m">niet</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">'t Komt uw liefd' altoos <rhyme label="c" type="f">gelegen</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Dat Gy <rhyme label="c" type="f" corresp="#s7:l4">Zeegen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">In 't ontfankbaar herte <rhyme label="b" type="m" corresp="#s7:l3">giet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Laat ons van uw liefde <rhyme label="a" type="f">drinken</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Zo <rhyme label="a" type="f" corresp="#s8:l1">verzinken</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Wy in uw' erbarmings <rhyme label="b" type="m">zee</rhyme>;</l>
        <l id="s8:l4" met="4" type="+-">Gy ontfangt den Zeegen <rhyme label="c" type="f">weeder</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Dien Gy <rhyme label="c" type="f" corresp="#s8:l4">needer</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Stort, en deelt uw Leeden <rhyme label="b" type="m" corresp="#s8:l3">meê</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Eene ziel, die Jesus <rhyme label="a" type="f">schatten</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Waarlyk <rhyme label="a" type="f" corresp="#s9:l1">vatten</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En tot vastheid komen <rhyme label="b" type="m">wil</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Moet by d'overvloed der <rhyme label="c" type="f">Gaaven</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Die haar <rhyme label="c" type="f" corresp="#s9:l4">laaven</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Blyven need'rig, klein en <rhyme label="b" type="m" corresp="#s9:l3">stil</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">O zoo laat ons, zonder <rhyme label="a" type="f">schroomen</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-">Tot U <rhyme label="a" type="f" corresp="#s10:l1">koomen</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Heiland, die onze armoê <rhyme label="b" type="m">ziet</rhyme>.</l>
        <l id="s10:l4" met="4" type="+-">Laat ons zoeken, bidden, <rhyme label="c" type="f">kloppen</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">Zich <rhyme label="c" type="f" corresp="#s10:l4">verstoppen</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Konnen doch uw' ooren <rhyme label="b" type="m" corresp="#s10:l3">niet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
