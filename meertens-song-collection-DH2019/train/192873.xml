<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>192873</id>
    <date>
      <year-start>1850</year-start>
      <year-end>1850</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Welkom welkom vaderland</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Aan de meisjes Van ons <rhyme label="a" type="m">land</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wil ik met 't glaasje in de <rhyme label="a" type="m" corresp="#s1:l1">hand</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Welgemeend een heildronk <rhyme label="b" type="f">wijden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Zij zijn 't, die ons hart <rhyme label="b" type="f" corresp="#s1:l3">verblijden</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Vrienden stoot eens met mij <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat 't haar immer goed moog <rhyme label="c" type="m" corresp="#s1:l5">gaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Blonde wimpers on blond <rhyme label="a" type="m">haar</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En een zacht blauw <rhyme label="a" type="m" corresp="#s2:l1">oogenpaar</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Roode blosje s op de <rhyme label="b" type="f">kaken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Lipjes, die bij 't kussen <rhyme label="b" type="f" corresp="#s2:l3">smaken</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Zoo vindt men ze in elken <rhyme label="c" type="m">stand</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Hier bij ons in <rhyme label="c" type="m" corresp="#s2:l5">Nederland</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Eerzaam zijn zij, braaf en <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Zacht en schuldloos haar <rhyme label="a" type="m" corresp="#s3:l1">gemoed</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Rein van zeden, rein van <rhyme label="b" type="f">handel</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Rein van taal en rein van <rhyme label="b" type="f" corresp="#s3:l3">wandel</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Minnen zij in eer en <rhyme label="c" type="m">deugd</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Schuldeloos vermaak en <rhyme label="c" type="m" corresp="#s3:l5">vreugd</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Bovenal een hartelijk <rhyme label="a" type="m">lied</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Haten onze schoonen <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'t Vroolijk lied streelt steeds haar <rhyme label="b" type="f">ooren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Wanklank kan haar niet <rhyme label="b" type="f" corresp="#s4:l3">bekoren</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Zuiver zij daarom het <rhyme label="c" type="m">lied</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat men onze schoonen <rhyme label="c" type="m" corresp="#s4:l5">bidet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Lieve meisjes, 't ga u <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Rein blijv' immer u <rhyme label="a" type="m" corresp="#s5:l1">gemoed</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Schoon prijkt 't blosje op uw <rhyme label="b" type="f">haken</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Immer moogt ge vreugde <rhyme label="b" type="f" corresp="#s5:l3">smaken</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">'t Ga u wel, smaak heil en <rhyme label="c" type="m">vree</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Ieder jongen zingt dan <rhyme label="c" type="m" corresp="#s5:l5">mee</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Jongens hebt ge nog geen <rhyme label="a" type="m">maagd</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Tot je vrouwtje hier <rhyme label="a" type="m" corresp="#s6:l1">gevraagd</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">'k Raad je aan het dan te <rhyme label="b" type="f">wagen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Om er spoedig een te <rhyme label="b" type="f" corresp="#s6:l3">vragen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Snoer je door den <rhyme label="c" type="m">huwlijksband</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Aan een meisje uit <rhyme label="c" type="m" corresp="#s6:l5">Nederland</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
