<song>
  <meta>
    <id>8023</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jan Jansz. Deutel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn ziel maeckt groot den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn geest verheught hem <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In mijnen Godt vol <rhyme label="b" type="f">trouwen</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Hy is mijn <rhyme label="c" type="m">saligheyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En wil oock de <rhyme label="c" type="m" corresp="#s1:l4">kleynheyt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Sijner dienstmaeght <rhyme label="b" type="f" corresp="#s1:l3">aenschouwen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Siet hierom sullen <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Alle gheslachten <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wel ghelucksaligh <rhyme label="b" type="f">achten</rhyme>?</l>
        <l id="s2:l4" met="3" type="-+">Want onse God seer <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Groote dinghen nu <rhyme label="c" type="m" corresp="#s2:l4">doet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Door zijn handt sterck in <rhyme label="b" type="f" corresp="#s2:l3">crachten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Heyligh is sijnen <rhyme label="a" type="m">naem</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En zijn goedtheyt <rhyme label="a" type="m" corresp="#s3:l1">bequaem</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Sal eeuwighlijck <rhyme label="b" type="f">beclijven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van kints kind'ren <rhyme label="c" type="m">voortaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Voor hen die recht wel <rhyme label="c" type="m" corresp="#s3:l4">gaen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">End' in Gods vreese <rhyme label="b" type="f" corresp="#s3:l3">blijven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een schoon end' heerlijck <rhyme label="a" type="m">werck</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Door zijnen arm seer <rhyme label="a" type="m" corresp="#s4:l1">sterck</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Heeft ghedaen Godt <rhyme label="b" type="f">almachtigh</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Hy heeft de stoute <rhyme label="c" type="m">quaedt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">End' hares herten <rhyme label="c" type="m" corresp="#s4:l4">raet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Tot niet gemaeckt seer <rhyme label="b" type="f" corresp="#s4:l3">crachtigh</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die stout zijn in <rhyme label="a" type="m">hooghmoet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Vol van eer en van <rhyme label="a" type="m" corresp="#s5:l1">goet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Heeft God neder <rhyme label="b" type="f">gedreven</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">En die arm zijn en <rhyme label="c" type="m">cleyn</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Heeft zijn goedtheyt <rhyme label="c" type="m" corresp="#s5:l4">alleyn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Seer heerlijck nu <rhyme label="b" type="f" corresp="#s5:l3">verheven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Die arm zijn na den <rhyme label="a" type="m">Gheest</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Den welcken hongert <rhyme label="a" type="m" corresp="#s6:l1">meest</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Versaet de Heer <rhyme label="b" type="f">gepresen</rhyme>.</l>
        <l id="s6:l4" met="3" type="-+">Die rijck zijn vol en <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Heeft hy ledigh en <rhyme label="c" type="m" corresp="#s6:l4">bloot</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van hem vry <rhyme label="b" type="f" corresp="#s6:l3">afghewesen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hy verheft <rhyme label="a" type="m">Israel</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Sijn Soon, en gedenckt <rhyme label="a" type="m" corresp="#s7:l1">wel</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Aen zijn groote <rhyme label="b" type="f">genade</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Soo hy heeft <rhyme label="c" type="m">Abraham</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En 'tvolck dat na hem <rhyme label="c" type="m" corresp="#s7:l4">quam</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Toegeseyt vroegh end' <rhyme label="b" type="f" corresp="#s7:l3">spade</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
