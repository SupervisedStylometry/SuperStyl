  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Siele loeft den Heere <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ick wil louen den <rhyme label="b" type="f">Heeren</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Ick wil soo lang als ick leue <rhyme label="a" type="m" corresp="#s1:l1">certeyn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Lofsinghen Godt ter <rhyme label="b" type="f" corresp="#s1:l2">eeren</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">En verlaet u op de Princen <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Noch op smenschen kindren die doch <rhyme label="c" type="m" corresp="#s1:l5">yet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">U gheensins en connen <rhyme label="d" type="f">ghehelpen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want smenschen gheest van hier wech <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En hy moet wedrom <rhyme label="b" type="f">wtuaren</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">En moet wederom worden tot eerde <rhyme label="a" type="m" corresp="#s2:l1">goet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Al zijn aenslaghen zijn dan <rhyme label="b" type="f" corresp="#s2:l2">verloren</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Salich is diens hulp Godt Jacobs <rhyme label="c" type="m">is</rhyme>:</l>
        <l id="s2:l6" met="4" type="-+">Salich is hy, diens hope <rhyme label="c" type="m" corresp="#s2:l5">ghewis</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Staet op Godt zijnen <rhyme label="d" type="f">Heere</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daertrijck, de Zee, en den <rhyme label="a" type="m">Hemel</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En al wat daer is <rhyme label="b" type="f">inne</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Dat heeft hy al ghemaeckt soo <rhyme label="a" type="m" corresp="#s3:l1">wel</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Oock sidt hy bouen <rhyme label="b" type="f" corresp="#s3:l2">Seraphinnen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">En hy hout ghelooue <rhyme label="c" type="m">eewelijck</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Oock doet hy recht seer <rhyme label="c" type="m" corresp="#s3:l5">wonderbaerlijck</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Den ghenen die onrecht <rhyme label="d" type="f">lijden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den hongherighen hy oock het broot <rhyme label="a" type="m">geeft</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De Heer verlost die <rhyme label="b" type="f">gheuanghen</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">De Heer den blinden tghesichte <rhyme label="a" type="m" corresp="#s4:l1">gheeft</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die om te sien hun <rhyme label="b" type="f" corresp="#s4:l2">verlanghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En die oock nedergheslaghen <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Die recht de Heer op wel so <rhyme label="c" type="m" corresp="#s4:l5">fijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">De rechtuaerdghe bemindt de <rhyme label="d" type="f">Heere</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">De Heer de vremdelinghen <rhyme label="a" type="m">bewaert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hy beschudt de Weesen te <rhyme label="b" type="f">deghe</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">En hy onderhoudt de Weedwen <rhyme label="a" type="m" corresp="#s5:l1">beswaert</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En keert te rug sGodloosen <rhyme label="b" type="f" corresp="#s5:l2">weghen</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">De Heer is coninck <rhyme label="c" type="m">eewelijck</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Godt, te Zyon daer duert u <rhyme label="c" type="m" corresp="#s5:l5">rijck</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Voort en voorts, <rhyme label="d" type="f">Alleluya</rhyme>.</l>
      </lg>
    </lg>
  </text>
