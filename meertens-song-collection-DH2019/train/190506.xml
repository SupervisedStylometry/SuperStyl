<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190506</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Un matin brusquement</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Terwyl 't nu is <rhyme label="a" type="m">geschied</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat 't Vrycorps is te <rhyme label="a" type="m" corresp="#s1:l1">niet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Door tyds <rhyme label="b" type="f">omwentelingen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Terwyl 't nu is <rhyme label="a" type="m" corresp="#s1:l1">geschied</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Zoo zullen wy te <rhyme label="c" type="m">zaam</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van vreugd Victorie <rhyme label="b" type="f" corresp="#s1:l3">zingen</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s1:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s1:l7">zy</rhyme>...</l>
        <l id="s1:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s1:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s1:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Prins van ons <rhyme label="a" type="m">bemint</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zyn haaters nu <rhyme label="a" type="m" corresp="#s2:l1">verwint</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Laat ons van blydschap <rhyme label="b" type="f">springen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy is ons beste <rhyme label="a" type="m" corresp="#s2:l1">vrind</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Nu zullen wy te <rhyme label="c" type="m">zaam</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van vreugd victorie <rhyme label="b" type="f" corresp="#s2:l3">zingen</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s2:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s2:l7">zy</rhyme>...</l>
        <l id="s2:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s2:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s2:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Fyne <rhyme label="a" type="m">kuypery</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die veelen bragt in <rhyme label="a" type="m" corresp="#s3:l1">Ley</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Door Armiaansche <rhyme label="b" type="f">benden</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Die syne <rhyme label="a" type="m" corresp="#s3:l1">kuypery</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Van 't Moordrot is <rhyme label="c" type="m">ondekt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En ingetrapt de <rhyme label="b" type="f" corresp="#s3:l3">lenden</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s3:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s3:l7">zy</rhyme>...</l>
        <l id="s3:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s3:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s3:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nu zal geen <rhyme label="a" type="m">Gyzelaar</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Het land meer in <rhyme label="a" type="m" corresp="#s4:l1">gevaar</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Of tweespalt kunnen <rhyme label="b" type="f">brengen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Nu zal men <rhyme label="a" type="m" corresp="#s4:l1">Gyzelaar</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zoo listig, als <rhyme label="c" type="m">Brutaal</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Nooit in den Raad <rhyme label="b" type="f" corresp="#s4:l3">gehengen</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s4:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s4:l7">zy</rhyme>...</l>
        <l id="s4:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s4:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s4:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laat die Aap van de <rhyme label="a" type="m">Wit</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Aan 't Y, daar hy nu <rhyme label="a" type="m" corresp="#s5:l1">zit</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zyn troubel bier maar <rhyme label="b" type="f">brouwen</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Het zal dat snoode <rhyme label="a" type="m" corresp="#s5:l1">Lit</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Schoon t' Amsterdam <rhyme label="c" type="m">gevlucht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hier na wel Eeuwig <rhyme label="b" type="f" corresp="#s5:l3">rouwen</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s5:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s5:l7">zy</rhyme>...</l>
        <l id="s5:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s5:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s5:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Een Bleiswyk zal nu <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Tot yder eens <rhyme label="a" type="m" corresp="#s6:l1">verdriet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Zyn loozen rol meer <rhyme label="b" type="f">speelen</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Een Bleiswyk zal nu <rhyme label="a" type="m" corresp="#s6:l1">niet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Wiens snoodheid is <rhyme label="c" type="m">ontdekt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Sireenen zang-meer <rhyme label="b" type="f" corresp="#s6:l3">kweelen</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s6:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s6:l7">zy</rhyme>...</l>
        <l id="s6:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s6:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s6:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Van Berkel zal niet <rhyme label="a" type="m">meer</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Met Visscher, naar <rhyme label="a" type="m" corresp="#s7:l1">begeer</rhyme></l>
        <l id="s7:l3" met="3" type="-+">'s Lands welvaart <rhyme label="b" type="f">ondervroeten</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Verbonden maaken <rhyme label="a" type="m" corresp="#s7:l1">weer</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Waar van de Staat niets <rhyme label="c" type="m">weet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Maar hunne snoodheid <rhyme label="b" type="f" corresp="#s7:l3">boeten</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s7:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s7:l7">zy</rhyme>...</l>
        <l id="s7:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s7:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s7:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Casteel en Zeeberg <rhyme label="a" type="m">zal</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Tot Haarlems <rhyme label="a" type="m" corresp="#s8:l1">ongeval</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Geen nieuwe wet meer <rhyme label="b" type="f">maaken</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Wy zien met bly <rhyme label="a" type="m" corresp="#s8:l1">geschal</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Hen van hun post <rhyme label="c" type="m">ontzet</rhyme>:</l>
        <l id="s8:l6" met="3" type="-+">Zy moesten 't alles <rhyme label="b" type="f" corresp="#s8:l3">staaken</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s8:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s8:l7">zy</rhyme>...</l>
        <l id="s8:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s8:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s8:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">'t Defensie-weezen, <rhyme label="a" type="m">dat</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Haar Hoogheid had <rhyme label="a" type="m" corresp="#s9:l1">gevat</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Moest meê zyn biezen <rhyme label="b" type="f">pakken</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">'t Verkwiste wel een <rhyme label="a" type="m" corresp="#s9:l1">schat</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Verhief zich boven <rhyme label="c" type="m">al'</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Maar liet den Moed ras <rhyme label="b" type="f" corresp="#s9:l3">zakken</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s9:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s9:l7">zy</rhyme>...</l>
        <l id="s9:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s9:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s9:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Een woeste <rhyme label="a" type="m">Legermacht</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Wie had het ooit <rhyme label="a" type="m" corresp="#s10:l1">gedacht</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Mogt strafloos Burgers <rhyme label="b" type="f">drukken</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">Nu is haar kragt <rhyme label="a" type="m" corresp="#s10:l1">verkragt</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Gesmolten en <rhyme label="c" type="m">gevlugt</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Betreuren zy die <rhyme label="b" type="f" corresp="#s10:l3">nukken</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s10:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s10:l7">zy</rhyme>...</l>
        <l id="s10:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s10:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s10:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Cartouches Bende <rhyme label="a" type="m">zal</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Vervult met bitt'ren <rhyme label="a" type="m" corresp="#s11:l1">gal</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Den Burger niet meer <rhyme label="b" type="f">plonderen</rhyme>.</l>
        <l id="s11:l4" met="3" type="-+">Cartouches Bende <rhyme label="a" type="m" corresp="#s11:l1">zal</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Geen vloeken op de <rhyme label="c" type="m">Prins</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Thans opentlyk meer <rhyme label="b" type="f" corresp="#s11:l3">donderen</rhyme>.</l>
        <l id="s11:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s11:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s11:l7">zy</rhyme>...</l>
        <l id="s11:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s11:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s11:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Een Schuurman en <rhyme label="a" type="m">Verlem</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Zyn deerlyk in de <rhyme label="a" type="m" corresp="#s12:l1">klem</rhyme>;</l>
        <l id="s12:l3" met="3" type="-+">Om hunne <rhyme label="b" type="f">schandgeschriften</rhyme>.</l>
        <l id="s12:l4" met="3" type="-+">Nu zal geen Jan <rhyme label="a" type="m" corresp="#s12:l1">Verlem</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Meer geeven ryke <rhyme label="c" type="m">stof</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Tot voedsel van de <rhyme label="b" type="f" corresp="#s12:l3">driften</rhyme>.</l>
        <l id="s12:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s12:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s12:l7">zy</rhyme>...</l>
        <l id="s12:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s12:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s12:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Costerus 't <rhyme label="a" type="m">galgeäas</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Nog Fynje maakt <rhyme label="a" type="m" corresp="#s13:l1">geraas</rhyme>.</l>
        <l id="s13:l3" met="3" type="-+">Zy moesten meê al <rhyme label="b" type="f">vluchten</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Met Post en Kruijers <rhyme label="a" type="m" corresp="#s13:l1">baas</rhyme>;</l>
        <l id="s13:l5" met="3" type="-+">En komen zy eens <rhyme label="c" type="m">weer</rhyme></l>
        <l id="s13:l6" met="3" type="-+">De galg staat hun te <rhyme label="b" type="f" corresp="#s13:l3">duchten</rhyme>.</l>
        <l id="s13:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s13:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s13:l7">zy</rhyme>...</l>
        <l id="s13:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s13:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s13:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Geen Klopper, <rhyme label="a" type="m">Enschedé</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Nog Utrechts Lasteraar <rhyme label="a" type="m" corresp="#s14:l1">meê</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Durft thans van Nero <rhyme label="b" type="f">kikken</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Geen Klopper, <rhyme label="a" type="m" corresp="#s14:l1">Enschedé</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Vloekt thans meer op den <rhyme label="c" type="m">Prins</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Maar beeft alle <rhyme label="b" type="f" corresp="#s14:l3">oogenblikken</rhyme>.</l>
        <l id="s14:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s14:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s14:l7">zy</rhyme>...</l>
        <l id="s14:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s14:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s14:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Geen Sluis en Delfsch' <rhyme label="a" type="m">Rot</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Dat Recht en Wet <rhyme label="a" type="m" corresp="#s15:l1">bespot</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Zal nu meer braaven <rhyme label="b" type="f">kwellen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Of dryven boos en <rhyme label="a" type="m" corresp="#s15:l1">zot</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Al het Oranje <rhyme label="c" type="m">Vee</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Voor in de spits te <rhyme label="b" type="f" corresp="#s15:l3">stellen</rhyme>.</l>
        <l id="s15:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s15:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s15:l7">zy</rhyme>...</l>
        <l id="s15:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s15:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s15:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Ja! GOD heeft ons <rhyme label="a" type="m">gered</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Door Pruissen perk <rhyme label="a" type="m" corresp="#s16:l1">gezet</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Aan heerschzucht, en doen <rhyme label="b" type="f">beeven</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Die 't moordzwaard had <rhyme label="a" type="m" corresp="#s16:l1">gewet</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Looft GOD! - En roept te <rhyme label="c" type="m">zaam</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Dat Pruissens Koning <rhyme label="b" type="f" corresp="#s16:l3">Leeve</rhyme>!</l>
        <l id="s16:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s16:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s16:l7">zy</rhyme>...</l>
        <l id="s16:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s16:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s16:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Lang leef ook, onze <rhyme label="a" type="m">Vorst</rhyme>!</l>
        <l id="s17:l2" met="3" type="-+">Die zo veel laster <rhyme label="a" type="m" corresp="#s17:l1">torscht</rhyme>;</l>
        <l id="s17:l3" met="3" type="-+">Met <rhyme label="b" type="f">zyne-Gemalinne</rhyme>!</l>
        <l id="s17:l4" met="3" type="-+">Elk roept uit volle <rhyme label="a" type="m" corresp="#s17:l1">borst</rhyme>.</l>
        <l id="s17:l5" met="3" type="-+">Lang leef hun dierbaar <rhyme label="c" type="m">Kroost</rhyme>!</l>
        <l id="s17:l6" met="3" type="-+">Dat elk hun teer <rhyme label="b" type="f" corresp="#s17:l3">beminne</rhyme>!</l>
        <l id="s17:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s17:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s17:l7">zy</rhyme>...</l>
        <l id="s17:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s17:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s17:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Lang leef Graaf <rhyme label="a" type="m">Bentinckook</rhyme>!</l>
        <l id="s18:l2" met="3" type="-+">Hy deed in damp en <rhyme label="a" type="m" corresp="#s18:l1">rook</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Het muiters plan <rhyme label="b" type="f">verdwynen</rhyme>.</l>
        <l id="s18:l4" met="3" type="-+">Lang leef Graaf Bentinck <rhyme label="a" type="m" corresp="#s18:l1">ook</rhyme>!</l>
        <l id="s18:l5" met="3" type="-+">Hy moog steeds als een <rhyme label="c" type="m">Zon</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Aan Neêrlands Hemel <rhyme label="b" type="f" corresp="#s18:l3">schynen</rhyme>!</l>
        <l id="s18:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s18:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s18:l7">zy</rhyme>...</l>
        <l id="s18:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s18:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s18:l7">wy</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Elk zing dan <rhyme label="a" type="m">overluit</rhyme>;</l>
        <l id="s19:l2" met="3" type="-+">Roept Nassouws glorie <rhyme label="a" type="m" corresp="#s19:l1">uit</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Laat ons dat Stamhuis <rhyme label="b" type="f">loven</rhyme>!</l>
        <l id="s19:l4" met="3" type="-+">Hier mee is 't Liedje <rhyme label="a" type="m" corresp="#s19:l1">uit</rhyme>.</l>
        <l id="s19:l5" met="3" type="-+">Koomt! - Drinken wy een <rhyme label="c" type="m">glas</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">En roept; ORANE <rhyme label="b" type="f" corresp="#s19:l3">BOOVEN</rhyme>.</l>
        <l id="s19:l7" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m">wy</rhyme>...</l>
        <l id="s19:l8" met="3" type="-+">Oranje op ons <rhyme label="d" type="m" corresp="#s19:l7">zy</rhyme>...</l>
        <l id="s19:l9" met="3" type="-+">De Barnevelders <rhyme label="e" type="f">beeven</rhyme>.</l>
        <l id="s19:l10" met="3" type="-+">Nu triumpheeren <rhyme label="d" type="m" corresp="#s19:l7">wy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
