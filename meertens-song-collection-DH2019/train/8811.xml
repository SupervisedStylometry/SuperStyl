    <melody>Es wollt uns Gott gen√§dig sein</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk wil Godt Loven alle <rhyme label="a" type="m">Tijdt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Syn Loff sal ick <rhyme label="b" type="f">belijden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Myn Siel sal roemen Godt so <rhyme label="a" type="m" corresp="#s1:l1">wijt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat d'armen haer <rhyme label="b" type="f" corresp="#s1:l2">verblijden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">So prijst dan nu den HEER met <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Verhoocht zijn Naem daer <rhyme label="d" type="f">neven</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Want doen ick Godt aensocht, heeft <rhyme label="c" type="m" corresp="#s1:l5">hy</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Goet Antwoort my <rhyme label="d" type="f" corresp="#s1:l6">ghegeven</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Verlost wt vrees mijn <rhyme label="d" type="f" corresp="#s1:l6">Leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want al die op den HEEre <rhyme label="a" type="m">sien</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dien coemt syn Troost ter <rhyme label="b" type="f">Handen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En t'Aensicht van alsulcke <rhyme label="a" type="m" corresp="#s2:l1">Lien</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Werdt nimmermeer te <rhyme label="b" type="f" corresp="#s2:l2">schanden</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Doen dees' Elende riep, heeft <rhyme label="c" type="m">Godt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Verhoort hem alsoo <rhyme label="d" type="f">haeste</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">End' holp hem wt al synen <rhyme label="c" type="m" corresp="#s2:l5">Noot</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Den Enghel Laghert <rhyme label="d" type="f" corresp="#s2:l6">vaste</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Rondtom des HEERen <rhyme label="d" type="f" corresp="#s2:l6">Gaste</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Siet, smaeckt, hoe vrientlijck is de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Salich die hem <rhyme label="b" type="f">betrouwen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ghy Heyligen Vreest hem al <rhyme label="a" type="m" corresp="#s3:l1">meer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ten mach u niet <rhyme label="b" type="f" corresp="#s3:l2">gherouwen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Die Rijcke moeten lijden <rhyme label="c" type="m">Noot</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al zijn sy stout van <rhyme label="d" type="f">Moede</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Maer wie den HEER aensoecken <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Heeft gheen ghebreck int <rhyme label="d" type="f" corresp="#s3:l6">Goede</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Blijft in des HEEren <rhyme label="d" type="f" corresp="#s3:l6">Hoede</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Coemt Kinder, hoort nae mijn <rhyme label="a" type="m">Vermaen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Des Heeren Vrees te <rhyme label="b" type="f">leeren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Wiens Wensch is, dat hem wel sou <rhyme label="a" type="m" corresp="#s4:l1">gaen</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">Is goede Daegh' <rhyme label="b" type="f" corresp="#s4:l2">begeeren</rhyme>?</l>
        <l id="s4:l5" met="4" type="-+">Dijn Tong' bewaer, voor t'geen is <rhyme label="c" type="m">quaet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dijn Mondt gheen Valsch en <rhyme label="d" type="f">waghe</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Doet Goetheyt, En die quaetheyt <rhyme label="c" type="m" corresp="#s4:l5">laet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Nae Vrede soeck end' <rhyme label="d" type="f" corresp="#s4:l6">Jaghe</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Dat brengt u goede <rhyme label="d" type="f" corresp="#s4:l6">Daghe</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Vriendelijck de HEEr syn Ooghen <rhyme label="a" type="m">slaet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Op der Gerechten <rhyme label="b" type="f">Wandel</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Haer G'bedt hy oock verhooren <rhyme label="a" type="m" corresp="#s5:l1">gaet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maer op der Boosen <rhyme label="b" type="f" corresp="#s5:l2">Handel</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Daer staet syn Aensicht <rhyme label="c" type="m">thoornichlijck</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Haer Ghedacht'nis te <rhyme label="d" type="f">ontleyen</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">De HEEr verhoort <rhyme label="c" type="m" corresp="#s5:l5">ghenadichlijck</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Als die Gerechte <rhyme label="d" type="f" corresp="#s5:l6">schreyen</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Gaet se recht wel <rhyme label="d" type="f" corresp="#s5:l6">bevrijen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De HEER is by den ghenen, <rhyme label="a" type="m">die</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ghebroken syn van <rhyme label="b" type="f">Herten</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Verslaghen int Ghemoedt zijn <rhyme label="a" type="m" corresp="#s6:l1">sy</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Hy helpt wt alle <rhyme label="b" type="f" corresp="#s6:l2">smerten</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Die Vroom' moet veel lijden op <rhyme label="c" type="m">Aerd'</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Godt helpt nochtans wt <rhyme label="d" type="f">alle</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Bewaert hem syn Gebeente <rhyme label="c" type="m" corresp="#s6:l5">waerd'</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dat dies niet een <rhyme label="d" type="f" corresp="#s6:l6">vervalle</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Noch Faelgier aent <rhyme label="d" type="f" corresp="#s6:l6">Ghetalle</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Den Godloosen sal Dooden <rhyme label="a" type="m">snel</rhyme></l>
        <l id="s7:l2" met="3" type="-+">d'Ong'luck van s'HEEren <rhyme label="b" type="f">Handen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">End' die den Vromen haten <rhyme label="a" type="m" corresp="#s7:l1">fel</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Sijn schuldich al met <rhyme label="b" type="f" corresp="#s7:l2">schanden</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">De HEEr verlost <rhyme label="c" type="m">ghenadenrijck</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die Ziel van zijnen <rhyme label="d" type="f">Knechten</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Die hem betrouwen all' <rhyme label="c" type="m" corresp="#s7:l5">ghelijck</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Gheen Schult machse <rhyme label="d" type="f" corresp="#s7:l6">Aenvechten</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Sijn vry vans' Doodes <rhyme label="d" type="f" corresp="#s7:l6">Rechten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
