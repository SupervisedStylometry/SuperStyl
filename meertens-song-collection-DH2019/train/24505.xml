<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24505</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Het gepeins ligt mij zeer vast en kwelt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ic heb die schoenste <rhyme label="a" type="f">wtuercaren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">oer liefte is vast in <rhyme label="b" type="m">stadicheit</rhyme></l>
        <l id="s1:l3" met="4" type="-+">hadde sy gedaen ic waer <rhyme label="a" type="f" corresp="#s1:l1">verlaren</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Verlaren oec inder <rhyme label="b" type="m" corresp="#s1:l2">ewicheit</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Maria du bist al die ic <rhyme label="c" type="m">meyn</rhyme></l>
        <l id="s1:l6" met="4" type="-+">bauen alle vrouwen schoen <rhyme label="c" type="m" corresp="#s1:l5">alleyn</rhyme></l>
        <l id="s1:l7" met="4" type="-+">och    laet my syn dyn troest <rhyme label="b" type="m" corresp="#s1:l2">bereyt</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Ick bidde di ic bidde <rhyme label="d" type="m">di</rhyme></l>
        <l id="s1:l9" met="3" type="-+">och staet my trouwelic <rhyme label="d" type="m" corresp="#s1:l8">by</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">God gruet v weerde maghet <rhyme label="a" type="f">reyne</rhyme></l>
        <l id="s2:l2" met="4" type="-+">ende moeder der <rhyme label="b" type="m">barmherticheit</rhyme></l>
        <l id="s2:l3" met="4" type="-+">ende der genaeden een ewige <rhyme label="a" type="f" corresp="#s2:l1">fonteyne</rhyme></l>
        <l id="s2:l4" met="4" type="-+">bewyset den sunders <rhyme label="b" type="m" corresp="#s2:l2">myldicheit</rhyme></l>
        <l id="s2:l5" met="4" type="-+">din macht is groet by god den <rhyme label="c" type="m">heren</rhyme></l>
        <l id="s2:l6" met="4" type="-+">seer gheerne volbrenget hi dine <rhyme label="c" type="m" corresp="#s2:l5">begeerte</rhyme></l>
        <l id="s2:l7" met="4" type="-+">syn moeder en mach hy weygeren <rhyme label="b" type="m" corresp="#s2:l2">niet</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Ick bidde di ic bidde <rhyme label="d" type="m">di</rhyme></l>
        <l id="s2:l9" met="3" type="-+">och staet my trouwelic <rhyme label="d" type="m" corresp="#s2:l8">by</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Der werlde vroude en mach niet <rhyme label="a" type="f">dueren</rhyme></l>
        <l id="s3:l2" met="4" type="-+">oer arch heeft mennich mensche <rhyme label="b" type="m">bedragen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">dat eynde der vrouden en is niet dan <rhyme label="a" type="f" corresp="#s3:l1">trueren</rhyme></l>
        <l id="s3:l4" met="4" type="-+">oer dienres heeft sy mannych <rhyme label="b" type="m" corresp="#s3:l2">gelage</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Maria gy syt die stedich <rhyme label="c" type="m">blyft</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Daer om keer ic toe di myn <rhyme label="c" type="m" corresp="#s3:l5">liefte</rhyme></l>
        <l id="s3:l7" met="4" type="-+">din dienre wyl ic geerne <rhyme label="b" type="m" corresp="#s3:l2">syn</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Ick bidde di ic bidde <rhyme label="d" type="m">di</rhyme></l>
        <l id="s3:l9" met="3" type="-+">och staet my trouwelic <rhyme label="d" type="m" corresp="#s3:l8">by</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dyn doegden en konde ic niet <rhyme label="a" type="f">wtspreken</rhyme></l>
        <l id="s4:l2" met="4" type="-+">al had ic alle tongen <rhyme label="b" type="m">ghewalt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">an mynre macht solt my <rhyme label="a" type="f" corresp="#s4:l1">ontbreken</rhyme></l>
        <l id="s4:l4" met="4" type="-+">seer guetlic is dyn wesen <rhyme label="b" type="m" corresp="#s4:l2">gestalt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">du biste des hemels een <rhyme label="c" type="m">conynginne</rhyme></l>
        <l id="s4:l6" met="4" type="-+">ende der werlde <rhyme label="c" type="m" corresp="#s4:l5">keysarynne</rhyme></l>
        <l id="s4:l7" met="4" type="-+">in dinen handen staetet <rhyme label="b" type="m" corresp="#s4:l2">al</rhyme></l>
        <l id="s4:l8" met="4" type="-+">ic bidde di ic bidde <rhyme label="d" type="m">di</rhyme></l>
        <l id="s4:l9" met="3" type="-+">och staet my trouwelic <rhyme label="d" type="m" corresp="#s4:l8">bi</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Myn ziele is dicke in swaren <rhyme label="a" type="f">noden</rhyme></l>
        <l id="s5:l2" met="4" type="-+">bange is dat jonge herte <rhyme label="b" type="m">myn</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Ick sorge die duuel wil my <rhyme label="a" type="f" corresp="#s5:l1">doden</rhyme></l>
        <l id="s5:l4" met="4" type="-+">oec vreese ic seer die helsche <rhyme label="b" type="m" corresp="#s5:l2">pyn</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Ick bidde dat gy altyt wilt <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s5:l6" met="4" type="-+">tegen alle quellynge een <rhyme label="c" type="m" corresp="#s5:l5">medecyne</rhyme></l>
        <l id="s5:l7" met="4" type="-+">ende my verblyden in allen <rhyme label="b" type="m" corresp="#s5:l2">liden</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Ick bidde di ic bidde <rhyme label="d" type="m">di</rhyme></l>
        <l id="s5:l9" met="3" type="-+">och staet my trouwelic <rhyme label="d" type="m" corresp="#s5:l8">by</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maket my van allen sunden <rhyme label="a" type="f">vry</rhyme></l>
        <l id="s6:l2" met="4" type="-+">behuedet myn herte ende alle myne <rhyme label="b" type="m">synnen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">mit allen doechden vercieret <rhyme label="a" type="f" corresp="#s6:l1">my</rhyme></l>
        <l id="s6:l4" met="4" type="-+">dat bid ic doer dine reyne <rhyme label="b" type="m" corresp="#s6:l2">mynne</rhyme></l>
        <l id="s6:l5" met="4" type="-+">gy syt den sunders een <rhyme label="c" type="m">troesterynne</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Ick lye dat ic een sunder <rhyme label="c" type="m" corresp="#s6:l5">byn</rhyme></l>
        <l id="s6:l7" met="4" type="-+">daer om soeke ic genaede aen <rhyme label="b" type="m" corresp="#s6:l2">di</rhyme></l>
        <l id="s6:l8" met="4" type="-+">Ick bidde di ic bidde <rhyme label="d" type="m">di</rhyme></l>
        <l id="s6:l9" met="3" type="-+">och staet my trouwelic <rhyme label="d" type="m" corresp="#s6:l8">by</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Och weerde vrouwe myns hertsen <rhyme label="a" type="f">lost</rhyme></l>
        <l id="s7:l2" met="4" type="-+">genaede begeer ic van di te <rhyme label="b" type="m">ontfaen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">gy syt myn hape ende alle myn <rhyme label="a" type="f" corresp="#s7:l1">troest</rhyme></l>
        <l id="s7:l4" met="4" type="-+">dede gi dat waer mit my alle <rhyme label="b" type="m" corresp="#s7:l2">gedaen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">ontfermt v mynre al coem ic <rhyme label="c" type="m">spade</rhyme></l>
        <l id="s7:l6" met="4" type="-+">ic hebbe my dickewile quellic <rhyme label="c" type="m" corresp="#s7:l5">verraden</rhyme></l>
        <l id="s7:l7" met="4" type="-+">och moeder mylde ic geer <rhyme label="b" type="m" corresp="#s7:l2">genade</rhyme></l>
        <l id="s7:l8" met="4" type="-+">Ick bidde di ic bidde <rhyme label="d" type="m">di</rhyme></l>
        <l id="s7:l9" met="3" type="-+">och staet my trouwelic <rhyme label="d" type="m" corresp="#s7:l8">by</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Teghen dat wy van hier nv <rhyme label="a" type="f">scheyden</rhyme></l>
        <l id="s8:l2" met="4" type="-+">als wy dit leuen moeten <rhyme label="b" type="m">laten</rhyme></l>
        <l id="s8:l3" met="4" type="-+">soe wilt ons hemelscher vrouden <rhyme label="a" type="f" corresp="#s8:l1">bereyden</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Daer vroude is altyt sonder <rhyme label="b" type="m" corresp="#s8:l2">mate</rhyme></l>
        <l id="s8:l5" met="4" type="-+">in des hemels throen daer gy syt <rhyme label="c" type="m">schoen</rhyme></l>
        <l id="s8:l6" met="4" type="-+">verheuen by dinen ewigen <rhyme label="c" type="m" corresp="#s8:l5">soen</rhyme></l>
        <l id="s8:l7" met="4" type="-+">daer di die enghelen choren <rhyme label="b" type="m" corresp="#s8:l2">lauen</rhyme></l>
        <l id="s8:l8" met="4" type="-+">ic bidde di ic bidde <rhyme label="d" type="m">di</rhyme></l>
        <l id="s8:l9" met="3" type="-+">och staet my trouwelic <rhyme label="d" type="m" corresp="#s8:l8">by</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
