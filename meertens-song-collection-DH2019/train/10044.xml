    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">BRuygom Heer, vlieght na u <rhyme label="a" type="m">vermaeck</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Proeft of het spel noch hebbe d'oude <rhyme label="b" type="m">smaek</rhyme>.</l>
        <l id="s1:l3" met="2" type="+-">Maar 'k <rhyme label="c" type="m">behoor</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Geen willigh Paardt de <rhyme label="c" type="m" corresp="#s1:l3">spoor</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Te geven, want ick <rhyme label="d" type="m">weet</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Ghy zijt <rhyme label="d" type="m" corresp="#s1:l5">gereedt</rhyme>.</l>
      </lg>
