  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WIlt singen// En <rhyme label="a" type="f">springen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Ionck en Out <rhyme label="b" type="m">gelijckerhant</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want de Vrede is <rhyme label="b" type="m" corresp="#s1:l2">geplant</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Nu, Nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s1:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s1:l4">mee</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Nu, Nu, heerst de gulde <rhyme label="c" type="m" corresp="#s1:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">In â€™t duyster// de <rhyme label="a" type="f">luyster</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Leght van Mars vertreden <rhyme label="b" type="m">gants</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Sijn Canon, Musquets, en <rhyme label="b" type="m" corresp="#s2:l2">Lants</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s2:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s2:l4">mee</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s2:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Oock mede// op <rhyme label="a" type="f">heede</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Werd het Masker <rhyme label="b" type="m">af-gedaen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Van de valsheyt <rhyme label="b" type="m" corresp="#s3:l2">onbelaen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s3:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s3:l4">mee</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s3:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Verslagen// dees <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Leght de bitse Nijt haer <rhyme label="b" type="m">vracht</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Met haer Slangen-pruyck <rhyme label="b" type="m" corresp="#s4:l2">veracht</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s4:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s4:l4">mee</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s4:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Verschooven// <rhyme label="a" type="f">berooven</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Werd Vulcaen de swarte <rhyme label="b" type="m">Smit</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Hy geen Wapens meer schuert <rhyme label="b" type="m" corresp="#s5:l2">wit</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s5:l5" met="6" type="+-">Door het Christenrijck// deel sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s5:l4">mee</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s5:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">De Schrapers// van <rhyme label="a" type="f">Kapers</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Moeten haer gestoole <rhyme label="b" type="m">buyt</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Laten, en naer sint <rhyme label="b" type="m" corresp="#s6:l2">Reyn-uyt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s6:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s6:l4">mee</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s6:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Oock Maetje// <rhyme label="a" type="f">Soldaetje</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Moet nu smeecken aen den <rhyme label="b" type="m">Boer</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Geeft een stuck mijn lieve <rhyme label="b" type="m" corresp="#s7:l2">Broer</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s7:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s7:l4">mee</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s7:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Vuer-pijlen// Die <rhyme label="a" type="f">zeylen</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Door de lucht <rhyme label="b" type="m">gelijckerhant</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Pick-tons staen in lichten <rhyme label="b" type="m" corresp="#s8:l2">brant</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s8:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s8:l4">mee</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s8:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Trompette// <rhyme label="a" type="f">Cornette</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">De Scharmeyen en <rhyme label="b" type="m">Dwars-fluyt</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Geven nu een soet <rhyme label="b" type="m" corresp="#s9:l2">geluyt</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s9:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s9:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s9:l4">mee</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s9:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Gedroncken// <rhyme label="a" type="f">Geschoncken</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Werd de Wijn by kleyn en <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Met een minnelijck <rhyme label="b" type="m" corresp="#s10:l2">accoort</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s10:l5" met="6" type="+-">Door het Christenrijck// deelt sy nu <rhyme label="d" type="m">gelijck</rhyme></l>
        <l id="s10:l6" met="3" type="+-">Haer gaven <rhyme label="c" type="m" corresp="#s10:l4">mee</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">Nu, nu, heerst de gulde <rhyme label="c" type="m" corresp="#s10:l4">Vree</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Set lijden// Ter <rhyme label="a" type="f">zijden</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Ende bidt dat Godt ons <rhyme label="b" type="m">geeft</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Dat de Vrede langh <rhyme label="b" type="m" corresp="#s11:l2">bekleeft</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Nu, nu, sal de soete <rhyme label="c" type="m">Vree</rhyme></l>
        <l id="s11:l5" met="6" type="+-">Hare Vruchten soet// Ons in <rhyme label="d" type="m">overvloet</rhyme></l>
        <l id="s11:l6" met="3" type="+-">Nu deelen <rhyme label="c" type="m" corresp="#s11:l4">mee</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Nu, nu, is het vaste <rhyme label="c" type="m" corresp="#s11:l4">Vree</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
