  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom vlecht my eenen <rhyme label="a" type="m">krans</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">van onverdorde <rhyme label="b" type="f">Bloemen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Waer Flora op kan <rhyme label="b" type="f" corresp="#s1:l2">roemen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">zoo ga ik bly ten <rhyme label="a" type="m" corresp="#s1:l1">dans</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">'k Zie Fillis, schoon van <rhyme label="c" type="f">kaken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In dezen Lente <rhyme label="d" type="m">tyd</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Het bloemryk veld <rhyme label="c" type="f" corresp="#s1:l5">genaken</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Myn min word haer <rhyme label="d" type="m" corresp="#s1:l6">gewyd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Kom dans in 't Jeugdig <rhyme label="a" type="m">groen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ã´ Eer der <rhyme label="b" type="f">Veldgodinnen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Waer duizend <rhyme label="b" type="f" corresp="#s2:l2">herderinnen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Uw schoonheid hulde <rhyme label="a" type="m" corresp="#s2:l1">doen</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">'k Zie drie <rhyme label="c" type="f">bevalligheden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">In u te saem <rhyme label="d" type="m">gepaerd</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Die daelden naer <rhyme label="c" type="f" corresp="#s2:l5">beneden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Wanneer gy werd <rhyme label="d" type="m" corresp="#s2:l6">gebaerd</rhyme>.</l>
      </lg>
    </lg>
  </text>
