<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181882</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Kom Orpheus kom Amphion</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ik lag laest op een <rhyme label="a" type="m">Morgenstond</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Te dromen op myn <rhyme label="b" type="m">Bed</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dat aen myn zy lag Roose <rhyme label="a" type="m" corresp="#s1:l1">mond</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Heel naekend blank en <rhyme label="b" type="m" corresp="#s1:l2">vet</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Haer oogjes waren als <rhyme label="c" type="m">vergit</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Haer Kaekjes als een <rhyme label="d" type="m">Roos</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Geplant op zuyver lely <rhyme label="c" type="m" corresp="#s1:l5">wit</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die ik voor 't beste <rhyme label="d" type="m" corresp="#s1:l6">koos</rhyme>...</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Haer Cantjes ware <rhyme label="a" type="m">Parlemoer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Een klooftje had haer <rhyme label="b" type="m">kin</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Uerdubbeld met een <rhyme label="a" type="m" corresp="#s2:l1">Peerelsnoer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Uan Uenus min <rhyme label="b" type="m" corresp="#s2:l2">Godin</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Haer Borsjes ware <rhyme label="c" type="m">Appelrond</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">Gelyk als melk en <rhyme label="d" type="m">room</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Op elk een robyntje <rhyme label="c" type="m" corresp="#s2:l5">stond</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Die kuste ik zonder <rhyme label="d" type="m" corresp="#s2:l6">schroom</rhyme>...</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Haer schoone Lichaem <rhyme label="a" type="m">nederwaerd</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Was net als een <rhyme label="b" type="m">Albast</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Niet lager dan twee spanne <rhyme label="a" type="m" corresp="#s3:l1">ras</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Een aerdig kloofje <rhyme label="b" type="m" corresp="#s3:l2">was</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Haer buikje my een Kruidberg <rhyme label="c" type="m">scheen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In Uenus Tempel <rhyme label="d" type="m">staen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Op 't Pilias van 't wit <rhyme label="c" type="m" corresp="#s3:l5">Elpenbeen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Beplant met Lely <rhyme label="d" type="m" corresp="#s3:l6">blaen</rhyme> ...</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Tempel daer men offer <rhyme label="a" type="m">doet</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Uan 't beste dat men <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ia zelfs daer men vlees en <rhyme label="a" type="m" corresp="#s4:l1">bloed</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En al ten besten <rhyme label="b" type="m" corresp="#s4:l2">geeft</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Daer vroeg ik of ik in mogt <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En doen myn <rhyme label="d" type="m">offerhand</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Zy lokte my zeer vriendelyk <rhyme label="c" type="m" corresp="#s4:l5">aen</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">En drukte my myn <rhyme label="d" type="m" corresp="#s4:l6">hand</rhyme> ...</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ik boog myn voor haer Min <rhyme label="a" type="m">altaer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ik krooper digt by <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Gestrengelt kuste wy <rhyme label="a" type="m" corresp="#s5:l1">elkaer</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Met Kusjses vol van <rhyme label="b" type="m" corresp="#s5:l2">min</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Maer doen myn Offer was <rhyme label="c" type="m">volbragt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">In 't binnenst Tempel <rhyme label="d" type="m">koor</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Ontwaekt ik heel <rhyme label="c" type="m" corresp="#s5:l5">onverwagt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Die schoone ik <rhyme label="d" type="m" corresp="#s5:l6">verloor</rhyme> ...</l>
      </lg>
    </lg>
  </text>
</song>
