<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14676</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Puer natus in Bethleem in hoc anno</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Pieter van Gelre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHeboren is in <rhyme label="a" type="m">Bethelem</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Dien Heylant, <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Na heeft verlanght <rhyme label="a" type="m" corresp="#s1:l1">Ierusalem</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">D'Heylant daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s1:l4">gheseten</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s1:l2">waer</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s1:l4">heeten</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s1:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het gheen dat Balam seyde <rhyme label="a" type="m">van</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Dien Heylant, <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Door komen nu veel Heyd'nen <rhyme label="a" type="m" corresp="#s2:l1">a'n</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">D'Heylant daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s2:l4">gheseten</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s2:l2">waer</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s2:l4">heeten</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s2:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Het scheen dat Moyses eertijdts <rhyme label="a" type="m">sach</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Dien Heylandt, <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Is'rels verlossingh' heel in <rhyme label="a" type="m" corresp="#s3:l1">lach</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">D'Heylant daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s3:l4">gheseten</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s3:l2">waer</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s3:l4">heeten</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s3:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer noyt heeft hy te recht <rhyme label="a" type="m">gesien</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Dien Heylandt, <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Den Satan voor te rug moet <rhyme label="a" type="m" corresp="#s4:l1">vlien</rhyme></l>
        <l id="s4:l4" met="4" type="-+">D'Heylant daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s4:l4">gheseten</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s4:l2">waer</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s4:l4">heeten</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s4:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer ons wordt nu te recht <rhyme label="a" type="m">vertoont</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Dien Heylandt, <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Ons hoop, ons troost, ons pays in <rhyme label="a" type="m" corresp="#s5:l1">woont</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">D'Heylant daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s5:l4">gheseten</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s5:l2">waer</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s5:l4">heeten</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s5:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Siet hy komt nu ten Tempel <rhyme label="a" type="m">in</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Dien Heylandt, <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Hy traentjes stort uyt liefdens <rhyme label="a" type="m" corresp="#s6:l1">min</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">D'Heylant daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s6:l4">gheseten</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s6:l2">waer</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s6:l4">heeten</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s6:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Och dat ick nu oock eens reys <rhyme label="a" type="m">mocht</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Dien Heylandt, <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Besproeyen met mijn ooghjes <rhyme label="a" type="m" corresp="#s7:l1">vocht</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">D'Heylant daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s7:l4">gheseten</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s7:l2">waer</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s7:l4">heeten</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s7:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ick soud' hem drucken aen mijn <rhyme label="a" type="m">borst</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Dien Heylandt, <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Hy mocht uytblusschen d'aerdtsche <rhyme label="a" type="m" corresp="#s8:l1">dorst</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">D'Heylant daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s8:l4">gheseten</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s8:l2">waer</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s8:l4">heeten</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s8:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Laet ons dan loven al <rhyme label="a" type="m">ghelijck</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Dien Heylandt <rhyme label="b" type="m">daer</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Ons' ziel door wordt van deuchden <rhyme label="a" type="m" corresp="#s9:l1">rijck</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">D'Heylandt daer soo veel <rhyme label="c" type="f">Propheten</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Treurich hebben om <rhyme label="c" type="f" corresp="#s9:l4">geseten</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Dat hy noch niet <rhyme label="b" type="m" corresp="#s9:l2">waer</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Is voor ons nu Iesus <rhyme label="c" type="f" corresp="#s9:l4">heeten</rhyme>,</l>
        <l id="s9:l8" met="3" type="+-">Tot een Nieuwe <rhyme label="b" type="m" corresp="#s9:l2">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
