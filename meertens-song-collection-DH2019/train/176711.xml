    <melody>De lustelijke mei is nu in de tijd</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Sterckt my o Godt mijn <rhyme label="a" type="m">toe-verlaet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Een strijt heb ick van <rhyme label="b" type="f">binnen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Den Vyant als een leeuw <rhyme label="a" type="m" corresp="#s1:l1">omgaet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Soeckt mijn Ziel te <rhyme label="b" type="f" corresp="#s1:l2">versinnen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Hy heeft sijn schut op my <rhyme label="c" type="m">gestelt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Om my te doden met <rhyme label="c" type="m" corresp="#s1:l5">gewelt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">O Heer helpt my nu <rhyme label="d" type="f">strijden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s1:l7">tijden</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mijn Vleesch gelust tegen den <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O Heer ick moet u <rhyme label="b" type="f">klaegen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want het altijt voor lijden <rhyme label="a" type="m" corresp="#s2:l1">vreest</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En wil u Kruys niet <rhyme label="b" type="f" corresp="#s2:l2">draegen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Want het bemint de Doot veel <rhyme label="c" type="m">meer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dan ‘t leven by u o mijn <rhyme label="c" type="m" corresp="#s2:l5">Heer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Het doet mijn hert <rhyme label="d" type="f">doorsnijden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s2:l7">tijden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Och wie sal my ellendigh <rhyme label="a" type="m">Mensch</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van desen vleesch <rhyme label="b" type="f">ontladen</rhyme>?</l>
        <l id="s3:l3" met="4" type="-+">Het wil steeds nae sijn eygen <rhyme label="a" type="m" corresp="#s3:l1">wensch</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met my op kromme <rhyme label="b" type="f" corresp="#s3:l2">paden</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">O Heer wijst my u rechte <rhyme label="c" type="m">baen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Mijn vleesch wil ick te buyten <rhyme label="c" type="m" corresp="#s3:l5">gaen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ick stel sijn raat <rhyme label="d" type="f">besijden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s3:l7">tijden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O Vleesch oft ick volghd uwen <rhyme label="a" type="m">raat</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Mijn Ziel soud moeten <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Veel liever wil ick sijn <rhyme label="a" type="m" corresp="#s4:l1">versmaat</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En met Godts volck <rhyme label="b" type="f" corresp="#s4:l2">omswerven</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Want haer druck sal verandert <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s4:l6" met="4" type="-+">In vreughd, maer d’andere in <rhyme label="c" type="m" corresp="#s4:l5">pijn</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Die Werelts haer <rhyme label="d" type="f">verblijden</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s4:l7">tijden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O Vleesch oft gy op Esau <rhyme label="a" type="m">doght</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die d’eerst-geboort <rhyme label="b" type="f">verkochte</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Schoon hy ‘s daer nae met tranen <rhyme label="a" type="m" corresp="#s5:l1">socht</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Geen plaets hy vinden <rhyme label="b" type="f" corresp="#s5:l2">mochte</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">O Vleesch gy sult niet volgen <rhyme label="c" type="m">hem</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Ick wil nae ‘t nieuw <rhyme label="c" type="m" corresp="#s5:l5">Ierusalem</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Al sout gy daerom <rhyme label="d" type="f">lijden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s5:l7">tijden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">O Heer wilt dogh mijn leytsman <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Nae dese stadt van <rhyme label="b" type="f">vreede</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">De Wegh is smal dat dincket <rhyme label="a" type="m" corresp="#s6:l1">mijn</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Gelijck een ‘s menschen <rhyme label="b" type="f" corresp="#s6:l2">trede</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Op d’een sijd water, d’ander <rhyme label="c" type="m">vier</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Bewaert haer die hem wand’len <rhyme label="c" type="m" corresp="#s6:l5">hier</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dat ‘s u oprecht <rhyme label="d" type="f">belijden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s6:l7">tijden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O Valsche Werelt schoon in ‘t <rhyme label="a" type="m">Oogh</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Adieu ick wil u <rhyme label="b" type="f">derven</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Appel der weeld verheven <rhyme label="a" type="m" corresp="#s7:l1">hoogh</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die van u eet moet <rhyme label="b" type="f" corresp="#s7:l2">sterven</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Hierom Eva <rhyme label="c" type="m">wedergebaert</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Wacht u dogh voor der Slangen <rhyme label="c" type="m" corresp="#s7:l5">aert</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Sy is bevaen met <rhyme label="d" type="f">nijden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s7:l7">tijden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">‘t Serpent hem als een Leeu nu <rhyme label="a" type="m">spoeyt</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Heer, om u Thuyn te <rhyme label="b" type="f">breken</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Want hy seer lieflijck stont en <rhyme label="a" type="m" corresp="#s8:l1">bloeyt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Als aen de <rhyme label="b" type="f" corresp="#s8:l2">Waterbeken</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Maer wilde verckens seer <rhyme label="c" type="m">onvroet</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Die hebben hem <rhyme label="c" type="m" corresp="#s8:l5">ommegewroet</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Sijn spruytjes hem <rhyme label="d" type="f">ontglijden</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s8:l7">tijden</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O Heer ons klaegen doch <rhyme label="a" type="m">verhoort</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Nae vreed wy seer <rhyme label="b" type="f">verlanghen</rhyme></l>
        <l id="s9:l3" met="4" type="-+">In Syon pleegh te zijn u <rhyme label="a" type="m" corresp="#s9:l1">woort</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Nu is ‘t vol Draken en <rhyme label="b" type="f" corresp="#s9:l2">Slangen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Daer zijn oock heydens in <rhyme label="c" type="m">gegaen</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Die u Tempel in stucken <rhyme label="c" type="m" corresp="#s9:l5">slaen</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Sijn Heylighdom <rhyme label="d" type="f">ontwijden</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">In dese quade <rhyme label="d" type="f" corresp="#s9:l7">tijden</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Heer sent getrouwe <rhyme label="a" type="m">Arbeytslieden</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Die u Thuyn <rhyme label="b" type="f">wederbouwen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Bemeuren met levende <rhyme label="a" type="m" corresp="#s10:l1">stien</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Als Herders recht <rhyme label="b" type="f" corresp="#s10:l2">getrouwe</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">En die de kleyne voskens <rhyme label="c" type="m">vaen</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">En ‘t door knaegende vee <rhyme label="c" type="m" corresp="#s10:l5">verslaen</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Dat bijt aen alle <rhyme label="d" type="f">sijden</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">In dese quaede <rhyme label="d" type="f" corresp="#s10:l7">tijden</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Prins Godt een recht <rhyme label="a" type="m">Wijngaerdenier</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Wy bidden u <rhyme label="b" type="f">ootmoedigh</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Plant u Wijnbergh aen een <rhyme label="a" type="m" corresp="#s11:l1">rewier</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Dat sy vrught brenght <rhyme label="b" type="f" corresp="#s11:l2">voortspoedigh</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">En maeckt daer om een stercke <rhyme label="c" type="m">muer</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Getrouwe Herders tot haer <rhyme label="c" type="m" corresp="#s11:l5">huer</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Geefst’ u weert aen haer <rhyme label="d" type="f">sijden</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">In dese quaede <rhyme label="d" type="f" corresp="#s11:l7">tijden</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Oorlof aen alle rankxkens <rhyme label="a" type="m">teer</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ick groet u al hier <rhyme label="b" type="f">mede</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Die staen op des Apostels <rhyme label="a" type="m" corresp="#s12:l1">leer</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Vruchtbaer nae Christi <rhyme label="b" type="f" corresp="#s12:l2">seede</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">U handen tot den Heer <rhyme label="c" type="m">oprecht</rhyme></l>
        <l id="s12:l6" met="4" type="-+">En dit voor uwen swacken <rhyme label="c" type="m" corresp="#s12:l5">knecht</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Dat hy valsch Leer mach <rhyme label="d" type="f">mijden</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">In dese quaede <rhyme label="d" type="f" corresp="#s12:l7">tijden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
