<song>
  <meta>
    <id>21113</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AErd' en Hemel nu <rhyme label="a" type="m">bewijst</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">En <rhyme label="a" type="m" corresp="#s1:l1">aenprijst</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Lentens lieffelijck <rhyme label="b" type="f">beginnen</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">'t Gaet nu beyde, vee en <rhyme label="c" type="m">mensch</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Na haer <rhyme label="c" type="m" corresp="#s1:l4">wensch</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Om genoegen te <rhyme label="b" type="f" corresp="#s1:l3">gewinnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Even soo zijn dese <rhyme label="a" type="m">twee</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Die oock <rhyme label="a" type="m" corresp="#s2:l1">mee</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Hier in d'Echten-staet <rhyme label="b" type="f">versamen</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Hier is Lentens soet <rhyme label="c" type="m">begin</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Door de <rhyme label="c" type="m" corresp="#s2:l4">min</rhyme>:</l>
        <l id="s2:l6" met="4" type="+-">Hier zijn twee vereende <rhyme label="b" type="f" corresp="#s2:l3">namen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hy die van een vroomen <rhyme label="a" type="m">stam</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Eerlijck <rhyme label="a" type="m" corresp="#s3:l1">quam</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Sy oock in gelijcker <rhyme label="b" type="f">voegen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Beyde Vogels eender <rhyme label="c" type="m">veer</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Waer was <rhyme label="c" type="m" corresp="#s3:l4">meer</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Waer was grooter <rhyme label="b" type="f" corresp="#s3:l3">vergenoegen</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ghy dan Bruyloft-gasten <rhyme label="a" type="m">singht</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Dat het <rhyme label="a" type="m" corresp="#s4:l1">klinckt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Singht den lof van dese <rhyme label="b" type="f">beyden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Die nu in der liefden <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Totter <rhyme label="c" type="m" corresp="#s4:l4">doodt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Nimmer hier en sullen <rhyme label="b" type="f" corresp="#s4:l3">scheyden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wel dan ghy vereenight <rhyme label="a" type="m">paer</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Wilt <rhyme label="a" type="m" corresp="#s5:l1">malkaer</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Handen, hart, en trouwe, <rhyme label="b" type="f">bieden</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Soo sal droefheydt, en <rhyme label="c" type="m">discoort</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Voort en <rhyme label="c" type="m" corresp="#s5:l4">voort</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Van u Huys gedurigh <rhyme label="b" type="f" corresp="#s5:l3">vlieden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Siet, sy wenschen u <rhyme label="a" type="m">geluck</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">In dit <rhyme label="a" type="m" corresp="#s6:l1">stuck</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat u welvaert magh <rhyme label="b" type="f">vermeeren</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Treckt dan t'samen eenen <rhyme label="c" type="m">lijn</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Dat u <rhyme label="c" type="m" corresp="#s6:l4">pijn</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Magh in soete Wijn <rhyme label="b" type="f" corresp="#s6:l3">verkeeren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Dat ghy eenen Soone <rhyme label="a" type="m">kust</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">En met <rhyme label="a" type="m" corresp="#s7:l1">lust</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Meught het Suye-Seuntje <rhyme label="b" type="f">singen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Als wy op een ander <rhyme label="c" type="m">Jaer</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Moer en <rhyme label="c" type="m" corresp="#s7:l4">Vaer</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Wederom geluck <rhyme label="b" type="f" corresp="#s7:l3">toe-bringen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Oorlof Bruydegom en <rhyme label="a" type="m">Bruydt</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Tot <rhyme label="a" type="m" corresp="#s8:l1">besluyt</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Wil den Hemel u <rhyme label="b" type="f">verleenen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Dat ghy nu te samen <rhyme label="c" type="m">meught</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Inder <rhyme label="c" type="m" corresp="#s8:l4">deught</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Tot u salicheydt <rhyme label="b" type="f" corresp="#s8:l3">vereenen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
