  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Daar was lestmaal een <rhyme label="a" type="m">bol</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die graag speelden zyn <rhyme label="a" type="m" corresp="#s1:l1">rol</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In het zoete spel der <rhyme label="b" type="f">minne</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hy treed zomtyds eens <rhyme label="b" type="f" corresp="#s1:l3">binne</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Al in Vrouw Venus <rhyme label="c" type="m">school</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat bragt hem op den <rhyme label="c" type="m" corresp="#s1:l5">Dool</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vooreerst zo leerde hy <rhyme label="a" type="m">straf</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het dansen by Jan <rhyme label="a" type="m" corresp="#s2:l1">Paf</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">De pasperjé de <rhyme label="b" type="f">Spanje</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">De nieuwe la <rhyme label="b" type="f" corresp="#s2:l3">bartanje</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Veel pluggen <rhyme label="c" type="m">abondant</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Danste hy na de <rhyme label="c" type="m" corresp="#s2:l5">trant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">’t Fiooltje moest daar <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hy speelde een zoet <rhyme label="a" type="m" corresp="#s3:l1">schalmy</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">Hy speelde op de <rhyme label="b" type="f">klaarnette</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">’t Was voor zyn schoon <rhyme label="b" type="f" corresp="#s3:l3">Lizette</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Al voor dat Venus <rhyme label="c" type="m">kind</rhyme>;</l>
        <l id="s3:l6" met="3" type="-+">Dat hy zoo teer <rhyme label="c" type="m" corresp="#s3:l5">bemind</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daar was geen <rhyme label="a" type="m">liefhebbery</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Of hy was ‘er altoos <rhyme label="a" type="m" corresp="#s4:l1">by</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Van draave over de <rhyme label="b" type="f">straaten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om hondert Goude <rhyme label="b" type="f" corresp="#s4:l3">Ducaaten</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Al met zyn witvoet <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat draave kon hy vry <rhyme label="c" type="m" corresp="#s4:l5">wel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Doen hy dat alles <rhyme label="a" type="m">kost</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Doen zwierde hy zeer <rhyme label="a" type="m" corresp="#s5:l1">tros</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Al in dat groote <rhyme label="b" type="f">schooltje</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maakte hy meenig <rhyme label="b" type="f" corresp="#s5:l3">kampriooltje</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Ja ook in de <rhyme label="c" type="m">Fontyn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daar vond hy zyn <rhyme label="c" type="m" corresp="#s5:l5">Catryn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy zwierde ook in de <rhyme label="a" type="m">Pyl</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Maar dat was maar <rhyme label="a" type="m" corresp="#s6:l1">zomwyl</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Maar in dat Bos, dat <rhyme label="b" type="f">klooster</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat hield hy voor zyn <rhyme label="b" type="f" corresp="#s6:l3">trooster</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Ja ook by Jan de <rhyme label="c" type="m">Pruyk</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Daar kogt hy meenig <rhyme label="c" type="m" corresp="#s6:l5">kruyk</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daar was geen een <rhyme label="a" type="m">Speelhuis</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Tot de Lydze buurt in ’t <rhyme label="a" type="m" corresp="#s7:l1">kluys</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Daar hy niet had staan <rhyme label="b" type="f">danssen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Zoo meenig deun de <rhyme label="b" type="f" corresp="#s7:l3">france</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Al met zoo meenig <rhyme label="c" type="m">Hoer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die daar liep over de <rhyme label="c" type="m" corresp="#s7:l5">vloer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
