    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Christ ist erstanden</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CHRIstus voer ten <rhyme label="a" type="f">HEmel</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wat seynd' hy ons hier <rhyme label="a" type="f" corresp="#s1:l1">weder</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Syn weerden heyl'ghen Geest <rhyme label="b" type="m">bereyt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Tot Troost den lieven <rhyme label="b" type="m" corresp="#s1:l3">Christenheyt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Christus onsen <rhyme label="a" type="f">Heylandt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Sitt ter Rechten Godes <rhyme label="a" type="f" corresp="#s2:l1">Handt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Verbidt dat Arm Mensch'lijck <rhyme label="b" type="m">geslecht</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat wy worden door hem <rhyme label="b" type="m" corresp="#s2:l3">gherecht</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Waer hy niet <rhyme label="a" type="f">Opgenomen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die Trooster waer niet <rhyme label="a" type="f" corresp="#s3:l1">gecomen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maer want hy Opgevaren <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">So hebben wy den Geest door <rhyme label="b" type="m" corresp="#s3:l3">Christ</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+"><rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
