  <author>Fran√ßois Foret</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Komt gy Engelsche <rhyme label="a" type="f">schaeren</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Uyt dat Hemelsch <rhyme label="b" type="m">Hof</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Roert uw' gulde <rhyme label="a" type="f" corresp="#s1:l1">snaeren</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Zingt Maria <rhyme label="b" type="m" corresp="#s1:l2">lof</rhyme> //</l>
        <l id="s1:l5" met="3" type="+-">Want ons is <rhyme label="c" type="f">geboren</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">In dat aerdsche <rhyme label="d" type="m">dal</rhyme> //</l>
        <l id="s1:l7" met="3" type="+-">En zoo naer als <rhyme label="c" type="f" corresp="#s1:l5">vooren</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Maget blyven <rhyme label="d" type="m" corresp="#s1:l6">zal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Heylig eeuwig <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Gods <rhyme label="b" type="m">Dryvuldigheyd</rhyme> //</l>
        <l id="s2:l3" met="3" type="+-">Heeft haer <rhyme label="a" type="f" corresp="#s2:l1">uytgelezen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En alzoo <rhyme label="b" type="m" corresp="#s2:l2">bereyd</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Om te zijn <rhyme label="c" type="f">gevonden</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Moeder, Vrouwe, <rhyme label="d" type="m">Maegd</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Vry van alle <rhyme label="c" type="f" corresp="#s2:l5">zonden</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Die den Heer <rhyme label="d" type="m" corresp="#s2:l6">behaegt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Lof zy dan <rhyme label="a" type="f">gezongen</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Van dees weirde <rhyme label="b" type="m">Bruyd</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Die met alle <rhyme label="a" type="f" corresp="#s3:l1">tongen</rhyme> //</l>
        <l id="s3:l4" met="3" type="+-">En een zoet <rhyme label="b" type="m" corresp="#s3:l2">geluyd</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Loven ende <rhyme label="c" type="f">pryzen</rhyme> //</l>
        <l id="s3:l6" met="3" type="+-">Dees hemelsch' <rhyme label="d" type="m">Koninginn'</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">En haer eer' <rhyme label="c" type="f" corresp="#s3:l5">bewyzen</rhyme> //</l>
        <l id="s3:l8" met="3" type="+-">Vol van haere <rhyme label="d" type="m" corresp="#s3:l6">minn'</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Dat nu alle <rhyme label="a" type="f">menschen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Scheppen eenen <rhyme label="b" type="m">moed</rhyme> //</l>
        <l id="s4:l3" met="3" type="+-">Die hier vierig <rhyme label="a" type="f" corresp="#s4:l1">wenschen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Nae het hoogste <rhyme label="b" type="m" corresp="#s4:l2">goed</rhyme> //</l>
        <l id="s4:l5" met="3" type="+-">Want ons is <rhyme label="c" type="f">geboren</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Een goed hemelsch <rhyme label="d" type="m">kind</rhyme> //</l>
        <l id="s4:l7" met="3" type="+-">En zoo naer als <rhyme label="c" type="f" corresp="#s4:l5">vooren</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Onze ziel' <rhyme label="d" type="m" corresp="#s4:l6">bemint</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Met wat blyd're <rhyme label="a" type="f">vreugden</rhyme></l>
        <l id="s5:l2" met="3" type="+-">'s Hemelsch <rhyme label="b" type="m">borgery'</rhyme> //</l>
        <l id="s5:l3" met="3" type="+-">Zag zy hun <rhyme label="a" type="f" corresp="#s5:l1">verheugen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Die met herten <rhyme label="b" type="m" corresp="#s5:l2">bly</rhyme> //</l>
        <l id="s5:l5" met="3" type="+-">Dees weirdige <rhyme label="c" type="f">Bloeme</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Dikwils schouwden <rhyme label="d" type="m">aen</rhyme> //</l>
        <l id="s5:l7" met="3" type="+-">In den Hemel <rhyme label="c" type="f" corresp="#s5:l5">omme</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Met haer mogen <rhyme label="d" type="m" corresp="#s5:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">O Maria! <rhyme label="a" type="f">Schoone</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Vol van 's hemelsch <rhyme label="b" type="m">glans</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Maekt een' <rhyme label="a" type="f" corresp="#s6:l1">Maegdekroone</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Een vergulden <rhyme label="b" type="m" corresp="#s6:l2">krans</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Komen wy u <rhyme label="c" type="f">groeten</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Met <rhyme label="d" type="m">oodmoedigheyd</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Wilt ons nu <rhyme label="c" type="f" corresp="#s6:l5">ontmoeten</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Met <rhyme label="d" type="m" corresp="#s6:l6">bermhertigheyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
