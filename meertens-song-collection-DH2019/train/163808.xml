<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>163808</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach schoonste nimf aanzie</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">WAt droevigh lot komt my elaes te <rhyme label="a" type="f">voren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O trouweloose <rhyme label="b" type="m">daet</rhyme>!</l>
        <l id="s1:l3" met="5" type="-+">Mijn Zieltje smelt, en wil van droefheyt <rhyme label="a" type="f" corresp="#s1:l1">smoren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Door schand en groote <rhyme label="b" type="m" corresp="#s1:l2">smaet</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Mijn suyv're Kroon is van mijn hooft <rhyme label="c" type="f">gestreken</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Mijn fleur is wech, ick ben <rhyme label="d" type="m">berooft</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Mijn schoone glants die is <rhyme label="d" type="m" corresp="#s1:l6">verdooft</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Door valsche <rhyme label="c" type="f" corresp="#s1:l5">treken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Een Konincks-kint door valsche schijn <rhyme label="a" type="f">bedrogen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Een teere Maeght <rhyme label="b" type="m">verkracht</rhyme>.</l>
        <l id="s2:l3" met="5" type="-+">O valsche Prins, en sonder <rhyme label="a" type="f" corresp="#s2:l1">mede-dogen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ey hoort mijn droeve <rhyme label="b" type="m" corresp="#s2:l2">klacht</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Gedenckt doch eens van waer ick ben <rhyme label="c" type="f">gesproten</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Denckt een-mael om uws Vaders <rhyme label="d" type="m">huys</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Denckt aen de smaet en 't bitter <rhyme label="d" type="m" corresp="#s2:l6">kruys</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">My te <rhyme label="c" type="f" corresp="#s2:l5">verstooten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">O felle Prins! Is dit  getrouwe <rhyme label="a" type="f">minne</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Is dit u soet <rhyme label="b" type="m">gevley</rhyme>?</l>
        <l id="s3:l3" met="5" type="-+">Door vals bedrogh een Maeght te <rhyme label="a" type="f" corresp="#s3:l1">overwinnen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En door geen naer <rhyme label="b" type="m" corresp="#s3:l2">geschrey</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Beweeght te zijn, ach Broeder! 'k buygh my <rhyme label="c" type="f">neder</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Laet my door smaet doch niet <rhyme label="d" type="m">vergaen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Neemt my in u genade <rhyme label="d" type="m" corresp="#s3:l6">aen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Och keert doch <rhyme label="c" type="f" corresp="#s3:l5">weder</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ach, wilt ghy van geen teere Maeght <rhyme label="a" type="f">verschoonen</rhyme>?</l>
        <l id="s4:l2" met="3" type="-+">Soo segh ick u <rhyme label="b" type="m">adieu</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Verrader boos, en schender van mijn <rhyme label="a" type="f" corresp="#s4:l1">Kroone</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gaet heen en weest vry <rhyme label="b" type="m" corresp="#s4:l2">schuw</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">Ick ben bereyt te treuren al mijn <rhyme label="c" type="f">dagen</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Maer ghy (o Prins) sult in der <rhyme label="d" type="m">daet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">De straffe van dit boose <rhyme label="d" type="m" corresp="#s4:l6">quaet</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Hier namaels <rhyme label="c" type="f" corresp="#s4:l5">dragen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
