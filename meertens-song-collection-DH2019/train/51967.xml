<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>51967</id>
    <date>
      <year-start>1701</year-start>
      <year-end>1800</year-end>
    </date>
    <tags>
      <tag standard-name="jagerslied" translation="hunter's song">jagerslied</tag>
    </tags>
    <melody>Das Jagen das ist ja mein Leben</melody>
    <categories>
      <category translation="work">werk</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Het jagen dat is ja myn <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Myn vermaak om te jagen en dat in 't <rhyme label="b" type="m">Bosch</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Om daaglyks te <rhyme label="c" type="f">schieten</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Het zal my niet <rhyme label="c" type="f" corresp="#s1:l3">verdrieten</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Zoo lang als ik <rhyme label="d" type="m">leef</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">En vuur ook ja <rhyme label="d" type="m" corresp="#s1:l5">geef</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">En toen ik in 't Bosch ben <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Toen zag ik een hertje van verre daar <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Myn Roertje moest <rhyme label="c" type="f">knallen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Het Hertje moest <rhyme label="c" type="f" corresp="#s2:l3">vallen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Van Kruit en Lood <rhyme label="d" type="m">daarby</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">In het Bosch zyn wy <rhyme label="d" type="m" corresp="#s2:l5">vry</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Het Hertje dat was nu <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Toen kwam daar van verren een jager <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Hy heeft zich <rhyme label="c" type="f">besloten</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">En Hertje <rhyme label="c" type="f" corresp="#s3:l3">geschooten</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Jager dat kan niet <rhyme label="d" type="m">zyn</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Het Hertje hoort <rhyme label="d" type="m" corresp="#s3:l5">myn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Kom Jager kom pak u nu maar <rhyme label="a" type="f">weg</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Uw leeven dat zal ik nu sparen in het <rhyme label="b" type="m">Bosch</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Geen Jagers nog <rhyme label="c" type="f">honde</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Die maken myn <rhyme label="c" type="f" corresp="#s4:l3">wonde</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Myn Roertje en <rhyme label="d" type="m">goed</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">En ik ben op myn <rhyme label="d" type="m" corresp="#s4:l5">hoed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Nacht begint nu aan te <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Alle sterren aan den hemel die schynen zeer <rhyme label="b" type="m">helder</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Het geeft niets meer te <rhyme label="c" type="f">jagen</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">'t Slapen kan myn <rhyme label="c" type="f" corresp="#s5:l3">behagen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Gantsch gerust en <rhyme label="d" type="m">bly</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Myn roer aan myn <rhyme label="d" type="m" corresp="#s5:l5">zy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">En als ons den honger doet <rhyme label="a" type="f">plagen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dan ben ik altoos nog van voorraad <rhyme label="b" type="m">voorzien</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Zo lang my het <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Van God wordt <rhyme label="c" type="f" corresp="#s6:l3">gegeeven</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Zoo lang het maar <rhyme label="d" type="m">duurt</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">En myn Roertje nog <rhyme label="d" type="m" corresp="#s6:l5">vuurt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
