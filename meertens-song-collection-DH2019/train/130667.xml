        <l id="s1:l4" met="3" type="-+">Maar lagt om al u <rhyme label="b" type="m" corresp="#s1:l2">smart</rhyme>!</l>
        <l id="s1:l5" met="3" type="+-">Weg   verdwaalde <rhyme label="a" type="f" corresp="#s1:l1">zinne</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Wilt dat nooit <rhyme label="a" type="f" corresp="#s1:l1">beginne</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Maar bant uit u hart en <rhyme label="d" type="m">zin</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gedagten al van de <rhyme label="d" type="m" corresp="#s1:l7">min</rhyme>.</l>
