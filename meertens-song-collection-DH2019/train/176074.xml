<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>176074</id>
    <date>
      <year-start>1640</year-start>
      <year-end>1640</year-end>
    </date>
    <location>
      <orginal-name>deventer</orginal-name>
      <standard-name>Deventer, The Netherlands</standard-name>
      <coordinates>
        <lon>52.2532272</lon>
        <lat>6.1584725</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 081 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SIngt met blijde <rhyme label="a" type="m">stem</rhyme></l>
        <l id="s1:l2" met="3" type="+-">God ons' sterckte <rhyme label="b" type="f">machtich</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Vroolijck dancket <rhyme label="a" type="m" corresp="#s1:l1">hem</rhyme>;</l>
        <l id="s1:l4" met="3" type="+-">Den God <rhyme label="c" type="m">Israël</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Geeft met keelen <rhyme label="c" type="m" corresp="#s1:l4">hel</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Prijs en eer <rhyme label="b" type="f" corresp="#s1:l2">aendachtich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Laet een <rhyme label="a" type="m">vreugden-sanck</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Door de wolcken <rhyme label="b" type="f">dringen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Trommelen <rhyme label="a" type="m" corresp="#s2:l1">geclanck</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Psalteren <rhyme label="c" type="m">geluyt</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">En de clare <rhyme label="c" type="m" corresp="#s2:l4">luyt'</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Wilt tevoorschijn <rhyme label="b" type="f" corresp="#s2:l2">bringen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Op het nieuwe <rhyme label="a" type="m">licht</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Blaset de <rhyme label="b" type="f">trompetten</rhyme>.</l>
        <l id="s3:l3" met="3" type="+-">Yder een hem <rhyme label="a" type="m" corresp="#s3:l1">richt'</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Ter bestemder <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Om te zijn <rhyme label="c" type="m" corresp="#s3:l4">verblijt</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Na des Heeren <rhyme label="b" type="f" corresp="#s3:l2">wetten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Dit is een <rhyme label="a" type="m">bevel</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Over-lang' <rhyme label="b" type="f">beschreven</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En aen <rhyme label="a" type="m" corresp="#s4:l1">Israël</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Van der Heeren <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s4:l5" met="3" type="+-">Tot syns names <rhyme label="c" type="m" corresp="#s4:l4">eer</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Ernstelijck <rhyme label="b" type="f" corresp="#s4:l2">gegeven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Hy heeft Iosephs <rhyme label="a" type="m">stam</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Dit te doen <rhyme label="b" type="f">verbonden</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">'sDaechs doe hyse <rhyme label="a" type="m" corresp="#s5:l1">nam</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Wt Egypten, <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s5:l5" met="3" type="+-">Sy de tale <rhyme label="c" type="m" corresp="#s5:l4">swaer</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Niet verstaen en <rhyme label="b" type="f" corresp="#s5:l2">conden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Syne schouders <rhyme label="a" type="m">crom</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Onder 't pack <rhyme label="b" type="f">gebogen</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Vrijd' ick <rhyme label="a" type="m" corresp="#s6:l1">wederom</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">En syn handen <rhyme label="c" type="m">bey</rhyme></l>
        <l id="s6:l5" met="3" type="+-">Vanden taeyen <rhyme label="c" type="m" corresp="#s6:l4">cley</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Heb ick <rhyme label="b" type="f" corresp="#s6:l2">afgetogen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Daer riept ghy my <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">En mijn hant ick <rhyme label="b" type="f">streckte</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Om u by te <rhyme label="a" type="m" corresp="#s7:l1">staen</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Tegen u <rhyme label="c" type="m">party</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Als de donder <rhyme label="c" type="m" corresp="#s7:l4">my</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Om end' om <rhyme label="b" type="f" corresp="#s7:l2">bedeckte</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Oock aen <rhyme label="a" type="m">Meriba</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Heb ick u <rhyme label="b" type="f">geproevet</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Daer ick speurde <rhyme label="a" type="m" corresp="#s8:l1">drae</rhyme></l>
        <l id="s8:l4" met="3" type="+-">V verhert <rhyme label="c" type="m">gemoet</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Doch, met woorden <rhyme label="c" type="m" corresp="#s8:l4">soet</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Sprack ick, seer <rhyme label="b" type="f" corresp="#s8:l2">bedroevet</rhyme>:</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Mijn volck, weset <rhyme label="a" type="m">stil</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Ick wil u <rhyme label="b" type="f">betuygen</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Van mijn goeden <rhyme label="a" type="m" corresp="#s9:l1">wil</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Of ghy voor <rhyme label="c" type="m">'tgebot</rhyme></l>
        <l id="s9:l5" met="3" type="+-">Van my uwen <rhyme label="c" type="m" corresp="#s9:l4">God</rhyme></l>
        <l id="s9:l6" met="3" type="+-">Vwen hals wou't <rhyme label="b" type="f" corresp="#s9:l2">buygen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">By u salmen <rhyme label="a" type="m">sien</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Geene vremde <rhyme label="b" type="f">goden</rhyme>.</l>
        <l id="s10:l3" met="3" type="+-">Niemant haer en <rhyme label="a" type="m" corresp="#s10:l1">dien'</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Doetse geene <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Valt niet voor haer <rhyme label="c" type="m" corresp="#s10:l4">neer</rhyme>.</l>
        <l id="s10:l6" met="3" type="+-">Siet, ick hebt <rhyme label="b" type="f" corresp="#s10:l2">verboden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="+-">Want ick ben de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s11:l2" met="3" type="+-">Die in droeven <rhyme label="b" type="f">stande</rhyme></l>
        <l id="s11:l3" met="3" type="+-">V op u <rhyme label="a" type="m" corresp="#s11:l1">begeer</rhyme></l>
        <l id="s11:l4" met="3" type="+-">Door mijn <rhyme label="c" type="m">mogentheyt</rhyme></l>
        <l id="s11:l5" met="3" type="+-">Seker heb ick <rhyme label="c" type="m" corresp="#s11:l4">geleydt</rhyme></l>
        <l id="s11:l6" met="3" type="+-">Wt <rhyme label="b" type="f" corresp="#s11:l2">Egyptenlande</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="+-">Opent uwen <rhyme label="a" type="m">mont</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Op dat ick hem <rhyme label="b" type="f">spijse</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">En hy sal <rhyme label="a" type="m" corresp="#s12:l1">terstont</rhyme></l>
        <l id="s12:l4" met="3" type="+-">Met groot <rhyme label="c" type="m">overvloet</rhyme></l>
        <l id="s12:l5" met="3" type="+-">Van my zijn <rhyme label="c" type="m" corresp="#s12:l4">gevoedt</rhyme></l>
        <l id="s12:l6" met="3" type="+-">Wonderlijcker <rhyme label="b" type="f" corresp="#s12:l2">wijse</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="+-">Maer mijn volck en <rhyme label="a" type="m">heeft</rhyme></l>
        <l id="s13:l2" met="3" type="+-">My niet willen <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s13:l3" met="3" type="+-">Israël en <rhyme label="a" type="m" corresp="#s13:l1">geeft</rhyme></l>
        <l id="s13:l4" met="3" type="+-">Tot mijn heylsaem <rhyme label="c" type="m">woort</rhyme></l>
        <l id="s13:l5" met="3" type="+-">Als het wel <rhyme label="c" type="m" corresp="#s13:l4">behoort</rhyme></l>
        <l id="s13:l6" met="3" type="+-">Geensins syne <rhyme label="b" type="f" corresp="#s13:l2">ooren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="+-">Dies ick seer <rhyme label="a" type="m">ontrust</rhyme></l>
        <l id="s14:l2" met="3" type="+-">Lietse heenen <rhyme label="b" type="f">treden</rhyme></l>
        <l id="s14:l3" met="3" type="+-">In haer sotte <rhyme label="a" type="m" corresp="#s14:l1">lust</rhyme>,</l>
        <l id="s14:l4" met="3" type="+-">Om te doen <rhyme label="c" type="m">onvroet</rhyme></l>
        <l id="s14:l5" met="3" type="+-">Al wat dochte <rhyme label="c" type="m" corresp="#s14:l4">goet</rhyme></l>
        <l id="s14:l6" met="3" type="+-">Haer verblinde <rhyme label="b" type="f" corresp="#s14:l2">reden</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="+-">Och! had <rhyme label="a" type="m">Israël</rhyme></l>
        <l id="s15:l2" met="3" type="+-">My gehoor <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s15:l3" met="3" type="+-">Volgens mijn <rhyme label="a" type="m" corresp="#s15:l1">bevel</rhyme>!</l>
        <l id="s15:l4" met="3" type="+-">En waer op den <rhyme label="c" type="m">pat</rhyme></l>
        <l id="s15:l5" met="3" type="+-">Dien hy voormaels <rhyme label="c" type="m" corresp="#s15:l4">trat</rhyme></l>
        <l id="s15:l6" met="3" type="+-">Trouwelijck <rhyme label="b" type="f" corresp="#s15:l2">gebleven</rhyme>!</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="+-">Ick sou haest <rhyme label="a" type="m">verdaen</rhyme></l>
        <l id="s16:l2" met="3" type="+-">Hebben syn <rhyme label="b" type="f">vyanden</rhyme>.</l>
        <l id="s16:l3" met="3" type="+-">Mijn hant sterck int <rhyme label="a" type="m" corresp="#s16:l1">slaen</rhyme></l>
        <l id="s16:l4" met="3" type="+-">Had in desen <rhyme label="c" type="m">val</rhyme></l>
        <l id="s16:l5" met="3" type="+-">Syne haters <rhyme label="c" type="m" corresp="#s16:l4">al</rhyme></l>
        <l id="s16:l6" met="3" type="+-">Omgebracht met <rhyme label="b" type="f" corresp="#s16:l2">schanden</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="+-">Godes <rhyme label="a" type="m">weerparty</rhyme></l>
        <l id="s17:l2" met="3" type="+-">Soude druck <rhyme label="b" type="f">bedrijven</rhyme>,</l>
        <l id="s17:l3" met="3" type="+-">En in <rhyme label="a" type="m" corresp="#s17:l1">huychely</rhyme></l>
        <l id="s17:l4" met="3" type="+-">Voor hem vallen <rhyme label="c" type="m">neer</rhyme>.</l>
        <l id="s17:l5" met="3" type="+-">Maer der vromen <rhyme label="c" type="m" corresp="#s17:l4">eer</rhyme></l>
        <l id="s17:l6" met="3" type="+-">Eeuwich soude <rhyme label="b" type="f" corresp="#s17:l2">blijven</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="+-">Tarwe wit en <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s18:l2" met="3" type="+-">Hadden sy <rhyme label="b" type="f">genoten</rhyme>,</l>
        <l id="s18:l3" met="3" type="+-">En den honich <rhyme label="a" type="m" corresp="#s18:l1">soet</rhyme></l>
        <l id="s18:l4" met="3" type="+-">Wt de rotse <rhyme label="c" type="m">hooch</rhyme></l>
        <l id="s18:l5" met="3" type="+-">Voor eens yders <rhyme label="c" type="m" corresp="#s18:l4">oock</rhyme></l>
        <l id="s18:l6" met="3" type="+-">Ware haer <rhyme label="b" type="f" corresp="#s18:l2">gevloten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
