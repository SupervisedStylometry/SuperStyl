  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LAet ons vrij met <rhyme label="a" type="f">corage</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Uerhueghen in den <rhyme label="b" type="m">gheest</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Al lijden wy <rhyme label="a" type="f" corresp="#s1:l1">domage</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Droefheyt en swaer <rhyme label="b" type="m" corresp="#s1:l2">tempeest</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Den druck sal weynich <rhyme label="c" type="f">dueren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die wy al hier <rhyme label="c" type="f" corresp="#s1:l5">besueren</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Deynde is <rhyme label="d" type="m">gheopenbaert</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dan sullen ons <rhyme label="c" type="f" corresp="#s1:l5">dolueren</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Ons schreyen en ons <rhyme label="c" type="f" corresp="#s1:l5">trueren</rhyme></l>
        <l id="s1:l10" met="3" type="-+">In vruecht worden <rhyme label="d" type="m" corresp="#s1:l7">verclaert</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een vrouwe in barens <rhyme label="a" type="f">pijne</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Uerkrijcht grief en <rhyme label="b" type="m">doluer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Maer als sulcks is ten <rhyme label="a" type="f" corresp="#s2:l1">fijne</rhyme></l>
        <l id="s2:l4" met="3" type="-+">veruruecht sy haers <rhyme label="b" type="m" corresp="#s2:l2">ghetruer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Also sal ons <rhyme label="c" type="f">droefhede</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Bedect zijn door den <rhyme label="c" type="f" corresp="#s2:l5">vrede</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Die Christus heeft <rhyme label="d" type="m">bereyt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Want zijn belofte en <rhyme label="c" type="f" corresp="#s2:l5">rede</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Is waer op elcker <rhyme label="c" type="f" corresp="#s2:l5">stede</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Die ons is <rhyme label="d" type="m" corresp="#s2:l7">toegheseyt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sijn beloften zijn <rhyme label="a" type="f">warachtich</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Uoor alle tijt <rhyme label="b" type="m">ghesien</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dies laet ons tvleesch <rhyme label="a" type="f" corresp="#s3:l1">bedachtich</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Om Christus name <rhyme label="b" type="m" corresp="#s3:l2">bien</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Want ons verrot <rhyme label="c" type="f">lichame</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat sal in synen <rhyme label="c" type="f" corresp="#s3:l5">name</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Uerkeeren <rhyme label="d" type="m">glorieus</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In een wesen <rhyme label="c" type="f" corresp="#s3:l5">bequame</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Een heerrlickheyt <rhyme label="c" type="f" corresp="#s3:l5">eersame</rhyme></l>
        <l id="s3:l10" met="3" type="-+">So Paulus tuycht <rhyme label="d" type="m" corresp="#s3:l7">Ioyieus</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghelijck den natten <rhyme label="a" type="f">dauwe</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wtspruyten doet het <rhyme label="b" type="m">kruyt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">So sal ons vleesch in <rhyme label="a" type="f" corresp="#s4:l1">trouwe</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Uercrijghen nieuw <rhyme label="b" type="m" corresp="#s4:l2">virtuyt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Uerchiert in een nieuw <rhyme label="c" type="f">leuen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dwelck ons Christus sal <rhyme label="c" type="f" corresp="#s4:l5">gheuen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Diet ons verworuen <rhyme label="d" type="m">heeft</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Daer op wy hopen <rhyme label="c" type="f" corresp="#s4:l5">beneuen</rhyme></l>
        <l id="s4:l9" met="3" type="-+">En hope so er staet <rhyme label="c" type="f" corresp="#s4:l5">gheschreuen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Laet niemant beshaemt die <rhyme label="d" type="m" corresp="#s4:l7">leeft</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het graen ghesaeyt in <rhyme label="a" type="f">deerde</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Uerrot somen <rhyme label="b" type="m">aenschout</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Daer na brenghet met <rhyme label="a" type="f" corresp="#s5:l1">weerde</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Sijn vruchtkens <rhyme label="b" type="m" corresp="#s5:l2">menichfout</rhyme></l>
        <l id="s5:l5" met="3" type="-+">So sullen wy <rhyme label="c" type="f">verrijsen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Door vruchtkens lof <rhyme label="c" type="f" corresp="#s5:l5">bewijsen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Christus den sayer <rhyme label="d" type="m">klaer</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En ons druckich <rhyme label="c" type="f" corresp="#s5:l5">afgrijsen</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Dat sal dan <rhyme label="c" type="f" corresp="#s5:l5">veriolijsen</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Heerlick in vruechden <rhyme label="d" type="m" corresp="#s5:l7">gaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Uerhuecht O kinders <rhyme label="a" type="f">cleyne</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Christus is <rhyme label="b" type="m">opghestaen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">En wy zijn leden <rhyme label="a" type="f" corresp="#s6:l1">reyne</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Uerwachten tselue <rhyme label="b" type="m" corresp="#s6:l2">opgaen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Want daer thooft is <rhyme label="c" type="f">gheresen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Daer moeten de leden <rhyme label="c" type="f" corresp="#s6:l5">wesen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Sou dlichaem zijn <rhyme label="d" type="m">volmaeckt</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Dies weest ghetroost in <rhyme label="c" type="f" corresp="#s6:l5">desen</rhyme></l>
        <l id="s6:l9" met="3" type="-+">O lieue kinders <rhyme label="c" type="f" corresp="#s6:l5">ghepresen</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Want ons triumphe <rhyme label="d" type="m" corresp="#s6:l7">naect</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Prince ghy Godes <rhyme label="a" type="f">kinders</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Die hier verworpen <rhyme label="b" type="m">zijt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Strijt vrij weest <rhyme label="a" type="f" corresp="#s7:l1">ouerwinders</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Teghen vleesch en bloet <rhyme label="b" type="m" corresp="#s7:l2">altijt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Merckt de beloften <rhyme label="c" type="f">schoone</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Die ons door Godes <rhyme label="c" type="f" corresp="#s7:l5">soone</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Gheschoncken zijn ter <rhyme label="d" type="m">vruecht</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Denckt hoe wy sullen ten <rhyme label="c" type="f" corresp="#s7:l5">loone</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Uerweruen sleuens <rhyme label="c" type="f" corresp="#s7:l5">kroone</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Door Christus louter <rhyme label="d" type="m" corresp="#s7:l7">duecht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
