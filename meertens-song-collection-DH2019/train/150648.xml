    <melody>Psalm 135 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KNechten Godts, verkoren <rhyme label="a" type="m">tal</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wien de salen zijn <rhyme label="b" type="m">vertrouwt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Daer de Heer sijn wooning <rhyme label="b" type="m" corresp="#s1:l2">houdt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Doet den Heere <rhyme label="a" type="m" corresp="#s1:l1">loff-geschal</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Prijst hem. want hy is ja <rhyme label="c" type="m">goedt</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">Singt hem. want sijn naem is <rhyme label="c" type="m" corresp="#s1:l5">soet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Iacob heeft hy tot sijn <rhyme label="a" type="m">deel</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Isrel heeft de milde <rhyme label="b" type="m">Godt</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Tot sijn eygen erff en <rhyme label="b" type="m" corresp="#s2:l2">lot</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Sich verkoren uyt soo <rhyme label="a" type="m" corresp="#s2:l1">veel</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Groot de Heer; (’tleyt klaer in <rhyme label="c" type="m">my</rhyme>)</l>
        <l id="s2:l6" met="4" type="+-">En geen Godtheydt heefter <rhyme label="c" type="m" corresp="#s2:l5">by</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Als sijn wil soo is sijn <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Beyde reyckens’ even <rhyme label="b" type="m">veer</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Niets en is den grooten <rhyme label="b" type="m" corresp="#s3:l2">Heer</rhyme>’</l>
        <l id="s3:l4" met="4" type="+-">Niets en is hem boven <rhyme label="a" type="m" corresp="#s3:l1">kracht</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Niets in hemel, aerde, <rhyme label="c" type="m">zee</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Dat sijn handt oyt sluyten <rhyme label="c" type="m" corresp="#s3:l5">de’e</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy is’t die van ’swereldts <rhyme label="a" type="m">endt</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Vochte dampen stijgen <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die den fellen blixem <rhyme label="b" type="m" corresp="#s4:l2">voedt</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Die op aerde regen <rhyme label="a" type="m" corresp="#s4:l1">sendt</rhyme></l>
        <l id="s4:l5" met="4" type="+-">En uyt een verborgen <rhyme label="c" type="m">oordt</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Brenght geswinde winden <rhyme label="c" type="m" corresp="#s4:l5">voort</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Die de eerst-geboorte <rhyme label="a" type="m">sloegh</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Sloegh en dood’ aen mensch en <rhyme label="b" type="m">beest</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Sloegh en dood’ in minst en <rhyme label="b" type="m" corresp="#s5:l2">meest</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Dien het gantsche Rahab <rhyme label="a" type="m" corresp="#s5:l1">droegh</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Die het God-blijck sijner <rhyme label="c" type="m">handt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dy, Egipt, soo duydlijck <rhyme label="c" type="m" corresp="#s5:l5">sandt</rhyme>:</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dy, Egipt, en <rhyme label="a" type="m">Pharao</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Met sijn adelijck <rhyme label="b" type="m">gesind</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">(’tOogh in eygen dwaesheydt <rhyme label="b" type="m" corresp="#s6:l2">blindt</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Siet somwijls al siettet <rhyme label="a" type="m" corresp="#s6:l1">noo</rhyme>’.)</l>
        <l id="s6:l5" met="4" type="+-">Die der volcken, door sijn <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Vele t’onder heeft <rhyme label="c" type="m" corresp="#s6:l5">gebracht</rhyme>:</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die soo menigh groot <rhyme label="a" type="m">tyran</rhyme></l>
        <l id="s7:l2" met="4" type="+-">(Hier een Sichon <rhyme label="b" type="m">Amorijt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Elders Og den <rhyme label="b" type="m" corresp="#s7:l2">Basanijt</rhyme>)</l>
        <l id="s7:l4" met="4" type="+-">Sloegh door ’tgantsche <rhyme label="a" type="m" corresp="#s7:l1">Canaan</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">En haer landt (na dese <rhyme label="c" type="m">straff</rhyme>)</l>
        <l id="s7:l6" met="4" type="+-">Aen sijn volck ten erve <rhyme label="c" type="m" corresp="#s7:l5">gaff</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Heere die de kleyne <rhyme label="a" type="m">spaert</rhyme></l>
        <l id="s8:l2" met="4" type="+-">En de grootsche <rhyme label="b" type="m">neder-smijt</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Groot, en nu en t’aller <rhyme label="b" type="m" corresp="#s8:l2">tijdt</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Is uw’ hoogen naem <rhyme label="a" type="m" corresp="#s8:l1">vermaert</rhyme></l>
        <l id="s8:l5" met="4" type="+-">Leeft by alle <rhyme label="c" type="m">na-geslacht</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Vwe glory in’t <rhyme label="c" type="m" corresp="#s8:l5">gedacht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Want de Heer (soo goedt is <rhyme label="a" type="m">hy</rhyme>)</l>
        <l id="s9:l2" met="4" type="+-">Sal eens treden ten <rhyme label="b" type="m">gericht</rhyme></l>
        <l id="s9:l3" met="4" type="+-">En sijns volcks bedroeft <rhyme label="b" type="m" corresp="#s9:l2">gesicht</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Maken door sijn klaerheydt <rhyme label="a" type="m" corresp="#s9:l1">bly</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Als hem nu het <rhyme label="c" type="m">ongeval</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Sijner knechten rouwen <rhyme label="c" type="m" corresp="#s9:l5">sal</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Maer wat is van’t heydensch <rhyme label="a" type="m">beeldt</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">’tMonster, op een yd’len <rhyme label="b" type="m">Meyn</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">In het bygeloovigh <rhyme label="b" type="m" corresp="#s10:l2">breyn</rhyme></l>
        <l id="s10:l4" met="4" type="+-">uyt een wilden keur <rhyme label="a" type="m" corresp="#s10:l1">geteelt</rhyme>?</l>
        <l id="s10:l5" met="4" type="+-">Niet dan silver ofte <rhyme label="c" type="m">goudt</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Van een swacken mensch <rhyme label="c" type="m" corresp="#s10:l5">gebouwt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Komt eens met uw’ Goden <rhyme label="a" type="m">voort</rhyme>:</l>
        <l id="s11:l2" met="4" type="+-">Daer is mondt, maer sonder <rhyme label="b" type="m">tael</rhyme>;</l>
        <l id="s11:l3" met="4" type="+-">Daer is oogh, maer sonder <rhyme label="b" type="m" corresp="#s11:l2">strael</rhyme>;</l>
        <l id="s11:l4" met="4" type="+-">Daer is oor, maer die niet <rhyme label="a" type="m" corresp="#s11:l1">hoort</rhyme>;</l>
        <l id="s11:l5" met="4" type="+-">Daer is (wat’s de mensch <rhyme label="c" type="m">verbeest’t</rhyme>!)</l>
        <l id="s11:l6" met="4" type="+-">Wind noch kracht die ’tlijff <rhyme label="c" type="m" corresp="#s11:l5">begeeft’t</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Dwaesheydt zy haer selfs een <rhyme label="a" type="m">loon</rhyme>;</l>
        <l id="s12:l2" met="4" type="+-">O! (het is een rechte <rhyme label="b" type="m">wensch</rhyme>)</l>
        <l id="s12:l3" met="4" type="+-">O! dat yeder ydel <rhyme label="b" type="m" corresp="#s12:l2">mensch</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Maker van alsulcke <rhyme label="a" type="m" corresp="#s12:l1">Goon</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">En al wiese neemt ten <rhyme label="c" type="m">wijck</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Sijnen dooden Goon <rhyme label="c" type="m" corresp="#s12:l5">gelijck</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Isr’els huys bemint <rhyme label="a" type="m">geslacht</rhyme>;</l>
        <l id="s13:l2" met="4" type="+-">Arons huys, gewijdde <rhyme label="b" type="m">stam</rhyme>;</l>
        <l id="s13:l3" met="4" type="+-">Levijs huys daer hy uyt <rhyme label="b" type="m" corresp="#s13:l2">quam</rhyme>;</l>
        <l id="s13:l4" met="4" type="+-">Vrome zielen, die Godts <rhyme label="a" type="m" corresp="#s13:l1">acht</rhyme></l>
        <l id="s13:l5" met="4" type="+-">En hem vreest uyt reynen <rhyme label="c" type="m">grondt</rhyme>:</l>
        <l id="s13:l6" met="4" type="+-">Naeckt den Heer met <rhyme label="c" type="m" corresp="#s13:l5">zegen-mondt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Hoogh-gezegent zy de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Hoogh-gelooft van alle <rhyme label="b" type="m">tong</rhyme></l>
        <l id="s14:l3" met="4" type="+-">Die oyt gunst van hem <rhyme label="b" type="m" corresp="#s14:l2">ontfong</rhyme>.</l>
        <l id="s14:l4" met="4" type="+-">Hem zy alle loff en <rhyme label="a" type="m" corresp="#s14:l1">eer</rhyme></l>
        <l id="s14:l5" met="4" type="+-">Die in Sion wooning <rhyme label="c" type="m">heeft</rhyme>.</l>
        <l id="s14:l6" met="4" type="+-">Loff den Heer, die eeuwigh <rhyme label="c" type="m" corresp="#s14:l5">leeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
