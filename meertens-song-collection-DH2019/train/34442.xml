      <category translation="religion">godsdienst</category>
    </categories>
  <author>Godfried Bussé</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IEsu der Maeghden <rhyme label="a" type="f">kroone</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En Bruydegom en <rhyme label="b" type="m">Heer</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Ontfanght in uwen <rhyme label="a" type="f" corresp="#s1:l1">throone</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Den lof der Maeghden <rhyme label="b" type="m" corresp="#s1:l2">teer</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Want ghy sijt hun <rhyme label="c" type="f">behoeder</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Gheweest in het <rhyme label="d" type="m">torment</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Een maeght was oock u <rhyme label="c" type="f" corresp="#s1:l5">Moeder</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die verplet heeft <rhyme label="d" type="m" corresp="#s1:l6">t'serpent</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Maeght hebt ghy <rhyme label="a" type="f">verkoren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Gheheeten <rhyme label="b" type="m">Margariet</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Uyt Edel-stam <rhyme label="a" type="f" corresp="#s2:l1">gheboren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Maer eelderd' in't <rhyme label="b" type="m" corresp="#s2:l2">verdriet</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Edisius haer <rhyme label="c" type="f">Vaeder</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">D'afgoden was <rhyme label="d" type="m">verplicht</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Maer dese Maeghet <rhyme label="c" type="f" corresp="#s2:l5">naerder</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Van Christo was <rhyme label="d" type="m" corresp="#s2:l6">verlicht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Den Vorst van <rhyme label="a" type="f">Orienten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Olibrius <rhyme label="b" type="m">ghenaemt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Haer in sijn hert ghinck <rhyme label="a" type="f" corresp="#s3:l1">prenten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Door haer schoonheyt <rhyme label="b" type="m" corresp="#s3:l2">ghepraemt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Belooft haer al sijn <rhyme label="c" type="f">rijcken</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En schatten wonder <rhyme label="d" type="m">veel</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Woud' sy van Christo <rhyme label="c" type="f" corresp="#s3:l5">wijcken</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Sijn huys-vrouw' wesen <rhyme label="d" type="m" corresp="#s3:l6">eel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doen sprack de Maeghet <rhyme label="a" type="f">kloecke</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Jck Jesum mijnen <rhyme label="b" type="m">Heer</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Sal nimmermeer <rhyme label="a" type="f" corresp="#s4:l1">vervloecke</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maer dienen meer en <rhyme label="b" type="m" corresp="#s4:l2">meer</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Sijn wet ick <rhyme label="c" type="f">onder-houwe</rhyme></l>
        <l id="s4:l6" met="3" type="-+">T'gheloof ick niet <rhyme label="d" type="m">verlaet</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Jck sal hem sijn <rhyme label="c" type="f" corresp="#s4:l5">ghetrouwe</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Als een goet <rhyme label="d" type="m" corresp="#s4:l6">ondersaet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Denckt hoe ghy my wilt <rhyme label="a" type="f">quellen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met pijnen oft <rhyme label="b" type="m">verdriet</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Godts lof sal ick <rhyme label="a" type="f" corresp="#s5:l1">vertellen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">K'en acht u schatten <rhyme label="b" type="m" corresp="#s5:l2">niet</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Hy sal my rijcken <rhyme label="c" type="f">gheven</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die hier al dwinght dat <rhyme label="d" type="m">leeft</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Ja die de hel doet <rhyme label="c" type="f" corresp="#s5:l5">beven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En die ons t'leven <rhyme label="d" type="m" corresp="#s5:l6">gheeft</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Doen wirt verscheurt <rhyme label="a" type="f">alomme</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Haer lichaem tot het <rhyme label="b" type="m">been</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Die suyver schoone <rhyme label="a" type="f" corresp="#s6:l1">blomme</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Bleef staen op vasten <rhyme label="b" type="m" corresp="#s6:l2">steen</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">De vorcken, nagh'len, <rhyme label="c" type="f">haecken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Vier, lampen plaeten <rhyme label="d" type="m">heet</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Al watmen konden <rhyme label="c" type="f" corresp="#s6:l5">maecken</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">De Maeght verduldigh <rhyme label="d" type="m" corresp="#s6:l6">leet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Doen wirt t'ghebodt <rhyme label="a" type="f">ghegheven</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Datmen haer soud' <rhyme label="b" type="m">terstont</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Benemen gaen het <rhyme label="a" type="f" corresp="#s7:l1">leven</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">T'welck wirt ghedaen <rhyme label="b" type="m" corresp="#s7:l2">terstont</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ghegaen is sy in <rhyme label="c" type="f">vreden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Gheslaghen met het <rhyme label="d" type="m">sweert</rhyme>;</l>
        <l id="s7:l7" met="3" type="-+">Heeft den Tyran <rhyme label="c" type="f" corresp="#s7:l5">betreden</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Verwonnen <rhyme label="d" type="m" corresp="#s7:l6">onverveert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O Maeght! ô strijdbaer <rhyme label="a" type="f">vrouwe</rhyme>!</l>
        <l id="s8:l2" met="3" type="-+">O peerel! eel <rhyme label="b" type="m">ghesteent</rhyme>!</l>
        <l id="s8:l3" met="3" type="-+">Ghy bleeft den Heer <rhyme label="a" type="f" corresp="#s8:l1">ghetrouwe</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">En sijt met hem <rhyme label="b" type="m" corresp="#s8:l2">vereent</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Wie u aenroept in <rhyme label="c" type="f">lijden</rhyme>;</l>
        <l id="s8:l6" met="3" type="-+">Jn noodt wat hem <rhyme label="d" type="m">behoeft</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Ontfanght ghy sonder <rhyme label="c" type="f" corresp="#s8:l5">bijden</rhyme>;</l>
        <l id="s8:l8" met="3" type="-+">Ghy niemandt laet <rhyme label="d" type="m" corresp="#s8:l6">bedroeft</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
