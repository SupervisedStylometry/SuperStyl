<song>
  <meta>
    <id>40790</id>
    <date>
      <year-start>1681</year-start>
      <year-end>1681</year-end>
    </date>
    <location>
      <orginal-name>leeuwarden</orginal-name>
      <standard-name>Leeuwarden, The Netherlands</standard-name>
      <coordinates>
        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 097 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Gysbert Japicx</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NU de heagste' HEER' stiet to <rhyme label="a" type="m">roer'</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Az Haed-rejinte, in <rhyme label="a" type="m" corresp="#s1:l1">stjoer'</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Oer 't wirck fenne' heel' <rhyme label="b" type="f">Natuwre</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Iz 't all' yn s jong-bly' <rhyme label="b" type="f" corresp="#s1:l3">nuwre</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Nu 's de yerd' trog-blier in <rhyme label="c" type="m">blier</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Yn laeyts-wille', yn <rhyme label="c" type="m" corresp="#s1:l5">wol-tier'</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Nu 's all' (trog See in <rhyme label="d" type="m">sânn'</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Fenn' wrade' oef-schaete') <rhyme label="d" type="m" corresp="#s1:l7">Eylann'</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Yn seyn' juwch', let in <rhyme label="c" type="m" corresp="#s1:l5">yer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ruwne om him wolck' swirck' <rhyme label="a" type="m">swomm't</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat him ynn' tjuest're' om <rhyme label="a" type="m" corresp="#s2:l1">momm't</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">In (ô wond're'!) uwt dat <rhyme label="b" type="f">tjuest're</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Gloar't sijn gerjuechtheyts <rhyme label="b" type="f" corresp="#s2:l3">ljuest're</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Dear bleackt sijn rjuechts glanze' <rhyme label="c" type="m">uwt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dear trog hy hecht'-fest <rhyme label="c" type="m" corresp="#s2:l5">sluwt't</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Sijn, oer-all'-troanen, <rhyme label="d" type="m">Troan'</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Gleon' fjoer giet Him fore <rhyme label="d" type="m" corresp="#s2:l7">oon</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Dat all' sijn sijnn' <rhyme label="c" type="m" corresp="#s2:l5">sry-buwt't</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sijn blix'me' oer-ljeacht't de' heel' <rhyme label="a" type="m">wrâd'</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dy dwelmt, fen kjealtme', <rhyme label="a" type="m" corresp="#s3:l1">yz-kàd</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">'t Bergt' raent az waegs, waems <rhyme label="b" type="f">Holl'-ljea</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Fen bopp'ne' oef ploffje', ynn' <rhyme label="b" type="f" corresp="#s3:l3">dol-Sea</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Az 't HEER'NE' hird <rhyme label="c" type="m">eansicht'-soer</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Jearme' oon-grijmm't grauw in <rhyme label="c" type="m" corresp="#s3:l5">stjoer</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">d'HEER' dy d'wrâd fet in <rhyme label="d" type="m">klaeyt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In, azynn' knieren <rhyme label="d" type="m" corresp="#s3:l7">draeyt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Fenne' oon-fangge', oere' oon <rhyme label="c" type="m" corresp="#s3:l5">oer'</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Salm' de hijmm'len kund'gje uwt <rhyme label="a" type="m">sijnn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Gloed'gouwn' gerjuechtheyts <rhyme label="a" type="m" corresp="#s4:l1">schijnn'</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wier uwt d'ljue, loayts-ljeacht, <rhyme label="b" type="f">leere</rhyme></l>
        <l id="s4:l4" met="3" type="-+">d'Gleon'-gloar'-glanz' fen Goads <rhyme label="b" type="f" corresp="#s4:l3">eere</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Jae moatte' all schamm'-rea <rhyme label="c" type="m">stean</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dy bijlld'ne' eer-tjienste' <rhyme label="c" type="m" corresp="#s4:l5">oon-dwaen'</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">In pochje' op de' O <rhyme label="d" type="m">Ef-goad'-popp'</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">Buwg', bock', dol-duwckje' ynn' <rhyme label="d" type="m" corresp="#s4:l7">dopp'</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">O goaden for Goads <rhyme label="c" type="m" corresp="#s4:l5">Trean'</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dit wicht'-Wird (ney <rhyme label="a" type="m">bejeart'</rhyme>)</l>
        <l id="s5:l2" met="3" type="-+">Het Syon blier <rhyme label="a" type="m" corresp="#s5:l1">oon-heard</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Dy Goad'-sill'g' <rhyme label="b" type="f">Frouwne-raeye</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Fen Juwdae Doayt'ren, <rhyme label="b" type="f" corresp="#s5:l3">baeyde</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ynne' hymmelsch' <rhyme label="c" type="m">swieterny'</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Asse', ynn' herts-oertinss', <rhyme label="c" type="m" corresp="#s5:l5">bly</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Uwt jon wirds swiet-wiet <rhyme label="d" type="m">diep't</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Jon oord lens on-gruwn' <rhyme label="d" type="m" corresp="#s5:l7">djiept'</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">O witt'nisse oer <rhyme label="c" type="m" corresp="#s5:l5">wirdy'</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O Allerheagste' <rhyme label="a" type="m">oer-HEER'</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">Jon heechheyte' het nin <rhyme label="a" type="m" corresp="#s6:l1">keer'</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Mar giet (ô <rhyme label="b" type="f">heachheyts-Sinne</rhyme>!)</l>
        <l id="s6:l4" met="3" type="-+">Oere' alle' Yerdz om-gong' <rhyme label="b" type="f" corresp="#s6:l3">hinne</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Het aern' Goad'-preall'-Namm' <rhyme label="c" type="m">bruwck't</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Djiep, for jon heaght', <rhyme label="c" type="m" corresp="#s6:l5">dol-duwck't</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">Jon heachheyt' buwcket <rhyme label="d" type="m">dol</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Het heag, trog heag moed', <rhyme label="d" type="m" corresp="#s6:l7">swol</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">'t Heag' wirt, trog jon heagt', <rhyme label="c" type="m" corresp="#s6:l5">fnuwck't</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Trouw' fromm' ljeafhabbers <rhyme label="a" type="m">Goads</rhyme></l>
        <l id="s7:l2" met="3" type="-+">(Oppe' HEER' festge', az opp' <rhyme label="a" type="m" corresp="#s7:l1">Roats'</rhyme>)</l>
        <l id="s7:l3" met="3" type="-+">Litt' litte' yn wol-dwaen <rhyme label="b" type="f">blijckje</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dat jiemme' fen 't quea <rhyme label="b" type="f" corresp="#s7:l3">werz-wijckje</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">d'HEER tjocht sijn <rhyme label="c" type="m">gunst'-genoat'</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Uwtt' schoer <rhyme label="c" type="m" corresp="#s7:l5">sjeack'-neyll-kloer'-poat'</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Uwtt' kners-knijpp'-knoarr'-kneag' <rhyme label="d" type="m">tann'</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Uwtt' goadleaz' schelmsch' <rhyme label="d" type="m" corresp="#s7:l7">moard'-hann'</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Dy dolckjet ney sijn <rhyme label="c" type="m" corresp="#s7:l5">stroat'</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Uwt uwnck het de' HEERE' him <rhyme label="a" type="m">teyn'</rhyme>.</l>
        <l id="s8:l2" met="3" type="-+">Dear boppa wirt Goads <rhyme label="a" type="m" corresp="#s8:l1">seyn'</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Oere' oprjuecht' rom <rhyme label="b" type="f">uwt-spried'ge</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">Him wirt ljeachts glanz'-will <rhyme label="b" type="f" corresp="#s8:l3">sied'gje</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Dit lock, spoed', wol-feart' <rhyme label="c" type="m">sied</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Eynling' de' oprjuechte' <rhyme label="c" type="m" corresp="#s8:l5">yn-mie't</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Wezz' bly rjuechtfird'ge <rhyme label="d" type="m">ljue</rhyme>.</l>
        <l id="s8:l8" met="3" type="-+">Lof sjong de' Heere', yn <rhyme label="d" type="m" corresp="#s8:l7">herts-njue</rhyme>.</l>
        <l id="s8:l9" met="3" type="-+">Sijn Heyl'gheyte huwgg'tanck <rhyme label="c" type="m" corresp="#s8:l5">bied'</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
