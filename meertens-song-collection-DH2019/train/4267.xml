<song>
  <meta>
    <id>4267</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Minzame grond</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="emotions">emoties</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">POÃ¯soenich strael, Dootlick <rhyme label="a" type="m">fenijn</rhyme> //</l>
        <l id="s1:l2" met="4" type="-+">gepijn // Deur siele, lijf, en <rhyme label="b" type="f">leden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Aldus mijns liefs, Verbannen t'zijn // hout <rhyme label="a" type="m" corresp="#s1:l1">mijn</rhyme> //</l>
        <l id="s1:l4" met="4" type="-+">Mistroostich hert t'onvre- <rhyme label="b" type="f" corresp="#s1:l2">den</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Ten helpt by <rhyme label="b" type="f" corresp="#s1:l2">reden</rhyme> //</l>
        <l id="s1:l6" met="2" type="-+">Gesmeert <rhyme label="b" type="f" corresp="#s1:l2">gebeden</rhyme> //</l>
        <l id="s1:l7" met="4" type="-+">Ick ligge vast  int leet // <rhyme label="c" type="m">bedwelmt</rhyme> //</l>
        <l id="s1:l8" met="2" type="+-">Sy heeft my <rhyme label="b" type="f" corresp="#s1:l2">heden</rhyme> //</l>
        <l id="s1:l9" met="2" type="-+">So <rhyme label="b" type="f" corresp="#s1:l2">overstreden</rhyme> //</l>
        <l id="s1:l10" met="4" type="-+">En staet noch e- ven wreet // <rhyme label="c" type="m" corresp="#s1:l7">gehelmt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ialours gepeyns, ontseght my <rhyme label="a" type="m">voort</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Een woort // troostbaer t'mijnen <rhyme label="b" type="f">vermake</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Verclaert haer oogen t'zijn <rhyme label="a" type="m" corresp="#s2:l1">gestoort</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Tot moort // en ongehoorde <rhyme label="b" type="f" corresp="#s2:l2">wrake</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Ben ick <rhyme label="b" type="f" corresp="#s2:l2">doorsake</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Ten gelt <rhyme label="b" type="f" corresp="#s2:l2">voorsprake</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Blijct waerheyt van mijn slecht // <rhyme label="c" type="m">bestaen</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Dat my de <rhyme label="b" type="f" corresp="#s2:l2">Drake</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Voetstaens <rhyme label="b" type="f" corresp="#s2:l2">ontschake</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">My werde meer dan recht // <rhyme label="c" type="m" corresp="#s2:l7">gedaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Eylacen neen! dat weet ick <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Niet el // versocht, dan t'point der <rhyme label="b" type="f">eeren</rhyme></l>
        <l id="s3:l3" met="4" type="-+">En heb ick oyt, waer toe <rhyme label="a" type="m" corresp="#s3:l1">rebel</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Dus fel // mijn edel hert <rhyme label="b" type="f" corresp="#s3:l2">matteeren</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Mach mijn <rhyme label="b" type="f" corresp="#s3:l2">verweeren</rhyme></l>
        <l id="s3:l6" met="2" type="-+">My <rhyme label="b" type="f" corresp="#s3:l2">excuseeren</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">T'wert noch, dat my haer tonge // <rhyme label="c" type="m">croont</rhyme>:</l>
        <l id="s3:l8" met="2" type="+-">Ick wil u <rhyme label="b" type="f" corresp="#s3:l2">sweeren</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Mijn Doende <rhyme label="b" type="f" corresp="#s3:l2">leeren</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">En blijft niet eewich onge- // <rhyme label="c" type="m" corresp="#s3:l7">loont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">T versuchten naer den tijt <rhyme label="a" type="m">wanneer</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Cost meer // dan gans de zee vol <rhyme label="b" type="f">tranen</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Hope seght wel, troost v <rhyme label="a" type="m" corresp="#s4:l1">iong-heer</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">T'sal eer // geschien, dan ghy sult <rhyme label="b" type="f" corresp="#s4:l2">wanen</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">Maer sulck <rhyme label="b" type="f" corresp="#s4:l2">vernamen</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Stoot buyter <rhyme label="b" type="f" corresp="#s4:l2">banen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Wat tot genesen langhst // <rhyme label="c" type="m">verstrect</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Noyt <rhyme label="b" type="f" corresp="#s4:l2">Courtesanen</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Noch <rhyme label="b" type="f" corresp="#s4:l2">Onderdanen</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">En stonden meerder angst // <rhyme label="c" type="m" corresp="#s4:l7">subiect</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">En of sy mijnen dienst acht <rhyme label="a" type="m">cleen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gemeen // ist dragen sulcken <rhyme label="b" type="f">rouwe</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Verduldich smelt ick mijn <rhyme label="a" type="m" corresp="#s5:l1">geween</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Als een // gestadich knecht <rhyme label="b" type="f" corresp="#s5:l2">getrouwe</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Dat ick <rhyme label="b" type="f" corresp="#s5:l2">verflouwe</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Om die <rhyme label="b" type="f" corresp="#s5:l2">kersouwe</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">T'verdient sulck een divyne // <rhyme label="c" type="m">ieught</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Als ick <rhyme label="b" type="f" corresp="#s5:l2">aenschouwe</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Die schoone <rhyme label="b" type="f" corresp="#s5:l2">vrouwe</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Soo dunct my al mijn pijne // <rhyme label="c" type="m" corresp="#s5:l7">vreught</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Redelick geest, nobel <rhyme label="a" type="m">Princier</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Int vier // ligh ick gevelt ter <rhyme label="b" type="f">slete</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Soo dat deur angst van mijn <rhyme label="a" type="m" corresp="#s6:l1">dangier</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Ick schier // mijn seluen gans <rhyme label="b" type="f" corresp="#s6:l2">vergete</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Ja! int <rhyme label="b" type="f" corresp="#s6:l2">secrete</rhyme></l>
        <l id="s6:l6" met="2" type="-+">Mijn tranen <rhyme label="b" type="f" corresp="#s6:l2">ete</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Omhelst met allerhande // <rhyme label="c" type="m">noot</rhyme>:</l>
        <l id="s6:l8" met="2" type="+-">Als ick (naer <rhyme label="b" type="f" corresp="#s6:l2">wete</rhyme>)</l>
        <l id="s6:l9" met="2" type="-+">Sulcx <rhyme label="b" type="f" corresp="#s6:l2">ouermete</rhyme>,</l>
        <l id="s6:l10" met="4" type="-+">Vlouck ick t'vertoeven vande // <rhyme label="c" type="m" corresp="#s6:l7">doot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
