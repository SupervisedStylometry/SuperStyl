  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">HOrt eens Klaesje <rhyme label="a" type="m">maet</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Wat ick u sal <rhyme label="b" type="f">singhen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Luystert <rhyme label="a" type="m" corresp="#s1:l1">Cameraet</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Dit wil ick <rhyme label="b" type="f" corresp="#s1:l2">bedinghen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">T’gheen ick segh is de <rhyme label="c" type="m">waerheyt</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Want daer van heb ick <rhyme label="c" type="m" corresp="#s1:l5">bescheyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Tryn Jans Nicht de <rhyme label="a" type="m">sloof</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Die gaet haer <rhyme label="b" type="f">verblijen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Want sy het <rhyme label="a" type="m" corresp="#s2:l1">t’gheloof</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Dat ick haer wil <rhyme label="b" type="f" corresp="#s2:l2">vrijen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Daerom is sy nu soo <rhyme label="c" type="m">praet</rhyme></l>
        <l id="s2:l6" met="4" type="+-">En beschimt my over de <rhyme label="c" type="m" corresp="#s2:l5">straet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Wel myn soete <rhyme label="a" type="m">Moer</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Wel het is te <rhyme label="b" type="f">bijster</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Hoe sou sulcken <rhyme label="a" type="m" corresp="#s3:l1">loer</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Sien na sulcken <rhyme label="b" type="f" corresp="#s3:l2">Vrijster</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Die het puyckje van de <rhyme label="c" type="m">straet</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Is daer sy alleene <rhyme label="c" type="m" corresp="#s3:l5">gaet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Ick was wel <rhyme label="a" type="m">ghesint</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Om by haer te <rhyme label="b" type="f">comen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Want sy was <rhyme label="a" type="m" corresp="#s4:l1">bemint</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Van myn t’haerder <rhyme label="b" type="f" corresp="#s4:l2">vromen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">‘T schaepje was het vroech en <rhyme label="c" type="m">laet</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Nu soo is het Meysje <rhyme label="c" type="m" corresp="#s4:l5">quaet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Wel wat of haer <rhyme label="a" type="m">deert</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Kan ick niet <rhyme label="b" type="f">bedencken</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Dat sy mijn <rhyme label="a" type="m" corresp="#s5:l1">blameert</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Of en ist maer <rhyme label="b" type="f" corresp="#s5:l2">wencken</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Door dien nu soo vreemden <rhyme label="c" type="m">quant</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Haer besoeckt wt <rhyme label="c" type="m" corresp="#s5:l5">Enghelant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Wel aen schoone <rhyme label="a" type="m">Maecht</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Laet campen dese <rhyme label="b" type="f">strijers</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Want ghy nu <rhyme label="a" type="m" corresp="#s6:l1">behaecht</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Wel vijf en dertich <rhyme label="b" type="f" corresp="#s6:l2">vrijers</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Soo my onlanghs is <rhyme label="c" type="m">vertelt</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Van een die u vaeck <rhyme label="c" type="m" corresp="#s6:l5">verselt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Hier in ben ick <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s7:l2" met="3" type="+-">By ghestelt als <rhyme label="b" type="f">ander</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Ach! wat groot <rhyme label="a" type="m" corresp="#s7:l1">verdriet</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Volcht als nu <rhyme label="b" type="f" corresp="#s7:l2">malcander</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Daerom stelt, ach stelt oock <rhyme label="c" type="m">myn</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Soo hebt ghy keur van drie <rhyme label="c" type="m" corresp="#s7:l5">dosijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Wel nu Klaesje <rhyme label="a" type="m">vrient</rhyme></l>
        <l id="s8:l2" met="3" type="+-">Dit sou wel een <rhyme label="b" type="f">Meysje</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Wesen dat jou <rhyme label="a" type="m" corresp="#s8:l1">dient</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Want saechdy haer een <rhyme label="b" type="f" corresp="#s8:l2">reysje</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">T’is het puyck van <rhyme label="c" type="m">Amsterdam</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Dat van Hooren eerst hier <rhyme label="c" type="m" corresp="#s8:l5">quam</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Gheluckich is <rhyme label="a" type="m">hy</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Die haer mach <rhyme label="b" type="f">gebeuren</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Wat ick om haer <rhyme label="a" type="m" corresp="#s9:l1">ly</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Verstaet sy door myn <rhyme label="b" type="f" corresp="#s9:l2">treuren</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Want de Koorts hadt myn <rhyme label="c" type="m">bevaen</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Maer sy is my weer <rhyme label="c" type="m" corresp="#s9:l5">ontgaen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Claesje voor het <rhyme label="a" type="m">lest</rhyme></l>
        <l id="s10:l2" met="3" type="+-">Dit Nimphe wilt doch <rhyme label="b" type="f">groeten</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Sy schuwt my als <rhyme label="a" type="m" corresp="#s10:l1">pest</rhyme></l>
        <l id="s10:l4" met="3" type="+-">Doch nummer ick myn <rhyme label="b" type="f" corresp="#s10:l2">voeten</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Af gaen sal stellen na haer <rhyme label="c" type="m">deur</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Want de Gansen staen daer <rhyme label="c" type="m" corresp="#s10:l5">veur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
