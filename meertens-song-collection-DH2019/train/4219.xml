<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4219</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Si c'est pour mon pucelage</melody>
  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VLecht van Lauwer, Tijm, en <rhyme label="a" type="f">Palmen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Eenen drie-gestrengden <rhyme label="b" type="m">Kans</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Tot een Kroon der Helden <rhyme label="b" type="m" corresp="#s1:l2">thans</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Vuld de ruyme-Lucht met <rhyme label="a" type="f" corresp="#s1:l1">Galmen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Huwd de blijdschap an de <rhyme label="c" type="m">vreugd</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Voegd de jonkheyt hy de <rhyme label="c" type="m" corresp="#s1:l5">jeugd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wringt een wrong om 't hoofd der <rhyme label="a" type="f">helden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Drie in een <rhyme label="b" type="m">gevlochten-kruyd</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Schreeuwd haar daden <rhyme label="b" type="m" corresp="#s2:l2">over-luyd</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Die haar Lijf, haar Leven <rhyme label="a" type="f" corresp="#s2:l1">stelden</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Voor de Vryheyt, van het <rhyme label="c" type="m">Land</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Daar Gods-Wijngaard is <rhyme label="c" type="m" corresp="#s2:l5">geplant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Die de Aarde (in het <rhyme label="a" type="f">Ronde</rhyme>)</l>
        <l id="s3:l2" met="4" type="+-">Staag om-zeylen: sonder <rhyme label="b" type="m">eynd'</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Daar de Son haar stralen <rhyme label="b" type="m" corresp="#s3:l2">seynd'</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">En 's Doods-Suster eerst <rhyme label="a" type="f" corresp="#s3:l1">verkonde</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dat haar <rhyme label="c" type="m">swarte-angesicht</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Sich verwisseld, in het <rhyme label="c" type="m" corresp="#s3:l5">licht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Voor der Nederlanden <rhyme label="a" type="f">Vryheyt</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Voor den Gods-dienst, <rhyme label="b" type="m">onbevlekt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Voor het geen' tot eere <rhyme label="b" type="m" corresp="#s4:l2">strekt</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Van de <rhyme label="a" type="f" corresp="#s4:l1">Goddelijke-Dryheyt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Leefd het <rhyme label="c" type="m">Oud-Batavia</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Ook in 't Goud-rijk <rhyme label="c" type="m" corresp="#s4:l5">Africa</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Holland: u <rhyme label="a" type="f">gevryde-krachten</rhyme></l>
        <l id="s5:l2" met="4" type="+-">In de leden <rhyme label="b" type="m">Negentien</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">(Wiens geweld men siet <rhyme label="b" type="m" corresp="#s5:l2">ontsien</rhyme>)</l>
        <l id="s5:l4" met="4" type="+-">Zijn de minste niet te <rhyme label="a" type="f" corresp="#s5:l1">achten</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Met haar Schépen op de <rhyme label="c" type="m">Zee</rhyme>:</l>
        <l id="s5:l6" met="4" type="+-">En haar sterkten in <rhyme label="c" type="m" corresp="#s5:l5">Guiné</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Daar (op 't Hoofd-Kasteel) <rhyme label="a" type="f">Ruychaver</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Het gebied (als <rhyme label="b" type="m">Generaal</rhyme>)</l>
        <l id="s6:l3" met="4" type="+-">Heeft, ter tijd van <rhyme label="b" type="m" corresp="#s6:l2">altemaal</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Wiens weer-dreunende <rhyme label="a" type="f" corresp="#s6:l1">gedaver</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">In de Rots den Weer-galm <rhyme label="c" type="m">tergd</rhyme>:</l>
        <l id="s6:l6" met="4" type="+-">En Mercuri vlerken <rhyme label="c" type="m" corresp="#s6:l5">vergd</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Dese heeft met <rhyme label="a" type="f">seven-Schepen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">(Dus beswangerd van <rhyme label="b" type="m">metaal</rhyme>)</l>
        <l id="s7:l3" met="4" type="+-">De Maranen <rhyme label="b" type="m" corresp="#s7:l2">altemaal</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Op de Goud-kust so <rhyme label="a" type="f" corresp="#s7:l1">benepen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Dat sy vloden: (in der <rhyme label="c" type="m">haast</rhyme>)</l>
        <l id="s7:l6" met="4" type="+-">Schandelijken, en <rhyme label="c" type="m" corresp="#s7:l5">ver-baasd</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Noyt en was den Hase <rhyme label="a" type="f">rasser</rhyme></l>
        <l id="s8:l2" met="4" type="+-">In het vlieden, voor den <rhyme label="b" type="m">Wind</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">(Eer hem sijn vervolger <rhyme label="b" type="m" corresp="#s8:l2">vind</rhyme>)</l>
        <l id="s8:l4" met="4" type="+-">Als dees' vlugten na <rhyme label="a" type="f" corresp="#s8:l1">Ang-kasser</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Snel verlatende <rhyme label="c" type="m">Axem</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Door de vrese van ons <rhyme label="c" type="m" corresp="#s8:l5">stem</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Daarom laat ons t' samen <rhyme label="a" type="f">streng'len</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Eenen krans van <rhyme label="b" type="m">Lauwer-blaân</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Daar meer and're-kruyden <rhyme label="b" type="m" corresp="#s9:l2">staan</rhyme>:</l>
        <l id="s9:l4" met="4" type="+-">Om die so door een te <rhyme label="a" type="f" corresp="#s9:l1">meng'len</rhyme>:</l>
        <l id="s9:l5" met="4" type="+-">Daar men Helden mee <rhyme label="c" type="m">bekroond</rhyme>:</l>
        <l id="s9:l6" met="4" type="+-">En noch groter deugden <rhyme label="c" type="m" corresp="#s9:l5">loond</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Dit's Ruychaver tot een <rhyme label="a" type="f">Ruyker</rhyme>:</l>
        <l id="s10:l2" met="4" type="+-">Die sijn Reuk, en <rhyme label="b" type="m">Soete-geur</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Spreyd de gansche-Wereld <rhyme label="b" type="m" corresp="#s10:l2">deur</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Als het lip're, <rhyme label="a" type="f" corresp="#s10:l1">malsse-suyker</rhyme>:</l>
        <l id="s10:l5" met="4" type="+-">Dat 't Gerucht van hem noyt <rhyme label="c" type="m">rust</rhyme>.</l>
        <l id="s10:l6" met="4" type="+-">En geen Dood sijn name <rhyme label="c" type="m" corresp="#s10:l5">kust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
