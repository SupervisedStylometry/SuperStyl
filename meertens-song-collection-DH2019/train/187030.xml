    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Onnoozelheit, op aard alom <rhyme label="a" type="f">verschoven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe arm van geest <rhyme label="b" type="m">geagt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Uw nedrigheit verdooft de <rhyme label="b" type="m" corresp="#s1:l2">pragt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Der koninklijke <rhyme label="a" type="f" corresp="#s1:l1">hoven</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Onnoozelheit van God <rhyme label="c" type="m">bemind</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Al leeft gy hier <rhyme label="a" type="f" corresp="#s1:l1">verschoven</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Daar waan-en werrelt wijsheit <rhyme label="c" type="m" corresp="#s1:l5">blind</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Uw luister tragt te <rhyme label="a" type="f" corresp="#s1:l1">doven</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Uw eygendom, Ã´ hemels <rhyme label="c" type="m" corresp="#s1:l5">kind</rhyme>!</l>
        <l id="s1:l10" met="3" type="-+">Is 't eewig hof hier <rhyme label="a" type="f" corresp="#s1:l1">boven</rhyme>.</l>
      </lg>
    </lg>
