  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">O Maghet, ô Maghet, ô roem van't Vrouw <rhyme label="a" type="m">geslacht</rhyme>!</l>
        <l id="s1:l2" met="2" type="-+">Die ons uyt <rhyme label="b" type="m">druck</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Hebt tot <rhyme label="b" type="m" corresp="#s1:l2">geluck</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Door uwen Soon <rhyme label="c" type="m">gebracht</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Die syt in d'hooghste <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Maeckt weerdigh ons noch <rhyme label="d" type="m" corresp="#s1:l5">meer</rhyme></l>
        <l id="s1:l7" met="4" type="+-">De gaeven, de gaeven van den <rhyme label="d" type="m" corresp="#s1:l5">Heer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Den Tempel, den Tempel van uwen Soon gy <rhyme label="a" type="m">syt</rhyme>;</l>
        <l id="s2:l2" met="2" type="-+">Voor ons <rhyme label="b" type="m">gesicht</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Syt gy het <rhyme label="b" type="m" corresp="#s2:l2">licht</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat ons van vrees <rhyme label="c" type="m">bevrydt</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Gy hebt des Hemels <rhyme label="d" type="m">baen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Voor ons doen <rhyme label="d" type="m" corresp="#s2:l5">open-gaen</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">En d'Helsche, en d'Helsche macht <rhyme label="d" type="m" corresp="#s2:l5">verslaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Gy Moeder, gy Moeder syt van die eeuwigh <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">U heeft <rhyme label="b" type="m">verciert</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Die t'al <rhyme label="b" type="m" corresp="#s3:l2">bestiert</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die t'al geschaepen <rhyme label="c" type="m">heeft</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Gy Roos, gy <rhyme label="d" type="m">Diamant</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Gy Leli, schoon <rhyme label="d" type="m" corresp="#s3:l5">geplant</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Besorght ons, besorght ons s'Hemels <rhyme label="d" type="m" corresp="#s3:l5">pandt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Geeft krachten, geeft krachten aen al myn spraeck, en <rhyme label="a" type="m">werck</rhyme>;</l>
        <l id="s4:l2" met="2" type="-+">Maeckt tot uw' <rhyme label="b" type="m">Naem</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Tot Lof, en <rhyme label="b" type="m" corresp="#s4:l2">Faem</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Myn tongh, en leden <rhyme label="c" type="m">sterck</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Myn hert, en tongh door <rhyme label="d" type="m">sanck</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Al syn sy veel te <rhyme label="d" type="m" corresp="#s4:l5">kranck</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">U moeten, u moeten bieden <rhyme label="d" type="m" corresp="#s4:l5">danck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
