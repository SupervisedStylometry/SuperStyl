    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="christuslied" translation="Christ song">Christuslied</tag>
    </tags>
    <melody>Hilf Gott dass mir es gelinge</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHy Christen broeders te <rhyme label="a" type="f">samen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wilt aenhooren een liedeken <rhyme label="b" type="m">recht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Uol hoochprijselicke <rhyme label="a" type="f" corresp="#s1:l1">namen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Uanden sone Gods <rhyme label="b" type="m" corresp="#s1:l2">ghesecht</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Aenschout die Schrift al na den <rhyme label="c" type="m">gheest</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Op dat ghy recht moecht <rhyme label="d" type="f">bekennen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Dat hy vant beghin is <rhyme label="c" type="m" corresp="#s1:l5">gheweest</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Inden eersten wilt <rhyme label="a" type="f">aenmercken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hoe Christus is dat Goddelick <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Daer door zijn Uader ginck <rhyme label="a" type="f" corresp="#s2:l1">wercken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Inden beghin, so wy hebben <rhyme label="b" type="m" corresp="#s2:l2">ghehoort</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Als God hemel ende aerde fondeerde <rhyme label="c" type="m">reyn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">So was dit woort by hem <rhyme label="d" type="f">verheuen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Een metten Uader <rhyme label="c" type="m" corresp="#s2:l5">ghemeyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy bekennen na Schrifts <rhyme label="a" type="f">belijden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Elck een met ooren <rhyme label="b" type="m">hoort</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hy was van eewighen <rhyme label="a" type="f" corresp="#s3:l1">tijden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Wie can vertellen zijn <rhyme label="b" type="m" corresp="#s3:l2">gheboort</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Hy is Gods euen beelde <rhyme label="c" type="m">alleyn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat schijnsel zijnder <rhyme label="d" type="f">heerlicheyt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Een spieghel zijns wesens <rhyme label="c" type="m" corresp="#s3:l5">reyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wten mont des alderhoochsten <rhyme label="a" type="f">verheuen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Is dat woort van <rhyme label="b" type="m">eewicheyt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En dat woort was by God <rhyme label="a" type="f" corresp="#s4:l1">beneuen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En God was dat woort, so Ioannes <rhyme label="b" type="m" corresp="#s4:l2">seyt</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Hy is alleen die wijsheyt Godes <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Gheboren vanden <rhyme label="d" type="f">Uader</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Om ons te verlossen vander <rhyme label="c" type="m" corresp="#s4:l5">doot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Als de mensch Gods ghebot had <rhyme label="a" type="f">ghebroken</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Door des listighen duyuels <rhyme label="b" type="m">raet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">So heeft God tot hem <rhyme label="a" type="f" corresp="#s5:l1">ghesproken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ende belouet eenen <rhyme label="b" type="m" corresp="#s5:l2">saet</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Die ons soude verlossen van sviants <rhyme label="c" type="m">nijt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En breken zijn hooft in <rhyme label="d" type="f">stucken</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Hieromme zijn wy <rhyme label="c" type="m" corresp="#s5:l5">verblijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dit saet vol alder <rhyme label="a" type="f">eeren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Wert Godes bruyt hier toe <rhyme label="b" type="m">gheseyt</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Uan tbeghin te <rhyme label="a" type="f" corresp="#s6:l1">genereren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Door die beloften hoort dit <rhyme label="b" type="m" corresp="#s6:l2">bescheyt</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Uan geslacht tot geslacht so Matheus <rhyme label="c" type="m">verclaert</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Na der beloften in <rhyme label="d" type="f">figueren</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Is die Sone <rhyme label="c" type="m" corresp="#s6:l5">gheopenbaert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dit saet, willet recht <rhyme label="a" type="f">aenschouwen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wt den ghelooue na den gheest <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Als eenen waren Isaac vol <rhyme label="a" type="f" corresp="#s7:l1">trouwen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Nochtans voor Abraham was <rhyme label="b" type="m" corresp="#s7:l2">hy</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Hy is die wijse Salomon <rhyme label="c" type="m">pertinent</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Uanden rechten Dauid <rhyme label="d" type="f">ghecomen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Nader beloften <rhyme label="c" type="m" corresp="#s7:l5">bekent</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Hy wert oock Dauid <rhyme label="a" type="f">gheheeten</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Als een wortel een rechte <rhyme label="b" type="m">stam</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Dauid noemde hem een Heer <rhyme label="a" type="f" corresp="#s8:l1">vermeten</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Eer hy op aerden <rhyme label="b" type="m" corresp="#s8:l2">quam</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Alle Coninghen waren van hem een <rhyme label="c" type="m">figuer</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Dat hy eewelick soude <rhyme label="d" type="f">regneren</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Met zijnen scepter <rhyme label="c" type="m" corresp="#s8:l5">puer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Dese roede was in Syon <rhyme label="a" type="f">gheseten</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Als een plantinghe des Heeren <rhyme label="b" type="m">wel</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Hooch edel bouen alle <rhyme label="a" type="f" corresp="#s9:l1">Propheten</rhyme></l>
        <l id="s9:l4" met="3" type="-+">De gherechte <rhyme label="b" type="m" corresp="#s9:l2">Emanuel</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Hy is dat woort der <rhyme label="c" type="m">gherechticheyt</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Eenen rechten vorst des <rhyme label="d" type="f">vreden</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Als Esaias heeft <rhyme label="c" type="m" corresp="#s9:l5">gheseyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Desen soon vol alder <rhyme label="a" type="f">eeren</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die in Godlicker ghedaente <rhyme label="b" type="m">was</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Die ghinck hem seluen <rhyme label="a" type="f" corresp="#s10:l1">verneren</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Na des Uaders wille te <rhyme label="b" type="m" corresp="#s10:l2">pas</rhyme></l>
        <l id="s10:l5" met="4" type="-+">De ghedaente eens knechts nam hy <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Om ons exempel te <rhyme label="d" type="f">wesen</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Sijn voetstappen na te <rhyme label="c" type="m" corresp="#s10:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Een reyne maghet <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Uan God voorsien en hooch <rhyme label="b" type="m">gheacht</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Als sy consenteerde in <rhyme label="a" type="f" corresp="#s11:l1">desen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Ontfinck sy door des gheests <rhyme label="b" type="m" corresp="#s11:l2">cracht</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Sy gheloofde Gods woort en is <rhyme label="c" type="m">ghewis</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Een kint wt haer <rhyme label="d" type="f">gheboren</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Ons tot een sone ghegheuen <rhyme label="c" type="m" corresp="#s11:l5">is</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Desen soon hooch van <rhyme label="a" type="f">weerden</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Uan den hemel hemelsch somen <rhyme label="b" type="m">beuint</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Die andere Adam op <rhyme label="a" type="f" corresp="#s12:l1">eerden</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Nochtans en was hy niet eertsch <rhyme label="b" type="m" corresp="#s12:l2">ghesint</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Al vander eerden en was hy <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Dat woort is vleesch <rhyme label="d" type="f">gheworden</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Als ons Ioannes <rhyme label="c" type="m" corresp="#s12:l5">bediet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Als hy hem op der werelt <rhyme label="a" type="f">verclaerde</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Heeft hy ghetoont zijn Godlicke <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Sijn Uader hem <rhyme label="a" type="f" corresp="#s13:l1">openbaerde</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Als zijn beminde sone <rhyme label="b" type="m" corresp="#s13:l2">gheacht</rhyme></l>
        <l id="s13:l5" met="4" type="-+">Dit isset broot vanden hemel <rhyme label="c" type="m">ghedaelt</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Sijn vleesch is ons een <rhyme label="d" type="f">spijse</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Onse schult heeft hy <rhyme label="c" type="m" corresp="#s13:l5">betaelt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Alle dat hy van zijnen Uader <rhyme label="a" type="f">hoorde</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Ende wat hy te vooren hadde <rhyme label="b" type="m">ghesien</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Dat selfde hy <rhyme label="a" type="f" corresp="#s14:l1">verantwoorde</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Noyt sulcken wonder en sachmen <rhyme label="b" type="m" corresp="#s14:l2">gheschien</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Daer en hadde noyt mensch God <rhyme label="c" type="m">aenschout</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Dan die eenighe soon des <rhyme label="d" type="f">Uaders</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Wel hem die op hem <rhyme label="c" type="m" corresp="#s14:l5">betrout</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Die dooden gaf hy dat <rhyme label="a" type="f">leuen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">En de blinden haer <rhyme label="b" type="m">ghesicht</rhyme></l>
        <l id="s15:l3" met="3" type="-+">De sonden mocht hy <rhyme label="a" type="f" corresp="#s15:l1">vergheuen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Dees wercken vielen hem <rhyme label="b" type="m" corresp="#s15:l2">licht</rhyme></l>
        <l id="s15:l5" met="4" type="-+">Hy is de poort ende rechte <rhyme label="c" type="m">baen</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Niemant en coemt totten <rhyme label="d" type="f">Uader</rhyme></l>
        <l id="s15:l7" met="3" type="-+">Hy en moet door den Sone <rhyme label="c" type="m" corresp="#s15:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Dese Soon die mach ons <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Want hy selfs de waerheyt is <rhyme label="b" type="m">pleyn</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Sijn schaepkens gaet hy <rhyme label="a" type="f" corresp="#s16:l1">informeren</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Door zijnen gheest een vertrooster <rhyme label="b" type="m" corresp="#s16:l2">alleyn</rhyme></l>
        <l id="s16:l5" met="4" type="-+">Wilt draghen zijnen last, so hy ons <rhyme label="c" type="m">ghebiet</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Hy is een fonteyne des <rhyme label="d" type="f">leuens</rhyme></l>
        <l id="s16:l7" met="3" type="-+">Sijn gratie gheeft hy om <rhyme label="c" type="m" corresp="#s16:l5">niet</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Onse sonden heeft hy <rhyme label="a" type="f">ghedraghen</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Ueel smerten met grooter <rhyme label="b" type="m">noot</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Al van tbeghin der <rhyme label="a" type="f" corresp="#s17:l1">daghen</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Is dat Lammeken <rhyme label="b" type="m" corresp="#s17:l2">ghedoot</rhyme></l>
        <l id="s17:l5" met="4" type="-+">Uoor ons ouertredinghe heeft hy <rhyme label="c" type="m">ghebeen</rhyme></l>
        <l id="s17:l6" met="3" type="-+">Doen wy Gods vianden <rhyme label="d" type="f">waren</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Stelde hy zijnen Uader te <rhyme label="c" type="m" corresp="#s17:l5">vreen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Dits onse salichmaker <rhyme label="a" type="f">warachtich</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Een rechte arm van zijnen <rhyme label="b" type="m">Uaer</rhyme></l>
        <l id="s18:l3" met="3" type="-+">In hem so woont <rhyme label="a" type="f" corresp="#s18:l1">lijfachtich</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Die volle Godheyt <rhyme label="b" type="m" corresp="#s18:l2">eenpaer</rhyme></l>
        <l id="s18:l5" met="4" type="-+">Ioannes noemt hem warachtich <rhyme label="c" type="m">Godt</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Daer toe dat eewich <rhyme label="d" type="f">leuen</rhyme></l>
        <l id="s18:l7" met="3" type="-+">Dit is die sommighe een <rhyme label="c" type="m" corresp="#s18:l5">spodt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Laet ons den Uader door Christum <rhyme label="a" type="f">bidden</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Ende vergaderen in zijnen <rhyme label="b" type="m">naem</rhyme></l>
        <l id="s19:l3" met="3" type="-+">So is hy by ons int <rhyme label="a" type="f" corresp="#s19:l1">midden</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Die hem inden gheest aenroepen <rhyme label="b" type="m" corresp="#s19:l2">bequaem</rhyme></l>
        <l id="s19:l5" met="4" type="-+">Tot prijs zijns Uaders altijt <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s19:l6" met="3" type="-+">So hem Thomas heeft <rhyme label="d" type="f">beleden</rhyme></l>
        <l id="s19:l7" met="3" type="-+">Bekennen wy Christum <rhyme label="c" type="m" corresp="#s19:l5">claer</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Desen Sone vol van <rhyme label="a" type="f">ghenaden</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Gaf zijnen discipelen de <rhyme label="b" type="m">macht</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Sy deden wel <rhyme label="a" type="f" corresp="#s20:l1">wonderdaden</rhyme></l>
        <l id="s20:l4" met="3" type="-+">Door zijnen naem <rhyme label="b" type="m" corresp="#s20:l2">gheacht</rhyme></l>
        <l id="s20:l5" met="4" type="-+">Hy beualse te doopen in zijnen <rhyme label="c" type="m">naem</rhyme></l>
        <l id="s20:l6" met="3" type="-+">Alle die hier aen <rhyme label="d" type="f">gheloouen</rhyme></l>
        <l id="s20:l7" met="3" type="-+">Die zijn daer oock toe <rhyme label="c" type="m" corresp="#s20:l5">bequaem</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Al dat daer meer staet <rhyme label="a" type="f">beschreuen</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Uan desen soon <rhyme label="b" type="m">excellent</rhyme></l>
        <l id="s21:l3" met="3" type="-+">Bekennen wy al <rhyme label="a" type="f" corresp="#s21:l1">beneuen</rhyme></l>
        <l id="s21:l4" met="3" type="-+">Dat oudt ende niewe <rhyme label="b" type="m" corresp="#s21:l2">testament</rhyme></l>
        <l id="s21:l5" met="4" type="-+">Hy is God ouer al in <rhyme label="c" type="m">eewicheyt</rhyme></l>
        <l id="s21:l6" met="3" type="-+">Int neghenste totten <rhyme label="d" type="f">Romeynen</rhyme></l>
        <l id="s21:l7" met="3" type="-+">Heuet Paulus wt <rhyme label="c" type="m" corresp="#s21:l5">gheleyt</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Ten heeft my niet <rhyme label="a" type="f">verdroten</rhyme></l>
        <l id="s22:l2" met="3" type="-+">Al om te dichten dit <rhyme label="b" type="m">liet</rhyme></l>
        <l id="s22:l3" met="3" type="-+">Om mijn rechte <rhyme label="a" type="f" corresp="#s22:l1">medeghenoten</rhyme></l>
        <l id="s22:l4" met="3" type="-+">So isset wt liefden <rhyme label="b" type="m" corresp="#s22:l2">gheschiet</rhyme></l>
        <l id="s22:l5" met="4" type="-+">En nemet ten besten mijn cort <rhyme label="c" type="m">ghesanck</rhyme></l>
        <l id="s22:l6" met="3" type="-+">Soument al van Christo <rhyme label="d" type="f">verhalen</rhyme></l>
        <l id="s22:l7" met="3" type="-+">Mijn liet viel veel te <rhyme label="c" type="m" corresp="#s22:l5">lanck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
