  <author>Johan Deknatel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Geest van booven, vlam ons <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Want wy zyn dog uw <rhyme label="b" type="f">Eigen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Veel is van ons niet regt <rhyme label="a" type="m" corresp="#s1:l1">gedaan</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maar zie ons voor uw <rhyme label="b" type="f" corresp="#s1:l2">buigen</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Wy zyn dog nog uw <rhyme label="c" type="m">Eigendom</rhyme></l>
        <l id="s1:l6" met="3" type="-+">By alle deeze <rhyme label="d" type="f">Zaaken</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Wy minnen uwen Naam en <rhyme label="c" type="m" corresp="#s1:l5">Roem</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En willen 't beeter <rhyme label="d" type="f" corresp="#s1:l6">maaken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Zo laat dan uwe <rhyme label="a" type="m">Liefde-wind</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ons heiligen en <rhyme label="b" type="f">dryven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Als hert met herte zich <rhyme label="a" type="m" corresp="#s2:l1">verbindt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Als Een voor U te <rhyme label="b" type="f" corresp="#s2:l2">blyven</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Dat wy met t'zaam verbond'nen <rhyme label="c" type="m">zin</rhyme></l>
        <l id="s2:l6" met="3" type="-+">U 't harte moogen <rhyme label="d" type="f">steelen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Of sluipt er eenig' argwaan <rhyme label="c" type="m" corresp="#s2:l5">in</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die zal de kracht <rhyme label="d" type="f" corresp="#s2:l6">verdeelen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy naderen <rhyme label="a" type="m">Gemeenschaplyk</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Tot U, ô trouwe <rhyme label="b" type="f">Liefde</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En kussen U gansch <rhyme label="a" type="m" corresp="#s3:l1">inniglyk</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Voor deeze nieuwe <rhyme label="b" type="f" corresp="#s3:l2">Liefde</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Waarmeê Gy onze ziel <rhyme label="c" type="m">vervult</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">O, laat ze vast <rhyme label="d" type="f">beklyven</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Tot dat g'ons alle brengen <rhyme label="c" type="m" corresp="#s3:l5">zult</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Daar w' eeuwig zullen <rhyme label="d" type="f" corresp="#s3:l6">blyven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
