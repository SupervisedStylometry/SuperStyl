<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>188356</id>
    <date>
      <year-start>1780</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>J'aime une ingrate beauté</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">J'Aime une ingrate <rhyme label="a" type="m">beauté</rhyme>;</l>
        <l id="s1:l2" met="4" type="+-">Mais c'est pour toute <rhyme label="b" type="f">mavie</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Je n'ai plus de <rhyme label="a" type="m" corresp="#s1:l1">volonté</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Ma liberte m'est <rhyme label="b" type="f" corresp="#s1:l2">Ravië</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Elle à des <rhyme label="c" type="m">Rigeurs</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Que mon coeur <rhyme label="d" type="f">prefere</rhyme>;</l>
        <l id="s1:l7" met="3" type="+-">Aux plus douces <rhyme label="c" type="m" corresp="#s1:l5">faveurs</rhyme></l>
        <l id="s1:l8" met="3" type="-+">De tout autre <rhyme label="d" type="f" corresp="#s1:l6">bergere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Quand aux Champs des le <rhyme label="a" type="m">Matin</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Le soing du troupeau <rhyme label="b" type="f">l'appelle</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Le jour devint plus <rhyme label="a" type="m" corresp="#s2:l1">serin</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">L'aurore nait avec <rhyme label="b" type="f" corresp="#s2:l2">elle</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Pour mourir sur son <rhyme label="c" type="m">sein</rhyme>;</l>
        <l id="s2:l6" met="3" type="+-">Onvoit les fleurs <rhyme label="d" type="f">Eclore</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">De l'Eclat de son <rhyme label="c" type="m" corresp="#s2:l5">teint</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">La Rose se <rhyme label="d" type="f" corresp="#s2:l6">Colore</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Le Rossignol va <rhyme label="a" type="m">chantant</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Joyeux de la voir si <rhyme label="b" type="f">belle</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Le papillon <rhyme label="a" type="m" corresp="#s3:l1">voltigeant</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">La prend pour la fleur <rhyme label="b" type="f" corresp="#s3:l2">nouvelle</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Les amoureux <rhyme label="c" type="m">Zephirs</rhyme>:</l>
        <l id="s3:l6" met="3" type="+-">Naissent de son <rhyme label="d" type="f">haleine</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Et mes ardents <rhyme label="c" type="m" corresp="#s3:l5">soupirs</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">La suivent dans la <rhyme label="d" type="f" corresp="#s3:l6">plaine</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Malgré sa <rhyme label="a" type="m">Timidité</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Qui la rend plus belle <rhyme label="b" type="f">encore</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">De ces tendres <rhyme label="a" type="m" corresp="#s4:l1">voluptez</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dans ces jeux j'ai vu <rhyme label="b" type="f" corresp="#s4:l2">l'aurore</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Et sa bouche <rhyme label="c" type="m">Exprime</rhyme>:</l>
        <l id="s4:l6" met="3" type="+-">Par un tendre <rhyme label="d" type="f">sourire</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Que le plaisir <rhyme label="c" type="m" corresp="#s4:l5">d'aimer</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Elle craint &amp; <rhyme label="d" type="f" corresp="#s4:l6">disere</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
