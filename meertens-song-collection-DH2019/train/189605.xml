<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>189605</id>
    <date>
      <year-start>1750</year-start>
      <year-end>1750</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>C'est un amant ouvrez la porte</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ô Liefelyke <rhyme label="a" type="f">Bloemmenryen</rhyme>!</l>
        <l id="s1:l2" met="4" type="-+">ô Lustprieelen! die 't <rhyme label="b" type="m">gezigt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wanneer ik 's avonds, onder 't <rhyme label="a" type="f" corresp="#s1:l1">vryen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">My spiegel in 't behoorlyk <rhyme label="b" type="m" corresp="#s1:l2">ligt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Van Kloris zielbekoorlyke <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">My tot een ander tempe <rhyme label="b" type="m">strekt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">'k Zal u vermelde het <rhyme label="a" type="f" corresp="#s2:l1">vermogen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Van haar, die myne liefde <rhyme label="b" type="m" corresp="#s2:l2">trekt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gints in 't verschiet kunt gy <rhyme label="a" type="f">aanschouwen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Het hoef van Bouman, Bouman, <rhyme label="b" type="m">die</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Uyt zwaantje, zyn aanminge <rhyme label="a" type="f" corresp="#s3:l1">vrouwe</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Ter wereld bragt een zoon of <rhyme label="b" type="m" corresp="#s3:l2">drie</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En eene Dogter, <rhyme label="a" type="f">uitgelezen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Zoo schoon gelyk de <rhyme label="b" type="m">middagzon</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dees' is, die myne jeugt <rhyme label="a" type="f" corresp="#s4:l1">voordezen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En fieren inborst <rhyme label="b" type="m" corresp="#s4:l2">overwon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Vaak heb ik alle <rhyme label="a" type="f">minnaryen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Met uytgelaten drift <rhyme label="b" type="m">belagt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Maar nu wort ik, ô <rhyme label="a" type="f" corresp="#s5:l1">Razernye</rhyme>!</l>
        <l id="s5:l4" met="4" type="-+">Op 't allerdeerlykst <rhyme label="b" type="m" corresp="#s5:l2">verkragt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ik dagt, in 't eerst den band <rhyme label="a" type="f">t'ontlopen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Der min, in vollen vaart en <rhyme label="b" type="m">vlugt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maar kragteloos was al myn <rhyme label="a" type="f" corresp="#s6:l1">hoopen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Des storte ik haast in <rhyme label="b" type="m" corresp="#s6:l2">ongenugt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">'t Was Kloris, die my bragt in <rhyme label="a" type="f">lyen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En myne ziel in <rhyme label="b" type="m">minnepyn</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Die nimmer hoorde naar myn <rhyme label="a" type="f" corresp="#s7:l1">vrye</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Geen quaal zoo bitter daar de <rhyme label="b" type="m" corresp="#s7:l2">myn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ag! schoone wilt myn pyn <rhyme label="a" type="f">verzagten</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Verzaak dog uwe <rhyme label="b" type="m">afkeerigheid</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Ey! zalf myn pyn, verhoor myn <rhyme label="a" type="f" corresp="#s8:l1">klagten</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Myn gadeloze, en schoone <rhyme label="b" type="m" corresp="#s8:l2">Meydt</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dan zal ik u een kransje <rhyme label="a" type="f">vlegten</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Van bloempjes aangenaam en <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">En daar in mirtetelgjes <rhyme label="a" type="f" corresp="#s9:l1">hegten</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Tot een aanminge huwlyks <rhyme label="b" type="m" corresp="#s9:l2">kroon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
