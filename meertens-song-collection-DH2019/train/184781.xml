    <melody>Wie zag ooit schonere schone</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">De waare <rhyme label="a" type="f">Vaderlander</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Is steeds een Vriend van <rhyme label="b" type="m">Deugd</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Getrouw aan <rhyme label="a" type="f" corresp="#s1:l1">Vryheidsstander</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Het weldoen is zyn <rhyme label="b" type="m" corresp="#s1:l2">vreugd</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Hy blyft met hart en <rhyme label="c" type="f">zinnen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">De lieflyke Eendragt <rhyme label="c" type="f" corresp="#s1:l5">minnen</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Want ze is de Steun <rhyme label="d" type="m">alleen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Van 't <rhyme label="d" type="m" corresp="#s1:l7">Algemee...n</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Van 't <rhyme label="d" type="m" corresp="#s1:l7">Algemeen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy vloekt de snoode <rhyme label="a" type="f">listen</rhyme>;</l>
        <l id="s2:l2" met="3" type="-+">By toont z ch t' allen <rhyme label="b" type="m">tyd</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Goed Burger en goed <rhyme label="a" type="f" corresp="#s2:l1">Christen</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Betracht zyn Pligt met <rhyme label="b" type="m" corresp="#s2:l2">vlyt</rhyme>;</l>
        <l id="s2:l5" met="3" type="-+">Voor Godsdienst, Recht en <rhyme label="c" type="f">Wetten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Zal hy zyn Leven <rhyme label="c" type="f" corresp="#s2:l5">zetten</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Dit acht hy 't waardste <rhyme label="d" type="m">Pand</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Van 't <rhyme label="d" type="m" corresp="#s2:l7">Vaderla...nd</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Van 't <rhyme label="d" type="m" corresp="#s2:l7">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Kan hy een uurtje <rhyme label="a" type="f">missen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Hy legt zyn <rhyme label="b" type="m">onderzoek</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Op 's Lands <rhyme label="a" type="f" corresp="#s3:l1">Geschiedenissen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Of ander nuttig <rhyme label="b" type="m" corresp="#s3:l2">Boek</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Hy hecht zyn Trouw aan <rhyme label="c" type="f">Vrinden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die hen welvoeglyk <rhyme label="c" type="f" corresp="#s3:l5">vinden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En tot hun nut <rhyme label="d" type="m">bereid</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Met <rhyme label="d" type="m" corresp="#s3:l7">billykhei...d</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Met <rhyme label="d" type="m" corresp="#s3:l7">billykheid</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy richt zich naar de <rhyme label="a" type="f">Reden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In al zyn <rhyme label="b" type="m">bezigheÃªn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Is altoos wel te <rhyme label="a" type="f" corresp="#s4:l1">vreden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met net genot <rhyme label="b" type="m" corresp="#s4:l2">alleen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dat hy verkrygt door <rhyme label="c" type="f">handel</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Steeds eerlyk in zyn <rhyme label="c" type="f" corresp="#s4:l5">wandel</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Ziet daar, in doen en <rhyme label="d" type="m">lot</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Een <rhyme label="d" type="m" corresp="#s4:l7">Patri...ot</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Een <rhyme label="d" type="m" corresp="#s4:l7">Patriot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
