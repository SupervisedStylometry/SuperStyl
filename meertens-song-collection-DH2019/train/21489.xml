  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LAet ons <rhyme label="a" type="f">ghebenedijen</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Met dicht en <rhyme label="b" type="m">sanck</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Den Heere tallen <rhyme label="a" type="f" corresp="#s1:l1">tijen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Ons leuen <rhyme label="b" type="m" corresp="#s1:l2">lanck</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Sijn liefde <rhyme label="b" type="m" corresp="#s1:l2">vranck</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Heeft ons wilt dit <rhyme label="c" type="f">aenhooren</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Beurijdt door Pharaons <rhyme label="c" type="f" corresp="#s1:l6">thooren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dien helschen vyant <rhyme label="b" type="m" corresp="#s1:l2">stranck</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wonderlick zijn sijn <rhyme label="a" type="f">krachten</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Die hy <rhyme label="b" type="m">oorboort</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Al wt de zee met <rhyme label="a" type="f" corresp="#s2:l1">machten</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Sijn wy <rhyme label="b" type="m" corresp="#s2:l2">gheschoort</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Pharao <rhyme label="b" type="m" corresp="#s2:l2">verstoort</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die heeft ons meenen <rhyme label="c" type="f">volghen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">De zee heeft hem <rhyme label="c" type="f" corresp="#s2:l6">verswolghen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En isser in <rhyme label="b" type="m" corresp="#s2:l2">versmoort</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wt Pharaodts diensten <rhyme label="a" type="f">vierich</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Sijn wy <rhyme label="b" type="m">gheleydt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Door Christus doot <rhyme label="a" type="f" corresp="#s3:l1">manierich</rhyme></l>
        <l id="s3:l4" met="2" type="-+">So schriftuer <rhyme label="b" type="m" corresp="#s3:l2">seydt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Sijn heerlick <rhyme label="b" type="m" corresp="#s3:l2">feidt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Heeft zijn gheweldt <rhyme label="c" type="f">ontbonden</rhyme></l>
        <l id="s3:l7" met="3" type="-+">En ons in alder <rhyme label="c" type="f" corresp="#s3:l6">stonden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Sijn eewich goet <rhyme label="b" type="m" corresp="#s3:l2">bereydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Kinders wlit v <rhyme label="a" type="f">verblijden</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Groot ende <rhyme label="b" type="m">cleyn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Wilt den Heere <rhyme label="a" type="f" corresp="#s4:l1">belijden</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Alle <rhyme label="b" type="m" corresp="#s4:l2">ghemeyn</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Ghy gheesten <rhyme label="b" type="m" corresp="#s4:l2">reyn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Wilt v <rhyme label="c" type="f">veriubileren</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Met sangk wilt <rhyme label="c" type="f" corresp="#s4:l6">exelteren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Sijn wonderdaet <rhyme label="b" type="m" corresp="#s4:l2">certeyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy hemelen ghy <rhyme label="a" type="f">eerden</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Ghy mane <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Ghy Enghelen vol <rhyme label="a" type="f" corresp="#s5:l1">weerden</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Ghy sterren <rhyme label="b" type="m" corresp="#s5:l2">vroet</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Prijst <rhyme label="b" type="m" corresp="#s5:l2">oueruloet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die heerschapende <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Singt hem toe prijs en <rhyme label="c" type="f" corresp="#s5:l6">eere</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Uoor weldaet die hy <rhyme label="b" type="m" corresp="#s5:l2">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghy dieren ghy <rhyme label="a" type="f">reuieren</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Danct hem <rhyme label="b" type="m">eenpaer</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Wilt synen naem <rhyme label="a" type="f" corresp="#s6:l1">verchieren</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Ionstich <rhyme label="b" type="m" corresp="#s6:l2">eerbaer</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Ghy berghen <rhyme label="b" type="m" corresp="#s6:l2">swaer</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Wilt hem oock lof <rhyme label="c" type="f">toewijsen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">En syne goetheyt <rhyme label="c" type="f" corresp="#s6:l6">prijsen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">In gheest en waerheyt <rhyme label="b" type="m" corresp="#s6:l2">claer</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Laet ons sonder <rhyme label="a" type="f">ophouwen</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Hem maken <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Den Prince vol van <rhyme label="a" type="f" corresp="#s7:l1">trouwen</rhyme></l>
        <l id="s7:l4" met="2" type="-+">In shemels <rhyme label="b" type="m" corresp="#s7:l2">conroot</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Ghy scharen <rhyme label="b" type="m" corresp="#s7:l2">bloot</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Wilt altijt sonder <rhyme label="c" type="f">sparen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Sijn weldaet <rhyme label="c" type="f" corresp="#s7:l6">openbaren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Uolstandich totter <rhyme label="b" type="m" corresp="#s7:l2">doodt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
