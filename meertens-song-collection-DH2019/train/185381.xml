<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185381</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Engelse fortuin</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Den Maeghden <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Te houden <rhyme label="b" type="m">onbevleckt</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Geeft Paulus <rhyme label="a" type="m" corresp="#s1:l1">raet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ghelijck hy claer <rhyme label="b" type="m" corresp="#s1:l2">ontdeckt</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Als hy daer <rhyme label="c" type="m">prijst</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Alleen te blyven, <rhyme label="d" type="m">want</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Veel aenstoot <rhyme label="c" type="m" corresp="#s1:l5">rijst</rhyme></l>
        <l id="s1:l8" met="3" type="+-">In des Houwelijcks <rhyme label="d" type="m" corresp="#s1:l6">bant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Een dochter <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die haer na Godts woort <rhyme label="b" type="m">draeght</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Die soeckt <rhyme label="a" type="m" corresp="#s2:l1">alleyn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wat den Heere <rhyme label="b" type="m" corresp="#s2:l2">behaeght</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Dies haer <rhyme label="c" type="m">aenstoot</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Is niet soo <rhyme label="d" type="m">veelderhant</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Als wel den <rhyme label="c" type="m" corresp="#s2:l5">noot</rhyme></l>
        <l id="s2:l8" met="3" type="+-">In des Houwelijcks <rhyme label="d" type="m" corresp="#s2:l6">bant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Want soo men <rhyme label="a" type="m">siet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat smerten <rhyme label="b" type="m">menichfout</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">In ‘t vleys <rhyme label="a" type="m" corresp="#s3:l1">gheschiet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Haer, die daer sijn <rhyme label="b" type="m" corresp="#s3:l2">ghetrout</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">‘t Can niet <rhyme label="c" type="m">gheseyt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Hier sijn, vaet het <rhyme label="d" type="m">verstant</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Dan d’aenstoot <rhyme label="c" type="m" corresp="#s3:l5">leyt</rhyme></l>
        <l id="s3:l8" met="3" type="+-">In des Houwelijcks <rhyme label="d" type="m" corresp="#s3:l6">bant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ghy wort <rhyme label="a" type="m">verschoont</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Ghy Maeghden al te <rhyme label="b" type="m">saem</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Soo ghy <rhyme label="a" type="m" corresp="#s4:l1">betoont</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Reynheyt in u <rhyme label="b" type="m" corresp="#s4:l2">lichaem</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Christus <rhyme label="c" type="m">bemintm</rhyme></l>
        <l id="s4:l6" met="3" type="-+">V alder-liefste <rhyme label="d" type="m">pant</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">V niet licht <rhyme label="c" type="m" corresp="#s4:l5">bint</rhyme></l>
        <l id="s4:l8" met="3" type="+-">In des Houwelijcks <rhyme label="d" type="m" corresp="#s4:l6">bant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Maer dit <rhyme label="a" type="m">voorstel</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En dient u niet als <rhyme label="b" type="m">wet</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Trout ghy ‘t is <rhyme label="a" type="m" corresp="#s5:l1">wel</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die niet en trout doet <rhyme label="b" type="m" corresp="#s5:l2">bet</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Dan die den <rhyme label="c" type="m">last</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ghenomen heeft ter <rhyme label="d" type="m">hant</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Die stae doch <rhyme label="c" type="m" corresp="#s5:l5">vast</rhyme></l>
        <l id="s5:l8" met="3" type="+-">In des Houwelijcks <rhyme label="d" type="m" corresp="#s5:l6">bant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Dan ist dat <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">V die ghenade <rhyme label="b" type="m">jont</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Dat ghy u <rhyme label="a" type="m" corresp="#s6:l1">tot</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Aendacht begeven <rhyme label="b" type="m" corresp="#s6:l2">cont</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Soo <rhyme label="c" type="m">oversomt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">D’aenstoot aen elcken <rhyme label="d" type="m">cant</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Eer dat ghy <rhyme label="c" type="m" corresp="#s6:l5">comt</rhyme></l>
        <l id="s6:l8" met="3" type="+-">In des Houwelijcks <rhyme label="d" type="m" corresp="#s6:l6">bant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Die met <rhyme label="a" type="m">insicht</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Alleen blijft in haer <rhyme label="b" type="m">jeught,l</rhyme></l>
        <l id="s7:l3" met="2" type="-+">‘t Valt haer meer <rhyme label="a" type="m" corresp="#s7:l1">licht</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Te dienen Godt in <rhyme label="b" type="m" corresp="#s7:l2">deught</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Beter ist <rhyme label="c" type="m">doch</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Dan te lyden den <rhyme label="d" type="m">brant</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Schickt u dan <rhyme label="c" type="m" corresp="#s7:l5">noch</rhyme></l>
        <l id="s7:l8" met="3" type="+-">In des Houwelijcks <rhyme label="d" type="m" corresp="#s7:l6">bant</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Princesse <rhyme label="a" type="m">vroet</rhyme></l>
        <l id="s8:l2" met="3" type="-+">En al die deught <rhyme label="b" type="m">aenvaert</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Het hoochste <rhyme label="a" type="m" corresp="#s8:l1">goet</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Als waertste schat <rhyme label="b" type="m" corresp="#s8:l2">bewaert</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Valt u dan <rhyme label="c" type="m">by</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Te trouwen, ‘t is geen <rhyme label="d" type="m">schant</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Schickt u dan <rhyme label="c" type="m" corresp="#s8:l5">bly</rhyme></l>
        <l id="s8:l8" met="3" type="+-">In des Houwelijcks <rhyme label="d" type="m" corresp="#s8:l6">bant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
