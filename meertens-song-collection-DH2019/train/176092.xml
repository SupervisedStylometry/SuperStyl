    <melody>Psalm 096 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SIngt een nieu liet God onsen <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">De gantsche werelt geev' hem <rhyme label="a" type="f" corresp="#s1:l1">eere</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Van synen name maeckt <rhyme label="b" type="m">gewach</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Bootschapt syn heyl van dach tot <rhyme label="b" type="m" corresp="#s1:l3">dach</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Aen alle volcken na en <rhyme label="a" type="f" corresp="#s1:l1">veere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Syn roem laet aende heydens <rhyme label="a" type="f">weten</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Syn wonderen wilt niet <rhyme label="a" type="f" corresp="#s2:l1">vergeten</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Want hooch-verheven is de <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En moet gevreeset zijn, veel <rhyme label="b" type="m" corresp="#s2:l3">meer</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Dan alle die hier goden <rhyme label="a" type="f" corresp="#s2:l1">heeten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Der volcken goden <rhyme label="a" type="f">altesamen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Syn ydelheyt en bloote <rhyme label="a" type="f" corresp="#s3:l1">namen</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Maer God den hemel heeft <rhyme label="b" type="m">bereyt</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Voor hem gaet eer en <rhyme label="b" type="m" corresp="#s3:l3">majesteyt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Die syne hoocheyt <rhyme label="a" type="f" corresp="#s3:l1">wel-betamen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Cieraet en sterckte men <rhyme label="a" type="f">al-omme</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Aenschout in synen <rhyme label="a" type="f" corresp="#s4:l1">heylichdomme</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Dat dan een yegelijck <rhyme label="b" type="m">geslacht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Om God te geven eer en <rhyme label="b" type="m" corresp="#s4:l3">cracht</rhyme></l>
        <l id="s4:l5" met="4" type="-+">In synen tempel vlijtich <rhyme label="a" type="f" corresp="#s4:l1">comme</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy volcken wilt den Heere <rhyme label="a" type="f">loven</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Geeft roem den Heere van hier <rhyme label="a" type="f" corresp="#s5:l1">boven</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Den Heere uwen offer <rhyme label="b" type="m">brengt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Den Heere uwe gaven <rhyme label="b" type="m" corresp="#s5:l3">schenckt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Comt in syn heylige <rhyme label="a" type="f" corresp="#s5:l1">voorhoven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Brengt tot den Heere u <rhyme label="a" type="f">gebeden</rhyme></l>
        <l id="s6:l2" met="4" type="-+">In syn uytmuntende <rhyme label="a" type="f" corresp="#s6:l1">woonsteden</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Ontsiet u voor syn godlijck <rhyme label="b" type="m">ooch</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Buycht u voor hem, en acht hem <rhyme label="b" type="m" corresp="#s6:l3">hooch</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Ghy alle menschen hier <rhyme label="a" type="f" corresp="#s6:l1">beneden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wilt aen de heydenen <rhyme label="a" type="f">verclaren</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Dat God de Heere der <rhyme label="a" type="f" corresp="#s7:l1">heyrscharen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Heeft s'werelts vaste <rhyme label="b" type="m">heerschappy</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Aen alle volckeren sal <rhyme label="b" type="m" corresp="#s7:l3">hy</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Syn recht en macht eens <rhyme label="a" type="f" corresp="#s7:l1">openbaren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dat sich de hemelen <rhyme label="a" type="f">verblijden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Het aerdrijck juych' aen allen <rhyme label="a" type="f" corresp="#s8:l1">zijden</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">De zee, soo verre alsse <rhyme label="b" type="m">vloeyt</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Velt, bos en wat daer inne <rhyme label="b" type="m" corresp="#s8:l3">groeyt</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Moet syne mogentheyt <rhyme label="a" type="f" corresp="#s8:l1">belijden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Laet dit geschien voor Gods <rhyme label="a" type="f">gesichte</rhyme>:</l>
        <l id="s9:l2" met="4" type="-+">Hy comt dat hy de aerde <rhyme label="a" type="f" corresp="#s9:l1">richte</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat hy de wijde werelt <rhyme label="b" type="m">leyd'</rhyme></l>
        <l id="s9:l4" met="4" type="-+">In oordeel en <rhyme label="b" type="m" corresp="#s9:l3">gerechticheyt</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">En met syn waerheyt <rhyme label="a" type="f" corresp="#s9:l1">voren-lichte</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
