<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156250</id>
    <date>
      <year-start>1669</year-start>
      <year-end>1669</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La princesse</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ERmgaart wuft en dartel <rhyme label="a" type="m">dier</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die door bossen, ruigt' en <rhyme label="b" type="f">heggen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">(Ongequetst van 't <rhyme label="a" type="m" corresp="#s1:l1">minnevier</rhyme>)</l>
        <l id="s1:l4" met="4" type="+-">Snel vervolgt het schuwe <rhyme label="c" type="m">wilt</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Daar gy menig pijl op <rhyme label="c" type="m" corresp="#s1:l4">spilt</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Al de vryers dart <rhyme label="b" type="f" corresp="#s1:l2">ontzeggen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En braveert met Pallas <rhyme label="c" type="m" corresp="#s1:l4">schilt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zijn de pijlen, scherp van <rhyme label="a" type="m">punt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Koorden, strikken, boogen, <rhyme label="b" type="f">netten</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En al 't geen de Jaghtlist <rhyme label="a" type="m" corresp="#s2:l1">munt</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Of het wilt zoo schuw van <rhyme label="c" type="m">aart</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">U zoo aangenaam en <rhyme label="c" type="m" corresp="#s2:l4">waart</rhyme>.</l>
        <l id="s2:l6" met="4" type="+-">Als de zoete <rhyme label="b" type="f" corresp="#s2:l2">minnewetten</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Die twee zielen t'zamen <rhyme label="c" type="m" corresp="#s2:l4">paart</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hoort gy liever het <rhyme label="a" type="m">gebas</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Van de brakke en snelle <rhyme label="b" type="f">winden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die door 't woeste wout zoo <rhyme label="a" type="m" corresp="#s3:l1">ras</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">'t Wilt vervolgen op het <rhyme label="c" type="m">spoor</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Als de droeve klaght die <rhyme label="c" type="m" corresp="#s3:l4">door</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Minnaars, die zoo trouw u <rhyme label="b" type="f" corresp="#s3:l2">minden</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Uitgestort zijn voor uw <rhyme label="c" type="m" corresp="#s3:l4">oor</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Kan een frisse <rhyme label="a" type="m">jongelingk</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">In het bloeyen van zijn <rhyme label="b" type="f">jaren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die nooit min ter herten <rhyme label="a" type="m" corresp="#s4:l1">gink</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En door u betovert <rhyme label="c" type="m">wiert</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">U dan niet bekoren? <rhyme label="c" type="m" corresp="#s4:l4">viert</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Gy daar maar Diaans <rhyme label="b" type="f" corresp="#s4:l2">Autaren</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">En het woeste <rhyme label="c" type="m" corresp="#s4:l4">bosgediert</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zijt gy straffer dan de <rhyme label="a" type="m">windt</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Die zoo bar blaast uit het <rhyme label="b" type="f">Noorden</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Ongevoelig harders <rhyme label="a" type="m" corresp="#s5:l1">kindt</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Heeft de Min op u geen <rhyme label="c" type="m">vat</rhyme>?</l>
        <l id="s5:l5" met="4" type="+-">Blijft gy eve trots en <rhyme label="c" type="m" corresp="#s5:l4">prat</rhyme>?</l>
        <l id="s5:l6" met="4" type="+-">Lacht gy om gebeÃªn en <rhyme label="b" type="f" corresp="#s5:l2">woorden</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Van de liefde hoog <rhyme label="c" type="m" corresp="#s5:l4">geschat</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Loop dan trosse en fiere <rhyme label="a" type="m">maagt</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Over bergen, diepe <rhyme label="b" type="f">dalen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Tot Cupid' u eens <rhyme label="a" type="m" corresp="#s6:l1">belaagt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En uw met zijn pjjl <rhyme label="c" type="m">deurwont</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Dan zal uwe preutze <rhyme label="c" type="m" corresp="#s6:l4">mont</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Klagen over Venus <rhyme label="b" type="f" corresp="#s6:l2">qualen</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Spreken uit een andre <rhyme label="c" type="m" corresp="#s6:l4">gront</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
