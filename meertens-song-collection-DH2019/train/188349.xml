  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MOn ange mon <rhyme label="a" type="m">plaisir</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Mon unique <rhyme label="b" type="f">désire</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Pour qui mon Coeur <rhyme label="b" type="f" corresp="#s1:l2">soupire</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Remplissez mes <rhyme label="a" type="m" corresp="#s1:l1">desirs</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Mon Ange, mon <rhyme label="a" type="m" corresp="#s1:l1">Plaisir</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">C'est a toi que je <rhyme label="c" type="f">Chante</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">C'est l'amour qui me <rhyme label="c" type="f" corresp="#s1:l6">tente</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Pour t'adresser mes <rhyme label="d" type="m">voeux</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">Et me rendre <rhyme label="d" type="m" corresp="#s1:l8">heureux</rhyme>;</l>
        <l id="s1:l10" met="3" type="-+">Permettez moy de <rhyme label="e" type="f">grace</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Que je vous dis en <rhyme label="e" type="f" corresp="#s1:l10">face</rhyme>,</l>
        <l id="s1:l12" met="3" type="+-">Que vous êtes <rhyme label="f" type="m">L'amy</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Qui me fais point <rhyme label="f" type="m" corresp="#s1:l12">d'ennuy</rhyme>.</l>
      </lg>
    </lg>
  </text>
