      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KNoddige Pan speeld' op zijn <rhyme label="a" type="m">Fluyt</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Knoddige Pan speeld' op zijn <rhyme label="a" type="m" corresp="#s1:l1">Fluyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat men hoor het soet <rhyme label="a" type="m" corresp="#s1:l1">geluyt</rhyme>,</l>
        <l id="s1:l4" met="1" type="-+"><rhyme label="b" type="f">Dryaden</rhyme>,</l>
        <l id="s1:l5" met="1" type="-+"><rhyme label="b" type="f" corresp="#s1:l4">Dryaden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Springht en singht, en tot dees <rhyme label="c" type="m">Feest</rhyme></l>
        <l id="s1:l7" met="3" type="-+">De and're Nimfen <rhyme label="b" type="f" corresp="#s1:l4">raden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wijl 't nu seecker is een <rhyme label="a" type="m">dagh</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Wijl 't nu seecker is een <rhyme label="a" type="m" corresp="#s2:l1">dagh</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Dat men vrolijck wesen <rhyme label="a" type="m" corresp="#s2:l1">magh</rhyme>,</l>
        <l id="s2:l4" met="1" type="-+">Want <rhyme label="b" type="f">heden</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">want <rhyme label="b" type="f" corresp="#s2:l4">heden</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Komt ons Jager met zijn <rhyme label="c" type="m">Hert</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Verheught na huys <rhyme label="b" type="f" corresp="#s2:l4">getreden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wack're Meysjes wilt oock <rhyme label="a" type="m">mee</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wack're Meysjes wilt oock <rhyme label="a" type="m" corresp="#s3:l1">mee</rhyme></l>
        <l id="s3:l3" met="4" type="+-">(Die vergaert zijn hier ter <rhyme label="a" type="m" corresp="#s3:l1">stee</rhyme>)</l>
        <l id="s3:l4" met="1" type="-+">V <rhyme label="b" type="f">voegen</rhyme>,</l>
        <l id="s3:l5" met="1" type="-+">u <rhyme label="b" type="f" corresp="#s3:l4">voegen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">By der soete <rhyme label="c" type="m">Nimfem-rey</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En toonen u <rhyme label="b" type="f" corresp="#s3:l4">vernoegen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Haest lijck wilt dan handt aen <rhyme label="a" type="m">handt</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Haest'lijck wilt dan handt aen <rhyme label="a" type="m" corresp="#s4:l1">handt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En gestooft door <rhyme label="a" type="m" corresp="#s4:l1">liefdens-brandt</rhyme>,</l>
        <l id="s4:l4" met="1" type="-+">Met <rhyme label="b" type="f">hoopen</rhyme>,</l>
        <l id="s4:l5" met="1" type="-+">met <rhyme label="b" type="f" corresp="#s4:l4">hoopen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">(Elck op 't schoonste <rhyme label="c" type="m">toe-gemaeckt</rhyme>)</l>
        <l id="s4:l7" met="3" type="-+">Den Jager tegen <rhyme label="b" type="f" corresp="#s4:l4">loopen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Roept: geluckigh is die <rhyme label="a" type="m">Man</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Roept: geluckigh is die <rhyme label="a" type="m" corresp="#s5:l1">Man</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Die soo seecker jagen <rhyme label="a" type="m" corresp="#s5:l1">kan</rhyme>,</l>
        <l id="s5:l4" met="1" type="-+">Godt <rhyme label="b" type="f">geve</rhyme>,</l>
        <l id="s5:l5" met="1" type="-+">Godt <rhyme label="b" type="f" corresp="#s5:l4">geve</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dat de Jager met zijn <rhyme label="c" type="m">Hert</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Veel duysend' nachten <rhyme label="b" type="f" corresp="#s5:l4">leve</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
