  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Fillis ay mint uw trouwe <rhyme label="a" type="f">Minnaar</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Verkies geen ander Herder als <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Maak my toch eens <rhyme label="a" type="f" corresp="#s1:l1">verwinnaar</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Na zo lang <rhyme label="b" type="m" corresp="#s1:l2">gevry</rhyme>,</l>
        <l id="s1:l5" met="1" type="-+">Al 't <rhyme label="c" type="f">kruitje</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Myn schaapjes, myn <rhyme label="c" type="f" corresp="#s1:l5">fluitje</rhyme>,</l>
        <l id="s1:l7" met="5" type="+-">Alles zal getuigen wat ik <rhyme label="b" type="m" corresp="#s1:l2">ly</rhyme>.</l>
      </lg>
    </lg>
