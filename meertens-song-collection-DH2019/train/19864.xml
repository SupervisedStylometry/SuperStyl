    <melody>J'aimerai en ce village</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Johan van Born</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">DOet u Oogjens <rhyme label="a" type="f">open</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Schoon LIef het is <rhyme label="b" type="m">dach</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Ick heb lang <rhyme label="a" type="f" corresp="#s1:l1">geroepen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Dat ick u sien <rhyme label="b" type="m" corresp="#s1:l2">mach</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Want u wesen <rhyme label="c" type="f">schoone</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Mijn hert doet verblijden // <rhyme label="d" type="m">seer</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Komt ontfangt de <rhyme label="c" type="f" corresp="#s1:l5">kroone</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Ghy en sult niet lijden // <rhyme label="d" type="m" corresp="#s1:l6">meer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Doet aen uwe <rhyme label="a" type="f">kleeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Hier is een kleet <rhyme label="b" type="m">wit</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">In het bloet des <rhyme label="a" type="f" corresp="#s2:l1">Heeren</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Is gewasschen <rhyme label="b" type="m" corresp="#s2:l2">dit</rhyme>:</l>
        <l id="s2:l5" met="3" type="+-">Uyt mijn dierbaer <rhyme label="c" type="f">wonde</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Vloeyt een reyne Beke // <rhyme label="d" type="m">klaer</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Die afwascht de <rhyme label="c" type="f" corresp="#s2:l5">sonde</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Die u gaf doodts steecken <rhyme label="d" type="m" corresp="#s2:l6">swaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Uyt u selfs wilt <rhyme label="a" type="f">treden</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Ende komt tot <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Want wy sullen <rhyme label="a" type="f" corresp="#s3:l1">heden</rhyme></l>
        <l id="s3:l4" met="3" type="+-">'t Samen wesen <rhyme label="b" type="m" corresp="#s3:l2">bly</rhyme></l>
        <l id="s3:l5" met="3" type="+-">In mijns Vaders <rhyme label="c" type="f">Rijcke</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Sullen wy gekroonet // <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Comet uyt den <rhyme label="c" type="f" corresp="#s3:l5">Rijcke</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Uwe liefd betoonet // <rhyme label="d" type="m" corresp="#s3:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">SIet de Sonne <rhyme label="a" type="f">schijnet</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Van mijns Liefs <rhyme label="b" type="m">gesicht</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En de nacht <rhyme label="a" type="f" corresp="#s4:l1">verdwijnet</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Lief ghy zijt mijn <rhyme label="b" type="m" corresp="#s4:l2">licht</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">Uwe <rhyme label="c" type="f">Sonne-oogen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Mijne ziel verlichten // <rhyme label="d" type="m">claer</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Ach ick moet <rhyme label="c" type="f" corresp="#s4:l5">verdrooghen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">LIefd komt my doorschichten // <rhyme label="d" type="m" corresp="#s4:l6">gaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Ick val aen u <rhyme label="a" type="f">voeten</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Lief van minne <rhyme label="b" type="m">kranck</rhyme>:</l>
        <l id="s5:l3" met="3" type="+-">Wilt den druck <rhyme label="a" type="f" corresp="#s5:l1">versoeten</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Schenckt my liefdes <rhyme label="b" type="m" corresp="#s5:l2">dranck</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">U bloet hoogh <rhyme label="c" type="f">verheven</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Is mijn Medecijne // <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">U doot doet mijn <rhyme label="c" type="f" corresp="#s5:l5">leven</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">En maeckt de Helsche pijne // <rhyme label="d" type="m" corresp="#s5:l6">soet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Werelts <rhyme label="a" type="f">ydelheden</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Ick ga ver van <rhyme label="b" type="m">u</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">'s Landts <rhyme label="a" type="f" corresp="#s6:l1">gerechtigheden</rhyme></l>
        <l id="s6:l4" met="3" type="+-">My bedecken <rhyme label="b" type="m" corresp="#s6:l2">nu</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Ick gae voor Gods <rhyme label="c" type="f">Throone</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Vry van u besmetten // <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Eene Goude <rhyme label="c" type="f" corresp="#s6:l5">kroone</rhyme></l>
        <l id="s6:l8" met="4" type="+-">'t Lam op mijn hooft setten // <rhyme label="d" type="m" corresp="#s6:l6">sal</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Eer sy Godt den <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Eere 't reyen <rhyme label="b" type="m">Lam</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">d'Heylge Geest te <rhyme label="a" type="f" corresp="#s7:l1">gader</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Looft Godt heylge <rhyme label="b" type="m" corresp="#s7:l2">stam</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Vallet voor Gods <rhyme label="c" type="f">Throone</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Ende roept met herten // <rhyme label="d" type="m">bly</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Eer sy Godes <rhyme label="c" type="f" corresp="#s7:l5">Sone</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Die ons maeckt van smerten // <rhyme label="d" type="m" corresp="#s7:l6">vry</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
