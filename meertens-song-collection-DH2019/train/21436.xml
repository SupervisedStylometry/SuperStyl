    <location>
      <orginal-name>emden]</orginal-name>
      <standard-name>Emden, Germany</standard-name>
      <coordinates>
        <lon>53.3670442</lon>
        <lat>7.2058004</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Ik had een gestadig minnetje</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe soudick my <rhyme label="a" type="f">verblijden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In al mijn boosheyt <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Want die tot allen <rhyme label="a" type="f" corresp="#s1:l1">tijden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Al naer mijn siele <rhyme label="b" type="m" corresp="#s1:l2">staet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">God ist dien ick <rhyme label="b" type="m" corresp="#s1:l2">verlaet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En gae naer vreemde <rhyme label="c" type="f">Goden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Wijckende van zijn <rhyme label="c" type="f" corresp="#s1:l6">gheboden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">En doe der boosheyt <rhyme label="b" type="m" corresp="#s1:l2">raet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick hebbe hooren <rhyme label="a" type="f">claghen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ouer mijn boosheyt <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Want die mijn siele <rhyme label="a" type="f" corresp="#s2:l1">iaghen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die haecken naer mijn <rhyme label="b" type="m" corresp="#s2:l2">doot</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Daer toe dat herte <rhyme label="b" type="m" corresp="#s2:l2">vloot</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Om ouerspel te <rhyme label="c" type="f">bedrijuen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Mocht ick noch troost <rhyme label="c" type="f" corresp="#s2:l6">ghecrijghen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Ick en claechde gheenen <rhyme label="b" type="m" corresp="#s2:l2">noot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soude hy my oock <rhyme label="a" type="f">begheuen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die eens ghelaten <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Sijn Rijcke ende hier zijn <rhyme label="a" type="f" corresp="#s3:l1">leuen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Uoor my die nv dus <rhyme label="b" type="m" corresp="#s3:l2">sneeft</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En met ander herders <rhyme label="b" type="m" corresp="#s3:l2">leeft</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Boelerende met <rhyme label="c" type="f">ontrouwen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dwelck my wel mach <rhyme label="c" type="f" corresp="#s3:l6">berouwen</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Want my mijn cracht <rhyme label="b" type="m" corresp="#s3:l2">begheeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick wil gaen <rhyme label="a" type="f">soecken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Al inder <rhyme label="b" type="m">Roosegaert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En vraghen in alle <rhyme label="a" type="f" corresp="#s4:l1">hoecken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Naer hem seer <rhyme label="b" type="m" corresp="#s4:l2">onghespaert</rhyme></l>
        <l id="s4:l5" met="3" type="-+">So Ieremias <rhyme label="b" type="m" corresp="#s4:l2">verclaert</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En laten nv <rhyme label="c" type="f">dboeleren</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Uan hout en steen <rhyme label="c" type="f" corresp="#s4:l6">passeren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Daer door my therte <rhyme label="b" type="m" corresp="#s4:l2">beswaert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Het staet my al te <rhyme label="a" type="f">haten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Daer ick my op <rhyme label="b" type="m">verliet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En schuwen die hooghe <rhyme label="a" type="f" corresp="#s5:l1">staten</rhyme></l>
        <l id="s5:l4" met="3" type="-+">En volghen hem sonder <rhyme label="b" type="m" corresp="#s5:l2">verdriet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Och wat is mijns <rhyme label="b" type="m" corresp="#s5:l2">gheschiet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat ick hem hebbe <rhyme label="c" type="f">begheuen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Den Coninck van mijn <rhyme label="c" type="f" corresp="#s5:l6">leuen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Want buyten hem en leeftmen <rhyme label="b" type="m" corresp="#s5:l2">niet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De Goden deser <rhyme label="a" type="f">weerelt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Sijn boos naer Schrifts <rhyme label="b" type="m">vermaen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Al zijnse net <rhyme label="a" type="f" corresp="#s6:l1">bepeerelt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Sy moeten alle <rhyme label="b" type="m" corresp="#s6:l2">vergaen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Dus wil ickse al <rhyme label="b" type="m" corresp="#s6:l2">versmaen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">En wederomme <rhyme label="c" type="f">keeren</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Tot mijnen bruydegom met <rhyme label="c" type="f" corresp="#s6:l6">eeren</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Hy sal my wel <rhyme label="b" type="m" corresp="#s6:l2">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Al dat hier is voor <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dats stof en asschen <rhyme label="b" type="m">snoot</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Dus wil ick my tot hem <rhyme label="a" type="f" corresp="#s7:l1">pooghen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Die voor my inde <rhyme label="b" type="m" corresp="#s7:l2">doot</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Ghinck door lijden <rhyme label="b" type="m" corresp="#s7:l2">groot</rhyme></l>
        <l id="s7:l6" met="3" type="-+">En heuet mijn last <rhyme label="c" type="f">ghedraghen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Om mijn schult is hy <rhyme label="c" type="f" corresp="#s7:l6">gheslaghen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">So die Schrift tuyghet <rhyme label="b" type="m" corresp="#s7:l2">bloot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Prince aenhoort mijn <rhyme label="a" type="f">kermen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ick come tot v <rhyme label="b" type="m">beschaemt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Ick bidde v wilt mijns <rhyme label="a" type="f" corresp="#s8:l1">ontfermen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Want v dat wel <rhyme label="b" type="m" corresp="#s8:l2">betaemt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Want ghy daerom hier <rhyme label="b" type="m" corresp="#s8:l2">quaemt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Dus wil ict al <rhyme label="c" type="f">verlaten</rhyme></l>
        <l id="s8:l7" met="3" type="-+">En houden my na der <rhyme label="c" type="f" corresp="#s8:l6">maten</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Met v in ionsten <rhyme label="b" type="m" corresp="#s8:l2">versaemt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
