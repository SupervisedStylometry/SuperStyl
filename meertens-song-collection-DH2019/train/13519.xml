        <l id="s1:l4" met="3" type="-+">Uw glans is <rhyme label="b" type="m" corresp="#s1:l2">over-schoon</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Gy zyt de <rhyme label="c" type="f">Morgen-sterre</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Uw straelen strekken <rhyme label="c" type="f" corresp="#s1:l5">verre</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Gy zyt der Sterren <rhyme label="b" type="m" corresp="#s1:l2">kroon</rhyme>.</l>
