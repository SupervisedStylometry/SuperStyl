<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>1509</id>
    <date>
      <year-start>1613</year-start>
      <year-end>1613</year-end>
    </date>
    <location>
      <orginal-name>amsterdam?</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon lief gij zijt prijs waard alleen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MEt vreught, en deught, schoon Lief <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Bemin ick u <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Bemin ick u <rhyme label="b" type="m" corresp="#s1:l2">voorwaer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Vreught, verheught, en kan <rhyme label="a" type="f" corresp="#s1:l1">ghenesen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Mijn bitter lijden <rhyme label="b" type="m" corresp="#s1:l2">swaer</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn bitter lijden <rhyme label="b" type="m" corresp="#s1:l2">swaer</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Sonder u Lief <rhyme label="b" type="m" corresp="#s1:l2">eerbaer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Is my 't leven een <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Dus laet, ick raet, van u <rhyme label="c" type="f" corresp="#s1:l8">verwerven</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Dus laet, ick raet, van u <rhyme label="c" type="f" corresp="#s1:l8">verwerven</rhyme>,</l>
        <l id="s1:l11" met="5" type="-+">Deur praet, tot baet, in desen nieuwen <rhyme label="b" type="m" corresp="#s1:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al eer, 'thert teer, u oyt jonst <rhyme label="a" type="f">gonde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Schoot Venus in my klaer, <rhyme label="b" type="m">schoot</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Schoot Venus in my klaer, <rhyme label="b" type="m" corresp="#s2:l2">schoot</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">'tBegeer, soo seer, tot dat ick <rhyme label="a" type="f" corresp="#s2:l1">vonde</rhyme></l>
        <l id="s2:l5" met="3" type="-+">V minnelijck wesen daer, u <rhyme label="b" type="m" corresp="#s2:l2">minnelijck</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">V minnelijck wesen daer, u <rhyme label="b" type="m" corresp="#s2:l2">minnelijck</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Ick nu aen vind' geen <rhyme label="b" type="m" corresp="#s2:l2">maer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Soo dat ick u moet <rhyme label="c" type="f">minnen</rhyme></l>
        <l id="s2:l9" met="4" type="-+">Laet 't hert, verwert, nu troost <rhyme label="c" type="f" corresp="#s2:l8">ghewinnen</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Laet 't hert, verwert, nu troost <rhyme label="c" type="f" corresp="#s2:l8">ghewinnen</rhyme>,</l>
        <l id="s2:l11" met="5" type="-+">Mijn smert, drijft vert, in desen nieuwen <rhyme label="b" type="m" corresp="#s2:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Reyn vrou, mijn rou, en sal niet <rhyme label="a" type="f">enden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Voor dat ick gantsch en gaer, <rhyme label="b" type="m">voor</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Voor dat ick gantsch en gaer, <rhyme label="b" type="m" corresp="#s3:l2">voor</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Ghetrou, aenschou, dat ghy gaet <rhyme label="a" type="f" corresp="#s3:l1">senden</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Troost tegens mijn ghevaer, <rhyme label="b" type="m" corresp="#s3:l2">troost</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Troost tegens mijn ghevaer, <rhyme label="b" type="m" corresp="#s3:l2">troost</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Want ick gheen moet en <rhyme label="b" type="m" corresp="#s3:l2">spaer</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Deughtsame vrou <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Dat ghy, seer bly, u sout <rhyme label="c" type="f" corresp="#s3:l8">begeven</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Dat ghy, seer bly, u sout <rhyme label="c" type="f" corresp="#s3:l8">begeven</rhyme>,</l>
        <l id="s3:l11" met="5" type="-+">Tot my, heel vry, in desen nieuwen <rhyme label="b" type="m" corresp="#s3:l2">Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ionst biet, 'tverdriet, Princes sal <rhyme label="a" type="f">keeren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Weer van u trou dienaer, weer <rhyme label="b" type="m">van</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Weer van u trou dienaer, weer <rhyme label="b" type="m" corresp="#s4:l2">van</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Iae 't Liet, gheschiet, tot uwer <rhyme label="a" type="f" corresp="#s4:l1">eeren</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Lieft my nu voor een aer, lieft <rhyme label="b" type="m" corresp="#s4:l2">my</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Lieft my nu voor een aer, lieft <rhyme label="b" type="m" corresp="#s4:l2">my</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Laet ons worden een <rhyme label="b" type="m" corresp="#s4:l2">paer</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En wilt qua klappers <rhyme label="c" type="f">snoeyen</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Met vlijt, verblijt, laet liefde <rhyme label="c" type="f" corresp="#s4:l8">bloeyen</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Met vlijt, verblijt, laet liefde <rhyme label="c" type="f" corresp="#s4:l8">bloeyen</rhyme>,</l>
        <l id="s4:l11" met="5" type="-+">Subijt, wient spijt, in desen nieuwen <rhyme label="b" type="m" corresp="#s4:l2">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
