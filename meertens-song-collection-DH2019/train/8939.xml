<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8939</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Meine Seele erhebt den Herrn mein</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Siel verheft den Heeren <rhyme label="a" type="m">myn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Myn Gheest doet hem <rhyme label="b" type="f">ontspringen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">In dien, die sal myn Heylant <rhyme label="a" type="m" corresp="#s1:l1">zijn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maria soo gaet <rhyme label="b" type="f" corresp="#s1:l2">singhen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Die ned'richeyt, van syn <rhyme label="c" type="m">Dienstmeyt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Heeft aenghesien die <rhyme label="d" type="f">Heere</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Siet van nu an// sal <rhyme label="e" type="m">Yederman</rhyme></l>
        <l id="s1:l8" met="3" type="-+">My salich prijsen <rhyme label="d" type="f" corresp="#s1:l6">seere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want hy heeft dingen groot aen <rhyme label="a" type="m">my</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ghedaen, die daer is <rhyme label="b" type="f">machtich</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wiens name is (bekenn' ick <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme>)</l>
        <l id="s2:l4" met="3" type="-+">Gantsch heylich en <rhyme label="b" type="f" corresp="#s2:l2">waerachtich</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">In eewicheyt// seer wel <rhyme label="c" type="m">bereyt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Blijft syn <rhyme label="d" type="f">barmhertichede</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Vast over dien// die hem <rhyme label="e" type="m">ontsien</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En Kintlijck vreesen <rhyme label="d" type="f" corresp="#s2:l6">mede</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy bruyckt ghewelt met synen <rhyme label="a" type="m">arm</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die stout Hovaert <rhyme label="b" type="f">beminnen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Verstroyt hy met grooten <rhyme label="a" type="m" corresp="#s3:l1">alarm</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In haerer herten <rhyme label="b" type="f" corresp="#s3:l2">sinnen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Hy stoot oock van// den stoel den <rhyme label="c" type="m">man</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die weldich is om <rhyme label="d" type="f">veere</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">En die daer zijn, verdruckt door <rhyme label="e" type="m">pijn</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Verheft hy hooch met <rhyme label="d" type="f" corresp="#s3:l6">eere</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die hong'rigen vult hy met <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">En weldaet <rhyme label="b" type="f">veelderhanden</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die rijcken vol van <rhyme label="a" type="m" corresp="#s4:l1">overvloet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hy ledich maeckt met <rhyme label="b" type="f" corresp="#s4:l2">schanden</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Vergeet oock niet, maer dencket <rhyme label="c" type="m">siet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Aen zijn <rhyme label="d" type="f">Barmhertichede</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">En helpet recht, zijn trouwen <rhyme label="e" type="m">knecht</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Israel op in <rhyme label="d" type="f" corresp="#s4:l6">vrede</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghelijck als hy heeft <rhyme label="a" type="m">toegeseyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En vastelijck <rhyme label="b" type="f">gesworen</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Onsen Vaders voor langen <rhyme label="a" type="m" corresp="#s5:l1">tijt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die hy had <rhyme label="b" type="f" corresp="#s5:l2">uytvercoren</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Den Abraham, en van zijn <rhyme label="c" type="m">stam</rhyme></l>
        <l id="s5:l6" met="3" type="-+">T'saedt dat hercomen <rhyme label="d" type="f">soude</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Wijt uytghespreyt// in <rhyme label="e" type="m">eewicheyt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Hy hem met haer <rhyme label="d" type="f" corresp="#s5:l6">vertroude</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
