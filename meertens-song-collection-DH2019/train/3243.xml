    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Psalm 038 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Joannes Cloeck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ach mijn God ick kom u <rhyme label="a" type="f">klagen</rhyme>!</l>
        <l id="s1:l2" met="2" type="+-">Met <rhyme label="a" type="f" corresp="#s1:l1">mishagen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat dat droevig eygen <rhyme label="b" type="m">Ick</rhyme></l>
        <l id="s1:l4" met="4" type="+-">My noch houd soo vast <rhyme label="c" type="f">gebonden</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Aan de <rhyme label="c" type="f" corresp="#s1:l4">zonden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En noch steeds is tot een <rhyme label="b" type="m" corresp="#s1:l3">strick</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als ick my denck te <rhyme label="a" type="f">begeven</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Om te <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Heyliglijck na u <rhyme label="b" type="m">geboon</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">O dat Ick komt my <rhyme label="c" type="f">beletten</rhyme>!</l>
        <l id="s2:l5" met="2" type="+-">Ja zich <rhyme label="c" type="f" corresp="#s2:l4">zetten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Stoutelijck op uwen <rhyme label="b" type="m" corresp="#s2:l3">troon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">En zoeckt u zelfs t’ <rhyme label="a" type="f">overheeren</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Heer der <rhyme label="a" type="f" corresp="#s3:l1">Heeren</rhyme>!</l>
        <l id="s3:l3" met="4" type="+-">Zich te maken tot een <rhyme label="b" type="m">God</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Door ‘t eygen te <rhyme label="c" type="f">betrachten</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">En ‘t <rhyme label="c" type="f" corresp="#s3:l4">verkrachten</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Van u Goddelijck <rhyme label="b" type="m" corresp="#s3:l3">Gebod</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Daar het niet wil onder <rhyme label="a" type="f">duygen</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Noch na <rhyme label="a" type="f" corresp="#s4:l1">buygen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Maar gedurig <rhyme label="b" type="m">tegen-kant</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Pogende op zijne <rhyme label="c" type="f">wijse</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Foey ick <rhyme label="c" type="f" corresp="#s4:l4">grijse</rhyme>!</l>
        <l id="s4:l6" met="4" type="+-">God te zetten na zijn <rhyme label="b" type="m" corresp="#s4:l3">hand</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ach ick ben dat Ick soo <rhyme label="a" type="f">moede</rhyme>!</l>
        <l id="s5:l2" met="2" type="+-">‘k Kan dat <rhyme label="a" type="f" corresp="#s5:l1">woede</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Niet meer dulden van dat <rhyme label="b" type="m">Ick</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Red my van mijn selven <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Wilt het <rhyme label="c" type="f" corresp="#s5:l4">were</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">‘t Is mijn ziel zoo <rhyme label="b" type="m" corresp="#s5:l3">schadelick</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laat ick doch niet meer Ick <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Maar <rhyme label="a" type="f" corresp="#s6:l1">verdreven</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Zijn al ‘t geen na eygen <rhyme label="b" type="m">haakt</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">En uyt my verbannen <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Steeds na <rhyme label="c" type="f" corresp="#s6:l4">desen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Al wat na zich zelven <rhyme label="b" type="m" corresp="#s6:l3">smaakt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Leert my Heere niet met all te <rhyme label="a" type="f">zijn</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Gy het al <rhyme label="a" type="f" corresp="#s7:l1">zijn</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Drijft het mijne doch daar <rhyme label="b" type="m">uyt</rhyme>;</l>
        <l id="s7:l4" met="4" type="+-">Eygen wil, en eygen <rhyme label="c" type="f">lusten</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Die daar <rhyme label="c" type="f" corresp="#s7:l4">rusten</rhyme></l>
        <l id="s7:l6" met="4" type="+-">In my, met wat daar uyt <rhyme label="b" type="m" corresp="#s7:l3">spruyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dat ick my in u <rhyme label="a" type="f">verliese</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s8:l1">verkiese</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Uwen wil oock voor de <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">En ‘k my selven zoo mach <rhyme label="c" type="f">dragen</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Al mijn <rhyme label="c" type="f" corresp="#s8:l4">dagen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Dat ick een met u mach <rhyme label="b" type="m" corresp="#s8:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">En ‘k voortaan my zelven <rhyme label="a" type="f">eenig</rhyme></l>
        <l id="s9:l2" met="2" type="+-">Heel <rhyme label="a" type="f" corresp="#s9:l1">alleenig</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Aan u Heer my <rhyme label="b" type="m">overgeef</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">En door ‘t mijne te <rhyme label="c" type="f">verzaaken</rhyme></l>
        <l id="s9:l5" met="2" type="+-">‘t Zoo mach <rhyme label="c" type="f" corresp="#s9:l4">maaken</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Dat ick u ter eeren <rhyme label="b" type="m" corresp="#s9:l3">leef</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
