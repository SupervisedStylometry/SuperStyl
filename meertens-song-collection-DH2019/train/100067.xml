    <melody>Laatst als de nachtgodin Diana</melody>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SAl noch mijn Engel <rhyme label="a" type="m">Clorid√©e</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">My langer houden in dees <rhyme label="a" type="m" corresp="#s1:l1">wee</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En noch myn Geesje laten <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ach 'k ben al af <rhyme label="c" type="m">geslooft</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ick kan geen ramp meer <rhyme label="b" type="f" corresp="#s1:l3">dragen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Want mijn leven <rhyme label="c" type="m" corresp="#s1:l4">dooft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maer dat my van u versse <rhyme label="a" type="m">Mondt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Een sapich Cusjen waer <rhyme label="a" type="m" corresp="#s2:l1">gejondt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hoe loof ick ben ick sou my <rhyme label="b" type="f">buygen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En vougen daer ick <rhyme label="c" type="m">sou</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dan van u Nectar <rhyme label="b" type="f" corresp="#s2:l3">zuygen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Aengename <rhyme label="c" type="m" corresp="#s2:l4">Vrou</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ay Nimphje vought u blijde <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Eens by myn die oock vreughdigh zy <rhyme label="a" type="m" corresp="#s3:l1">geweest</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Als dan u Ieught, myn Ieught doet <rhyme label="b" type="f">bloeyen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sal myn herschape <rhyme label="c" type="m">jeught</rhyme></l>
        <l id="s3:l5" met="3" type="-+">U weer met dau <rhyme label="b" type="f" corresp="#s3:l3">besproeyen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Midden inde <rhyme label="c" type="m" corresp="#s3:l4">vreught</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
