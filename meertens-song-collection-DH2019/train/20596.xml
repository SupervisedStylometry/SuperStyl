    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Joan Ysermans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">,,DIe Fortun door gonft <rhyme label="a" type="m">verblijt</rhyme></l>
        <l id="s1:l2" met="2" type="+-">,,Wort <rhyme label="a" type="m" corresp="#s1:l1">bestrijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">T'is aen Peleus vvel <rhyme label="b" type="f">ghebleken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als hy Thetijs had vol <rhyme label="a" type="m" corresp="#s1:l1">vrijt</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Heeft den <rhyme label="a" type="m" corresp="#s1:l1">nijt</rhyme></l>
        <l id="s1:l6" met="4" type="+-">t'Bruyloft volck met t'vvist <rhyme label="b" type="f" corresp="#s1:l3">onsteken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Twee-dracht brocht daer by <rhyme label="a" type="m">gheval</rhyme></l>
        <l id="s2:l2" met="2" type="+-">In het <rhyme label="a" type="m" corresp="#s2:l1">dal</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Eenen Appel root van <rhyme label="b" type="f">gouden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Waer op stont die van u <rhyme label="a" type="m" corresp="#s2:l1">al</rhyme></l>
        <l id="s2:l5" met="2" type="+-">t'Schoonst sijn <rhyme label="a" type="m" corresp="#s2:l1">sal</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Die sal desen prijs <rhyme label="b" type="f" corresp="#s2:l3">behouden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Iuno, Pallas, en oock <rhyme label="a" type="m">me</rhyme></l>
        <l id="s3:l2" met="2" type="+-">
          <rhyme label="a" type="m" corresp="#s3:l1">CytherÃ«</rhyme>
        </l>
        <l id="s3:l3" met="4" type="+-">Sochten dese eer te <rhyme label="b" type="f">crijghen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Want elck voor haer schoonheyt <rhyme label="a" type="m" corresp="#s3:l1">stre</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Gansch <rhyme label="a" type="m" corresp="#s3:l1">t'onvre</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Des al d'ander moesten <rhyme label="b" type="f" corresp="#s3:l3">svvijghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Paris riepen sy te <rhyme label="a" type="m">saem</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Is <rhyme label="a" type="m" corresp="#s4:l1">bequaem</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Om hier oordeel van te <rhyme label="b" type="f">vvijsen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Die sy thoonden <rhyme label="a" type="m" corresp="#s4:l1">aen-ghenaem</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Hun <rhyme label="a" type="m" corresp="#s4:l1">lichaem</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Maer meest moest hy Venus <rhyme label="b" type="f" corresp="#s4:l3">prijsen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Venus door belofte <rhyme label="a" type="m">trouvv'</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Gaf hem <rhyme label="a" type="m" corresp="#s5:l1">bouvv'</rhyme></l>
        <l id="s5:l3" met="4" type="+-">t'Paragon van alle <rhyme label="b" type="f">menschen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Maer den Bruydegom en <rhyme label="a" type="m" corresp="#s5:l1">souvv'</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Voor sijn <rhyme label="a" type="m" corresp="#s5:l1">vrouvv'</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Wed'ick, geen Helena <rhyme label="b" type="f" corresp="#s5:l3">vvenschen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
