  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">V Minne gunst // is <rhyme label="a" type="m">t’overkunst</rhyme></l>
        <l id="s1:l2" met="3" type="-+">O gey,e <rhyme label="b" type="f">Herderinnetjes</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En t’ooghenblick // is als een <rhyme label="c" type="m">strick</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Voor ons verliefde <rhyme label="b" type="f" corresp="#s1:l2">sinnetjes</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Als ick int groen // eens dertel <rhyme label="a" type="m">soen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">V nat bedaude <rhyme label="b" type="f">lipjes</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Soo vind ick mijn // gheseylt te <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Aen twee Corale <rhyme label="b" type="f" corresp="#s2:l2">Clipjes</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daer ick dan koom // tegen de <rhyme label="a" type="m">stroom</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van de ghebruysde <rhyme label="b" type="f">baertjes</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Soo dat ick voel // doort soet <rhyme label="c" type="m">ghewoel</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Een bevingh in mijn <rhyme label="b" type="f" corresp="#s3:l2">aertjes</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick steeck het sweert // en span de <rhyme label="a" type="m">gheert</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Nu styfjes dan weer <rhyme label="b" type="f">slapjes</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dan clim ick op // tot byde <rhyme label="c" type="m">top</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Vander Hooft-touwen <rhyme label="b" type="f" corresp="#s4:l2">trapjes</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daer ick de schoot // vast aende <rhyme label="a" type="m">boot</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Belegh met strick en <rhyme label="b" type="f">knoopjes</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En deur het nat // maeck ick weer <rhyme label="c" type="m">rat</rhyme></l>
        <l id="s5:l4" met="3" type="-+">De stramme <rhyme label="b" type="f" corresp="#s5:l2">overloopjes</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">En sien ick weer // als ick <rhyme label="a" type="m">laveer</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Nu hoochjes dan weer <rhyme label="b" type="f">laechjes</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dat ick niet win // soo vind ick <rhyme label="c" type="m">in</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Een reefjen voor de <rhyme label="b" type="f" corresp="#s6:l2">vlaechjes</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Tis tovery // siel <rhyme label="a" type="m">rovery</rhyme></l>
        <l id="s7:l2" met="3" type="-+">En soete <rhyme label="b" type="f">Boereknaepjes</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Als wy met u // int Bosjen <rhyme label="c" type="m">luw</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Gaen houden middach <rhyme label="b" type="f" corresp="#s7:l2">slaepjes</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Daer ghy met ons // den brant des <rhyme label="a" type="m">Sons</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ontschuylt in diepe <rhyme label="b" type="f">holletjes</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Daer ghy ons solt // daer ghy ons <rhyme label="c" type="m">rolt</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Als kittelsiecke <rhyme label="b" type="f" corresp="#s8:l2">snolletjes</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Het schijnt dat ghy // gheen <rhyme label="a" type="m">vryery</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Ghebruyckt, maer <rhyme label="b" type="f">toverkunsjes</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dat ghy kort // deelachtich <rhyme label="c" type="m">wort</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Ons lieve Minne <rhyme label="b" type="f" corresp="#s9:l2">gunsjes</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">En Coridon // myn Goude <rhyme label="a" type="m">Son</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ick haet al myn <rhyme label="b" type="f">Vriendinnetjes</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Wanneer u beelt // soo haestich <rhyme label="c" type="m">speelt</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Int diepste van myn <rhyme label="b" type="f" corresp="#s10:l2">sinnetjes</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Wanneer ick voel, het soet <rhyme label="a" type="m">ghewoel</rhyme></l>
        <l id="s11:l2" met="3" type="-+">En twemfelen uwes <rhyme label="b" type="f">tonghetjes</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Myn hertjen teer // doet op en <rhyme label="c" type="m">neer</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Veel Minnelijcke <rhyme label="b" type="f" corresp="#s11:l2">sprongetjes</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Het nockt, het beeft // het lockt, het <rhyme label="a" type="m">leeft</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Nu laechjes dan weer <rhyme label="b" type="f">hoochjes</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Het tuycht en spreeckt // wat myn <rhyme label="c" type="m">ghebreeckt</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Deurt dryven van myn <rhyme label="b" type="f" corresp="#s12:l2">ooghjes</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Maer als ick wen // en beter <rhyme label="a" type="m">ken</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Tusschen door beyde <rhyme label="b" type="f">rotsjes</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Soo peur ick an // ghelijck een <rhyme label="c" type="m">Man</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Tot spyt van Thetys <rhyme label="b" type="f" corresp="#s13:l2">trotsjes</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Daer myn de vloet // strackx dryven <rhyme label="a" type="m">doet</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Tusschen twee hooghe <rhyme label="b" type="f">baeckjes</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Waer ick met vlyt // strackx buyten <rhyme label="c" type="m">smyt</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Het Ancker met twee <rhyme label="b" type="f" corresp="#s14:l2">haeckjes</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">En als myn hert // heel quackjes <rhyme label="a" type="m">wert</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Doort wellen vande <rhyme label="b" type="f">stroomjes</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Soo legh ick aen // een sloech te <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Naer de belade <rhyme label="b" type="f" corresp="#s15:l2">Boomjes</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Maer Ameril  // wat coude <rhyme label="a" type="m">gril</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Cruypt door myn <rhyme label="b" type="f">moedeleetjes</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Als ick opt strant // gae deur het <rhyme label="c" type="m">sant</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Met wanckelbare <rhyme label="b" type="f" corresp="#s16:l2">treetjes</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Myn dunckt de Zee // waer in ick <rhyme label="a" type="m">tree</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Staech welt onder myn <rhyme label="b" type="f">voetjes</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Dan vloetse seer // dan ebtse <rhyme label="c" type="m">weer</rhyme></l>
        <l id="s17:l4" met="3" type="-+">En hippelt oock seer <rhyme label="b" type="f" corresp="#s17:l2">soetjes</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Het klaecht tot dat // ick weet niet <rhyme label="a" type="m">wat</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Voor wonderlijcke <rhyme label="b" type="f">cluchjes</rhyme></l>
        <l id="s18:l3" met="4" type="-+">Door ons geslacht // soo weerdich <rhyme label="c" type="m">acht</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Verdryven al myn <rhyme label="b" type="f" corresp="#s18:l2">suchjes</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">O Coridon // mijn Goude <rhyme label="a" type="m">Son</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Hoe syt ghy nu dus <rhyme label="b" type="f">stilletjes</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Is dan u lust // soo haest <rhyme label="c" type="m">gheblust</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Van u verhitte <rhyme label="b" type="f" corresp="#s19:l2">willetjes</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Wiens inghewant // int eerste <rhyme label="a" type="m">brant</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Soo vierichjes, soo <rhyme label="b" type="f">graechjes</rhyme></l>
        <l id="s20:l3" met="4" type="-+">Als of de vreucht // van uwe <rhyme label="c" type="m">Jeucht</rhyme></l>
        <l id="s20:l4" met="3" type="-+">Sou dueren duysent <rhyme label="b" type="f" corresp="#s20:l2">daechjes</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Maer als terstont // u vast <rhyme label="a" type="m">verbont</rhyme></l>
        <l id="s21:l2" met="3" type="-+">En Ionckend soete <rhyme label="b" type="f">lachjes</rhyme></l>
        <l id="s21:l3" met="4" type="-+">Verkeerden in // gheveynsde <rhyme label="c" type="m">min</rhyme></l>
        <l id="s21:l4" met="3" type="-+">En trouweloose <rhyme label="b" type="f" corresp="#s21:l2">clachjes</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Eerst vluchten wy // doe suchten <rhyme label="a" type="m">ghy</rhyme></l>
        <l id="s22:l2" met="3" type="-+">Met meer minne <rhyme label="b" type="f">sanghetjes</rhyme>,</l>
        <l id="s22:l3" met="4" type="-+">Nu vlucht ghy schuw, nu soeck ick <rhyme label="c" type="m">u</rhyme></l>
        <l id="s22:l4" met="3" type="-+">Met nat betraende <rhyme label="b" type="f" corresp="#s22:l2">wanghetjes</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
