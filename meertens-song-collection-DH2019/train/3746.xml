    <melody>Psalm 143 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WILT myn ghebedt O Heer <rhyme label="a" type="f">verhooren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Laet doch kommen tot uwen <rhyme label="a" type="f" corresp="#s1:l1">ooren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Myn smeeken end myn <rhyme label="b" type="m">truericheit</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">End nae uwe goetheit al <rhyme label="a" type="f" corresp="#s1:l1">vooren</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Antwoordt my in myn <rhyme label="b" type="m" corresp="#s1:l3">teghenheit</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt Heer met uwen knecht niet <rhyme label="a" type="f">treden</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">In't recht nae v' <rhyme label="a" type="f" corresp="#s2:l1">gherechticheden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat hy niet komm' in straff' end <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Want Heer gheen mensche hier <rhyme label="a" type="f" corresp="#s2:l1">beneden</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">En kan voor v onschuldigh <rhyme label="b" type="m" corresp="#s2:l3">sijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Myn viandt veruolght my gaer <rhyme label="a" type="f">seere</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Om my neder te werpen <rhyme label="a" type="f" corresp="#s3:l1">Heere</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Hy laetet niet blyuen daer <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maer in eenen kuyl met <rhyme label="a" type="f" corresp="#s3:l1">oneere</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Als waer' ick doot, verberght hy <rhyme label="b" type="m" corresp="#s3:l3">my</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daer doer is myn siel' seer <rhyme label="a" type="f">belaeden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Met benauwtheit vroegh ende <rhyme label="a" type="f" corresp="#s4:l1">spaede</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ick schyne verlaeten met <rhyme label="b" type="m">haest</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Dies doer dees teghenheit end <rhyme label="a" type="f" corresp="#s4:l1">schaede</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Is myn hert' beroert end <rhyme label="b" type="m" corresp="#s4:l3">verbaest</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">In desen duysteren kuyl <rhyme label="a" type="f">klachtigh</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ben ick des ouden tydts <rhyme label="a" type="f" corresp="#s5:l1">ghedachtigh</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">End' uwer wercken Heer seer <rhyme label="b" type="m">goet</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Ick verhael' t'mynen trooste <rhyme label="a" type="f" corresp="#s5:l1">krachtigh</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Die grootdaeden die v' handt <rhyme label="b" type="m" corresp="#s5:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Daer sucht'ick seer in sulcken <rhyme label="a" type="f">standen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">End strecke tot v Heer' myn <rhyme label="a" type="f" corresp="#s6:l1">handen</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Myn siel' is doer t'roepen <rhyme label="b" type="m">ghelyck</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Den dorren wtghedrooghden <rhyme label="a" type="f" corresp="#s6:l1">landen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">End als een seer dorstigh <rhyme label="b" type="m" corresp="#s6:l3">Ä™erdtryck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Verhoort my nu, haestt v Heer <rhyme label="a" type="f">goedigh</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">T'hert' is flauw, ick ben schier <rhyme label="a" type="f" corresp="#s7:l1">kleynmoedigh</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En verberght my v' aenschyn <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Of ick moet hem ghelyck sijn <rhyme label="a" type="f" corresp="#s7:l1">spoedigh</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Dien men in die graeuen diep' <rhyme label="b" type="m" corresp="#s7:l3">schiett</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Laet my vroegh v' ghenaed' <rhyme label="a" type="f">aenschauwen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Op v staet myn hop' in't <rhyme label="a" type="f" corresp="#s8:l1">benauwen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Maeckt my doch den rechten wegh <rhyme label="b" type="m">kondt</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Dien ick gaen moet, want Heer vol <rhyme label="a" type="f" corresp="#s8:l1">trauwen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Tot v heff'ick op hert' end <rhyme label="b" type="m" corresp="#s8:l3">mondt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">O Godt myn hopeningh' seer <rhyme label="a" type="f">reyne</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Verlost my wt den noot niet <rhyme label="a" type="f" corresp="#s9:l1">kleyne</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Myner vianden wreedt end <rhyme label="b" type="m">fel</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Ghy sijt Heer myn toevlucht <rhyme label="a" type="f" corresp="#s9:l1">alleyne</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Ia ghy O Godt end niemant <rhyme label="b" type="m" corresp="#s9:l3">el</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Leert my Heer nae v' <rhyme label="a" type="f">welbehaeghen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Wandelen recht sonder <rhyme label="a" type="f" corresp="#s10:l1">vertzaeghen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Want ghy doch sijt myn Godt <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Dat uwe Gheest my al myn <rhyme label="a" type="f" corresp="#s10:l1">daeghen</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">My leyd' in uwen wegh <rhyme label="b" type="m" corresp="#s10:l3">eerbaer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">O Heer' wilt doch doer uwen <rhyme label="a" type="f">naeme</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Myn siele verquicken <rhyme label="a" type="f" corresp="#s11:l1">bequaeme</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Ende leuendigh maeken <rhyme label="b" type="m">bly</rhyme>:</l>
        <l id="s11:l4" met="4" type="-+">Verlost my wt noot angst end <rhyme label="a" type="f" corresp="#s11:l1">blaeme</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Doer uwe gherechticheit <rhyme label="b" type="m" corresp="#s11:l3">vry</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Myn vianden die my <rhyme label="a" type="f">bestryden</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Doet Heer' te niet tot desen <rhyme label="a" type="f" corresp="#s12:l1">tyden</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Doer v' goetheit, verderft oock <rhyme label="b" type="m">slecht</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Sy, die myn siel' aendoen groot <rhyme label="a" type="f" corresp="#s12:l1">lyden</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Want ick ben v' ghetrauwe <rhyme label="b" type="m" corresp="#s12:l3">knecht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
