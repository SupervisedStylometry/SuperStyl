      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Maria <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Seer excellente <rhyme label="a" type="m" corresp="#s1:l1">persoon</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Mijn haet u lof te singhen <rhyme label="b" type="m">pooght</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Der Maeghden <rhyme label="a" type="m" corresp="#s1:l1">croon</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Waaerachtich Salomons <rhyme label="a" type="m" corresp="#s1:l1">throon</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Naer mijn ghebedt u ooren <rhyme label="b" type="m" corresp="#s1:l3">booght</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">En u groote bermherticheyt <rhyme label="b" type="m" corresp="#s1:l3">tooght</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Dat ick uwen <rhyme label="c" type="m">dienaer</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Hier mach prijsen een <rhyme label="c" type="m" corresp="#s1:l8">paer</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En u deuchden seer <rhyme label="c" type="m" corresp="#s1:l8">claer</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">En u weghen <rhyme label="c" type="m" corresp="#s1:l8">eerbaer</rhyme>:</l>
        <l id="s1:l12" met="3" type="-+">Mijn hert tot u <rhyme label="b" type="m" corresp="#s1:l3">verhooght</rhyme>.</l>
        <l id="s1:l13" met="4" type="-+">En my van alle sonden <rhyme label="b" type="m" corresp="#s1:l3">drooght</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Godts Moeder <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Een Maeght der Maeghden <rhyme label="a" type="m" corresp="#s2:l1">certeyn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Duyfken van Noe seer <rhyme label="b" type="m">ghetrouw</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">V macht niet <rhyme label="a" type="m" corresp="#s2:l1">cleyn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Thoont ons toch alle <rhyme label="a" type="m" corresp="#s2:l1">ghemeyn</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Troost ons altijt in onsen <rhyme label="b" type="m" corresp="#s2:l3">rouw</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Want ghy zijt een seer weerdighe <rhyme label="b" type="m" corresp="#s2:l3">vrouw</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Wien saet <rhyme label="c" type="m">excellent</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Heeft dat helsche <rhyme label="c" type="m" corresp="#s2:l8">serpent</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">D'welck ons bracht in <rhyme label="c" type="m" corresp="#s2:l8">torment</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Verplet en <rhyme label="c" type="m" corresp="#s2:l8">gheschent</rhyme>:</l>
        <l id="s2:l12" met="3" type="-+">Naest Godt ick op u <rhyme label="b" type="m" corresp="#s2:l3">bouw'</rhyme></l>
        <l id="s2:l13" met="4" type="-+">Wilt my helpen uyt dit <rhyme label="b" type="m" corresp="#s2:l3">benouw</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Des levens <rhyme label="a" type="m">hout</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Spieghel sonder eenighe <rhyme label="a" type="m" corresp="#s3:l1">fout</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Leere die Iacob heeft sien <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Slach-oorden <rhyme label="a" type="m" corresp="#s3:l1">stout</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Verwermt ons herte seer <rhyme label="a" type="m" corresp="#s3:l1">kout</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Leert ons den rechten wech in <rhyme label="b" type="m" corresp="#s3:l3">gaen</rhyme>;</l>
        <l id="s3:l7" met="4" type="-+">Want t'is toch al u macht <rhyme label="b" type="m" corresp="#s3:l3">onderdaen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Godt den Vader u <rhyme label="c" type="m">mindt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Godt den Soone is u <rhyme label="c" type="m" corresp="#s3:l8">kindt</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Den heylighen Gheest is u <rhyme label="c" type="m" corresp="#s3:l8">vrindt</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Al den Hemel u <rhyme label="c" type="m" corresp="#s3:l8">diendt</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Den vyant op die <rhyme label="b" type="m" corresp="#s3:l3">baen</rhyme></l>
        <l id="s3:l13" met="4" type="-+">Moet voor u aenschijn vlieden <rhyme label="b" type="m" corresp="#s3:l3">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
