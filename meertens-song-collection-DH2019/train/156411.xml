<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156411</id>
    <date>
      <year-start>1628</year-start>
      <year-end>1628</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon liefje jent zeer excellent</melody>
  <author>Augustinus van Teylingen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Apostel <rhyme label="a" type="m">fier</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Francois <rhyme label="a" type="m" corresp="#s1:l1">Xavier</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Die voor veel <rhyme label="b" type="f">onwijsen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Door Godts <rhyme label="a" type="m" corresp="#s1:l1">bestier</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Op een soete <rhyme label="a" type="m" corresp="#s1:l1">manier</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ontvonckt met Hemels <rhyme label="a" type="m" corresp="#s1:l1">vier</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Den Son weer deede <rhyme label="b" type="f" corresp="#s1:l3">rijsen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Den Son wiens <rhyme label="a" type="m">dach</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Men schijnen <rhyme label="a" type="m" corresp="#s2:l1">sach</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">In ’t Oost langhe te <rhyme label="b" type="f">vooren</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Als Thomas <rhyme label="a" type="m" corresp="#s2:l1">plagh</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Met yverich <rhyme label="a" type="m" corresp="#s2:l1">ghewach</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Te roepen, siet op, <rhyme label="a" type="m" corresp="#s2:l1">ach</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En laet geen licht <rhyme label="b" type="f" corresp="#s2:l3">verlooren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Groot-moedich <rhyme label="a" type="m">Helt</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Ghy zijt <rhyme label="a" type="m" corresp="#s3:l1">ghestelt</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Voor die <rhyme label="b" type="f">Indianen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Die <rhyme label="a" type="m" corresp="#s3:l1">op-ghebelt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Nu moedich zijn te <rhyme label="a" type="m" corresp="#s3:l1">velt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Tegen Satans <rhyme label="a" type="m" corresp="#s3:l1">ghewelt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met Kruycen en met <rhyme label="b" type="f" corresp="#s3:l3">vanen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Iapan hebt <rhyme label="a" type="m">ghy</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Bekeert daer <rhyme label="a" type="m" corresp="#s4:l1">by</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En veel <rhyme label="b" type="f">onderwesen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Daer nu zy <rhyme label="a" type="m" corresp="#s4:l1">vry</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zijn van <rhyme label="a" type="m" corresp="#s4:l1">Afgodery</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En van ’s Duyvels <rhyme label="a" type="m" corresp="#s4:l1">gery</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ten vollen zijn <rhyme label="b" type="f" corresp="#s4:l3">ghenesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ghy hebt <rhyme label="a" type="m">gheboon</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Staet op, o <rhyme label="a" type="m" corresp="#s5:l1">Doon</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Sy zijn rasch op <rhyme label="b" type="f">gheresen</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Ghesondt en <rhyme label="a" type="m" corresp="#s5:l1">schoon</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Stonden zy daer ten <rhyme label="a" type="m" corresp="#s5:l1">toon</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En hebben Godes <rhyme label="a" type="m" corresp="#s5:l1">Soon</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Gedanckt ende <rhyme label="b" type="f" corresp="#s5:l3">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">V liefden <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Sterck als de <rhyme label="a" type="m" corresp="#s6:l1">doot</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Deede u <rhyme label="b" type="f">verschijnen</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Op eenen <rhyme label="a" type="m" corresp="#s6:l1">stoot</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">In het schip ende <rhyme label="a" type="m" corresp="#s6:l1">boot</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Daer zy waren in <rhyme label="a" type="m" corresp="#s6:l1">noodt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">En meenden te <rhyme label="b" type="f" corresp="#s6:l3">verdwijnen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Tot Ignatium <rhyme label="a" type="m">vroet</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Schreef ghy met <rhyme label="a" type="m" corresp="#s7:l1">ootmoet</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Op uwe knien <rhyme label="b" type="f">gheboghen</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Den Hemel <rhyme label="a" type="m" corresp="#s7:l1">goet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Die den kleynen <rhyme label="a" type="m" corresp="#s7:l1">behoedt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Heeft u met liefde <rhyme label="a" type="m" corresp="#s7:l1">soet</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">In de locht op <rhyme label="b" type="f" corresp="#s7:l3">ghetoghen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Ghy zijt den <rhyme label="a" type="m">Man</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Die ‘tHeyr <rhyme label="a" type="m" corresp="#s8:l1">verwan</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Door yvere <rhyme label="b" type="f">ghebeden</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Die geven <rhyme label="a" type="m" corresp="#s8:l1">kan</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">De droppelen die <rhyme label="a" type="m" corresp="#s8:l1">van</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">V volheyt komen <rhyme label="a" type="m" corresp="#s8:l1">an</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Vol van eeuwighe <rhyme label="b" type="f" corresp="#s8:l3">vreden</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Al ‘swerelts <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Stier ghy met de <rhyme label="a" type="m" corresp="#s9:l1">voet</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">En zijt <rhyme label="b" type="f">ghebleven</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Vast van <rhyme label="a" type="m" corresp="#s9:l1">ghemoet</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Als een vroom-vechter <rhyme label="a" type="m" corresp="#s9:l1">doet</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">In’t suyr soo braef als <rhyme label="a" type="m" corresp="#s9:l1">‘tsoet</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Om hier boven te <rhyme label="b" type="f" corresp="#s9:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Ghy hebt <rhyme label="a" type="m">gheleert</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Iesum <rhyme label="a" type="m" corresp="#s10:l1">begheert</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">In leven en in <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Sijn Cruys u <rhyme label="a" type="m" corresp="#s10:l1">eert</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Door sweeren <rhyme label="a" type="m" corresp="#s10:l1">uyt-gheteert</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En wonder <rhyme label="a" type="m" corresp="#s10:l1">preserveert</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">V lichaem van <rhyme label="b" type="f" corresp="#s10:l3">bederven</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Prince, sta <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">Aen onse <rhyme label="a" type="m" corresp="#s11:l1">zy</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">En wilt van nu <rhyme label="b" type="f">verschoonen</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Ons fauten <rhyme label="a" type="m" corresp="#s11:l1">vry</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">By Iesus t’aller <rhyme label="a" type="m" corresp="#s11:l1">ty</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Die ons <rhyme label="a" type="m" corresp="#s11:l1">ghebenedy</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Van boven uyt zijn <rhyme label="b" type="f" corresp="#s11:l3">throonen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
