<song>
  <meta>
    <id>32798</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Nu wees gegroet Maria zoet</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy gheestelijcke maeghden <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wilt met <rhyme label="b" type="f">Lof-sanghen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Mariam die reyn Maghet <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt haer toch eer <rhyme label="b" type="f" corresp="#s1:l2">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt haer altijt naer mijn <rhyme label="a" type="m">bedien</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Als Maeght en Moeder <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die van 't beginsel was <rhyme label="a" type="m" corresp="#s2:l1">voorsien</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Tot een Moeder des <rhyme label="b" type="f" corresp="#s2:l2">Heeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zy was verciert die suyver <rhyme label="a" type="m">maecht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Met deughden <rhyme label="b" type="f">overvloedich</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maer sonderlingh heeft Godt <rhyme label="a" type="m" corresp="#s3:l1">behaeght</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Om dat sy was <rhyme label="b" type="f" corresp="#s3:l2">ootmoedigh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gheen tonghe can uytspreken <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Haer deught, en heylich <rhyme label="b" type="f">leven</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Een herte kan't bedencken <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hoe sy nu is <rhyme label="b" type="f" corresp="#s4:l2">verheven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dus prijst haer alle Maeghden <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Haer deughden wilt <rhyme label="b" type="f">aenmercken</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Wilt ghy met haer in vreughden <rhyme label="a" type="m" corresp="#s5:l1">zijn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Volght naer haer goede <rhyme label="b" type="f" corresp="#s5:l2">wercken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
