  <author>Jacob Claesz</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">VVEest nu <rhyme label="a" type="m">verblijdt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In desen laetsten <rhyme label="a" type="m" corresp="#s1:l1">tijdt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Want Godt <rhyme label="a" type="m" corresp="#s1:l1">ghebenedijt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Heeft door Liefden <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Tot ons <rhyme label="a" type="m" corresp="#s1:l1">profijt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ghesonden die ons al <rhyme label="a" type="m" corresp="#s1:l1">bevrijt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Iesum sijn Soon <rhyme label="a" type="m" corresp="#s1:l1">subijt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dus zijt vrolijck in <rhyme label="b" type="f" corresp="#s1:l4">desen</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Want hy <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Ons vrijt <rhyme label="c" type="m" corresp="#s1:l9">eenpaer</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Int nieuwe Iaer <rhyme label="b" type="f" corresp="#s1:l4">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Duyvel doot <rhyme label="a" type="m">Hel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Met al haer zwaer <rhyme label="a" type="m" corresp="#s2:l1">ghequel</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Heeft hy verwonnen <rhyme label="a" type="m" corresp="#s2:l1">wel</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Door Liefden uyt <rhyme label="b" type="f">ghelesen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">O Mens <rhyme label="a" type="m" corresp="#s2:l1">rebel</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Keert u tot hem seer <rhyme label="a" type="m" corresp="#s2:l1">snel</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En doet na sijn <rhyme label="a" type="m" corresp="#s2:l1">bevel</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Soo sal hy u <rhyme label="b" type="f" corresp="#s2:l4">ghenesen</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Want hy <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Ons vrijt <rhyme label="c" type="m" corresp="#s2:l9">eenpaer</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Int nieuwe Iaer <rhyme label="b" type="f" corresp="#s2:l4">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dit is het <rhyme label="a" type="m">saet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat belooft was tot onser <rhyme label="a" type="m" corresp="#s3:l1">baet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Der vromen <rhyme label="a" type="m" corresp="#s3:l1">toeverlaet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dus sijt vrolijck in <rhyme label="b" type="f">desen</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">In de Wet <rhyme label="a" type="m" corresp="#s3:l1">staet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Hier af gheschreven boven <rhyme label="a" type="m" corresp="#s3:l1">maet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">d' Apostels trompet <rhyme label="a" type="m" corresp="#s3:l1">slaet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Hier af seer hooch <rhyme label="b" type="f" corresp="#s3:l4">gheresen</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Want hy <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Ons vrijt <rhyme label="c" type="m" corresp="#s3:l9">eenpaer</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Int nieuwe Iaer <rhyme label="b" type="f" corresp="#s3:l4">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wy laghen <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Al inder sonden <rhyme label="a" type="m" corresp="#s4:l1">noot</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Maer sijnen Liefde <rhyme label="a" type="m" corresp="#s4:l1">groot</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Toonde hy ons in <rhyme label="b" type="f">desen</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Ons menschen <rhyme label="a" type="m" corresp="#s4:l1">snoot</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Verloste hy seer <rhyme label="a" type="m" corresp="#s4:l1">bloot</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Al met sijn bloede <rhyme label="a" type="m" corresp="#s4:l1">root</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Aent cruys wert hy <rhyme label="b" type="f" corresp="#s4:l4">gheresen</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Want hy <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Ons vrijt <rhyme label="c" type="m" corresp="#s4:l9">eenpaer</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Int nieuwe Iaer <rhyme label="b" type="f" corresp="#s4:l4">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Die nacht <rhyme label="a" type="m">onsoet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Verdwijnt doort licht seer <rhyme label="a" type="m" corresp="#s5:l1">goet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hy wies ons met sijn <rhyme label="a" type="m" corresp="#s5:l1">bloet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wy die laghen <rhyme label="b" type="f">verknesen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Dus doet nu <rhyme label="a" type="m" corresp="#s5:l1">boet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ghelooft sijn name <rhyme label="a" type="m" corresp="#s5:l1">vroet</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Gheeft u tot waer <rhyme label="a" type="m" corresp="#s5:l1">ootmoet</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Wilt sijn Majesteyt <rhyme label="b" type="f" corresp="#s5:l4">vreesen</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Want hy <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">Ons vrijt <rhyme label="c" type="m" corresp="#s5:l9">eenpaer</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Int nieuwe Iaer <rhyme label="b" type="f" corresp="#s5:l4">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Prince <rhyme label="a" type="m">devijn</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Toont ons u vriendelijck <rhyme label="a" type="m" corresp="#s6:l1">aenschijn</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ghy cont ons Ziel <rhyme label="a" type="m" corresp="#s6:l1">verblijn</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Ende gheheel <rhyme label="b" type="f">ghenesen</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Op dit <rhyme label="a" type="m" corresp="#s6:l1">termijn</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">In d'aenghename <rhyme label="a" type="m" corresp="#s6:l1">tijn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Ghy sijt ons <rhyme label="a" type="m" corresp="#s6:l1">Medecijn</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">O Ziel keert u tot <rhyme label="b" type="f" corresp="#s6:l4">desen</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Want hy <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">Ons vrijt <rhyme label="c" type="m" corresp="#s6:l9">eenpaer</rhyme></l>
        <l id="s6:l11" met="3" type="+-">Int nieuwe Iaer <rhyme label="b" type="f" corresp="#s6:l4">ghepresen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
