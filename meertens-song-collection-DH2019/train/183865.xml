  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Godheid! die, aan al wat <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Bij ontelbre <rhyme label="b" type="f">gunstbewijzen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Onderhoud en voedsel <rhyme label="a" type="m" corresp="#s1:l1">geeft</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Duld, daar wij uw goedheid <rhyme label="b" type="f" corresp="#s1:l2">prijzen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat de dank en <rhyme label="c" type="m">vreugdetoon</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Klimme tot uw' <rhyme label="c" type="m" corresp="#s1:l5">hemeltroon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Doe ons steeds de <rhyme label="a" type="m">matigheid</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Onder 't blijd genot, <rhyme label="b" type="f">betrachten</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Overdaad, die smart <rhyme label="a" type="m" corresp="#s2:l1">bereidt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">En den mensch verlaagt, <rhyme label="b" type="f" corresp="#s2:l2">verächten</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Zo daal' zegen, <rhyme label="c" type="m">Opperheer</rhyme>!</l>
        <l id="s2:l6" met="4" type="+-">Op deez' disch milddaadig <rhyme label="c" type="m" corresp="#s2:l5">neêr</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
