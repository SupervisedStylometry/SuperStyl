  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">EEne Henglaar aan een <rhyme label="a" type="m">Sloot</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Met zyn Hengeltuigje <rhyme label="a" type="m" corresp="#s1:l1">bloot</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Zag een jonge <rhyme label="b" type="m">Maagt</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Voor hem <rhyme label="c" type="f">pronken</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s1:l4">belonken</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Zag een jonge <rhyme label="b" type="m" corresp="#s1:l3">Maagt</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die aan hem voort heeft <rhyme label="b" type="m" corresp="#s1:l3">gevraagt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vrind hoe gaat het visschen <rhyme label="a" type="m">aan</rhyme>?</l>
        <l id="s2:l2" met="4" type="+-">Zeg hebt gy al wat <rhyme label="a" type="m" corresp="#s2:l1">gevaân</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Wilt uw <rhyme label="b" type="m">wurmbak</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Niet <rhyme label="c" type="f">ontbloote</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Van die <rhyme label="c" type="f" corresp="#s2:l4">groote</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Wilt uw <rhyme label="b" type="m" corresp="#s2:l3">wurmbak</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Niet ontbloote, wagt tot <rhyme label="b" type="m" corresp="#s2:l3">strak</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want ik weet ô! <rhyme label="a" type="m">Hengelaar</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Eene Slootje dat is <rhyme label="a" type="m" corresp="#s3:l1">klaar</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">En daar is voor <rhyme label="b" type="m">u</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Visch te <rhyme label="c" type="f">vangen</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Met <rhyme label="c" type="f" corresp="#s3:l4">verlangen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">En daar is voor <rhyme label="b" type="m" corresp="#s3:l3">uw</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Wis te vangen, zyt niet <rhyme label="b" type="m" corresp="#s3:l3">schuw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Neemt uw Hengeltuichje <rhyme label="a" type="m">meê</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'k Zalje brengen op die <rhyme label="a" type="m" corresp="#s4:l1">steê</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En zo gy daar <rhyme label="b" type="m">mist</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Zou ik <rhyme label="c" type="f">liegen</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">U <rhyme label="c" type="f" corresp="#s4:l4">bedriegen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">'k Wed gy niet en <rhyme label="b" type="m" corresp="#s4:l3">mift</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Zo je maar knaphandig <rhyme label="b" type="m" corresp="#s4:l3">vist</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Daar meê ging dien jonge <rhyme label="a" type="m">Kwant</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Met dat Meisje door het <rhyme label="a" type="m" corresp="#s5:l1">Land</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Zy wees hem <rhyme label="b" type="m">terstond</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Eene <rhyme label="c" type="f">Vyver</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Hy vol <rhyme label="c" type="f" corresp="#s5:l4">yver</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Zy wees hem <rhyme label="b" type="m" corresp="#s5:l3">terstond</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Eene Vyver zonder <rhyme label="b" type="m" corresp="#s5:l3">grond</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Hy maakt voort zyn Hengel <rhyme label="a" type="m">klaar</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Neemt een Wurm en ging <rhyme label="a" type="m" corresp="#s6:l1">daar</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Voort heel rads zyn <rhyme label="b" type="m">gang</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Hy wel hoek <rhyme label="c" type="f">kreeg</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Maar geen Snoek <rhyme label="c" type="f" corresp="#s6:l4">kreeg</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Hy ging weer zyn <rhyme label="b" type="m" corresp="#s6:l3">gang</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">'t Zoete Meisje zag niet <rhyme label="b" type="m" corresp="#s6:l3">bang</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">'t Vissen dat ging na haar <rhyme label="a" type="m">zin</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Riep t'za leg nu nog reis <rhyme label="a" type="m" corresp="#s7:l1">in</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Hier is t'zelde <rhyme label="b" type="m">mis</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">Hy al <rhyme label="c" type="f">weder</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Op en <rhyme label="c" type="f" corresp="#s7:l4">neder</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Hier is 't zelde <rhyme label="b" type="m" corresp="#s7:l3">mis</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Hy kreeg hoek en had een <rhyme label="b" type="m" corresp="#s7:l3">Vis</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Hier op sprak de jonge <rhyme label="a" type="m">Maagd</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Vissertje dat my <rhyme label="a" type="m" corresp="#s8:l1">behaagd</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Gaat meê daar ik <rhyme label="b" type="m">woon</rhyme></l>
        <l id="s8:l4" met="2" type="+-">Heene <rhyme label="c" type="f">streeve</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">'k Zal jou <rhyme label="c" type="f" corresp="#s8:l4">geeve</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">Gaat meê daar ik <rhyme label="b" type="m" corresp="#s8:l3">woon</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">'k Zalje geeve ryklyk <rhyme label="b" type="m" corresp="#s8:l3">loon</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Hy van 't Visse reedlyk <rhyme label="a" type="m">moe</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Ging meê na haar woning <rhyme label="a" type="m" corresp="#s9:l1">toe</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Zy sprak wilje <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s9:l4" met="2" type="+-">'k Zalje <rhyme label="c" type="f">geven</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Om te <rhyme label="c" type="f" corresp="#s9:l4">leven</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Zy sprak wilje <rhyme label="b" type="m" corresp="#s9:l3">my</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Ik geef u myn <rhyme label="b" type="m" corresp="#s9:l3">Vislery</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Deze wierde door de <rhyme label="a" type="m">Trouw</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Met elkander Man en <rhyme label="a" type="m" corresp="#s10:l1">Vrouw</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Hy bleef <rhyme label="b" type="m">Hengelaar</rhyme>,</l>
        <l id="s10:l4" met="2" type="+-">Zy <rhyme label="c" type="f">genegen</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">Daar nooit <rhyme label="c" type="f" corresp="#s10:l4">tegen</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Hy bleef <rhyme label="b" type="m" corresp="#s10:l3">Hengelaar</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">En zy hield haar Vyver <rhyme label="b" type="m" corresp="#s10:l3">klaar</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
