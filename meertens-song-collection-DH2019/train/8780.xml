<song>
  <meta>
    <id>8780</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Aus tiefer Not schrei ich zu dir</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VErhoort my als ick roep tot <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">GOdt mijn <rhyme label="b" type="f">gherechticheede</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghy die in Ancxten troostet <rhyme label="a" type="m" corresp="#s1:l1">my</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ghenadich hoort mijn <rhyme label="b" type="f" corresp="#s1:l2">Beede</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Ghy Hanssen hier op Aerden <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wat sal mijn Eer in uwen <rhyme label="c" type="m" corresp="#s1:l5">Moet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Noch lang' ghescheyndet <rhyme label="d" type="f">werden</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">D'onwaerheyt is by u <rhyme label="a" type="m">bemint</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ghy hebt die Leugen <rhyme label="b" type="f">geeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Nochtans men met der Daet <rhyme label="a" type="m" corresp="#s2:l1">bevint</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat wonderlijck die <rhyme label="b" type="f" corresp="#s2:l2">HEEre</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Syn Heyligen regiert end' <rhyme label="c" type="m">voert</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Als ick aenroep, hy my <rhyme label="c" type="m" corresp="#s2:l5">verhoort</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dies troost ick my in <rhyme label="d" type="f">Lijden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sondicht doch niet so <rhyme label="a" type="m">thoornichlijc</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Maer spreeckt met uwen <rhyme label="b" type="f">Herten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Op uwen Leger <rhyme label="a" type="m" corresp="#s3:l1">duldichlijck</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ten sal u nimmer <rhyme label="b" type="f" corresp="#s3:l2">smerten</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Offert den Heere <rhyme label="c" type="m">gherechticheyt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">End' hoopt op hem nae syn <rhyme label="c" type="m" corresp="#s3:l5">bescheyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">So moocht ghy God <rhyme label="d" type="f">behaghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Veel segghen, soud' ons dese <rhyme label="a" type="m">Mensch</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wat goet is, moghen <rhyme label="b" type="f">leeren</rhyme>?</l>
        <l id="s4:l3" met="4" type="-+">Maer HEER tot u is onse <rhyme label="a" type="m" corresp="#s4:l1">wensch</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Wilt selvest tot ons <rhyme label="b" type="f" corresp="#s4:l2">keeren</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Ghenadichlijck u <rhyme label="c" type="m">aenghesicht</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">So wand'len wy in uwen <rhyme label="c" type="m" corresp="#s4:l5">licht</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">End' hebben vry <rhyme label="d" type="f">gheleyde</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">HEER ghy verheucht mijns herten <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Meer dan met coorn en <rhyme label="b" type="f">wijnen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dat ick met vreden slape <rhyme label="a" type="m" corresp="#s5:l1">soet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">All' sorghe moet <rhyme label="b" type="f" corresp="#s5:l2">verdwijnen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Want Heer alleyn ghy zijt myn <rhyme label="c" type="m">schilt</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Ghy maeckt dat ick in rust en <rhyme label="c" type="m" corresp="#s5:l5">stilt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Wel sekerlijck mach <rhyme label="d" type="f">woonen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
