<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14656</id>
    <date>
      <year-start>1633</year-start>
      <year-end>1633</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Schoon liefje jent zeer excellent</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Pieter van Gelre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">GRoot <rhyme label="a" type="m">wonderheydt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Wert ons <rhyme label="a" type="m" corresp="#s1:l1">verbreydt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Van d'Hemelsche <rhyme label="b" type="f">Heyr-scharen</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Want Godts soon <rhyme label="a" type="m" corresp="#s1:l1">leyt</rhyme></l>
        <l id="s1:l5" met="2" type="-+">In't hoy en <rhyme label="a" type="m" corresp="#s1:l1">schreyt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Om ons <rhyme label="a" type="m" corresp="#s1:l1">ellendigheydt</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">Om <rhyme label="c" type="m">ons</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Tot een nieuwe <rhyme label="b" type="f" corresp="#s1:l3">Iare</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Aen Davids <rhyme label="a" type="m">Stadt</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Is dese <rhyme label="a" type="m" corresp="#s2:l1">schat</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ghegheven, te <rhyme label="b" type="f">beware</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Dit' bloedigh <rhyme label="a" type="m" corresp="#s2:l1">badt</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Door't snijdtsel, <rhyme label="a" type="m" corresp="#s2:l1">wat</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van daegh al heeft <rhyme label="a" type="m" corresp="#s2:l1">omvat</rhyme></l>
        <l id="s2:l7" met="1" type="-+">Van <rhyme label="c" type="m">daegh</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Tot eenen nieuwe <rhyme label="b" type="f" corresp="#s2:l3">Iare</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dus <rhyme label="a" type="m">Israel</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Wilt u doch <rhyme label="a" type="m" corresp="#s3:l1">wel</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Bereyden <rhyme label="b" type="f">allegare</rhyme>;</l>
        <l id="s3:l4" met="2" type="-+">Want hy de <rhyme label="a" type="m" corresp="#s3:l1">Hel</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Met Satan <rhyme label="a" type="m" corresp="#s3:l1">fel</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Benemen sal <rhyme label="a" type="m" corresp="#s3:l1">'tghequel</rhyme>.</l>
        <l id="s3:l7" met="1" type="-+">
          <rhyme label="c" type="m">Benemen</rhyme>
        </l>
        <l id="s3:l8" met="3" type="-+">Tot eenen nieuwe <rhyme label="b" type="f" corresp="#s3:l3">Iare</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Looft dan dit <rhyme label="a" type="m">Kindt</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Dat al <rhyme label="a" type="m" corresp="#s4:l1">beghint</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ons yets te <rhyme label="b" type="f">openbaren</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Hoe wel het <rhyme label="a" type="m" corresp="#s4:l1">int</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Verborghen <rhyme label="a" type="m" corresp="#s4:l1">mint</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat doet dat het ons <rhyme label="a" type="m" corresp="#s4:l1">vint</rhyme>,</l>
        <l id="s4:l7" met="1" type="-+">Dat <rhyme label="c" type="m">doet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In dese nieuwe <rhyme label="b" type="f" corresp="#s4:l3">Iare</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
