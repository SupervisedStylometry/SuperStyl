      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Leenaert Clock</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">In nijn jeuchdelijck <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wilt my lief Heere <rhyme label="a" type="f" corresp="#s1:l1">gheven</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Vwe rechte <rhyme label="b" type="m">wijsheyt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Soo in mijn jonghe <rhyme label="c" type="f">daghen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Naer u goet <rhyme label="c" type="f" corresp="#s1:l4">welbehaghen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Mijn hert maken <rhyme label="b" type="m" corresp="#s1:l3">bereyt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Alsdan soo met <rhyme label="a" type="f">verlanghen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Recht gaen in uwe <rhyme label="a" type="f" corresp="#s2:l1">ganghen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Stadich in den smallen <rhyme label="b" type="m">padt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Alhier tot elcker <rhyme label="c" type="f">stonden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Tot dat ick heb <rhyme label="c" type="f" corresp="#s2:l4">ghevonden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Den over-grooten <rhyme label="b" type="m" corresp="#s2:l3">schat</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Nu verborghen der <rhyme label="a" type="f">blinden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">O Heer mocht ick hem <rhyme label="a" type="f" corresp="#s3:l1">vinden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Daer toe gheeft my <rhyme label="b" type="m">t’ghesicht</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Al t’sichtbaer te <rhyme label="c" type="f">verlaten</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">D’eyghen leven te <rhyme label="c" type="f" corresp="#s3:l4">haten</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">My doch alsoo <rhyme label="b" type="m" corresp="#s3:l3">verlicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Niet anders hier <rhyme label="a" type="f">beginnen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dan t’hemelrijck te <rhyme label="a" type="f" corresp="#s4:l1">winnen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Door u ghenade <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Wat my daer toe <rhyme label="c" type="f">ghepresen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hier van nooden sal <rhyme label="c" type="f" corresp="#s4:l4">wesen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Vervult uyt liefde <rhyme label="b" type="m" corresp="#s4:l3">soet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En wilt my Heer recht <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Wat ick tot uwer <rhyme label="a" type="f" corresp="#s5:l1">eeren</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Sal nemen by der <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En hoe ick nu heel <rhyme label="c" type="f">stille</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Sal doen Heer uwen <rhyme label="c" type="f" corresp="#s5:l4">wille</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Hier in dit Aerdtsche <rhyme label="b" type="m" corresp="#s5:l3">landt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Gheen dinck int herte <rhyme label="a" type="f">binnen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Nemen, nochte <rhyme label="a" type="f" corresp="#s6:l1">beginnen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dan dat ghy Heer <rhyme label="b" type="m">bemint</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En daer nae met <rhyme label="c" type="f">verhoeghen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Mijn jeugd’lijck leven <rhyme label="c" type="f" corresp="#s6:l4">voeghen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Als een ghehoorsaem <rhyme label="b" type="m" corresp="#s6:l3">kint</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">En dat ick my mach <rhyme label="a" type="f">wachten</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wt alle mijnen <rhyme label="a" type="f" corresp="#s7:l1">crachten</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Voor dat ghy Heere <rhyme label="b" type="m">haet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En niet en cont <rhyme label="c" type="f">verdraghen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Die Weerelt te <rhyme label="c" type="f" corresp="#s7:l4">behaghen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En wat in haer <rhyme label="b" type="m" corresp="#s7:l3">bestaet</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Genoeghelijck voor <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Staet sy: maer heel <rhyme label="a" type="f" corresp="#s8:l1">bedroghen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Is hy die haer <rhyme label="b" type="m">liefheeft</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Vyandt moet hy <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Behoedt my doch voor <rhyme label="c" type="f" corresp="#s8:l4">desen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">My u ghenade <rhyme label="b" type="m" corresp="#s8:l3">gheeft</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Reyn en heylich <rhyme label="a" type="f">vercoren</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Te leven nae <rhyme label="a" type="f" corresp="#s9:l1">t’behooren</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Als mijnen roep <rhyme label="b" type="m">betaemt</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Soo ghetuyght de <rhyme label="c" type="f">Schriftuere</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Als een nieu <rhyme label="c" type="f" corresp="#s9:l4">creatuere</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Naer uwen naem <rhyme label="b" type="m" corresp="#s9:l3">ghenaemt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Recht doch hier toe mijn <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Van boven tot <rhyme label="a" type="f" corresp="#s10:l1">beneden</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Buyten en binnen <rhyme label="b" type="m">meest</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Mijn sinnen en <rhyme label="c" type="f">ghedachten</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">O Heer, dit zijn mijn <rhyme label="c" type="f" corresp="#s10:l4">clachten</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Stiert my door uwen <rhyme label="b" type="m" corresp="#s10:l3">Gheest</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">In u sterckt mijn <rhyme label="a" type="f">betrouwen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Om mijn huys vast te <rhyme label="a" type="f" corresp="#s11:l1">bouwen</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Dat het oock mach <rhyme label="b" type="m">bestaen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Voor alle swaer <rhyme label="c" type="f">tempeesten</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Winden ende boose <rhyme label="c" type="f" corresp="#s11:l4">gheesten</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Die om de Menschen <rhyme label="b" type="m" corresp="#s11:l3">gaen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Toont u goetheyt, o <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Spoet my te helpen <rhyme label="a" type="f" corresp="#s12:l1">seere</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Mijn vermoghen is <rhyme label="b" type="m">cleyn</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Het is op u <rhyme label="c" type="f">ghenade</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Dat ick my vroech en <rhyme label="c" type="f" corresp="#s12:l4">spade</rhyme></l>
        <l id="s12:l6" met="3" type="-+">‘Gantsch vertrooste <rhyme label="b" type="m" corresp="#s12:l3">alleyn</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Sonder u hulpe <rhyme label="a" type="f">crachtich</rhyme></l>
        <l id="s13:l2" met="3" type="-+">En ben ick gheensins <rhyme label="a" type="f" corresp="#s13:l1">machtich</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Maer die maeckt ons nu <rhyme label="b" type="m">sterck</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Laet my niet gaen te <rhyme label="c" type="f">schanden</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Houdt my in uwe <rhyme label="c" type="f" corresp="#s13:l4">handen</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Bewaert my u <rhyme label="b" type="m" corresp="#s13:l3">handtwerck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
