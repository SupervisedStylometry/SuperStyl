      <category translation="religion">godsdienst</category>
    </categories>
  <author>Dirk Rafaelsz. Camphuysen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">U O Godt, u loven <rhyme label="a" type="m">wy</rhyme>.</l>
        <l id="s1:l2" met="4" type="+-">Want de wond’rens-waerde <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s1:l3" met="4" type="+-">(Door mijn dicht in’t licht <rhyme label="b" type="m" corresp="#s1:l2">gebracht</rhyme>)</l>
        <l id="s1:l4" met="4" type="+-">Vwes Godtheydts is <rhyme label="a" type="m" corresp="#s1:l1">na-by</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Is naby: want sulcken <rhyme label="c" type="m">woordt</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Isser uyt uw’ mondt <rhyme label="c" type="m" corresp="#s1:l5">gehoort</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als de tijdt volboren <rhyme label="a" type="m">is</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Dieder aff-gemeten <rhyme label="b" type="m">staet</rhyme></l>
        <l id="s2:l3" met="4" type="+-">In den on-door-grondden <rhyme label="b" type="m" corresp="#s2:l2">raedt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van mijns wils <rhyme label="a" type="m" corresp="#s2:l1">geheymenis</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dan sal ick ten oordeel <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">En een yeder recht <rhyme label="c" type="m" corresp="#s2:l5">onfaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Deuchde vreuchde, sonde <rhyme label="a" type="m">pijn</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">(Yeder na hy hier <rhyme label="b" type="m">geleeft</rhyme></l>
        <l id="s3:l3" met="4" type="+-">En sijn tijdt vol-togen <rhyme label="b" type="m" corresp="#s3:l2">heeft</rhyme>)</l>
        <l id="s3:l4" met="4" type="+-">Sal mijns oordeels inhoudt <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Niemandts uyt-steck geldter <rhyme label="c" type="m">yet</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Daermen maer op’t recht en <rhyme label="c" type="m" corresp="#s3:l5">siet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Schoon de staet bouw-valligh <rhyme label="a" type="m">staet</rhyme>;</l>
        <l id="s4:l2" met="4" type="+-">Schoon de menschen in haer <rhyme label="b" type="m">geest</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Zijn onseker en <rhyme label="b" type="m" corresp="#s4:l2">bevreest</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Door het dicht-genakend’ <rhyme label="a" type="m" corresp="#s4:l1">quaedt</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Schoon de wetten (stijl van’t <rhyme label="c" type="m">Landt</rhyme>)</l>
        <l id="s4:l6" met="4" type="+-">Moeten schricken uyt haer <rhyme label="c" type="m" corresp="#s4:l5">standt</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Schoon de werelt, <rhyme label="a" type="m">overhoop</rhyme></l>
        <l id="s5:l2" met="4" type="+-">En vol allerley <rhyme label="b" type="m">ellendt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Schijnt te haesten na haer <rhyme label="b" type="m" corresp="#s5:l2">endt</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Met een onbestuerden <rhyme label="a" type="m" corresp="#s5:l1">loop</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Schoon het alles holt en <rhyme label="c" type="m">dolt</rhyme></l>
        <l id="s5:l6" met="4" type="+-">En in’t wilde henen <rhyme label="c" type="m" corresp="#s5:l5">rolt</rhyme>:</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Schoon het onbesuyst <rhyme label="a" type="m">Geval</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Door de loss’ <rhyme label="b" type="m">onordening</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Dieder sweeft in alle <rhyme label="b" type="m" corresp="#s6:l2">ding</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Heer en stuerman schijnt van <rhyme label="a" type="m" corresp="#s6:l1">all</rhyme>’:</l>
        <l id="s6:l5" met="4" type="+-">Noch soo ben Ick, Godt, de <rhyme label="c" type="m">geen</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Die’t al stuer en hou’ by <rhyme label="c" type="m" corresp="#s6:l5">een</rhyme>;</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Die de werelt <rhyme label="a" type="m">onderstut</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Datse vast sta in’t <rhyme label="b" type="m">gebeeff</rhyme>;</l>
        <l id="s7:l3" met="4" type="+-">Die den ramp sijn palen <rhyme label="b" type="m" corresp="#s7:l2">geeff</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">En het onnut maeck te <rhyme label="a" type="m" corresp="#s7:l1">nut</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Die mijn handt door alles <rhyme label="c" type="m">streck</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En noch licht uyt duyster <rhyme label="c" type="m" corresp="#s7:l5">treck</rhyme>;</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Die d’uyt-wettiche’ en haer <rhyme label="a" type="m">wet</rhyme></l>
        <l id="s8:l2" met="4" type="+-">En de <rhyme label="b" type="m">onordentlijckheydt</rhyme></l>
        <l id="s8:l3" met="4" type="+-">(Hoe wanschickelijck <rhyme label="b" type="m" corresp="#s8:l2">verspreydt</rhyme>)</l>
        <l id="s8:l4" met="4" type="+-">Haer gepaste mate <rhyme label="a" type="m" corresp="#s8:l1">set</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Die ’toneffen wijss’lijck <rhyme label="c" type="m">slecht</rhyme>’</l>
        <l id="s8:l6" met="4" type="+-">En alom het kromme <rhyme label="c" type="m" corresp="#s8:l5">recht</rhyme>’.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Dit sprack Godt. Dies mijn <rhyme label="a" type="m">gemoedt</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Sterck getroffen en <rhyme label="b" type="m">doorsne’en</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Door de kracht die ware <rhyme label="b" type="m" corresp="#s9:l2">Re’en</rhyme></l>
        <l id="s9:l4" met="4" type="+-">In een buyghsaem herte <rhyme label="a" type="m" corresp="#s9:l1">doet</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Grooter aendacht heeft <rhyme label="c" type="m">gehadt</rhyme></l>
        <l id="s9:l6" met="4" type="+-">En een diepen ernst <rhyme label="c" type="m" corresp="#s9:l5">gevat</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Och! hoe <rhyme label="a" type="m">menigh-menigh-werff</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Heb ick in <rhyme label="b" type="m">ernstachticheydt</rhyme></l>
        <l id="s10:l3" met="4" type="+-">Tot den dommen hoop <rhyme label="b" type="m" corresp="#s10:l2">geseyt</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">(Hopend’ off hy noch <rhyme label="a" type="m" corresp="#s10:l1">’tverderff</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Tijdelijck ontschampen <rhyme label="c" type="m">mocht</rhyme></l>
        <l id="s10:l6" met="4" type="+-">En het heyl sijns zielen <rhyme label="c" type="m" corresp="#s10:l5">socht</rhyme>)</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Weest (o!) weest niet soo <rhyme label="a" type="m">verbeest</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Niet soo onbesint van <rhyme label="b" type="m">sin</rhyme>;</l>
        <l id="s11:l3" met="4" type="+-">Laet de Wijsheydt eenmael <rhyme label="b" type="m" corresp="#s11:l2">in</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Guntse herbergh in uw’ <rhyme label="a" type="m" corresp="#s11:l1">geest</rhyme>;</l>
        <l id="s11:l5" met="4" type="+-">Geeft de dwaesheydt niet meer <rhyme label="c" type="m">plaets</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Stopt de ader alles <rhyme label="c" type="m" corresp="#s11:l5">quaedts</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">O! Wat heb ick <rhyme label="a" type="m">menigh-reys</rhyme></l>
        <l id="s12:l2" met="4" type="+-">Hier en daer een godloos <rhyme label="b" type="m">mensch</rhyme></l>
        <l id="s12:l3" met="4" type="+-">(Met een hoop’ en <rhyme label="b" type="m" corresp="#s12:l2">herten-wensch</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Dat hy ’tonbeschofte <rhyme label="a" type="m" corresp="#s12:l1">vleys</rhyme></l>
        <l id="s12:l5" met="4" type="+-">In sijn drift sou <rhyme label="c" type="m">tegenstaen</rhyme>)</l>
        <l id="s12:l6" met="4" type="+-">Dus met reden <rhyme label="c" type="m" corresp="#s12:l5">a’ngegaen</rhyme>:</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Waer toe soo onmatich <rhyme label="a" type="m">stout</rhyme>?</l>
        <l id="s13:l2" met="4" type="+-">Waerom swelt u hert soo <rhyme label="b" type="m">dick</rhyme></l>
        <l id="s13:l3" met="4" type="+-">Nu ghy vast een kleynen <rhyme label="b" type="m" corresp="#s13:l2">blick</rhyme></l>
        <l id="s13:l4" met="4" type="+-">Van het vleyend’ luck <rhyme label="a" type="m" corresp="#s13:l1">aenschouwt</rhyme>?</l>
        <l id="s13:l5" met="4" type="+-">Steeckt (o!) steeckt den grootschen <rhyme label="c" type="m">kop</rhyme></l>
        <l id="s13:l6" met="4" type="+-">Niet soo bats ten Hemel <rhyme label="c" type="m" corresp="#s13:l5">op</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Wech met harden neck en <rhyme label="a" type="m">beck</rhyme>:</l>
        <l id="s14:l2" met="4" type="+-">Laet de tonge zijn <rhyme label="b" type="m">besnoeyt</rhyme></l>
        <l id="s14:l3" met="4" type="+-">Daer soo stouten tael uyt <rhyme label="b" type="m" corresp="#s14:l2">vloeyt</rhyme>.</l>
        <l id="s14:l4" met="4" type="+-">Alle uyt-steck heeft <rhyme label="a" type="m" corresp="#s14:l1">gebreck</rhyme>.</l>
        <l id="s14:l5" met="4" type="+-">Weest niet langer al te <rhyme label="c" type="m">trots</rhyme></l>
        <l id="s14:l6" met="4" type="+-">Op gewaende gunst des <rhyme label="c" type="m" corresp="#s14:l5">Lots</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Arbeyt, draeft, en woelt uw’ <rhyme label="a" type="m">best</rhyme>;</l>
        <l id="s15:l2" met="4" type="+-">Al des werelts macht en <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s15:l3" met="4" type="+-">Zy op eenen hoop <rhyme label="b" type="m" corresp="#s15:l2">gebracht</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Vloey te saem van Oost en <rhyme label="a" type="m" corresp="#s15:l1">West</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Koom uyt Bergh en uyt <rhyme label="c" type="m">Woestijn</rhyme></l>
        <l id="s15:l6" met="4" type="+-">En al waers’ op aerd’ magh <rhyme label="c" type="m" corresp="#s15:l5">zijn</rhyme>:</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Hoemen’t schickt, bekloeckt, off <rhyme label="a" type="m">past</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Niemandt, niemandt wieder <rhyme label="b" type="m">leeft</rhyme></l>
        <l id="s16:l3" met="4" type="+-">Die sijns hoocheydts wisheydt <rhyme label="b" type="m" corresp="#s16:l2">heeft</rhyme>;</l>
        <l id="s16:l4" met="4" type="+-">Alle kroonen staen <rhyme label="a" type="m" corresp="#s16:l1">onvast</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Alle throon is <rhyme label="c" type="m">wanckelijck</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">All’ het aerdtsch <rhyme label="c" type="m" corresp="#s16:l5">verganckelijck</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Godt, der dingen <rhyme label="a" type="m">opper-voochdt</rhyme>,</l>
        <l id="s17:l2" met="4" type="+-">Palen-stelder van <rhyme label="b" type="m">’tGeval</rhyme>,</l>
        <l id="s17:l3" met="4" type="+-">Groote Richter over <rhyme label="b" type="m" corresp="#s17:l2">all</rhyme>’,</l>
        <l id="s17:l4" met="4" type="+-">Heer van alle heerschens <rhyme label="a" type="m" corresp="#s17:l1">hoocht</rhyme>’,</l>
        <l id="s17:l5" met="4" type="+-">Middelaer van sacht en <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s17:l6" met="4" type="+-">Geeft al ’swerelts doen een <rhyme label="c" type="m" corresp="#s17:l5">keer</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Menigh mensch, in hooght’ <rhyme label="a" type="m">gestelt</rhyme>,</l>
        <l id="s18:l2" met="4" type="+-">Stout van tredt en groot van <rhyme label="b" type="m">moedt</rhyme></l>
        <l id="s18:l3" met="4" type="+-">Op sijn adelijcke <rhyme label="b" type="m" corresp="#s18:l2">bloedt</rhyme></l>
        <l id="s18:l4" met="4" type="+-">Op sijn rijckdom en <rhyme label="a" type="m" corresp="#s18:l1">geweldt</rhyme>,</l>
        <l id="s18:l5" met="4" type="+-">Werpt hy van het hooge <rhyme label="c" type="m">Rijck</rhyme></l>
        <l id="s18:l6" met="4" type="+-">In verned’rings diepen <rhyme label="c" type="m" corresp="#s18:l5">slijck</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Menigh die eerst schrael en <rhyme label="a" type="m">nauw</rhyme>,</l>
        <l id="s19:l2" met="4" type="+-">Ongeacht en oolijck <rhyme label="b" type="m">leeft</rhyme>,</l>
        <l id="s19:l3" met="4" type="+-">En sijn duyst’ren oorspronck <rhyme label="b" type="m" corresp="#s19:l2">heeft</rhyme></l>
        <l id="s19:l4" met="4" type="+-">uyt de heffe van het <rhyme label="a" type="m" corresp="#s19:l1">Grauw</rhyme>,</l>
        <l id="s19:l5" met="4" type="+-">Brengt Hy ter <rhyme label="c" type="m">uyt-stekentheydt</rhyme></l>
        <l id="s19:l6" met="4" type="+-">uyt den dreck daer hy in <rhyme label="c" type="m" corresp="#s19:l5">leyt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">’tMagh den boosen hier een <rhyme label="a" type="m">poos</rhyme></l>
        <l id="s20:l2" met="4" type="+-">Na begeert’ en wenschen <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s20:l3" met="4" type="+-">En haer dingen schijnbaer <rhyme label="b" type="m" corresp="#s20:l2">staen</rhyme>:</l>
        <l id="s20:l4" met="4" type="+-">Doch haer Nu is geen <rhyme label="a" type="m" corresp="#s20:l1">Altoos</rhyme>.</l>
        <l id="s20:l5" met="4" type="+-">Godt besnoeyt haer <rhyme label="c" type="m">’twaen-geluck</rhyme></l>
        <l id="s20:l6" met="4" type="+-">En verwisselt vreuchd met <rhyme label="c" type="m" corresp="#s20:l5">druck</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Want Hy heeft (aldus <rhyme label="a" type="m">verbeeldt</rhyme></l>
        <l id="s21:l2" met="4" type="+-">Sich de saeck in mijn <rhyme label="b" type="m">gedacht</rhyme></l>
        <l id="s21:l3" met="4" type="+-">Twijl de geest op alles <rhyme label="b" type="m" corresp="#s21:l2">acht</rhyme></l>
        <l id="s21:l4" met="4" type="+-">En uyt kennis kennis <rhyme label="a" type="m" corresp="#s21:l1">teelt</rhyme>)</l>
        <l id="s21:l5" met="4" type="+-">In sijn handt een kelck vol <rhyme label="c" type="m">wijn</rhyme>,</l>
        <l id="s21:l6" met="4" type="+-">Door-gemengelt met <rhyme label="c" type="m" corresp="#s21:l5">venijn</rhyme>:</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Roodt als bloedt, vol <rhyme label="a" type="m">ongeval</rhyme>,</l>
        <l id="s22:l2" met="4" type="+-">Mildt van allerley <rhyme label="b" type="m">ellendt</rhyme>,</l>
        <l id="s22:l3" met="4" type="+-">Die een yeder boos’ in’t <rhyme label="b" type="m" corresp="#s22:l2">endt</rhyme></l>
        <l id="s22:l4" met="4" type="+-">Drincken sal met heff’ met <rhyme label="a" type="m" corresp="#s22:l1">al</rhyme>,</l>
        <l id="s22:l5" met="4" type="+-">Drincken sal (en tegen <rhyme label="c" type="m">danck</rhyme>)</l>
        <l id="s22:l6" met="4" type="+-">Tot sijn eygen <rhyme label="c" type="m" corresp="#s22:l5">onderganck</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Hierentusschen sal mijn <rhyme label="a" type="m">mondt</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Sal mijne stemme, sal mijn <rhyme label="b" type="m">sanck</rhyme>,</l>
        <l id="s23:l3" met="4" type="+-">Met een eeuwigh <rhyme label="b" type="m" corresp="#s23:l2">loff-geklanck</rhyme>,</l>
        <l id="s23:l4" met="4" type="+-">Godes eer gaen maken <rhyme label="a" type="m" corresp="#s23:l1">kondt</rhyme>,</l>
        <l id="s23:l5" met="4" type="+-">Godes eer, dien Iacobs <rhyme label="c" type="m">saet</rhyme></l>
        <l id="s23:l6" met="4" type="+-">Aenbidt en ten dienste <rhyme label="c" type="m" corresp="#s23:l5">staet</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Alle vrucht der <rhyme label="a" type="m">ydelheydt</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">Alle schijn-groot en <rhyme label="b" type="m">geweldt</rhyme></l>
        <l id="s24:l3" met="4" type="+-">Daer een godloos hert door <rhyme label="b" type="m" corresp="#s24:l2">swelt</rhyme>,</l>
        <l id="s24:l4" met="4" type="+-">Sal ter neder zijn <rhyme label="a" type="m" corresp="#s24:l1">geleyt</rhyme>,</l>
        <l id="s24:l5" met="4" type="+-">Sal mijn kloeck gedicht <rhyme label="c" type="m">vertre’en</rhyme></l>
        <l id="s24:l6" met="4" type="+-">Door onfeylb’re waerheydts <rhyme label="c" type="m" corresp="#s24:l5">re’en</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">Maer de Godt-gesinde <rhyme label="a" type="m">ziel</rhyme></l>
        <l id="s25:l2" met="4" type="+-">Die al haren lust en <rhyme label="b" type="m">vreuchdt</rhyme></l>
        <l id="s25:l3" met="4" type="+-">Heeft in onbevleckte <rhyme label="b" type="m" corresp="#s25:l2">Deuchdt</rhyme></l>
        <l id="s25:l4" met="4" type="+-">En noyt van de waerheydt <rhyme label="a" type="m" corresp="#s25:l1">viel</rhyme>,</l>
        <l id="s25:l5" met="4" type="+-">Maeck ick groot, en voerse <rhyme label="c" type="m">op</rhyme></l>
        <l id="s25:l6" met="4" type="+-">Tot des glorijs hooghsten <rhyme label="c" type="m" corresp="#s25:l5">top</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
