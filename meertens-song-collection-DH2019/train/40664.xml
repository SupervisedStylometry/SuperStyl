<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>40664</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Met deze nieuwe jaar</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MEt desen nieuwen <rhyme label="a" type="f">jaere</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Soo wordt ons <rhyme label="a" type="f" corresp="#s1:l1">openbaere</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Hoe dat een Maghet <rhyme label="a" type="f" corresp="#s1:l1">vruchtbaere</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die wereldt heeft <rhyme label="b" type="m">verblijdt</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Ghelovet moet <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s1:l5">kindekijn</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">Ghe-eert moet oock <rhyme label="c" type="m" corresp="#s1:l5">zijn</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s1:l5">Maeghdekijn</rhyme>,</l>
        <l id="s1:l9" met="5" type="+-">Nu ende eeuwelijck tot <rhyme label="b" type="m" corresp="#s1:l4">alder-tijdt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe wel was sy te <rhyme label="a" type="f">moede</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Doen sy in vleesche en <rhyme label="a" type="f" corresp="#s2:l1">bloede</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Aensagh haers herten <rhyme label="a" type="f" corresp="#s2:l1">hoede</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Den Heere der werelt <rhyme label="b" type="m">wijt</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Ghelovet moet <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s2:l5">kindekijn</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">Ghe-eert moet oock <rhyme label="c" type="m" corresp="#s2:l5">zijn</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s2:l5">Maeghdekijn</rhyme>,</l>
        <l id="s2:l9" met="5" type="+-">Nu ende eeuwelijck tot <rhyme label="b" type="m" corresp="#s2:l4">alder-tijdt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy baerd' hem sonder <rhyme label="a" type="f">pijne</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En bleef een Maghet <rhyme label="a" type="f" corresp="#s3:l1">fijne</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Das sondaers <rhyme label="a" type="f" corresp="#s3:l1">medecijne</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dies is de Ioden <rhyme label="b" type="m">nijt</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Ghelovet moet <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s3:l5">kindekijn</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">Ghe-eert moet oock <rhyme label="c" type="m" corresp="#s3:l5">zijn</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s3:l5">Maeghdekijn</rhyme>,</l>
        <l id="s3:l9" met="5" type="+-">Nu ende eeuwelijck tot <rhyme label="b" type="m" corresp="#s3:l4">alder-tijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die Enghels songhen <rhyme label="a" type="f">schoone</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Glory al in den <rhyme label="a" type="f" corresp="#s4:l1">throone</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ter eeren ter <rhyme label="a" type="f" corresp="#s4:l1">croone</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Des kindts <rhyme label="b" type="m">ghebenedijdt</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Ghelovet moet <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s4:l5">kindekijn</rhyme>.</l>
        <l id="s4:l7" met="2" type="-+">Ghe-eert moet oock <rhyme label="c" type="m" corresp="#s4:l5">zijn</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s4:l5">Maeghdekijn</rhyme>,</l>
        <l id="s4:l9" met="5" type="+-">Nu ende eeuwelijck tot <rhyme label="b" type="m" corresp="#s4:l4">alder-tijdt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Naer acht daghen <rhyme label="a" type="f">gheleden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Soo wiert Iesus <rhyme label="a" type="f" corresp="#s5:l1">besneden</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Al naer de Iodsche <rhyme label="a" type="f" corresp="#s5:l1">seden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">T'welck ons van sonden <rhyme label="b" type="m">vrijdt</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Ghelovet moet <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s5:l5">kindekijn</rhyme>.</l>
        <l id="s5:l7" met="2" type="-+">Ghe-eert moet oock <rhyme label="c" type="m" corresp="#s5:l5">zijn</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s5:l5">Maeghdekijn</rhyme>,</l>
        <l id="s5:l9" met="5" type="+-">Nu ende eeuwelijck tot <rhyme label="b" type="m" corresp="#s5:l4">alder-tijdt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wt Orienten <rhyme label="a" type="f">lande</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Quamen ter <rhyme label="a" type="f" corresp="#s6:l1">offerhande</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Dry Heeren <rhyme label="a" type="f" corresp="#s6:l1">onbekande</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van Godt <rhyme label="b" type="m">ghebenedijdt</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Ghelovet moet <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s6:l5">kindekijn</rhyme>.</l>
        <l id="s6:l7" met="2" type="-+">Ghe-eert moet oock <rhyme label="c" type="m" corresp="#s6:l5">zijn</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s6:l5">kindekijn</rhyme>,</l>
        <l id="s6:l9" met="5" type="+-">Nu ende eeuwelijck tot <rhyme label="b" type="m" corresp="#s6:l4">alder-tijdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
