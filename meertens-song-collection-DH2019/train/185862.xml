<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185862</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De nachtegaal die zong een lied</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ghebenedijt, den Heer <rhyme label="a" type="m">altijt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Maeghden <rhyme label="b" type="m">bereyt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die u verblijt, diens naem <rhyme label="a" type="m" corresp="#s1:l1">belijt</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Met <rhyme label="b" type="m" corresp="#s1:l2">dancbaerheyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Laet uwen blijtschap <rhyme label="c" type="f">hooren</rhyme></l>
        <l id="s1:l6" met="1" type="-+">De <rhyme label="c" type="f" corresp="#s1:l5">ooren</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Des Bruygoms schoon int Hemels <rhyme label="d" type="m">troon</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Die u heeft <rhyme label="c" type="f" corresp="#s1:l5">wtvercooren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maeghden bequaem, siet uwen <rhyme label="a" type="m">naem</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Is <rhyme label="b" type="m">suyverheyt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ziel en Lichaem zy <rhyme label="a" type="m" corresp="#s2:l1">aenghedaen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Met <rhyme label="b" type="m" corresp="#s2:l2">eerbaerheyt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In seden heel <rhyme label="c" type="f">manierich</rhyme></l>
        <l id="s2:l6" met="1" type="-+">Oock <rhyme label="c" type="f" corresp="#s2:l5">vierich</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Al int ghebedt, innich te <rhyme label="d" type="m">met</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">V naesten <rhyme label="c" type="f" corresp="#s2:l5">goedertierich</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Suyverheyt puer, Enghels <rhyme label="a" type="m">natuer</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Is heel <rhyme label="b" type="m">ghelijck</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Tot aller uur, al valtet <rhyme label="a" type="m" corresp="#s3:l1">suer</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Behout, ghy <rhyme label="b" type="m" corresp="#s3:l2">rijckt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Verwint u wert <rhyme label="c" type="f">ghegeven</rhyme></l>
        <l id="s3:l6" met="1" type="-+">Nae <rhyme label="c" type="f" corresp="#s3:l5">t’leven</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Der maeghden Croon, suyver en <rhyme label="d" type="m">schoon</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Tot eenen loon <rhyme label="c" type="f" corresp="#s3:l5">verheven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O waeroms begheer, sal zijn de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">En anders <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Simpel in kleer, stichtich <rhyme label="a" type="m" corresp="#s4:l1">verneert</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Om God die <rhyme label="b" type="m" corresp="#s4:l2">t’siet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">T’behaghen schuwt, wilt <rhyme label="c" type="f">mijen</rhyme>,</l>
        <l id="s4:l6" met="1" type="-+"><rhyme label="c" type="f" corresp="#s4:l5">T’partyen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Wort ghy versmaet, bidt voor de <rhyme label="d" type="m">haet</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">V gheest sal hem <rhyme label="c" type="f" corresp="#s4:l5">verblijen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Een maeght eersaem, nae haer <rhyme label="a" type="m">betaem</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Tot deughde <rhyme label="b" type="m">spoedt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">T’wesen vreedsaem, sal altijt <rhyme label="a" type="m" corresp="#s5:l1">staen</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Nae t’opperste <rhyme label="b" type="m" corresp="#s5:l2">goet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">In volheyt <rhyme label="c" type="f">overvloedich</rhyme>,</l>
        <l id="s5:l6" met="1" type="-+"><rhyme label="c" type="f" corresp="#s5:l5">Ootmoedich</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">In liefden soet, wert zy <rhyme label="d" type="m">ghevoet</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">En leeft in God <rhyme label="c" type="f" corresp="#s5:l5">voorspoedich</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">In eenicheyt, zijn Brytt hy <rhyme label="a" type="m">leyt</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">En spreecktse <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">V hert bereyt t’reyn hert (hy <rhyme label="a" type="m" corresp="#s6:l1">seyt</rhyme>)</l>
        <l id="s6:l4" met="2" type="-+">Sal God <rhyme label="b" type="m" corresp="#s6:l2">voorwaer</rhyme></l>
        <l id="s6:l5" met="3" type="-+">In eewicheyt <rhyme label="c" type="f">aenschouwen</rhyme>,</l>
        <l id="s6:l6" met="1" type="-+">Gaet <rhyme label="c" type="f" corresp="#s6:l5">bouwen</rhyme></l>
        <l id="s6:l7" met="4" type="-+">In u ghemoet, hy smaken <rhyme label="d" type="m">doet</rhyme></l>
        <l id="s6:l8" met="3" type="+-">De liefde hoogh vol <rhyme label="c" type="f" corresp="#s6:l5">trouwen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ghy maeghden reyn, die hier zijt <rhyme label="a" type="m">cleyn</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Bewaert u <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ghy sult alleyn, volghen <rhyme label="a" type="m" corresp="#s7:l1">ghemeyn</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Dat Lam waert <rhyme label="b" type="m" corresp="#s7:l2">gaet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ghehoorsaemt <rhyme label="c" type="f">allegader</rhyme>,</l>
        <l id="s7:l6" met="1" type="-+">U <rhyme label="c" type="f" corresp="#s7:l5">Vader</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Al nae den Geest, minst ende <rhyme label="d" type="m">meest</rhyme>,</l>
        <l id="s7:l8" met="3" type="+-">Schriftuer ghetuyght dat <rhyme label="c" type="f" corresp="#s7:l5">claerder</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Princessen fijn, die maeghden <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Voor God <rhyme label="b" type="m">certeyn</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Schout quaet fenijn, van die den <rhyme label="a" type="m" corresp="#s8:l1">schijn</rhyme></l>
        <l id="s8:l4" met="2" type="-+">Hebben <rhyme label="b" type="m" corresp="#s8:l2">onreyn</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Haer woorden, aensicht <rhyme label="c" type="f">mede</rhyme>,</l>
        <l id="s8:l6" met="1" type="-+">Mindt <rhyme label="c" type="f" corresp="#s8:l5">vrede</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Weest wel gemoet, in <rhyme label="d" type="m">teghenspoet</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">T’bereyt u in Gods <rhyme label="c" type="f" corresp="#s8:l5">stede</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
