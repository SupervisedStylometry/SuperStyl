  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WEl Aaltje, wel <rhyme label="a" type="f">Staaltje</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Dat kusje dat <rhyme label="b" type="m">smaakt</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Als zuiker, de <rhyme label="c" type="f">duiker</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Ik voel my <rhyme label="b" type="m" corresp="#s1:l2">geraakt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Van buiten en <rhyme label="d" type="f">binne</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Door 't aardige <rhyme label="d" type="f" corresp="#s1:l5">minne</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Myn hartje van liefde <rhyme label="b" type="m" corresp="#s1:l2">blaakt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Myn troosje, myn roosje, myn <rhyme label="e" type="f">bloosje</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Myn slabbertje <rhyme label="f" type="m">zoet</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Myn jeugd, myn vreugd, Zo vol <rhyme label="g" type="m">geneugd</rhyme>,</l>
        <l id="s1:l11" met="4" type="-+">U lieve lippen, Zyn honing <rhyme label="h" type="f">klippen</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Ik ben verheugd <rhyme label="g" type="m" corresp="#s1:l10">verheugd</rhyme>.</l>
      </lg>
    </lg>
  </text>
