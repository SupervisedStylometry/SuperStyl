    <melody>Psalm 138 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk wil, o Heer, uyt 'sherten <rhyme label="a" type="m">gront</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Met mijnen <rhyme label="a" type="m" corresp="#s1:l1">mont</rhyme></l>
        <l id="s1:l3" met="2" type="-+">V danck <rhyme label="b" type="f">bewijsen</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Ick wil by menich vorst en <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s1:l5" met="2" type="-+">V nae en <rhyme label="c" type="m" corresp="#s1:l4">veer</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Met psalmen <rhyme label="b" type="f" corresp="#s1:l3">prijsen</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Ick wil, Heer, nae u <rhyme label="d" type="m">heylichdom</rhyme></l>
        <l id="s1:l8" met="2" type="-+">My buygen, <rhyme label="d" type="m" corresp="#s1:l7">om</rhyme></l>
        <l id="s1:l9" met="2" type="-+">uw' naem te <rhyme label="e" type="f">singen</rhyme>.</l>
        <l id="s1:l10" met="4" type="-+">Om uwe trou en <rhyme label="f" type="m">goedicheyt</rhyme></l>
        <l id="s1:l11" met="2" type="+-">Ben ick <rhyme label="f" type="m" corresp="#s1:l10">bereyt</rhyme></l>
        <l id="s1:l12" met="2" type="-+">V lof te <rhyme label="e" type="f" corresp="#s1:l9">bringen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want ghy maeckt uwen name <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Als ghy in <rhyme label="a" type="m" corresp="#s2:l1">noot</rhyme></l>
        <l id="s2:l3" met="2" type="-+">V toont <rhyme label="b" type="f">waerachtich</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Als ick u aenroep, Heere, <rhyme label="c" type="m">ghy</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Verhooret <rhyme label="c" type="m" corresp="#s2:l4">my</rhyme></l>
        <l id="s2:l6" met="2" type="-+">En sterckt my <rhyme label="b" type="f" corresp="#s2:l3">crachtich</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">De coningen des aertrijcx <rhyme label="d" type="m">al</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Met bly <rhyme label="d" type="m" corresp="#s2:l7">geschal</rhyme></l>
        <l id="s2:l9" met="2" type="-+">V sullen <rhyme label="e" type="f">eeren</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Wanneer sy hebben <rhyme label="f" type="m">aengehoort</rhyme></l>
        <l id="s2:l11" met="2" type="+-">V heylsaem <rhyme label="f" type="m" corresp="#s2:l10">woort</rhyme></l>
        <l id="s2:l12" met="2" type="-+">'tWelck haer sal <rhyme label="e" type="f" corresp="#s2:l9">leeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy sullen singen gants <rhyme label="a" type="m">verheucht</rhyme></l>
        <l id="s3:l2" met="2" type="-+">De reyne <rhyme label="a" type="m" corresp="#s3:l1">deucht</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Van uwe <rhyme label="b" type="f">wegen</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Want Godes groote <rhyme label="c" type="m">heerlijckheyt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Is wijt <rhyme label="c" type="m" corresp="#s3:l4">verspreyt</rhyme></l>
        <l id="s3:l6" met="2" type="-+">En hooch <rhyme label="b" type="f" corresp="#s3:l3">gestegen</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Al is syn stoel verheven <rhyme label="d" type="m">hooch</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Noch siet syn <rhyme label="d" type="m" corresp="#s3:l7">ooch</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Den mensch <rhyme label="e" type="f">deemoedich</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">De stouten hy van verre <rhyme label="f" type="m">kent</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Die hy <rhyme label="f" type="m" corresp="#s3:l10">toesendt</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Haer straffe <rhyme label="e" type="f" corresp="#s3:l9">spoedich</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wanneer ick ben in <rhyme label="a" type="m">tegenspoet</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Ghy, Heere, <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Mijn siele <rhyme label="b" type="f">leven</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Als mijnes vyants tooren <rhyme label="c" type="m">brandt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">V <rhyme label="c" type="m" corresp="#s4:l4">rechterhant</rhyme></l>
        <l id="s4:l6" met="2" type="-+">My heyl wil <rhyme label="b" type="f" corresp="#s4:l3">geven</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Voor my sult ghy het eynden <rhyme label="d" type="m">drae</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Want u <rhyme label="d" type="m" corresp="#s4:l7">genae</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Moet eeuwich <rhyme label="e" type="f">blijven</rhyme>.</l>
        <l id="s4:l10" met="4" type="-+">Het werck van uwe handen, <rhyme label="f" type="m">Heer</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">Ghy <rhyme label="f" type="m" corresp="#s4:l10">nimmermeer</rhyme></l>
        <l id="s4:l12" met="2" type="-+">Sult laten <rhyme label="e" type="f" corresp="#s4:l9">drijven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
