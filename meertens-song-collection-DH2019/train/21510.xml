    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Erhalt uns Herr bei deinem Wort</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">NU houdt ons Heer by dynen <rhyme label="a" type="m">woort</rhyme></l>
        <l id="s1:l2" met="4" type="-+">En keert den Paus en Turcken <rhyme label="a" type="m" corresp="#s1:l1">moort</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Die Iesum Christum dynen <rhyme label="b" type="m">soon</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Stooten willen van dynen <rhyme label="b" type="m" corresp="#s1:l3">troon</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Bewijst dijn macht Heere Iesu <rhyme label="a" type="m">Christ</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Ghy die Heer alder Heeren <rhyme label="a" type="m" corresp="#s2:l1">bist</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Beschermt dijn arme <rhyme label="b" type="m">Christenheyt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Dat sy v louen inder <rhyme label="b" type="m" corresp="#s2:l3">eewicheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">God heylighe gheeft die trooster <rhyme label="a" type="m">weerdt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Gheeft dijn volck eenderley sin op <rhyme label="a" type="m" corresp="#s3:l1">eerdt</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Staet ons by inder lester <rhyme label="b" type="m">noot</rhyme></l>
        <l id="s3:l4" met="4" type="-+">En leyt ons int leuen wt den <rhyme label="b" type="m" corresp="#s3:l3">doot</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Haer aenslaghen hier te niete <rhyme label="a" type="m">maeckt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Laet op haer treffen dese boose <rhyme label="a" type="m" corresp="#s4:l1">saeck</rhyme></l>
        <l id="s4:l3" met="4" type="-+">En stootse in de grauen <rhyme label="b" type="m">yn</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Diese maken voor de Christen <rhyme label="b" type="m" corresp="#s4:l3">dijn</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">So sullen sy bekennen <rhyme label="a" type="m">doch</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Dat ghy onse God leuest <rhyme label="a" type="m" corresp="#s5:l1">noch</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Ende helpt geweldich dyne <rhyme label="b" type="m">schaer</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Die haer op dy verlaten <rhyme label="b" type="m" corresp="#s5:l3">gaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
