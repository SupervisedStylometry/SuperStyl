  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GEen meerder vreught natuer en <rhyme label="a" type="m">baert</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dan als de Liefde <rhyme label="a" type="m" corresp="#s1:l1">naert</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En wordt met haer <rhyme label="a" type="m" corresp="#s1:l1">gepaert</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Als sy met ziel, hert, wil, en <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Te saem vereenicht <rhyme label="b" type="m" corresp="#s1:l4">in</rhyme></l>
        <l id="s1:l6" met="3" type="-+">'t Gemoet die soete <rhyme label="b" type="m" corresp="#s1:l4">MIN</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat my gebeurden soo veel <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Geen vreught en wenscht ick <rhyme label="a" type="m" corresp="#s2:l1">meer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dan dat mijn liefken <rhyme label="a" type="m" corresp="#s2:l1">teer</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Haer hert aen my eens had <rhyme label="b" type="m">verthoont</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Die in het myne <rhyme label="b" type="m" corresp="#s2:l4">woont</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dan waer ick wel <rhyme label="b" type="m" corresp="#s2:l4">geloont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">k' Heb nu door-ryst soo menigh <rhyme label="a" type="m">Ryck</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Veel Steden en haer <rhyme label="a" type="m" corresp="#s3:l1">wyck</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maer geen aen haer <rhyme label="a" type="m" corresp="#s3:l1">gelijck</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Gevonden inde heele <rhyme label="b" type="m">ront</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die my soo thert <rhyme label="b" type="m" corresp="#s3:l4">door-wont</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En dan weer maeckt <rhyme label="b" type="m" corresp="#s3:l4">gesont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het is om u Ã´ schoone <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">U Dienaer smerten <rhyme label="a" type="m" corresp="#s4:l1">draeght</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En u soo dickwils <rhyme label="a" type="m" corresp="#s4:l1">klaeght</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Ja die soo menigh dooden <rhyme label="b" type="m">sterft</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Als ick u aff-syn <rhyme label="b" type="m" corresp="#s4:l4">derft</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Die mijne zie <rhyme label="b" type="m" corresp="#s4:l4">door-kerft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Komt dan Princers, en siet wie <rhyme label="a" type="m">nu</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Soo lijden moet om <rhyme label="a" type="m" corresp="#s5:l1">u</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En die by naer is <rhyme label="a" type="m" corresp="#s5:l1">mu</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Want hy te seer gepeynight <rhyme label="b" type="m">wert</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Laet dan sijn droeve <rhyme label="b" type="m" corresp="#s5:l4">smert</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Bewegen eens u <rhyme label="b" type="m" corresp="#s5:l4">hert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
