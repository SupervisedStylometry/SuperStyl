<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181965</id>
    <date>
      <year-start>1782</year-start>
      <year-end>1782</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Gelijk de schone bloempjes kwijnen</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Gelyk de schoone bloempjes <rhyme label="a" type="f">kwyne</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Wanneer zy zien de Zon <rhyme label="a" type="f" corresp="#s1:l1">verdwyne</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Zoo ben ik ook in een droevige <rhyme label="b" type="m">staat</rhyme>;</l>
        <l id="s1:l4" met="4" type="-+">Wanneer myn minnaar van myn <rhyme label="b" type="m" corresp="#s1:l3">gaat</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Wanneer myn minnaar van myn <rhyme label="b" type="m" corresp="#s1:l3">gaat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Myn ziel verrukt en <rhyme label="a" type="f">opgetooge</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Verkwikt zig in myn minnaars <rhyme label="a" type="f" corresp="#s2:l1">ogen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ey welk een trouwheid myn <rhyme label="b" type="m">geviel</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Geen voedsel aan myn teder <rhyme label="b" type="m" corresp="#s2:l3">ziel</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Geen voedsel aan myn teder <rhyme label="b" type="m" corresp="#s2:l3">ziel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Myn min is trouw en zonder <rhyme label="a" type="f">vlekken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Gy kunt myn ziel tot min <rhyme label="a" type="f" corresp="#s3:l1">verwekken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Elk een kussi dat u mont myn <rhyme label="b" type="m">geeft</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Een onbekende zoetheid <rhyme label="b" type="m" corresp="#s3:l3">heeft</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Een onbekende zoetheid <rhyme label="b" type="m" corresp="#s3:l3">heeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Myn Damon ach myn tweede <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Wild u met myn in min <rhyme label="a" type="f" corresp="#s4:l1">begeven</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">U Cloris tragt myn min <rhyme label="b" type="m">gezond</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">U mond te drukken met myn <rhyme label="b" type="m" corresp="#s4:l3">mond</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">U mond te drukken met myn <rhyme label="b" type="m" corresp="#s4:l3">mond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gy zyt myn wellust en <rhyme label="a" type="f">verlangen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ik tragt u in myn arm te <rhyme label="a" type="f" corresp="#s5:l1">ontvangen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Komt haastig herwaarts zo gy <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Myn min die leid geen uitstel <rhyme label="b" type="m" corresp="#s5:l3">dan</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Myn min die leid geen uitstel <rhyme label="b" type="m" corresp="#s5:l3">dan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zo lang de Zon zal aan de hemel <rhyme label="a" type="f">draaije</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Zo lang de zoele wind zal <rhyme label="a" type="f" corresp="#s6:l1">waaije</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Zo lang zal ik de uwe <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">En gy voor altoos blyven <rhyme label="b" type="m" corresp="#s6:l3">myn</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">En gy voor altoos blyven <rhyme label="b" type="m" corresp="#s6:l3">myn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Weest maar getrouw myn <rhyme label="a" type="f">uitverkoren</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Wy zyn voor malkaar <rhyme label="a" type="f" corresp="#s7:l1">gebooren</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Niets hindert ons geen druk nog <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Gy zult altoos myn liefsten <rhyme label="b" type="m" corresp="#s7:l3">zyn</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Gy zult altoos myn liefsten <rhyme label="b" type="m" corresp="#s7:l3">zyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
