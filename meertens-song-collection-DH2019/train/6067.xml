    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Ik stond op hoge bergen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk groet u vol van <rhyme label="a" type="f">graci</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sprack d'Engel <rhyme label="b" type="m">Gabriel</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Met u tot aller <rhyme label="a" type="f" corresp="#s1:l1">spaci'</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Is Godt <rhyme label="b" type="m" corresp="#s1:l2">Emanuel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want onder alle <rhyme label="a" type="f">vrouwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Zijt ghy <rhyme label="b" type="m">ghebenedijt</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Geluckigh die <rhyme label="a" type="f" corresp="#s2:l1">aenschouwen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">V glory met <rhyme label="b" type="m" corresp="#s2:l2">jolijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gebenedijt moet <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Jesus, ws lichaems <rhyme label="b" type="m">dracht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Door hem zijn wy <rhyme label="a" type="f" corresp="#s3:l1">ghenesen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Al wt des Duyvels <rhyme label="b" type="m" corresp="#s3:l2">macht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maria Godes <rhyme label="a" type="f">Moeder</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wilt ons ghedachtigh <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Bidt voor ons onsen <rhyme label="a" type="f" corresp="#s4:l1">Hoeder</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Nu, en in doodes <rhyme label="b" type="m" corresp="#s4:l2">pijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
