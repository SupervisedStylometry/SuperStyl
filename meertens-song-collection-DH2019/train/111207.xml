    <melody>Zoutmans glorie</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zingen wy tot Zoutmans <rhyme label="a" type="f">Glorie</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Hy heeft Neêrlands roem <rhyme label="b" type="m">hersteld</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Zoutmans arm schenkt ons <rhyme label="a" type="f" corresp="#s1:l1">Victorie</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Hy verbrak het Brits <rhyme label="b" type="m" corresp="#s1:l2">geweld</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Eer en Roem van deze <rhyme label="c" type="f">streeken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Welkom welkom aan dit <rhyme label="d" type="m">strand</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Yder hoort men van u <rhyme label="c" type="f" corresp="#s1:l5">spreeken</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Als de steun van 't <rhyme label="d" type="m" corresp="#s1:l6">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zingen wy tot Zoutmans <rhyme label="a" type="f">Glorie</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Hy heeft Neêrlands roem <rhyme label="b" type="m">hersteld</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Zoutmans arm schenkt ons <rhyme label="a" type="f" corresp="#s2:l1">Victorie</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Hy verbrak het Brits <rhyme label="b" type="m" corresp="#s2:l2">geweld</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Hy die wy ter eere <rhyme label="c" type="f">zingen</rhyme>:</l>
        <l id="s2:l6" met="4" type="+-">Word van elk geëerd <rhyme label="d" type="m">bemind</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">'t Hooft der dappere <rhyme label="c" type="f" corresp="#s2:l5">Vlootelinge</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Is ons aller boesem <rhyme label="d" type="m" corresp="#s2:l6">vrind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zingen wy tot Zoutmans <rhyme label="a" type="f">Glorie</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Hy heeft Neêrlands roem <rhyme label="b" type="m">hersteld</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Zoutmans arm schenkt ons <rhyme label="a" type="f" corresp="#s3:l1">Victorie</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Hy verbrak het Brits <rhyme label="b" type="m" corresp="#s3:l2">geweld</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Ik zou my ook wel laaten <rhyme label="c" type="f">horen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Voor den dapper <rhyme label="d" type="m">Admiraal</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Vreesde ik niet hem te <rhyme label="c" type="f" corresp="#s3:l5">verstooren</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Door myn laffe <rhyme label="d" type="m" corresp="#s3:l6">Kindertaal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zingen wy tot Zoutmans <rhyme label="a" type="f">Glorie</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Hy heeft Neêrlands roem <rhyme label="b" type="m">hersteld</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Zoutmans arm schenkt ons <rhyme label="a" type="f" corresp="#s4:l1">Victorie</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Hy verbrak het Brits <rhyme label="b" type="m" corresp="#s4:l2">geweld</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Zulk een voorbeeld na te <rhyme label="c" type="f">streeven</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Zyn bevel ten dienste <rhyme label="d" type="m">staan</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Kan zelfs moed aan bloodaars <rhyme label="c" type="f" corresp="#s4:l5">geven</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Die den Britschen hond <rhyme label="d" type="m" corresp="#s4:l6">verslaan</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zingen wy tot Zoutmans <rhyme label="a" type="f">Glorie</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Hy heeft Neêrlands roem <rhyme label="b" type="m">hersteld</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">Zoutmans arm schenkt ons <rhyme label="a" type="f" corresp="#s5:l1">Victorie</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Hy verbrak het Brits <rhyme label="b" type="m" corresp="#s5:l2">geweld</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Onder hem de stryd te <rhyme label="c" type="f">waagen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Te overwinne door zyn <rhyme label="d" type="m">moed</rhyme>!</l>
        <l id="s5:l7" met="4" type="+-">'s vyands magt uit zee te <rhyme label="c" type="f" corresp="#s5:l5">jaagen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Is voor Helden het grootste <rhyme label="d" type="m" corresp="#s5:l6">zoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
