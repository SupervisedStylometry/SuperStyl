    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Dat hier nu ook een speelman <rhyme label="a" type="m">kom</rhyme> /</l>
        <l id="s1:l2" met="4" type="+-">Laten wy eens dansen en <rhyme label="b" type="f">springen</rhyme> /</l>
        <l id="s1:l3" met="4" type="-+">En ook een vroolyk liedje <rhyme label="b" type="f" corresp="#s1:l2">zingen</rhyme> /</l>
        <l id="s1:l4" met="4" type="+-">Voor de Bruid / en de <rhyme label="a" type="m" corresp="#s1:l1">Bruidegom</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Wy moeten ons eens vrolyk <rhyme label="c" type="f">maken</rhyme> /</l>
        <l id="s1:l6" met="2" type="+-">Op deez' <rhyme label="d" type="m">dag</rhyme> /</l>
        <l id="s1:l7" met="3" type="+-">En geen vreugd <rhyme label="c" type="f" corresp="#s1:l5">verzaaken</rhyme>.</l>
      </lg>
