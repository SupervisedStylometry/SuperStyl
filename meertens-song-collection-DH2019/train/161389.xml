<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161389</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het was een fraai rijk burgerskind</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn waerde Vrouw, bedenckt met <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe Godt ons heeft <rhyme label="b" type="f">gezegent</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Ons Kind'ren staen hier op een <rhyme label="a" type="m" corresp="#s1:l1">ry</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die heeft sijn gunst <rhyme label="b" type="f" corresp="#s1:l2">beregent</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Met gaven <rhyme label="c" type="f">overvloedigh</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Sy zijn gesont, <rhyme label="c" type="f" corresp="#s1:l5">voorspoedigh</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Iae oock veel meer, Als ons' <rhyme label="d" type="m">begeer</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Kan wesen van den <rhyme label="d" type="m" corresp="#s1:l7">Heer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ons' Kamers proncken tot de <rhyme label="a" type="m">top</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Van datmen kan <rhyme label="b" type="f">versinnen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Ons' Kassen geven volheyt <rhyme label="a" type="m" corresp="#s2:l1">op</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Van Wollen en van <rhyme label="b" type="f" corresp="#s2:l2">Linnen</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">De Kelder mach wel <rhyme label="c" type="f">spreecken</rhyme></l>
        <l id="s2:l6" met="3" type="-+">My komt geen dingh t' <rhyme label="c" type="f" corresp="#s2:l5">ontbreecken</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Want Boter, Wijn, Visch, Vleys, <rhyme label="d" type="m">Asijn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En Bier vindt ghy in <rhyme label="d" type="m" corresp="#s2:l7">mijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Geldt-kas roemt meer als <rhyme label="a" type="m">gemeen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van al haer ronde <rhyme label="b" type="f">Schijven</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Van al ons Vrienden mach'er <rhyme label="a" type="m" corresp="#s3:l1">geen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">(In rijckdom) by ons <rhyme label="b" type="f" corresp="#s3:l2">blijven</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">O Heer, laet ons u <rhyme label="c" type="f">gaven</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Niet maecken tot haer <rhyme label="c" type="f" corresp="#s3:l5">slaven</rhyme>;</l>
        <l id="s3:l7" met="4" type="-+">Geeft dat u goet Ons dienen <rhyme label="d" type="m">moet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Tot salige <rhyme label="d" type="m" corresp="#s3:l7">voor-spoet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daer wort geklopt wie mach daer <rhyme label="a" type="m">zijn</rhyme>?</l>
        <l id="s4:l2" met="3" type="-+">Ey Liefste gae nae <rhyme label="b" type="f">beuren</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Hoor lieve Man, doet dit voor <rhyme label="a" type="m" corresp="#s4:l1">mijn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ick ben geheel vol <rhyme label="b" type="f" corresp="#s4:l2">treuren</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Kom, ick sal selver <rhyme label="c" type="f">loopen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En doen de deur eens <rhyme label="c" type="f" corresp="#s4:l5">oopen</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Segh wie klopt daer? Wat is de <rhyme label="d" type="m">maer</rhyme>?</l>
        <l id="s4:l8" met="3" type="-+">Wat wilje, en van <rhyme label="d" type="m" corresp="#s4:l7">waer</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick ben de Doodt, verstaeje <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En meen niet langh te <rhyme label="b" type="f">dralen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Wie soeckje hier? Doet open <rhyme label="a" type="m" corresp="#s5:l1">snel</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Om u van hier te <rhyme label="b" type="f" corresp="#s5:l2">halen</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Kom in, ick moet u <rhyme label="c" type="f">spreecken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Licht sijn 't maer loose <rhyme label="c" type="f" corresp="#s5:l5">treecken</rhyme>:</l>
        <l id="s5:l7" met="4" type="-+">Ick ben bereyt, Toon klaer <rhyme label="d" type="m">bescheyt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Of dit u Meester <rhyme label="d" type="m" corresp="#s5:l7">seyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy hebt voor desen wel <rhyme label="a" type="m">gefaelt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Hiskias kan 't <rhyme label="b" type="f">gewagen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En Enoch is u oock <rhyme label="a" type="m" corresp="#s6:l1">onthaelt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Elias op een <rhyme label="b" type="f" corresp="#s6:l2">Wagen</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Veel, die ghy naemt het <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Moest ghy eens <rhyme label="c" type="f" corresp="#s6:l5">weder-geven</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Het nare Graf Wist daer wel <rhyme label="d" type="m">af</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Als Christus u <rhyme label="d" type="m" corresp="#s6:l7">begaf</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Sie daer, ick toon mijn volle <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hebt gy hier nu wat <rhyme label="b" type="f">tegen</rhyme>?</l>
        <l id="s7:l3" met="4" type="-+">Neen ick, maar ghy komt <rhyme label="a" type="m" corresp="#s7:l1">onverwacht</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En voor my niet <rhyme label="b" type="f" corresp="#s7:l2">gelegen</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Ghy dient noch wat te <rhyme label="c" type="f">beyden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Ick kan soo haest niet <rhyme label="c" type="f" corresp="#s7:l5">scheyden</rhyme>;</l>
        <l id="s7:l7" met="4" type="-+">Gun noch een Iaer, Tot dat ick <rhyme label="d" type="m">maer</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Mijn reeckeningh heb <rhyme label="d" type="m" corresp="#s7:l7">klaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy hebt noch maend, noch dagh <rhyme label="a" type="m">respijt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">'t Is nu u uur van <rhyme label="b" type="f">scheyden</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">O Godt, my doch genadigh <rhyme label="a" type="m" corresp="#s8:l1">zijt</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">U Geest laet my <rhyme label="b" type="f" corresp="#s8:l2">geleyden</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Heer, wilt u mijns <rhyme label="c" type="f">ontfermen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Verhoort het droevigh <rhyme label="c" type="f" corresp="#s8:l5">kermen</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Van uwen Knecht, Bewaert mijn <rhyme label="d" type="m">recht</rhyme></l>
        <l id="s8:l8" met="3" type="-+">In 's doodts bedroeft <rhyme label="d" type="m" corresp="#s8:l7">gevecht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Voor u stae ick vol schulden <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Voor my gemaeckt tot <rhyme label="b" type="f">heden</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">O Heer, wil in dees laetste <rhyme label="a" type="m" corresp="#s9:l1">noot</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Met my in't recht niet <rhyme label="b" type="f" corresp="#s9:l2">treden</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Mijn hoop en mijn <rhyme label="c" type="f">vertrouwen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Is op uw' Soon te <rhyme label="c" type="f" corresp="#s9:l5">bouwen</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Die al mijn schult Heeft met <rhyme label="d" type="m">gedult</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Betaelt, en al <rhyme label="d" type="m" corresp="#s9:l7">vervult</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Adieu mijn Vrouw, en Kind'ren <rhyme label="a" type="m">mee</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Adieu, wy sullen <rhyme label="b" type="f">scheyden</rhyme>;</l>
        <l id="s10:l3" met="4" type="-+">Leeft met malkaer in rust en <rhyme label="a" type="m" corresp="#s10:l1">vree</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Godts Geest wil u <rhyme label="b" type="f" corresp="#s10:l2">geleyden</rhyme>;</l>
        <l id="s10:l5" met="3" type="-+">Houdt Godt altijdt voor <rhyme label="c" type="f">oogen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Dan sal hy u <rhyme label="c" type="f" corresp="#s10:l5">verhoogen</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">En brengen al Uyt 't <rhyme label="d" type="m">tranen-dal</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Daer ick nu voor-gaen <rhyme label="d" type="m" corresp="#s10:l7">sal</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Kom Doodt, 't is hier al <rhyme label="a" type="m">af-gedaen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Ick heb u niet te <rhyme label="b" type="f">vreesen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Door u sal ick nu <rhyme label="a" type="m" corresp="#s11:l1">over-gaen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Om eeuwigh wel te <rhyme label="b" type="f" corresp="#s11:l2">wesen</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Mijn droefheyt zijn mijn <rhyme label="c" type="f">sonden</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Mijn troost sijn Christi <rhyme label="c" type="f" corresp="#s11:l5">wonden</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Mijn saligh deel is by <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Mijn ziel ick hem <rhyme label="d" type="m" corresp="#s11:l7">beveel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
