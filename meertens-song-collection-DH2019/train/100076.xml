  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">CUpido 'k rusten <rhyme label="a" type="m">sach</rhyme>:</l>
        <l id="s1:l2" met="4" type="-+">Hy sliep op Bloemtjes groene <rhyme label="b" type="f">graesjes</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Daer gemenelijck <rhyme label="b" type="f" corresp="#s1:l2">Herders-baesjes</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hen rust te nemen <rhyme label="a" type="m" corresp="#s1:l1">plach</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick sagh zijn poesel <rhyme label="a" type="m">Lijf</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Ick sagh Gekrolde haertjes <rhyme label="b" type="f">sweven</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Ront om 't Soetste van zijn <rhyme label="b" type="f" corresp="#s2:l2">Leven</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met swellent aertjes <rhyme label="a" type="m" corresp="#s2:l1">stijf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zijn Wieckjes lagen <rhyme label="a" type="m">tam</rhyme>:</l>
        <l id="s3:l2" met="4" type="-+">Zijn Pijl en Boog en gulden <rhyme label="b" type="f">Koocker</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Lach in rust dat eerst den <rhyme label="b" type="f" corresp="#s3:l2">Stoocker</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Tot vlamment schichten <rhyme label="a" type="m" corresp="#s3:l1">nam</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick rocht den Koocker <rhyme label="a" type="m">aen</rhyme>:</l>
        <l id="s4:l2" met="4" type="-+">Ick sey nu wil ick neerstigh <rhyme label="b" type="f">soecken</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Soete pijlen om te <rhyme label="b" type="f" corresp="#s4:l2">locken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">De Vreede-diertjes <rhyme label="a" type="m" corresp="#s4:l1">aen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ach laes! ick had geen <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Een Pijl uyt zijn Koocker te <rhyme label="b" type="f">trecken</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Om mijn pijn, met soet te <rhyme label="b" type="f" corresp="#s5:l2">decken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hoe wel ick het eerst <rhyme label="a" type="m" corresp="#s5:l1">dacht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dat menig Nimph 't <rhyme label="a" type="m">geluck</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Eens kreegh, dat sy hem mocht zien <rhyme label="b" type="f">rusten</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">('k Wet dat Zy hem vriendelijck <rhyme label="b" type="f" corresp="#s6:l2">kusten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En blusten haren <rhyme label="a" type="m" corresp="#s6:l1">druck</rhyme>).</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ey Cupido roep <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">U Moeder oock om hier te <rhyme label="b" type="f">rusten</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Op dat ick mijn brand oock <rhyme label="b" type="f" corresp="#s7:l2">blusten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Tot eynd' van <rhyme label="a" type="m" corresp="#s7:l1">slaverny</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
