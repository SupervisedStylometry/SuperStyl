    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Weest nu <rhyme label="a" type="m">verheught</rhyme></l>
        <l id="s1:l2" met="2" type="-+">en maeckt <rhyme label="a" type="m" corresp="#s1:l1">geneught</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Om dese Feest te <rhyme label="b" type="f">stichten</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Laet komen <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Singht met <rhyme label="c" type="m" corresp="#s1:l4">accoort</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Bruylofts soete <rhyme label="b" type="f" corresp="#s1:l3">gedichten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">‘t Huwelijck <rhyme label="a" type="m">jent</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">seer <rhyme label="a" type="m" corresp="#s2:l1">excelent</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Is in den <rhyme label="b" type="f">Paradijse</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Van aen ‘t <rhyme label="c" type="m">begin</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Gestelt <rhyme label="c" type="m" corresp="#s2:l4">in</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van Godt na goede <rhyme label="b" type="f" corresp="#s2:l3">wijse</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Als Adam <rhyme label="a" type="m">sliep</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Een slaep seer <rhyme label="a" type="m" corresp="#s3:l1">diep</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Godt nam uyt sijnen <rhyme label="b" type="f">lijve</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Een ribben <rhyme label="c" type="m">been</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Daer uyt hy <rhyme label="c" type="m" corresp="#s3:l4">reen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Schiep Eva hem ten <rhyme label="b" type="f" corresp="#s3:l3">wijve</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Adam <rhyme label="a" type="m">verbaest</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Siend’ Eva <rhyme label="a" type="m" corresp="#s4:l1">haest</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Aensprack haer als <rhyme label="b" type="f">vriendinne</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Dees is <rhyme label="c" type="m">alleen</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Mijn vleesch en <rhyme label="c" type="m" corresp="#s4:l4">been</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hy noemdese <rhyme label="b" type="f" corresp="#s4:l3">Maninne</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Heer Bruyd’gom <rhyme label="a" type="m">wijs</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Vrou Bruydt <rhyme label="a" type="m" corresp="#s5:l1">propijs</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dits uwen staet ter <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Ghy die vreemt <rhyme label="c" type="m">waert</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Zijt nu <rhyme label="c" type="m" corresp="#s5:l4">gepaert</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Door desen bandt des <rhyme label="b" type="f" corresp="#s5:l3">Heeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Geyn liefde <rhyme label="a" type="m">pleeght</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Blijft <rhyme label="a" type="m" corresp="#s6:l1">onbeweeght</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Nocht rechts noch slincks wilt <rhyme label="b" type="f">wijcken</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Schuwt haet en <rhyme label="c" type="m">twist</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Die ‘t al <rhyme label="c" type="m" corresp="#s6:l4">verquist</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Laet vreed gedurigh <rhyme label="b" type="f" corresp="#s6:l3">blijcken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">V wenschen <rhyme label="a" type="m">blijdt</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Sal t’sijner <rhyme label="a" type="m" corresp="#s7:l1">ttijdt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Seer lieflijck hem <rhyme label="b" type="f">vermeeren</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Tot Kind’ren <rhyme label="c" type="m">goedt</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Die ghy <rhyme label="c" type="m" corresp="#s7:l4">opvoedt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">In de vreese des <rhyme label="b" type="f" corresp="#s7:l3">Heeren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Die ‘t alles <rhyme label="a" type="m">geeft</rhyme></l>
        <l id="s8:l2" met="2" type="-+">En eeuwigh <rhyme label="a" type="m" corresp="#s8:l1">leeft</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Sal u uyt ‘s Hemels <rhyme label="b" type="f">erven</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Nootdrufts <rhyme label="c" type="m">behoort</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Geven nu <rhyme label="c" type="m" corresp="#s8:l4">voort</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Hier toe een saligh <rhyme label="b" type="f" corresp="#s8:l3">sterven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Ons Prins en <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s9:l2" met="2" type="-+">Schickt sijner <rhyme label="a" type="m" corresp="#s9:l1">eer</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Dit Huw’lijck naer <rhyme label="b" type="f">betamen</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Die genoot <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Op dit <rhyme label="c" type="m" corresp="#s9:l4">termijn</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Wenscht hen geluck te <rhyme label="b" type="f" corresp="#s9:l3">samen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
