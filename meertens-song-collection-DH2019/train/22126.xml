      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ORdeelt my Heer, mijn saeck neemt <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Teghen de onheylighe <rhyme label="b" type="f">schare</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Verlost my vanden valschen <rhyme label="a" type="m" corresp="#s1:l1">man</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En vanden Godloosen <rhyme label="b" type="f" corresp="#s1:l2">allgaren</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Ghy zijt de Godt, en sterckheyt <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Waerom laet ghy my soo truerich <rhyme label="c" type="m" corresp="#s1:l5">zijn</rhyme>?</l>
        <l id="s1:l7" met="3" type="-+">Als mijn vyanden my soo <rhyme label="d" type="f">quellen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Seyndt u licht, ende oock u <rhyme label="a" type="m">waerheyt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat sy my leyden ras, <rhyme label="b" type="f">seere</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Tot uwen heylighen berch <rhyme label="a" type="m" corresp="#s2:l1">bereyt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Tot u wooninghe, o <rhyme label="b" type="f" corresp="#s2:l2">Heere</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Dat ick mach tot uwen Oultaer <rhyme label="c" type="m">ingaen</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Tot den Godt mijnder vruechden <rhyme label="c" type="m" corresp="#s2:l5">zaen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En hem dancken op mijn <rhyme label="d" type="f">Herpe</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Waerom bedroeft ghy my mijn <rhyme label="a" type="m">Seel</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Wat beroert ghy my inder <rhyme label="b" type="f">stille</rhyme>?</l>
        <l id="s3:l3" met="4" type="-+">Verbeydt den Godt <rhyme label="a" type="m" corresp="#s3:l1">Emanueel</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dien ick nu dancken <rhyme label="b" type="f" corresp="#s3:l2">wille</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Om zijns aenghesichts <rhyme label="c" type="m">salicheyt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Heeft hy ons verlossinghe <rhyme label="c" type="m" corresp="#s3:l5">bereyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Door Christum onsen <rhyme label="d" type="f">Heere</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
