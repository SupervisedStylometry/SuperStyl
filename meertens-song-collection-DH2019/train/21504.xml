  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">MIin Heere mijn <rhyme label="a" type="m">God</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Mijn hemels vader <rhyme label="b" type="f">wtuercoren</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Uan al v <rhyme label="a" type="m" corresp="#s1:l1">ghebot</rhyme></l>
        <l id="s1:l4" met="4" type="+-">En houdt ick niet een achter noch <rhyme label="b" type="f" corresp="#s1:l2">voren</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Maer Uader slaect toch uwen <rhyme label="b" type="f" corresp="#s1:l2">thoren</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Mijn crancheyt <rhyme label="c" type="m">aensiet</rhyme></l>
        <l id="s1:l7" met="2" type="+-">Ick en twijfel <rhyme label="c" type="m" corresp="#s1:l6">niet</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Ick sal noch eewich met v vruecht <rhyme label="b" type="f" corresp="#s1:l2">oorboren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Beuechtinghe ende <rhyme label="a" type="m">aenstoot</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Moet ick lijden van daghen tot <rhyme label="b" type="f">daghen</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Uiant, weerelt, helle, sonde en <rhyme label="a" type="m" corresp="#s2:l1">doot</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Legghen na my listen en <rhyme label="b" type="f" corresp="#s2:l2">laghen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Maer Uader in v hebbe ick <rhyme label="b" type="f" corresp="#s2:l2">behaghen</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Gheeft my de <rhyme label="c" type="m">macht</rhyme></l>
        <l id="s2:l7" met="2" type="+-">Dach ende <rhyme label="c" type="m" corresp="#s2:l6">nacht</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Dat ick v cruys mach helpen <rhyme label="b" type="f" corresp="#s2:l2">draghen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Int Babylonsche <rhyme label="a" type="m">lant</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Hout my die doot ende die sonde <rhyme label="b" type="f">gheuanghen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Maer Heere ghy hebt den <rhyme label="a" type="m" corresp="#s3:l1">bant</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Ontdaen met uwer hant niet om <rhyme label="b" type="f" corresp="#s3:l2">verstrangen</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Dies ic wt rechter liefden v moet <rhyme label="b" type="f" corresp="#s3:l2">aenhangen</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Die weerelt <rhyme label="c" type="m">loos</rhyme></l>
        <l id="s3:l7" met="2" type="+-">Is wt <rhyme label="c" type="m" corresp="#s3:l6">altoos</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Om my te brenghen in haer <rhyme label="b" type="f" corresp="#s3:l2">bedwanghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Sondich ende <rhyme label="a" type="m">cranck</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Is mijn nature <rhyme label="b" type="f">ghecomplexioneert</rhyme></l>
        <l id="s4:l3" met="3" type="+-">In sviants <rhyme label="a" type="m" corresp="#s4:l1">bedwanck</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Was ick door Adams val <rhyme label="b" type="f" corresp="#s4:l2">ghemessuseeert</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Maer Uader ghy hebbet al <rhyme label="b" type="f" corresp="#s4:l2">gherefereert</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Adams <rhyme label="c" type="m">misdaet</rhyme></l>
        <l id="s4:l7" met="2" type="+-">En Euaens <rhyme label="c" type="m" corresp="#s4:l6">raet</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Hebdy voor uwen Uader <rhyme label="b" type="f" corresp="#s4:l2">ghepayseert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">U bermherticheyt <rhyme label="a" type="m">ydoon</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Begheer ick Heer maer niet v <rhyme label="b" type="f">recht</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Hadde ick na mijnen <rhyme label="a" type="m" corresp="#s5:l1">loon</rhyme></l>
        <l id="s5:l4" met="4" type="+-">So waer ick een arm verwaten <rhyme label="b" type="f" corresp="#s5:l2">knecht</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Uan trijcke so ons de Schriftuere <rhyme label="b" type="f" corresp="#s5:l2">secht</rhyme></l>
        <l id="s5:l6" met="2" type="+-">Ghy zijt <rhyme label="c" type="m">alleyn</rhyme></l>
        <l id="s5:l7" met="2" type="+-">Die ons maect <rhyme label="c" type="m" corresp="#s5:l6">reyn</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Uan al ons sonden suyuer en <rhyme label="b" type="f" corresp="#s5:l2">oprecht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Lof eer en <rhyme label="a" type="m">prijs</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Moet v toevloeyen God van <rhyme label="b" type="f">Israel</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Dats mijn <rhyme label="a" type="m" corresp="#s6:l1">aduijs</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Want ghy ons hebt gebrocht wt groot <rhyme label="b" type="f" corresp="#s6:l2">gequel</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Dus ick v wil aenbeden en niemant <rhyme label="b" type="f" corresp="#s6:l2">el</rhyme></l>
        <l id="s6:l6" met="2" type="+-">Want door v <rhyme label="c" type="m">cracht</rhyme></l>
        <l id="s6:l7" met="2" type="+-">Hebdy die <rhyme label="c" type="m" corresp="#s6:l6">macht</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Babylon ghenomen met haren raet seer <rhyme label="b" type="f" corresp="#s6:l2">fel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Prince van <rhyme label="a" type="m">Iuda</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Alle leden moeten voor v <rhyme label="b" type="f">beuen</rhyme></l>
        <l id="s7:l3" met="3" type="+-">En roepen aen v <rhyme label="a" type="m" corresp="#s7:l1">ghena</rhyme></l>
        <l id="s7:l4" met="4" type="+-">So ons die Propheten te kennen <rhyme label="b" type="f" corresp="#s7:l2">gheuen</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Salicheyt wt v moet elck <rhyme label="b" type="f" corresp="#s7:l2">aencleuen</rhyme></l>
        <l id="s7:l6" met="2" type="+-">O dierbaer <rhyme label="c" type="m">pant</rhyme></l>
        <l id="s7:l7" met="2" type="+-">Seer <rhyme label="c" type="m" corresp="#s7:l6">triumphant</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Ghy zijt die wech waerheyt en <rhyme label="b" type="f" corresp="#s7:l2">tleuen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
