  <author>Karel van Mander</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">MUsijck <rhyme label="a" type="m">gheschal</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Heeft ghevonden <rhyme label="a" type="m" corresp="#s1:l1">Jubal</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Eerst end' voor al// om te <rhyme label="b" type="f">verblijden</rhyme></l>
        <l id="s1:l4" met="2" type="-+">In't jammer <rhyme label="a" type="m" corresp="#s1:l1">dal</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Vol lijdens end' <rhyme label="a" type="m" corresp="#s1:l1">ongheval</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Vinden men sal// van ouden <rhyme label="b" type="f" corresp="#s1:l3">tijden</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">T'ghebruyck van t'singhen <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In <rhyme label="c" type="m" corresp="#s1:l7">Mesopotamy</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Om vreucht t'hanteren <rhyme label="d" type="m">saen</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Want Laban op de <rhyme label="d" type="m" corresp="#s1:l9">baen</rhyme>,</l>
        <l id="s1:l11" met="4" type="-+">Van Godt beraen// tot Jacob <rhyme label="e" type="f">seyde</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Waerdy soo niet <rhyme label="d" type="m" corresp="#s1:l9">ghegaen</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Harpen hadd' ick doen <rhyme label="d" type="m" corresp="#s1:l9">slaen</rhyme>,</l>
        <l id="s1:l14" met="4" type="-+">End' u ghedaen met sanck <rhyme label="e" type="f" corresp="#s1:l11">gheleyde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Moses die <rhyme label="a" type="m">sanck</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Naer Pharaos <rhyme label="a" type="m" corresp="#s2:l1">onderganck</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Den Heere danck// lustich om <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Myriam <rhyme label="a" type="m" corresp="#s2:l1">dwanck</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Oock den gheest, dat sy eer <rhyme label="a" type="m" corresp="#s2:l1">lanck</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Sanck, ende spranck// die Vroukens <rhyme label="b" type="f" corresp="#s2:l3">vooren</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Oock over een <rhyme label="c" type="m">fonteyn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Sanck Israel <rhyme label="c" type="m" corresp="#s2:l7">ghemeyn</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">D'een voor en d'ander <rhyme label="d" type="m">naer</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Debora sanck <rhyme label="d" type="m" corresp="#s2:l9">eerbaer</rhyme>,</l>
        <l id="s2:l11" met="4" type="-+">Barack met haer// Godt danck <rhyme label="e" type="f">bewesen</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">T'was soet om hooren <rhyme label="d" type="m" corresp="#s2:l9">daer</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Oock al der Vrouwen <rhyme label="d" type="m" corresp="#s2:l9">schaer</rhyme></l>
        <l id="s2:l14" met="4" type="-+">David seer claer// met sanghe <rhyme label="e" type="f" corresp="#s2:l11">presen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Victory <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Brandt-offer, Conincx <rhyme label="a" type="m" corresp="#s3:l1">ghecroon</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Israel ghewoon// was te <rhyme label="b" type="f">vercieren</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Met soeten <rhyme label="a" type="m" corresp="#s3:l1">soon</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Der Musijcken sanck <rhyme label="a" type="m" corresp="#s3:l1">ydoon</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Na t'Schrifts vertoon// op veel <rhyme label="b" type="f" corresp="#s3:l3">manieren</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Die oude Vaders <rhyme label="c" type="m">wijs</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Gheest Jesus Syrach <rhyme label="c" type="m" corresp="#s3:l7">prijs</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Dat sy dichten <rhyme label="d" type="m">playsant</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Gheestelijcke lieden, <rhyme label="d" type="m" corresp="#s3:l9">want</rhyme></l>
        <l id="s3:l11" met="4" type="-+">Groot haer verstant// was in <rhyme label="e" type="f">Mysijcke</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Haer ghedicht stont <rhyme label="d" type="m" corresp="#s3:l9">gheplant</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Al op maet <rhyme label="d" type="m" corresp="#s3:l9">elegant</rhyme>,</l>
        <l id="s3:l14" met="4" type="-+">Als nu int landt, die <rhyme label="e" type="f" corresp="#s3:l11">Rethorijcke</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ghedichten <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Door s Heeren Gheest <rhyme label="a" type="m" corresp="#s4:l1">overvloet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Heeft gemaeckt vroet// David Gods <rhyme label="b" type="f">diender</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">T'Christen <rhyme label="a" type="m" corresp="#s4:l1">ghemoet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Tot troost ende leeringhe <rhyme label="a" type="m" corresp="#s4:l1">goet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ghelijck oock doet, Assaph den <rhyme label="b" type="f" corresp="#s4:l3">Siender</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Salomon oock <rhyme label="c" type="m">verlicht</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Heeft menich Liedt <rhyme label="c" type="m" corresp="#s4:l7">ghedicht</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Want hy was wijser <rhyme label="d" type="m">dan</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Ander dichters <rhyme label="d" type="m" corresp="#s4:l9">Ethan</rhyme>,</l>
        <l id="s4:l11" met="4" type="-+">Chalchal, Heman, oft Darda <rhyme label="e" type="f">waren</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Dat Gods lof vroech <rhyme label="d" type="m" corresp="#s4:l9">began</rhyme></l>
        <l id="s4:l13" met="3" type="-+">In Liedekens men <rhyme label="d" type="m" corresp="#s4:l9">can</rhyme></l>
        <l id="s4:l14" met="4" type="-+">Bewijsen van// over veel <rhyme label="e" type="f" corresp="#s4:l11">Jaren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Noch <rhyme label="a" type="m">principael</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Salomon, nae zijn <rhyme label="a" type="m" corresp="#s5:l1">verhael</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Sanghers loyael// end <rhyme label="b" type="f">Sangheressen</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Hadde ter <rhyme label="a" type="m" corresp="#s5:l1">sael</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die hem songhen <rhyme label="a" type="m" corresp="#s5:l1">menichmael</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Schoon musicael, verscheyden <rhyme label="b" type="f" corresp="#s5:l3">lessen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Vol Gheestelijck <rhyme label="c" type="m">gheluyt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Van een Minnaer en <rhyme label="c" type="m" corresp="#s5:l7">Bruydt</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Als men te recht <rhyme label="d" type="m">insiet</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Der C[....]cken <rhyme label="d" type="m" corresp="#s5:l9">bediet</rhyme>,</l>
        <l id="s5:l11" met="4" type="-+">Oock menich Liedt// in die <rhyme label="e" type="f">Propheten</rhyme></l>
        <l id="s5:l12" met="3" type="-+">T'is al van Godt <rhyme label="d" type="m" corresp="#s5:l9">gheschiet</rhyme>,</l>
        <l id="s5:l13" met="3" type="-+">Anders ten baet al <rhyme label="d" type="m" corresp="#s5:l9">niet</rhyme>,</l>
        <l id="s5:l14" met="4" type="-+">Daer sonder yet// doen, oft <rhyme label="e" type="f" corresp="#s5:l11">vermeten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Paulus <rhyme label="a" type="m">devijn</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Schrijft, en suypt u niet vol <rhyme label="a" type="m" corresp="#s6:l1">wijn</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maer elck termijn// vol gheests wilt <rhyme label="b" type="f">blijven</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">In't herte <rhyme label="a" type="m" corresp="#s6:l1">dijn</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Laet overvloeyende <rhyme label="a" type="m" corresp="#s6:l1">zijn</rhyme></l>
        <l id="s6:l6" met="4" type="-+">T'woordt Christi fijn// om wel <rhyme label="b" type="f" corresp="#s6:l3">beclijven</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Psalmen, Lofsanghen <rhyme label="c" type="m">meest</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Liedekens nae den <rhyme label="c" type="m" corresp="#s6:l7">gheest</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Wilt hebben t'elcker <rhyme label="d" type="m">stondt</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Onderlingh' in den <rhyme label="d" type="m" corresp="#s6:l9">mondt</rhyme>,</l>
        <l id="s6:l11" met="4" type="-+">In s'herten grondt// spelen end' <rhyme label="e" type="f">singhen</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Den Heere die u <rhyme label="d" type="m" corresp="#s6:l9">jont</rhyme></l>
        <l id="s6:l13" met="3" type="-+">Sijn vredelijck <rhyme label="d" type="m" corresp="#s6:l9">verbondt</rhyme>,</l>
        <l id="s6:l14" met="4" type="-+">T leven ghesont// end' alle <rhyme label="e" type="f" corresp="#s6:l11">dinghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Die <rhyme label="a" type="m">Majesteyt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ons Princen wijdt <rhyme label="a" type="m" corresp="#s7:l1">verbreyt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dijn lof gheseyt// ende <rhyme label="b" type="f">ghesonghen</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Met goet <rhyme label="a" type="m" corresp="#s7:l1">bescheyt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">In Christen <rhyme label="a" type="m" corresp="#s7:l1">zedicheyt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Laet zijn bereyt// ons hert end' <rhyme label="b" type="f" corresp="#s7:l3">tonghen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">O broeders nacht end' <rhyme label="c" type="m">dach</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Daer ment wel hooren <rhyme label="c" type="m" corresp="#s7:l7">mach</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">In des singhens <rhyme label="d" type="m">voorstel</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Om met inwendich <rhyme label="d" type="m" corresp="#s7:l9">spel</rhyme>,</l>
        <l id="s7:l11" met="4" type="-+">Ons ghemoet snel, troostich <rhyme label="e" type="f">versaden</rhyme>,</l>
        <l id="s7:l12" met="3" type="-+">Want t'is Paulus <rhyme label="d" type="m" corresp="#s7:l9">bevel</rhyme>,</l>
        <l id="s7:l13" met="3" type="-+">Dus en doen sy niet <rhyme label="d" type="m" corresp="#s7:l9">wel</rhyme>,</l>
        <l id="s7:l14" met="4" type="-+">Die ons rebel// t'singhen <rhyme label="e" type="f" corresp="#s7:l11">versmaden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
