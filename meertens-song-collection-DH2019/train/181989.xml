<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181989</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ohne Lieb und ohne Wein</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">OP maendag ziet men de beste <rhyme label="a" type="m">trant</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Van de Vrouwtjes wilt <rhyme label="b" type="f">weeten</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Op de Noordermarkt <rhyme label="a" type="m" corresp="#s1:l1">playzant</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Daer loopenze dat zy <rhyme label="b" type="f" corresp="#s1:l2">zweeten</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Om het een of andere op te <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Daerom ziet menze <rhyme label="d" type="f">loopen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Alles moet 'er weezen na <rhyme label="c" type="m" corresp="#s1:l5">fatzoen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Al zouden zy haer hembt <rhyme label="d" type="f" corresp="#s1:l6">verkoopen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Iakken met staerten is haer <rhyme label="a" type="m">manier</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">En op zyde een paer <rhyme label="b" type="f">heupen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">De rokken die zy dragen zyn na de <rhyme label="a" type="m" corresp="#s2:l1">swier</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Op de zyde wat <rhyme label="b" type="f" corresp="#s2:l2">sleepent</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Van vooren en van agtere <rhyme label="c" type="m">kort</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Wilze maer <rhyme label="d" type="f">beschouwe</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Een gedoste Kiep al op haer <rhyme label="c" type="m" corresp="#s2:l5">hooft</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En strikjes al op haer <rhyme label="d" type="f" corresp="#s2:l6">mouwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zoo benne zy dan <rhyme label="a" type="m">uitgedorst</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Met een Maniel van <rhyme label="b" type="f">laken</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En een blaeuwe Satyne <rhyme label="a" type="m" corresp="#s3:l1">borst</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Om ons te <rhyme label="b" type="f" corresp="#s3:l2">vermaeken</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Groene of roode Schoentjes <rhyme label="c" type="m">net</rhyme>:</l>
        <l id="s3:l6" met="3" type="+-">Witte Kousjes aen haer <rhyme label="d" type="f">beenen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">En haer Borsjes wat <rhyme label="c" type="m" corresp="#s3:l5">opgezet</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Daer zy 't een of ander mee <rhyme label="d" type="f" corresp="#s3:l6">meenen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ziene zy den een of den <rhyme label="a" type="m">aer</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Het is aenstonds wenke of <rhyme label="b" type="f">lonken</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Zy bennen dan ten eersten <rhyme label="a" type="m" corresp="#s4:l1">klaer</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Dan gaeuw een fles <rhyme label="b" type="f" corresp="#s4:l2">gedronken</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En dan ook een rook <rhyme label="c" type="m">party</rhyme>:</l>
        <l id="s4:l6" met="3" type="+-">Daer zy veel van <rhyme label="d" type="f">houwen</rhyme>;</l>
        <l id="s4:l7" met="4" type="-+">Als is haer Man daer juist niet <rhyme label="c" type="m" corresp="#s4:l5">by</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Zo gaet het met een party <rhyme label="d" type="f" corresp="#s4:l6">Vrouwen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hebben de Vrouwtjes het eens <rhyme label="a" type="m">beet</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Dan zyn zy wel te <rhyme label="b" type="f">vreeden</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">De Man die daer niet af en <rhyme label="a" type="m" corresp="#s5:l1">weet</rhyme>:</l>
        <l id="s5:l4" met="3" type="+-">Vraegt ook na geen <rhyme label="b" type="f" corresp="#s5:l2">reeden</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Krygen zy dan pyn in haer <rhyme label="c" type="m">lyf</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Of in haer hooft wilt <rhyme label="d" type="f">weeten</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">De meester moet komen tot <rhyme label="c" type="m" corresp="#s5:l5">tydverdryf</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Zy moeten pregeeren of <rhyme label="d" type="f" corresp="#s5:l6">zweeten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Daerom een ieder wel op <rhyme label="a" type="m">let</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Op die kuyerende <rhyme label="b" type="f">Zusjes</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Zy houwen zo graeg van <rhyme label="a" type="m" corresp="#s6:l1">venusbet</rhyme>;</l>
        <l id="s6:l4" met="3" type="+-">En ook wel van wat <rhyme label="b" type="f" corresp="#s6:l2">kusjes</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Een ieder die daer mee <rhyme label="c" type="m">verkeert</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Moet wel oplettend <rhyme label="d" type="f">wezen</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Want daer zijn der mee <rhyme label="c" type="m" corresp="#s6:l5">geleert</rhyme>;</l>
        <l id="s6:l8" met="3" type="+-">De smous moet haer <rhyme label="d" type="f" corresp="#s6:l6">geneezen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
