        <l id="s1:l5" met="2" type="-+">Van Helicon <rhyme label="c" type="m" corresp="#s1:l4">terght</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wanneers' uyt den borst een Sangrijmpjen <rhyme label="a" type="m" corresp="#s1:l1">uyt</rhyme>.</l>
