    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 068 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HOe Lieff is my, als dat de <rhyme label="a" type="m">HEER</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Mijn stemme en mijn schreyen <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Verhoort also <rhyme label="b" type="f">Ghenaedich</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat hy syn Oore neycht tot <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dies wil ick hem Aenroepen <rhyme label="c" type="m" corresp="#s1:l4">vry</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Mijn Leven lanck <rhyme label="b" type="f" corresp="#s1:l3">ghestaedich</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Doots stricken hadden my <rhyme label="d" type="m">Omvaen</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Groot Ongheluck viel my oock <rhyme label="d" type="m" corresp="#s1:l7">aen</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Het trefften my seer <rhyme label="e" type="f">sware</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Die Angst end Vrees der Hellen <rhyme label="f" type="m">groot</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Ick quam in Jammer end in <rhyme label="f" type="m" corresp="#s1:l10">Noot</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">In Pijn en groote <rhyme label="e" type="f" corresp="#s1:l9">Vare</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maer ick Riep aen des Heeren <rhyme label="a" type="m">Naem</rhyme></l>
        <l id="s2:l2" met="4" type="-+">O HEEr verlost mijn Siel <rhyme label="a" type="m" corresp="#s2:l1">bequaem</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wt allen Noot en <rhyme label="b" type="f">Lijden</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">O HEER ghenaedich my by <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ghy zijt Rechtveerdich, vol <rhyme label="c" type="m" corresp="#s2:l4">Ghenaed</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Barmhertich t'Allen <rhyme label="b" type="f" corresp="#s2:l3">Tijden</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">De HEER behoedt <rhyme label="d" type="m">d'Eenvoudighe</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Die Oprecht syn van Herten <rhyme label="d" type="m" corresp="#s2:l7">mee</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Want doen ick was <rhyme label="e" type="f">Verarmet</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">En Neder lach, so heeft de <rhyme label="f" type="m">HEER</rhyme></l>
        <l id="s2:l11" met="4" type="+-">Ghenaed'lijck my geholpen <rhyme label="f" type="m" corresp="#s2:l10">weer</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Hem over my <rhyme label="e" type="f" corresp="#s2:l9">Ontfarmet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mijn Siel nu weer te Vreden <rhyme label="a" type="m">sy</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">De Heer (want) door syn Goetheydt <rhyme label="a" type="m" corresp="#s3:l1">dy</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Geholpen heeft nu <rhyme label="b" type="f">weder</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Mijn Siel hebt ghy O HEEr <rhyme label="c" type="m">bevrijdt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">End wt den Doot gherucht <rhyme label="c" type="m" corresp="#s3:l4">subijt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die heel daer lach ter <rhyme label="b" type="f" corresp="#s3:l3">Neder</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Mijn Ooghen vrijdt van Tranen <rhyme label="d" type="m">fijn</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Daer toe oock HEEr de Voeten <rhyme label="d" type="m" corresp="#s3:l7">mijn</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Voor slibberen en <rhyme label="e" type="f">glijden</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">Dies wil ick voor den HEERE <rhyme label="f" type="m">staen</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">End Wand'len in den Land' <rhyme label="f" type="m" corresp="#s3:l10">voortaen</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Der Levenden <rhyme label="e" type="f" corresp="#s3:l9">Altijden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat ick Gelooff, bekenn' ick <rhyme label="a" type="m">claer</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Hoe wel ick Jammerlijck en <rhyme label="a" type="m" corresp="#s4:l1">swaer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ghequelt ben met veel <rhyme label="b" type="f">Plaghen</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Ick sprack in het Vertzaghen <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Die Menschen alle Loog'naers <rhyme label="c" type="m" corresp="#s4:l4">zijn</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Vol Boosheydt t' Allen <rhyme label="b" type="f" corresp="#s4:l3">Daghen</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Wat soud' ick dan doch <rhyme label="d" type="m">Immermeer</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Voor syn Weldaden die de <rhyme label="d" type="m" corresp="#s4:l7">HEER</rhyme></l>
        <l id="s4:l9" met="3" type="-+">My doet, hem weder <rhyme label="e" type="f">geven</rhyme>?</l>
        <l id="s4:l10" met="4" type="-+">Ick wil des HEEren Kelck <rhyme label="f" type="m">Heylsaem</rhyme></l>
        <l id="s4:l11" met="4" type="+-">Aen nemen, En des Heeren <rhyme label="f" type="m" corresp="#s4:l10">Naem</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Aenroepen all mijn <rhyme label="e" type="f" corresp="#s4:l9">Leven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den HEer ick mijn Ghelofften <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Betalen voor synen Volck <rhyme label="a" type="m" corresp="#s5:l1">al</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">End hem daer mee <rhyme label="b" type="f">Verteeren</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Hoe geldet doch der Heyl'ghen <rhyme label="c" type="m">Doot</rhyme></l>
        <l id="s5:l5" met="4" type="-+">So veel, End al haer Pijne <rhyme label="c" type="m" corresp="#s5:l4">groot</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Voor Godt den HEER der <rhyme label="b" type="f" corresp="#s5:l3">Heeren</rhyme>?</l>
        <l id="s5:l7" met="4" type="-+">Ick ben O Heere uwen <rhyme label="d" type="m">Knecht</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Ick ben iae uwen Knecht <rhyme label="d" type="m" corresp="#s5:l7">Oprecht</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Ws Dienstmaechts Soon daer <rhyme label="e" type="f">neven</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Ghy hebt mijn Banden met dijn <rhyme label="f" type="m">Handt</rhyme></l>
        <l id="s5:l11" met="4" type="+-">Verscheurt, en my oock t'allen <rhyme label="f" type="m" corresp="#s5:l10">kant</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Verlost O Heer <rhyme label="e" type="f" corresp="#s5:l9">verheven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Danckoffer wil ick Off'ren <rhyme label="a" type="m">dy</rhyme></l>
        <l id="s6:l2" met="4" type="-+">En s'Heeren Naem Verconden <rhyme label="a" type="m" corresp="#s6:l1">bly</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ick wil den HEER <rhyme label="b" type="f">Betalen</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Nu myn Ghelofften <rhyme label="c" type="m">openbaer</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Voor al syn Volcke ende <rhyme label="c" type="m" corresp="#s6:l4">claer</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Syn Loff end Eer <rhyme label="b" type="f" corresp="#s6:l3">verhalen</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">In die Voorhoven schoon en <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Van s'HEEren Huys wil ick <rhyme label="d" type="m" corresp="#s6:l7">Certeyn</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Aenroepen ende <rhyme label="e" type="f">eeren</rhyme>,</l>
        <l id="s6:l10" met="4" type="-+">Des Heeren Naem met luyder <rhyme label="f" type="m">stem</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">In dy oock O <rhyme label="f" type="m" corresp="#s6:l10">Jerusalem</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Ghelooft zij Godt de <rhyme label="e" type="f" corresp="#s6:l9">HEEre</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
