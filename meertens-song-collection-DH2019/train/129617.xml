  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Als Boxvoetje speeld op sijn Pypje in ’t <rhyme label="a" type="m">dal</rhyme>,</l>
        <l id="s1:l2" met="1" type="-+">Zoo <rhyme label="b" type="f">singen</rhyme></l>
        <l id="s1:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s1:l2">springen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">De Satertjes <rhyme label="a" type="m" corresp="#s1:l1">al</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">De Boompjes in ’t <rhyme label="c" type="m">wilt</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">’t Gansch bosjen dat <rhyme label="c" type="m" corresp="#s1:l5">drilt</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En schatert van ’t lieffelyk <rhyme label="a" type="m" corresp="#s1:l1">na-geschal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het riet daar wel eertyds Syringe   in <rhyme label="a" type="m">zat</rhyme>,</l>
        <l id="s2:l2" met="1" type="-+">Staat <rhyme label="b" type="f">levend</rhyme>’</l>
        <l id="s2:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s2:l2">bevend</rhyme>’</l>
        <l id="s2:l4" met="2" type="-+">En schut als een <rhyme label="a" type="m" corresp="#s2:l1">blad</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ja, zwiert heen en <rhyme label="c" type="m">weer</rhyme></l>
        <l id="s2:l6" met="2" type="+-">Gelyken een <rhyme label="c" type="m" corresp="#s2:l5">Veer</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Daar ’t ruysende windje maar even <rhyme label="a" type="m" corresp="#s2:l1">om-vat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dit wilden d’Arkadische Nimphjes eens <rhyme label="a" type="m">zien</rhyme></l>
        <l id="s3:l2" met="1" type="-+">Wat <rhyme label="b" type="f">wonder</rhyme></l>
        <l id="s3:l3" met="1" type="-+">Dat <rhyme label="b" type="f" corresp="#s3:l2">onder</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Dit speelen kon <rhyme label="a" type="m" corresp="#s3:l1">schien</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Dat langs het <rhyme label="c" type="m">geboomt</rhyme></l>
        <l id="s3:l6" met="2" type="+-">En Beeckjes <rhyme label="c" type="m" corresp="#s3:l5">gestroomt</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Haar bruisende golfjens heenen <rhyme label="a" type="m" corresp="#s3:l1">vlien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dus zyn sy het Bosje ten einde <rhyme label="a" type="m">gegaan</rhyme></l>
        <l id="s4:l2" met="1" type="-+">En <rhyme label="b" type="f">liepen</rhyme></l>
        <l id="s4:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s4:l2">riepen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">De Satertjes <rhyme label="a" type="m" corresp="#s4:l1">aan</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Maar ’t loose <rhyme label="c" type="m">gebroed</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Licht haastig de <rhyme label="c" type="m" corresp="#s4:l5">voet</rhyme></l>
        <l id="s4:l7" met="4" type="+-">En hebben de Harderinne <rhyme label="a" type="m" corresp="#s4:l1">gevaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daar waren doen, laci, de Nimphies in <rhyme label="a" type="m">noot</rhyme>.</l>
        <l id="s5:l2" met="1" type="-+">Sy <rhyme label="b" type="f">lilden</rhyme></l>
        <l id="s5:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s5:l2">trilden</rhyme></l>
        <l id="s5:l4" met="2" type="-+">En sagen als <rhyme label="a" type="m" corresp="#s5:l1">dood</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">En riepen: ‘og, <rhyme label="c" type="m">og</rhyme>!’</l>
        <l id="s5:l6" met="2" type="+-">Maar ’t lose <rhyme label="c" type="m" corresp="#s5:l5">gedrog</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Die hebben haar poesle leden <rhyme label="a" type="m" corresp="#s5:l1">ontbloot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Terwyl dat Pan vast schaterd en <rhyme label="a" type="m">lagt</rhyme>,</l>
        <l id="s6:l2" met="1" type="-+">Zoo <rhyme label="b" type="f">songen</rhyme></l>
        <l id="s6:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s6:l2">sprongen</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Dit geyte <rhyme label="a" type="m" corresp="#s6:l1">geslagt</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Ja, deden in ’t <rhyme label="c" type="m">veld</rhyme></l>
        <l id="s6:l6" met="2" type="+-">Daar ’t meeste <rhyme label="c" type="m" corresp="#s6:l5">geweld</rhyme></l>
        <l id="s6:l7" met="4" type="+-">En hebben d’onnosele Nimphjes <rhyme label="a" type="m" corresp="#s6:l1">verkragt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dit hebben de Harders door d’Echo <rhyme label="a" type="m">gehoord</rhyme>,</l>
        <l id="s7:l2" met="1" type="-+">Wiens <rhyme label="b" type="f">harten</rhyme>,</l>
        <l id="s7:l3" met="1" type="-+">Vol <rhyme label="b" type="f" corresp="#s7:l2">smarten</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">So waren <rhyme label="a" type="m" corresp="#s7:l1">gestoort</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Dat yder een <rhyme label="c" type="m">eed</rhyme></l>
        <l id="s7:l6" met="2" type="+-">Aan Jupiter <rhyme label="c" type="m" corresp="#s7:l5">deed</rhyme></l>
        <l id="s7:l7" met="4" type="+-">Te wreken, of alle te worden <rhyme label="a" type="m" corresp="#s7:l1">vermoord</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dus vielen sy op de Rovertjes <rhyme label="a" type="m">aan</rhyme>.</l>
        <l id="s8:l2" met="1" type="-+">
          <rhyme label="b" type="f">Begonden</rhyme>
        </l>
        <l id="s8:l3" met="1" type="-+">In ’t <rhyme label="b" type="f" corresp="#s8:l2">ronden</rhyme></l>
        <l id="s8:l4" met="2" type="-+">Met knuppels te <rhyme label="a" type="m" corresp="#s8:l1">slaan</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Met Schoppen en <rhyme label="c" type="m">spa</rhyme>,</l>
        <l id="s8:l6" met="2" type="+-">En riepen: ‘Sa, <rhyme label="c" type="m" corresp="#s8:l5">sa</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Hey Saters, wat hebtge ons Nimphjes <rhyme label="a" type="m" corresp="#s8:l1">gedaan</rhyme>!’</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">De splitte-koots   namen wel haastig de <rhyme label="a" type="m">vlugt</rhyme>,</l>
        <l id="s9:l2" met="1" type="-+">Maar <rhyme label="b" type="f">lachten</rhyme></l>
        <l id="s9:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s9:l2">dagten</rhyme></l>
        <l id="s9:l4" met="2" type="-+">Om sulken <rhyme label="a" type="m" corresp="#s9:l1">klucht</rhyme></l>
        <l id="s9:l5" met="2" type="-+">De Harders <rhyme label="c" type="m">verbaast</rhyme>.</l>
        <l id="s9:l6" met="2" type="+-">Die vonden wel <rhyme label="c" type="m" corresp="#s9:l5">haast</rhyme></l>
        <l id="s9:l7" met="4" type="+-">Haar Nimphjes, maar waren   in groot <rhyme label="a" type="m" corresp="#s9:l1">gesugt</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Daar was het doen, laci, een droevige <rhyme label="a" type="m">tyd</rhyme>.</l>
        <l id="s10:l2" met="1" type="-+">Sy <rhyme label="b" type="f">knarsten</rhyme></l>
        <l id="s10:l3" met="1" type="-+">En <rhyme label="b" type="f" corresp="#s10:l2">parsten</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Haar tranen van <rhyme label="a" type="m" corresp="#s10:l1">spyt</rhyme>;</l>
        <l id="s10:l5" met="2" type="-+">En hebben uit <rhyme label="c" type="m">lust</rhyme></l>
        <l id="s10:l6" met="2" type="+-">Haar Nimphjes <rhyme label="c" type="m" corresp="#s10:l5">gekust</rhyme>.</l>
        <l id="s10:l7" met="4" type="+-">Maar veele die waren haar maagdom <rhyme label="a" type="m" corresp="#s10:l1">quyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
