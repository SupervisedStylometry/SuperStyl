<song>
  <meta>
    <id>8977</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Onze vader in hemelrijk</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VAder onse in <rhyme label="a" type="m">HEmelrijck</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ghy die ons alle heet <rhyme label="a" type="m" corresp="#s1:l1">ghelijck</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Broeder zijn en dy Roepen <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En wilt dat Beeden van ons <rhyme label="b" type="m" corresp="#s1:l3">haen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Geeft dat niet Bidt alleen de <rhyme label="c" type="m">Mondt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Maer dat het gae van Herten <rhyme label="c" type="m" corresp="#s1:l5">grondt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gheheylicht wordt den Name <rhyme label="a" type="m">dijn</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dijn Woort by ons help houden <rhyme label="a" type="m" corresp="#s2:l1">Reyn</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En dat wy leven <rhyme label="b" type="m">Heylichlijck</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Nae dijnen Name <rhyme label="b" type="m" corresp="#s2:l3">weerdichlijck</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Bewaert ons HEEr voor valsche <rhyme label="c" type="m">Leer</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Dat arm verdoolde Volck <rhyme label="c" type="m" corresp="#s2:l5">bekeer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dijn Rijck toecom in desen <rhyme label="a" type="m">Tijdt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">End oock hier nae in <rhyme label="a" type="m" corresp="#s3:l1">Eewicheyt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">De heylig' Gheest ons woone <rhyme label="b" type="m">by</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Met synen Gaven <rhyme label="b" type="m" corresp="#s3:l3">menigerley</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Des Sathans thoorn en groot <rhyme label="c" type="m">gewalt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Te breeck, voor hem dijn Kerck <rhyme label="c" type="m" corresp="#s3:l5">Erhalt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dijn Wil geschie Heer Godt <rhyme label="a" type="m">gelijck</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Op Aerden als in <rhyme label="a" type="m" corresp="#s4:l1">HEmelrijck</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gheeft ons ghedult in Lijdens <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Gehoorsaem syn, in Lieff en <rhyme label="b" type="m" corresp="#s4:l3">Leydt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Weert en keert alle Vleesch en <rhyme label="c" type="m">Bloet</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Dat teghen dijnen Wille <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Geeft ons huyd' onse daeglijcx <rhyme label="a" type="m">broot</rhyme></l>
        <l id="s5:l2" met="4" type="-+">End wat behoort tot Lijves <rhyme label="a" type="m" corresp="#s5:l1">Noot</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Bewaert ons Heer voor twist en <rhyme label="b" type="m">strijdt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Voor Sieckten end voor duyre <rhyme label="b" type="m" corresp="#s5:l3">Tijdt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Dat wy in goeden Vrede <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">De sorg' end giericheyt van ons <rhyme label="c" type="m" corresp="#s5:l5">slaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Al onse schult vergeeft ons <rhyme label="a" type="m">HEEr</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat sy ons niet bedroeven <rhyme label="a" type="m" corresp="#s6:l1">meer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Als wy oock onsen <rhyme label="b" type="m">schuldener'n</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Haer schult en Fout vergeven <rhyme label="b" type="m" corresp="#s6:l3">geern</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Te dienen maeck ons all' <rhyme label="c" type="m">bereydt</rhyme></l>
        <l id="s6:l6" met="4" type="-+">In rechter Lieffd' end <rhyme label="c" type="m" corresp="#s6:l5">Eenicheyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Leyd' ons Heer in becoring' <rhyme label="a" type="m">nicht</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Als ons de boose Gheest <rhyme label="a" type="m" corresp="#s7:l1">aenvicht</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ter slincker end ter Rechter <rhyme label="b" type="m">Handt</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Helpt ons doen stercken <rhyme label="b" type="m" corresp="#s7:l3">Wederstandt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">In t'Geloove vast end wel <rhyme label="c" type="m">gherust</rhyme></l>
        <l id="s7:l6" met="4" type="-+">En door des heylighen Geestes <rhyme label="c" type="m" corresp="#s7:l5">troost</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Van allen quaden ons <rhyme label="a" type="m">verloos</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Het syn de Tijd' en Daghen <rhyme label="a" type="m" corresp="#s8:l1">boos</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Verlost ons van den Eewighen <rhyme label="b" type="m">Doot</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">End troost ons in der lesten <rhyme label="b" type="m" corresp="#s8:l3">Noot</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Verleent ons HEEr een salich <rhyme label="c" type="m">Endt</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Neemt onse Siel in dijne <rhyme label="c" type="m" corresp="#s8:l5">Handt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Amen dat is het worde <rhyme label="a" type="m">waer</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Sterck ons Gheloove ymmer <rhyme label="a" type="m" corresp="#s9:l1">daer</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Op dat wy niet twijff'len daer <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Wat wy hier met ghebeden <rhyme label="b" type="m" corresp="#s9:l3">haen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Op dijn Woort in den Name <rhyme label="c" type="m">dijn</rhyme></l>
        <l id="s9:l6" met="4" type="-+">So spreken wy dat Amen <rhyme label="c" type="m" corresp="#s9:l5">fijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
