    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Ne vous offensez madame</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Dianier ik kent u <rhyme label="a" type="f">wezen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Trotst des Hemels Goden <rhyme label="b" type="m">hoog</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En u oogen hoog <rhyme label="a" type="f" corresp="#s1:l1">geprezen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Trotzen 't eenig wereldts <rhyme label="b" type="m" corresp="#s1:l2">oog</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Lelien en Roozen <rhyme label="a" type="f">t'zamen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Op u wangen zijn <rhyme label="b" type="m">gemengt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En u vlechten 't gout <rhyme label="a" type="f" corresp="#s2:l1">beschamen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Dan men uyt Milanen <rhyme label="b" type="m" corresp="#s2:l2">brengt</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ik beken, 't zijn groote <rhyme label="a" type="f">krachten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Doch geen wonder geeft het <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat zy niemandt t' onder <rhyme label="a" type="f" corresp="#s3:l1">brachten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Want zy niet gewapent <rhyme label="b" type="m" corresp="#s3:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hoogh en uytgenomen <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">'t Herte dat de minne <rhyme label="b" type="m">vliet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">'t Wapen daar 't mee is te <rhyme label="a" type="f" corresp="#s4:l1">winnen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Is beleeftheydt, anders <rhyme label="b" type="m" corresp="#s4:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Veele Vrouwen die <rhyme label="a" type="f">geboren</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Zijn geweest van krachten <rhyme label="b" type="m">slecht</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Hebben in het ziel <rhyme label="a" type="f" corresp="#s5:l1">bekoren</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Hier mee wonder <rhyme label="b" type="m" corresp="#s5:l2">uytgerecht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die aan schoonheyt haar <rhyme label="a" type="f">vergapen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Of aan 't wesen hoogh <rhyme label="b" type="m">geacht</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dunkt my dat haar schoonder <rhyme label="a" type="f" corresp="#s6:l1">wapen</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Laten winnen van de <rhyme label="b" type="m" corresp="#s6:l2">kracht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Gy versuurt uwe <rhyme label="a" type="f">manieren</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Met u trots en <rhyme label="b" type="m">hovaardy</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Hoeveel d' ander gaven <rhyme label="a" type="f" corresp="#s7:l1">cieren</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Alzo veel ontzieren <rhyme label="b" type="m" corresp="#s7:l2">zy</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Gy moocht ander gaan <rhyme label="a" type="f">vermaken</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Die zo schoon u selven <rhyme label="b" type="m">vijndt</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Iuliette doet my <rhyme label="a" type="f" corresp="#s8:l1">blaken</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Die my veel volmaakter <rhyme label="b" type="m" corresp="#s8:l2">schijnt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
