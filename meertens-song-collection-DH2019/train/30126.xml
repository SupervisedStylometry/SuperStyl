  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">INd' hoogste heemlen sy den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Den hoogsten God', de hoogste <rhyme label="a" type="f" corresp="#s1:l1">eere</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">End op der eirden, <rhyme label="b" type="m">overvloet</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Van alle vreed end goeden <rhyme label="b" type="m" corresp="#s1:l3">spoet</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Want Gods behagen met der <rhyme label="c" type="m">daet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Nu op der mensschen kindren <rhyme label="c" type="m" corresp="#s1:l5">staet</rhyme>.</l>
      </lg>
    </lg>
