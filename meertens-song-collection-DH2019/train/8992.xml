    <melody>O Herr Gott dein g√∂ttliches Wort</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Heere <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s1:l2" met="2" type="-+">dijn Godlijck <rhyme label="a" type="m" corresp="#s1:l1">woort</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Is lang' Verdonckert <rhyme label="b" type="f">bleven</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Tot dat ons <rhyme label="c" type="m">is</rhyme></l>
        <l id="s1:l5" met="2" type="-+">geseyt <rhyme label="c" type="m" corresp="#s1:l4">ghewis</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Wat Paulus heeft <rhyme label="b" type="f" corresp="#s1:l3">beschreven</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">End ander <rhyme label="d" type="m">meer</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Apost'len <rhyme label="d" type="m" corresp="#s1:l7">Heer</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Wt dijn Godlijcken <rhyme label="e" type="f">Monde</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Wy dancken <rhyme label="f" type="m">dy</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">O Heer dat <rhyme label="f" type="m" corresp="#s1:l10">wy</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Beleeft hebben die <rhyme label="e" type="f" corresp="#s1:l9">Stonde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Dat door u <rhyme label="a" type="m">Macht</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Int licht is <rhyme label="a" type="m" corresp="#s2:l1">bracht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Als claerlijck is voor <rhyme label="b" type="f">ooghen</rhyme>.</l>
        <l id="s2:l4" met="2" type="-+">Ach Godt mijn <rhyme label="c" type="m">HEER</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ontfarmt u <rhyme label="c" type="m" corresp="#s2:l4">seer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die dy noch nu <rhyme label="b" type="f" corresp="#s2:l3">Verlooch'nen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">End achten <rhyme label="d" type="m">meer</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Op Menschen <rhyme label="d" type="m" corresp="#s2:l7">Leer</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Daer in sy doch <rhyme label="e" type="f">verderven</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Dijns Woorts <rhyme label="f" type="m">verstandt</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Maeckt haer <rhyme label="f" type="m" corresp="#s2:l10">bekandt</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Dat sy niet Eewich <rhyme label="e" type="f" corresp="#s2:l9">sterven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Wilt ghy nu <rhyme label="a" type="m">fijn</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Goet Christen <rhyme label="a" type="m" corresp="#s3:l1">zijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">So moet ghy recht <rhyme label="b" type="f">Ghelooven</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Sett dijn <rhyme label="c" type="m">Vertrouw'</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Daer op vast <rhyme label="c" type="m" corresp="#s3:l4">bouw'</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Hoop en Liefd' int <rhyme label="b" type="f" corresp="#s3:l3">Gheloove</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Alleen door <rhyme label="d" type="m">Christ</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Tot aller <rhyme label="d" type="m" corresp="#s3:l7">Vrist</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Dijn Naesten mint daer <rhyme label="e" type="f">neven</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Conscientie <rhyme label="f" type="m">vry</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Reyn Hert daer <rhyme label="f" type="m" corresp="#s3:l10">by</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Gheen Creatuer kan <rhyme label="e" type="f" corresp="#s3:l9">t'gheven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ghy Heer <rhyme label="a" type="m">alleyn</rhyme></l>
        <l id="s4:l2" met="2" type="-+">moet sulcx doen <rhyme label="a" type="m" corresp="#s4:l1">reyn</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Doch gantschlijck wt <rhyme label="b" type="f">ghenaden</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Wie hem des <rhyme label="c" type="m">troost</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Die is <rhyme label="c" type="m" corresp="#s4:l4">verloost</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">End mach hem niemandt <rhyme label="b" type="f" corresp="#s4:l3">schaden</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Al wouden <rhyme label="d" type="m">ghelijck</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Paus, Keyser, <rhyme label="d" type="m" corresp="#s4:l7">t'Rijck</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Dy, end dijn Woort <rhyme label="e" type="f">verdrijven</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Is doch haer <rhyme label="f" type="m">Macht</rhyme></l>
        <l id="s4:l11" met="2" type="+-">By dy niet <rhyme label="f" type="m" corresp="#s4:l10">gheacht</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Sy moetent laten <rhyme label="e" type="f" corresp="#s4:l9">blijven</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Help HEEre <rhyme label="a" type="m">Godt</rhyme></l>
        <l id="s5:l2" met="2" type="-+">in deser <rhyme label="a" type="m" corresp="#s5:l1">Noot</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat haer die oock <rhyme label="b" type="f">bekeeren</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Die niet <rhyme label="c" type="m">betracht</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Dijn Woort <rhyme label="c" type="m" corresp="#s5:l4">veracht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">End wil het oock niet <rhyme label="b" type="f" corresp="#s5:l3">leeren</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Sy spreken <rhyme label="d" type="m">slecht</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Het is niet <rhyme label="d" type="m" corresp="#s5:l7">Recht</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">En hebben niet <rhyme label="e" type="f">ghelesen</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">Oock niet <rhyme label="f" type="m">ghehoort</rhyme></l>
        <l id="s5:l11" met="2" type="+-">Dat Eedle <rhyme label="f" type="m" corresp="#s5:l10">Woort</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Ist niet een Duyvels <rhyme label="e" type="f" corresp="#s5:l9">wesen</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ick g'looff gantsch <rhyme label="a" type="m">gaer</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Dat het zij <rhyme label="a" type="m" corresp="#s6:l1">waer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wat Paulus ons doet <rhyme label="b" type="f">schrijven</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Al moet <rhyme label="c" type="m">voortaen</rhyme></l>
        <l id="s6:l5" met="2" type="-+">All' dinck <rhyme label="c" type="m" corresp="#s6:l4">vergaen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dijn Godlijck Woort sal <rhyme label="b" type="f" corresp="#s6:l3">blijven</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">In <rhyme label="d" type="m">Eewicheet</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Al waert oock <rhyme label="d" type="m" corresp="#s6:l7">Leedt</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Veel hart verstockte <rhyme label="e" type="f">Herten</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">Keerens' niet <rhyme label="f" type="m">om</rhyme>,</l>
        <l id="s6:l11" met="2" type="+-">Sullens' <rhyme label="f" type="m" corresp="#s6:l10">daerom</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Lijden gaer groote <rhyme label="e" type="f" corresp="#s6:l9">smerten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Godt is mijn <rhyme label="a" type="m">HEER</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Soo ben ick <rhyme label="a" type="m" corresp="#s7:l1">dier</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dijn sterven coemt te <rhyme label="b" type="f">goede</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Daer door wy <rhyme label="c" type="m">vast</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Wt allen <rhyme label="c" type="m" corresp="#s7:l4">Last</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Verlost met dijnen <rhyme label="b" type="f" corresp="#s7:l3">Bloede</rhyme>.</l>
        <l id="s7:l7" met="2" type="-+">Des danck ick <rhyme label="d" type="m">dy</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Dies sult ghy <rhyme label="d" type="m" corresp="#s7:l7">my</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Nae dijn Belofftnis <rhyme label="e" type="f">gheven</rhyme>,</l>
        <l id="s7:l10" met="2" type="-+">Wat ick dy <rhyme label="f" type="m">bid'</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">Ontseg't my <rhyme label="f" type="m" corresp="#s7:l10">niet</rhyme>,</l>
        <l id="s7:l12" met="3" type="-+">In Doot end oock in <rhyme label="e" type="f" corresp="#s7:l9">Leven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">HEER ick hoop <rhyme label="a" type="m">noch</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Ghy werdet <rhyme label="a" type="m" corresp="#s8:l1">doch</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">In gheener Noot <rhyme label="b" type="f">verlaten</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Die dijn Woort <rhyme label="c" type="m">recht</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Als Trouwe <rhyme label="c" type="m" corresp="#s8:l4">Knecht'</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Int recht Gheloove <rhyme label="b" type="f" corresp="#s8:l3">vaten</rhyme>.</l>
        <l id="s8:l7" met="2" type="-+">Gheeft hun <rhyme label="d" type="m">bereyt</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">Die <rhyme label="d" type="m" corresp="#s8:l7">Salicheyt</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">En laetse niet <rhyme label="e" type="f">verderven</rhyme>,</l>
        <l id="s8:l10" met="2" type="-+">O HEER door <rhyme label="f" type="m">dy</rhyme>,</l>
        <l id="s8:l11" met="2" type="+-">Bid' ick laet <rhyme label="f" type="m" corresp="#s8:l10">my</rhyme></l>
        <l id="s8:l12" met="3" type="-+">Vrolijck end willich <rhyme label="e" type="f" corresp="#s8:l9">sterven</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
