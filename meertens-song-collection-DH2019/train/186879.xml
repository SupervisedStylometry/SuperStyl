<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186879</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody>Wilhelmus</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Komt jonge Maets! Komt <rhyme label="a" type="f">Mannen</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Helpt meê het <rhyme label="b" type="m">Roofgebroed</rhyme></l>
        <l id="s1:l3" met="3" type="-+">In zyn Spelonken <rhyme label="a" type="f" corresp="#s1:l1">bannen</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Komt hebt ge <rhyme label="b" type="m" corresp="#s1:l2">beldendenmoed</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dan moet ge met ons <rhyme label="c" type="f">stryden</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">De Brit verschopt den <rhyme label="d" type="m">Vreê</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Wie kan hun roofzucht <rhyme label="c" type="f" corresp="#s1:l5">lyden</rhyme>?</l>
        <l id="s1:l8" met="3" type="-+">Komt lustig meê ter <rhyme label="d" type="m" corresp="#s1:l6">Zee</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Kon Tromp zyn graf <rhyme label="a" type="f">verlaaten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hy vloog terstond naer <rhyme label="b" type="m">Strand</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En riep: ‘ter Zee, <rhyme label="a" type="f" corresp="#s2:l1">Soldaaten</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">'t Is voor ons <rhyme label="b" type="m" corresp="#s2:l2">Vaderland</rhyme>!’</l>
        <l id="s2:l5" met="3" type="-+">Wy zyn zyn braave <rhyme label="c" type="f">Zoonen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En ieder <rhyme label="d" type="m">Capitein</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Zal met de daad <rhyme label="c" type="f" corresp="#s2:l5">betoonen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Een wakkre Tromp te <rhyme label="d" type="m" corresp="#s2:l6">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ziet elk met smert zyn <rhyme label="a" type="f">Scheepen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Door Britten <rhyme label="b" type="m">agterhaeld</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Wy zetten hun dit <rhyme label="a" type="f" corresp="#s3:l1">sleepen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Wel draa zeer duur <rhyme label="b" type="m" corresp="#s3:l2">betaald</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Wy zullen als 's Lands <rhyme label="c" type="f">Magten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gebiên in Zee te <rhyme label="d" type="m">gaen</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Hem met gelyke <rhyme label="c" type="f" corresp="#s3:l5">kragten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Wel ras tot pulver <rhyme label="d" type="m" corresp="#s3:l6">slaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Komt jonge Maets? komt <rhyme label="a" type="f">Mannen</rhyme>?</l>
        <l id="s4:l2" met="3" type="-+">Komt helpt nu meê 't <rhyme label="b" type="m">gebroed</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat ons plaegt te <rhyme label="a" type="f" corresp="#s4:l1">verbannen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Geeft van uw <rhyme label="b" type="m" corresp="#s4:l2">Heldenmoed</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Aan 't Vaderland nu <rhyme label="c" type="f">blyken</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Wy vreezen nood noch <rhyme label="d" type="m">dood</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">De Brit zal moeten <rhyme label="c" type="f" corresp="#s4:l5">wyken</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Voor Neêrlands kruid en <rhyme label="d" type="m" corresp="#s4:l6">loot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
