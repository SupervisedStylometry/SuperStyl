    <melody>Mocht men om niet drinken vat mijn bedied</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Frans vander Straten de jongere</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">LIefde en <rhyme label="a" type="m">accoort</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wenschen wy <rhyme label="a" type="m" corresp="#s1:l1">ongestoort</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">V vrienden soo <rhyme label="a" type="m" corresp="#s1:l1">t'behoort</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Door een ionstich <rhyme label="b" type="m">gemoet</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Tot deuchden <rhyme label="a" type="m" corresp="#s1:l1">spoort</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Waer ghy zijt, Suyt ofte <rhyme label="a" type="m" corresp="#s1:l1">Noort</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Leeft recht naer Godts <rhyme label="a" type="m" corresp="#s1:l1">woort</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In eenicheyt seer <rhyme label="b" type="m" corresp="#s1:l4">soet</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">En metter <rhyme label="b" type="m" corresp="#s1:l4">spoet</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Altijt het beste <rhyme label="b" type="m" corresp="#s1:l4">doet</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Weest hertelick <rhyme label="b" type="m" corresp="#s1:l4">ghegroet</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Al door dit cleyn <rhyme label="c" type="m">vermaen</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Want tis uyt jonsten <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">Al is de conste <rhyme label="d" type="m" corresp="#s1:l13">cleyn</rhyme>,</l>
        <l id="s1:l15" met="3" type="-+">Wt liefden ist <rhyme label="c" type="m" corresp="#s1:l12">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Eenich <rhyme label="a" type="m">ghesint</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Weest doch vrienden <rhyme label="a" type="m" corresp="#s2:l1">bemint</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Groote ghenade <rhyme label="a" type="m" corresp="#s2:l1">vint</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die de Vrede voort <rhyme label="b" type="m">bringt</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Maer naer Schrifts <rhyme label="a" type="m" corresp="#s2:l1">onvint</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Diet hier al <rhyme label="a" type="m" corresp="#s2:l1">overwint</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Sal wesen Christus <rhyme label="a" type="m" corresp="#s2:l1">Kint</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dus u vant quade <rhyme label="b" type="m" corresp="#s2:l4">dinckt</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">En recht door <rhyme label="b" type="m" corresp="#s2:l4">drinckt</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Naer eenicheyt <rhyme label="b" type="m" corresp="#s2:l4">verlint</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Wie dit leest ofte <rhyme label="b" type="m" corresp="#s2:l4">singt</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Willet int goede <rhyme label="c" type="m">ontfaen</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Dit ist uyt jonsten <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s2:l14" met="3" type="-+">Als is de conste <rhyme label="d" type="m" corresp="#s2:l13">cleyn</rhyme>,</l>
        <l id="s2:l15" met="3" type="-+">Wt liefden ist <rhyme label="c" type="m" corresp="#s2:l12">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">In liefde <rhyme label="a" type="m">claer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Blijft vrienden <rhyme label="a" type="m" corresp="#s3:l1">allegaer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">List en twist <rhyme label="a" type="m" corresp="#s3:l1">on-eerbaer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Schaut eertrijck als <rhyme label="b" type="m">slick</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Volcht Christum <rhyme label="a" type="m" corresp="#s3:l1">naer</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En vreest gheen lyden <rhyme label="a" type="m" corresp="#s3:l1">swaer</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Ons Hooft ende <rhyme label="a" type="m" corresp="#s3:l1">Pijlaer</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Is hy seer <rhyme label="b" type="m" corresp="#s3:l4">autentijck</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Sijn rijck <rhyme label="b" type="m" corresp="#s3:l4">publijck</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Winnen sy alle <rhyme label="b" type="m" corresp="#s3:l4">ghelijck</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Die hier sonder <rhyme label="b" type="m" corresp="#s3:l4">afwijck</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Totter doot blyven <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">Ontfangt ons jonste <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s3:l14" met="3" type="-+">Al is de conste <rhyme label="d" type="m" corresp="#s3:l13">cleyn</rhyme>,</l>
        <l id="s3:l15" met="3" type="-+">Wt liefden ist <rhyme label="c" type="m" corresp="#s3:l12">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Der Liefden <rhyme label="a" type="m">brant</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En den eenigen <rhyme label="a" type="m" corresp="#s4:l1">bant</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Haude die <rhyme label="a" type="m" corresp="#s4:l1">Overhant</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In uwer herten <rhyme label="b" type="m">meest</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Hope <rhyme label="a" type="m" corresp="#s4:l1">valiant</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Door t'geloove <rhyme label="a" type="m" corresp="#s4:l1">constant</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Sy by u <rhyme label="a" type="m" corresp="#s4:l1">abondant</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Hier in dit wilt <rhyme label="b" type="m" corresp="#s4:l4">foreest</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">V niet en <rhyme label="b" type="m" corresp="#s4:l4">vreest</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Voor het ghecroonde <rhyme label="b" type="m" corresp="#s4:l4">Beest</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Den geest vol <rhyme label="b" type="m" corresp="#s4:l4">keest</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Haut u op d <rhyme label="c" type="m">engebaen</rhyme>,</l>
        <l id="s4:l13" met="3" type="-+">Dit is ons jonste <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s4:l14" met="3" type="-+">Al is de conste <rhyme label="d" type="m" corresp="#s4:l13">cleyn</rhyme>,</l>
        <l id="s4:l15" met="3" type="-+">Wt liefden ist <rhyme label="c" type="m" corresp="#s4:l12">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Een Christen <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die moet op elck <rhyme label="a" type="m" corresp="#s5:l1">termijn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Vreetsaem van herten <rhyme label="a" type="m" corresp="#s5:l1">zijn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tuycht ons die schrefteur <rhyme label="b" type="m">ient</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Heylighen <rhyme label="a" type="m" corresp="#s5:l1">schijn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Schijnen ghelijck <rhyme label="a" type="m" corresp="#s5:l1">fenijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En aen den rechten <rhyme label="a" type="m" corresp="#s5:l1">Lijn</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Hauden tot inden <rhyme label="b" type="m" corresp="#s5:l4">ent</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Om geen <rhyme label="b" type="m" corresp="#s5:l4">torment</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Wijckt niet vant <rhyme label="b" type="m" corresp="#s5:l4">fondament</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Godts woorden <rhyme label="b" type="m" corresp="#s5:l4">excelent</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Prent nu ter herten <rhyme label="c" type="m">saen</rhyme>,</l>
        <l id="s5:l13" met="3" type="-+">Ontfangt ons ionste <rhyme label="d" type="m">reyn</rhyme>,</l>
        <l id="s5:l14" met="3" type="-+">Al is de conste <rhyme label="d" type="m" corresp="#s5:l13">cleyn</rhyme>,</l>
        <l id="s5:l15" met="3" type="-+">Wt liefden ist <rhyme label="c" type="m" corresp="#s5:l12">ghedaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Neempt onsen <rhyme label="a" type="m">sanck</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Voor een oorlof in <rhyme label="a" type="m" corresp="#s6:l1">danck</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Al ist ghesant niet <rhyme label="a" type="m" corresp="#s6:l1">lanck</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wt liefden ist <rhyme label="b" type="m">gheschiet</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Jonste die <rhyme label="a" type="m" corresp="#s6:l1">dranck</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Den geest vierich <rhyme label="a" type="m" corresp="#s6:l1">ontspranck</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Wt herten vry en <rhyme label="a" type="m" corresp="#s6:l1">vranck</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dus wel den gront <rhyme label="b" type="m" corresp="#s6:l4">insiet</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">En wanckelt <rhyme label="b" type="m" corresp="#s6:l4">niet</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Ghelijck het wilde <rhyme label="b" type="m" corresp="#s6:l4">riet</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Neempt danckelic dit <rhyme label="b" type="m" corresp="#s6:l4">liet</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">En blyft bevolen <rhyme label="c" type="m">Godt</rhyme>,</l>
        <l id="s6:l13" met="3" type="-+">Nu nae t'betamen <rhyme label="d" type="m">recht</rhyme>,</l>
        <l id="s6:l14" met="3" type="-+">Te samen Amen <rhyme label="d" type="m" corresp="#s6:l13">seght</rhyme>,</l>
        <l id="s6:l15" met="3" type="-+">Vriendekens voor een <rhyme label="c" type="m" corresp="#s6:l12">slot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
