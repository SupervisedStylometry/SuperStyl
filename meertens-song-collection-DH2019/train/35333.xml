<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35333</id>
    <date>
      <year-start>1712</year-start>
      <year-end>1712</year-end>
    </date>
    <location>
      <orginal-name>duinkerken</orginal-name>
      <standard-name>Duinkerken, France</standard-name>
      <coordinates>
        <lon>51.1907405</lon>
        <lat>2.4978867</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Jacobus de Ruyter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WEest eens <rhyme label="a" type="m">gegroet</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Mijn Engel <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die ick soo seer <rhyme label="b" type="m">bemin</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Uw' soet <rhyme label="c" type="m">gelaedt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">My wel <rhyme label="c" type="m" corresp="#s1:l4">aen-staet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hey toont my <rhyme label="b" type="m" corresp="#s1:l3">weder-min</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wie doet sijn <rhyme label="a" type="m">klacht</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Hier in den <rhyme label="a" type="m" corresp="#s2:l1">nacht</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Gestaedigh voor mijn <rhyme label="b" type="m">deur</rhyme>?</l>
        <l id="s2:l4" met="2" type="-+">Wie magh het <rhyme label="c" type="m">zijn</rhyme>?</l>
        <l id="s2:l5" met="2" type="-+">Die klaeght van <rhyme label="c" type="m" corresp="#s2:l4">pijn</rhyme>?</l>
        <l id="s2:l6" met="3" type="-+">Jonghman staeckt uw <rhyme label="b" type="m" corresp="#s2:l3">getreur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Mijn soete <rhyme label="a" type="m">lief</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Mijns herten <rhyme label="a" type="m" corresp="#s3:l1">dief</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Vergeeft mijn <rhyme label="b" type="m">stoutigheydt</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Dat ick hier <rhyme label="c" type="m">kom</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Gy weet <rhyme label="c" type="m" corresp="#s3:l4">waerom</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ick hebb' het u <rhyme label="b" type="m" corresp="#s3:l3">geseydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Waerom komt <rhyme label="a" type="m">gy</rhyme>?</l>
        <l id="s4:l2" met="2" type="-+">Hey seght het <rhyme label="a" type="m" corresp="#s4:l1">my</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">'K en hebb' u niet <rhyme label="b" type="m">verstaen</rhyme>.</l>
        <l id="s4:l4" met="2" type="-+">Gy komt by <rhyme label="c" type="m">nacht</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Hier doen uw' <rhyme label="c" type="m" corresp="#s4:l4">klacht</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In 't schynen van de <rhyme label="b" type="m" corresp="#s4:l3">maen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Mijn schoon <rhyme label="a" type="m">Goddin</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Toont <rhyme label="a" type="m" corresp="#s5:l1">weder-min</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">'K versoeck' u tot de <rhyme label="b" type="m">trouw</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Geeft het <rhyme label="c" type="m">Jae-woordt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Wy zijn d' <rhyme label="c" type="m" corresp="#s5:l4">accoordt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ick kies u voor mijn <rhyme label="b" type="m" corresp="#s5:l3">vrouw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Uw' woorden <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Maer uw' <rhyme label="a" type="m" corresp="#s6:l1">gemoedt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En is my niet <rhyme label="b" type="m">bekent</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Opent uw' <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Wilt gy van <rhyme label="c" type="m" corresp="#s6:l4">smert</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Hier van my zijn <rhyme label="b" type="m" corresp="#s6:l3">gespent</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Gy schoone <rhyme label="a" type="m">maeght</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Die my <rhyme label="a" type="m" corresp="#s7:l1">behaeght</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ick sweir' u op mijn <rhyme label="b" type="m">eedt</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">'K en zal <rhyme label="c" type="m">voort-aen</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">U noyt <rhyme label="c" type="m" corresp="#s7:l4">af-gaen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Mijn lief het waer my <rhyme label="b" type="m" corresp="#s7:l3">leedt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Geeft my uw <rhyme label="a" type="m">handt</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Tot <rhyme label="a" type="m" corresp="#s8:l1">onder-pandt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Vol van <rhyme label="b" type="m">getrouwigheydt</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Ick van <rhyme label="c" type="m">content</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Op het <rhyme label="c" type="m" corresp="#s8:l4">moment</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Tot uwen dienst <rhyme label="b" type="m" corresp="#s8:l3">bereydt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Mijn <rhyme label="a" type="m">suycker-mondt</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Laet ons <rhyme label="a" type="m" corresp="#s9:l1">terstondt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Nu onder-trouw' gaen <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Ick ben te <rhyme label="c" type="m">vred'</rhyme>,</l>
        <l id="s9:l5" met="2" type="-+">Mijn lief gaet <rhyme label="c" type="m" corresp="#s9:l4">med'</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Mijn Engel wilt u <rhyme label="b" type="m" corresp="#s9:l3">spoên</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Mijn lief <rhyme label="a" type="m">Ariaen</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Hey laet ons <rhyme label="a" type="m" corresp="#s10:l1">gaen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Want wy zijn nu d' <rhyme label="b" type="m">accoordt</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Laet ons te <rhyme label="c" type="m">gaer</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Al met <rhyme label="c" type="m" corresp="#s10:l4">malkaer</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Vertrecken met 't <rhyme label="b" type="m" corresp="#s10:l3">jae-woordt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Ach liefste <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">'K ben uyt de <rhyme label="a" type="m" corresp="#s11:l1">pijn</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Mijn hert' springht op van <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s11:l4" met="2" type="-+">Ick ben <rhyme label="c" type="m">gerust</rhyme>,</l>
        <l id="s11:l5" met="2" type="-+">Lief eens <rhyme label="c" type="m" corresp="#s11:l4">gekust</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">En dit in eer' en <rhyme label="b" type="m" corresp="#s11:l3">deught</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">Sy zijn <rhyme label="a" type="m">getrouwt</rhyme>,</l>
        <l id="s12:l2" met="2" type="-+">Al sonder <rhyme label="a" type="m" corresp="#s12:l1">faut</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Deês twee gelieven t' <rhyme label="b" type="m">saem</rhyme>,</l>
        <l id="s12:l4" met="2" type="-+">Het <rhyme label="c" type="m">Houwelijck</rhyme>,</l>
        <l id="s12:l5" met="2" type="-+">Een weirdigh <rhyme label="c" type="m" corresp="#s12:l4">stijck</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Aen Godt seer <rhyme label="b" type="m" corresp="#s12:l3">aengenaem</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
