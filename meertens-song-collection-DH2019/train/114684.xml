  <author>Guilielmus Bolognino</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ANtwerpen, schoon bloem der <rhyme label="a" type="f">steden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">U Norbertus heeft <rhyme label="b" type="m">bewaert</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Doen gh'onsalich <rhyme label="a" type="f" corresp="#s1:l1">afghetreden</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Van de waerheydt eertijdts <rhyme label="b" type="m" corresp="#s1:l2">waert</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s1:l5">weldaden</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s1:l6">gheschiedt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Tanchelinus door sijn <rhyme label="a" type="f">locken</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Had u van het recht <rhyme label="b" type="m">gheloof</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Van u suyverheydt <rhyme label="a" type="f" corresp="#s2:l1">ghetrocken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En ghestelt de hel tot <rhyme label="b" type="m" corresp="#s2:l2">roof</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s2:l5">weldaden</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s2:l6">gheschiedt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'t Hemels broodt hy u <rhyme label="a" type="f">ontnomen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Had door sijn te valsche <rhyme label="b" type="m">leer</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat ghy wed'rom hebt <rhyme label="a" type="f" corresp="#s3:l1">becomen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Door Norbertum: doet hem <rhyme label="b" type="m" corresp="#s3:l2">eer</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s3:l5">weldaden</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s3:l6">gheschiedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die u heeft ghebroght het <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En daer van het <rhyme label="b" type="m">onderhoudt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Alle kettery <rhyme label="a" type="f" corresp="#s4:l1">verdreven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Weer met Christo u <rhyme label="b" type="m" corresp="#s4:l2">ghetrouwt</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s4:l5">weldaden</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s4:l6">gheschiedt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Stelt dan soo niet uyt u <rhyme label="a" type="f">sinnen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Hem die u weert heeft <rhyme label="b" type="m">verlicht</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Gaet hem hertelijck <rhyme label="a" type="f" corresp="#s5:l1">beminnen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Die in Godt u heeft <rhyme label="b" type="m" corresp="#s5:l2">hersticht</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s5:l5">weldaden</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s5:l6">gheschiedt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laet 't gheloof in u oock <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat Norbertus heeft <rhyme label="b" type="m">gheplant</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Gaet u tot de deucht <rhyme label="a" type="f" corresp="#s6:l1">begheven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Alle sond' en dolingh <rhyme label="b" type="m" corresp="#s6:l2">bant</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s6:l5">weldaden</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s6:l6">gheschiedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Op dat ghy eens moght <rhyme label="a" type="f">herbloeien</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">En becomen d' eerste <rhyme label="b" type="m">jeught</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Door Godts gratie altijdt <rhyme label="a" type="f" corresp="#s7:l1">groeien</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Rijck in alderhande <rhyme label="b" type="m" corresp="#s7:l2">deught</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s7:l5">weldaden</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s7:l6">gheschiedt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">O Norberte, die met <rhyme label="a" type="f">preken</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Hebt ons Stadt gheschoncken 't <rhyme label="b" type="m">licht</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">'t Heylich vier daer in <rhyme label="a" type="f" corresp="#s8:l1">ontsteken</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Met u leven seer <rhyme label="b" type="m" corresp="#s8:l2">ghesticht</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s8:l5">weldaden</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s8:l6">gheschiedt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Van den Heer krijght haer <rhyme label="a" type="f">ghenaden</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">En vergeet haer kranckheydt <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Voeght die jonst by u <rhyme label="a" type="f" corresp="#s9:l1">weldaden</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Door u haer van Godt <rhyme label="b" type="m" corresp="#s9:l2">gheschiedt</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Looft den Heer van sijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">En vergeet s' ondanckbaer <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Maer ghedenckt all' sijn <rhyme label="c" type="f" corresp="#s9:l5">weldaden</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Door Norbertum u <rhyme label="d" type="m" corresp="#s9:l6">gheschiedt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
