<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184621</id>
    <date>
      <year-start>1755</year-start>
      <year-end>1755</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Een koopmansdochter tot Oostende</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Aanhoord dog <rhyme label="a" type="f">allegaaren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wat ik u verhalen <rhyme label="b" type="m">zal</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die by der Zee gaan <rhyme label="a" type="f" corresp="#s1:l1">vaaren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Luystert Menschen in ’t <rhyme label="b" type="m" corresp="#s1:l2">getal</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">En Land-Luyden hier ter <rhyme label="c" type="m">Stee</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Luystert na dit Liedje <rhyme label="c" type="m" corresp="#s1:l5">mee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik zal ’t u gaan <rhyme label="a" type="f">verbreyden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Tot Lof van de Visschers <rhyme label="b" type="m">daar</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En hoe haar Godt gaat <rhyme label="a" type="f" corresp="#s2:l1">leyden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Als zy t’Zeyl gaan <rhyme label="b" type="m" corresp="#s2:l2">voorwaar</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En dat op de Zee met <rhyme label="c" type="m">spoed</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En hoe dat haar Godt <rhyme label="c" type="m" corresp="#s2:l5">behoed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gelijk een Harder <rhyme label="a" type="f">mede</rhyme>:</l>
        <l id="s3:l2" met="4" type="+-">Die zijn Schaapkens hoed in ’t <rhyme label="b" type="m">Veld</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Zo geleyd Godt met <rhyme label="a" type="f" corresp="#s3:l1">vreede</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">De Vissertjes wel <rhyme label="b" type="m" corresp="#s3:l2">geteld</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Als zy vaaren op de <rhyme label="c" type="m">Zee</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Met haar Schepen alzo <rhyme label="c" type="m" corresp="#s3:l5">ree</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Godt komt ons wel <rhyme label="a" type="f">beproeven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Met Stormen en <rhyme label="b" type="m">Onweer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy weet wat wy <rhyme label="a" type="f" corresp="#s4:l1">behoeven</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Als wy zijn op het <rhyme label="b" type="m" corresp="#s4:l2">Meer</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Als de Zee onstuymig <rhyme label="c" type="m">gaat</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dan en weeten wy geen <rhyme label="c" type="m" corresp="#s4:l5">raad</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dan oleggen wy ook <rhyme label="a" type="f">meden</rhyme>;</l>
        <l id="s5:l2" met="4" type="+-">Aan ons Anker dit <rhyme label="b" type="m">verstaat</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als de Baaren met <rhyme label="a" type="f" corresp="#s5:l1">reden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ons Schip schier te gronde <rhyme label="b" type="m" corresp="#s5:l2">slaat</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">En dan ziene wy de <rhyme label="c" type="m">dood</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Stadig voor ons oogen <rhyme label="c" type="m" corresp="#s5:l5">groot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dan roepen wy zeer <rhyme label="a" type="f">kragtig</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En met weenen alzo <rhyme label="b" type="m">zeer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Bewaard ons Heer <rhyme label="a" type="f" corresp="#s6:l1">Almagtig</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Dat wy niet zinken in ’t <rhyme label="b" type="m" corresp="#s6:l2">Meer</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Dat wy niet versmooren <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">In ons Zonden met <rhyme label="c" type="m" corresp="#s6:l5">verdriet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dat moeten wy al <rhyme label="a" type="f">lyde</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Wat ons komt al tegen <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Godt kan ons wel <rhyme label="a" type="f" corresp="#s7:l1">bevryde</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Van het Onweder <rhyme label="b" type="m" corresp="#s7:l2">voortaan</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Land-luyde neemt in ’t <rhyme label="c" type="m">gedagt</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Als gy gaat slapen zeer <rhyme label="c" type="m" corresp="#s7:l5">zagt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Daarom wild niet <rhyme label="a" type="f">veragte</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">ô d’Visschers verfoeye <rhyme label="b" type="m">zeer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">By Dagen of by <rhyme label="a" type="f" corresp="#s8:l1">Nagten</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Is de dood haar niet <rhyme label="b" type="m" corresp="#s8:l2">veer</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Gy Land-luy dit wel <rhyme label="c" type="m">verstaat</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Als haar Schip naby <rhyme label="c" type="m" corresp="#s8:l5">vergaat</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">En gy legt in u <rhyme label="a" type="f">ruste</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Met u Kinderen ook <rhyme label="b" type="m">mee</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En wat u dog mag <rhyme label="a" type="f" corresp="#s9:l1">luste</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">En dat krygt gy hier ter <rhyme label="b" type="m" corresp="#s9:l2">stee</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Godt kan u ook vinde <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">En de dood ook mee zeer <rhyme label="c" type="m" corresp="#s9:l5">snel</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Daarom kend gy niet <rhyme label="a" type="f">roeme</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Van u leven hier <rhyme label="b" type="m">minjoot</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hy kan wel haastig <rhyme label="a" type="f" corresp="#s10:l1">kome</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Met zijn Zeyzen ook zeer <rhyme label="b" type="m" corresp="#s10:l2">groot</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Daarom veragt den Zee-man <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Hy brengt Vis van alles <rhyme label="c" type="m" corresp="#s10:l5">ziet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wat moet hy nog wel <rhyme label="a" type="f">lyden</rhyme>?</l>
        <l id="s11:l2" met="4" type="+-">Eer zijn Reys dan is <rhyme label="b" type="m">gedaan</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Eer Godt hem komt <rhyme label="a" type="f" corresp="#s11:l1">verblyden</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">En brengt hem aan ’t Land <rhyme label="b" type="m" corresp="#s11:l2">voortaan</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Van de Baaren is hy <rhyme label="c" type="m">stil</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">En dan krijgt hy daar zijn <rhyme label="c" type="m" corresp="#s11:l5">wil</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Veragt geen Land-luy <rhyme label="a" type="f">mede</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Want ten is niet wel <rhyme label="b" type="m">gedaan</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Want Godt die heeft met <rhyme label="a" type="f" corresp="#s12:l1">vreede</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Elk zijn Werk gewezen <rhyme label="b" type="m" corresp="#s12:l2">aan</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">Want het eene is de <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">’t Ander de voet van het <rhyme label="c" type="m" corresp="#s12:l5">Land</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Oorlof nu voor het <rhyme label="a" type="f">leste</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">En neemt dog dit Lied in <rhyme label="b" type="m">dank</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">En houd het mijn ten <rhyme label="a" type="f" corresp="#s13:l1">beste</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Al en is het niet heel <rhyme label="b" type="m" corresp="#s13:l2">lank</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Ik wensch al de Menschen <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Goed lust en Zegen in <rhyme label="c" type="m" corresp="#s13:l5">vree</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
