        <l id="s2:l4" met="4" type="-+">Teld op de aldereerste <rhyme label="a" type="m" corresp="#s2:l1">nacht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wanneer dat gy <rhyme label="a" type="m" corresp="#s2:l1">verwacht</rhyme>,</l>
