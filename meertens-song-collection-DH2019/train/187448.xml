<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187448</id>
    <date>
      <year-start>1796</year-start>
      <year-end>1796</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wees welkom lieflijke lente</melody>
  <author>Johannes Wilhelmus Bussingh</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Hij komt: - Hij komt: - De sterke <rhyme label="a" type="m">Held</rhyme>! -</l>
        <l id="s1:l2" met="3" type="-+">Ontzachlijk groot in <rhyme label="b" type="m">macht</rhyme>. -</l>
        <l id="s1:l3" met="4" type="-+">Nu wordt de Satan <rhyme label="a" type="m" corresp="#s1:l1">neêrgeveld</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Nu derft de Dood zijn <rhyme label="b" type="m" corresp="#s1:l2">kracht</rhyme>. -</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wie komt? - Wie komt? - Wie is die <rhyme label="a" type="m">Held</rhyme>? -</l>
        <l id="s2:l2" met="3" type="-+">Zo vreeslijk groot in <rhyme label="b" type="m">macht</rhyme>! -</l>
        <l id="s2:l3" met="4" type="-+">messia is 't! - Heel de aerde <rhyme label="a" type="m" corresp="#s2:l1">meld'</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Het heil haer <rhyme label="b" type="m" corresp="#s2:l2">aengebracht</rhyme>! -</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hij komt: - Hij komt: - Gods eigen <rhyme label="a" type="m">Zoon</rhyme>: -</l>
        <l id="s3:l2" met="3" type="-+">De bron van vrede en <rhyme label="b" type="m">troost</rhyme>! -</l>
        <l id="s3:l3" met="4" type="-+">Hij daelt ter neêr van 's Hemels <rhyme label="a" type="m" corresp="#s3:l1">throon</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Tot heil van Adams <rhyme label="b" type="m" corresp="#s3:l2">kroost</rhyme>! -</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wie is, - Wie is Gods eigen <rhyme label="a" type="m">Zoon</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De bron van vrede en <rhyme label="b" type="m">troost</rhyme>? -</l>
        <l id="s4:l3" met="4" type="-+">messia is 't! - van 's Hemels <rhyme label="a" type="m" corresp="#s4:l1">throon</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Daelt heil voor Adams <rhyme label="b" type="m" corresp="#s4:l2">kroost</rhyme>! -</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hij komt: - Hij komt: - Hij brengt <rhyme label="a" type="m">genaê</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Aen 't zondig Menschdom <rhyme label="b" type="m">weêr</rhyme>! -</l>
        <l id="s5:l3" met="4" type="-+">In 't need'rig Bethlem <rhyme label="a" type="m" corresp="#s5:l1">Efrata</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Verschijnt des weerelds <rhyme label="b" type="m" corresp="#s5:l2">Heer</rhyme>! -</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wie komt? - Wie komt? - Wie brengt <rhyme label="a" type="m">genaê</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Aen 't zondig menschdom <rhyme label="b" type="m">weêr</rhyme>? -</l>
        <l id="s6:l3" met="4" type="-+">messia is 't! - In <rhyme label="a" type="m" corresp="#s6:l1">Efrata</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Daelt God in 't vleesch ter <rhyme label="b" type="m" corresp="#s6:l2">neêr</rhyme>! -</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Triömf! - Wij huwen onzen <rhyme label="a" type="m">toon</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Aen 't Lied van <rhyme label="b" type="m">Efrata</rhyme>! -</l>
        <l id="s7:l3" met="4" type="-+">Wees welkom, Gods gezalfde <rhyme label="a" type="m" corresp="#s7:l1">Zoon</rhyme>! -</l>
        <l id="s7:l4" met="3" type="-+">Triömf! - <rhyme label="b" type="m" corresp="#s7:l2">Hallelujah</rhyme>! -</l>
      </lg>
    </lg>
  </text>
</song>
