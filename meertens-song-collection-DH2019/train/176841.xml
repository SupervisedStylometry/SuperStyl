      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wy dancken u o Godt en <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat ghy ons nu tot uwer <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Gheprijst hebt door u heyligh <rhyme label="b" type="m">woort</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat wy alhier hebben <rhyme label="b" type="m" corresp="#s1:l3">ghehoort</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt dat oock nu maken <rhyme label="a" type="m">vruchtbaer</rhyme></l>
        <l id="s2:l2" met="4" type="-+">In onse herten <rhyme label="a" type="m" corresp="#s2:l1">allegaer</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat wy volbringhen met der <rhyme label="b" type="m">daet</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Nae uwen Goddelijcken <rhyme label="b" type="m" corresp="#s2:l3">raet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daer toe lief Heere geeft ons <rhyme label="a" type="m">kracht</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dat bidden wy u met <rhyme label="a" type="m" corresp="#s3:l1">aendacht</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Sonder u hulp waert al om <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">U ghenade over ons <rhyme label="b" type="m" corresp="#s3:l3">giet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Eer zy u Vader ende <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En Heyl’ghe Gheest in ‘s Hemels <rhyme label="a" type="m" corresp="#s4:l1">troon</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">By ons alle te samen <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Moet dit doch jae en Amen <rhyme label="b" type="m" corresp="#s4:l3">zijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
