    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ach Gott vom Himmel</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEEr mijn Confoort als ick <rhyme label="a" type="m">aenroep</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wilt my niet swijghen <rhyme label="b" type="f">stille</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Swijcht ghy, soo coem ick in den <rhyme label="a" type="m" corresp="#s1:l1">Hoop</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die varen in die <rhyme label="b" type="f" corresp="#s1:l2">Helle</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Verhoor mijn smeecken end' <rhyme label="c" type="m">Geschrey</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Als ick mijn Handen streck tot <rhyme label="c" type="m" corresp="#s1:l5">dy</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Nae dijnen Heyl'gen <rhyme label="d" type="f">Choore</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wilt my [...] Godloosen <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Vernielen noch wech <rhyme label="b" type="f">rucken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die voor Weldaet die haer <rhyme label="a" type="m" corresp="#s2:l1">gheschiet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Niet doen dan quade <rhyme label="b" type="f" corresp="#s2:l2">Stucken</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Die goede Woorde spreken <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Maer hebben boose Herten <rhyme label="c" type="m" corresp="#s2:l5">fel</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Om ander te <rhyme label="d" type="f">bedrieghen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Geeft haer nae hare Feyten <rhyme label="a" type="m">swaer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En nae haers Handen <rhyme label="b" type="f">Wercken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Nae haer Verdienst vergeldet <rhyme label="a" type="m" corresp="#s3:l1">haer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Want sy op GOdt niet <rhyme label="b" type="f" corresp="#s3:l2">mercken</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Noch op die Wercken zijner <rhyme label="c" type="m">Handt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Dies maeckt hyse al heel te <rhyme label="c" type="m" corresp="#s3:l5">schandt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Maer mijn Ghebedt <rhyme label="d" type="f">verhooret</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Mijn stercke Schilt is GOdt de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Mijn Hert is vro en <rhyme label="b" type="f">blijde</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hy helpt my altijt, dies ick <rhyme label="a" type="m" corresp="#s4:l1">meer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met Danck syn Loff <rhyme label="b" type="f" corresp="#s4:l2">belijde</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">De HEER is onser aller <rhyme label="c" type="m">Sterckt</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Helpt syn Gesalffden, dat wel <rhyme label="c" type="m" corresp="#s4:l5">merckt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Sijn Erff hy seghent <rhyme label="d" type="f">Eewich</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
