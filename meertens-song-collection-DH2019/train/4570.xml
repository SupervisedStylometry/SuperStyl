      <year-start>1667</year-start>
      <year-end>1667</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Mon mari est à Paris</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Pieter Elsevier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SEg my dan volmaeckte <rhyme label="a" type="m">Beeldt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zal ick u noyt <rhyme label="b" type="f">verwinnen</rhyme>?</l>
        <l id="s1:l3" met="4" type="+-">Al te straffe <rhyme label="c" type="m">Rosemond</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Nu mijn zieltjen is <rhyme label="c" type="m" corresp="#s1:l3">gewond</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van binnen, van binnen, van <rhyme label="b" type="f" corresp="#s1:l2">binnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Zal ick dan door mijn <rhyme label="a" type="m">gebeên</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Noyt komen in u <rhyme label="b" type="f">gracy</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Soo en zie ick noyt het <rhyme label="c" type="m">endt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Van mijn droefheyt en <rhyme label="c" type="m" corresp="#s2:l3">ellendt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Och lacy, och lacy, och <rhyme label="b" type="f" corresp="#s2:l2">lacy</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ach volmaeckte <rhyme label="a" type="m">Rosemond</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Ick weet geen troost te <rhyme label="b" type="f">vinden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Laat doch van u wreetheyt <rhyme label="c" type="m">af</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Of ghy stort mijn in het <rhyme label="c" type="m" corresp="#s3:l3">graf</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Beminde, Beminde. <rhyme label="b" type="f" corresp="#s3:l2">Beminde</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hemel ick en weet geen <rhyme label="a" type="m">raet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Mijn klachten zijn <rhyme label="b" type="f">verlooren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ick zal sonder <rhyme label="c" type="m">Medecijn</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">In mijn droeve <rhyme label="c" type="m" corresp="#s4:l3">minne-pijn</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Versmooren, versmooren, <rhyme label="b" type="f" corresp="#s4:l2">versmooren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Kan ick niet mijn <rhyme label="a" type="m">Rosemond</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Bewegen met een <rhyme label="b" type="f">suchje</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Ach mijn Rosemondje <rhyme label="c" type="m">kom</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Anders raeckt u Minnaer <rhyme label="c" type="m" corresp="#s5:l3">om</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Een luchje, een luchje, een <rhyme label="b" type="f" corresp="#s5:l2">luchje</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Laet ick u maer tot een <rhyme label="a" type="m">kus</rhyme></l>
        <l id="s6:l2" met="3" type="-+">O Rosemond <rhyme label="b" type="f">bekooren</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Soo is al mijn smert <rhyme label="c" type="m">gedaen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En ick sal weer gaen en <rhyme label="c" type="m" corresp="#s6:l3">staen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Als vooren, als vooren, als <rhyme label="b" type="f" corresp="#s6:l2">vooren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ach, hoe kan u killig <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Soo onbeweeglijck <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Daer ghy door u lieve <rhyme label="c" type="m">mond</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Lichtelijck mijn droefheyt <rhyme label="c" type="m" corresp="#s7:l3">kond</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Genesen, genesen, <rhyme label="b" type="f" corresp="#s7:l2">genesen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Laet u dan ô <rhyme label="a" type="m">Rosemond</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Mijn smeecken <rhyme label="b" type="f">overwinnen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dat ick al 't geleden <rhyme label="c" type="m">leet</rhyme></l>
        <l id="s8:l4" met="4" type="+-">En mijn hertzeer weer <rhyme label="c" type="m" corresp="#s8:l3">vergeet</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Van 't minnen, van 't minnen, van 't <rhyme label="b" type="f" corresp="#s8:l2">minnen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Dan zal ick u waerde <rhyme label="a" type="m">Lief</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Altijdt als mijn <rhyme label="b" type="f">Beminde</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Geven 't allerzoetst <rhyme label="c" type="m">vermaeck</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Waer in dat ghy goeden <rhyme label="c" type="m" corresp="#s9:l3">smaeck</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Zult vinden, zult vinden, zult <rhyme label="b" type="f" corresp="#s9:l2">vinden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
