    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Bernardus Busschoff</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VEel gelucks Doorluchtigh <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Roem en <rhyme label="a" type="m" corresp="#s1:l1">Eer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Van de wijd-beroemde <rhyme label="b" type="f">Landen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Veel gelucks trouw Nassous <rhyme label="c" type="m">Bloedt</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Met <rhyme label="c" type="m" corresp="#s1:l4">ootmoet</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Kussen wy u strijdtbaer' <rhyme label="b" type="f" corresp="#s1:l3">handen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Nu ghy in de plaetse <rhyme label="a" type="m">treet</rhyme></l>
        <l id="s2:l2" met="2" type="+-">'t Ampt <rhyme label="a" type="m" corresp="#s2:l1">bekleet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Van uw' hoogh-gebooren <rhyme label="b" type="f">Vader</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En Heer broeder wijdt <rhyme label="c" type="m">befaemt</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Ons <rhyme label="c" type="m" corresp="#s2:l4">betaemt</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Godt te dancken <rhyme label="b" type="f" corresp="#s2:l3">allegader</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Godt wy dancken al te <rhyme label="a" type="m">saem</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Uwen <rhyme label="a" type="m" corresp="#s3:l1">naem</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat ghy hebt voor ons <rhyme label="b" type="f">behouden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Een soo eel Orangien <rhyme label="c" type="m">Spruyt</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Breydt hem <rhyme label="c" type="m" corresp="#s3:l4">uyt</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Dat hy werdt gelijck den <rhyme label="b" type="f" corresp="#s3:l3">Ouden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">d' Oude Stam veel vruchten <rhyme label="a" type="m">droegh</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Laet en <rhyme label="a" type="m" corresp="#s4:l1">vroegh</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Smaeckelijck van ons <rhyme label="b" type="f">genooten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Soo sal oock dees schoonen <rhyme label="c" type="m">Boom</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Met den <rhyme label="c" type="m" corresp="#s4:l4">stroom</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Uwer gunst zijnd' <rhyme label="b" type="f" corresp="#s4:l3">overgooten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Heer die alle kruyden <rhyme label="a" type="m">bout</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">
          <rhyme label="a" type="m" corresp="#s5:l1">Onderhout</rhyme>
        </l>
        <l id="s5:l3" met="4" type="+-">d' Hoogh en Edel stam <rhyme label="b" type="f">Orangien</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Dat wij in sijn schaduw' <rhyme label="c" type="m">soel</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Sitten <rhyme label="c" type="m" corresp="#s5:l4">koel</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Vry van 't heete vyer van <rhyme label="b" type="f" corresp="#s5:l3">Spangien</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Schoort hem doch aen elcken <rhyme label="a" type="m">kant</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Met uw' <rhyme label="a" type="m" corresp="#s6:l1">handt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Nu hy is in 't best van <rhyme label="b" type="f">dragen</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Deckt hem toe voor wint en <rhyme label="c" type="m">storm</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Laet geen <rhyme label="c" type="m" corresp="#s6:l4">worm</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Aen sijn hert of wortel <rhyme label="b" type="f" corresp="#s6:l3">knagen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Soo langh als de Son en <rhyme label="a" type="m">Maen</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Sullen <rhyme label="a" type="m" corresp="#s7:l1">gaen</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Haer nu langh betreden <rhyme label="b" type="f">paden</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Laet soo langh t' Nassous <rhyme label="c" type="m">geslacht</rhyme></l>
        <l id="s7:l5" met="2" type="+-">Zijn <rhyme label="c" type="m" corresp="#s7:l4">geacht</rhyme></l>
        <l id="s7:l6" met="4" type="+-">En gedacht haer vroome <rhyme label="b" type="f" corresp="#s7:l3">daden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
