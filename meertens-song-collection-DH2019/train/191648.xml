  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Bomen, Rotzen, <rhyme label="a" type="m">Watervloen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">En het <rhyme label="a" type="m" corresp="#s1:l1">groen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Getuygen hoe ik in 't <rhyme label="b" type="f">Minnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Trots de getrouwste <rhyme label="c" type="m">Herderin</rhyme></l>
        <l id="s1:l5" met="4" type="-+">U meerder als my zelfs <rhyme label="c" type="m" corresp="#s1:l4">bemin</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Niet met Mond, maar met Hert en <rhyme label="b" type="f" corresp="#s1:l3">Zinnen</rhyme>.</l>
      </lg>
    </lg>
