<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185083</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wilhelmus</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Kom, staak uw droevig <rhyme label="a" type="f">treuren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Beminnaars van den <rhyme label="b" type="m">Heer</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Wyl God ons op wil <rhyme label="a" type="f" corresp="#s1:l1">beuren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ons red, gelyk wel <rhyme label="b" type="m" corresp="#s1:l2">eer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daar Hy ons uit <rhyme label="c" type="f">elenden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Verlost, en ’t <rhyme label="d" type="m">doodsgevaar</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Goedgunstig af wil <rhyme label="c" type="f" corresp="#s1:l5">wenden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Loof Hem, Godvruchte <rhyme label="d" type="m" corresp="#s1:l6">Schaar</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Laat ons, met blyde <rhyme label="a" type="f">klanken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hem geeven lof en <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Laat ons verheugd, Hem <rhyme label="a" type="f" corresp="#s2:l1">danken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Daar Hy Prins willem <rhyme label="b" type="m" corresp="#s2:l2">weer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wou brengen tot zyn <rhyme label="c" type="f">Wooning</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En redden uit den <rhyme label="d" type="m">druk</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">O Groote Hemel – <rhyme label="c" type="f" corresp="#s2:l5">Koning</rhyme>!</l>
        <l id="s2:l8" met="3" type="-+">Geef verder hem <rhyme label="d" type="m" corresp="#s2:l6">geluk</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wil onze <rhyme label="a" type="f">Erfstadhouder</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">O aller Vorsten <rhyme label="b" type="m">Heer</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">Nu Gy op zyne <rhyme label="a" type="f" corresp="#s3:l1">schouder</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Doet rusten, als wel <rhyme label="b" type="m" corresp="#s3:l2">eer</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zyn Hooge <rhyme label="c" type="f">Waardigheden</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En Ampten, by de <rhyme label="d" type="m">hand</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Steeds leiden, richt zyn <rhyme label="c" type="f" corresp="#s3:l5">treeden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Tot heil van Kerk en <rhyme label="d" type="m" corresp="#s3:l6">Land</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Laat nimmer ramp hem <rhyme label="a" type="f">hindren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Noch zyne <rhyme label="b" type="m">Gemaalin</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wil ook hun dierbre <rhyme label="a" type="f" corresp="#s4:l1">Kindren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">O God! uit tedre <rhyme label="b" type="m" corresp="#s4:l2">min</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Met hen genadig <rhyme label="c" type="f">hoeden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Voor tegenspoed en <rhyme label="d" type="m">druk</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Schenk aan hem altoos ’t <rhyme label="c" type="f" corresp="#s4:l5">goede</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Ja, geef hem ’t grootst <rhyme label="d" type="m" corresp="#s4:l6">geluk</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Geef die Regeerders <rhyme label="a" type="f">zegen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die ’t heil van Kerk en <rhyme label="b" type="m">Land</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Zeer zwaar op ’t harte <rhyme label="a" type="f" corresp="#s5:l1">weegen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Gelei hen by de <rhyme label="b" type="m" corresp="#s5:l2">hand</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Doe hen toch steeds <rhyme label="c" type="f">ervaaren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat Gy hen hulp en <rhyme label="d" type="m">raad</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Kunt schenken, wil hen <rhyme label="c" type="f" corresp="#s5:l5">spaaren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Tot nut van Kerk en <rhyme label="d" type="m" corresp="#s5:l6">Staat</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Geef, Braaven, die hier <rhyme label="a" type="f">woonen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Uw heil, ô groote <rhyme label="b" type="m">God</rhyme>!</l>
        <l id="s6:l3" met="3" type="-+">Wil ons met zegen <rhyme label="a" type="f" corresp="#s6:l1">kroonen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Geef ons dit zalig <rhyme label="b" type="m" corresp="#s6:l2">lot</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat Neêrlands Burgren <rhyme label="c" type="f">leeven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Als Broeders, met <rhyme label="d" type="m">elkaâr</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">En wil ons ’t goede <rhyme label="c" type="f" corresp="#s6:l5">geeven</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Bescherm ons voor <rhyme label="d" type="m" corresp="#s6:l6">gevaar</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Laat nimmer onheil <rhyme label="a" type="f">schade</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ons dierbaar <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Maar breng dat, uit <rhyme label="a" type="f" corresp="#s7:l1">genade</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Weer in verheven <rhyme label="b" type="m" corresp="#s7:l2">stand</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Wil tog in gunst <rhyme label="c" type="f">vergeeven</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Om jezus wil, al ’t <rhyme label="d" type="m">kwaad</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dat wy zo lang <rhyme label="c" type="f" corresp="#s7:l5">bedreeven</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Blyf onze <rhyme label="d" type="m" corresp="#s7:l6">Toeverlaat</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wil, door Uw Geest ons <rhyme label="a" type="f">leider</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Op ’t pad der waare <rhyme label="b" type="m">deugd</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Maak ons ten allen <rhyme label="a" type="f" corresp="#s8:l1">tyden</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Door Uwe daân <rhyme label="b" type="m" corresp="#s8:l2">verheugd</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Doe ons steeds dankbaar <rhyme label="c" type="f">weezen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Dat de Erfstadhouder <rhyme label="d" type="m">weer</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Door U, gelyk voor <rhyme label="c" type="f" corresp="#s8:l5">deezen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Hersteld is in zyn <rhyme label="d" type="m" corresp="#s8:l6">eer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
