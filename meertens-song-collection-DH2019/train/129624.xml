  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">‘Dat alle Bergen goude <rhyme label="a" type="f">waren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En alle waters <rhyme label="b" type="m">Wyn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En alle waters <rhyme label="b" type="m" corresp="#s1:l2">Wyn</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Soo had ik jou veel <rhyme label="c" type="m">liever</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Moi meisje, waarje <rhyme label="b" type="m" corresp="#s1:l2">myn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Moi meisje, waarje <rhyme label="b" type="m" corresp="#s1:l2">myn</rhyme>.’</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">‘Had gy mijn dan veel <rhyme label="a" type="f">liever</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als gy mijn doet in <rhyme label="b" type="m">schyn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Als gy mijn doet in <rhyme label="b" type="m" corresp="#s2:l2">schyn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Zo gaat eens voor myn Vader <rhyme label="c" type="m">staan</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En bid, en bid om <rhyme label="b" type="m" corresp="#s2:l2">myn</rhyme>’</l>
        <l id="s2:l6" met="3" type="-+">En bid, en bid om <rhyme label="b" type="m" corresp="#s2:l2">myn</rhyme>’</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">‘ ‘k Heb al voor u Vader <rhyme label="a" type="f">geweest</rhyme></l>
        <l id="s3:l2" met="3" type="-+">En hy ontsegt het <rhyme label="b" type="m">mijn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En hy ontsegt het <rhyme label="b" type="m" corresp="#s3:l2">mijn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Neemt oorlof aan u <rhyme label="c" type="m">selven</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Schoon lief, en gaat met <rhyme label="b" type="m" corresp="#s3:l2">mijn</rhyme>’,</l>
        <l id="s3:l6" met="3" type="-+">Schoon lief, en gaat met <rhyme label="b" type="m" corresp="#s3:l2">mijn</rhyme>’,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">‘Oorlof aan mijn <rhyme label="a" type="f">selven</rhyme>?</l>
        <l id="s4:l2" met="3" type="-+">De knaapjes zijn soo <rhyme label="b" type="m">loos</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">De knaapjes zijn soo <rhyme label="b" type="m" corresp="#s4:l2">loos</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">O gy my dan verliet, schoon <rhyme label="c" type="m">lief</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zo was ik <rhyme label="b" type="m" corresp="#s4:l2">vriendeloos</rhyme>’,</l>
        <l id="s4:l6" met="3" type="-+">Zo was ik <rhyme label="b" type="m" corresp="#s4:l2">vriendeloos</rhyme>’,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">‘‘k En sal u niet <rhyme label="a" type="f">verlaten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van nu tot in der <rhyme label="b" type="m">dood</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Van nu tot in der <rhyme label="b" type="m" corresp="#s5:l2">dood</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Gy bent een Konings <rhyme label="c" type="m">Dochterken</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Een Roosjen alsoo <rhyme label="b" type="m" corresp="#s5:l2">root</rhyme>’,</l>
        <l id="s5:l6" met="3" type="-+">Een Roosjen alsoo <rhyme label="b" type="m" corresp="#s5:l2">root</rhyme>’,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">‘Ben ik een Konings <rhyme label="a" type="f">Dochterken</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En gy een Grave <rhyme label="b" type="m">kinde</rhyme>’</l>
        <l id="s6:l3" met="3" type="-+">En gy een Grave <rhyme label="b" type="m" corresp="#s6:l2">kinde</rhyme>’</l>
        <l id="s6:l4" met="4" type="-+">Sy namen malkander by de <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En gingen onder de <rhyme label="b" type="m" corresp="#s6:l2">Linde</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En gingen onder de <rhyme label="b" type="m" corresp="#s6:l2">Linde</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Zy namen malkander by de <rhyme label="a" type="f">hand</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En gongen onder de <rhyme label="b" type="m">Lindt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En gongen onder de <rhyme label="b" type="m" corresp="#s7:l2">Lindt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Daar speelden sy twee het <rhyme label="c" type="m">minnespel</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">De schoone werd met <rhyme label="b" type="m" corresp="#s7:l2">kind</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">De schoone werd met <rhyme label="b" type="m" corresp="#s7:l2">kind</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">‘Nu sit ik hier <rhyme label="a" type="f">gebonden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Met mynen kleinen <rhyme label="b" type="m">kind</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Met mynen kleinen <rhyme label="b" type="m" corresp="#s8:l2">kind</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">So bid ik de suiver <rhyme label="c" type="m">Maria</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Dat sy my weer <rhyme label="b" type="m" corresp="#s8:l2">ontbind</rhyme>’,</l>
        <l id="s8:l6" met="3" type="-+">Dat sy my weer <rhyme label="b" type="m" corresp="#s8:l2">ontbind</rhyme>’,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">‘Ik wou dat gy eens ontbonden <rhyme label="a" type="f">waard</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Van uwen kleinen <rhyme label="b" type="m">kind</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Van uwen kleinen <rhyme label="b" type="m" corresp="#s9:l2">kind</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">En dat ik jou begraven <rhyme label="c" type="m">sou</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Onder dees groene <rhyme label="b" type="m" corresp="#s9:l2">Lind</rhyme>’,</l>
        <l id="s9:l6" met="3" type="-+">Onder dees groene <rhyme label="b" type="m" corresp="#s9:l2">Lind</rhyme>’,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">‘Woud gy dat ik begraven <rhyme label="a" type="f">lag</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Al onder deese <rhyme label="b" type="m">Lind</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Al onder deese <rhyme label="b" type="m" corresp="#s10:l2">Lind</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Soo woud ik liever, stout <rhyme label="c" type="m">Ruyter</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Dat gy by de keel <rhyme label="b" type="m" corresp="#s10:l2">ophingt</rhyme>.’</l>
        <l id="s10:l6" met="3" type="-+">Dat gy by de keel <rhyme label="b" type="m" corresp="#s10:l2">ophingt</rhyme>.’</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">De Ruiter hief op sijn <rhyme label="a" type="f">linkerhand</rhyme></l>
        <l id="s11:l2" met="3" type="-+">En gaf haare eene <rhyme label="b" type="m">slag</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">En gaf haare eene <rhyme label="b" type="m" corresp="#s11:l2">slag</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">So dat sy neder ter aarden <rhyme label="c" type="m">viel</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Sy hoorde noch sy <rhyme label="b" type="m" corresp="#s11:l2">sag</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Sy hoorde noch sy <rhyme label="b" type="m" corresp="#s11:l2">sag</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">‘Al hebje myn nu <rhyme label="a" type="f">geslagen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Schoon lief, ’t en is geen <rhyme label="b" type="m">nood</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Schoon lief, ’t en is geen <rhyme label="b" type="m" corresp="#s12:l2">nood</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Al eerder seven jaren om <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">So selje komen om <rhyme label="b" type="m" corresp="#s12:l2">broot</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">So selje komen om <rhyme label="b" type="m" corresp="#s12:l2">broot</rhyme>,</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Al eer de seven jaren <rhyme label="a" type="f">waren</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Den Ruiter kwam om <rhyme label="b" type="m">brood</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Den Ruiter kwam om <rhyme label="b" type="m" corresp="#s13:l2">brood</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Met een Lazarus klep in sijn <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Hy haddet seer van <rhyme label="b" type="m" corresp="#s13:l2">nood</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Hy haddet seer van <rhyme label="b" type="m" corresp="#s13:l2">nood</rhyme>,</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">‘O kind’, sey sy, ‘van seven <rhyme label="a" type="f">jaar</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Nou set jou Vader een <rhyme label="b" type="m">stoel</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Nou set jou Vader een <rhyme label="b" type="m" corresp="#s14:l2">stoel</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Ik hebber den dag wel eer <rhyme label="c" type="m">gesien</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Hy wasser een Ridder <rhyme label="b" type="m" corresp="#s14:l2">koen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Hy wasser een Ridder <rhyme label="b" type="m" corresp="#s14:l2">koen</rhyme>,</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">O kind’, sey sey, ‘wel kleine <rhyme label="a" type="f">kind</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Nou geeft jou Vader <rhyme label="b" type="m">brood</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Nou geeft jou Vader <rhyme label="b" type="m" corresp="#s15:l2">brood</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Ik heb den dag wel eer <rhyme label="c" type="m">gesien</rhyme></l>
        <l id="s15:l5" met="3" type="-+">Hy haddet niet van <rhyme label="b" type="m" corresp="#s15:l2">nood</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Hy haddet niet van <rhyme label="b" type="m" corresp="#s15:l2">nood</rhyme>,</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">O kind’, sey sy, ‘wel kleine <rhyme label="a" type="f">kind</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Nou schenkt jou Vader <rhyme label="b" type="m">bier</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Nou schenkt jou Vader <rhyme label="b" type="m" corresp="#s16:l2">bier</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Ik heb den dag wel eer <rhyme label="c" type="m">geleeft</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Dat hy was de liefste <rhyme label="b" type="m" corresp="#s16:l2">fier</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Dat hy was de liefste <rhyme label="b" type="m" corresp="#s16:l2">fier</rhyme>,</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">O kind’, sey sy, ‘wel kleine <rhyme label="a" type="f">kind</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Nou schenkt jou Vader <rhyme label="b" type="m">wyn</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Nou schenkt jou Vader <rhyme label="b" type="m" corresp="#s17:l2">wyn</rhyme>,</l>
        <l id="s17:l4" met="4" type="-+">Ik heb den dag wel eer <rhyme label="c" type="m">beleeft</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Hy was de liefste <rhyme label="b" type="m" corresp="#s17:l2">myn</rhyme>’,</l>
        <l id="s17:l6" met="3" type="-+">Hy was de liefste <rhyme label="b" type="m" corresp="#s17:l2">myn</rhyme>’,</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Haar Vader agter de deure <rhyme label="a" type="f">stont</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Hy hoorden ’t hooge <rhyme label="b" type="m">Woord</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Hy hoorden ’t hooge <rhyme label="b" type="m" corresp="#s18:l2">Woord</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">Hy trock sijn blank zweert uit de <rhyme label="c" type="m">schee</rhyme></l>
        <l id="s18:l5" met="3" type="-+">En hieuw hem af zyn <rhyme label="b" type="m" corresp="#s18:l2">hoofd</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">En hieuw hem af zyn <rhyme label="b" type="m" corresp="#s18:l2">hoofd</rhyme>,</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Hy nam dat hoofje by het <rhyme label="a" type="f">hair</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">En wierp het in haar <rhyme label="b" type="m">schoot</rhyme>:</l>
        <l id="s19:l3" met="3" type="-+">En wierp het in haar <rhyme label="b" type="m" corresp="#s19:l2">schoot</rhyme>:</l>
        <l id="s19:l4" met="4" type="-+">‘Hou daar mijn jongste <rhyme label="c" type="m">Dochterken</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Beweent dees Appel <rhyme label="b" type="m" corresp="#s19:l2">root</rhyme>’,</l>
        <l id="s19:l6" met="3" type="-+">Beweent dees Appel <rhyme label="b" type="m" corresp="#s19:l2">root</rhyme>’,</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">‘Soud ik het al <rhyme label="a" type="f">beweenen</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Dat hier te beweenen <rhyme label="b" type="m">waar</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Dat hier te beweenen <rhyme label="b" type="m" corresp="#s20:l2">waar</rhyme>,</l>
        <l id="s20:l4" met="4" type="-+">So had ik wel al de dagen <rhyme label="c" type="m">werk</rhyme></l>
        <l id="s20:l5" met="3" type="-+">Die daar komen in ’t <rhyme label="b" type="m" corresp="#s20:l2">Jaar</rhyme>’</l>
        <l id="s20:l6" met="3" type="-+">Die daar komen in ’t <rhyme label="b" type="m" corresp="#s20:l2">Jaar</rhyme>’</l>
      </lg>
    </lg>
  </text>
</song>
