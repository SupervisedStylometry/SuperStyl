<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>175895</id>
    <date>
      <year-start>1609</year-start>
      <year-end>1609</year-end>
    </date>
    <location>
      <orginal-name>amstelredam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdesklacht" translation="love complaint">liefdesklacht</tag>
    </tags>
    <melody>Ik klaag u Venus' dieren</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Arnoldus Cobbault</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VVIe sal mijn bitter <rhyme label="a" type="f">lijden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">End' mijn druck <rhyme label="b" type="m">wederstaan</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Nu my van allen <rhyme label="a" type="f" corresp="#s1:l1">syden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">De liefde comt <rhyme label="b" type="m" corresp="#s1:l2">beslaan</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Cupido wilt my <rhyme label="b" type="m" corresp="#s1:l2">saan</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met pijlen scherp <rhyme label="a" type="f" corresp="#s1:l1">bestrijden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Wie sal my nu <rhyme label="b" type="m" corresp="#s1:l2">voortaan</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Van sijnen schicht <rhyme label="a" type="f" corresp="#s1:l1">bevrijden</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want ick word' voor mijn <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van Cupido <rhyme label="b" type="m">doorwont</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Tweemaal thien <rhyme label="a" type="f" corresp="#s2:l1">openbaren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Nu nauwlijcx mijnen <rhyme label="b" type="m" corresp="#s2:l2">gront</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Pallas aan mijnen <rhyme label="b" type="m" corresp="#s2:l2">mont</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Schildert nau gulden <rhyme label="a" type="f" corresp="#s2:l1">haren</rhyme></l>
        <l id="s2:l7" met="3" type="-+">End' ick ben int <rhyme label="b" type="m" corresp="#s2:l2">verbondt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Van liefden bange <rhyme label="a" type="f" corresp="#s2:l1">scharen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn herte moet <rhyme label="a" type="f">verteeren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met herte liefd' <rhyme label="b" type="m">omrandt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Als Icarus sijn <rhyme label="a" type="f" corresp="#s3:l1">veeren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Smelt voor der Sonnen <rhyme label="b" type="m" corresp="#s3:l2">brandt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Icarus moest met <rhyme label="b" type="m" corresp="#s3:l2">schandt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Noch leeger vliegen <rhyme label="a" type="f" corresp="#s3:l1">leeren</rhyme></l>
        <l id="s3:l7" met="3" type="-+">End' ick moet mijn <rhyme label="b" type="m" corresp="#s3:l2">verstant</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Tot u soo hooch niet <rhyme label="a" type="f" corresp="#s3:l1">keeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Meestersse die dees <rhyme label="a" type="f">vieren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Stoockt in mijn dorstich <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Wat sal het u <rhyme label="a" type="f" corresp="#s4:l1">vercieren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat ick verlooren <rhyme label="b" type="m" corresp="#s4:l2">werdt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Oft groeyt ghy in mijn <rhyme label="b" type="m" corresp="#s4:l2">smert</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Gelijck de wreede <rhyme label="a" type="f" corresp="#s4:l1">dieren</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Sulcx waar een quade <rhyme label="b" type="m" corresp="#s4:l2">pert</rhyme></l>
        <l id="s4:l8" met="3" type="-+">End' tegen u <rhyme label="a" type="f" corresp="#s4:l1">manieren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Comt Godlijcke <rhyme label="a" type="f">Najaden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Brengt cruycken vol van <rhyme label="b" type="m">wijn</rhyme></l>
        <l id="s5:l3" met="3" type="-+">End' soeckt aan allen <rhyme label="a" type="f" corresp="#s5:l1">paden</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Geluckich <rhyme label="b" type="m" corresp="#s5:l2">Medecijn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Soeckt my Lethes <rhyme label="b" type="m" corresp="#s5:l2">venijn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Oft Moly <rhyme label="a" type="f" corresp="#s5:l1">onberaden</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Om dese snoode <rhyme label="b" type="m" corresp="#s5:l2">pijn</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Wt mijn gemoet t' <rhyme label="a" type="f" corresp="#s5:l1">onladen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Geen wateren noch <rhyme label="a" type="f">cruyden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Connen blusschen de <rhyme label="b" type="m">Min</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Int Noorden noch het <rhyme label="a" type="f" corresp="#s6:l1">Suyden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Vint ghy daar toe <rhyme label="b" type="m" corresp="#s6:l2">gewin</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ghy die in mijnen <rhyme label="b" type="m" corresp="#s6:l2">sin</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Meesterssche zijt <rhyme label="a" type="f" corresp="#s6:l1">ghepresen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Gheeft ghy my drancken <rhyme label="b" type="m" corresp="#s6:l2">in</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Soo werd' ick stracx <rhyme label="a" type="f" corresp="#s6:l1">genesen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Menckt ghy u liepkens <rhyme label="a" type="f">roode</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Aan mijnen bleecken <rhyme label="b" type="m">mondt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Van dese quale <rhyme label="a" type="f" corresp="#s7:l1">snoode</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Werd' ick dan voort <rhyme label="b" type="m" corresp="#s7:l2">gesondt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Oft my alleene <rhyme label="b" type="m" corresp="#s7:l2">jondt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Te wesen uwen <rhyme label="a" type="f" corresp="#s7:l1">boode</rhyme></l>
        <l id="s7:l7" met="3" type="-+">End' suyvert mijne <rhyme label="b" type="m" corresp="#s7:l2">wondt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Eer my Cupido <rhyme label="a" type="f" corresp="#s7:l1">doode</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Al mijn geluck end' <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Wacht ick van u <rhyme label="b" type="m">verheugt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Want ick heb u <rhyme label="a" type="f" corresp="#s8:l1">gegeven</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Den sleutel van mijn <rhyme label="b" type="m" corresp="#s8:l2">jeugt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Ontfermet want ghy <rhyme label="b" type="m" corresp="#s8:l2">meugt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">V knecht God in <rhyme label="a" type="f" corresp="#s8:l1">verheeven</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Tis een onachtbaar <rhyme label="b" type="m" corresp="#s8:l2">deugt</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Te lanck gestoort <rhyme label="a" type="f" corresp="#s8:l1">gebleven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">VVie sal ick nu recht <rhyme label="a" type="f">clagen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Mijn passien end' <rhyme label="b" type="m">leet</rhyme></l>
        <l id="s9:l3" met="3" type="-+">VVie salick nu raat <rhyme label="a" type="f" corresp="#s9:l1">vragen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">VVaarlijck ick niemandt <rhyme label="b" type="m" corresp="#s9:l2">weet</rhyme></l>
        <l id="s9:l5" met="3" type="-+">VVant in mijn cortsen <rhyme label="b" type="m" corresp="#s9:l2">heet</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Neemt mijn Meesters <rhyme label="a" type="f" corresp="#s9:l1">behagen</rhyme></l>
        <l id="s9:l7" met="3" type="-+">De tranen die ick <rhyme label="b" type="m" corresp="#s9:l2">sweedt</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Connen haar niet <rhyme label="a" type="f" corresp="#s9:l1">versagen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Steyle Clippen end' <rhyme label="a" type="f">Bergen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Eenige bosschen <rhyme label="b" type="m">wijdt</rhyme></l>
        <l id="s10:l3" met="3" type="-+">In u wil ick my <rhyme label="a" type="f" corresp="#s10:l1">bergen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">End' eynden mijnen <rhyme label="b" type="m" corresp="#s10:l2">tijt</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Dat Cupido vol <rhyme label="b" type="m" corresp="#s10:l2">nijt</rhyme></l>
        <l id="s10:l6" met="3" type="-+">My niet meer come <rhyme label="a" type="f" corresp="#s10:l1">tergen</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Maar voor der liefden <rhyme label="b" type="m" corresp="#s10:l2">strijt</rhyme></l>
        <l id="s10:l8" met="3" type="-+">CaÌ„men sich bergen <rhyme label="a" type="f" corresp="#s10:l1">nergen</rhyme>,</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">VVaar salick my dan <rhyme label="a" type="f">wenden</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Om t' ontgaart desen <rhyme label="b" type="m">bast</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Mane wil tot my <rhyme label="a" type="f" corresp="#s11:l1">senden</rhyme></l>
        <l id="s11:l4" met="3" type="-+">V kerre snel <rhyme label="b" type="m" corresp="#s11:l2">gepast</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Eer my de liefd' <rhyme label="b" type="m" corresp="#s11:l2">verrast</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Wt sijne groene <rhyme label="a" type="f" corresp="#s11:l1">benden</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Voert my met desen <rhyme label="b" type="m" corresp="#s11:l2">last</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Elders aan swerelts <rhyme label="a" type="f" corresp="#s11:l1">enden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">End' wat soudt my dan <rhyme label="a" type="f">helpen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Soo verd te zijn, van <rhyme label="b" type="m">dy</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Soud' dat mijnen smerten <rhyme label="a" type="f" corresp="#s12:l1">stelpen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Oft dese <rhyme label="b" type="m" corresp="#s12:l2">raserny</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Neen, dit beken' ick <rhyme label="b" type="m" corresp="#s12:l2">vry</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Datick by u mijn <rhyme label="a" type="f" corresp="#s12:l1">schoone</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Moet wesen end' daar <rhyme label="b" type="m" corresp="#s12:l2">by</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Cryg' ick droef heyt ten <rhyme label="a" type="f" corresp="#s12:l1">loone</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Hoe salick het dan <rhyme label="a" type="f">maken</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Dewyl ick self mijn <rhyme label="b" type="m">plaach</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Naar tracht end' neem <rhyme label="a" type="f" corresp="#s13:l1">vermaken</rhyme></l>
        <l id="s13:l4" met="3" type="-+">In mijn sucht met <rhyme label="b" type="m" corresp="#s13:l2">geknaach</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Niet dan dat ick <rhyme label="b" type="m" corresp="#s13:l2">verdraach</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Der liefden durich <rhyme label="a" type="f" corresp="#s13:l1">blaken</rhyme></l>
        <l id="s13:l7" met="3" type="-+">End' hoop' dat alle <rhyme label="b" type="m" corresp="#s13:l2">daach</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Mijn droeffenis sal <rhyme label="a" type="f" corresp="#s13:l1">staaken</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Princersse jaa <rhyme label="a" type="f">Goodinne</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Door uwer schoonheyts <rhyme label="b" type="m">licht</rhyme></l>
        <l id="s14:l3" met="3" type="-+">De stof end' die <rhyme label="a" type="f" corresp="#s14:l1">voochdinne</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Van alle mijn <rhyme label="b" type="m" corresp="#s14:l2">gedicht</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Bestraalt door u <rhyme label="b" type="m" corresp="#s14:l2">gesicht</rhyme></l>
        <l id="s14:l6" met="3" type="-+">Mijn hert, te coel van <rhyme label="a" type="f" corresp="#s14:l1">sinne</rhyme></l>
        <l id="s14:l7" met="3" type="-+">Welck op dy heeft <rhyme label="b" type="m" corresp="#s14:l2">gesticht</rhyme></l>
        <l id="s14:l8" met="3" type="-+">Sijn yverige <rhyme label="a" type="f" corresp="#s14:l1">Minne</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
