    <melody>Schoon liefje jent zeer excellent</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Aegidius Haeffacker</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn hert ontwaeckt, Te loven <rhyme label="a" type="m">haeckt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Jesu Christi <rhyme label="b" type="f">Legaten</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Van hem geraeckt // Sy bloot en <rhyme label="a" type="m" corresp="#s1:l1">naect</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">De wereldt heel <rhyme label="a" type="m" corresp="#s1:l1">versaeckt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">De wereldt heel <rhyme label="a" type="m" corresp="#s1:l1">versaeckt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En al hebben <rhyme label="b" type="f" corresp="#s1:l2">verlaten</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Van hem geraeckt // Sy bloot en <rhyme label="a" type="m" corresp="#s1:l1">naect</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De wereldt heel <rhyme label="a" type="m" corresp="#s1:l1">versaeckt</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">De wereldt heel <rhyme label="a" type="m" corresp="#s1:l1">versaeckt</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En al hebben <rhyme label="b" type="f" corresp="#s1:l2">verlaten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Twalif ghetelt // Sy zijn <rhyme label="a" type="m">ghestelt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Teghen al Godts <rhyme label="b" type="f">vyanden</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Hebben ghequelt // En neer <rhyme label="a" type="m" corresp="#s2:l1">gheveldt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Des duyvels swaer <rhyme label="a" type="m" corresp="#s2:l1">geweldt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Des duyvels swaer <rhyme label="a" type="m" corresp="#s2:l1">geweldt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Verwonnen alle <rhyme label="b" type="f" corresp="#s2:l2">landen</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Hebben ghequelt // En neer <rhyme label="a" type="m" corresp="#s2:l1">gheveldt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Des duyvels swaer <rhyme label="a" type="m" corresp="#s2:l1">geweldt</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Des duyvels swaer <rhyme label="a" type="m" corresp="#s2:l1">geweldt</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Verwonnen alle <rhyme label="b" type="f" corresp="#s2:l2">landen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy zijn bereyt // Met goet <rhyme label="a" type="m">bescheyt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In alle landt <rhyme label="b" type="f">ghesonden</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Hebben, beschreyt // Haer saet <rhyme label="a" type="m" corresp="#s3:l1">gespreyt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Maer vrolijck we' er <rhyme label="a" type="m" corresp="#s3:l1">ghemeyt</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Maer vrolijck we' er <rhyme label="a" type="m" corresp="#s3:l1">ghemeyt</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Na dat sy zijn <rhyme label="b" type="f" corresp="#s3:l2">ontbonden</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Hebben, beschreyt // Haer saet <rhyme label="a" type="m" corresp="#s3:l1">gespreyt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Maer vrolijck we' er <rhyme label="a" type="m" corresp="#s3:l1">ghemeyt</rhyme>:</l>
        <l id="s3:l9" met="3" type="-+">Maer vrolijck we' er <rhyme label="a" type="m" corresp="#s3:l1">ghemeyt</rhyme>:</l>
        <l id="s3:l10" met="3" type="-+">Na dat sy zijn <rhyme label="b" type="f" corresp="#s3:l2">ontbonden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De ware leer // Van Godt den <rhyme label="a" type="m">Heer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sy hebben vast gaen <rhyme label="b" type="f">stichten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Die nimmermeer // Sou vallen we' <rhyme label="a" type="m" corresp="#s4:l1">er</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Maer wassen altijdt <rhyme label="a" type="m" corresp="#s4:l1">meer</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Maer wassen altijdt <rhyme label="a" type="m" corresp="#s4:l1">meer</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Teghen der Ketters <rhyme label="b" type="f" corresp="#s4:l2">dichten</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Die nimmermeer // Sou vallen we' <rhyme label="a" type="m" corresp="#s4:l1">er</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Maer wassen altijdt <rhyme label="a" type="m" corresp="#s4:l1">meer</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Maer wassen altijdt <rhyme label="a" type="m" corresp="#s4:l1">meer</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Teghen der Ketters <rhyme label="b" type="f" corresp="#s4:l2">dichten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sy zijn ge-eert // Met macht <rhyme label="a" type="m">vermeert</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Princen van 't <rhyme label="b" type="f">Hemel-rijcke</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Sy voeren 't sweert // Der zielen <rhyme label="a" type="m" corresp="#s5:l1">weert</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De menschen haer op d' <rhyme label="a" type="m" corresp="#s5:l1">eerd</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">De menschen haer op d' <rhyme label="a" type="m" corresp="#s5:l1">eerd</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Subject zijn al <rhyme label="b" type="f" corresp="#s5:l2">ghelijcke</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Sy voeren 't sweert // Der zielen <rhyme label="a" type="m" corresp="#s5:l1">weert</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">De menschen haer op d' <rhyme label="a" type="m" corresp="#s5:l1">eerd</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">De menschen haer op d' <rhyme label="a" type="m" corresp="#s5:l1">eerd</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Subject zijn al <rhyme label="b" type="f" corresp="#s5:l2">ghelijcke</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als Christus sal // Dit aerdsche <rhyme label="a" type="m">dal</rhyme></l>
        <l id="s6:l2" met="3" type="-+">In 't oordeel komen <rhyme label="b" type="f">loonen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Sullen sy al // In haer <rhyme label="a" type="m" corresp="#s6:l1">ghetal</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Over der menschen <rhyme label="a" type="m" corresp="#s6:l1">val</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Over der menschen <rhyme label="a" type="m" corresp="#s6:l1">val</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Sitten op Rechters <rhyme label="b" type="f" corresp="#s6:l2">throonen</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Sullen sy al // In haer <rhyme label="a" type="m" corresp="#s6:l1">ghetal</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Over der menschen <rhyme label="a" type="m" corresp="#s6:l1">val</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Over der menschen <rhyme label="a" type="m" corresp="#s6:l1">val</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Sitten op Rechters <rhyme label="b" type="f" corresp="#s6:l2">throonen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dus menschen net // En neerstigh <rhyme label="a" type="m">let</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Op hare leer en <rhyme label="b" type="f">leven</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Behout de wet // Door haer <rhyme label="a" type="m" corresp="#s7:l1">gheset</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Der Kett'ren leer <rhyme label="a" type="m" corresp="#s7:l1">verplet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Der Kett'ren leer <rhyme label="a" type="m" corresp="#s7:l1">verplet</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Soo sult ghy Godt <rhyme label="b" type="f" corresp="#s7:l2">aenkleven</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Behout de wet // Door haer <rhyme label="a" type="m" corresp="#s7:l1">gheset</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Der Kett'ren leer <rhyme label="a" type="m" corresp="#s7:l1">verplet</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Der Kett'ren leer <rhyme label="a" type="m" corresp="#s7:l1">verplet</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Soo sult ghy Godt <rhyme label="b" type="f" corresp="#s7:l2">aenkleven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
