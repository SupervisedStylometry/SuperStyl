<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185826</id>
    <date>
      <year-start>1605</year-start>
      <year-end>1605</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het viel een hemelse dauw</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Het viel een hemels <rhyme label="a" type="f">douwe</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Al in een cleyn <rhyme label="b" type="m">Maechdelijn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ten was noyt beter <rhyme label="a" type="f" corresp="#s1:l1">vrouwe</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat dede haer cleyn <rhyme label="b" type="m" corresp="#s1:l2">kindekijn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dat van haer was <rhyme label="c" type="f">gheboren</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Ende sy bleef Maghet <rhyme label="d" type="m">fijn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">O Maghet <rhyme label="c" type="f" corresp="#s1:l5">uytvercoren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Lof moet u altoos <rhyme label="d" type="m" corresp="#s1:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die Maghet ginck met <rhyme label="a" type="f">kinde</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gheen swaerheyt hinck haer <rhyme label="b" type="m">aen</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Als Ioseph dat <rhyme label="a" type="f" corresp="#s2:l1">versinde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die goede rechtveerdighe <rhyme label="b" type="m" corresp="#s2:l2">man</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hy dachte, ick wilse <rhyme label="c" type="f">laten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ick en ben de Vader <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ende trecken mijner <rhyme label="c" type="f" corresp="#s2:l5">straten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Eer my meer schanden <rhyme label="d" type="m" corresp="#s2:l6">gheschiet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Al van des Hemels <rhyme label="a" type="f">throone</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sprack hem den Engel <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ioseph Davids <rhyme label="a" type="f" corresp="#s3:l1">sone</rhyme></l>
        <l id="s3:l4" met="3" type="-+">O waerde salighe <rhyme label="b" type="m" corresp="#s3:l2">Man</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Blijft alle beyde te <rhyme label="c" type="f">gader</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Het is boven natueren <rhyme label="d" type="m">cracht</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat Godt almachtich <rhyme label="c" type="f" corresp="#s3:l5">Vader</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In haer dus heeft <rhyme label="d" type="m" corresp="#s3:l6">ghebracht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Corts daer nae is <rhyme label="a" type="f">ghecomen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Een keyserlijck <rhyme label="b" type="m">ghebodt</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Dat niemant uyt <rhyme label="a" type="f" corresp="#s4:l1">ghenomen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hy en quame sonder <rhyme label="b" type="m" corresp="#s4:l2">spot</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Al daer hy was <rhyme label="c" type="f">gheboren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ende brenghen zijn <rhyme label="d" type="m">tribuyt</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dat dede men daer <rhyme label="c" type="f" corresp="#s4:l5">hooren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">En roepen <rhyme label="d" type="m" corresp="#s4:l6">overluyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maria en Ioseph <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s5:l2" met="3" type="-+">quamen te Bethlehem <rhyme label="b" type="m">waert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Want daer was Iosephs <rhyme label="a" type="f" corresp="#s5:l1">stede</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Als ons die Schriftuer <rhyme label="b" type="m" corresp="#s5:l2">verclaert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Maer zy mochten nieuwers <rhyme label="c" type="f">inne</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Men wijs dese altoos <rhyme label="d" type="m">voort</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Die Hemelsche <rhyme label="c" type="f" corresp="#s5:l5">Coninginne</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En was daer niet <rhyme label="d" type="m" corresp="#s5:l6">ghehoort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Int veldt hebben zy <rhyme label="a" type="f">ghevonden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Een huys seer dinne <rhyme label="b" type="m">ghedaeckt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Binnen soo Corte <rhyme label="a" type="f" corresp="#s6:l1">stonden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hebben zy daer logijs <rhyme label="b" type="m" corresp="#s6:l2">gemaeckt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Daer wert die Maghet <rhyme label="c" type="f">Moeder</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Al sonder wee oft sonder <rhyme label="d" type="m">pijn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Dies tâ€™smenschen soon en <rhyme label="c" type="f" corresp="#s6:l5">Broeder</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Hoe mocht hy ons naerder <rhyme label="d" type="m" corresp="#s6:l6">zijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wt moederlijcker <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Leyde zy hem op haren <rhyme label="b" type="m">schoot</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Haer herte verblijden van <rhyme label="a" type="f" corresp="#s7:l1">binnen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dat dede zijn mondeken <rhyme label="b" type="m" corresp="#s7:l2">root</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Sy custen aen zijn <rhyme label="c" type="f">wanghen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Sy custen <rhyme label="d" type="m">menichfout</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat hy quam zijn <rhyme label="c" type="f" corresp="#s7:l5">ghevanghen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Verlossen ionck en <rhyme label="d" type="m" corresp="#s7:l6">out</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Maria suyver <rhyme label="a" type="f">Fonteyne</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Daer Godt zijn ruste in <rhyme label="b" type="m">nam</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Bidt voor ons al <rhyme label="a" type="f" corresp="#s8:l1">ghemeyne</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ende versoent dat Godlijck <rhyme label="b" type="m" corresp="#s8:l2">Lam</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Soo dat wy moghen <rhyme label="c" type="f">gheraken</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Met hem in dat soete <rhyme label="d" type="m">dal</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Daer vreucht is boven <rhyme label="c" type="f" corresp="#s8:l5">maten</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Die eewelijck dueren <rhyme label="d" type="m" corresp="#s8:l6">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
