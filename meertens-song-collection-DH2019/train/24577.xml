<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24577</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
      <tag standard-name="verhalend lied" translation="narrative song">verhalend lied</tag>
    </tags>
    <melody>Heer Jezus Christus van Nazarene</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="narratives">verhalen</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Alle enghelen van <rhyme label="a" type="f">hemelryke</rhyme></l>
        <l id="s1:l2" met="4" type="-+">ende alle menschen op <rhyme label="a" type="f" corresp="#s1:l1">eertryke</rhyme></l>
        <l id="s1:l3" met="4" type="-+">verblyden hem alle tot eenre <rhyme label="b" type="m">tyt</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s1:l4">alleyne</rhyme></l>
        <l id="s1:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s1:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Alle cleyne kynder van tween <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s2:l2" met="4" type="-+">die dede herodes sueken daer sy <rhyme label="a" type="f" corresp="#s2:l1">waren</rhyme></l>
        <l id="s2:l3" met="4" type="-+">ende hi dede hem alle nemen dat <rhyme label="b" type="m">lyf</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s2:l4">alleyne</rhyme></l>
        <l id="s2:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s2:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Alsoe vroe als josep dat <rhyme label="a" type="f">verhoerden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">hi sprack tot onse vrouwen dese <rhyme label="a" type="f" corresp="#s3:l1">woerde</rhyme></l>
        <l id="s3:l3" met="4" type="-+">laet ons gaen vlien tys meer dan <rhyme label="b" type="m">tyt</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s3:l4">alleyne</rhyme></l>
        <l id="s3:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s3:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Alse sy een halue myle waeren <rhyme label="a" type="f">gevaren</rhyme></l>
        <l id="s4:l2" met="4" type="-+">daer saghen sy enen man staen <rhyme label="a" type="f" corresp="#s4:l1">saeyen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">sy groten hem ende leden voer <rhyme label="b" type="m">by</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s4:l4">alleyne</rhyme></l>
        <l id="s4:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s4:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Nu hoert guet man die dit  coernken <rhyme label="a" type="f">seyet</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Ist dat daer yemant nae ons <rhyme label="a" type="f" corresp="#s5:l1">vraget</rhyme></l>
        <l id="s5:l3" met="4" type="-+">soe segt wy voeren hier hen doe gi dit <rhyme label="b" type="m">seyden</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s5:l4">alleyne</rhyme></l>
        <l id="s5:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s5:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Als herodes dat <rhyme label="a" type="f">verhoerden</rhyme></l>
        <l id="s6:l2" met="4" type="-+">hi sprack tot synen knechten dese <rhyme label="a" type="f" corresp="#s6:l1">woerde</rhyme></l>
        <l id="s6:l3" met="4" type="-+">laet ons gaen sueken dat hilige <rhyme label="b" type="m">wyf</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s6:l4">alleyne</rhyme></l>
        <l id="s6:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s6:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Als sy een halue myle waren <rhyme label="a" type="f">geuaeren</rhyme></l>
        <l id="s7:l2" met="4" type="-+">daer sagen sy enen man staen <rhyme label="a" type="f" corresp="#s7:l1">meyen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">sy grueten hem ende worden <rhyme label="b" type="m">verblyt</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s7:l4">alleyne</rhyme></l>
        <l id="s7:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s7:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Nv segt ons man die dit coernken <rhyme label="a" type="f">meyet</rhyme></l>
        <l id="s8:l2" met="4" type="-+">saegdi hier niet een joncfrouwe voerby <rhyme label="a" type="f" corresp="#s8:l1">vaeren</rhyme></l>
        <l id="s8:l3" met="4" type="-+">sy was gecledet imt witten <rhyme label="b" type="m">habyt</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s8:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s8:l4">alleyne</rhyme></l>
        <l id="s8:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s8:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Doe ic dat seyden dat ic nv <rhyme label="a" type="f">meye</rhyme></l>
        <l id="s9:l2" met="4" type="-+">doe voer hier ene joncfrouwe voer <rhyme label="a" type="f" corresp="#s9:l1">by</rhyme></l>
        <l id="s9:l3" met="4" type="-+">sy was ghecledet imt witten <rhyme label="b" type="m">habyt</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">nasarenen</rhyme></l>
        <l id="s9:l5" met="4" type="-+">ghy  syt waer mensche ende god <rhyme label="c" type="f" corresp="#s9:l4">alleyne</rhyme></l>
        <l id="s9:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s9:l3">gebenedyt</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Herodes sprack tot synen <rhyme label="a" type="f">schare</rhyme></l>
        <l id="s10:l2" met="4" type="-+">wal op laet ons toe huys wert <rhyme label="a" type="f" corresp="#s10:l1">varen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">tys al om niet dat wy hier <rhyme label="b" type="m">syn</rhyme></l>
        <l id="s10:l4" met="4" type="+-">Jhesus christus van <rhyme label="c" type="f">naseren</rhyme></l>
        <l id="s10:l5" met="4" type="-+">gy syt waer mensche ende god <rhyme label="c" type="f" corresp="#s10:l4">alleene</rhyme></l>
        <l id="s10:l6" met="4" type="-+">daer om sy di <rhyme label="b" type="m" corresp="#s10:l3">gebenedyt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
