  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Waeyer die de het te <rhyme label="a" type="m">weert</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die myn schoone Kloris <rhyme label="a" type="m" corresp="#s1:l1">deert</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">Ei ontvonk veel eer de <rhyme label="b" type="f">vonkjes</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Die zy heeft in haer <rhyme label="c" type="m">gemoedt</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Want de hette van haer <rhyme label="b" type="f" corresp="#s1:l3">lonkjes</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Toont my dat zy vlammen <rhyme label="c" type="m" corresp="#s1:l4">voedt</rhyme>.</l>
      </lg>
    </lg>
  </text>
