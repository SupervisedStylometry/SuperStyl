    <melody>Ik klaag u Venus' dieren</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="emotions">emoties</category>
    </categories>
  <author>G.A. Bredero</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Seght my, seght my <rhyme label="a" type="f">Vriendinne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Waarom verbercht ghy <rhyme label="b" type="m">u</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Om dat ick u <rhyme label="a" type="f" corresp="#s1:l1">beminne</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Syt ghy daarom soo <rhyme label="b" type="m" corresp="#s1:l2">schuu</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Het schijnt ghy crycht een <rhyme label="b" type="m" corresp="#s1:l2">gruu</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Als ghy my siet <rhyme label="c" type="f">aencoomen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Laat my toch weeten <rhyme label="b" type="m" corresp="#s1:l2">nu</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wat u voor my doet <rhyme label="c" type="f" corresp="#s1:l6">schroomen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Heb ick buyten <rhyme label="a" type="f">t'behooren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Teghen u sin <rhyme label="b" type="m">ghedaen</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Dat u verweckt tot <rhyme label="a" type="f" corresp="#s2:l1">tooren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ay doet my dat <rhyme label="b" type="m" corresp="#s2:l2">verstaan</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Een valsche Tongh of <rhyme label="b" type="m" corresp="#s2:l2">waan</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Kan somtijts veel quaats <rhyme label="c" type="f">wercken</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Daarom wilt u <rhyme label="b" type="m" corresp="#s2:l2">beraan</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En op de waarheydt <rhyme label="c" type="f" corresp="#s2:l6">mercken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Is dit dat ghy <rhyme label="a" type="f">bekende</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Doe ghy my hebt <rhyme label="b" type="m">gheseyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat nimmermeer sou <rhyme label="a" type="f" corresp="#s3:l1">enden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">U trouwe <rhyme label="b" type="m" corresp="#s3:l2">sinlijckheyt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die ghy hadt doen <rhyme label="b" type="m" corresp="#s3:l2">gheleyt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Op my maar t'syn maar <rhyme label="c" type="f">vlaghen</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Want die lichtvaardich <rhyme label="b" type="m" corresp="#s3:l2">scheyt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Wat Liefde can die <rhyme label="c" type="f" corresp="#s3:l6">draaghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Immers wist ghy te <rhyme label="a" type="f">vooren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Hoe hooch dat ick u <rhyme label="b" type="m">hiel</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Voor tgroost by my <rhyme label="a" type="f" corresp="#s4:l1">vercooren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Jae als mijn hart mijn <rhyme label="b" type="m" corresp="#s4:l2">Ziel</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Och oft noch soo <rhyme label="b" type="m" corresp="#s4:l2">gheviel</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat ghy u eens wout <rhyme label="c" type="f">wenden</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Om my die voor u <rhyme label="b" type="m" corresp="#s4:l2">kniel</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Te troosten in <rhyme label="c" type="f" corresp="#s4:l6">d'ellenden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ist u Lief al <rhyme label="a" type="f">vergheeten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat ghy in mijn <rhyme label="b" type="m">ghedacht</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Mijn werelt plach te <rhyme label="a" type="f" corresp="#s5:l1">heeten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Mijn ick, mijn al, mijn <rhyme label="b" type="m" corresp="#s5:l2">cracht</rhyme></l>
        <l id="s5:l5" met="3" type="-+">O goude Mensch <rhyme label="b" type="m" corresp="#s5:l2">gheacht</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In myne jonghe <rhyme label="c" type="f">sinnen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Hoe onghesien <rhyme label="b" type="m" corresp="#s5:l2">verwacht</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Ick noch u jonst te <rhyme label="c" type="f" corresp="#s5:l6">winnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Soo langh sal ick u <rhyme label="a" type="f">dienen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Met eerbaarheyt en <rhyme label="b" type="m">schaamt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Als Godt my sal <rhyme label="a" type="f" corresp="#s6:l1">verlienen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">t'Leven dat in my <rhyme label="b" type="m" corresp="#s6:l2">aamt</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Och of ghy selfs <rhyme label="b" type="m" corresp="#s6:l2">quaamt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">U Dienaars smart <rhyme label="c" type="f">aanschouwen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Ghy sout werden <rhyme label="b" type="m" corresp="#s6:l2">ghepraamt</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Met deerenis mijn <rhyme label="c" type="f" corresp="#s6:l6">Vrouwe</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maar als my quam <rhyme label="a" type="f">bestraalen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Het vlick'ren van u <rhyme label="b" type="m">ooch</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sou dan mijn kracht <rhyme label="a" type="f" corresp="#s7:l1">verhaalen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Alleen door u <rhyme label="b" type="m" corresp="#s7:l2">vertoogh</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Al comt my laat en <rhyme label="b" type="m" corresp="#s7:l2">vrooch</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">U beelt in mijn <rhyme label="c" type="f">ghedachten</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ick kan de schyn soo <rhyme label="b" type="m" corresp="#s7:l2">hooch</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Niet als het weesen <rhyme label="c" type="f" corresp="#s7:l6">achten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Princesje Lief <rhyme label="a" type="f">verheeven</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Doot u verborghen <rhyme label="b" type="m">haat</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En laat de Liefde <rhyme label="a" type="f" corresp="#s8:l1">leeven</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Weer in syn eersten <rhyme label="b" type="m" corresp="#s8:l2">staat</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">U rijpelijck <rhyme label="b" type="m" corresp="#s8:l2">beraat</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En wilt gheen waan <rhyme label="c" type="f">ghelooven</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Noch snoode clappers <rhyme label="b" type="m" corresp="#s8:l2">quaat</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Die d'eere van my <rhyme label="c" type="f" corresp="#s8:l6">rooven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
