<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2514</id>
    <date>
      <year-start>1622</year-start>
      <year-end>1622</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Sei tanto graziosa</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>G.A. Bredero</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ghy drye-mael dry <rhyme label="a" type="f">Godinnen</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Apollos bloed, Mercuri naeste <rhyme label="b" type="f">Nichten</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Schrandert en spitst mijn <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Heldert mijn breyn, en leert my deftigh <rhyme label="b" type="f" corresp="#s1:l2">dichten</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Hoe <rhyme label="c" type="m">wonderlijck</rhyme></l>
        <l id="s1:l6" met="2" type="-+">De <rhyme label="c" type="m" corresp="#s1:l5">Redenrijck</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Met Pallas is <rhyme label="d" type="f">geboren</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Doe Vulcaan <rhyme label="e" type="f">kloofde</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Iupijns bevruchten <rhyme label="e" type="f" corresp="#s1:l8">hoofde</rhyme></l>
        <l id="s1:l10" met="2" type="+-">Hier te <rhyme label="d" type="f" corresp="#s1:l7">voren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als dit de Rymers <rhyme label="a" type="f">singhen</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">En gheestigh hier van <rhyme label="b" type="f">vernufteliseren</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Men achtet dolle <rhyme label="a" type="f" corresp="#s2:l1">dinghen</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">'t Belul-loos volck weet van geen <rhyme label="b" type="f" corresp="#s2:l2">redeneren</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Noch darmen <rhyme label="c" type="m">kan</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Wt d'herssen <rhyme label="c" type="m" corresp="#s2:l5">pan</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Veel puyck van lessen <rhyme label="d" type="f">smeden</rhyme>:</l>
        <l id="s2:l8" met="2" type="-+">De suyv're <rhyme label="e" type="f">breynen</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Sijn kloecke <rhyme label="e" type="f" corresp="#s2:l8">kond-fonteynen</rhyme></l>
        <l id="s2:l10" met="2" type="+-">Vande <rhyme label="d" type="f" corresp="#s2:l7">reden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dit was 't geluyt der <rhyme label="a" type="f">snaren</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Van Orpheus Lier, die stillen kon de <rhyme label="b" type="f">stromen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dien oock wist te <rhyme label="a" type="f" corresp="#s3:l1">vergaren</rhyme></l>
        <l id="s3:l4" met="5" type="-+">De beesten bot, ja beemden, berghen, <rhyme label="b" type="f" corresp="#s3:l2">bomen</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">De <rhyme label="c" type="m">reden-konst</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Vind kracht en <rhyme label="c" type="m" corresp="#s3:l5">gonst</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Om Landen te <rhyme label="d" type="f">versetten</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Sy kan de <rhyme label="e" type="f">lieden</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Hoe wilt, hoe woest, <rhyme label="e" type="f" corresp="#s3:l8">gebieden</rhyme></l>
        <l id="s3:l10" met="2" type="+-">Met haer <rhyme label="d" type="f" corresp="#s3:l7">Wetten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Scepters en <rhyme label="a" type="f">heerschappye</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Vonnis onthiet, en 's werelts grootschen <rhyme label="b" type="f">handel</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Die gnocken haer <rhyme label="a" type="f" corresp="#s4:l1">vooghdye</rhyme></l>
        <l id="s4:l4" met="5" type="-+">Van u wijsheyd, hooghe Princes <rhyme label="b" type="f" corresp="#s4:l2">verstandel</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Ghy weet de <rhyme label="c" type="m">schijn</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Van 't ware <rhyme label="c" type="m" corresp="#s4:l5">zijn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Te schiften, en te <rhyme label="d" type="f">scheyen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">En door u <rhyme label="e" type="f">tolcken</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Kund ghy verweende <rhyme label="e" type="f" corresp="#s4:l8">volcken</rhyme></l>
        <l id="s4:l10" met="2" type="+-">Te recht <rhyme label="d" type="f" corresp="#s4:l7">leyen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Lof Reden-rijcke <rhyme label="a" type="f">konste</rhyme>!</l>
        <l id="s5:l2" met="5" type="-+">Lof weet-Godin, van wien de Rymen <rhyme label="b" type="f">vloeyen</rhyme>!</l>
        <l id="s5:l3" met="3" type="-+">Ghy baert 't leven der <rhyme label="a" type="f" corresp="#s5:l1">jonsten</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Ghy stiert tot deught, en doet in liefde <rhyme label="b" type="f" corresp="#s5:l2">bloeyen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Ghy leerd met <rhyme label="c" type="m">vreught</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Ghy sticht de <rhyme label="c" type="m" corresp="#s5:l5">Jeught</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ghy blinckt van glans op <rhyme label="d" type="f">glanssen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Dus wy <rhyme label="e" type="f">vercieren</rhyme></l>
        <l id="s5:l9" met="3" type="-+">U hooft met <rhyme label="e" type="f" corresp="#s5:l8">Lauwerieren</rhyme></l>
        <l id="s5:l10" met="2" type="+-">Varsche <rhyme label="d" type="f" corresp="#s5:l7">kranssen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
