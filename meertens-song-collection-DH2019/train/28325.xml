        <l id="s1:l1" met="3" type="-+">Mon keur &amp; eun <rhyme label="a" type="f">brunette</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ick stoor mijn aen geen <rhyme label="b" type="m">swart</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Maer haet het schoon <rhyme label="a" type="f" corresp="#s1:l1">blankette</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Daer yeder geck om <rhyme label="b" type="m" corresp="#s1:l2">wart</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat bruyn is sal niet <rhyme label="a" type="f" corresp="#s1:l1">smette</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Croy√©, ma mie, mijn <rhyme label="b" type="m" corresp="#s1:l2">hart</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Temoyn dat blanch veel eerder <rhyme label="c" type="m">schaet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dan het de schoonheydt <rhyme label="c" type="m" corresp="#s1:l7">baet</rhyme>.</l>
