  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GEestighe Phillida lieve <rhyme label="a" type="f">Matressie</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Loof jy die jabeltjes die jou het <rhyme label="a" type="f" corresp="#s1:l1">Bessie</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Onder de Linde-boom maeckte <rhyme label="b" type="m">wijs</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Soetert de totebel schijnt soo <rhyme label="b" type="m" corresp="#s1:l3">vijs</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Sy remelt steets die mint is <rhyme label="c" type="m">dom</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">En t' ruwe vel vergaet 'er <rhyme label="c" type="m" corresp="#s1:l5">om</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die sy u aldermeest teghen wil <rhyme label="a" type="f">maken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Looft vry dat daer al haer sinnen naer <rhyme label="a" type="f" corresp="#s2:l1">haken</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Schelt sy ons Herders niet lelijck <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Want sy waer garen met een de <rhyme label="b" type="m" corresp="#s2:l3">Bruyt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Merckt, die de waer laeckt koopse <rhyme label="c" type="m">eerst</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Of die 't veracht bemint het <rhyme label="c" type="m" corresp="#s2:l5">seerst</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Heeft u 't gerucht noch in 't oor niet <rhyme label="a" type="f">gekloncken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Hoe sy aen Thirsis haer vlammende <rhyme label="a" type="f" corresp="#s3:l1">voncken</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Onder een heuveltje stelde <rhyme label="b" type="m">voor</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Met groote lust doch kleyn <rhyme label="b" type="m" corresp="#s3:l3">ghehoor</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Van Thirses' die voor d' ouwe <rhyme label="c" type="m">queen</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Met Phillida het waer te <rhyme label="c" type="m" corresp="#s3:l5">vreen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Stervende salse noch schreyent <rhyme label="a" type="f">beklaghe</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dat sy haer maeghdom in d' aerde moet <rhyme label="a" type="f" corresp="#s4:l1">draghe</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Sonder van yemant te sijnen <rhyme label="b" type="m">gelieft</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Had haer een goet gesel eens <rhyme label="b" type="m" corresp="#s4:l3">gerieft</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Soo waer haer ouwe hart <rhyme label="c" type="m">gerust</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Welck nu verdwijnt om geyle <rhyme label="c" type="m" corresp="#s4:l5">lust</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Laetse dan varen die vijse <rhyme label="a" type="f">klappye</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">En kom begeeft u met ons aen de <rhyme label="a" type="f" corresp="#s5:l1">reye</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Daer gy de jeughdighe herders sult <rhyme label="b" type="m">zien</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Yder om 't vlytighst u gunste <rhyme label="b" type="m" corresp="#s5:l3">bien</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Want ouwe kouwe queene <rhyme label="c" type="m">vreught</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Doet selve jonghe luyde <rhyme label="c" type="m" corresp="#s5:l5">deught</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
