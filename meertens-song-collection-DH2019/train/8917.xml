      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O HEEr gedenckt aen David <rhyme label="a" type="m">Jent</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Aen all' syn Moeyt en groot <rhyme label="a" type="m" corresp="#s1:l1">Elent</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Want hy swoer Godt den <rhyme label="b" type="f">HEERE</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Godt Jacobs Looffd' hy: Ick wil <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Gaen in mijn Huys, noch Liggen <rhyme label="c" type="m" corresp="#s1:l4">(siet</rhyme>)</l>
        <l id="s1:l6" met="3" type="-+">Op mijn Bedd' Legher <rhyme label="b" type="f" corresp="#s1:l3">neere</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Ick laet niet slapen d' Ooghen <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Noch sluym'rich mijn Oog'scheelen <rhyme label="d" type="m" corresp="#s1:l7">zijn</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Tot ick een Woonsteed' <rhyme label="e" type="f">vinde</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Voor Jacobs GOdt, daer van dat <rhyme label="f" type="m">wy</rhyme></l>
        <l id="s1:l11" met="4" type="+-">In Ephrata aff hoorden <rhyme label="f" type="m" corresp="#s1:l10">bly</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">End vondens' in <rhyme label="e" type="f" corresp="#s1:l9">Woestijne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wy willen in syn Wooningh' <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Hem voor syn Voetbanck bidden <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">HEER maeckt dy t'uwer <rhyme label="b" type="f">Ruste</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Met d'Arck dijns machts, der Priester <rhyme label="c" type="m">Cleet</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Laet syn alleen <rhyme label="c" type="m" corresp="#s2:l4">Gerechticheet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dijn Heyl'gen bly met <rhyme label="b" type="f" corresp="#s2:l3">Luste</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">En neemt om David dijnen <rhyme label="d" type="m">Knecht</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Het Regiment dijns Christ niet <rhyme label="d" type="m" corresp="#s2:l7">wech</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Godt heeft een Eedt <rhyme label="e" type="f">ghesworen</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Den David, dien hy houden <rhyme label="f" type="m">sal</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Op dijn Stoel sal ick setten <rhyme label="f" type="m" corresp="#s2:l10">al</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Dijns Lijves Vrucht <rhyme label="e" type="f" corresp="#s2:l9">vercooren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ist dat dijn Kindren mijn <rhyme label="a" type="m">Verbondt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">End mijn Woort houden t'Alder <rhyme label="a" type="m" corresp="#s3:l1">stondt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dwelck ick haer recht sal <rhyme label="b" type="f">Leeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">So sullen hare Kindren <rhyme label="c" type="m">meed'</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Op dijnen Stoel in <rhyme label="c" type="m" corresp="#s3:l4">Eewicheedt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Oock sitten end <rhyme label="b" type="f" corresp="#s3:l3">floreeren</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">De HEER heeft Zyon gants end' <rhyme label="d" type="m">gaer</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Vercoor'n, hem lust te Woonen <rhyme label="d" type="m" corresp="#s3:l7">daer</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Want dit is (spreeckt de <rhyme label="e" type="f">HEERE</rhyme>)</l>
        <l id="s3:l10" met="4" type="-+">Mijn Rustplaets end mijn lust daer <rhyme label="f" type="m">ick</rhyme></l>
        <l id="s3:l11" met="4" type="+-">Wil syn end Woonen <rhyme label="f" type="m" corresp="#s3:l10">Eewelick</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Want het Behaecht my <rhyme label="e" type="f" corresp="#s3:l9">seere</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ick wil haer spijse seeg'nen <rhyme label="a" type="m">daer</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En Broots ghenoech oock geven <rhyme label="a" type="m" corresp="#s4:l1">haer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ick wil met Heyl <rhyme label="b" type="f">becleeden</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Haer Priesters, end haer Heyl'gen <rhyme label="c" type="m">fijn</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Die sullen al te samen <rhyme label="c" type="m" corresp="#s4:l4">zijn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In groote <rhyme label="b" type="f" corresp="#s4:l3">Vrolickheeden</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Aldaer sal opgaen Davids <rhyme label="d" type="m">Hoor'n</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Ick heb bereyt mijn Christ <rhyme label="d" type="m" corresp="#s4:l7">vercoor'n</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Een claer Lanteern al <rhyme label="e" type="f">schoone</rhyme>.</l>
        <l id="s4:l10" met="4" type="-+">Syn Vyanden wil ick <rhyme label="f" type="m">becleen</rhyme></l>
        <l id="s4:l11" met="4" type="+-">Met schandt: maer over hem seer <rhyme label="f" type="m" corresp="#s4:l10">Reen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Sal Bloeyen syne <rhyme label="e" type="f" corresp="#s4:l9">Croone</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
