<song>
  <meta>
    <id>187853</id>
    <date>
      <year-start>1669</year-start>
      <year-end>1669</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Brave juffers fiere maagden</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KOm nu Vryers, kom nu <rhyme label="a" type="f">Vrysters</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Kom, hoe elk u noemen <rhyme label="b" type="m">magh</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Kom nu Vossen, kom nu <rhyme label="a" type="f" corresp="#s1:l1">Lysters</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Kom nu vry maar aan den <rhyme label="b" type="m" corresp="#s1:l2">dagh</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Kom elkander <rhyme label="c" type="f">onderschraagen</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Trek nu saamen eene <rhyme label="d" type="m">lijn</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">D'een sal d'ander niet <rhyme label="c" type="f" corresp="#s1:l5">beklaagen</rhyme>:</l>
        <l id="s1:l8" met="4" type="+-">Kom ik sal uw rechter <rhyme label="d" type="m" corresp="#s1:l6">sijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Aapen sullen dan wel <rhyme label="a" type="f">luysen</rhyme>.</l>
        <l id="s2:l2" met="4" type="+-">Vossen krijgen dan een <rhyme label="b" type="m">gat</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Katten sullen leeren <rhyme label="a" type="f" corresp="#s2:l1">muysen</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Huysen raakken aan een <rhyme label="b" type="m" corresp="#s2:l2">kat</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Haasen swemmen dan in <rhyme label="c" type="f">soussen</rhyme>.</l>
        <l id="s2:l6" met="4" type="+-">Oorlogh word verkeert in <rhyme label="d" type="m">vreê</rhyme>.</l>
        <l id="s2:l7" met="4" type="+-">Beenen pronkken met de <rhyme label="c" type="f" corresp="#s2:l5">koussen</rhyme>.</l>
        <l id="s2:l8" met="4" type="+-">Messen steektmen in de <rhyme label="d" type="m" corresp="#s2:l6">scheê</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Paarden krijgen <rhyme label="a" type="f">Hengste-speenen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Cyters krijgen toon en <rhyme label="b" type="m">quint</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Koussen trektmen aan de <rhyme label="a" type="f" corresp="#s3:l1">beenen</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Wiegen vultmen met een <rhyme label="b" type="m" corresp="#s3:l2">kind</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Koeyen sullen niet meer <rhyme label="c" type="f">loeyen</rhyme>.</l>
        <l id="s3:l6" met="4" type="+-">Kaarens geeven room en <rhyme label="d" type="m">huy</rhyme>.</l>
        <l id="s3:l7" met="4" type="+-">Landen sal een ploegh <rhyme label="c" type="f" corresp="#s3:l5">vermoeyen</rhyme>.</l>
        <l id="s3:l8" met="4" type="+-">Klapper-werkken worden <rhyme label="d" type="m" corresp="#s3:l6">luy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ruyters raakken dan aan <rhyme label="a" type="f">paarden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Officieren aan een <rhyme label="b" type="m">lans</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Bokken aan de klaauw en <rhyme label="a" type="f" corresp="#s4:l1">baarden</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Kerkkemissen aan een <rhyme label="b" type="m" corresp="#s4:l2">dans</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Kerkken draagen dan haar <rhyme label="c" type="f">toorens</rhyme>.</l>
        <l id="s4:l6" met="4" type="+-">Lijfland krijgt een <rhyme label="d" type="m">guarnisoen</rhyme>.</l>
        <l id="s4:l7" met="4" type="+-">Hinden pochchen op haar <rhyme label="c" type="f" corresp="#s4:l5">hoorens</rhyme>.</l>
        <l id="s4:l8" met="4" type="+-">Hoenders sullen eyers <rhyme label="d" type="m" corresp="#s4:l6">broên</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Haanen treeden Hen en <rhyme label="a" type="f">Kiekken</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Nachtegaalen soekken 't <rhyme label="b" type="m">bos</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Doctors helpen dan de <rhyme label="a" type="f" corresp="#s5:l1">siekken</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Water krijgt dan baars en <rhyme label="b" type="m" corresp="#s5:l2">pos</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Keegel-perkken geeven <rhyme label="c" type="f">keegels</rhyme>.</l>
        <l id="s5:l6" met="4" type="+-">Rammen loopen met de <rhyme label="d" type="m">Bel</rhyme>.</l>
        <l id="s5:l7" met="4" type="+-">Maayers swaayen dan de <rhyme label="c" type="f" corresp="#s5:l5">fleegels</rhyme>.</l>
        <l id="s5:l8" met="4" type="+-">Duyvels raakken aan een <rhyme label="d" type="m" corresp="#s5:l6">Hel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Weergunst doet dan gunst <rhyme label="a" type="f">beklyven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En de Bok krijgt smaak in 't <rhyme label="b" type="m">Sout</rhyme>.</l>
        <l id="s6:l3" met="4" type="+-">Niemant sal dan Vrijster <rhyme label="a" type="f" corresp="#s6:l1">blyven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Noch de Vryers <rhyme label="b" type="m" corresp="#s6:l2">ongetrouwt</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Stuur dan niet na vreemde <rhyme label="c" type="f">oorden</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Nu dit goed hier nodigh <rhyme label="d" type="m">is</rhyme>.</l>
        <l id="s6:l7" met="4" type="+-">Holland word het ruyge <rhyme label="c" type="f" corresp="#s6:l5">Noorden</rhyme>.</l>
        <l id="s6:l8" met="4" type="+-">Holland word een Keulse <rhyme label="d" type="m" corresp="#s6:l6">Mis</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
