  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ach wollig vee! ach wollig <rhyme label="a" type="m">vee</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Hoe liefderyk vind ik uwe <rhyme label="b" type="f">weiden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Deez' zilver kruin der <rhyme label="a" type="m" corresp="#s1:l1">Alpiné</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Schenk my veel vreugde en <rhyme label="b" type="f" corresp="#s1:l2">verblyden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Myn starend oog, myn starend <rhyme label="c" type="m">oog</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Blikt naar u dart'lend <rhyme label="d" type="f">grazen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Daar my omhoog aan 's hemels <rhyme label="c" type="m" corresp="#s1:l5">boog</rhyme>;</l>
        <l id="s1:l8" met="3" type="-+">Een sneeuw klomp doet <rhyme label="d" type="f" corresp="#s1:l6">verbazen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Hoe meenigmaal, hoe <rhyme label="a" type="m">meenigmaal</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Komt my de zucht in u te <rhyme label="b" type="f">streelen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Als myn natuur stelt perk nog <rhyme label="a" type="m" corresp="#s2:l1">paal</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Om met u vrolyk rond te <rhyme label="b" type="f" corresp="#s2:l2">speelen</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">O dan wanneer een sneeuw <rhyme label="c" type="m">kolom</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Bedekt u schoone <rhyme label="d" type="f">velden</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Zien ik met verbaasheid van <rhyme label="c" type="m" corresp="#s2:l5">rondom</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Wyl steeds u klagt my <rhyme label="d" type="f" corresp="#s2:l6">kwelden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ach zoete vreugd u hulp en <rhyme label="a" type="m">vlyt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zien ik steeds in het oosten <rhyme label="b" type="f">kimmen</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Gelyk de zon in 't <rhyme label="a" type="m" corresp="#s3:l1">morgenrood</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ver boven de Alpen <rhyme label="b" type="f" corresp="#s3:l2">glimmen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Hy nadert reeds zyn zachte <rhyme label="c" type="m">lonk</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Blyft eed en pligt <rhyme label="d" type="f">bewaken</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Waar deugd en eer zeer klaar <rhyme label="c" type="m" corresp="#s3:l5">ontvonk</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Om zyn wensch te <rhyme label="d" type="f" corresp="#s3:l6">genaken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy heeft zyn woord hetwelk hy <rhyme label="a" type="m">gaf</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daar ginds in die <rhyme label="b" type="f">valeijen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gehouden dus niets als het <rhyme label="a" type="m" corresp="#s4:l1">graf</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Kan ons van elkander <rhyme label="b" type="f" corresp="#s4:l2">scheiden</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">Kom spoeden wy thans met <rhyme label="c" type="m">malkaar</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Naar myne vaders <rhyme label="d" type="f">woning</rhyme>;</l>
        <l id="s4:l7" met="4" type="-+">Zoo zyn wy spoedig soms een <rhyme label="c" type="m" corresp="#s4:l5">paar</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">De deugd is uw <rhyme label="d" type="f" corresp="#s4:l6">belooning</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ach wollig vee, ach wollig <rhyme label="a" type="m">vee</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">U voedster gaat nu <rhyme label="b" type="f">scheiden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Geniet de vreugde der <rhyme label="a" type="m" corresp="#s5:l1">Alpiné</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wilt u met elkaar <rhyme label="b" type="f" corresp="#s5:l2">verblyden</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Adieu valei en berg en <rhyme label="c" type="m">dal</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat myn vaarwel u <rhyme label="d" type="f">streelen</rhyme>;</l>
        <l id="s5:l7" met="4" type="-+">En de Echgo door het groot <rhyme label="c" type="m" corresp="#s5:l5">heelal</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Myn afscheidsgroet moog <rhyme label="d" type="f" corresp="#s5:l6">speelen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maar als de spys door myne <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">U voed in 't stemmig <rhyme label="b" type="f">loeijen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Daar berg en dal myn vaders <rhyme label="a" type="m" corresp="#s6:l1">hand</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Door 's Hemels gunst laat <rhyme label="b" type="f" corresp="#s6:l2">gloeijen</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Dan zing ik bly in 't <rhyme label="c" type="m">snarenspel</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Weergalm langs groene <rhyme label="d" type="f">weiden</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Daar de Echgo door het reinst <rhyme label="c" type="m" corresp="#s6:l5">bevel</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Beantwoord van ter <rhyme label="d" type="f" corresp="#s6:l6">zeyden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">En waarom heeft myn vaders <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">My aan deez' oord <rhyme label="b" type="f">verbonden</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Schoon dat de zucht in een ander <rhyme label="a" type="m" corresp="#s7:l1">land</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Myn hart reeds heeft <rhyme label="b" type="f" corresp="#s7:l2">verslonden</rhyme>;</l>
        <l id="s7:l5" met="4" type="-+">Zal ik getrouw lief wollig <rhyme label="c" type="m">vee</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Steeds aan u afzyn <rhyme label="d" type="f">denken</rhyme>;</l>
        <l id="s7:l7" met="4" type="-+">Waar ook myn lot en liefde in <rhyme label="c" type="m" corresp="#s7:l5">vree</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">De reinste vreugd moog <rhyme label="d" type="f" corresp="#s7:l6">schenken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maar stil, ik hoor een krygs <rhyme label="a" type="m">rumoer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Veel ligt is hy thans ten <rhyme label="b" type="f">stryden</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ach dat toch eenmaal zyn <rhyme label="a" type="m" corresp="#s8:l1">retoer</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Zyn weerkomst my <rhyme label="b" type="f" corresp="#s8:l2">verblyden</rhyme>;</l>
        <l id="s8:l5" met="4" type="-+">Dan weet ik dat een zachte <rhyme label="c" type="m">druk</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Myn hart en hand doet <rhyme label="d" type="f">gloeijen</rhyme>;</l>
        <l id="s8:l7" met="4" type="-+">Terwyl een heilzon van <rhyme label="c" type="m" corresp="#s8:l5">geluk</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Ons aan elkaar zal <rhyme label="d" type="f" corresp="#s8:l6">boeijen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
