<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>10748</id>
    <date>
      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Grâce au bon petit Jésus</melody>
  <author>Jacob Janszoon Colevelt</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Ware Voedster van mijn <rhyme label="a" type="m">Jeught</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Wiens groote <rhyme label="a" type="m" corresp="#s1:l1">deught</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wiens gaven blincken als de <rhyme label="b" type="m">Son</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">O velt <rhyme label="c" type="f">Godinne</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">O <rhyme label="c" type="f" corresp="#s1:l4">Harderinne</rhyme>:</l>
        <l id="s1:l6" met="2" type="-+">Die <rhyme label="b" type="m" corresp="#s1:l3">overwon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mijn hart, mijn ziel, en al't <rhyme label="a" type="m">ghemoet</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Noyt <rhyme label="a" type="m" corresp="#s2:l1">teghenspoet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Ghenaeckten my in't luwe <rhyme label="b" type="m">dal</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Daer ick met <rhyme label="c" type="f">lusten</rhyme></l>
        <l id="s2:l5" met="2" type="-+">So vaeck gingh <rhyme label="c" type="f" corresp="#s2:l4">rusten</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">O eenighst <rhyme label="b" type="m" corresp="#s2:l3">al</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Goden schenen selfs <rhyme label="a" type="m">verblijt</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Dat doe ter <rhyme label="a" type="m" corresp="#s3:l1">tijt</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Wy Min met Min strenghelden <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Soo d'een als <rhyme label="c" type="f">d'ander</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Al door <rhyme label="c" type="f" corresp="#s3:l4">malkander</rhyme></l>
        <l id="s3:l6" met="2" type="-+">In <rhyme label="b" type="m" corresp="#s3:l3">overvloet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De doot ghescheyden heeft ons <rhyme label="a" type="m">Twee</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Ey my: o <rhyme label="a" type="m" corresp="#s4:l1">wee</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Verdoolt loop ick door Bosch en <rhyme label="b" type="m">Wout</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Dat selfs de <rhyme label="c" type="f">Dieren</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Schricken voort <rhyme label="c" type="f" corresp="#s4:l4">gieren</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Mijns stem <rhyme label="b" type="m" corresp="#s4:l3">verflout</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ick woel, ick draef, ick run, ick <rhyme label="a" type="m">jaegh</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Al <rhyme label="a" type="m" corresp="#s5:l1">even-staegh</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En ga vaeck sitten op u <rhyme label="b" type="m">graf</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Bedouwt met <rhyme label="c" type="f">tranen</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">En moet <rhyme label="c" type="f" corresp="#s5:l4">vermanen</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Wat jonst ghy <rhyme label="b" type="m" corresp="#s5:l3">gaf</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wel eer doen ick de aerd' <rhyme label="a" type="m">opgroef</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">U tot <rhyme label="a" type="m" corresp="#s6:l1">behoef</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En stack het spaetjen in het <rhyme label="b" type="m">zand</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Het druckigh <rhyme label="c" type="f">lyden</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Voor't lief <rhyme label="c" type="f" corresp="#s6:l4">verblijden</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Nam <rhyme label="b" type="m" corresp="#s6:l3">d'overhand</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Leght daer, dacht ick, o lieve <rhyme label="a" type="m">Licht</rhyme>:</l>
        <l id="s7:l2" met="2" type="-+">U sy <rhyme label="a" type="m" corresp="#s7:l1">ghesticht</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Een Marmer-Tomb, voor trouwe <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Voor alle <rhyme label="c" type="f">pijnen</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Voor al het <rhyme label="c" type="f" corresp="#s7:l4">quijnen</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">O <rhyme label="b" type="m" corresp="#s7:l3">Harderin</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Boomen en beeckjens vrylijck <rhyme label="a" type="m">tuyght</rhyme>:</l>
        <l id="s8:l2" met="2" type="-+">Het wilde <rhyme label="a" type="m" corresp="#s8:l1">ruyght</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Betreden is vaeck van mijn <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Met lieffelijck <rhyme label="c" type="f">kosen</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Met minnelijck <rhyme label="c" type="f" corresp="#s8:l4">blosen</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Met blye <rhyme label="b" type="m" corresp="#s8:l3">smart</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Rust daer, ey rust, verkoren <rhyme label="a" type="m">vrouw</rhyme>:</l>
        <l id="s9:l2" met="2" type="-+">Kond ghy mijn <rhyme label="a" type="m" corresp="#s9:l1">rouw</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Be-ooghen eens, dan soud'ghy <rhyme label="b" type="m">sien</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Hoe in het <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Ick na moet <rhyme label="c" type="f" corresp="#s9:l4">sweven</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Troost'loos <rhyme label="b" type="m" corresp="#s9:l3">allien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
