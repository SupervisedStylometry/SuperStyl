<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8846</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Durch Adams Fall ist ganz verderbt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">STaet op (Heer) den Vyant <rhyme label="a" type="m">v'drijft</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Vlien laet all' die dy <rhyme label="b" type="f">Haten</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ghelijck als Roock die niet en <rhyme label="a" type="m" corresp="#s1:l1">blijft</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Als Vuyr doet Wasch <rhyme label="b" type="f" corresp="#s1:l2">Ontlaten</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">So worden all' voor GOdt te <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die Godloose met <rhyme label="d" type="f">smerten</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Maer die Gherechte syn <rhyme label="c" type="m" corresp="#s1:l5">altijdt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In Godt verheucht van <rhyme label="d" type="f" corresp="#s1:l6">Herten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Singt Godt, maeckt hem een goede <rhyme label="a" type="m">Baen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hy is der Weesen <rhyme label="b" type="f">Vader</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hy neemt in syn Beschermingh' <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">De Weduwen <rhyme label="b" type="f" corresp="#s2:l2">All'gader</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Hy is Godt van seer groote <rhyme label="c" type="m">Cracht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Woont in syn Heyl'ghen <rhyme label="d" type="f">Throone</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Een Godt ghenaedich ende <rhyme label="c" type="m" corresp="#s2:l5">sacht</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die steedts syn Hulp <rhyme label="d" type="f" corresp="#s2:l6">bethoone</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den Eensamen hy gundt en <rhyme label="a" type="m">gheeft</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Het Huys vol goede <rhyme label="b" type="f">Kinder</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Oock Goets ghenoech, daer by hy <rhyme label="a" type="m" corresp="#s3:l1">leeft</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Met all' syn <rhyme label="b" type="f" corresp="#s3:l2">Huysghesinde</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Hy maeckt all' die Ghevanghen <rhyme label="c" type="m">vry</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die synder Hulp <rhyme label="d" type="f">verwachten</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Maer den Boosen laet blijven <rhyme label="c" type="m" corresp="#s3:l5">hy</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">End' in ghebreck <rhyme label="d" type="f" corresp="#s3:l6">versmachten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Godt als in Ouden Tijden <rhyme label="a" type="m">ghy</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Met dijnes Volckes <rhyme label="b" type="f">Heyre</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Ginckt Reysen in die <rhyme label="a" type="m" corresp="#s4:l1">Woesteny</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">De Aerd' doen beefde <rhyme label="b" type="f" corresp="#s4:l2">seere</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Den HEmel droop voor desen <rhyme label="c" type="m">Godt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die hem in Syna <rhyme label="d" type="f">Thoonde</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Een Godt zijns Volckes sonder <rhyme label="c" type="m" corresp="#s4:l5">spot</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die Israel <rhyme label="d" type="f" corresp="#s4:l6">verschoonde</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maer nu (Godt) geeft ghy Regen <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Jae, Reghen vol <rhyme label="b" type="f">ghenade</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Verquickt alsoo dijn Erve <rhyme label="a" type="m" corresp="#s5:l1">soet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat dorr' was vroech end' <rhyme label="b" type="f" corresp="#s5:l2">spade</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Dies dijn Ghediert daer woonen <rhyme label="c" type="m">mach</rhyme></l>
        <l id="s5:l6" met="3" type="-+">End' hem heel wel nu <rhyme label="d" type="f">schicket</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Dijn Elenden ghy sonder <rhyme label="c" type="m" corresp="#s5:l5">Claegh</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Door dijn Goetheyt <rhyme label="d" type="f" corresp="#s5:l6">verquicket</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">De HEEr heeft ons syn Woort so <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met veel <rhyme label="b" type="f">Evangelisten</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">End' als die Hoofden Eenich <rhyme label="a" type="m" corresp="#s6:l1">zijn</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Die Buyt en mach niet <rhyme label="b" type="f" corresp="#s6:l2">missen</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Ligt ghy te Veld' het glintzet <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Als Duyven vleughels <rhyme label="d" type="f">Glansse</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Ghelijck Silver end' Gout te <rhyme label="c" type="m" corresp="#s6:l5">gaer</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Soo schijnen uwe <rhyme label="d" type="f" corresp="#s6:l6">Schansse</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Als Godt Almachtich selver <rhyme label="a" type="m">gaet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Die Ampten wel <rhyme label="b" type="f">bestieren</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">So wort all' Licht door syn <rhyme label="a" type="m" corresp="#s7:l1">Ghenaed</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Daer Duysternis ginck <rhyme label="b" type="f" corresp="#s7:l2">Tieren</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Want Gods Bergh' is een vruchtbaer <rhyme label="c" type="m">Berg'</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Een vruchtbaer Berg vol <rhyme label="d" type="f">Eere</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Wat roemt ghy Berg' des Werelts <rhyme label="c" type="m" corresp="#s7:l5">argh</rhyme>?</l>
        <l id="s7:l8" met="3" type="-+">Op desen Rust de <rhyme label="d" type="f" corresp="#s7:l6">HEERE</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dees Bergh is Godes Wooningh <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Veel Duysenden syn <rhyme label="b" type="f">Wagen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Daer hy int Heyl'ge <rhyme label="a" type="m" corresp="#s8:l1">Synai</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Op vaert, end' sy hem <rhyme label="b" type="f" corresp="#s8:l2">draghen</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Ghy zijt HEER hooch ghevaren <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">End' hebt die Hell' <rhyme label="d" type="f">ghevangen</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Voor ons verplet des Vyants <rhyme label="c" type="m" corresp="#s8:l5">Cop</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">End' Gaven ons <rhyme label="d" type="f" corresp="#s8:l6">ontfanghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Veel Vyanden tot Godt <rhyme label="a" type="m">ghebracht</rhyme></l>
        <l id="s9:l2" met="3" type="-+">End in ghenaed' <rhyme label="b" type="f">aeng'nomen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Door dy (die Godt niet sloeghen <rhyme label="a" type="m" corresp="#s9:l1">acht</rhyme>)</l>
        <l id="s9:l4" met="3" type="-+">Sijn in syn Rijck <rhyme label="b" type="f" corresp="#s9:l2">ghecomen</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Ghelooft sy Godt de HEEr die <rhyme label="c" type="m">leeft</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Die ons met Noot <rhyme label="d" type="f">beladen</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Sijn hulp rijck'lijck medeylet <rhyme label="c" type="m" corresp="#s9:l5">heeft</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">En schenckt ons veel <rhyme label="d" type="f" corresp="#s9:l6">Weldaden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Door Godt die onse Heylandt <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Wy sijn den Doot <rhyme label="b" type="f">ontloopen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Des Vyants Hooft breeckt hy <rhyme label="a" type="m" corresp="#s10:l1">ghewis</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Verslaetse all' met <rhyme label="b" type="f" corresp="#s10:l2">Hoopen</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">Nochtans Godt oock bekeeren <rhyme label="c" type="m">gaet</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Wt Basan end' <rhyme label="d" type="f">Eylanden</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Maer synen Honden lecken <rhyme label="c" type="m" corresp="#s10:l5">laet</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Het bloet van syn <rhyme label="d" type="f" corresp="#s10:l6">Vyanden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Dijn Intocht schoon int <rhyme label="a" type="m">Heylichdom</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Aensiet men Godt mijn <rhyme label="b" type="f">HEEre</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Die Sangers end' Speel luyden <rhyme label="a" type="m" corresp="#s11:l1">vroom</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Die Maechden oock met <rhyme label="b" type="f" corresp="#s11:l2">Eere</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Gaen Loven dy, End' al te <rhyme label="c" type="m">saem</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Dijn Volck van Herten <rhyme label="d" type="f">reyne</rhyme>.</l>
        <l id="s11:l7" met="4" type="-+">Wy dancken dijnen Heyl'ghen <rhyme label="c" type="m" corresp="#s11:l5">Naem</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Voor Israels <rhyme label="d" type="f" corresp="#s11:l6">Fonteyne</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Daer heyrschet midden onder <rhyme label="a" type="m">haer</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Dijn Beniamin die <rhyme label="b" type="f">Cleyne</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Die Vorsten Juda oock al <rhyme label="a" type="m" corresp="#s12:l1">daer</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Heyrschen met haer <rhyme label="b" type="f" corresp="#s12:l2">Ghemeyne</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Die Vorsten Zebulon all <rhyme label="c" type="m">vroet</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Naphthali <rhyme label="d" type="f">desghelijcke</rhyme>.</l>
        <l id="s12:l7" met="4" type="-+">Dees Evangelij Boden <rhyme label="c" type="m" corresp="#s12:l5">goet</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">Ons Leeren Godes <rhyme label="d" type="f" corresp="#s12:l6">Rijcke</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Dijn Godt heeft dijn Rijck wel <rhyme label="a" type="m">bereyt</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Godt ons dat selffd' ooc <rhyme label="b" type="f">stercke</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Ons' Heylandt ghy in <rhyme label="a" type="m" corresp="#s13:l1">Eewicheyt</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Het is dijn eyghen <rhyme label="b" type="f" corresp="#s13:l2">Wercke</rhyme>.</l>
        <l id="s13:l5" met="4" type="-+">Om dijnes Tempels willen <rhyme label="c" type="m">HEER</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Die Coning' sullen <rhyme label="d" type="f">bringhen</rhyme>,</l>
        <l id="s13:l7" met="4" type="-+">Haer Gaven veel tot dijner <rhyme label="c" type="m" corresp="#s13:l5">Eer</rhyme>,</l>
        <l id="s13:l8" met="3" type="-+">End' vrolijck daer toe <rhyme label="d" type="f" corresp="#s13:l6">singhen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Die valsche Leeraers hier en <rhyme label="a" type="m">daer</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Wilt schelden end' <rhyme label="b" type="f">Mismaecken</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Die Ossen die naer Gelt all <rhyme label="a" type="m" corresp="#s14:l1">gaer</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Met haren Calver'n <rhyme label="b" type="f" corresp="#s14:l2">haecken</rhyme>.</l>
        <l id="s14:l5" met="4" type="-+">Verstroyt de Volcken (HEEr) <rhyme label="c" type="m">geheel</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Die geerne quaet <rhyme label="d" type="f">bedrijven</rhyme>,</l>
        <l id="s14:l7" met="4" type="-+">Bevechten altoos dijn <rhyme label="c" type="m" corresp="#s14:l5">Erffdeel</rhyme>,</l>
        <l id="s14:l8" met="3" type="-+">Haer Werck laet niet <rhyme label="d" type="f" corresp="#s14:l6">beclijven</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Egypten end' oock Mooren <rhyme label="a" type="m">Lant</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Tot Godt haer sullen <rhyme label="b" type="f">keeren</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Lofsingt den HEER aen alle <rhyme label="a" type="m" corresp="#s15:l1">kant</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Die over al met <rhyme label="b" type="f" corresp="#s15:l2">Eeren</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">In HEmel vaert van <rhyme label="c" type="m">Aenbegin</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Want hy sal sterckheyt <rhyme label="d" type="f">geven</rhyme>,</l>
        <l id="s15:l7" met="4" type="-+">Sijns Donders Stem nae synen <rhyme label="c" type="m" corresp="#s15:l5">Sin</rhyme>,</l>
        <l id="s15:l8" met="3" type="-+">End' syn Woort hooch <rhyme label="d" type="f" corresp="#s15:l6">verheven</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Geeft Godt de Macht, In <rhyme label="a" type="m">Israel</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Daer is syn <rhyme label="b" type="f">Heerlickhede</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Sijn Macht blijckt aen de Wolcken <rhyme label="a" type="m" corresp="#s16:l1">wel</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">In syn Heylichdom <rhyme label="b" type="f" corresp="#s16:l2">mede</rhyme>.</l>
        <l id="s16:l5" met="4" type="-+">Godt is Israels hoochste <rhyme label="c" type="m">Goet</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Hy sal syn Volck noch <rhyme label="d" type="f">meere</rhyme></l>
        <l id="s16:l7" met="4" type="-+">Machtich helpen wt alle <rhyme label="c" type="m" corresp="#s16:l5">Noot</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">Ghelooft sy Godt de <rhyme label="d" type="f" corresp="#s16:l6">HEERE</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
