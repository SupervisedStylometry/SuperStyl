<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178383</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Adieu dan Roselinde</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Zingt vry vereende <rhyme label="a" type="f">Schaaren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Op dit Geboorte - <rhyme label="b" type="m">feest</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Verheft weer stem en <rhyme label="a" type="f" corresp="#s1:l1">snaaren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verheugd met blyde <rhyme label="b" type="m" corresp="#s1:l2">geest</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Ons Erf - Prins moet'n wy <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Op zyn Geboorte <rhyme label="d" type="m">dag</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hoe dat hem d'Heer van <rhyme label="c" type="f" corresp="#s1:l5">boven</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Aan dit ons Neêrland <rhyme label="d" type="m" corresp="#s1:l6">gaf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Komt trouwe <rhyme label="a" type="f">Vaderlander</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die Nassauws - Huis <rhyme label="b" type="m">bemint</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die voor d'Oranje <rhyme label="a" type="f" corresp="#s2:l1">Stander</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Getrouw blyvt Prins <rhyme label="b" type="m" corresp="#s2:l2">gesint</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Het is Elf Jaar <rhyme label="c" type="f">geleeden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat God d'Oranje <rhyme label="d" type="m">Stam</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Verhoorden haar <rhyme label="c" type="f" corresp="#s2:l5">gebeeden</rhyme>;</l>
        <l id="s2:l8" met="3" type="-+">En onze Erf - Prins <rhyme label="d" type="m" corresp="#s2:l6">kwam</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die steun van Neêrlands <rhyme label="a" type="f">Staaten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die waarde <rhyme label="b" type="m">Palinuur</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Waar op men zig <rhyme label="a" type="f" corresp="#s3:l1">verlaaten</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">Voor Kerk en Land een <rhyme label="b" type="m" corresp="#s3:l2">Muur</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Een Spruit van Neêrlands <rhyme label="c" type="f">Helden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Een Fred'riks <rhyme label="d" type="m">naamgenoot</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wie kan uw roem <rhyme label="c" type="f" corresp="#s3:l5">vermelden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">ô Telg uit Nassauws <rhyme label="d" type="m" corresp="#s3:l6">schoot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy wenschen U veel <rhyme label="a" type="f">zegen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Van Neêrlands <rhyme label="b" type="m">Opperheer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat z'als een zagte <rhyme label="a" type="f" corresp="#s4:l1">reegen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Daal' op uw Hoogheid <rhyme label="b" type="m" corresp="#s4:l2">neer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat nooit geen druk of <rhyme label="c" type="f">smerten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Aantreffen uw <rhyme label="d" type="m">Persoon</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Dat wenschen wy van <rhyme label="c" type="f" corresp="#s4:l5">herten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">ô Grooten Willems <rhyme label="d" type="m" corresp="#s4:l6">Zoon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dat uwe jonge <rhyme label="a" type="f">Jaren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Versieren d'edele <rhyme label="b" type="m">deugd</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Godsvrugt met wysheid <rhyme label="a" type="f" corresp="#s5:l1">paren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tot aller vroomen <rhyme label="b" type="m" corresp="#s5:l2">vreugd</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Wyd zoo aan Godt uw <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hy zal u in den <rhyme label="d" type="m">noot</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Verlaaten nog <rhyme label="c" type="f" corresp="#s5:l5">begeeven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Zelfs niet tot in den <rhyme label="d" type="m" corresp="#s5:l6">doodt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Schenk zoo, ô Heer! uw <rhyme label="a" type="f">zegen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Aan 't gantsche Vorst'lyk <rhyme label="b" type="m">Huis</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Troost haar in al uw <rhyme label="a" type="f" corresp="#s6:l1">wegen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Bewaar het voor 't <rhyme label="b" type="m" corresp="#s6:l2">gespuis</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dier snoode laster <rhyme label="c" type="f">benden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Stel 's Vorsten zaak aan 't <rhyme label="d" type="m">ligt</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Wil Heer dit gunstig <rhyme label="c" type="f" corresp="#s6:l5">wenden</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Leer Neêrlanders haar <rhyme label="d" type="m" corresp="#s6:l6">pligt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
