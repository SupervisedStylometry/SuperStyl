      <category translation="amusement">vermaak</category>
    </categories>
  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">WAt pocht, en blaest ghy <rhyme label="a" type="f">spaensche-kroon-beminders</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Op uwe daden, en u groote <rhyme label="b" type="m">Slagh</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Wat snorckt en raest ghy groote <rhyme label="a" type="f" corresp="#s1:l1">Slagh-winders</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Als of d' Oraengjens-boom ter Aerden <rhyme label="b" type="m" corresp="#s1:l2">lagh</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Door een <rhyme label="c" type="f">Victory</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Van u <rhyme label="d" type="m">Blockers-dijck</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">O krancke <rhyme label="c" type="f" corresp="#s1:l5">glory</rhyme>!</l>
        <l id="s1:l8" met="3" type="+-">'t Schijnt of Spaensche <rhyme label="d" type="m" corresp="#s1:l6">Rijck</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Ons dempten in de <rhyme label="d" type="m" corresp="#s1:l6">slijck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Maer denckt vry neen ghy Papen en <rhyme label="a" type="f">Nonnen</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Ick lagh u uyt om uwe blijschap <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Ghy hebt doch niet een handt-breet Landt <rhyme label="a" type="f" corresp="#s2:l1">gewonnen</rhyme>:</l>
        <l id="s2:l4" met="5" type="-+">Maer 'k weet de reden, 't was om dat de <rhyme label="b" type="m" corresp="#s2:l2">doot</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Lagh op u <rhyme label="c" type="f">Lippen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">G'lijck een Schip in <rhyme label="d" type="m">Zee</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Stoot aen de <rhyme label="c" type="f" corresp="#s2:l5">Klippen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Even gingt u <rhyme label="d" type="m" corresp="#s2:l6">mee</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Want het u beven <rhyme label="d" type="m" corresp="#s2:l6">dee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Ghy lastert onse Nederlandtsche <rhyme label="a" type="f">Helden</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Door uwe Versen, vals en plomp <rhyme label="b" type="m">gemaeckt</rhyme></l>
        <l id="s3:l3" met="5" type="-+">Maer henght u niet de schrick'lijcke <rhyme label="a" type="f" corresp="#s3:l1">gewelden</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Die voor Breda nu 't Jaer zijn <rhyme label="b" type="m" corresp="#s3:l2">uyt-gebraeckt</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Soo komt ter <rhyme label="c" type="f">Tolen</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Op de <rhyme label="d" type="m">Mossel-kreeck</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Komt daer weer <rhyme label="c" type="f" corresp="#s3:l5">dolen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Daer de <rhyme label="d" type="m" corresp="#s3:l6">silte-Beeck</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Het water Bloedt <rhyme label="d" type="m" corresp="#s3:l6">geleeck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Ons Hollants-vorst en vreesde noyt de <rhyme label="a" type="f">Wallen</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Noch diepe Vesten van de Stadt <rhyme label="b" type="m">Maestricht</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Kloeckmoedigh heeft hy Wesel <rhyme label="a" type="f" corresp="#s4:l1">overvallen</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">En dwangh 's Hertogen-bosch tot Hollands <rhyme label="b" type="m" corresp="#s4:l2">plicht</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Grol wert <rhyme label="c" type="f">beklommen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Spaengjen mostert <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Wilt ghy dan <rhyme label="c" type="f" corresp="#s4:l5">brommen</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Van u groote <rhyme label="d" type="m" corresp="#s4:l6">Bruydt</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Och 't een soete <rhyme label="d" type="m" corresp="#s4:l6">kluydt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Want quaemt ghy eens op 't Hof van Hollant <rhyme label="a" type="f">kijcken</rhyme></l>
        <l id="s5:l2" met="5" type="-+">De teeckens van soo mennig' braven <rhyme label="b" type="m">Slagh</rhyme></l>
        <l id="s5:l3" met="5" type="-+">Ick weet ghy sout al bevende <rhyme label="a" type="f" corresp="#s5:l1">bezwijcken</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Als ghy 't geflicker voor u oogen <rhyme label="b" type="m" corresp="#s5:l2">sagh</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Van veel <rhyme label="c" type="f">Standaren</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Vaendels, daer 't <rhyme label="d" type="m">geschreeuw</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Brack u <rhyme label="c" type="f" corresp="#s5:l5">Altaren</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Die Hollantsche <rhyme label="d" type="m" corresp="#s5:l6">Leeuw</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Bewaert tot onser <rhyme label="d" type="m" corresp="#s5:l6">eeuw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Dat doet ons Prins, den <rhyme label="a" type="f">Spaensen-kroon-bedwinger</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Door Gods beleyt, en Neerlands <rhyme label="b" type="m">onderstant</rhyme>:</l>
        <l id="s6:l3" met="5" type="-+">Hy is ons Vorst, die als een <rhyme label="a" type="f" corresp="#s6:l1">Blixems-slinger</rhyme></l>
        <l id="s6:l4" met="5" type="-+">Sijn Vleugels zwaeyt rontom ons <rhyme label="b" type="m" corresp="#s6:l2">Vaders-landt</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Gaet dan vry <rhyme label="c" type="f">streven</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Op Caloyens <rhyme label="d" type="m">kans</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Komt liever <rhyme label="c" type="f" corresp="#s6:l5">zweven</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Om een <rhyme label="d" type="m" corresp="#s6:l6">Schencke-schans</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">En leert den <rhyme label="d" type="m" corresp="#s6:l6">Beyren-dans</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Dus denckt Fortuyn kan haest haer vleugels <rhyme label="a" type="f">wenden</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">En roept geen hey, voor dat ghy steden <rhyme label="b" type="m">siet</rhyme>;</l>
        <l id="s7:l3" met="5" type="-+">Want weet den Somer is noch niet ten <rhyme label="a" type="f" corresp="#s7:l1">enden</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">En 't is voor desen oock wel meer <rhyme label="b" type="m" corresp="#s7:l2">geschiet</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Denckt, wy <rhyme label="c" type="f">Hollanders</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Sullen (eer wy <rhyme label="d" type="m">gaen</rhyme>)</l>
        <l id="s7:l7" met="2" type="-+">Noch Spaensche <rhyme label="c" type="f" corresp="#s7:l5">Standers</rhyme></l>
        <l id="s7:l8" met="3" type="+-">Van u nemen <rhyme label="d" type="m" corresp="#s7:l6">aen</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Want s' u t' hans seer <rhyme label="d" type="m" corresp="#s7:l6">belaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
