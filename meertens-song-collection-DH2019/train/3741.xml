    <melody>Psalm 138 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICK danck' v HEER wt s'herten <rhyme label="a" type="m">grondt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Lippen end <rhyme label="a" type="m" corresp="#s1:l1">mondt</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">V' eer' <rhyme label="b" type="f">voortbringhen</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Voor den vorsten wil ick O <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">V' lof end <rhyme label="c" type="m" corresp="#s1:l4">eer</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Ghestaedigh <rhyme label="b" type="f" corresp="#s1:l3">singhen</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Ick wil in uwen tempel <rhyme label="d" type="m">saen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">V bidden <rhyme label="d" type="m" corresp="#s1:l7">aen</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">End eer' <rhyme label="e" type="f">bewysen</rhyme>:</l>
        <l id="s1:l10" met="4" type="-+">End v dancken om v' <rhyme label="f" type="m">goetheit</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">End <rhyme label="f" type="m" corresp="#s1:l10">ghetrauwheit</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">niet om <rhyme label="e" type="f" corresp="#s1:l9">volprysen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy hebt uwen naem ghemaeckt <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Als ghy in <rhyme label="a" type="m" corresp="#s2:l1">noot</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">V' toont <rhyme label="b" type="f">waerachtigh</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Als ick v aenroepe Heer', <rhyme label="c" type="m">ghy</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Verhooret <rhyme label="c" type="m" corresp="#s2:l4">my</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">End troost my <rhyme label="b" type="f" corresp="#s2:l3">krachtigh</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Dies moeten die Koninghen <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">In desen <rhyme label="d" type="m" corresp="#s2:l7">val</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">V seer <rhyme label="e" type="f">vereeren</rhyme>:</l>
        <l id="s2:l10" met="4" type="-+">Als se verstaen dat steedts v' <rhyme label="f" type="m">Woort</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Vast blyft nu <rhyme label="f" type="m" corresp="#s2:l10">voort</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">O Heer der <rhyme label="e" type="f" corresp="#s2:l9">Heeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy moeten ghedencken <rhyme label="a" type="m">verhueght</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">In groote <rhyme label="a" type="m" corresp="#s3:l1">vrueghdt</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Heer uwer <rhyme label="b" type="f">wercken</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">End bekennen dat Godts lof <rhyme label="c" type="m">fyn</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Eewigh sal <rhyme label="c" type="m" corresp="#s3:l4">sijn</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">WÄ™erdigh <rhyme label="b" type="f" corresp="#s3:l3">t'aenmercken</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Want ghy seer hoogh' sittend' <rhyme label="d" type="m">aensiet</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Dat hier is <rhyme label="d" type="m" corresp="#s3:l7">niet</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Gheacht, <rhyme label="e" type="f">demoedigh</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">Van verren ghy die stoute <rhyme label="f" type="m">kent</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Daer van ghy <rhyme label="f" type="m" corresp="#s3:l10">wendt</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">V' ooghen <rhyme label="e" type="f" corresp="#s3:l9">goedigh</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Als ick in angst end <rhyme label="a" type="m">teghenspoet</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Ben in <rhyme label="a" type="m" corresp="#s4:l1">kleynmoet</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Ghy my <rhyme label="b" type="f">verquicket</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Oock teghen myn wreetsten <rhyme label="c" type="m">viandt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">V' rechte <rhyme label="c" type="m" corresp="#s4:l4">handt</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">My hulp' <rhyme label="b" type="f" corresp="#s4:l3">toeschicket</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Ghy sult myn kruys eyndighen <rhyme label="d" type="m">schier</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Want <rhyme label="d" type="m" corresp="#s4:l7">goedertier</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Sijt ghy <rhyme label="e" type="f">ghestaedigh</rhyme>.</l>
        <l id="s4:l10" met="4" type="-+">Het werck uwer handen sult <rhyme label="f" type="m">ghy</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Voluoeren <rhyme label="f" type="m" corresp="#s4:l10">vry</rhyme>,</l>
        <l id="s4:l12" met="2" type="-+">O Heer' <rhyme label="e" type="f" corresp="#s4:l9">ghenadigh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
