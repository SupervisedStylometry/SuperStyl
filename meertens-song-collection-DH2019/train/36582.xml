<song>
  <meta>
    <id>36582</id>
    <date>
      <year-start>1722</year-start>
      <year-end>1722</year-end>
    </date>
    <location>
      <orginal-name>duinkerken</orginal-name>
      <standard-name>Duinkerken, France</standard-name>
      <coordinates>
        <lon>51.1907405</lon>
        <lat>2.4978867</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="ambachtslied" translation="trademan's song">ambachtslied</tag>
      <tag standard-name="verhalend lied" translation="narrative song">verhalend lied</tag>
    </tags>
    <melody>Het droevige nonnetje</melody>
    <categories>
      <category translation="narratives">verhalen</category>
      <category translation="work">werk</category>
    </categories>
  <author>Jacobus de Ruyter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Des Wereldts sorghen heb ick hier <rhyme label="a" type="f">beschreven</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Hoe dat een ieder tracht naer gelt en <rhyme label="b" type="m">goedt</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Als oft men hondert jaeren had te <rhyme label="a" type="f" corresp="#s1:l1">leven</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">'t Welck als men sterft hier alles laeten <rhyme label="b" type="m" corresp="#s1:l2">moet</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Elck siet men <rhyme label="c" type="f">sorghen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Altijdt om <rhyme label="c" type="f" corresp="#s1:l5">morghen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Niemant die <rhyme label="d" type="m">leeft</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Die hier gheen sorgh en <rhyme label="d" type="m" corresp="#s1:l7">heeft</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Den eenen sorght en tracht naer hooghe <rhyme label="a" type="f">staeten</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Een ander om te wesen schoon en <rhyme label="b" type="m">rijck</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Den derden wilt het aerdtsche goedt <rhyme label="a" type="f" corresp="#s2:l1">verlaeten</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Den vierden om te hebben veel <rhyme label="b" type="m" corresp="#s2:l2">pratijck</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Oock ander <rhyme label="c" type="f">trachten</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Naer groote <rhyme label="c" type="f" corresp="#s2:l5">prachten</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Veel om een <rhyme label="d" type="m">Vrouw</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die schoon is en <rhyme label="d" type="m" corresp="#s2:l7">ghetrou'</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">De Kleere-maeckers sorghen om te <rhyme label="a" type="f">naeyen</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">En de Barbiers om t'hebben een <rhyme label="b" type="m">Patient</rhyme></l>
        <l id="s3:l3" met="5" type="-+">Den Acker-man om sijn Landt te <rhyme label="a" type="f" corresp="#s3:l1">besaeyen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">De Schilders om te wercken konstigh <rhyme label="b" type="m" corresp="#s3:l2">jent</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Brouwers om <rhyme label="c" type="f">Brouwen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Dochters om <rhyme label="c" type="f" corresp="#s3:l5">trouwen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Backers om <rhyme label="d" type="m">broodt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Te backen in den <rhyme label="d" type="m" corresp="#s3:l7">noodt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Procureurs om de Boeren te <rhyme label="a" type="f">bedrieghen</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Daerom sorght oock den wijsen <rhyme label="b" type="m">Advocaet</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Sanghers om daer somtijdts wat by te <rhyme label="a" type="f" corresp="#s4:l1">lieghen</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Om buyt te maecken soo sorght den <rhyme label="b" type="m" corresp="#s4:l2">Soldaet</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">De Smits om <rhyme label="c" type="f">Smeden</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Veldt-heers om <rhyme label="c" type="f" corresp="#s4:l5">steden</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Den <rhyme label="d" type="m">Herbergier</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Om venten wijn en <rhyme label="d" type="m" corresp="#s4:l7">bier</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">De Dronckaerts sorghen alrijdt om te <rhyme label="a" type="f">drincken</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">En de Paterkens naer de <rhyme label="b" type="m">soberheyt</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">De Weerden om de glaesen vol te <rhyme label="a" type="f" corresp="#s5:l1">schincken</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">De Nonnekens voor hunne <rhyme label="b" type="m" corresp="#s5:l2">Saligheyt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Druckers om <rhyme label="c" type="f">drucken</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Pluckreurs om <rhyme label="c" type="f" corresp="#s5:l5">plucken</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Een Schippers <rhyme label="d" type="m">kindt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Sorght naer een goeden <rhyme label="d" type="m" corresp="#s5:l7">windt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">De Meulenaers die sorghen om te <rhyme label="a" type="f">maelen</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Timmerlien om te maecken een goedt <rhyme label="b" type="m">werck</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Voer-lien om de Koopmans hun goedt te <rhyme label="a" type="f" corresp="#s6:l1">haelen</rhyme></l>
        <l id="s6:l4" met="5" type="-+">Pastoors en Kosters sorghen naer de <rhyme label="b" type="m" corresp="#s6:l2">Kerck</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Schrijvers om <rhyme label="c" type="f">schrijven</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Mannen om <rhyme label="c" type="f" corresp="#s6:l5">Wijven</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Jaeghers naer 't <rhyme label="d" type="m">wilt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En zijn schier noyt <rhyme label="d" type="m" corresp="#s6:l7">ghestilt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">De Visschers sorghen om wat vis te <rhyme label="a" type="f">vanghen</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">En Bedelaers om te krijghen den <rhyme label="b" type="m">kost</rhyme>,</l>
        <l id="s7:l3" met="5" type="-+">Een dief die steelt tot dat hy wordt <rhyme label="a" type="f" corresp="#s7:l1">ghehanghen</rhyme></l>
        <l id="s7:l4" met="5" type="-+">Slampampers om te smeiren hunnen <rhyme label="b" type="m" corresp="#s7:l2">borst</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Slaghers om <rhyme label="c" type="f">beesten</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">Lappers om <rhyme label="c" type="f" corresp="#s7:l5">leesten</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Den Wever <rhyme label="d" type="m">koel</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Naer 't ghetouw en <rhyme label="d" type="m" corresp="#s7:l7">Schiet-spoel</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">d'Hoveniers om te saeyen en te <rhyme label="a" type="f">planten</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">Schip-Timmerliens die maecken schepen <rhyme label="b" type="m">ree</rhyme>,</l>
        <l id="s8:l3" met="5" type="-+">De Winckeliers sorghen voor hun <rhyme label="a" type="f" corresp="#s8:l1">kalanten</rhyme>,</l>
        <l id="s8:l4" met="5" type="-+">De Kaepers om te kaepen op de <rhyme label="b" type="m" corresp="#s8:l2">zee</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Zaeghers om <rhyme label="c" type="f">zaeghen</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Plaeghers om <rhyme label="c" type="f" corresp="#s8:l5">plaeghen</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Sorghen <rhyme label="d" type="m">altijdt</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Elck een naer sijn <rhyme label="d" type="m" corresp="#s8:l7">profijt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">De Laecke-sneyers sorghen op 't <rhyme label="a" type="f">verkoopen</rhyme></l>
        <l id="s9:l2" met="5" type="-+">Hoedt maeckers om te maecken eenen <rhyme label="b" type="m">Hoedt</rhyme>,</l>
        <l id="s9:l3" met="5" type="-+">Scherp-rechters om de dieven op te <rhyme label="a" type="f" corresp="#s9:l1">knoopen</rhyme>,</l>
        <l id="s9:l4" met="5" type="-+">En al de Minnaers voor hun Liefste <rhyme label="b" type="m" corresp="#s9:l2">soet</rhyme></l>
        <l id="s9:l5" met="2" type="-+">De Kockx om <rhyme label="c" type="f">koken</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">Spokers om <rhyme label="c" type="f" corresp="#s9:l5">spoken</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Kuypers naer <rhyme label="d" type="m">Wijn</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Die wel besorght moet <rhyme label="d" type="m" corresp="#s9:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="-+">De Matsenaers die sorghen om te <rhyme label="a" type="f">Matsen</rhyme>,</l>
        <l id="s10:l2" met="5" type="-+">Stoel-draeyers om te draeyen eenen <rhyme label="b" type="m">stoel</rhyme>,</l>
        <l id="s10:l3" met="5" type="-+">Speelders om Kaerten oft wel om te <rhyme label="a" type="f" corresp="#s10:l1">katsen</rhyme>,</l>
        <l id="s10:l4" met="5" type="-+">De Schotters om te schieten naer den <rhyme label="b" type="m" corresp="#s10:l2">doel</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Maeckers van <rhyme label="c" type="f">glaesen</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Blaesers om <rhyme label="c" type="f" corresp="#s10:l5">blaesen</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+"><rhyme label="d" type="m">d'Apotikaer</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Naer sieckten veel en <rhyme label="d" type="m" corresp="#s10:l7">swaer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="5" type="-+">Doctoors om veel Recepten <rhyme label="a" type="f">t'ordonneeren</rhyme>,</l>
        <l id="s11:l2" met="5" type="-+">Den Boer te pluymen geirt den <rhyme label="b" type="m">Oprateur</rhyme>,</l>
        <l id="s11:l3" met="5" type="-+">School-meesters om de Kinders wel te <rhyme label="a" type="f" corresp="#s11:l1">leeren</rhyme>,</l>
        <l id="s11:l4" met="5" type="-+">De Verwers om te verwen schoon <rhyme label="b" type="m" corresp="#s11:l2">kouleur</rhyme>,</l>
        <l id="s11:l5" met="2" type="-+">Wijfs <rhyme label="c" type="f">eyghenschappen</rhyme>,</l>
        <l id="s11:l6" met="2" type="-+">Veel quaedt te <rhyme label="c" type="f" corresp="#s11:l5">klappen</rhyme>,</l>
        <l id="s11:l7" met="2" type="-+">Al <rhyme label="d" type="m">tijdelijck</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Maer niet naer 't <rhyme label="d" type="m" corresp="#s11:l7">Hemel-rijck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
