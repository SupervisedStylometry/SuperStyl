  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ROts en wout heeft <rhyme label="a" type="m">mede-ly</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Met myn weenen, en <rhyme label="a" type="m" corresp="#s1:l1">gheschrey</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">'k Sagh hun myne droefheyt <rhyme label="b" type="f">spyten</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En myn eeuwigh <rhyme label="c" type="m">onghemack</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Want den druck hun quam te <rhyme label="b" type="f" corresp="#s1:l3">splyten</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wyl hun het ghe-treur <rhyme label="c" type="m" corresp="#s1:l4">ontbrack</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">'t Beecxjen dat ick <rhyme label="a" type="m">menighmael</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Hebb' gheopent myne <rhyme label="a" type="m" corresp="#s2:l1">quael</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Schynt my altydt te <rhyme label="b" type="f">beklaghen</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Want het met een droef <rhyme label="c" type="m">ghezwier</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Komt de tydingh' voort te <rhyme label="b" type="f" corresp="#s2:l3">draghen</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Aen de bemden wat van <rhyme label="c" type="m" corresp="#s2:l4">hier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy die al myn smert <rhyme label="a" type="m">aen-siet</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">En noyt traentjen daerom <rhyme label="a" type="m" corresp="#s3:l1">liet</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Laet ten eynd' u hert' eens <rhyme label="b" type="f">splyten</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Volght de rotsen immers <rhyme label="c" type="m">naer</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Daer sy het gheween <rhyme label="b" type="f" corresp="#s3:l3">versmyten</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Maken sy doch dit <rhyme label="c" type="m" corresp="#s3:l4">misbaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
