<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>158813</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Van de mosselkreekse paap</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE Keucken-voocht op 't <rhyme label="a" type="m">Bruylofts-mael</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In Kana's <rhyme label="b" type="m">Vreughde-Feest</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Als hy sijn Gasten deed <rhyme label="a" type="m" corresp="#s1:l1">onthael</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verheughd hy haer de <rhyme label="b" type="m" corresp="#s1:l2">Gheest</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Eerst met de beste <rhyme label="c" type="m">Wijn</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Maer als zy waren <rhyme label="d" type="f">droncken</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Verkeerd de gunst in <rhyme label="c" type="m" corresp="#s1:l5">schijn</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Want 't slechst word dan <rhyme label="d" type="f" corresp="#s1:l6">geschoncken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maer Jesus de Fonteyn des <rhyme label="a" type="m">goedts</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Daer 't alles van komt <rhyme label="b" type="m">af</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die schonck haer op het lest wat <rhyme label="a" type="m" corresp="#s2:l1">soets</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Doe hy daer Wijn toe <rhyme label="b" type="m" corresp="#s2:l2">gaf</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">En voerd de Gheest soo <rhyme label="c" type="m">op</rhyme></l>
        <l id="s2:l6" met="3" type="-+">In vreughde boven <rhyme label="d" type="f">vreughde</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Tot zy bereyckt de <rhyme label="c" type="m" corresp="#s2:l5">top</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En Hooft-punt van <rhyme label="d" type="f" corresp="#s2:l6">geneughden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dees Bruyloft die vertoont ons <rhyme label="a" type="m">hier</rhyme></l>
        <l id="s3:l2" met="3" type="-+">De Wereldt in sijn <rhyme label="b" type="m">Aerdt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Daer in de Sathan biedt <rhyme label="a" type="m" corresp="#s3:l1">playsier</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Maer bitterheden <rhyme label="b" type="m" corresp="#s3:l2">spaert</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die hy sijn Dienaers <rhyme label="c" type="m">sent</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Na 't lockende <rhyme label="d" type="f">vermaecken</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Op t eynd geeft hy <rhyme label="c" type="m" corresp="#s3:l5">torment</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En Helsche pijn te <rhyme label="d" type="f" corresp="#s3:l6">smaecken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer Christi uytverkoren <rhyme label="a" type="m">Volck</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Eerst met ellend <rhyme label="b" type="m">versaed</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Wordt hier in eenen <rhyme label="a" type="m" corresp="#s4:l1">Jammer-Kolck</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Tot over 't hooft <rhyme label="b" type="m" corresp="#s4:l2">gebaed</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Maer 't eynde van die <rhyme label="c" type="m">strijdt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Is d'intree van de <rhyme label="d" type="f">vreughde</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Daer in men drinckt <rhyme label="c" type="m" corresp="#s4:l5">altijdt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Wt stroomen van <rhyme label="d" type="f" corresp="#s4:l6">geneughden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
