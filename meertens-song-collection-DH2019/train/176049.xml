    <melody>Psalm 075 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Jacobus Revius</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">V Van herten loven <rhyme label="a" type="m">wy</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">V wy dancken, onse <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat u name vol van <rhyme label="b" type="m" corresp="#s1:l2">eer</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Ons gecomen is <rhyme label="a" type="m" corresp="#s1:l1">na-by</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Men vertellet hel en <rhyme label="c" type="m">claer</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Vwe daden <rhyme label="c" type="m" corresp="#s1:l5">wonderbaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als de tijt is voor de <rhyme label="a" type="m">hant</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En ick in mijn ampt <rhyme label="b" type="m">gesett'</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Ick sal richten sonder <rhyme label="b" type="m" corresp="#s2:l2">smet</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Al versmolt het heele <rhyme label="a" type="m" corresp="#s2:l1">lant</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Van my sal het zijn <rhyme label="c" type="m">gesticht</rhyme></l>
        <l id="s2:l6" met="4" type="+-">En syn stijlen <rhyme label="c" type="m" corresp="#s2:l5">opgericht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Dickmael seyd' ick tot de <rhyme label="a" type="m">sots</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Laet u dwase rancken <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">De godloose sprack ick <rhyme label="b" type="m" corresp="#s3:l2">aen</rhyme>;</l>
        <l id="s3:l4" met="4" type="+-">Heft den horen niet te <rhyme label="a" type="m" corresp="#s3:l1">trots</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Draeft niet met een harden <rhyme label="c" type="m">neck</rhyme>.</l>
        <l id="s3:l6" met="4" type="+-">Spreeckt niet met een stijven <rhyme label="c" type="m" corresp="#s3:l5">beck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'tComt van oosten noch van <rhyme label="a" type="m">west</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'tComt niet uyt de <rhyme label="b" type="m">wildernis</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Dat een man verheven <rhyme label="b" type="m" corresp="#s4:l2">is</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Loopt en slavet al u <rhyme label="a" type="m" corresp="#s4:l1">best</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">God is richter ende <rhyme label="c" type="m">voocht</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">God vernedert, God <rhyme label="c" type="m" corresp="#s4:l5">verhoocht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">In Gods handen is een <rhyme label="a" type="m">cop</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Vol van wijn <rhyme label="b" type="m">door-een-gemengt</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Yegelijck hy daer uyt <rhyme label="b" type="m" corresp="#s5:l2">schenckt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Maer het slijmerige <rhyme label="a" type="m" corresp="#s5:l1">sop</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Sal der goddelozen <rhyme label="c" type="m">mont</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Suypen tot den diepsten <rhyme label="c" type="m" corresp="#s5:l5">gront</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Eeuwich doe ick dies <rhyme label="a" type="m">vermaen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Ick wil prijsen Iacobs <rhyme label="b" type="m">God</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En der goddelosen <rhyme label="b" type="m" corresp="#s6:l2">rot</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Haren hoorn te morsel <rhyme label="a" type="m" corresp="#s6:l1">slaen</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Dan wert aller vromen <rhyme label="c" type="m">cracht</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Tot de hoochste eer <rhyme label="c" type="m" corresp="#s6:l5">gebracht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
