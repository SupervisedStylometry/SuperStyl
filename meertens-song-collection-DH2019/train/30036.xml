    <melody>Psalm 077 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Philips van Marnix van Sint Aldegonde</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MIjn stem is tot God <rhyme label="a" type="f">geresen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Tot den Heere God <rhyme label="a" type="f" corresp="#s1:l1">gepresen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Is geklommen mijn <rhyme label="b" type="m">gebedt</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Hy heeft op mijn stem <rhyme label="b" type="m" corresp="#s1:l3">gelet</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Als ick was met ancxt <rhyme label="c" type="f">bevangen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Socht ick God met groot <rhyme label="c" type="f" corresp="#s1:l5">verlangen</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">Mijne handt snachts <rhyme label="d" type="m">uytgestrect</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Bleef geduerich <rhyme label="d" type="m" corresp="#s1:l7">uytgereckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Mijne siel bang uyt der <rhyme label="a" type="f">maten</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Woud haer geensins troosten <rhyme label="a" type="f" corresp="#s2:l1">laten</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Quam my God in mijn <rhyme label="b" type="m">gedacht</rhyme>?</l>
        <l id="s2:l4" met="4" type="+-">Thert viel my schier in <rhyme label="b" type="m" corresp="#s2:l3">onmacht</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Of ick mijn gebeden <rhyme label="c" type="f">seyde</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">End wat troostlijcx <rhyme label="c" type="f" corresp="#s2:l5">overleyde</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Mijnen geest bleef doch <rhyme label="d" type="m">benaut</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">End thert even seer <rhyme label="d" type="m" corresp="#s2:l7">verflaut</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Du hielst op, mijn <rhyme label="a" type="f">oogenschelen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dies my twaecken dede <rhyme label="a" type="f" corresp="#s3:l1">quelen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Ick was heel bedwelmt end <rhyme label="b" type="m">swack</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">So dat ick gantz niet en <rhyme label="b" type="m" corresp="#s3:l3">sprack</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Ick docht in mijn hert <rhyme label="c" type="f">verslagen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Op de voirgeleden <rhyme label="c" type="f" corresp="#s3:l5">dagen</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">End den tijt van <rhyme label="d" type="m">overlanck</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Quam my voir, in mijn <rhyme label="d" type="m" corresp="#s3:l7">gedanck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">My gedacht, hoe ick op <rhyme label="a" type="f">snaren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Dijnen lof plach <rhyme label="a" type="f" corresp="#s4:l1">t'openbaren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">So dat my thert nacht end <rhyme label="b" type="m">dach</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Vol becommernissen <rhyme label="b" type="m" corresp="#s4:l3">lach</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Mijnen geest om dit te <rhyme label="c" type="f">soecken</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Keerdet om, aen alle <rhyme label="c" type="f" corresp="#s4:l5">hoecken</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Van het eynd tot aent <rhyme label="d" type="m">begin</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Doe sprack ick in mijnen <rhyme label="d" type="m" corresp="#s4:l7">sin</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sal God eewlijck dan <rhyme label="a" type="f">verstooten</rhyme>?</l>
        <l id="s5:l2" met="4" type="+-">End my sijner gunst <rhyme label="a" type="f" corresp="#s5:l1">ontblooten</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Sal hy my van nu <rhyme label="b" type="m">voortaen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Niet meer in genaed <rhyme label="b" type="m" corresp="#s5:l3">ontfaen</rhyme>?</l>
        <l id="s5:l5" met="4" type="+-">Sal sijn goetheyt, noyt <rhyme label="c" type="f">volpresen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Voir altoos ten eynde <rhyme label="c" type="f" corresp="#s5:l5">wesen</rhyme>?</l>
        <l id="s5:l7" met="4" type="+-">Is dan sijn woirt end <rhyme label="d" type="m">verbondt</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Uytgeput tot inden <rhyme label="d" type="m" corresp="#s5:l7">grondt</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Heeft dan God (om hoog <rhyme label="a" type="f">geseten</rhyme>)</l>
        <l id="s6:l2" met="4" type="+-">Sijn barmhertigheyt <rhyme label="a" type="f" corresp="#s6:l1">vergeten</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Is sijn goetheyt <rhyme label="b" type="m">teenegaer</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Toegestopt met toirne <rhyme label="b" type="m" corresp="#s6:l3">swaer</rhyme>?</l>
        <l id="s6:l5" met="4" type="+-">Doch hernam ick my noch <rhyme label="c" type="f">weder</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Ende sprack: Al lig ick <rhyme label="c" type="f" corresp="#s6:l5">neder</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">End al is mijn smert <rhyme label="d" type="m">verseert</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">s'Hoogsten handt is haest <rhyme label="d" type="m" corresp="#s6:l7">gekeert</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Dies wil ick nu naerder <rhyme label="a" type="f">mercken</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">s'Heeren wonderbare <rhyme label="a" type="f" corresp="#s7:l1">wercken</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Ia oock sijne groote <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Die hy voirtijts heeft <rhyme label="b" type="m" corresp="#s7:l3">volbracht</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Ick wil vlijtich <rhyme label="c" type="f">overleggen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Al dijn werck, end wonder <rhyme label="c" type="f" corresp="#s7:l5">seggen</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Van dijn doen, dwelck groot <rhyme label="d" type="m">gewis</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Ende wonderbaerlijck <rhyme label="d" type="m" corresp="#s7:l7">is</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">O God, inden heylgen <rhyme label="a" type="f">tempel</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Sietmen menich schoon <rhyme label="a" type="f" corresp="#s8:l1">exempel</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Van dijn wegen ende <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Wie is doch God die dy <rhyme label="b" type="m" corresp="#s8:l3">slacht</rhyme>?</l>
        <l id="s8:l5" met="4" type="+-">Du bist alleen God <rhyme label="c" type="f">almachtig</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Die werckst wonderdaden <rhyme label="c" type="f" corresp="#s8:l5">crachtig</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Die dijn sterckt' end <rhyme label="d" type="m">majesteyt</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Onder d'Heydens hebst <rhyme label="d" type="m" corresp="#s8:l7">verbreyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Du hebst dy dijns volcx <rhyme label="a" type="f">ontfermet</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">End met dijnen erm <rhyme label="a" type="f" corresp="#s9:l1">beschermet</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Iacobs ende Iosephs <rhyme label="b" type="m">stam</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Theylig saet van <rhyme label="b" type="m" corresp="#s9:l3">Abraham</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Waterbaren, <rhyme label="c" type="f">waterbaren</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Sagen dy, Heer der <rhyme label="c" type="f" corresp="#s9:l5">heyrscharen</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Ende waren seer <rhyme label="d" type="m">verschrickt</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Ia den afgrondt heeft <rhyme label="d" type="m" corresp="#s9:l7">geswickt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Men sach wolcken dick met <rhyme label="a" type="f">hoopen</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Van slachregen <rhyme label="a" type="f" corresp="#s10:l1">overloopen</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Dijnen donder brack daer <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Met een eysselijck <rhyme label="b" type="m" corresp="#s10:l3">geluyt</rhyme>:</l>
        <l id="s10:l5" met="4" type="+-">Men sach dijn vierpijlen <rhyme label="c" type="f">jagen</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Met den krack der <rhyme label="c" type="f" corresp="#s10:l5">donderslagen</rhyme>:</l>
        <l id="s10:l7" met="4" type="+-">End den blixem viel so <rhyme label="d" type="m">dicht</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Dat de locht heel werd <rhyme label="d" type="m" corresp="#s10:l7">verlicht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
