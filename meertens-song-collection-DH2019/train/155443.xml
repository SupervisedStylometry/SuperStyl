<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>155443</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Iets moet ik u Laura vragen</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DOe Godts Volck haer kracht in <rhyme label="a" type="f">spande</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Om den vyandt aen te <rhyme label="a" type="f" corresp="#s1:l1">randen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ende Amaleck te <rhyme label="b" type="m">slaen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hief (in't bloedighe <rhyme label="c" type="f">gewemel</rhyme>)</l>
        <l id="s1:l5" met="4" type="+-">Mose d'handen op ten <rhyme label="c" type="f" corresp="#s1:l4">Hemel</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dan kon Isr'el boven <rhyme label="b" type="m" corresp="#s1:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maer wanneer sijn handen <rhyme label="a" type="f">laghen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Worde Jacob weer <rhyme label="a" type="f" corresp="#s2:l1">verslaghen</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">En de Vyandt viel weer <rhyme label="b" type="m">af</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Als hy die ten Hemel <rhyme label="c" type="f">streckte</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Dies hy staegh na boven <rhyme label="c" type="f" corresp="#s2:l4">reckte</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Tot dat hem de kracht <rhyme label="b" type="m" corresp="#s2:l3">begaf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doe hem nu de Zen'wen <rhyme label="a" type="f">slapten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En des vyandts voeten <rhyme label="a" type="f" corresp="#s3:l1">trapten</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Trotsigh op 't besneden <rhyme label="b" type="m">Heyr</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Deden twee van sijne <rhyme label="c" type="f">Maghen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Hem de handen <rhyme label="c" type="f" corresp="#s3:l4">onderschraghen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Tot den vyandt stordt ter <rhyme label="b" type="m" corresp="#s3:l3">neer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Wanneer wy in vast <rhyme label="a" type="f">betrouwen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Met geloov'ge ooghen, <rhyme label="a" type="f" corresp="#s4:l1">schouwen</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Godt aen, door de Heem'len <rhyme label="b" type="m">heen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Sullen wy door het <rhyme label="c" type="f">Gheloove</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">'t Vleys, en oock de Wereldt, <rhyme label="c" type="f" corresp="#s4:l4">boven</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Op haer ys'ren necken <rhyme label="b" type="m" corresp="#s4:l3">treÃªn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Doch wanneer 't geloove <rhyme label="a" type="f">flauwer</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Aen den vyandt, licht den <rhyme label="a" type="f" corresp="#s5:l1">Lauwer</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Laten soud, soo wordt <rhyme label="b" type="m">nochtans</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Het gestijft, in <rhyme label="c" type="f">moeylijckheden</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Door Gods Woordt, en door <rhyme label="c" type="f" corresp="#s5:l4">Ghebeden</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En verkrijght soo <rhyme label="b" type="m" corresp="#s5:l3">d'Eeren-krans</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
