<song>
  <meta>
    <id>20879</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>O herders al zoetjes al zonder getier</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">KOmt neder op d' aerde, O Hemelsche <rhyme label="a" type="m">hof</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En singht Mariaes <rhyme label="a" type="m" corresp="#s1:l1">lof</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Siet hoe die Maget <rhyme label="b" type="m">baert</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Uw' Koningh op dees <rhyme label="b" type="m" corresp="#s1:l3">Aerd</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">In 't Bethlemijtsche <rhyme label="c" type="m">stal</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Den Schepper van ons <rhyme label="c" type="m" corresp="#s1:l5">all'</rhyme>.</l>
        <l id="s1:l7" met="5" type="-+">O Eng'len! Eng'len! doet aen Haer uw <rhyme label="d" type="m">groet</rhyme>:</l>
        <l id="s1:l8" met="5" type="-+">O Eng'len! valt uw' Koningin te <rhyme label="d" type="m" corresp="#s1:l7">voet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Komt neder op d' aerde, komt dichter wat <rhyme label="a" type="m">by</rhyme></l>
        <l id="s2:l2" met="3" type="-+">O hemels <rhyme label="a" type="m" corresp="#s2:l1">Heerschappy</rhyme>!</l>
        <l id="s2:l3" met="3" type="-+">U hooge Hof <rhyme label="b" type="m">verlaet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Siet wat hier <rhyme label="b" type="m" corresp="#s2:l3">ommegaet</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Wilt U toch haestich <rhyme label="c" type="m">spoen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Siet wat hier is te <rhyme label="c" type="m" corresp="#s2:l5">doen</rhyme>.</l>
        <l id="s2:l7" met="5" type="-+">Siet hoe uw' Koningh 't Mensch-geslacht <rhyme label="d" type="m">bemint</rhyme>,</l>
        <l id="s2:l8" met="5" type="-+">Dat Hy om haer geworden is een <rhyme label="d" type="m" corresp="#s2:l7">kind</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En Kindtje geworden is d' Oppersten <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Voor ons uyt liefde <rhyme label="a" type="m" corresp="#s3:l1">teer</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Maria brenght ons <rhyme label="b" type="m">voort</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Goods-Vaders eeuwigh <rhyme label="b" type="m" corresp="#s3:l3">Woord</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die ons verlossen <rhyme label="c" type="m">sal</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Uyt Adams diepen <rhyme label="c" type="m" corresp="#s3:l5">val</rhyme>.</l>
        <l id="s3:l7" met="5" type="-+">O wonder! wonder boven maten <rhyme label="d" type="m">groot</rhyme>!</l>
        <l id="s3:l8" met="5" type="-+">Dat God ons helpt uyt onse groote <rhyme label="d" type="m" corresp="#s3:l7">noot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Geluckich, en salich is die reyne <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die zoo Haer God <rhyme label="a" type="m" corresp="#s4:l1">behaeght</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat die voor d' eeuwe <rhyme label="b" type="m">waer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Verkoos tot Moeder <rhyme label="b" type="m" corresp="#s4:l3">Haer</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Wat is dat voor <rhyme label="c" type="m">geluck</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Voor Haer, en onsen <rhyme label="c" type="m" corresp="#s4:l5">druck</rhyme>!</l>
        <l id="s4:l7" met="5" type="-+">O waerde Moeder Goods! O Godes <rhyme label="d" type="m">Bruyd</rhyme>!</l>
        <l id="s4:l8" met="5" type="-+">En Godes Dochter! Ghy in all's munt <rhyme label="d" type="m" corresp="#s4:l7">uyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Och of ick eens gracy by, U, Maget, <rhyme label="a" type="m">vondt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">'k Sou worden haest <rhyme label="a" type="m" corresp="#s5:l1">gesont</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Gesont van mijn <rhyme label="b" type="m">misdaed</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Gesont van alle <rhyme label="b" type="m" corresp="#s5:l3">quaed</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Want wien Ghy zijt <rhyme label="c" type="m">gesint</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die wordt van God <rhyme label="c" type="m" corresp="#s5:l5">bemint</rhyme>:</l>
        <l id="s5:l7" met="5" type="-+">Want Ghy als Godes Moeder veel <rhyme label="d" type="m">vermenght</rhyme>,</l>
        <l id="s5:l8" met="5" type="-+">Die hemel ende Aerde hebt <rhyme label="d" type="m" corresp="#s5:l7">verheught</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dies waerdicht te bidden, O edelste <rhyme label="a" type="m">Vrouw</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">Voor mijnder ziels <rhyme label="a" type="m" corresp="#s6:l1">behouw</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">O Vrouw van grooter <rhyme label="b" type="m">macht</rhyme>!</l>
        <l id="s6:l4" met="3" type="-+">O Vrouw van grooter <rhyme label="b" type="m" corresp="#s6:l3">kracht</rhyme>!</l>
        <l id="s6:l5" met="3" type="-+">Bevrijdt my van de <rhyme label="c" type="m">Hell'</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Besorght my 't eeuw'ge <rhyme label="c" type="m" corresp="#s6:l5">Wel</rhyme>.</l>
        <l id="s6:l7" met="5" type="-+">Dit kond Ghy (bloem en Pronck des Hemels <rhyme label="d" type="m">throon</rhyme>!)</l>
        <l id="s6:l8" met="5" type="-+">Soo 't U belieft, verkrijgen van uw' <rhyme label="d" type="m" corresp="#s6:l7">Soon</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Och of ick behagen, Goods Moeder! U <rhyme label="a" type="m">kon</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">U, schoonder als de <rhyme label="a" type="m" corresp="#s7:l1">Sonn'</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">'k Sal doen al wat ick <rhyme label="b" type="m">kan</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Neemt in uw' dienst my <rhyme label="b" type="m" corresp="#s7:l3">an</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">'k Sal doen al wat ick <rhyme label="c" type="m">mach</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Tot mijnen laetsten <rhyme label="c" type="m" corresp="#s7:l5">dagh</rhyme>.</l>
        <l id="s7:l7" met="5" type="-+">Maer of door kranckheyd ick weerom <rhyme label="d" type="m">verviel</rhyme>,</l>
        <l id="s7:l8" met="5" type="-+">Gedachtich zijt 't ontfermen mijne <rhyme label="d" type="m" corresp="#s7:l7">ziel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
