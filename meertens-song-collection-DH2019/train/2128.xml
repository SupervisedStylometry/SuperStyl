    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Echo wil ik tergen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Daniel Bellemans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">GOdt woont by de <rhyme label="a" type="f">menschen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Alles gaet naer <rhyme label="a" type="f" corresp="#s1:l1">wenschen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">'t Lam weydt by den <rhyme label="b" type="m">Leeuw</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">'t Is een gulden <rhyme label="b" type="m" corresp="#s1:l3">eeuw</rhyme>,</l>
        <l id="s1:l5" met="1" type="-+">Den <rhyme label="c" type="f">Vrede</rhyme></l>
        <l id="s1:l6" met="1" type="-+">Wordt <rhyme label="c" type="f" corresp="#s1:l5">heden</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Aen den mensch <rhyme label="d" type="f">verkondight</rhyme></l>
        <l id="s1:l8" met="3" type="+-">In het <rhyme label="e" type="m">Kribbeken</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Door een teer <rhyme label="d" type="f" corresp="#s1:l7">onmondigh</rhyme></l>
        <l id="s1:l10" met="3" type="+-">En soet <rhyme label="e" type="m" corresp="#s1:l8">Kindeken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Siet den Prins van <rhyme label="a" type="f">Vrede</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Brenght den Vrede <rhyme label="a" type="f" corresp="#s2:l1">mede</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Uyt syn <rhyme label="b" type="m">Hemel-ryck</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Op het <rhyme label="b" type="m" corresp="#s2:l3">Aerde-ryck</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">Die <rhyme label="c" type="f">reyne</rhyme></l>
        <l id="s2:l6" met="1" type="-+"><rhyme label="c" type="f" corresp="#s2:l5">Fonteyne</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Die schoon klaer <rhyme label="d" type="f">riviere</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Komt naer onsen ey <rhyme label="e" type="m">sch</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Naer 't dorr' Landt toe <rhyme label="d" type="f" corresp="#s2:l7">swieren</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Uyt het <rhyme label="e" type="m" corresp="#s2:l8">Paradys</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Noë komt <rhyme label="a" type="f">ghetreden</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Met d'Olyf van <rhyme label="a" type="f" corresp="#s3:l1">vrede</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Uyt de Arck met <rhyme label="b" type="m">spoedt</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Naer den <rhyme label="b" type="m" corresp="#s3:l3">water-vloedt</rhyme>,</l>
        <l id="s3:l5" met="1" type="-+">'t Schoon <rhyme label="c" type="f">weder</rhyme></l>
        <l id="s3:l6" met="1" type="-+">Komt <rhyme label="c" type="f" corresp="#s3:l5">weder</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Uyt de Maeght <rhyme label="d" type="f">Maria</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Uyt die suyver <rhyme label="e" type="m">Arck</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Komdy ô <rhyme label="d" type="f" corresp="#s3:l7">Messia</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Troosten d'Hey l'ghe <rhyme label="e" type="m" corresp="#s3:l8">Karck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
