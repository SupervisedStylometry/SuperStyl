  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Drinken wy eens vrindschap <rhyme label="a" type="f">t'samen</rhyme>,</l>
        <l id="s1:l2" met="8" type="+-">Schenk, schenk, schenk, schenk, schenk, schenk, schenk, <rhyme label="b" type="m">schenk</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Dat 's een dronkje naar <rhyme label="a" type="f" corresp="#s1:l1">betamen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat zich niemant hoeft te <rhyme label="a" type="f" corresp="#s1:l1">schamen</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">Maar aan ons <rhyme label="c" type="f">allen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Maar aan ons <rhyme label="c" type="f" corresp="#s1:l5">allen</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Zal gewis <rhyme label="c" type="f" corresp="#s1:l5">gevallen</rhyme>.</l>
        <l id="s1:l8" met="2" type="-+">Maar aan ons <rhyme label="c" type="f" corresp="#s1:l5">allen</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Maar aan ons <rhyme label="c" type="f" corresp="#s1:l5">allen</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Zal gewis <rhyme label="c" type="f" corresp="#s1:l5">gevallen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Is een dronkje van veel <rhyme label="a" type="f">waarden</rhyme>,</l>
        <l id="s2:l2" met="8" type="+-">Klink, klink, klink, klink, klink, klink, klink, <rhyme label="b" type="m">klink</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Vrindschap is het eÃªlst' op <rhyme label="a" type="f" corresp="#s2:l1">aarden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Nimmer 't Goud haar <rhyme label="a" type="f" corresp="#s2:l1">eevenaarden</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">Vivat de <rhyme label="c" type="f">Vrindschap</rhyme>!</l>
        <l id="s2:l6" met="2" type="-+">Vivat de <rhyme label="c" type="f" corresp="#s2:l5">Vrindschap</rhyme>!</l>
        <l id="s2:l7" met="3" type="+-">Zy klimm' tot den hoogsten <rhyme label="c" type="f" corresp="#s2:l5">trap</rhyme>.</l>
        <l id="s2:l8" met="2" type="-+">Vivat de <rhyme label="c" type="f" corresp="#s2:l5">Vrindschap</rhyme>!</l>
        <l id="s2:l9" met="2" type="-+">Vivat de <rhyme label="c" type="f" corresp="#s2:l5">Vrindschap</rhyme>!</l>
        <l id="s2:l10" met="3" type="+-">Zy klimm' tot den hoogsten <rhyme label="c" type="f" corresp="#s2:l5">trap</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laat ons lang in Vrindschap <rhyme label="a" type="f">leven</rhyme>;</l>
        <l id="s3:l2" met="8" type="+-">Drink, drink, drink, drink, drink, drink, drink, <rhyme label="b" type="m">drink</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">'k Wensch dat ons nooit <rhyme label="a" type="f" corresp="#s3:l1">tegenstreven</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Stoff' tot vyandschap mag <rhyme label="a" type="f" corresp="#s3:l1">geven</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Maar dat wy <rhyme label="c" type="f">rustig</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Maar dat wy <rhyme label="c" type="f" corresp="#s3:l5">rustig</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Leven bly en <rhyme label="c" type="f" corresp="#s3:l5">lustig</rhyme>.</l>
        <l id="s3:l8" met="2" type="-+">Maar dat wy <rhyme label="c" type="f" corresp="#s3:l5">rustig</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Maar dat wy <rhyme label="c" type="f" corresp="#s3:l5">rustig</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Leven bly en <rhyme label="c" type="f" corresp="#s3:l5">lustig</rhyme>.</l>
      </lg>
    </lg>
