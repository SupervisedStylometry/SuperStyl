<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19184</id>
    <date>
      <year-start>1611</year-start>
      <year-end>1611</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
