    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Guilielmus Bolognino</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">OOrloft, Elia, jonstich <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat ick dit lof-dicht singh u <rhyme label="a" type="m" corresp="#s1:l1">bly</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Met eenen vierighen <rhyme label="b" type="f">waghen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Nae't Paradijs ghevoert die <rhyme label="c" type="m">zijt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ghebleven maeght in dien <rhyme label="c" type="m" corresp="#s1:l4">tijdt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">En u verwacht de joncxste <rhyme label="b" type="f" corresp="#s1:l3">daghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Om dan met Enoch te <rhyme label="a" type="m">weerstaen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Anti-christ, en met u <rhyme label="a" type="m" corresp="#s2:l1">vermaen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">'t Christenen volck te <rhyme label="b" type="f">verstercken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die in volmaecktheyt uyt hier <rhyme label="c" type="m">staeckt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">En in Godts iever hebt <rhyme label="c" type="m" corresp="#s2:l4">gheblaeckt</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Uyt'-ghemunt in wonder <rhyme label="b" type="f" corresp="#s2:l3">wercken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dry jaer en half door u <rhyme label="a" type="m">ghebedt</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Hebdy het hemels nat <rhyme label="a" type="m" corresp="#s3:l1">belet</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">'t Landt van ghebreck ghingh <rhyme label="b" type="f">verloren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maer door u bed' heeft weer de <rhyme label="c" type="m">locht</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Planteyt den reghen <rhyme label="c" type="m" corresp="#s3:l4">voortghebrocht</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Dat 't al groeyden, als te <rhyme label="b" type="f" corresp="#s3:l3">voren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">G' hebt teghen Baal u <rhyme label="a" type="m">ghebedt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En door de cracht van u <rhyme label="a" type="m" corresp="#s4:l1">ghebedt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">'t Vier uyt den hemel doen <rhyme label="b" type="f">comen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">En Baals Priesters hebdy <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Vergadert daer in groot <rhyme label="c" type="m" corresp="#s4:l4">ghetal</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">'t Leven met het sweert <rhyme label="b" type="f" corresp="#s4:l3">ghenomen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'t Vier, uyt den hemel noch daer <rhyme label="a" type="m">naer</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Tweemaels ghevallen, heeft te <rhyme label="a" type="m" corresp="#s5:l1">gaer</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Nae u begheerte, <rhyme label="b" type="f">verslonden</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Meer als een hondert op den <rhyme label="c" type="m">voet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ghecomen met een trots <rhyme label="c" type="m" corresp="#s5:l4">ghemoet</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Van Ochosias <rhyme label="b" type="f" corresp="#s5:l3">ghesonden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">G' hebt door 't ghebedt fris en <rhyme label="a" type="m">ghesont</rhyme></l>
        <l id="s6:l2" met="4" type="-+">De Sareptane weef <rhyme label="a" type="m" corresp="#s6:l1">ghejont</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Haer eenich kindt, nu <rhyme label="b" type="f">ghestorven</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Waer ghy, aen de stadt <rhyme label="c" type="m">ontmoet</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">(Eerst langh van een raef <rhyme label="c" type="m" corresp="#s6:l4">ghevoet</rhyme>)</l>
        <l id="s6:l6" met="4" type="-+">Had een coecksken van <rhyme label="b" type="f" corresp="#s6:l3">verworven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">U oock den Enghel heeft <rhyme label="a" type="m">besocht</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">In uwen noodt u broodt <rhyme label="a" type="m" corresp="#s7:l1">ghebrocht</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Onder 'nen boom schier <rhyme label="b" type="f">besweken</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Van swaer verdriet, en groot <rhyme label="c" type="m">gheducht</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Uyt Jesabels vrees <rhyme label="c" type="m" corresp="#s7:l4">ghevlucht</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Die haer Priest'ren doodt wou <rhyme label="b" type="f" corresp="#s7:l3">wreken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">G' hebt in Carmelo op <rhyme label="a" type="m">gherecht</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Een nieuw Religie, die <rhyme label="a" type="m" corresp="#s8:l1">oprecht</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Diende den Heer daer der <rhyme label="b" type="f">heeren</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Die onder u noch heden <rhyme label="c" type="m">bloeit</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En in ghetal, in deuchden <rhyme label="c" type="m" corresp="#s8:l4">groeit</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Sal de glorie Godts <rhyme label="b" type="f" corresp="#s8:l3">vermeeren</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Uyt 't Paradijs 'nen brief <rhyme label="a" type="m">hebdy</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Joram den Koninck (om dat <rhyme label="a" type="m" corresp="#s9:l1">hy</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Godt seer vergramt had) <rhyme label="b" type="f">gheschreven</rhyme>:</l>
        <l id="s9:l4" met="4" type="-+">Ons oock, u dienaers, niet <rhyme label="c" type="m">vergheet</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Staet ons daer by met u <rhyme label="c" type="m" corresp="#s9:l4">ghebedt</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Dat wy eewich met u <rhyme label="b" type="f" corresp="#s9:l3">leven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
