<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160285</id>
    <date>
      <year-start>1661</year-start>
      <year-end>1661</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nadien uw goddelijkheid</melody>
  <author>Cornelis Maertsz.</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MAer doe ons <rhyme label="a" type="m">Vaderlandt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dus tegen Godt aen <rhyme label="a" type="m" corresp="#s1:l1">kant</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En terghd het alle <rhyme label="b" type="f">dagen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Vergrimde oock de <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En sondt sijn harde <rhyme label="b" type="f" corresp="#s1:l3">plagen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Op dese Landen <rhyme label="c" type="m" corresp="#s1:l4">neer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De eerste trof de <rhyme label="a" type="m">Staet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van Hollandts hooghste <rhyme label="a" type="m" corresp="#s2:l1">Raet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Soo dat het Hof der <rhyme label="b" type="f">Grooten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Verwerde in't <rhyme label="c" type="m">geheel</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En worde <rhyme label="b" type="f" corresp="#s2:l3">overgoten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Met twistende <rhyme label="c" type="m" corresp="#s2:l4">krackeel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wt dese twist soo <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Een droeve <rhyme label="a" type="m" corresp="#s3:l1">Oorloghs-vlam</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die tot den Hemel <rhyme label="b" type="f">brande</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ons Leger quam <rhyme label="c" type="m">gegaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En tast in dese <rhyme label="b" type="f" corresp="#s3:l3">Lande</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ons eygen Koop-Stadt <rhyme label="c" type="m" corresp="#s3:l4">aen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hier na wat anders <rhyme label="a" type="m">quam</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Want den <rhyme label="a" type="m" corresp="#s4:l1">Oranjen-stam</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Wt 't edel Hof <rhyme label="b" type="f">Nassouwen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die al het Landt <rhyme label="c" type="m">verheught</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Wordt schielijck <rhyme label="b" type="f" corresp="#s4:l3">omgehouwen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In 't beste van sijn <rhyme label="c" type="m" corresp="#s4:l4">jeught</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De woeste Zee, oock <rhyme label="a" type="m">wordt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Door stercke Storm <rhyme label="a" type="m" corresp="#s5:l1">gheport</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die met sijn grijse <rhyme label="b" type="f">golven</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Al schuymende <rhyme label="c" type="m">aen-koomt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En doet de Dijck <rhyme label="b" type="f" corresp="#s5:l3">omtolven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Soo dat hy <rhyme label="c" type="m" corresp="#s5:l4">omme-stroomt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Godt sondt sijn plaghen <rhyme label="a" type="m">mee</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Tot onder 't domme <rhyme label="a" type="m" corresp="#s6:l1">Vee</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dat meest vergingh in <rhyme label="b" type="f">quellingh</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En schier van honger <rhyme label="c" type="m">storf</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Door dien een vreemde <rhyme label="b" type="f" corresp="#s6:l3">swellingh</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Haer mont, en voet <rhyme label="c" type="m" corresp="#s6:l4">door-korf</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Den Hemel wordt <rhyme label="a" type="m">vergramt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Soo dat hy brandigh <rhyme label="a" type="m" corresp="#s7:l1">vlamt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En door de <rhyme label="b" type="f">Zonne-stralen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Versenghd' het gras, en <rhyme label="c" type="m">kruyt</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Men sagh gheen Reghen <rhyme label="b" type="f" corresp="#s7:l3">dalen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Ter Hemel-sluysen <rhyme label="c" type="m" corresp="#s7:l4">uyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oock vanght een Oorlogh <rhyme label="a" type="m">an</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met ons, en d' <rhyme label="a" type="m" corresp="#s8:l1">Engels-man</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Die met een swerm van <rhyme label="b" type="f">Schepen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Bedeckt de bracke <rhyme label="c" type="m">Zee</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Daer mee hy ons geeft <rhyme label="b" type="f" corresp="#s8:l3">nepen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Die 't hert des Landts doen <rhyme label="c" type="m" corresp="#s8:l4">wee</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Want hier door worde <rhyme label="a" type="m">strack</rhyme></l>
        <l id="s9:l2" met="3" type="-+">De neeringh doodtlijck <rhyme label="a" type="m" corresp="#s9:l1">swack</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ja storf ten langhe <rhyme label="b" type="f">lesten</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En blies het leven <rhyme label="c" type="m">uyt</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Want 't goedt van ons <rhyme label="b" type="f" corresp="#s9:l3">gheweste</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Streckt d' Engels-man tot <rhyme label="c" type="m" corresp="#s9:l4">buyt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">De Doodt oock Godts <rhyme label="a" type="m">Dienst-knecht</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Die sijn bevel <rhyme label="a" type="m" corresp="#s10:l1">uyt-recht</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Schoot veel vergifte <rhyme label="b" type="f">Pijlen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Tot in des Menschen <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Soo dat in korte <rhyme label="b" type="f" corresp="#s10:l3">wijlen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Veel volck begraven <rhyme label="c" type="m" corresp="#s10:l4">werdt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">En eer dit Jaer was <rhyme label="a" type="m">uyt</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Quam 's avonts in het <rhyme label="a" type="m" corresp="#s11:l1">Zuydt</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Eenen Komeet te <rhyme label="b" type="f">vooren</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Die scheen ons te <rhyme label="c" type="m">bedien</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Dat Godt sijn strenge <rhyme label="b" type="f" corresp="#s11:l3">tooren</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Noch meer woud' laten <rhyme label="c" type="m" corresp="#s11:l4">sien</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">En 't is soo oock <rhyme label="a" type="m">geschiedt</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Een Jaer dat met <rhyme label="a" type="m" corresp="#s12:l1">verdriet</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Was vol gepropt, quam <rhyme label="b" type="f">volgen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En heeft door <rhyme label="c" type="m">tegenspoet</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Seer hongerigh <rhyme label="b" type="f" corresp="#s12:l3">verswolghen</rhyme></l>
        <l id="s12:l6" met="3" type="-+">De rest van Hollandts <rhyme label="c" type="m" corresp="#s12:l4">goedt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ick 't alles niet <rhyme label="a" type="m">verhael</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Maer Tromp, ons <rhyme label="a" type="m" corresp="#s13:l1">Admirael</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">De Paerle van de <rhyme label="b" type="f">Grooten</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Verlooren wy doe <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Door d' Engels-man <rhyme label="b" type="f" corresp="#s13:l3">geschoten</rhyme></l>
        <l id="s13:l6" met="3" type="-+">By Egmont in de <rhyme label="c" type="m" corresp="#s13:l4">Zee</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Oock word in 't Landt <rhyme label="a" type="m">gestiert</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Een schadelijck <rhyme label="a" type="m" corresp="#s14:l1">Gediert</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Veel duysenden van <rhyme label="b" type="f">Muysen</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">In een ontelb're <rhyme label="c" type="m">som</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Die komen 't Gras af <rhyme label="b" type="f" corresp="#s14:l3">pluysen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">En wroeten 't Landt voort <rhyme label="c" type="m" corresp="#s14:l4">om</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Den Hemel sagh dit <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">En schoot een vloedt van <rhyme label="a" type="m" corresp="#s15:l1">traen</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Al wenend' uyt de <rhyme label="b" type="f">Oogen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">En maeckt van 't Landt een <rhyme label="c" type="m">Zee</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Soo dat de Boeren <rhyme label="b" type="f" corresp="#s15:l3">togen</rhyme></l>
        <l id="s15:l6" met="3" type="-+">Nae huys toe met haer <rhyme label="c" type="m" corresp="#s15:l4">Vee</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Daer oock een Storm <rhyme label="a" type="m">ontstack</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Die menigh Huys <rhyme label="a" type="m" corresp="#s16:l1">verbrack</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Veel Schepen sijn <rhyme label="b" type="f">verdorven</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Die op de woeste <rhyme label="c" type="m">Zee</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Haer spitse Masten <rhyme label="b" type="f" corresp="#s16:l3">korven</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">En noch vergingen <rhyme label="c" type="m" corresp="#s16:l4">mee</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">En in dees storm by <rhyme label="a" type="m">nacht</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">Daer niet een mensch op <rhyme label="a" type="m" corresp="#s17:l1">dacht</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Soo quam de brandt <rhyme label="b" type="f">verraschen</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">De Rijp, dat heerlijck <rhyme label="c" type="m">Dorp</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Soo dat het in der <rhyme label="b" type="f" corresp="#s17:l3">asschen</rhyme></l>
        <l id="s17:l6" met="3" type="-+">In korten neder <rhyme label="c" type="m" corresp="#s17:l4">worp</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">d' Hollandtsche <rhyme label="a" type="m">Wapen-schuer</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Vol Buskruyt, springht door <rhyme label="a" type="m" corresp="#s18:l1">vuer</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">En stort Delft mede <rhyme label="b" type="f">onder</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Vermorselt in het <rhyme label="c" type="m">gruys</rhyme> !</l>
        <l id="s18:l5" met="3" type="-+">Men hoord, O schricklijck <rhyme label="b" type="f" corresp="#s18:l3">wonder</rhyme> !</l>
        <l id="s18:l6" met="3" type="-+">Tot Tessel het <rhyme label="c" type="m" corresp="#s18:l4">ghedruys</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Godt stiert oock weer in 't <rhyme label="a" type="m">Landt</rhyme></l>
        <l id="s19:l2" met="3" type="-+">De Pest, die door haer <rhyme label="a" type="m" corresp="#s19:l1">Brandt</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Veel Menschen doet <rhyme label="b" type="f">versengen</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Als stoppelen door 't <rhyme label="c" type="m">vuer</rhyme>;</l>
        <l id="s19:l5" met="3" type="-+">Die 't Lijck na 't Graf gaet <rhyme label="b" type="f" corresp="#s19:l3">brengen</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Is 't Morghen self sijn <rhyme label="c" type="m" corresp="#s19:l4">uur</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Het Rijck van <rhyme label="a" type="m">Portugael</rhyme>,</l>
        <l id="s20:l2" met="3" type="-+">Vat oock het Sweert van <rhyme label="a" type="m" corresp="#s20:l1">stael</rhyme></l>
        <l id="s20:l3" met="3" type="-+">En scherpt het op dees' <rhyme label="b" type="f">Landen</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">Daer mee het in dees <rhyme label="c" type="m">tijdt</rhyme></l>
        <l id="s20:l5" met="3" type="-+">Ons Schepen gaet <rhyme label="b" type="f" corresp="#s20:l3">aenranden</rhyme></l>
        <l id="s20:l6" met="3" type="-+">Die wy dus worden <rhyme label="c" type="m" corresp="#s20:l4">quijt</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Daer komen <rhyme label="a" type="m">wederom</rhyme>,</l>
        <l id="s21:l2" met="3" type="-+">In een ontelb're <rhyme label="a" type="m" corresp="#s21:l1">som</rhyme></l>
        <l id="s21:l3" met="3" type="-+">De Muysen, en <rhyme label="b" type="f">af-knauwen</rhyme></l>
        <l id="s21:l4" met="3" type="-+">De Vruchten van het <rhyme label="c" type="m">Velt</rhyme>,</l>
        <l id="s21:l5" met="3" type="-+">Daer door zy 't Vee <rhyme label="b" type="f" corresp="#s21:l3">benauwen</rhyme></l>
        <l id="s21:l6" met="3" type="-+">En stroopen 't <rhyme label="c" type="m" corresp="#s21:l4">Boere-Geldt</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">De plaegh wordt weer <rhyme label="a" type="m">versterckt</rhyme>,</l>
        <l id="s22:l2" met="3" type="-+">Doe <rhyme label="a" type="m" corresp="#s22:l1">Groote-Broecker-Merckt</rhyme></l>
        <l id="s22:l3" met="3" type="-+">Het hert des Stadts <rhyme label="b" type="f">verbrande</rhyme>;</l>
        <l id="s22:l4" met="3" type="-+">De Kermis Kerm-tijdt <rhyme label="c" type="m">was</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Eerst vol van vuyle <rhyme label="b" type="f" corresp="#s22:l3">schansen</rhyme>,</l>
        <l id="s22:l6" met="3" type="-+">Daer na vol vuylen <rhyme label="c" type="m" corresp="#s22:l4">asch</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">Noch quam oock sulcken <rhyme label="a" type="m">Storm</rhyme>,</l>
        <l id="s23:l2" met="3" type="-+">Daer om gants Hollandt <rhyme label="a" type="m" corresp="#s23:l1">korm</rhyme>;</l>
        <l id="s23:l3" met="3" type="-+">Dewijl men sagh <rhyme label="b" type="f">vernielen</rhyme></l>
        <l id="s23:l4" met="3" type="-+">De vruchten op het <rhyme label="c" type="m">Veldt</rhyme>,</l>
        <l id="s23:l5" met="3" type="-+">Veel Boomen neder <rhyme label="b" type="f" corresp="#s23:l3">vielen</rhyme>,</l>
        <l id="s23:l6" met="3" type="-+">Door 't wonderlijck <rhyme label="c" type="m" corresp="#s23:l4">geweldt</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">De Koningen in 't <rhyme label="a" type="m">Noordt</rhyme>,</l>
        <l id="s24:l2" met="3" type="-+">Sijn heftighlijck <rhyme label="a" type="m" corresp="#s24:l1">verstoort</rhyme>,</l>
        <l id="s24:l3" met="3" type="-+">Haer twist kost ons <rhyme label="b" type="f">welvaren</rhyme>;</l>
        <l id="s24:l4" met="3" type="-+">En wat ons Scheeps-vloot <rhyme label="c" type="m">doet</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">Ten kan gheen Lants-heyl <rhyme label="b" type="f" corresp="#s24:l3">baren</rhyme>,</l>
        <l id="s24:l6" met="3" type="-+">Ons macht stort onder <rhyme label="c" type="m" corresp="#s24:l4">voet</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">De Hemel sloot haer <rhyme label="a" type="m">nat</rhyme></l>
        <l id="s25:l2" met="3" type="-+">In 't <rhyme label="a" type="m" corresp="#s25:l1">Opper-Water-Vat</rhyme>,</l>
        <l id="s25:l3" met="3" type="-+">De Wolcken worden <rhyme label="b" type="f">Koper</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">De Aerde scheur aen <rhyme label="c" type="m">scheur</rhyme>,</l>
        <l id="s25:l5" met="3" type="-+">De <rhyme label="b" type="f" corresp="#s25:l3">Sonne-stralen-looper</rhyme></l>
        <l id="s25:l6" met="3" type="-+">Tot aen den Af-grondt <rhyme label="c" type="m" corresp="#s25:l4">deur</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">Het loveloose <rhyme label="a" type="m">Kruydt</rhyme>,</l>
        <l id="s26:l2" met="3" type="-+">Brandt door de hitten <rhyme label="a" type="m" corresp="#s26:l1">uyt</rhyme>;</l>
        <l id="s26:l3" met="3" type="-+">De Boomen staen' <rhyme label="b" type="f">geladen</rhyme></l>
        <l id="s26:l4" met="3" type="-+">Vol stinckend' <rhyme label="c" type="m">ongediert</rhyme>,</l>
        <l id="s26:l5" met="3" type="-+">Ontbloodt van vrucht en <rhyme label="b" type="f" corresp="#s26:l3">bladen</rhyme>;</l>
        <l id="s26:l6" met="3" type="-+">De Wereldt staet <rhyme label="c" type="m" corresp="#s26:l4">ontciert</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="3" type="-+">Maer d' <rhyme label="a" type="m">Opper-Water-Born</rhyme></l>
        <l id="s27:l2" met="3" type="-+">Barst los, als door een <rhyme label="a" type="m" corresp="#s27:l1">torn</rhyme>,</l>
        <l id="s27:l3" met="3" type="-+">En slist door haer <rhyme label="b" type="f">geklater</rhyme></l>
        <l id="s27:l4" met="3" type="-+">In korten 's Wereldts <rhyme label="c" type="m">brandt</rhyme>,</l>
        <l id="s27:l5" met="3" type="-+">En spoelt het Vee door 't <rhyme label="b" type="f" corresp="#s27:l3">Water</rhyme></l>
        <l id="s27:l6" met="3" type="-+">Van het verschreyde <rhyme label="c" type="m" corresp="#s27:l4">Landt</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="3" type="-+">De Wint van Toom <rhyme label="a" type="m">beroyt</rhyme>,</l>
        <l id="s28:l2" met="3" type="-+">Vaert woester uyt als <rhyme label="a" type="m" corresp="#s28:l1">oyt</rhyme></l>
        <l id="s28:l3" met="3" type="-+">Toorn, Huys, en Boom, moet <rhyme label="b" type="f">wijcken</rhyme></l>
        <l id="s28:l4" met="3" type="-+">'t Is grau-bont op de <rhyme label="c" type="m">Zee</rhyme></l>
        <l id="s28:l5" met="3" type="-+">Van Wracken, en van <rhyme label="b" type="f" corresp="#s28:l3">Lijcken</rhyme>;</l>
        <l id="s28:l6" met="3" type="-+">'s Landts stem is, Och! en <rhyme label="c" type="m" corresp="#s28:l4">Wee'</rhyme></l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="3" type="-+">Soo dat ons dagh aen <rhyme label="a" type="m">dagh</rhyme></l>
        <l id="s29:l2" met="3" type="-+">Aentreffen slagh op <rhyme label="a" type="m" corresp="#s29:l1">slagh</rhyme>.</l>
        <l id="s29:l3" met="3" type="-+">O gruwelijcke <rhyme label="b" type="f">sonden</rhyme>!</l>
        <l id="s29:l4" met="3" type="-+">Ghy baert door-galde <rhyme label="c" type="m">vrucht</rhyme>.</l>
        <l id="s29:l5" met="3" type="-+">Om u slaet Godt die <rhyme label="b" type="f" corresp="#s29:l3">wonden</rhyme></l>
        <l id="s29:l6" met="3" type="-+">Daer 't Landt nu dus om <rhyme label="c" type="m" corresp="#s29:l4">sucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
