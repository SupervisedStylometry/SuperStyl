    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Florida zo het wezen mag</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">KOmt nu ghy Sangeressen <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En toont ons hier een bly <rhyme label="a" type="m" corresp="#s1:l1">geschal</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Al van ‘t vreughdige <rhyme label="b" type="f">Sparen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Daer ghy soo dickmaels met <rhyme label="c" type="m">vermaeck</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van d’Echo hoort u <rhyme label="c" type="m" corresp="#s1:l4">spraeck</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En u gespeel van <rhyme label="b" type="f" corresp="#s1:l3">snaren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het Tweede Deel van Sparens <rhyme label="a" type="m">Vreught</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Siet dat vry door, met goe <rhyme label="a" type="m" corresp="#s2:l1">geneught</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En schept daer in <rhyme label="b" type="f">behagen</rhyme>:</l>
        <l id="s2:l4" met="4" type="-+">Want ick het niet naer laten <rhyme label="c" type="m">kon</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dit vreughdigh Sparens <rhyme label="c" type="m" corresp="#s2:l4">Bron</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Mijn liefde toe te <rhyme label="b" type="f" corresp="#s2:l3">draghen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die ick bewesen heb met <rhyme label="a" type="m">gunst</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Dus bid ick u, dat ghy de <rhyme label="a" type="m" corresp="#s3:l1">kunst</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Al t’samen wilt <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Op datse door ons gantsche <rhyme label="c" type="m">Landt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Magh worden wel <rhyme label="c" type="m" corresp="#s3:l4">bekant</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En oock niet zijn <rhyme label="b" type="f" corresp="#s3:l3">verdreven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maer laet die met een goedt <rhyme label="a" type="m">geluydt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Klincken tot heldre keelen <rhyme label="a" type="m" corresp="#s4:l1">uyt</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Soo sal ons Bron <rhyme label="b" type="f">ghepresen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Worden van alle menschen <rhyme label="c" type="m">schier</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die dan by Wijn of <rhyme label="c" type="m" corresp="#s4:l4">Bier</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Het zinghen ofte <rhyme label="b" type="f" corresp="#s4:l3">lesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die hier vermaeck in vinden <rhyme label="a" type="m">kan</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Die komt by onsen <rhyme label="a" type="m" corresp="#s5:l1">SEGERMAN</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En wil sijn geldt <rhyme label="b" type="f">besteden</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Aen ‘t Sparens Vreughden-Bron seer <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Want hy in <rhyme label="c" type="m" corresp="#s5:l4">overvloedt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">V gherieft, met goe <rhyme label="b" type="f" corresp="#s5:l3">zeden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
