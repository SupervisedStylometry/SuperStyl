    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Met deze nieuwe jaar</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Met desen nieuwen <rhyme label="a" type="f">Iare</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">So wort ons <rhyme label="a" type="f" corresp="#s1:l1">openbare</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hoe dat een Maeghet <rhyme label="a" type="f" corresp="#s1:l1">vruchtbare</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die werelt heeft <rhyme label="b" type="m">verblijt</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Gelovet moet <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s1:l5">kindekijn</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Geeret moet <rhyme label="c" type="m" corresp="#s1:l5">sijn</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s1:l5">Maeghdekijn</rhyme>,</l>
        <l id="s1:l9" met="5" type="+-">Nu een eewiglick tot aller <rhyme label="b" type="m" corresp="#s1:l4">tijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe wel was haer te <rhyme label="a" type="f">moede</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat sy in vleesch en <rhyme label="a" type="f" corresp="#s2:l1">bloede</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Aensach haers hertzen <rhyme label="a" type="f" corresp="#s2:l1">hoede</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Den Heere <rhyme label="b" type="m">gebenedijt</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Gelovet moet <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s2:l5">kindekijn</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Geeret moet <rhyme label="c" type="m" corresp="#s2:l5">sijn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s2:l5">Maeghdekijn</rhyme>,</l>
        <l id="s2:l9" met="5" type="+-">Nu een eewiglick tot aller <rhyme label="b" type="m" corresp="#s2:l4">tijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">sy baerd’ hem sonder <rhyme label="a" type="f">pijne</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En bleef een Maghet <rhyme label="a" type="f" corresp="#s3:l1">fijne</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Des sondaers <rhyme label="a" type="f" corresp="#s3:l1">medecijne</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Des hebben de Ioden <rhyme label="b" type="m">nijt</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Gelovet moet <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s3:l5">kindekijn</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Geeret moet <rhyme label="c" type="m" corresp="#s3:l5">sijn</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s3:l5">Maeghdekijn</rhyme>,</l>
        <l id="s3:l9" met="5" type="+-">Nu een eewiglick tot aller <rhyme label="b" type="m" corresp="#s3:l4">tijt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Engels songen <rhyme label="a" type="f">schone</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Glori al inden <rhyme label="a" type="f" corresp="#s4:l1">throne</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ter eeren, en ter <rhyme label="a" type="f" corresp="#s4:l1">croone</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Der kints <rhyme label="b" type="m">gebenedijt</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Gelovet moet <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s4:l5">kindekijn</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Geeret moet <rhyme label="c" type="m" corresp="#s4:l5">sijn</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s4:l5">Maeghdekijn</rhyme>,</l>
        <l id="s4:l9" met="5" type="+-">Nu een eewiglick tot aller <rhyme label="b" type="m" corresp="#s4:l4">tijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Als acht dagen waren <rhyme label="a" type="f">geleden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Doe wert Iesus <rhyme label="a" type="f" corresp="#s5:l1">besneden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Al na der Ioden <rhyme label="a" type="f" corresp="#s5:l1">seden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">D’welck ons van sonden <rhyme label="b" type="m">vryt</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Gelovet moet <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s5:l5">kindekijn</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Geeret moet <rhyme label="c" type="m" corresp="#s5:l5">sijn</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s5:l5">Maeghdekijn</rhyme>,</l>
        <l id="s5:l9" met="5" type="+-">Nu een eewiglick tot aller <rhyme label="b" type="m" corresp="#s5:l4">tijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wt Orienten <rhyme label="a" type="f">lande</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Quamen ter <rhyme label="a" type="f" corresp="#s6:l1">offerhande</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Dry Koningen <rhyme label="a" type="f" corresp="#s6:l1">onbekande</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Godt sy <rhyme label="b" type="m">gebenedijt</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Gelovet moet <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s6:l5">kindekijn</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Geeret moet <rhyme label="c" type="m" corresp="#s6:l5">sijn</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dat soete <rhyme label="c" type="m" corresp="#s6:l5">Maeghdekijn</rhyme>,</l>
        <l id="s6:l9" met="5" type="+-">Nu een eewiglick tot aller <rhyme label="b" type="m" corresp="#s6:l4">tijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
