    <melody>Het was wel eertijds</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Franciscus de Laat</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'T VVas wel eertijdts vry wat <rhyme label="a" type="f">wonders</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Dat men vryde hier en <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Maer nu isset niet <rhyme label="a" type="f" corresp="#s1:l1">besonders</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Yeder speelt dat kunsje <rhyme label="b" type="m" corresp="#s1:l2">naer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'t Is met vryen en <rhyme label="a" type="f">verkoopen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Nu al even eens <rhyme label="b" type="m">gestelt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Yeder weet 'er op te <rhyme label="a" type="f" corresp="#s2:l1">loopen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Yeder siet nae 't meeste <rhyme label="b" type="m" corresp="#s2:l2">gelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Isser yemandt <rhyme label="a" type="f">voort-gekomen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Van een treffelijck <rhyme label="b" type="m">gheslacht</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Al sijn eer werdt wech <rhyme label="a" type="f" corresp="#s3:l1">genomen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Sy en werdt niet meer <rhyme label="b" type="m" corresp="#s3:l2">geacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Soo hy niet en heeft te <rhyme label="a" type="f">tellen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">'t Vryen is een blinde <rhyme label="b" type="m">kaert</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">O let hier op <rhyme label="a" type="f" corresp="#s4:l1">jongh-ghesellen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'t Ghelt is daer men wel af <rhyme label="b" type="m" corresp="#s4:l2">vaert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Letter-kunsten zijn <rhyme label="a" type="f">vergheten</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">VVetenschap is buyten 't <rhyme label="b" type="m">berd</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">'t Ghelt kan alle dinck <rhyme label="a" type="f" corresp="#s5:l1">af-meten</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Niet en gaet 'er nu soo <rhyme label="b" type="m" corresp="#s5:l2">verd</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Eer en treffelijcke <rhyme label="a" type="f">zeden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">VVaren eertijdts wel <rhyme label="b" type="m">bemint</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Maer nu werdense <rhyme label="a" type="f" corresp="#s6:l1">vertreden</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Van het blinde Venus <rhyme label="b" type="m" corresp="#s6:l2">Kindt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Loopt ghy hier of elders <rhyme label="a" type="f">henen</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Nae de steden nae het <rhyme label="b" type="m">veldt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Ghy komt weÃªr met blaeuwe <rhyme label="a" type="f" corresp="#s7:l1">scheenen</rhyme>;</l>
        <l id="s7:l4" met="4" type="+-">Niet en krijght ghy sonder <rhyme label="b" type="m" corresp="#s7:l2">gheldt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
