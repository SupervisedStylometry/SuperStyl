    <melody>Psalm 003 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Philips van Marnix</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">OCh Heer hoe menich <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die my doen ancxt end' <rhyme label="a" type="m" corresp="#s1:l1">pijn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">End' schier heel <rhyme label="b" type="f">t'onderdringen</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Met wat een swaer <rhyme label="c" type="m">gewelt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Beghevens' haer in't <rhyme label="c" type="m" corresp="#s1:l4">velt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Om my hardt te <rhyme label="b" type="f" corresp="#s1:l3">bespringen</rhyme>?</l>
        <l id="s1:l7" met="3" type="+-">3. Het spreken veel tot <rhyme label="d" type="m">my</rhyme>:</l>
        <l id="s1:l8" met="3" type="+-">Nu ymmers ist met <rhyme label="d" type="m" corresp="#s1:l7">dy</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Ten eynde gaer <rhyme label="e" type="f">gecomen</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Dijn siel en heeft nu <rhyme label="f" type="m">meer</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Gheen hulp aen God den <rhyme label="f" type="m" corresp="#s1:l10">Heer</rhyme>,</l>
        <l id="s1:l12" met="3" type="+-">Dijn troost is dy <rhyme label="e" type="f" corresp="#s1:l9">benomen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">* Doch du bist myne <rhyme label="a" type="m">schilt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">O Heer die du my <rhyme label="a" type="m" corresp="#s2:l1">wilt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Beschutten end' <rhyme label="b" type="f">beschermen</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Dijn handt helpt my vry <rhyme label="c" type="m">op</rhyme></l>
        <l id="s2:l5" met="3" type="-+">End' richt op mynen <rhyme label="c" type="m" corresp="#s2:l4">kop</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Spijt haer ontsinnich <rhyme label="b" type="f" corresp="#s2:l3">zwermen</rhyme>.</l>
        <l id="s2:l7" met="3" type="+-">5. Ick hebbe met <rhyme label="d" type="m">gheschal</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Mijn stem' van d'aertsche <rhyme label="d" type="m" corresp="#s2:l7">dal</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Wel meer tot Godt <rhyme label="e" type="f">verheuen</rhyme>:</l>
        <l id="s2:l10" met="3" type="+-">Oock heeft hy sonder <rhyme label="f" type="m">erg</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Wt synen heyl'ghen <rhyme label="f" type="m" corresp="#s2:l10">berg</rhyme></l>
        <l id="s2:l12" met="3" type="+-">Goed' antwoord' my <rhyme label="e" type="f" corresp="#s2:l9">ghegheuen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">* Daerom soo wil ick <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Nu neder legghen <rhyme label="a" type="m" corresp="#s3:l1">my</rhyme></l>
        <l id="s3:l3" met="3" type="-+">End' mynen slaep vry <rhyme label="b" type="f">nutten</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">End' daer nae wacker <rhyme label="c" type="m">aen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">We'er uyten slaep' <rhyme label="c" type="m" corresp="#s3:l4">opstaen</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Want God sal <rhyme label="b" type="f" corresp="#s3:l3">m'onderstutten</rhyme>.</l>
        <l id="s3:l7" met="3" type="+-">7. Ick wil niet zijn <rhyme label="d" type="m">onstelt</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Al traden daer int <rhyme label="d" type="m" corresp="#s3:l7">velt</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Veel hondert duysent <rhyme label="e" type="f">mannen</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">End' hadden my int <rhyme label="f" type="m">rondt</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Belegert: met <rhyme label="f" type="m" corresp="#s3:l10">verbondt</rhyme></l>
        <l id="s3:l12" met="3" type="+-">Hardt teghen my <rhyme label="e" type="f" corresp="#s3:l9">ghespannen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">* Stae nu dan op, o <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">End' help my lancx so <rhyme label="a" type="m" corresp="#s4:l1">meer</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Die du met straffen <rhyme label="b" type="f">handen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Op't kinnebacken <rhyme label="c" type="m">slaest</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Mijn vyanden <rhyme label="c" type="m" corresp="#s4:l4">verbaest</rhyme></l>
        <l id="s4:l6" met="3" type="-+">End' breeck' s'god'loosen <rhyme label="b" type="f" corresp="#s4:l3">tanden</rhyme>.</l>
        <l id="s4:l7" met="3" type="+-">9. Voorwaer by dy Heer <rhyme label="d" type="m">is</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Heyl ende troost <rhyme label="d" type="m" corresp="#s4:l7">ghewis</rhyme></l>
        <l id="s4:l9" met="3" type="+-">In allerley <rhyme label="e" type="f">ghevaren</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Du streckst met <rhyme label="f" type="m">ouervloet</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Dijn seghen ende <rhyme label="f" type="m" corresp="#s4:l10">spoet</rhyme></l>
        <l id="s4:l12" met="3" type="+-">Ouer dijn heyl'ghe <rhyme label="e" type="f" corresp="#s4:l9">scharen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
