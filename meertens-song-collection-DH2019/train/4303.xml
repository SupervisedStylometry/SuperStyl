<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4303</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Cupido god der minne stout</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
      <category translation="emotions">emoties</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">CVpido god der minnen // <rhyme label="a" type="m">stout</rhyme> //</l>
        <l id="s1:l2" met="2" type="-+">Ghy // geeft <rhyme label="b" type="m">my</rhyme> //</l>
        <l id="s1:l3" met="3" type="+-">Int hert menich <rhyme label="c" type="f">wonde</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En in bedwanck mijn sinnen // <rhyme label="a" type="m" corresp="#s1:l1">hout</rhyme> //</l>
        <l id="s1:l5" met="2" type="-+">Dat // ick <rhyme label="d" type="m">mat</rhyme> //</l>
        <l id="s1:l6" met="3" type="+-">Ben versmacht te <rhyme label="c" type="f" corresp="#s1:l3">gronde</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Noyt en conde // my <rhyme label="e" type="f">ontmoeten</rhyme> //</l>
        <l id="s1:l8" met="4" type="+-">Troost voor een verslegen // <rhyme label="f" type="m">moet</rhyme> //</l>
        <l id="s1:l9" met="4" type="+-">Ander ionde // ghy <rhyme label="e" type="f" corresp="#s1:l7">t'versoeten</rhyme> //</l>
        <l id="s1:l10" met="4" type="+-">My, veel drucx en tegen // <rhyme label="f" type="m" corresp="#s1:l8">spoet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als ghy te mywaerts wrimpich // <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Lijdt // en <rhyme label="b" type="m">strijdt</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Mijn ieught <rhyme label="c" type="f">overstreden</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Ghy zijt die my so schimpich // <rhyme label="a" type="m" corresp="#s2:l1">liet</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Van // elck <rhyme label="d" type="m">man</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Als een worm <rhyme label="c" type="f" corresp="#s2:l3">vertreden</rhyme>:</l>
        <l id="s2:l7" met="4" type="+-">By wat reden // hebdy <rhyme label="e" type="f">voren</rhyme></l>
        <l id="s2:l8" met="4" type="+-">My te dooden sonder <rhyme label="f" type="m">schult</rhyme>:</l>
        <l id="s2:l9" met="4" type="+-">Laer verbeden // zijn dijn <rhyme label="e" type="f" corresp="#s2:l7">toren</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Naer u goddelick <rhyme label="f" type="m" corresp="#s2:l8">gedult</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Tuwaerts moest t'allen dagen // <rhyme label="a" type="m">oyt</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Mijn // <rhyme label="b" type="m">aenschijn</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Hem subiect <rhyme label="c" type="f">verschoonen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Nochtans ghy my veel lagen // <rhyme label="a" type="m" corresp="#s3:l1">strooyt</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Ach // ick <rhyme label="d" type="m">mach</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Suchten voor <rhyme label="c" type="f" corresp="#s3:l3">t'beloonen</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Om soo schoonen // kindt der <rhyme label="e" type="f">eeren</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Lijd ick veel, die my can <rhyme label="f" type="m">doen</rhyme></l>
        <l id="s3:l9" met="4" type="+-">Deur haer Croonen // <rhyme label="e" type="f" corresp="#s3:l7">triumpheeren</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Als deurstreden <rhyme label="f" type="m" corresp="#s3:l8">Campioen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Heb ick Apollo dan // <rhyme label="a" type="m">geslacht</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Met // <rhyme label="b" type="m">opset</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">V geschut <rhyme label="c" type="f">verschoven</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">O neen ick niet! wie can // de // <rhyme label="a" type="m" corresp="#s4:l1">cracht</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Gaen // <rhyme label="d" type="m">weerstaen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Diet al gaet te <rhyme label="c" type="f" corresp="#s4:l3">boven</rhyme>?</l>
        <l id="s4:l7" met="4" type="+-">Princen hoven // u <rhyme label="e" type="f">geboden</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Nemen met groot wonder // <rhyme label="f" type="m">aen</rhyme>:</l>
        <l id="s4:l9" met="4" type="+-">Dies u loven // alle <rhyme label="e" type="f" corresp="#s4:l7">goden</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">En u macht zijn onder- // <rhyme label="f" type="m" corresp="#s4:l8">daen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Al lijd ick dan een smerte // <rhyme label="a" type="m">wreet</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Lof // hier <rhyme label="b" type="m">of</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Moet u zijn <rhyme label="c" type="f">gesongen</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Venus (t'moederlick herte // <rhyme label="a" type="m" corresp="#s5:l1">heet</rhyme>)</l>
        <l id="s5:l5" met="2" type="-+">Heeft // <rhyme label="d" type="m">beleeft</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Vwen schicht <rhyme label="c" type="f" corresp="#s5:l3">deurdrongen</rhyme>:</l>
        <l id="s5:l7" met="4" type="+-">Hoe bedwongen // ghy elck <rhyme label="e" type="f">slave</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Int prisoen van suchten <rhyme label="f" type="m">leydt</rhyme>:</l>
        <l id="s5:l9" met="4" type="+-">Als ontsprongen // wt den <rhyme label="e" type="f" corresp="#s5:l7">grave</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Geeft my voetstaens <rhyme label="f" type="m" corresp="#s5:l8">liberteyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Reden? beveelt met billich // <rhyme label="a" type="m">recht</rhyme></l>
        <l id="s6:l2" met="2" type="-+">V // alls <rhyme label="b" type="m">nu</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Vroom te <rhyme label="c" type="f">adoreeren</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Ick dan, als een goetwillich // <rhyme label="a" type="m" corresp="#s6:l1">knecht</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Naer // <rhyme label="d" type="m">d'Altaer</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Wil ick t'aenschijn <rhyme label="c" type="f" corresp="#s6:l3">keeren</rhyme>:</l>
        <l id="s6:l7" met="4" type="+-">Supplieeren // daer <rhyme label="e" type="f">soetmondich</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Om gehoor, tot mijn <rhyme label="f" type="m">propoost</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">En begeeren // seer <rhyme label="e" type="f" corresp="#s6:l7">hertgrondich</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Eens in liefden t'sijn <rhyme label="f" type="m" corresp="#s6:l8">getroost</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ist dat u gramschap neder // <rhyme label="a" type="m">daelt</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Is // <rhyme label="b" type="m">gewis</rhyme></l>
        <l id="s7:l3" met="3" type="+-">T'eynden mijn <rhyme label="c" type="f">vernoyen</rhyme>:</l>
        <l id="s7:l4" met="4" type="+-">Mijn aenschijn bleeck // dan weder // <rhyme label="a" type="m" corresp="#s7:l1">haelt</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Een // seer <rhyme label="d" type="m">cleen</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Schoon rosierich <rhyme label="c" type="f" corresp="#s7:l3">bloyen</rhyme>:</l>
        <l id="s7:l7" met="4" type="+-">Weeldich groyen // is <rhyme label="e" type="f">voorhanden</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Al was ick seer straf // <rhyme label="f" type="m">gevaen</rhyme>:</l>
        <l id="s7:l9" met="4" type="+-">Kerkers, boyen // ende <rhyme label="e" type="f" corresp="#s7:l7">banden</rhyme>,</l>
        <l id="s7:l10" met="4" type="+-">Zijn subyt my af // <rhyme label="f" type="m" corresp="#s7:l8">gedaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">NAer dat ghy (als machtich // <rhyme label="a" type="m">princier</rhyme>)</l>
        <l id="s8:l2" met="2" type="-+">Slaet // en <rhyme label="b" type="m">laet</rhyme></l>
        <l id="s8:l3" met="3" type="+-">Wederom <rhyme label="c" type="f">genesen</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Regiert deur u crachtich // <rhyme label="a" type="m" corresp="#s8:l1">bestier</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Haer // <rhyme label="d" type="m">eerbaer</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Princesselick <rhyme label="c" type="f" corresp="#s8:l3">wesen</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Die gepresen // ieught <rhyme label="e" type="f">bepeerelt</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Heb ick wtvercoren // <rhyme label="f" type="m">ras</rhyme>,</l>
        <l id="s8:l9" met="4" type="+-">Wtgelesen // noyt ter <rhyme label="e" type="f" corresp="#s8:l7">weerelt</rhyme></l>
        <l id="s8:l10" met="4" type="+-">Soeter kindt geboren // <rhyme label="f" type="m" corresp="#s8:l8">was</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
