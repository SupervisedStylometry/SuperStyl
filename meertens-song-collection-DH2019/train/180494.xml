<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180494</id>
    <date>
      <year-start>1660</year-start>
      <year-end>1660</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Si je suis travaillé</melody>
  <author>Gaudentius van Loemel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Doet Godt <rhyme label="a" type="m">bermhertigheyt</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Bid ick u <rhyme label="a" type="m" corresp="#s1:l1">Majesteyt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En wilt mijn Vonnis <rhyme label="b" type="f">wysen</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">Al tot mijn <rhyme label="a" type="m" corresp="#s1:l1">saligheyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Boven al om te <rhyme label="b" type="f" corresp="#s1:l3">prijsen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Doet my <rhyme label="c" type="m">ghenae</rhyme>!</l>
        <l id="s1:l7" met="2" type="-+">Eer ‘t wordt te <rhyme label="c" type="m" corresp="#s1:l6">spae</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Doet my <rhyme label="c" type="m" corresp="#s1:l6">genae</rhyme>!</l>
        <l id="s1:l9" met="2" type="-+">Eer ‘t wordt te <rhyme label="c" type="m" corresp="#s1:l6">spae</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Dat ick voor sonden niet <rhyme label="c" type="m" corresp="#s1:l6">vergae</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">V Oordeel altijdt <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Rechtveerdigh, en <rhyme label="a" type="m" corresp="#s2:l1">gewis</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Maer wilt mijn quaet <rhyme label="b" type="f">uytvegen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op dat ick niet en <rhyme label="a" type="m" corresp="#s2:l1">mis</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat door u is <rhyme label="b" type="f" corresp="#s2:l3">verkreghen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Mijn <rhyme label="c" type="m">saligheyt</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+"><rhyme label="c" type="m" corresp="#s2:l6">Bermhertigheyt</rhyme>!</l>
        <l id="s2:l8" met="2" type="-+">Mijn <rhyme label="c" type="m" corresp="#s2:l6">saligheyt</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+"><rhyme label="c" type="m" corresp="#s2:l6">Bermhertigheyt</rhyme>!</l>
        <l id="s2:l10" met="4" type="-+">En doet my geen <rhyme label="c" type="m" corresp="#s2:l6">Rechtveerdigheyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Aensiet Heer uwen <rhyme label="a" type="m">knecht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Berooft van deughden <rhyme label="a" type="m" corresp="#s3:l1">slecht</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die niet en kan <rhyme label="b" type="f">betalen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Daer hy behoorden <rhyme label="a" type="m" corresp="#s3:l1">recht</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">In ‘t minste niet te <rhyme label="b" type="f" corresp="#s3:l3">falen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Het is mijn <rhyme label="c" type="m">schult</rhyme>!</l>
        <l id="s3:l7" met="2" type="-+">Hebt Heer <rhyme label="c" type="m" corresp="#s3:l6">geduldt</rhyme>!</l>
        <l id="s3:l8" met="2" type="-+">Het is mijn <rhyme label="c" type="m" corresp="#s3:l6">schuldt</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Hebt Heer <rhyme label="c" type="m" corresp="#s3:l6">geduldt</rhyme>!</l>
        <l id="s3:l10" met="4" type="-+">V goetheyt ons hier all’ <rhyme label="c" type="m" corresp="#s3:l6">vervult</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy hebt veel sondaers <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Getroost in haeren <rhyme label="a" type="m" corresp="#s4:l1">noodt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wilt mijnder oock <rhyme label="b" type="f">ontfermen</rhyme>!</l>
        <l id="s4:l4" met="3" type="-+">Laet u genade <rhyme label="a" type="m" corresp="#s4:l1">schoot</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">My dienen tot <rhyme label="b" type="f" corresp="#s4:l3">beschermen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Mijn hert dat <rhyme label="c" type="m">beeft</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Genade <rhyme label="c" type="m" corresp="#s4:l6">geeft</rhyme>!</l>
        <l id="s4:l8" met="2" type="-+">Mijn hert dat <rhyme label="c" type="m" corresp="#s4:l6">beeft</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Genade <rhyme label="c" type="m" corresp="#s4:l6">geeft</rhyme>!</l>
        <l id="s4:l10" met="4" type="-+">Op dat mijn Ziele eeuwigh <rhyme label="c" type="m" corresp="#s4:l6">leeft</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O Iesu laet u <rhyme label="a" type="m">Cruysch</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">My maecken niet <rhyme label="a" type="m" corresp="#s5:l1">confuys</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Maer altijdst hier <rhyme label="b" type="f">bevrijen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Op dat het helsch <rhyme label="a" type="m" corresp="#s5:l1">gespuys</rhyme></l>
        <l id="s5:l5" met="3" type="-+">My niet en brenght in <rhyme label="b" type="f" corresp="#s5:l3">lijen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Dan sal ick <rhyme label="c" type="m">stoff</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Oock uwen <rhyme label="c" type="m" corresp="#s5:l6">loff</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Dan sal ick <rhyme label="c" type="m" corresp="#s5:l6">stoff</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Oock uwen <rhyme label="c" type="m" corresp="#s5:l6">Loff</rhyme></l>
        <l id="s5:l10" met="4" type="-+">Eens singen in u Hemelsch <rhyme label="c" type="m" corresp="#s5:l6">Hoff</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
