<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19278</id>
    <date>
      <year-start>1552</year-start>
      <year-end>1554</year-end>
    </date>
    <location>
      <orginal-name>antwerpen]</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="vermaanlied" translation="admonitory song">vermaanlied</tag>
    </tags>
    <melody>Lijntjes oven is bestoven</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEt zijn nv soo periculuese <rhyme label="a" type="f">tijden</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Het is wel recht dat elck toe <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Wilt v van valsche leeringhe <rhyme label="a" type="f" corresp="#s1:l1">mijden</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Het	leeft noch dat Eua <rhyme label="b" type="m" corresp="#s1:l2">verriet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s1:l5">byten</rhyme></l>
        <l id="s1:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s1:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Aensiet in alle uwe <rhyme label="a" type="f">ghewercken</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Christus uwen exempel <rhyme label="b" type="m">soet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Schout leeringhe van valsche <rhyme label="a" type="f" corresp="#s2:l1">clercken</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Die leeren wat hem duncken <rhyme label="b" type="m" corresp="#s2:l2">goet</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s2:l5">byten</rhyme></l>
        <l id="s2:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s2:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Al schijnet dat si veele <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Sprekende woorden vol <rhyme label="b" type="m">ghewichts</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Weet dat de duyuel hem can <rhyme label="a" type="f" corresp="#s3:l1">verkeeren</rhyme></l>
        <l id="s3:l4" met="4" type="-+">In eenen Inghel des <rhyme label="b" type="m" corresp="#s3:l2">lichts</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s3:l5">byten</rhyme></l>
        <l id="s3:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s3:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sy sullen comen in schaps <rhyme label="a" type="f">cleeren</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Binnen werden si woluen <rhyme label="b" type="m">quaet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Wildy dese kennen <rhyme label="a" type="f" corresp="#s4:l1">leeren</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Hoort en doet na Christus <rhyme label="b" type="m" corresp="#s4:l2">raet</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s4:l5">byten</rhyme></l>
        <l id="s4:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s4:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Men can van gheenen doornen <rhyme label="a" type="f">lesen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Druyuen want hy gheen en <rhyme label="b" type="m">draecht</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Noch van dijstelen vijghen <rhyme label="a" type="f" corresp="#s5:l1">ghepresen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Dus siet wel toe alsmen v <rhyme label="b" type="m" corresp="#s5:l2">vraecht</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s5:l5">byten</rhyme></l>
        <l id="s5:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s5:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Merckt hoe Amasa heeft <rhyme label="a" type="f">gheuaren</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Doen hem Ioab schoon <rhyme label="b" type="m">toesprack</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Met een vrindelijck <rhyme label="a" type="f" corresp="#s6:l1">ghebaren</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Uredelijck hy hem doot <rhyme label="b" type="m" corresp="#s6:l2">stack</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s6:l5">byten</rhyme></l>
        <l id="s6:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s6:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dien selue Ioab wilde <rhyme label="a" type="f">spreken</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Abner met een Iudas <rhyme label="b" type="m">poort</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Dwelck hy dede met loose <rhyme label="a" type="f" corresp="#s7:l1">treken</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Want hy heeft hem oock <rhyme label="b" type="m" corresp="#s7:l2">vermoort</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s7:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s7:l5">byten</rhyme></l>
        <l id="s7:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s7:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Aensiet by wat subtylen <rhyme label="a" type="f">rade</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Thamar wert <rhyme label="b" type="m">gheuioleert</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Uan Amon diese <rhyme label="a" type="f" corresp="#s8:l1">versmade</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Doen sinen wellust was <rhyme label="b" type="m" corresp="#s8:l2">ghepasseert</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s8:l5">byten</rhyme></l>
        <l id="s8:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s8:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Laet v oock comen te <rhyme label="a" type="f">voren</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Hoe Sampson quam int <rhyme label="b" type="m">ghepijn</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Doen zijn haer wert <rhyme label="a" type="f" corresp="#s9:l1">afgheschoren</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Uan Dalida seer listich <rhyme label="b" type="m" corresp="#s9:l2">fijn</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s9:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s9:l5">byten</rhyme></l>
        <l id="s9:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s9:l6">verraen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Prinche al is Iudas in midden <rhyme label="a" type="f">geborsten</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Sijn gheslacht en is noch niet <rhyme label="b" type="m">doot</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Die Gods volck bringen voor Heeren en <rhyme label="a" type="f" corresp="#s10:l1">vorsten</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Die welck men doot met nidicheyt <rhyme label="b" type="m" corresp="#s10:l2">groot</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Wacht v voor die <rhyme label="c" type="f">Ipocrijten</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Die Gods leeringhe veele <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s10:l7" met="4" type="-+">Schoon sprekende sullen si v <rhyme label="c" type="f" corresp="#s10:l5">byten</rhyme></l>
        <l id="s10:l8" met="4" type="-+">En met listen si v <rhyme label="d" type="m" corresp="#s10:l6">verraen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
