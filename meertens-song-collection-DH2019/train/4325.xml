<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4325</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>C'est trop courir les eaux</melody>
  <author>Pieter Dubbels</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">PRonkpaarl aan Amstels <rhyme label="a" type="m">Kroon</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">VVen' ik 't allokb're <rhyme label="a" type="m" corresp="#s1:l1">schoon</rhyme>,</l>
        <l id="s1:l3" met="6" type="-+">Van uw' gezigt bestraal, voel ik, o grote <rhyme label="b" type="m">kragt</rhyme>!</l>
        <l id="s1:l4" met="6" type="-+">Door 't schieten van een lonk, mijn Ziel in Cypris <rhyme label="b" type="m" corresp="#s1:l3">magt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">VVas ooyt de God <rhyme label="a" type="m">Apoll'</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Door minnend'-yver <rhyme label="a" type="m" corresp="#s2:l1">dol</rhyme>,</l>
        <l id="s2:l3" met="6" type="-+">Om Dafnis schoonheit? is het wonder, dat mijn <rhyme label="b" type="m">ziel</rhyme>,</l>
        <l id="s2:l4" met="6" type="-+">Op d'alverwinb're burg van uwe schoon-heit <rhyme label="b" type="m" corresp="#s2:l3">viel</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Alwaar Natuur haar <rhyme label="a" type="m">schoont</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zo zegenrijk <rhyme label="a" type="m" corresp="#s3:l1">vertoont</rhyme>;</l>
        <l id="s3:l3" met="6" type="-+">Dat, wien het Zonneligt van uwe glans <rhyme label="b" type="m">bestraalt</rhyme>,</l>
        <l id="s3:l4" met="6" type="-+">Verwonnen en gewont, voor uwe voeten <rhyme label="b" type="m" corresp="#s3:l3">daalt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Mijn jeugdig brein, <rhyme label="a" type="m">verkragt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Opoffert klagt op <rhyme label="a" type="m" corresp="#s4:l1">klagt</rhyme>,</l>
        <l id="s4:l3" met="6" type="-+">Op 't autaar uw's gehoor, op hope dat mijn <rhyme label="b" type="m">be</rhyme></l>
        <l id="s4:l4" met="6" type="-+">De diamante rotzen van uw' zinnen <rhyme label="b" type="m" corresp="#s4:l3">kne</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laat 't goude woortje, <rhyme label="a" type="m">vlot</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ontgrendelen het <rhyme label="a" type="m" corresp="#s5:l1">slot</rhyme>,</l>
        <l id="s5:l3" met="6" type="-+">Van uw' gekluysde Ziel, op dat mijn jagend' <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s5:l4" met="6" type="-+">Van 't lang gewilde wilt, gelukkig win-naar <rhyme label="b" type="m" corresp="#s5:l3">ward'</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
