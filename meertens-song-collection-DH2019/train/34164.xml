    <melody>Ik heb menig pint en kan</melody>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  <author>Petrus Suetmans, Eustachius de Meyer</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'kHeb menigh pint en <rhyme label="a" type="m">kan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghelost met eenen <rhyme label="b" type="m">teugh</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En weder <rhyme label="c" type="f">vol-gheschoncken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In't nat ben ick een <rhyme label="a" type="m" corresp="#s1:l1">man</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">k'En drinck toch maer myn <rhyme label="b" type="m" corresp="#s1:l2">meugh</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Tot dat ick word' half <rhyme label="c" type="f" corresp="#s1:l3">droncken</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">'kVind' lust, en smaeck daer <rhyme label="d" type="m">in</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Draeyt my daer naer den <rhyme label="e" type="m">kop</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Soo word' ick bly van <rhyme label="d" type="m" corresp="#s1:l7">sin</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Door krachten van dit <rhyme label="e" type="m" corresp="#s1:l8">sop</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Door krachten van dit <rhyme label="e" type="m" corresp="#s1:l8">sop</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den dranck toch maer en <rhyme label="a" type="m">wast</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Om dat m'hem drincken <rhyme label="b" type="m">sou</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Waer-om dan niet <rhyme label="c" type="f">ghesopen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Voor-waer ick ben een <rhyme label="a" type="m" corresp="#s2:l1">gast</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Die van het suypen <rhyme label="b" type="m" corresp="#s2:l2">hou</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Laet swalghen met heel <rhyme label="c" type="f" corresp="#s2:l3">stoopen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Drinckt een ghesontheyt <rhyme label="d" type="m">om</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En smeert u drooghe <rhyme label="e" type="m">borst</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Suypt wacker aen den <rhyme label="d" type="m" corresp="#s2:l7">brom</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Soo sterft-men noyt van <rhyme label="e" type="m" corresp="#s2:l8">dorst</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Soo sterft-men noyt van <rhyme label="e" type="m" corresp="#s2:l8">dorst</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Elck ooghen-blick een <rhyme label="a" type="m">dronck</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Laet ons niet ledig <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Want ledigheydt is <rhyme label="c" type="f">sonde</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die ons den wyn eerst <rhyme label="a" type="m" corresp="#s3:l1">schonck</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die heeft het vreudigh <rhyme label="b" type="m" corresp="#s3:l2">zyn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In't edel nat <rhyme label="c" type="f" corresp="#s3:l3">bevonden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">'tWelck deughdigh is doet <rhyme label="d" type="m">deucht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Soo moet altyt den <rhyme label="e" type="m">wyn</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Tot voetsel van de <rhyme label="d" type="m" corresp="#s3:l7">vreucht</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Van ons ghedroncken <rhyme label="e" type="m" corresp="#s3:l8">zyn</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Van ons ghedroncken <rhyme label="e" type="m" corresp="#s3:l8">zyn</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Doen ick een teugh <rhyme label="a" type="m">bescheet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ick slaen 'tghesicht om <rhyme label="b" type="m">hoogh</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ghedurigh naer den <rhyme label="c" type="f">hemel</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">'kBeween dan al myn <rhyme label="a" type="m" corresp="#s4:l1">leet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Met traenen in myn <rhyme label="b" type="m" corresp="#s4:l2">oogh</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Waer door ick krygh <rhyme label="c" type="f" corresp="#s4:l3">gheschemel</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">My dunckt dat d'aerde <rhyme label="d" type="m">beeft</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Elck voet-stap die ick <rhyme label="e" type="m">gaen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Eer my den gangh <rhyme label="d" type="m" corresp="#s4:l7">begheeft</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Brenght noch een teughskens <rhyme label="e" type="m" corresp="#s4:l8">aen</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Brenght noch een teughskens <rhyme label="e" type="m" corresp="#s4:l8">aen</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O goddelycke <rhyme label="a" type="m">nat</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hoe wonder is u <rhyme label="b" type="m">kracht</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ghy dryft van ons het <rhyme label="c" type="f">suchten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Naer dat m'u lost uyt <rhyme label="a" type="m" corresp="#s5:l1">'tvat</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Laeyt m'in den buyck u <rhyme label="b" type="m" corresp="#s5:l2">vracht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dan jaeght ghy 'thooft vol <rhyme label="c" type="f" corresp="#s5:l3">kluchten</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Lof dan die al-tydt <rhyme label="d" type="m">drinckt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En leeft met bly <rhyme label="e" type="m">ghemoet</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Want ieder singht, en <rhyme label="d" type="m" corresp="#s5:l7">klinckt</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Als hy is vol, en <rhyme label="e" type="m" corresp="#s5:l8">soet</rhyme>.</l>
        <l id="s5:l11" met="3" type="+-">Als hy is vol, en <rhyme label="e" type="m" corresp="#s5:l8">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
