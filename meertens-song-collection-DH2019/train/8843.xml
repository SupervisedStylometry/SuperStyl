      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GOdt Zyon lovet dy in <rhyme label="a" type="m">stilt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dy gaet men G'lofft' <rhyme label="b" type="f">betalen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Want ghy t'Ghebet verhooren <rhyme label="a" type="m" corresp="#s1:l1">wilt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">All' Vleesch hulp van u <rhyme label="b" type="f" corresp="#s1:l2">halen</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Ons drucken swaer ons <rhyme label="c" type="m">Missedaet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wilt ons die Sond' <rhyme label="d" type="f">vergheven</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Salich, dien ghy verkiesen <rhyme label="c" type="m" corresp="#s1:l5">gaet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">End' laet hem oock daer <rhyme label="d" type="f" corresp="#s1:l6">neven</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">In dijnen Huyse <rhyme label="d" type="f" corresp="#s1:l6">Leven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die heeft dijns huyses rijcken <rhyme label="a" type="m">troost</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van dijnen Tempel <rhyme label="b" type="f">vroede</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Verhoort ons Godt end' ons <rhyme label="a" type="m" corresp="#s2:l1">verloost</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Nae wonderlijcke <rhyme label="b" type="f" corresp="#s2:l2">Goede</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ghy aller Menschen <rhyme label="c" type="m">Toeversicht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Te Land' end' aen <rhyme label="d" type="f">Zeecanten</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Hebt die Berghen wel vast <rhyme label="c" type="m" corresp="#s2:l5">verplicht</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Door d'Almacht dijner <rhyme label="d" type="f" corresp="#s2:l6">Handen</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Dijn Cracht wort niet te <rhyme label="d" type="f" corresp="#s2:l6">schanden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy stilt des Zees onstuerich <rhyme label="a" type="m">Vloet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat bruysen syner <rhyme label="b" type="f">Golven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ghy stilt der Volcker boosen <rhyme label="a" type="m" corresp="#s3:l1">Moet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Al zijn sy g'lijck die <rhyme label="b" type="f" corresp="#s3:l2">Wolven</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Dies schricken all' die woonen <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Voor dijne <rhyme label="d" type="f">Wonderheyden</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Ghy maeckt oock vrolijck <rhyme label="c" type="m" corresp="#s3:l5">allegaer</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Wat om dyn Spijs' gaet <rhyme label="d" type="f" corresp="#s3:l6">Weyden</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Savonts en s'morghens <rhyme label="d" type="f" corresp="#s3:l6">beyden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dijn Goetheyt over al <rhyme label="a" type="m">ghespreyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">(HEEr Godt ghy <rhyme label="b" type="f">goedertieren</rhyme>)</l>
        <l id="s4:l3" met="4" type="-+">Geeft onsen Lande <rhyme label="a" type="m" corresp="#s4:l1">Vruchtbaerheyt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">End' gaet het Jaer <rhyme label="b" type="f" corresp="#s4:l2">vercieren</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dijn Fonteyn, Waters heeft <rhyme label="c" type="m">genoech</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ghy doet het Zaet oock <rhyme label="d" type="f">bloeyen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Met Regen volghet ghy den <rhyme label="c" type="m" corresp="#s4:l5">Ploech</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">End' dijn Voetstappe <rhyme label="d" type="f" corresp="#s4:l6">vloeyen</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Daer van all' Wasdom <rhyme label="d" type="f" corresp="#s4:l6">groeyen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Waer ghy (HEEr) gaet, daer wasse al <rhyme label="a" type="m">vroet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Vet syn die <rhyme label="b" type="f">Woestenijen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die Heuv'len ghy Lustich <rhyme label="a" type="m" corresp="#s5:l1">ontdoet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">All' Vee gaet hem <rhyme label="b" type="f" corresp="#s5:l2">verblijen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Die Schapen van so groot <rhyme label="c" type="m">ghetal</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">T'velt staet soo vol end' <rhyme label="d" type="f">blijde</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Dat men gaet singhen over <rhyme label="c" type="m" corresp="#s5:l5">al</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">End' met gheschal wel <rhyme label="d" type="f" corresp="#s5:l6">wijde</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Roemt sulcke goede <rhyme label="d" type="f" corresp="#s5:l6">Tijde</rhyme>.</l>
      </lg>
    </lg>
  </text>
