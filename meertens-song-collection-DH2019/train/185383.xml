    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Nu laat ons allegaar</melody>
  <author>Johannes Stichter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">NU laet ons allegaer dankbaer <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ons Heer, ons Godt, ons <rhyme label="b" type="f">Vader</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die ons van Bier, van Brood, van <rhyme label="a" type="m" corresp="#s1:l1">Wijn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verzaed heeft <rhyme label="b" type="f" corresp="#s1:l2">allegader</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Van Visch, van Vleys, van andere <rhyme label="c" type="m">spijs</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Gekookt, bereyd op meenigerley <rhyme label="c" type="m" corresp="#s1:l5">wijs</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Waer van wy hem nu <rhyme label="d" type="f">seere</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dancken met grooter <rhyme label="d" type="f" corresp="#s1:l7">eere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den Soone Godts <rhyme label="a" type="m">gebenedijd</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die willen wy ook gaen <rhyme label="b" type="f">loven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die ons van sonden heeft <rhyme label="a" type="m" corresp="#s2:l1">bevrijd</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Doen wy waren <rhyme label="b" type="f" corresp="#s2:l2">verschoven</rhyme>:</l>
        <l id="s2:l5" met="4" type="-+">Die ons soo minnelijk heeft <rhyme label="c" type="m">verzaed</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En in der noodt niet af en <rhyme label="c" type="m" corresp="#s2:l5">gaet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Bemint hem in dit <rhyme label="d" type="f">leven</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Hy sal u niet <rhyme label="d" type="f" corresp="#s2:l7">begeven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy dancken ook den Heyligen <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van zijne <rhyme label="b" type="f">visitacy</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Want had hy by ons niet <rhyme label="a" type="m" corresp="#s3:l1">geweest</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ons vreugt was <rhyme label="b" type="f" corresp="#s3:l2">tribulacy</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Hy is een Vertrooster in ’t <rhyme label="c" type="m">verdriet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Sonder hem en vermogen wy <rhyme label="c" type="m" corresp="#s3:l5">niet</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Dus wilt hem u hert <rhyme label="d" type="f">bereyen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Hy sal niet van u <rhyme label="d" type="f" corresp="#s3:l7">scheyen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die Waert willen wy dancken <rhyme label="a" type="m">zeer</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat hy heeft laten <rhyme label="b" type="f">koocken</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hy heeft soo wel bewaert zijn <rhyme label="a" type="m" corresp="#s4:l1">eer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ons en heeft niet <rhyme label="b" type="f" corresp="#s4:l2">ontbroocken</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Om vroolijk te zingen met herten <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">En bedancken dat Geselschap <rhyme label="c" type="m" corresp="#s4:l5">goet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat zy hier zijn <rhyme label="d" type="f">gekomen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">’t Word danckelijk <rhyme label="d" type="f" corresp="#s4:l7">genomen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wy willen ook vergeeten <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die van hier zijn <rhyme label="b" type="f">gescheyden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Op datse den Heer uyt alle <rhyme label="a" type="m" corresp="#s5:l1">verdriet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Al in zijn Rijck wil <rhyme label="b" type="f" corresp="#s5:l2">leyden</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Alsoo wy doen, sal ons <rhyme label="c" type="m">geschien</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Wanneer wy uyt dit leven <rhyme label="c" type="m" corresp="#s5:l5">vlien</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Hierom soo wilt voor <rhyme label="d" type="f">desen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Een Pater-noster <rhyme label="d" type="f" corresp="#s5:l7">lesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
