      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Geen Venus of <rhyme label="a" type="m">Diaan</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Of dochter van de <rhyme label="a" type="m" corresp="#s1:l1">Zwaan</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Zal op mijn veder <rhyme label="b" type="f">vlerken</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">Maar een Bataafsche <rhyme label="c" type="m">Maagt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">VVien Minerve, op haar <rhyme label="b" type="f" corresp="#s1:l3">vlerken</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Boven de starren <rhyme label="c" type="m" corresp="#s1:l4">draagt</rhyme>.</l>
      </lg>
