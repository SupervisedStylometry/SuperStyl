      <category translation="love and sex">liefde en seks</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">‘Godt groet u, mijn <rhyme label="a" type="f">waarde</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Silvia, mijn <rhyme label="b" type="m">Herderin</rhyme>.</l>
        <l id="s1:l3" met="3" type="+-">Geen schoonder op <rhyme label="a" type="f" corresp="#s1:l1">Aerde</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Quam ooit in mijn <rhyme label="b" type="m" corresp="#s1:l2">zin</rhyme></l>
        <l id="s1:l5" met="3" type="+-">Als gy, <rhyme label="c" type="f">Uitgelesen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Myn Troosjen, myn <rhyme label="d" type="m">Hert</rhyme>.</l>
        <l id="s1:l7" met="3" type="+-">O Lief, mag het <rhyme label="c" type="f" corresp="#s1:l5">wesen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Verlicht doch myn <rhyme label="d" type="m" corresp="#s1:l6">smert</rhyme>!</l>
        <l id="s1:l9" met="3" type="+-">Ick sal u <rhyme label="e" type="f">geleiden</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Dus kleet 'er u <rhyme label="f" type="m">strack</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">En vlytig <rhyme label="e" type="f" corresp="#s1:l9">bevreiden</rhyme></l>
        <l id="s1:l12" met="3" type="+-">Voor alle <rhyme label="f" type="m" corresp="#s1:l10">ongemack</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">De son aen 't <rhyme label="a" type="f">klimmen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Die daalt op ons <rhyme label="b" type="m">spoor</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Uit sijn gulde <rhyme label="a" type="f" corresp="#s2:l1">klimmen</rhyme>.</l>
        <l id="s2:l4" met="3" type="+-">Myn Engeltjen, <rhyme label="b" type="m" corresp="#s2:l2">hoor</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Hoe het <rhyme label="c" type="f">Nachtegaaltjen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Met een vrolyk <rhyme label="d" type="m">gemoet</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Singt, dat al de <rhyme label="c" type="f" corresp="#s2:l5">Vogeltjes</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Mee queelen <rhyme label="d" type="m" corresp="#s2:l6">doet</rhyme>.</l>
        <l id="s2:l9" met="3" type="+-">En sy telen staag <rhyme label="e" type="f">verder</rhyme></l>
        <l id="s2:l10" met="3" type="+-">En ick smelt schier' van <rhyme label="f" type="m">min</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Dus troost uwen <rhyme label="e" type="f" corresp="#s2:l9">Herder</rhyme></l>
        <l id="s2:l12" met="3" type="+-">Myn lieve <rhyme label="f" type="m" corresp="#s2:l10">Herderin</rhyme>.’</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">‘Wie doet myn <rhyme label="a" type="f">ontwaken</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Door sijn <rhyme label="b" type="m">minneklag</rhyme>?</l>
        <l id="s3:l3" met="3" type="+-">Wat komt gy hier <rhyme label="a" type="f" corresp="#s3:l1">maken</rhyme>?</l>
        <l id="s3:l4" met="3" type="+-">Ik sien nog geen <rhyme label="b" type="m" corresp="#s3:l2">dag</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Ik hoor nog niet <rhyme label="c" type="f">fluiten</rhyme></l>
        <l id="s3:l6" met="3" type="+-">De Vogelkens <rhyme label="d" type="m">al</rhyme>.</l>
        <l id="s3:l7" met="3" type="+-">Waarom souw ik <rhyme label="c" type="f" corresp="#s3:l5">ontsluiten</rhyme></l>
        <l id="s3:l8" met="3" type="+-">Soo vroeg mynen <rhyme label="d" type="m" corresp="#s3:l6">stal</rhyme>?</l>
        <l id="s3:l9" met="3" type="+-">Mijn lust nog geen <rhyme label="e" type="f">minnen</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Wel Harder, soo <rhyme label="f" type="m">gou</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Dus stelter uw <rhyme label="e" type="f" corresp="#s3:l9">zinnen</rhyme></l>
        <l id="s3:l12" met="3" type="+-">Op een ander <rhyme label="f" type="m" corresp="#s3:l10">Jonkvrouw</rhyme>.’</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">‘Wel Harder, uw <rhyme label="a" type="f">reden</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Die staan myn wel <rhyme label="b" type="m">aan</rhyme>.</l>
        <l id="s4:l3" met="3" type="+-">'k Sal myn spoedig <rhyme label="a" type="f" corresp="#s4:l1">kleeden</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Om met uw te <rhyme label="b" type="m" corresp="#s4:l2">gaan</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Na de groene <rhyme label="c" type="f">Weiden</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Met mijn schaapjes <rhyme label="d" type="m">al</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Sal ik uw <rhyme label="c" type="f" corresp="#s4:l5">geleiden</rhyme>:</l>
        <l id="s4:l8" met="3" type="+-">Ontsluit mynen <rhyme label="d" type="m" corresp="#s4:l6">stal</rhyme>.</l>
        <l id="s4:l9" met="3" type="+-">Ik sal by uw <rhyme label="e" type="f">blyven</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Leander - 't wort <rhyme label="f" type="m">laet</rhyme> -</l>
        <l id="s4:l11" met="3" type="+-">En helpen <rhyme label="e" type="f" corresp="#s4:l9">dryven</rhyme></l>
        <l id="s4:l12" met="3" type="+-">Tot de Son <rhyme label="f" type="m" corresp="#s4:l10">ondergaat</rhyme>.’</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">‘Mijn Engel, wat <rhyme label="a" type="f">vreugden</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Gevoelt mijn Jonk <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Dat gy, Lief vol <rhyme label="a" type="f" corresp="#s5:l1">deugden</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Verligt al myn <rhyme label="b" type="m" corresp="#s5:l2">smert</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Laten wy in de <rhyme label="c" type="f">weiden</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Gaan maken een <rhyme label="d" type="m">dans</rhyme></l>
        <l id="s5:l7" met="3" type="+-">En vlegten ons <rhyme label="c" type="f" corresp="#s5:l5">beiden</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Daer elk een <rhyme label="d" type="m" corresp="#s5:l6">krans</rhyme>.</l>
        <l id="s5:l9" met="3" type="+-">Daer sal ik uw <rhyme label="e" type="f">geven</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Myn lichaem op <rhyme label="f" type="m">trouw</rhyme></l>
        <l id="s5:l11" met="3" type="+-">En staag met uw <rhyme label="e" type="f" corresp="#s5:l9">leven</rhyme></l>
        <l id="s5:l12" met="3" type="+-">Als een man met sijn <rhyme label="f" type="m" corresp="#s5:l10">vrouw</rhyme>.’</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">‘Wel aan dan, <rhyme label="a" type="f">Leander</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Laet ons beide met <rhyme label="b" type="m">vreugt</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Trouwen met <rhyme label="a" type="f" corresp="#s6:l1">malkander</rhyme></l>
        <l id="s6:l4" met="3" type="+-">In eer en <rhyme label="b" type="m" corresp="#s6:l2">deugt</rhyme>.’</l>
        <l id="s6:l5" met="3" type="+-">‘Lief, ik ben te <rhyme label="c" type="f">vreden</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Al was het <rhyme label="d" type="m">terstont</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Om met u te <rhyme label="c" type="f" corresp="#s6:l5">treden</rhyme></l>
        <l id="s6:l8" met="3" type="+-">In een vast <rhyme label="d" type="m" corresp="#s6:l6">verbont</rhyme>.’</l>
        <l id="s6:l9" met="3" type="+-">‘Ja, de Son is aan 't <rhyme label="e" type="f">dalen</rhyme>.</l>
        <l id="s6:l10" met="3" type="+-">Lieven Harder, hoort <rhyme label="f" type="m">aan</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Komt morgen myn <rhyme label="e" type="f" corresp="#s6:l9">halen</rhyme></l>
        <l id="s6:l12" met="3" type="+-">Ik sal met u <rhyme label="f" type="m" corresp="#s6:l10">gaan</rhyme>.’</l>
      </lg>
    </lg>
  </text>
</song>
