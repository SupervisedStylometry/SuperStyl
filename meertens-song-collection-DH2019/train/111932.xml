    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'T Soet Gevogelt hoortmen <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s1:l2" met="2" type="+-">In het <rhyme label="b" type="m">Wout</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Niemant kan hem hun weeld' <rhyme label="a" type="f" corresp="#s1:l1">bedwingen</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Waer 't oock <rhyme label="b" type="m" corresp="#s1:l2">hout</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Maer ick vin,, en <rhyme label="c" type="m">versin</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Dat de Min,, 't singen my <rhyme label="d" type="m">belet</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Ach wreede <rhyme label="d" type="m" corresp="#s1:l6">Wet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Achter 't Lommer moet ick <rhyme label="a" type="f">schuylen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Met Godt <rhyme label="b" type="m">Pan</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">By de Sayer duyst're <rhyme label="a" type="f" corresp="#s2:l1">kuylen</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Wat ick <rhyme label="b" type="m" corresp="#s2:l2">kan</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Schoone Son,, die 't hart <rhyme label="c" type="m">won</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">En mijn kon,, doen volgen u <rhyme label="d" type="m">gebiet</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">U laten <rhyme label="d" type="m" corresp="#s2:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">O Goddin mijn schoon der <rhyme label="a" type="f">schoonen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Siet de <rhyme label="b" type="m">Krans</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Daer ick soud' mede <rhyme label="a" type="f" corresp="#s3:l1">Kroonen</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Aen den <rhyme label="b" type="m" corresp="#s3:l2">dans</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Soo ghy my,, d'Harder <rhyme label="c" type="m">dy</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Stelden vry alderwaerste <rhyme label="d" type="m">Harderin</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Door u <rhyme label="d" type="m" corresp="#s3:l6">weer-min</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laura laet u Lipjes <rhyme label="a" type="f">kussen</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Bid ick <rhyme label="b" type="m">uw'</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Om der Sonnes hett' te <rhyme label="a" type="f" corresp="#s4:l1">blussen</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Zijt niet <rhyme label="b" type="m" corresp="#s4:l2">schuw</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Komt doch voort,, spreeckt een <rhyme label="c" type="m">woort</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Niet verstoort,, uwen Menander <rhyme label="d" type="m">haet</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Jont hem <rhyme label="d" type="m" corresp="#s4:l6">genaet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sult ghy mijn Princes <rhyme label="a" type="f">genesen</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Alle mijn <rhyme label="b" type="m">smart</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Sult ghy Lief genadigh <rhyme label="a" type="f" corresp="#s5:l1">wesen</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Dit mijn <rhyme label="b" type="m" corresp="#s5:l2">hart</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Soo sal 't leet, dat ghy <rhyme label="c" type="m">deet</rhyme></l>
        <l id="s5:l6" met="5" type="-+">En wel weet,, my moeyen <rhyme label="d" type="m">nimmermeer</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Door 't haest <rhyme label="d" type="m" corresp="#s5:l6">verkeert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
