    <tags>
      <tag standard-name="dialooglied" translation="dialogue song">dialooglied</tag>
    </tags>
    <melody>Rozemondje die mijn oog</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Thetis Golven gulsigh <rhyme label="a" type="m">nat</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghy myn niet meer <rhyme label="a" type="m" corresp="#s1:l1">bespat</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Want geen bracke <rhyme label="b" type="m">vogh</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">noyt Ziel, <rhyme label="a" type="m" corresp="#s1:l1">besat</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Die 't soet voort sout wel <rhyme label="b" type="m" corresp="#s1:l3">moght</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Vliet dan Thety uyt myn <rhyme label="c" type="m">sin</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Want al u stuyr <rhyme label="d" type="m">gewelt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Noyt soetheyt geeft tot <rhyme label="c" type="m" corresp="#s1:l6">Min</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Die Venus queeckt, en <rhyme label="d" type="m" corresp="#s1:l7">stelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Waerom ben ick nu so <rhyme label="a" type="m">wreet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Deed' ick u immer <rhyme label="a" type="m" corresp="#s2:l1">leet</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Doe ghy mynen <rhyme label="b" type="m">rugh</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Met Kielen <rhyme label="a" type="m" corresp="#s2:l1">sneet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Door Seyl, die wint maeckt <rhyme label="b" type="m" corresp="#s2:l3">vlugh</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Liet ick u niet alles <rhyme label="c" type="m">toe</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En braghtje daer ghy <rhyme label="d" type="m">wou</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Waerom bent ghy my <rhyme label="c" type="m" corresp="#s2:l6">moe</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Daer ick u diende <rhyme label="d" type="m" corresp="#s2:l7">trou</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ist om dat ick door de <rhyme label="a" type="m">smert</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vertrock myn woeligh <rhyme label="a" type="m" corresp="#s3:l1">hert</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Dat u trots <rhyme label="b" type="m">Gallioen</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Bevochtight <rhyme label="a" type="m" corresp="#s3:l1">werdt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat kan ick niet <rhyme label="b" type="m" corresp="#s3:l3">verhoen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Lastert, lastert, lastert <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En noemt my soo ghy <rhyme label="d" type="m">wilt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">De wreetheyt blijft myn <rhyme label="c" type="m" corresp="#s3:l6">by</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Myn vlieten blyven <rhyme label="d" type="m" corresp="#s3:l7">zilt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Voert ghy niet een machtigh <rhyme label="a" type="m">Schip</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Op harde Key of <rhyme label="a" type="m" corresp="#s4:l1">Klip</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Dat de Kiel dan <rhyme label="b" type="m">barst</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">En Mast, en <rhyme label="a" type="m" corresp="#s4:l1">Tip</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Zinght, zit op gronden <rhyme label="b" type="m" corresp="#s4:l3">vast</rhyme>.</l>
        <l id="s4:l6" met="4" type="+-">Delght ghy dan niet meenigh <rhyme label="c" type="m">borst</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En haer na Garon <rhyme label="d" type="m">sent</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Daer haer de Vis <rhyme label="c" type="m" corresp="#s4:l6">vermorst</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En pijnelijck <rhyme label="d" type="m" corresp="#s4:l7">verslent</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wegh dan wreeden fellen <rhyme label="a" type="m">Beest</rhyme>:</l>
        <l id="s5:l2" met="3" type="-+">Wegh, wegh, onrusten <rhyme label="a" type="m" corresp="#s5:l1">Geest</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Hier is meerder <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Hier's Ianus <rhyme label="a" type="m" corresp="#s5:l1">Feest</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die myn met Nectar <rhyme label="b" type="m" corresp="#s5:l3">voet</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Hier is Cipres Venus <rhyme label="c" type="m">Landt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En d'Ambrozijne <rhyme label="d" type="m">klip</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Hier sweeft Cupid' langst de <rhyme label="c" type="m" corresp="#s5:l6">strant</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Wegh Zee, weg houte <rhyme label="d" type="m" corresp="#s5:l7">Schip</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zy is van Coelioos <rhyme label="a" type="m">Bloot</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Doch ick heb haer <rhyme label="a" type="m" corresp="#s6:l1">gevoet</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Ick heb haer <rhyme label="b" type="m">bewaert</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Ick ben't die't <rhyme label="a" type="m" corresp="#s6:l1">soet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Van Schuym eerst heb <rhyme label="b" type="m" corresp="#s6:l3">vergaert</rhyme>;</l>
        <l id="s6:l6" met="4" type="+-">Swijght want 'k ben den eersten <rhyme label="c" type="m">doch</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die Cipres vreugde <rhyme label="d" type="m">zand</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Ich ben haer Minne <rhyme label="c" type="m" corresp="#s6:l6">noch</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">'k Ben <rhyme label="d" type="m" corresp="#s6:l7">Venus-Vaderlandt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hier melt ghy nu dat ghy <rhyme label="a" type="m">sijt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Een die het soet <rhyme label="a" type="m" corresp="#s7:l1">versmijt</rhyme>.</l>
        <l id="s7:l3" met="3" type="+-">En met fel <rhyme label="b" type="m">geruys</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Begooten <rhyme label="a" type="m" corresp="#s7:l1">zijt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Je gantsche Huyt en <rhyme label="b" type="m" corresp="#s7:l3">Huys</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Want je kost geen <rhyme label="c" type="m">vrolickheen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Noch soet gelach <rhyme label="d" type="m">verstaen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Daerom sy ons <rhyme label="c" type="m" corresp="#s7:l6">verscheen</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">g' Hebt geen soet meer <rhyme label="d" type="m" corresp="#s7:l7">gelaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
