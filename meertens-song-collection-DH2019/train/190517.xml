<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190517</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Pantalone door kunst verheven</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Neen Ontaarden <rhyme label="a" type="f">Patriotten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Heerschzucht was alleen uw <rhyme label="b" type="m">doel</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Gy zult ons niet meer <rhyme label="a" type="f" corresp="#s1:l1">bedotten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Met uw haatelyk <rhyme label="b" type="m" corresp="#s1:l2">gewoel</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Niet dan Vryheit was uw <rhyme label="c" type="f">Snappen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Daar door werd gy Baas <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Zie uw valsheid nu <rhyme label="c" type="f" corresp="#s1:l5">betrappen</rhyme>.</l>
        <l id="s1:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s1:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Weg gevloekten <rhyme label="a" type="f">Aristocraaten</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Wiens gezag nooit paalen <rhyme label="b" type="m">kent</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die ons wist zoo mooi te <rhyme label="a" type="f" corresp="#s2:l1">praaten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Teegen het wettig <rhyme label="b" type="m" corresp="#s2:l2">Reglement</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Nu lagt elk om die <rhyme label="c" type="f">franje</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Men ontdekt uw list <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Tans roept elk, lang leeft <rhyme label="c" type="f" corresp="#s2:l5">Oranje</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s2:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Leugenaars en laagen <rhyme label="a" type="f">zielen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Kruijer Post der <rhyme label="b" type="m">Neder-Rhyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Hoevenaar en andere Fiel <rhyme label="a" type="f" corresp="#s3:l1">n</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Volks verleiders in het <rhyme label="b" type="m" corresp="#s3:l2">geheel</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Vlucht met Gordon na <rhyme label="c" type="f">Brittanje</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Want gy zyt nu hier te <rhyme label="d" type="m">veel</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Gy vervl ekers van <rhyme label="c" type="f" corresp="#s3:l5">Oranje</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s3:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Burgers die naar Vryheid <rhyme label="a" type="f">tragten</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Eert uw wettige <rhyme label="b" type="m">Overheid</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Laat ons het schadelyk rot <rhyme label="a" type="f" corresp="#s4:l1">veragten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat ons listig heeft <rhyme label="b" type="m" corresp="#s4:l2">misleid</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Denkt hoe dat een Lit de <rhyme label="c" type="f">Jeude</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Vol van wind zyn <rhyme label="d" type="m">schorrelkeel</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Ons deed hooren op de <rhyme label="c" type="f" corresp="#s4:l5">Neude</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s4:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Waar is Eyk zyn torts <rhyme label="a" type="f">gebleeven</rhyme>.</l>
        <l id="s5:l2" met="4" type="+-">Hy beschreid zyn dwaalens <rhyme label="b" type="m">kuer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Waart gy in uw kring <rhyme label="a" type="f" corresp="#s5:l1">gebleeven</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Thans berooiden <rhyme label="b" type="m" corresp="#s5:l2">Gouverneur</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Het gezag is uw <rhyme label="c" type="f">ontvallen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Het was een korten wyl uw <rhyme label="d" type="m">deel</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Staaten Volk ziet men op de <rhyme label="c" type="f" corresp="#s5:l5">Wallen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s5:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Auxillairen zotten <rhyme label="a" type="f">klanten</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Zwetzers, Vloekers veeltyds <rhyme label="b" type="m">zat</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">En gy Sociteits <rhyme label="a" type="f" corresp="#s6:l1">holanten</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Wakkeren baazen by het <rhyme label="b" type="m" corresp="#s6:l2">nat</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Roemt vry op uw <rhyme label="c" type="f">Heldendaaden</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Wy veragten die <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Nu moet gy uw zien <rhyme label="c" type="f" corresp="#s6:l5">versmaaden</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Eik zyn beurt is niet te <rhyme label="d" type="m" corresp="#s6:l6">veel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Zalm zien wy hier niet meer <rhyme label="a" type="f">Eeren</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Die vol zotten <rhyme label="b" type="m">hovaardy</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Onze Staat kwam <rhyme label="a" type="f" corresp="#s7:l1">Overheeren</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Tot bederf der <rhyme label="b" type="m" corresp="#s7:l2">Burgery</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Hy is weg, zyn Volkje <rhyme label="c" type="f">meeden</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Niets blyft van hem in het <rhyme label="d" type="m">geheel</rhyme>.</l>
        <l id="s7:l7" met="4" type="+-">WILLEM kwam, met hem de <rhyme label="c" type="f" corresp="#s7:l5">VREEDEN</rhyme>.</l>
        <l id="s7:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s7:l6">veel</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Goeden Hemel wilt dog <rhyme label="a" type="f">geeven</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Dat wy in ons <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Vreeden en Eendragt zien <rhyme label="a" type="f" corresp="#s8:l1">herleeven</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Dat 's het behout der Eendragts <rhyme label="b" type="m" corresp="#s8:l2">Band</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Doet Oranje in voorspoet <rhyme label="c" type="f">bloeijen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Zoo behouden wy het <rhyme label="d" type="m">Geheel</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Vry van Patriotten <rhyme label="c" type="f" corresp="#s8:l5">Boeijen</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Elk zyn beurt is niet te <rhyme label="d" type="m" corresp="#s8:l6">veel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
