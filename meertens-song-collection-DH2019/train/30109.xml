      <category translation="religion">godsdienst</category>
    </categories>
  <author>Philips van Marnix van Sint Aldegonde</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">PRijst den Heer al om end <rhyme label="a" type="m">om</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Prijst hem in sijn <rhyme label="a" type="m" corresp="#s1:l1">heyligdom</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Inden Hemel <rhyme label="b" type="m">uytgebreydt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">End doir sijne cracht <rhyme label="b" type="m" corresp="#s1:l3">bereydt</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Godt de Heer sy hoog <rhyme label="c" type="f">gepresen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Om sijn wonderbare <rhyme label="d" type="m">macht</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Om de grootheyt ende <rhyme label="d" type="m" corresp="#s1:l6">cracht</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Van sijn eewich godlijck <rhyme label="c" type="f" corresp="#s1:l5">wesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Datmen byder <rhyme label="a" type="m">stemgesang</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Voege des basuyns <rhyme label="a" type="m" corresp="#s2:l1">geclanck</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Psalter ende harpen <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Om te loven Gods naem <rhyme label="b" type="m" corresp="#s2:l3">goet</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Op violen ende <rhyme label="c" type="f">snaren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Citer, orgels ende <rhyme label="d" type="m">luyt</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Wilt met trommelslach end <rhyme label="d" type="m" corresp="#s2:l6">fluyt</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Sijnen lofsanck hoog <rhyme label="c" type="f" corresp="#s2:l5">vermaren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Loift God met cimbalen <rhyme label="a" type="m">hel</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Ende met schalmeyen <rhyme label="a" type="m" corresp="#s3:l1">spel</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Sijnd' in u ghemoet <rhyme label="b" type="m">verheugt</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Loift den Heer, met rechter <rhyme label="b" type="m" corresp="#s3:l3">vreugt</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Laet al wat kan tonge <rhyme label="c" type="f">roeren</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Oft wat geest end adem <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">End op deser werelt <rhyme label="d" type="m" corresp="#s3:l6">leeft</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">In den mont, Gods lofsanck <rhyme label="c" type="f" corresp="#s3:l5">voeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
