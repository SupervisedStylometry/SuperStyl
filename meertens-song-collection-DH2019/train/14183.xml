  <author>Frans vander Straten de jongere</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MEt blyschap wilt <rhyme label="a" type="f">aenhooren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Een liedt vol van <rhyme label="b" type="m">jolijt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Want nu een is <rhyme label="a" type="f" corresp="#s1:l1">ghebooren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die ons van sonden <rhyme label="b" type="m" corresp="#s1:l2">bevrijt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Van Godt <rhyme label="b" type="m" corresp="#s1:l2">ghebenedijt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Des menschen <rhyme label="c" type="m">middelaer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">V hert in hem <rhyme label="b" type="m" corresp="#s1:l2">verblijt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In desen nieuwen <rhyme label="c" type="m" corresp="#s1:l6">Jaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al uyt des Hemels <rhyme label="a" type="f">throone</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Door liefde <rhyme label="b" type="m">delicaet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Heeft Godt zijn eenich <rhyme label="a" type="f" corresp="#s2:l1">Soone</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ghesonden t'onser <rhyme label="b" type="m" corresp="#s2:l2">baet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Wy die seer <rhyme label="b" type="m" corresp="#s2:l2">desolaet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Waren oock <rhyme label="c" type="m">allegaer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Sijn verlost uyt <rhyme label="b" type="m" corresp="#s2:l2">ghenae</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Met desen nieuwen <rhyme label="c" type="m" corresp="#s2:l6">Jaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wie die door Adams <rhyme label="a" type="f">sonden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Waren <rhyme label="b" type="m">ghecondamneert</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Eeuwich tot allen <rhyme label="a" type="f" corresp="#s3:l1">stonden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Te zijn <rhyme label="b" type="m" corresp="#s3:l2">ghetormenteert</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maer Christus hooch <rhyme label="b" type="m" corresp="#s3:l2">ghe-eert</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die pyn uyt liefden <rhyme label="c" type="m">claer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Te lyden heeft <rhyme label="b" type="m" corresp="#s3:l2">begheert</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Voor ons int nieuwe <rhyme label="c" type="m" corresp="#s3:l6">Jaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Laet ons wel <rhyme label="a" type="f">overdencken</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wat pijn en smerte <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy leedt om ons te <rhyme label="a" type="f" corresp="#s4:l1">schencken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Het leven voor de <rhyme label="b" type="m" corresp="#s4:l2">doot</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Jae seer beancxt in <rhyme label="b" type="m" corresp="#s4:l2">noot</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hinck hy aent cruys voor <rhyme label="c" type="m">waer</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Sijn dierbaer Bloet <rhyme label="b" type="m" corresp="#s4:l2">uytgoot</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Voor ons int nieuwe <rhyme label="c" type="m" corresp="#s4:l6">Jaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Soo groote liefde <rhyme label="a" type="f">ghepresen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Niemant oyt heeft <rhyme label="b" type="m">volbracht</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als Christus heeft <rhyme label="a" type="f" corresp="#s5:l1">bewesen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Aent Menschelick <rhyme label="b" type="m" corresp="#s5:l2">gheslacht</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Want ons uyt s'vyants <rhyme label="b" type="m" corresp="#s5:l2">macht</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Heeft hy verlost <rhyme label="c" type="m">eenpaer</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wilt hem Dach ende <rhyme label="b" type="m" corresp="#s5:l2">Nacht</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Prysen int nieuwe <rhyme label="c" type="m" corresp="#s5:l6">Jaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maer vierich uyt den <rhyme label="a" type="f">geest</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met een hert reyn van <rhyme label="b" type="m">gronde</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Altijt den Heere <rhyme label="a" type="f" corresp="#s6:l1">vreest</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dient hem minst ende <rhyme label="b" type="m" corresp="#s6:l2">meest</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Leeft niet in sonden <rhyme label="b" type="m" corresp="#s6:l2">swaer</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Maer Godts naevolghers <rhyme label="c" type="m">weest</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">In desen nieuwen <rhyme label="b" type="m" corresp="#s6:l2">Jaer</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Laet ons niet ydel <rhyme label="c" type="m" corresp="#s6:l6">draghen</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Den naem Christenen <rhyme label="a" type="f">reyn</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Och tis wel te <rhyme label="b" type="m">beclaghen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dat ment soo siet <rhyme label="a" type="f" corresp="#s7:l1">ghemeyn</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Want in Gods Kerck <rhyme label="b" type="m" corresp="#s7:l2">certeyn</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Noch veel begheven <rhyme label="b" type="m" corresp="#s7:l2">haer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Te volghen t'vleesch <rhyme label="c" type="m">vieleyn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Schaut sulcx int nieuwe <rhyme label="b" type="m" corresp="#s7:l2">Jaer</rhyme>.</l>
        <l id="s7:l8" met="3" type="-+">En laet ons doch <rhyme label="c" type="m" corresp="#s7:l6">versaken</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">s'Vleesch wercken <rhyme label="a" type="f">altemael</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ende na deucht nu <rhyme label="b" type="m">haken</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Oock na liefde <rhyme label="a" type="f" corresp="#s8:l1">principael</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Wandelt in Christus <rhyme label="b" type="m" corresp="#s8:l2">Sael</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Daer zijn stem clinckt <rhyme label="b" type="m" corresp="#s8:l2">eerbaer</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Naer sulcken <rhyme label="c" type="m">Nachtegael</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Luystert int nieuwe <rhyme label="b" type="m" corresp="#s8:l2">Jaer</rhyme>.</l>
        <l id="s8:l8" met="3" type="-+">Op dat wy moghen <rhyme label="c" type="m" corresp="#s8:l6">raken</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Naemaels int soete <rhyme label="a" type="f">dal</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Daer wy vreucht sullen <rhyme label="b" type="m">smaken</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die eeuwich deuren <rhyme label="a" type="f" corresp="#s9:l1">sal</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Met Godts lieve <rhyme label="b" type="m" corresp="#s9:l2">ghetal</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">By die Engelsche <rhyme label="b" type="m" corresp="#s9:l2">schaer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Jae met Godts Kinders <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Juychen int nieuwe <rhyme label="b" type="m" corresp="#s9:l2">Jaer</rhyme>.</l>
        <l id="s9:l8" met="3" type="-+">Oorlof Broeders <rhyme label="c" type="m" corresp="#s9:l6">verheven</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ons Prins danckbaerheyt <rhyme label="a" type="f">biet</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Tracht nae zijn woort <rhyme label="b" type="m">t'leven</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Altijt de Sonde <rhyme label="a" type="f" corresp="#s10:l1">vliet</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Soo naeckt u gheen <rhyme label="b" type="m" corresp="#s10:l2">verdriet</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Want die hem volghen <rhyme label="b" type="m" corresp="#s10:l2">naer</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Wilt hy schencken om <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Een salich nieuwe <rhyme label="b" type="m" corresp="#s10:l2">Jaer</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Verheucht in <rhyme label="c" type="m" corresp="#s10:l6">deucht</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
