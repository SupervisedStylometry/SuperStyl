  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAer is een Vent <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met Waveltjes te <rhyme label="b" type="m">koop</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Wie zou daer niet van <rhyme label="a" type="f" corresp="#s1:l1">droomen</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Hij had een heele <rhyme label="b" type="m" corresp="#s1:l2">hoop</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Veel Volk quam te <rhyme label="c" type="f">zamen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hy riep een keer drie, <rhyme label="d" type="m">vier</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Voelam Plaisier  de <rhyme label="c" type="f" corresp="#s1:l5">Dame</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Voelam, Voelam <rhyme label="d" type="m" corresp="#s1:l6">Plaisier</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy quam by een Mooij <rhyme label="a" type="f">Meisje</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hij zeij mijn Liefste <rhyme label="b" type="m">Kind</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Jou poezele blanke <rhyme label="a" type="f" corresp="#s2:l1">Vleisje</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat werd van my <rhyme label="b" type="m" corresp="#s2:l2">bemind</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Zeg mij hoe is uw <rhyme label="c" type="f">name</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Bent gij geen <rhyme label="d" type="m">Kamenier</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Voelam Plaisier de <rhyme label="c" type="f" corresp="#s2:l5">Dame</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Voelam, Voelam <rhyme label="d" type="m" corresp="#s2:l6">Plaisier</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat mag die Vent al <rhyme label="a" type="f">praten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sprak zy of is hij <rhyme label="b" type="m">Gek</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">En dat hier op de <rhyme label="a" type="f" corresp="#s3:l1">Straten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ik zeg dat gy <rhyme label="b" type="m" corresp="#s3:l2">vertrekt</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Gij hoorde je wat te <rhyme label="c" type="f">schaeme</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Jou onhebbelyk <rhyme label="d" type="m">Dier</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Voelam Plaisier de <rhyme label="c" type="f" corresp="#s3:l5">Dame</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Voelam, Voelam <rhyme label="d" type="m" corresp="#s3:l6">Plaisier</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hoe komje me zoo <rhyme label="a" type="f">veragten</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">Je bent een rare <rhyme label="b" type="m">Meid</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Ik had andere <rhyme label="a" type="f" corresp="#s4:l1">gedagten</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">Maer ziet ik ben <rhyme label="b" type="m" corresp="#s4:l2">misleid</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">En ik ben wel <rhyme label="c" type="f">bekwame</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Al was ’t een keer drie, <rhyme label="d" type="m">vier</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Voelam Plaisier de <rhyme label="c" type="f" corresp="#s4:l5">Dame</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Voelam, Voelam <rhyme label="d" type="m" corresp="#s4:l6">Plaisier</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Bent gij der zoo een <rhyme label="a" type="f">kwantje</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Sprak ‘er dat Meisje <rhyme label="b" type="m">zoet</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Ik waeg met jou een <rhyme label="a" type="f" corresp="#s5:l1">Trantje</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met dat je ’t stilletjes <rhyme label="b" type="m" corresp="#s5:l2">doet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Kom gaene wy dan <rhyme label="c" type="f">zaeme</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ik woon niet ver van <rhyme label="d" type="m">hier</rhyme>;</l>
        <l id="s5:l7" met="3" type="-+">Voelam Plaisier de <rhyme label="c" type="f" corresp="#s5:l5">Dame</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Voelam, Voelam <rhyme label="d" type="m" corresp="#s5:l6">Plaisier</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Zoo zijn de Meisjes <rhyme label="a" type="f">alle</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En het is al maer <rhyme label="b" type="m">praet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Zij houw zo graeg van ’t <rhyme label="a" type="f" corresp="#s6:l1">malle</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En valle strak op haer <rhyme label="b" type="m" corresp="#s6:l2">plat</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Je hoefje maer niet te <rhyme label="c" type="f">schame</rhyme>;</l>
        <l id="s6:l6" met="3" type="-+">En maken ook geen <rhyme label="d" type="m">getier</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Voelam Plaisier de <rhyme label="c" type="f" corresp="#s6:l5">Dame</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Voelam, Voelam <rhyme label="d" type="m" corresp="#s6:l6">Plaisier</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
