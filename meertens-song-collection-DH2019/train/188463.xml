  <author>Antoine Mahaut, Kornelis Elzevier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ik wil wel <rhyme label="a" type="f">beminnen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Maer laet myne <rhyme label="a" type="f" corresp="#s1:l1">zinnen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Zoo niet <rhyme label="a" type="f" corresp="#s1:l1">verwinnen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Dat 'k treuren <rhyme label="b" type="m">zou</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Neen ik ben zoo zot <rhyme label="c" type="f">niet</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Neen Jan is zoo bot <rhyme label="c" type="f" corresp="#s1:l5">niet</rhyme></l>
        <l id="s1:l7" met="2" type="-+">'k Ben Venus spot <rhyme label="c" type="f" corresp="#s1:l5">niet</rhyme>.</l>
        <l id="s1:l8" met="2" type="-+">'k Lach met de <rhyme label="b" type="m" corresp="#s1:l4">trouw</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Een Nimf mooi van <rhyme label="a" type="f">kaken</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Tracht zich wys te <rhyme label="a" type="f" corresp="#s2:l1">maken</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">My te doen <rhyme label="a" type="f" corresp="#s2:l1">blaken</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Maer 't is <rhyme label="b" type="m">gemist</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">'k Ben nauwelyks <rhyme label="c" type="f">henen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Of 't Vuur is <rhyme label="c" type="f" corresp="#s2:l5">verdwenen</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">'k Zorg voor myn <rhyme label="c" type="f" corresp="#s2:l5">schenen</rhyme>.</l>
        <l id="s2:l8" met="2" type="-+">'k Lach met haer <rhyme label="b" type="m" corresp="#s2:l4">list</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ik voed haere <rhyme label="a" type="f">lusjes</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Met sappige <rhyme label="a" type="f" corresp="#s3:l1">kusjes</rhyme>:</l>
        <l id="s3:l3" met="2" type="-+">Maer 't is als <rhyme label="a" type="f" corresp="#s3:l1">flusjes</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">'k Spot met het <rhyme label="b" type="m">dier</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Wyl ik vaek <rhyme label="c" type="f">verander</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Ik ben haer te <rhyme label="c" type="f" corresp="#s3:l5">schrander</rhyme>;</l>
        <l id="s3:l7" met="2" type="-+">Als een <rhyme label="c" type="f" corresp="#s3:l5">Sal'mander</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Leef ik in 't <rhyme label="b" type="m" corresp="#s3:l4">Vier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
