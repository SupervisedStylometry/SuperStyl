      <category translation="religion">godsdienst</category>
    </categories>
  <author>Cornelis Stribe√©</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">AL wat ick jaegh het is al <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Hoe dat ick smeek sy snelder <rhyme label="a" type="m" corresp="#s1:l1">vliedt</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Of ick <rhyme label="b" type="m">sucht</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">En u <rhyme label="b" type="m" corresp="#s1:l3">vlucht</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Soeck op te <rhyme label="c" type="f">schorten</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Of ren, en men my na u <rhyme label="d" type="m">toe</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">En maeck mijn jacht en winden <rhyme label="d" type="m" corresp="#s1:l6">moe</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Dus de <rhyme label="e" type="m">rou</rhyme></l>
        <l id="s1:l9" met="2" type="+-">Sal o <rhyme label="e" type="m" corresp="#s1:l8">Vrou</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Noch mijn Lent <rhyme label="c" type="f" corresp="#s1:l5">verkorten</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Maer en <rhyme label="f" type="m">zijt</rhyme></l>
        <l id="s1:l12" met="2" type="+-">t' Geender <rhyme label="f" type="m" corresp="#s1:l11">tijt</rhyme></l>
        <l id="s1:l13" met="2" type="+-">dan <rhyme label="g" type="m">bevreest</rhyme>,</l>
        <l id="s1:l14" met="2" type="+-">Als mijn <rhyme label="g" type="m" corresp="#s1:l13">Geest</rhyme></l>
        <l id="s1:l15" met="2" type="+-">(Heel <rhyme label="g" type="m" corresp="#s1:l13">bedeest</rhyme>)</l>
        <l id="s1:l16" met="3" type="-+">Voor u komt tranen <rhyme label="c" type="f" corresp="#s1:l5">storten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Emilia schoon <rhyme label="a" type="m">Diamandt</rhyme>!</l>
        <l id="s2:l2" met="4" type="-+">Wiens glinster-strael als Pheby <rhyme label="a" type="m" corresp="#s2:l1">brandt</rhyme>:</l>
        <l id="s2:l3" met="2" type="+-">Want het <rhyme label="b" type="m">licht</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Uws <rhyme label="b" type="m" corresp="#s2:l3">Gesicht</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Veel vlams <rhyme label="c" type="f">ontsteken</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Want gaet ghy in u Vaders <rhyme label="d" type="m">Hof</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">U duyven-ooghjens doen het <rhyme label="d" type="m" corresp="#s2:l6">lof</rhyme></l>
        <l id="s2:l8" met="2" type="+-">Open <rhyme label="e" type="m">gaen</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Dat de <rhyme label="e" type="m" corresp="#s2:l8">blaen</rhyme></l>
        <l id="s2:l10" met="3" type="+-">Met een kracht <rhyme label="c" type="f" corresp="#s2:l5">uytbreken</rhyme>;</l>
        <l id="s2:l11" met="2" type="+-">Hoe veel <rhyme label="f" type="m">meer</rhyme></l>
        <l id="s2:l12" met="2" type="+-">En te <rhyme label="f" type="m" corresp="#s2:l11">eer</rhyme></l>
        <l id="s2:l13" met="2" type="+-">Ick die <rhyme label="g" type="m">blaeck</rhyme></l>
        <l id="s2:l14" met="2" type="+-">Voel de <rhyme label="g" type="m" corresp="#s2:l13">smaeck</rhyme></l>
        <l id="s2:l15" met="2" type="+-">Met <rhyme label="g" type="m" corresp="#s2:l13">vermaeck</rhyme></l>
        <l id="s2:l16" met="3" type="-+">Uws lieffelijcke <rhyme label="c" type="f" corresp="#s2:l5">treken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Men siet de blauwe Aders <rhyme label="a" type="m">wel</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Spelende onder 't witte <rhyme label="a" type="m" corresp="#s3:l1">vel</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Als een <rhyme label="b" type="m">Ree</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Of een <rhyme label="b" type="m" corresp="#s3:l3">Zee</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Met sijn <rhyme label="c" type="f">Revieren</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Veel duysent krulletjens siet men <rhyme label="d" type="m">daer</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Krunckelen onder 't blonde <rhyme label="d" type="m" corresp="#s3:l6">haer</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Wiens <rhyme label="e" type="m">ghekrans</rhyme></l>
        <l id="s3:l9" met="2" type="+-">Ende <rhyme label="e" type="m" corresp="#s3:l8">glans</rhyme></l>
        <l id="s3:l10" met="3" type="+-">De Goden <rhyme label="c" type="f" corresp="#s3:l5">vieren</rhyme>;</l>
        <l id="s3:l11" met="2" type="+-">Maer u <rhyme label="f" type="m">Jeucht</rhyme></l>
        <l id="s3:l12" met="2" type="+-">En u <rhyme label="f" type="m" corresp="#s3:l11">deucht</rhyme></l>
        <l id="s3:l13" met="2" type="+-">En u <rhyme label="g" type="m">eer</rhyme></l>
        <l id="s3:l14" met="2" type="+-">Blinckt veel <rhyme label="g" type="m" corresp="#s3:l13">meer</rhyme></l>
        <l id="s3:l15" met="2" type="+-">Van de <rhyme label="g" type="m" corresp="#s3:l13">Veer</rhyme></l>
        <l id="s3:l16" met="3" type="-+">Van d' Africaensche <rhyme label="c" type="f" corresp="#s3:l5">Dieren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Tis Venus die u Tuytjes <rhyme label="a" type="m">dreyt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Haer Soontje die de <rhyme label="a" type="m" corresp="#s4:l1">aerdigheyt</rhyme></l>
        <l id="s4:l3" met="2" type="+-">In u <rhyme label="b" type="m">teelt</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Als ghy <rhyme label="b" type="m" corresp="#s4:l3">speelt</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Ofte quelt een <rhyme label="c" type="f">Lietje</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Dan straelt u schoonheydt door het <rhyme label="d" type="m">glas</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Schoonder, of Phoebus by u <rhyme label="d" type="m" corresp="#s4:l6">was</rhyme>.</l>
        <l id="s4:l8" met="2" type="+-">En den <rhyme label="e" type="m">toon</rhyme></l>
        <l id="s4:l9" met="2" type="+-">Gaet soo <rhyme label="e" type="m" corresp="#s4:l8">schoon</rhyme></l>
        <l id="s4:l10" met="3" type="+-">Als of Pan sijn <rhyme label="c" type="f" corresp="#s4:l5">Rietje</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Weer in 't <rhyme label="f" type="m">Velt</rhyme></l>
        <l id="s4:l12" met="2" type="+-">Hadd' <rhyme label="f" type="m" corresp="#s4:l11">gestelt</rhyme>,</l>
        <l id="s4:l13" met="2" type="+-">
          <rhyme label="g" type="m">Onbeschroomt</rhyme>
        </l>
        <l id="s4:l14" met="2" type="+-">By 't <rhyme label="g" type="m" corresp="#s4:l13">gheboomt</rhyme>;</l>
        <l id="s4:l15" met="2" type="+-">Dan soo <rhyme label="g" type="m" corresp="#s4:l13">stroomt</rhyme></l>
        <l id="s4:l16" met="3" type="-+">Elck traen een water <rhyme label="c" type="f" corresp="#s4:l5">vlietje</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dus ghy o schoonste <rhyme label="a" type="m">Sonne-licht</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Laet doch het kleen en soete <rhyme label="a" type="m" corresp="#s5:l1">Wicht</rhyme></l>
        <l id="s5:l3" met="2" type="+-">Met een <rhyme label="b" type="m">pijl</rhyme></l>
        <l id="s5:l4" met="2" type="+-">Inder <rhyme label="b" type="m" corresp="#s5:l3">yl</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Uwe Borst <rhyme label="c" type="f">aenransen</rhyme>:</l>
        <l id="s5:l6" met="4" type="-+">Wy sullen jaghen met ons <rhyme label="d" type="m">jacht</rhyme></l>
        <l id="s5:l7" met="4" type="-+">By Maneschijn: of duyster <rhyme label="d" type="m" corresp="#s5:l6">nacht</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">In het <rhyme label="e" type="m">Bos</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">Daer het <rhyme label="e" type="m" corresp="#s5:l8">blos</rhyme></l>
        <l id="s5:l10" met="3" type="+-">Bloeyt <rhyme label="c" type="f" corresp="#s5:l5">Myrthe-Transsen</rhyme>:</l>
        <l id="s5:l11" met="2" type="+-">En het <rhyme label="f" type="m">sal</rhyme></l>
        <l id="s5:l12" met="2" type="+-">Met <rhyme label="f" type="m" corresp="#s5:l11">gheschal</rhyme></l>
        <l id="s5:l13" met="2" type="+-">Al wat <rhyme label="g" type="m">leeft</rhyme>,</l>
        <l id="s5:l14" met="2" type="+-">'t Geen daer <rhyme label="g" type="m" corresp="#s5:l13">zweeft</rhyme></l>
        <l id="s5:l15" met="2" type="+-">Of Adam <rhyme label="g" type="m" corresp="#s5:l13">heeft</rhyme></l>
        <l id="s5:l16" met="3" type="-+">Om ons met vreughde <rhyme label="c" type="f" corresp="#s5:l5">danssen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
