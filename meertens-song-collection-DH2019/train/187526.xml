<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187526</id>
    <date>
      <year-start>1693</year-start>
      <year-end>1693</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Laatstmaal ging ik ter jacht</melody>
  <author>Frank A. Metaal</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoort toe Visschers <rhyme label="a" type="m">gelijck</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die de Noort-zee <rhyme label="b" type="f">beminnen</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">’t Zy arrem ofte <rhyme label="a" type="m" corresp="#s1:l1">rijck</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wilt het met Godt <rhyme label="b" type="f" corresp="#s1:l2">beginnen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Laet den Heer leggen in u <rhyme label="c" type="m">hert</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Wijckt doch van hem niet alsoo <rhyme label="c" type="m" corresp="#s1:l5">vart</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Of anders naeckt u <rhyme label="c" type="m" corresp="#s1:l5">smart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want gy moet heen en <rhyme label="a" type="m">weer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Door seer veel woeste <rhyme label="b" type="f">baren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Met kommer en <rhyme label="a" type="m" corresp="#s2:l1">hert-seer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In uwe Schepen <rhyme label="b" type="f" corresp="#s2:l2">varen</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Siet dat gy den Heer krijght te <rhyme label="c" type="m">baet</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Op dat hy u in nood <rhyme label="c" type="m" corresp="#s2:l5">bystaet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als het u qualijck <rhyme label="c" type="m" corresp="#s2:l5">gaet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gy hebt oock veel <rhyme label="a" type="m">aen-stoot</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van Zee ende <rhyme label="b" type="f">Vyanden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die met haer krachten <rhyme label="a" type="m" corresp="#s3:l1">groot</rhyme></l>
        <l id="s3:l4" met="3" type="-+">U dickmael fel <rhyme label="b" type="f" corresp="#s3:l2">aenranden</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">En gy en hebt gantsch geen <rhyme label="c" type="m">geweer</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Dus neemt u toe-vlucht tot den <rhyme label="c" type="m" corresp="#s3:l5">Heer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">In al u groot <rhyme label="c" type="m" corresp="#s3:l5">hart-seer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Past wel op ’t <rhyme label="a" type="m">gety</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Om op geen droogh te <rhyme label="b" type="f">raecken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Siet toe wel voor de <rhyme label="a" type="m" corresp="#s4:l1">ly</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Op dat gy meught <rhyme label="b" type="f" corresp="#s4:l2">genaecken</rhyme></l>
        <l id="s4:l5" met="4" type="-+">In een gewenste haven <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Danckt dan den Heer met groot <rhyme label="c" type="m" corresp="#s4:l5">ootmoet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Voor ’t goet dat hy <rhyme label="c" type="m" corresp="#s4:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wel dan gy <rhyme label="a" type="m">Visser-man</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Zijt oprecht in u <rhyme label="b" type="f">saecken</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als gy uyt Zee komt <rhyme label="a" type="m" corresp="#s5:l1">an</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En dat de Koop-luy <rhyme label="b" type="f" corresp="#s5:l2">naecken</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">U Visch te koopen met <rhyme label="c" type="m">bescheyt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Dan dient de waerheyt wel <rhyme label="c" type="m" corresp="#s5:l5">geseyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dus geen leugens <rhyme label="c" type="m" corresp="#s5:l5">uytbreyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Waer dat gy u dan <rhyme label="a" type="m">keert</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">O Visschers seer <rhyme label="b" type="f">loofwaerdigh</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Daer sult gy zijn <rhyme label="a" type="m" corresp="#s6:l1">ge-eert</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Van veel menschen seer <rhyme label="b" type="f" corresp="#s6:l2">aerdigh</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Want gy wert gesien en <rhyme label="c" type="m">geacht</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Om ’t geen dat van u wert  <rhyme label="c" type="m" corresp="#s6:l5">gebracht</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Uyt Zee door Godes <rhyme label="c" type="m" corresp="#s6:l5">macht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O Princen Godt <rhyme label="a" type="m">eerbaer</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Wilt den Visschers <rhyme label="b" type="f">bewaren</rhyme>!</l>
        <l id="s7:l3" met="3" type="-+">Voor allerley <rhyme label="a" type="m" corresp="#s7:l1">gevaer</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Als hy tijdt op de <rhyme label="b" type="f" corresp="#s7:l2">baren</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">En oock ons menschen hier te <rhyme label="c" type="m">Landt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Geeft uwen zegen <rhyme label="c" type="m" corresp="#s7:l5">abondant</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Door uwe milde <rhyme label="c" type="m" corresp="#s7:l5">handt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
