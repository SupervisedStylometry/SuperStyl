<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35683</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Engelse fortuin</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">NA d' ouden <rhyme label="a" type="m">Wet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Was, al 't Vrouwe <rhyme label="b" type="m">geslacht</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Een tijdt <rhyme label="a" type="m" corresp="#s1:l1">besmet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dat Eenigh Kindt <rhyme label="b" type="m" corresp="#s1:l2">voort-bracht</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">Is 't Kindt een <rhyme label="c" type="m">Zoon</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Blijft dagen veertigh <rhyme label="d" type="m">thuys</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Wort reyn en <rhyme label="c" type="m" corresp="#s1:l5">schoon</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gaet zy in Godes <rhyme label="d" type="m" corresp="#s1:l6">huys</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Geeft zy <rhyme label="a" type="m">aldaer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Een Lam, of Duyven <rhyme label="b" type="m">twee</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Wordt reyn en <rhyme label="a" type="m" corresp="#s2:l1">klaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Offert zy 't Kindt Godt <rhyme label="b" type="m" corresp="#s2:l2">mee</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Mar√Øaes <rhyme label="c" type="m">ziel</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Besmet noyt is <rhyme label="d" type="m">gheweest</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">In zond noyt <rhyme label="c" type="m" corresp="#s2:l5">viel</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Haer alder-reynste <rhyme label="d" type="m" corresp="#s2:l6">geest</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Maria <rhyme label="a" type="m">Maeght</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Komt tot <rhyme label="b" type="m">Iesusalem</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Haer Zoonken <rhyme label="a" type="m" corresp="#s3:l1">draeght</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gebaert tot <rhyme label="b" type="m" corresp="#s3:l2">Bethlehem</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Gaet na de <rhyme label="c" type="m">Kerck</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van smetten geheel <rhyme label="d" type="m">vry</rhyme>;</l>
        <l id="s3:l7" met="2" type="-+">Met liefde <rhyme label="c" type="m" corresp="#s3:l5">sterck</rhyme></l>
        <l id="s3:l8" met="3" type="-+">d' Ouden Wet vol-doet <rhyme label="d" type="m" corresp="#s3:l6">zy</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Verarmt tot <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gebaert uyt Conincx <rhyme label="b" type="m">bloedt</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Zoo men nu <rhyme label="a" type="m" corresp="#s4:l1">ziet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Haer offerhanden <rhyme label="b" type="m" corresp="#s4:l2">doet</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Tot lossinghs <rhyme label="c" type="m">loon</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Vijf penningen <rhyme label="d" type="m">mee-brocht</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Met Duyven <rhyme label="c" type="m" corresp="#s4:l5">schoon</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die zy had <rhyme label="d" type="m" corresp="#s4:l6">op-gezocht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Uyt Adams <rhyme label="a" type="m">zaet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ontfanght niet in haer <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Net <rhyme label="a" type="m" corresp="#s5:l1">tegen-staet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Godts Wet, daer van <rhyme label="b" type="m" corresp="#s5:l2">bevrijdt</rhyme>.</l>
        <l id="s5:l5" met="2" type="-+">Al baerts' een <rhyme label="c" type="m">Kindt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Verliest geen <rhyme label="d" type="m">zuyverheydt</rhyme>;</l>
        <l id="s5:l7" met="2" type="-+">In haer men <rhyme label="c" type="m" corresp="#s5:l5">vint</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Noch Adams quade <rhyme label="d" type="m" corresp="#s5:l6">seyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Haer Zoon, haer <rhyme label="a" type="m">pandt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Tot Godes eers zy <rhyme label="b" type="m">geeft'</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Tot <rhyme label="a" type="m" corresp="#s6:l1">offerhandt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Al 't geenen dat zy <rhyme label="b" type="m" corresp="#s6:l2">heeft</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Haer Godt, haer <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Den Vader geeft <rhyme label="d" type="m">geheel</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Zy soeckt niet <rhyme label="c" type="m" corresp="#s6:l5">meer</rhyme></l>
        <l id="s6:l8" met="3" type="-+">In haer Zoon eenigh <rhyme label="d" type="m" corresp="#s6:l6">deel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Zoo mee <rhyme label="a" type="m">bemint</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ons menschen al <rhyme label="b" type="m">verdoemt</rhyme>;</l>
        <l id="s7:l3" met="2" type="-+">Geeft ons haer <rhyme label="a" type="m" corresp="#s7:l1">kindt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Salighmaecker <rhyme label="b" type="m" corresp="#s7:l2">ghenoemt</rhyme>;</l>
        <l id="s7:l5" met="2" type="-+">Die door zijn <rhyme label="c" type="m">bloet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Ons, ons verlossen <rhyme label="d" type="m">zal</rhyme>;</l>
        <l id="s7:l7" met="2" type="-+">Zijn 's Hemels <rhyme label="c" type="m" corresp="#s7:l5">goet</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Den goeden geven <rhyme label="d" type="m" corresp="#s7:l6">af</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Komt <rhyme label="a" type="m">Seraphijn</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Prijst haer <rhyme label="b" type="m">ootmoedigheyt</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Komt <rhyme label="a" type="m" corresp="#s8:l1">Cherubijn</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Looft haer <rhyme label="b" type="m" corresp="#s8:l2">gehoorzaemheyt</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">Ghy mensch niet <rhyme label="c" type="m">min</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Looft met Lof-zanghen <rhyme label="d" type="m">eer</rhyme></l>
        <l id="s8:l7" met="2" type="-+">Deez' <rhyme label="c" type="m" corresp="#s8:l5">Koningin</rhyme>;</l>
        <l id="s8:l8" met="3" type="-+">U geeft haer Al, haer <rhyme label="d" type="m" corresp="#s8:l6">Heer</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">Jesum, den <rhyme label="a" type="m">Zon</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Na een begeerte <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Heeft <rhyme label="a" type="m" corresp="#s9:l1">Zimeon</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Omhelst voor sijnen <rhyme label="b" type="m" corresp="#s9:l2">doodt</rhyme>.</l>
        <l id="s9:l5" met="2" type="-+">Singht als de <rhyme label="c" type="m">Swaen</rhyme>:</l>
        <l id="s9:l6" met="3" type="-+">Laet uw' Dienaer in <rhyme label="d" type="m">rust</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">In vrede <rhyme label="c" type="m" corresp="#s9:l5">gaen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Mijn brant is <rhyme label="d" type="m" corresp="#s9:l6">uyt-gheblust</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Met groot <rhyme label="a" type="m">ootmoet</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Aen Iesu schenckt uw' <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s10:l3" met="2" type="-+">Lichaem, ziel, <rhyme label="a" type="m" corresp="#s10:l1">goet</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Tot voorspoet, en tot <rhyme label="b" type="m" corresp="#s10:l2">smart</rhyme>.</l>
        <l id="s10:l5" met="2" type="-+">Heel <rhyme label="c" type="m">onder-houdt</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Gods Wet sonder <rhyme label="d" type="m">verschil</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Soo jongh als <rhyme label="c" type="m" corresp="#s10:l5">oudt</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Leeft altijdt na Goodts <rhyme label="d" type="m" corresp="#s10:l6">wil</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Maget en <rhyme label="a" type="m">Vrouw</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Maria! Staet ons <rhyme label="b" type="m">by</rhyme></l>
        <l id="s11:l3" met="2" type="-+">In pijn, in <rhyme label="a" type="m" corresp="#s11:l1">rouw</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Ende als wy zijn <rhyme label="b" type="m" corresp="#s11:l2">bly</rhyme>,</l>
        <l id="s11:l5" met="2" type="-+">Suyv're <rhyme label="c" type="m">fonteyn</rhyme>!</l>
        <l id="s11:l6" met="3" type="-+">Bidt dat wy ned'righ <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s11:l7" met="2" type="-+">G'hoorzaem, en <rhyme label="c" type="m" corresp="#s11:l5">reyn</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Vol liefde zonder <rhyme label="d" type="m" corresp="#s11:l6">schijn</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">Iesu! gunt <rhyme label="a" type="m">vree</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Ons: in ons levens <rhyme label="b" type="m">endt</rhyme>,</l>
        <l id="s12:l3" met="2" type="-+">Bieght, berouw, <rhyme label="a" type="m" corresp="#s12:l1">mee</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">'t Heylighste <rhyme label="b" type="m" corresp="#s12:l2">Sacrament</rhyme>,</l>
        <l id="s12:l5" met="2" type="-+">In d'uur des <rhyme label="c" type="m">doodts</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Vertoont ons <rhyme label="d" type="m">allegaer</rhyme>,</l>
        <l id="s12:l7" met="2" type="-+">Geeft ons Lam <rhyme label="c" type="m" corresp="#s12:l5">Goodts</rhyme>.</l>
        <l id="s12:l8" met="3" type="-+">Uw' vreught, en aenschijn <rhyme label="d" type="m" corresp="#s12:l6">klaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
