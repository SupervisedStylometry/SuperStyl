    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Ach wat is de maagdenstaat</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Tot lof en eer geheel <rhyme label="a" type="m">fermand</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Gaan ik de Boeren wat <rhyme label="b" type="f">verheffen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En ook met reden en <rhyme label="a" type="m" corresp="#s1:l1">verstand</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Want zy het al <rhyme label="b" type="f" corresp="#s1:l2">overtreffen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Den boer dat was den eersten <rhyme label="c" type="m">mensch</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Die van God is eedel <rhyme label="d" type="f">geschapen</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Want Adam was den eersten boer en <rhyme label="c" type="m" corresp="#s1:l5">heer</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Vivat de boer is onzen <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den boer dat was den eerste <rhyme label="a" type="m">heer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En de edelman <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ik doe aan den boer zo veel <rhyme label="a" type="m" corresp="#s2:l1">eer</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Als ik aan eenen heer zou <rhyme label="b" type="f" corresp="#s2:l2">geven</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Wy zyn al van eenen <rhyme label="c" type="m">God</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Hier gesteld en edel <rhyme label="d" type="f">geschapen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Maar den boer die ’t al <rhyme label="c" type="m" corresp="#s2:l5">bezorgt</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Vivat den boer is onze <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Geen Kyzer Koning of te <rhyme label="a" type="m">graaf</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Mogt het aardsch paradys <rhyme label="b" type="f">aanschouwen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">God gaf Adam een <rhyme label="a" type="m" corresp="#s3:l1">wyf</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Eva tot zyn <rhyme label="b" type="f" corresp="#s3:l2">huisvrouwen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Maar na den val van hun alle <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Moesten zy in ’t zweet hun aanschyn, <rhyme label="d" type="f">slaaven</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">En werken vroeg en <rhyme label="c" type="m" corresp="#s3:l5">laat</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Vivat den boer is onzen <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den boer brengt alles na de <rhyme label="a" type="m">markt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Om te koopen daar van te <rhyme label="b" type="f">leeven</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Koorn en alles wat hy <rhyme label="a" type="m" corresp="#s4:l1">heeft</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Kool aardappels daar <rhyme label="b" type="f" corresp="#s4:l2">benevens</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">’t Is waar de boer verkoopt het <rhyme label="c" type="m">duur</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Zoo een ieder kan <rhyme label="d" type="f">verklaaren</rhyme>:</l>
        <l id="s4:l7" met="4" type="-+">Maar ’t komt al van de <rhyme label="c" type="m" corresp="#s4:l5">boer</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Vivat de boer is onze <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Zelfs de Regeerders van ’t <rhyme label="a" type="m">land</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Moeten van den Boer ook <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">’t Komt al van den boer zyn <rhyme label="a" type="m" corresp="#s5:l1">land</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Het is den boer die ’t al moet <rhyme label="b" type="f" corresp="#s5:l2">geven</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Kyzer Koning en de <rhyme label="c" type="m">Graaf</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Moeten van den boer ook <rhyme label="d" type="f">leven</rhyme>:</l>
        <l id="s5:l7" met="4" type="-+">En eyder dat men my <rhyme label="c" type="m" corresp="#s5:l5">verstaat</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Vivat de boer is onzen <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Den boer die prys ik om ’t <rhyme label="a" type="m">meest</rhyme></l>
        <l id="s6:l2" met="4" type="-+">En dat om zyn <rhyme label="b" type="f">behoeftigheeden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Om dat hy den armen <rhyme label="a" type="m" corresp="#s6:l1">mensch</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Een stuk brood komt te <rhyme label="b" type="f" corresp="#s6:l2">geeven</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Die van de ryken word <rhyme label="c" type="m">versmaat</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">So hy zelver komt <rhyme label="d" type="f">verklaaren</rhyme>:</l>
        <l id="s6:l7" met="4" type="-+">Het is de boer die het alles <rhyme label="c" type="m" corresp="#s6:l5">geeft</rhyme>.</l>
        <l id="s6:l8" met="4" type="-+">Vivat de boer is onzen <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Den boer die wort van veel <rhyme label="a" type="m">verragt</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">Hy wil niet incrojabel <rhyme label="b" type="f">wezen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Een eyder leer tog aan zyn <rhyme label="a" type="m" corresp="#s7:l1">dragt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Hy is een man gelyk voor <rhyme label="b" type="f" corresp="#s7:l2">dezen</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">In de stad loop menigen <rhyme label="c" type="m">vent</rhyme>:</l>
        <l id="s7:l6" met="4" type="-+">By de een of anderen <rhyme label="d" type="f">vrouwe</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Den boer die houd het by zyn <rhyme label="c" type="m" corresp="#s7:l5">wyf</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Vivat den boer is onzen <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">’t Is waar den burger die <rhyme label="a" type="m">zeid</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Dat hy alles duur moet <rhyme label="b" type="f">kopen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Boter en kaas al wat ’t er <rhyme label="a" type="m" corresp="#s8:l1">is</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Het geen den burger veel doet <rhyme label="b" type="f" corresp="#s8:l2">loopen</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Maar dat is den wil van <rhyme label="c" type="m">God</rhyme>:</l>
        <l id="s8:l6" met="4" type="-+">Dat verstaat gy nu wel <rhyme label="d" type="f">nader</rhyme>.</l>
        <l id="s8:l7" met="4" type="-+">Wyt het dog niet aan den <rhyme label="c" type="m" corresp="#s8:l5">boer</rhyme>:</l>
        <l id="s8:l8" met="4" type="-+">Vivat den boer is onzen <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dan wat weet ik zanger <rhyme label="a" type="m">mee</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Ik moet van den boer ook <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Den boeren koopen ook van <rhyme label="a" type="m" corresp="#s9:l1">myn</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Ik prys den boeren al myn <rhyme label="b" type="f" corresp="#s9:l2">leven</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Vivat den boeren van ’t <rhyme label="c" type="m">land</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Die staan ook in myn <rhyme label="d" type="f">behagen</rhyme>.</l>
        <l id="s9:l7" met="4" type="-+">Daar voor roep ik myn leven <rhyme label="c" type="m" corresp="#s9:l5">lang</rhyme>:</l>
        <l id="s9:l8" met="4" type="-+">Vivat den boer is onzen <rhyme label="e" type="f">vader</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
