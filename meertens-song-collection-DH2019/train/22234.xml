<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22234</id>
    <date>
      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Kom doch hier bij zij allegader blij</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Kompt doch hier <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Sijt alle gader <rhyme label="a" type="m" corresp="#s1:l1">bly</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En met iolijdt singt den <rhyme label="b" type="f">Heere</rhyme>:</l>
        <l id="s1:l4" met="2" type="-+">Want Godt onse <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">En zijn salicheyt <rhyme label="c" type="m" corresp="#s1:l4">gwis</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Laet ons met danck <rhyme label="b" type="f" corresp="#s1:l3">iubileeren</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Met soeten <rhyme label="d" type="m">thoon</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En Psalmen <rhyme label="d" type="m" corresp="#s1:l7">schoon</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Synen lof altijts <rhyme label="b" type="f" corresp="#s1:l3">vermeeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Want Godt de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Vol alder <rhyme label="a" type="m" corresp="#s2:l1">eer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Is der Goden Coninck <rhyme label="b" type="f">oueral</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Want zijne handt <rhyme label="c" type="m">gendt</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Die ist al <rhyme label="c" type="m" corresp="#s2:l4">bekendt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wat het aerdtrijck voorts brenghen <rhyme label="b" type="f" corresp="#s2:l3">sal</rhyme>:</l>
        <l id="s2:l7" met="2" type="-+">En die berghen <rhyme label="d" type="m">hooch</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Die Zee, en die <rhyme label="d" type="m" corresp="#s2:l7">droocht</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Tzijn doch zijn wercken <rhyme label="b" type="f" corresp="#s2:l3">altemael</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Daerom comt nu <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Doet eenen <rhyme label="a" type="m" corresp="#s3:l1">voetval</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En wilt knielen voor den <rhyme label="b" type="f">Heere</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Die ons ghemaeckt <rhyme label="c" type="m">heeft</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Van niet, al watter <rhyme label="c" type="m" corresp="#s3:l4">leeft</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hy maeckt dat zijn volck hun <rhyme label="b" type="f" corresp="#s3:l3">gheneeren</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">Bermhertich is <rhyme label="d" type="m">hy</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Tot allen <rhyme label="d" type="m" corresp="#s3:l7">tijd</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Laet ons nu tot hem <rhyme label="b" type="f" corresp="#s3:l3">keeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Als ghy heden <rhyme label="a" type="m">hoort</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Sijn stem, en <rhyme label="a" type="m" corresp="#s4:l1">woort</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dan verhert niet uwe <rhyme label="b" type="f">herten</rhyme>:</l>
        <l id="s4:l4" met="2" type="-+">Glijckt gheschieden <rhyme label="c" type="m">claer</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Te Messa <rhyme label="c" type="m" corresp="#s4:l4">voorwaer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Al daer my u vaders met <rhyme label="b" type="f" corresp="#s4:l3">smerten</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Tempteerden <rhyme label="d" type="m">seer</rhyme>:</l>
        <l id="s4:l8" met="2" type="-+">En dolende <rhyme label="d" type="m" corresp="#s4:l7">meer</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Laeyden sy op hun pijn, en <rhyme label="b" type="f" corresp="#s4:l3">smerten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Ick hebbe <rhyme label="a" type="m">voorwaer</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Ghehadt veertich <rhyme label="a" type="m" corresp="#s5:l1">iaer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Met dit volck moeyte, en <rhyme label="b" type="f">arbeydt</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Ick sprack met <rhyme label="c" type="m">bedien</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Het zijn doch <rhyme label="c" type="m" corresp="#s5:l4">lien</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Diens hert tot dwalinghe so <rhyme label="b" type="f" corresp="#s5:l3">gheneycht</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Daerom sweer ick <rhyme label="d" type="m">hen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Met toornigher <rhyme label="d" type="m" corresp="#s5:l7">stem</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Mijn ruste en is hun niet <rhyme label="b" type="f" corresp="#s5:l3">bereyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
