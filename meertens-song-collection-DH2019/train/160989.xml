  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Meesteresse van de <rhyme label="a" type="m">Jagt</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Die ik <rhyme label="a" type="m" corresp="#s1:l1">agt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Boven alle de <rhyme label="b" type="f">Godinnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wiens altaren ik steeds <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'k Sal u dienen meer en <rhyme label="c" type="m" corresp="#s1:l4">meer</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Ja, voor ewig u <rhyme label="b" type="f" corresp="#s1:l3">beminnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">'k Slyt met u in lust en <rhyme label="a" type="m">vreugd</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Mijne <rhyme label="a" type="m" corresp="#s2:l1">jeugd</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Noit door laffe min <rhyme label="b" type="f">ontsteken</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">'k Eer in bossen, berg en <rhyme label="c" type="m">dal</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Uwe godheid boven <rhyme label="c" type="m" corresp="#s2:l4">al</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Noit voor Venus Son   <rhyme label="b" type="f" corresp="#s2:l3">besweken</rhyme>.</l>
      </lg>
    </lg>
  </text>
