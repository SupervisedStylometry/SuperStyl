<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>175890</id>
    <date>
      <year-start>1609</year-start>
      <year-end>1609</year-end>
    </date>
    <location>
      <orginal-name>amstelredam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Aanhoor geklaag o bloeiende jeugd</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Arnoldus Cobbault</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHelijck een purpur roose <rhyme label="a" type="m">muyckt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Alst gloeyend' licht der Sonnen <rhyme label="a" type="m" corresp="#s1:l1">duyekt</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Mijn hert vol <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Als het u niet <rhyme label="b" type="m" corresp="#s1:l3">siet</rhyme></l>
        <l id="s1:l5" met="2" type="-+">In droefhyt <rhyme label="a" type="m" corresp="#s1:l1">luyckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghelijck de roos weer open <rhyme label="a" type="m">gaat</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Int criecken vanden <rhyme label="a" type="m" corresp="#s2:l1">dageraat</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Soo oock mijn hert <rhyme label="b" type="m">bly</rhyme>â€™</l>
        <l id="s2:l4" met="2" type="-+">Als her comt voor <rhyme label="b" type="m" corresp="#s2:l3">dy</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ontwelgt met <rhyme label="a" type="m" corresp="#s2:l1">baat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy zijt een Sonne <rhyme label="a" type="m">t'eenemaal</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Die mijn hert brandt door uwen <rhyme label="a" type="m" corresp="#s3:l1">straal</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Want door u groot <rhyme label="b" type="m">licht</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Is blint mijn <rhyme label="b" type="m" corresp="#s3:l3">ghesicht</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Dat ick dus <rhyme label="a" type="m" corresp="#s3:l1">dwaal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ghy zijt een Sonne van <rhyme label="a" type="m">schoonheyt</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Die uwe glansen rontom <rhyme label="a" type="m" corresp="#s4:l1">spreyt</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Iaa der sterren <rhyme label="b" type="m">schijn</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Maackt ghy tot <rhyme label="b" type="m" corresp="#s4:l3">verdwijn</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Door dijn <rhyme label="a" type="m" corresp="#s4:l1">beleydt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Die Zee-regente cromme <rhyme label="a" type="m">Maan</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Mach niet by uwen luyster <rhyme label="a" type="m" corresp="#s5:l1">staan</rhyme></l>
        <l id="s5:l3" met="2" type="-+">Want der Maanen <rhyme label="b" type="m">glans</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Verandert <rhyme label="b" type="m" corresp="#s5:l3">bycans</rhyme></l>
        <l id="s5:l5" met="2" type="-+">All' uren <rhyme label="a" type="m" corresp="#s5:l1">saan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Maat uwe schoonheydt altijt <rhyme label="a" type="m">groeydt</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Oft Flora nu met blommen <rhyme label="a" type="m" corresp="#s6:l1">bloeydt</rhyme></l>
        <l id="s6:l3" met="2" type="-+">Oft dat dorren <rhyme label="b" type="m">wint</rhyme></l>
        <l id="s6:l4" met="2" type="-+">'t Water <rhyme label="b" type="m" corresp="#s6:l3">ooverblint</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Aan d'aartrijck <rhyme label="a" type="m" corresp="#s6:l1">boeydt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Och oft ghy als de Maane <rhyme label="a" type="m">waardt</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Beweeglijek end' heel <rhyme label="a" type="m" corresp="#s7:l1">onbeswaardt</rhyme></l>
        <l id="s7:l3" met="2" type="-+">Hoe wel dat sy <rhyme label="b" type="m">wis</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Ongestadich <rhyme label="b" type="m" corresp="#s7:l3">is</rhyme></l>
        <l id="s7:l5" met="2" type="-+">End licht van <rhyme label="a" type="m" corresp="#s7:l1">aardt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Soo moet ghy ymmers dynen <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Veranderen in soete <rhyme label="a" type="m" corresp="#s8:l1">min</rhyme></l>
        <l id="s8:l3" met="2" type="-+">End' blyven niet soo <rhyme label="b" type="m">herdt</rhyme></l>
        <l id="s8:l4" met="2" type="-+">Waar wtick mijn <rhyme label="b" type="m" corresp="#s8:l3">smerdt</rhyme></l>
        <l id="s8:l5" met="2" type="-+">End leet <rhyme label="a" type="m" corresp="#s8:l1">begin</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maar waarlijck ghy blijft <rhyme label="a" type="m">onbeweecht</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Tsedert dat ghy mijn hert <rhyme label="a" type="m" corresp="#s9:l1">doorreecht</rhyme></l>
        <l id="s9:l3" met="2" type="-+">Saturnus stuer, <rhyme label="b" type="m">wreet</rhyme></l>
        <l id="s9:l4" met="2" type="-+">Moet zijn u <rhyme label="b" type="m" corresp="#s9:l3">Planeet</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Dien ghy <rhyme label="a" type="m" corresp="#s9:l1">vercreecht</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">V Voorhooft nochtans anders <rhyme label="a" type="m">thoont</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Dat Venus selfs u had <rhyme label="a" type="m" corresp="#s10:l1">verschoont</rhyme></l>
        <l id="s10:l3" met="2" type="-+">End dit seydmen <rhyme label="b" type="m">my</rhyme></l>
        <l id="s10:l4" met="2" type="-+">Dat Cupido <rhyme label="b" type="m" corresp="#s10:l3">vry</rhyme>.</l>
        <l id="s10:l5" met="2" type="-+">Nymph by u <rhyme label="a" type="m" corresp="#s10:l1">woont</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Hoordt woont Cupido by u <rhyme label="a" type="m">dan</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Ghelijck oock sulcx wel wesen <rhyme label="a" type="m" corresp="#s11:l1">can</rhyme></l>
        <l id="s11:l3" met="2" type="-+">Hy die is een <rhyme label="b" type="m">Godt</rhyme></l>
        <l id="s11:l4" met="2" type="-+">Soud een Godt met <rhyme label="b" type="m" corresp="#s11:l3">spot</rhyme></l>
        <l id="s11:l5" met="2" type="-+">My leedt doen <rhyme label="a" type="m" corresp="#s11:l1">an</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Dit onghemack end' <rhyme label="a" type="m">teghenspoet</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Schoone voorwaar ghy my <rhyme label="a" type="m" corresp="#s12:l1">aandoen</rhyme></l>
        <l id="s12:l3" met="2" type="-+">Want door yver <rhyme label="b" type="m">gram</rhyme></l>
        <l id="s12:l4" met="2" type="-+">Ontvonckt ghy een <rhyme label="b" type="m" corresp="#s12:l3">vlam</rhyme></l>
        <l id="s12:l5" met="2" type="-+">In mijn <rhyme label="a" type="m" corresp="#s12:l1">ghemoet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Mijn herte knaacht met een <rhyme label="a" type="m">ghetier</rhyme></l>
        <l id="s13:l2" met="4" type="-+">Als AEtna braket solfer <rhyme label="a" type="m" corresp="#s13:l1">vier</rhyme></l>
        <l id="s13:l3" met="2" type="-+">Dampen vol <rhyme label="b" type="m">ghesucht</rhyme></l>
        <l id="s13:l4" met="2" type="-+">Clachten <rhyme label="b" type="m" corresp="#s13:l3">ongenucht</rhyme></l>
        <l id="s13:l5" met="2" type="-+">Gaan van my <rhyme label="a" type="m" corresp="#s13:l1">hier</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">End' al om dat ick vol van <rhyme label="a" type="m">trouw'</rhyme></l>
        <l id="s14:l2" met="4" type="-+">V eer end' dien suyver <rhyme label="a" type="m" corresp="#s14:l1">lonckvrouw'</rhyme></l>
        <l id="s14:l3" met="2" type="-+">Verdraach ick dit <rhyme label="b" type="m">quaat</rhyme></l>
        <l id="s14:l4" met="2" type="-+">Als ghy my <rhyme label="b" type="m" corresp="#s14:l3">verlaat</rhyme></l>
        <l id="s14:l5" met="2" type="-+">In bitter <rhyme label="a" type="m" corresp="#s14:l1">rouw'</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Voorwart 'tis nu wel Martis <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Dat men 'tquaat neemt en 'tgoede <rhyme label="a" type="m" corresp="#s15:l1">mijdt</rhyme></l>
        <l id="s15:l3" met="2" type="-+">Oft ist u <rhyme label="b" type="m">ghepeyns</rhyme></l>
        <l id="s15:l4" met="2" type="-+">Dat ick my <rhyme label="b" type="m" corresp="#s15:l3">gheveyns</rhyme></l>
        <l id="s15:l5" met="2" type="-+">Sulcx vliede <rhyme label="a" type="m" corresp="#s15:l1">wijdt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Ick wil my in Cysize <rhyme label="a" type="m">stroom</rhyme></l>
        <l id="s16:l2" met="4" type="-+">Proeven, daar sal mijn liefde <rhyme label="a" type="m" corresp="#s16:l1">vroom</rhyme></l>
        <l id="s16:l3" met="2" type="-+">Blijcken end' zijn <rhyme label="b" type="m">groen</rhyme></l>
        <l id="s16:l4" met="2" type="-+">Als Laurieren <rhyme label="b" type="m" corresp="#s16:l3">doen</rhyme></l>
        <l id="s16:l5" met="2" type="-+">In blixems <rhyme label="a" type="m" corresp="#s16:l1">schroom</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Ghelooft dat mijn hert waar <rhyme label="a" type="m">gelas</rhyme></l>
        <l id="s17:l2" met="4" type="-+">Ghy soudt daar sien van binnen <rhyme label="a" type="m" corresp="#s17:l1">ras</rhyme></l>
        <l id="s17:l3" met="2" type="-+">Een oprechte <rhyme label="b" type="m">cracht</rhyme></l>
        <l id="s17:l4" met="2" type="-+">Die met yver <rhyme label="b" type="m" corresp="#s17:l3">tracht</rhyme></l>
        <l id="s17:l5" met="2" type="-+">Naaru <rhyme label="a" type="m" corresp="#s17:l1">ghepas</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Certeyn Cupido ghy zijt <rhyme label="a" type="m">wreet</rhyme></l>
        <l id="s18:l2" met="4" type="-+">Du bist de quaal die my <rhyme label="a" type="m" corresp="#s18:l1">opeet</rhyme></l>
        <l id="s18:l3" met="2" type="-+">Een Getul' <rhyme label="b" type="m">Leeuwin</rhyme></l>
        <l id="s18:l4" met="2" type="-+">Niet Paphos <rhyme label="b" type="m" corresp="#s18:l3">Goodin</rhyme></l>
        <l id="s18:l5" met="2" type="-+">Heeft dy <rhyme label="a" type="m" corresp="#s18:l1">ghereet</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Wat claacht mijn hert doch so <rhyme label="a" type="m">seer</rhyme></l>
        <l id="s19:l2" met="4" type="-+">Daar ick duysent tormenten <rhyme label="a" type="m" corresp="#s19:l1">meer</rhyme></l>
        <l id="s19:l3" met="2" type="-+">Hoor' te lyden <rhyme label="b" type="m">recht</rhyme></l>
        <l id="s19:l4" met="2" type="-+">End' als eeu trou <rhyme label="b" type="m" corresp="#s19:l3">knecht</rhyme></l>
        <l id="s19:l5" met="2" type="-+">Haar bieden <rhyme label="a" type="m" corresp="#s19:l1">eer</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Sy diedoor soet beval <rhyme label="a" type="m">verdient</rhyme></l>
        <l id="s20:l2" met="4" type="-+">Te wesen hooger Goden <rhyme label="a" type="m" corresp="#s20:l1">vrient</rhyme></l>
        <l id="s20:l3" met="2" type="-+">Om wien Paris <rhyme label="b" type="m">noch</rhyme></l>
        <l id="s20:l4" met="2" type="-+">Der Griecken <rhyme label="b" type="m" corresp="#s20:l3">bedroch</rhyme></l>
        <l id="s20:l5" met="2" type="-+">Niet soud' <rhyme label="a" type="m" corresp="#s20:l1">ontsien</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Leader om Mero <rhyme label="a" type="m">bemint</rhyme></l>
        <l id="s21:l2" met="4" type="-+">En vreesde niet den stuyren <rhyme label="a" type="m" corresp="#s21:l1">wint</rhyme></l>
        <l id="s21:l3" met="2" type="-+">Maar hy wierp sijn <rhyme label="b" type="m">lijf</rhyme></l>
        <l id="s21:l4" met="2" type="-+">Van den oever <rhyme label="b" type="m" corresp="#s21:l3">stijf</rhyme></l>
        <l id="s21:l5" met="2" type="-+">In Zee <rhyme label="a" type="m" corresp="#s21:l1">ontsint</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Soud ick om haar die booven <rhyme label="a" type="m">gaat</rhyme></l>
        <l id="s22:l2" met="4" type="-+">De Sestische van Venus <rhyme label="a" type="m" corresp="#s22:l1">staat</rhyme></l>
        <l id="s22:l3" met="2" type="-+">Tperijckel <rhyme label="b" type="m">ontsien</rhyme></l>
        <l id="s22:l4" met="2" type="-+">Oft selfs die dat <rhyme label="b" type="m" corresp="#s22:l3">vlien</rhyme></l>
        <l id="s22:l5" met="2" type="-+">Neen, metter <rhyme label="a" type="m" corresp="#s22:l1">daat</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Schoon zijty ghend' vol geuren <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s23:l2" met="4" type="-+">Verciert met veel coleuren <rhyme label="a" type="m" corresp="#s23:l1">greyn</rhyme></l>
        <l id="s23:l3" met="2" type="-+">Als die <rhyme label="b" type="m">Coningin</rhyme></l>
        <l id="s23:l4" met="2" type="-+">Tot Cypren vaart <rhyme label="b" type="m" corresp="#s23:l3">in</rhyme></l>
        <l id="s23:l5" met="2" type="-+">Met grooten <rhyme label="a" type="m" corresp="#s23:l1">treyn</rhyme></l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Ick wil lijden om dijnent <rhyme label="a" type="m">wil</rhyme></l>
        <l id="s24:l2" met="4" type="-+">Een bitter plaach end swijgen <rhyme label="a" type="m" corresp="#s24:l1">stil</rhyme></l>
        <l id="s24:l3" met="2" type="-+">Siet ick ben <rhyme label="b" type="m">bereet</rhyme></l>
        <l id="s24:l4" met="2" type="-+">Te dulden veel <rhyme label="b" type="m" corresp="#s24:l3">leet</rhyme></l>
        <l id="s24:l5" met="2" type="-+">Sonder <rhyme label="a" type="m" corresp="#s24:l1">geschil</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Ick ben bereet te lijden <rhyme label="a" type="m">oock</rhyme></l>
        <l id="s25:l2" met="4" type="-+">Promethei quaal int hels <rhyme label="a" type="m" corresp="#s25:l1">gesmoock</rhyme></l>
        <l id="s25:l3" met="2" type="-+">End' Tantali <rhyme label="b" type="m">dorst</rhyme></l>
        <l id="s25:l4" met="2" type="-+">Commer, hit end' <rhyme label="b" type="m" corresp="#s25:l3">vorst</rhyme></l>
        <l id="s25:l5" met="2" type="-+">Dampen end' <rhyme label="a" type="m" corresp="#s25:l1">roock</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Als ick noch maar den lesten <rhyme label="a" type="m">dach</rhyme></l>
        <l id="s26:l2" met="4" type="-+">Crijg een saacht eynd' tot mijn <rhyme label="a" type="m" corresp="#s26:l1">geclach</rhyme></l>
        <l id="s26:l3" met="2" type="-+">Als ick tot mijn <rhyme label="b" type="m">loon</rhyme></l>
        <l id="s26:l4" met="2" type="-+">V gedaante <rhyme label="b" type="m" corresp="#s26:l3">schoon</rhyme></l>
        <l id="s26:l5" met="2" type="-+">Noch crygen <rhyme label="a" type="m" corresp="#s26:l1">mach</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Smert, ancxst, be nau theyt, druck end <rhyme label="a" type="m">pijn</rhyme></l>
        <l id="s27:l2" met="4" type="-+">Hoe groot die dan oock mochten <rhyme label="a" type="m" corresp="#s27:l1">zijn</rhyme></l>
        <l id="s27:l3" met="2" type="-+">Wil ick met <rhyme label="b" type="m">gedult</rhyme></l>
        <l id="s27:l4" met="2" type="-+">Dragen end' die <rhyme label="b" type="m" corresp="#s27:l3">schult</rhyme></l>
        <l id="s27:l5" met="2" type="-+">Noch seggen <rhyme label="a" type="m" corresp="#s27:l1">mijn</rhyme></l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Maar (hou) mijn troost is veer van <rhyme label="a" type="m">daar</rhyme></l>
        <l id="s28:l2" met="4" type="-+">Mijn druck moet altijdt blijven <rhyme label="a" type="m" corresp="#s28:l1">waar</rhyme></l>
        <l id="s28:l3" met="2" type="-+">Tot dat eens de <rhyme label="b" type="m">doot</rhyme></l>
        <l id="s28:l4" met="2" type="-+">Met haar seysen <rhyme label="b" type="m" corresp="#s28:l3">stoot</rhyme></l>
        <l id="s28:l5" met="2" type="-+">Mijn leven <rhyme label="a" type="m" corresp="#s28:l1">swaar</rhyme></l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">De doot sal zijn mijn Liefden <rhyme label="a" type="m">endt</rhyme></l>
        <l id="s29:l2" met="4" type="-+">Ten sy dat ghy u tot my <rhyme label="a" type="m" corresp="#s29:l1">wendt</rhyme>.</l>
        <l id="s29:l3" met="2" type="-+">V straal my <rhyme label="b" type="m">ontsonckt</rhyme></l>
        <l id="s29:l4" met="2" type="-+">Want ghy te seer <rhyme label="b" type="m" corresp="#s29:l3">lonckt</rhyme></l>
        <l id="s29:l5" met="2" type="-+">Meesterse <rhyme label="a" type="m" corresp="#s29:l1">jendt</rhyme>.</l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="4" type="-+">Princesse schoon die my <rhyme label="a" type="m">verwon</rhyme></l>
        <l id="s30:l2" met="4" type="-+">End' brant alst licht van <rhyme label="a" type="m" corresp="#s30:l1">Phaeton</rhyme></l>
        <l id="s30:l3" met="2" type="-+">Bestraalt mijn bang <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s30:l4" met="2" type="-+">Dat het jeugdich <rhyme label="b" type="m" corresp="#s30:l3">wert</rhyme></l>
        <l id="s30:l5" met="2" type="-+">Blinckende <rhyme label="a" type="m" corresp="#s30:l1">Son</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
