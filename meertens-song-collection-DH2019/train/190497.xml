<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190497</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Zoutmans glorie</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Allen druk en rouw <rhyme label="a" type="f">verschoven</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Daar den blyden zaal'gen <rhyme label="b" type="m">Vreê</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Op ons nederdaalt van <rhyme label="a" type="f" corresp="#s1:l1">boven</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">Maastricht juicht de Vreede <rhyme label="b" type="m" corresp="#s1:l2">meê</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Vrede komt ons leet <rhyme label="c" type="f">verzachten</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Vreê is ieders beste <rhyme label="d" type="m">pand</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Geeft aan Staat en Land de <rhyme label="c" type="f" corresp="#s1:l5">kragten</rhyme>;</l>
        <l id="s1:l8" met="4" type="+-">Vreê is 't heil van 't <rhyme label="d" type="m" corresp="#s1:l6">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Eendragt! lang verward, <rhyme label="a" type="f">geschonden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Dat de Tuinmaagd zugten <rhyme label="b" type="m">deê</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Word weêrom te saam' <rhyme label="a" type="f" corresp="#s2:l1">gebonden</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Willem plant den boom van <rhyme label="b" type="m" corresp="#s2:l2">Vreê</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">‘Nassou plant op nieuw <rhyme label="c" type="f">d'Olyven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Zegt ons hier de snelle <rhyme label="d" type="m">Faam</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">En de Oranje Zon zal <rhyme label="c" type="f" corresp="#s2:l5">blyven</rhyme></l>
        <l id="s2:l8" met="4" type="+-">In haar luister en haar <rhyme label="d" type="m" corresp="#s2:l6">naam</rhyme>’.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Is het zoo? ô! bly <rhyme label="a" type="f">geheugen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Dat ons Nazaad hebben <rhyme label="b" type="m">zal</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">En de Zee zelf <rhyme label="a" type="f" corresp="#s3:l1">nektarteugen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Gieten zal in Batoo's <rhyme label="b" type="m" corresp="#s3:l2">dal</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Steeden, Dorpen, Beemden, <rhyme label="c" type="f">Haagen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Juichen steeds aan ééne <rhyme label="d" type="m">ry</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En wy helpen <rhyme label="c" type="f" corresp="#s3:l5">onderschraagen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Wat wy kunnen 't <rhyme label="d" type="m" corresp="#s3:l6">Vreêgety</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">G'lyk de Tweedragt, by de <rhyme label="a" type="f">Vad'ren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Van het dierbaar <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Wegvlood, en den Vreê kwam <rhyme label="a" type="f" corresp="#s4:l1">nad'ren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Knoopen ieder hand aan <rhyme label="b" type="m" corresp="#s4:l2">hand</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Zoo moet uit ons Stad <rhyme label="c" type="f">verdwynen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Alle Tweedragts <rhyme label="d" type="m">misverstand</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">Niemand mag ter Feest <rhyme label="c" type="f" corresp="#s4:l5">verschynen</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Zonder ééndragts heil'gen <rhyme label="d" type="m" corresp="#s4:l6">band</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zie Vorst Willem 't voorbeeld <rhyme label="a" type="f">geeven</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Als hy winnaar binnen <rhyme label="b" type="m">treed</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">‘Alles, zegt hy, <rhyme label="a" type="f" corresp="#s5:l1">misbedreeven</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Is voor eeuwig in <rhyme label="b" type="m" corresp="#s5:l2">vergeet</rhyme>.’</l>
        <l id="s5:l5" met="4" type="+-">Laaten wy dan Burgers <rhyme label="c" type="f">weezen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">G'lyk een Burger weezen <rhyme label="d" type="m">moet</rhyme>;</l>
        <l id="s5:l7" met="4" type="+-">Want Oranje liet ons <rhyme label="c" type="f" corresp="#s5:l5">leezen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Wat alleen den Vrede <rhyme label="d" type="m" corresp="#s5:l6">voed</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Slaan wy thans naauwkeurige <rhyme label="a" type="f">oogen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Op de Vad'ren van den <rhyme label="b" type="m">Staat</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wanbegrip is <rhyme label="a" type="f" corresp="#s6:l1">weggevlogen</rhyme>;</l>
        <l id="s6:l4" met="4" type="+-">Niemand kent meer wrok noch <rhyme label="b" type="m" corresp="#s6:l2">haat</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Ieders hart van Liefde <rhyme label="c" type="f">teder</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Keert weêr tot zyn heil'ge <rhyme label="d" type="m">pligt</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">En d'Oranje Zon schynt <rhyme label="c" type="f" corresp="#s6:l5">weder</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Met een nog veel schoonder <rhyme label="d" type="m" corresp="#s6:l6">licht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hebt dan dank Europens <rhyme label="a" type="f">Helden</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Die een bloedbad hebt <rhyme label="b" type="m">verhoed</rhyme></l>
        <l id="s7:l3" met="4" type="+-">Eeuwig zal men Fredrik <rhyme label="a" type="f" corresp="#s7:l1">melden</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Die den Vreê herleeven <rhyme label="b" type="m" corresp="#s7:l2">doet</rhyme></l>
        <l id="s7:l5" met="4" type="+-">Josef, Lood'wyk, schrandre <rhyme label="c" type="f">Britten</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Spanjen, Zweed, en braaven <rhyme label="d" type="m">Deen</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Gy zult mede 't hart <rhyme label="c" type="f" corresp="#s7:l5">bezitten</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Van ons Stad, in 't <rhyme label="d" type="m" corresp="#s7:l6">algemeen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Gy helpt onze vreugd <rhyme label="a" type="f">vergrooten</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Achtb're Vad'ren van ons <rhyme label="b" type="m">Stad</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Vrede zy u niet <rhyme label="a" type="f" corresp="#s8:l1">ontschooten</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Gaat ons voor op 't <rhyme label="b" type="m" corresp="#s8:l2">Vredepad</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Ondersteund ons blyde <rhyme label="c" type="f">toonen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">G'lyk gy altyd hebt <rhyme label="d" type="m">gedaan</rhyme>:</l>
        <l id="s8:l7" met="4" type="+-">Daar wy 't Vaderland zien <rhyme label="c" type="f" corresp="#s8:l5">kroonen</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Met de <rhyme label="d" type="m" corresp="#s8:l6">blyd'Oranjevaan</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Dat des Hemels hoogen <rhyme label="a" type="f">zegen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Voor ons uwe zorg <rhyme label="b" type="m">betaal</rhyme>!</l>
        <l id="s9:l3" met="4" type="+-">En gelyk een malschen <rhyme label="a" type="f" corresp="#s9:l1">regen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Gunstig op u <rhyme label="b" type="m" corresp="#s9:l2">nederdaal</rhyme>:</l>
        <l id="s9:l5" met="4" type="+-">Dat de Vad'ren, <rhyme label="c" type="f">Batavieren</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Leeven in Gods <rhyme label="d" type="m">zegening</rhyme>.</l>
        <l id="s9:l7" met="4" type="+-">Recht en Eendragt zal hun <rhyme label="c" type="f" corresp="#s9:l5">sieren</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">By den Dorp- en <rhyme label="d" type="m" corresp="#s9:l6">Stedeling</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Dat Oranjes doen en <rhyme label="a" type="f">laaten</rhyme></l>
        <l id="s10:l2" met="4" type="+-">Zy van 's Hemels hand <rhyme label="b" type="m">gestouwd</rhyme>.</l>
        <l id="s10:l3" met="4" type="+-">Dat hy leef het Land te <rhyme label="a" type="f" corresp="#s10:l1">baaten</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">En niet als op ééndragt <rhyme label="b" type="m" corresp="#s10:l2">bouwt</rhyme>.</l>
        <l id="s10:l5" met="4" type="+-">G'lu kig leef hy lange <rhyme label="c" type="f">Jaaren</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">En zyn Koninglyk <rhyme label="d" type="m">geslagt</rhyme>.</l>
        <l id="s10:l7" met="4" type="+-">Vreed' olyf bezwaai zyn <rhyme label="c" type="f" corresp="#s10:l5">haaren</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Tot hy word in 't graf <rhyme label="d" type="m" corresp="#s10:l6">gebragt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Mogt thans, 't geen wy nimmer <rhyme label="a" type="f">hoopen</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Vreê nog niet zyn vast <rhyme label="b" type="m">geplant</rhyme>;</l>
        <l id="s11:l3" met="4" type="+-">Laaten wy ten tempel <rhyme label="a" type="f" corresp="#s11:l1">loopen</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">En met opgeheve <rhyme label="b" type="m" corresp="#s11:l2">hand</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Smeeken 't eeuwig <rhyme label="c" type="f">alvermoogen</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Dat hy van den hoogen <rhyme label="d" type="m">zie</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">En in g'naâ op 't Land <rhyme label="c" type="f" corresp="#s11:l5">gebogen</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">Ieders hart en wil <rhyme label="d" type="m" corresp="#s11:l6">gebie</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
