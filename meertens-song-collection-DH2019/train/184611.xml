  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SInght de Moeder hoogh <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van de hooghste <rhyme label="b" type="m">Majesteyt</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Wilt erkentenisse <rhyme label="a" type="f" corresp="#s1:l1">geven</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Voor haer sorgh, tot ons <rhyme label="b" type="m" corresp="#s1:l2">bereydt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">s'Hemels grati' haer <rhyme label="c" type="f">geschoncken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Boven alles wonder <rhyme label="d" type="m">is</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Sonder vleck heeft sy <rhyme label="c" type="f" corresp="#s1:l5">gebloncken</rhyme></l>
        <l id="s1:l8" met="4" type="+-">In haer eerst <rhyme label="d" type="m" corresp="#s1:l6">ontfangenis</rhyme>:</l>
        <l id="s1:l9" met="4" type="+-">Sy soo suyver moeste <rhyme label="e" type="m">syn</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Soo moest blincken haeren <rhyme label="e" type="m" corresp="#s1:l9">schyn</rhyme>;</l>
        <l id="s1:l11" met="4" type="+-">Om de Sonn' eens <rhyme label="f" type="f">voorts-te-brengen</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Moest soo schoon haer luyster <rhyme label="e" type="m" corresp="#s1:l9">syn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Heel geluckigh sy <rhyme label="a" type="f">geboren</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Brocht de Wereldt haer <rhyme label="b" type="m">geluck</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Quamp alleen om ons <rhyme label="a" type="f" corresp="#s2:l1">verloren</rhyme></l>
        <l id="s2:l4" met="4" type="+-">t'Helpen uyt ons eeuwigh <rhyme label="b" type="m" corresp="#s2:l2">druck</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Om de schaede te <rhyme label="c" type="f">herstellen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van den droeven Adams <rhyme label="d" type="m">val</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">En den Vyandt <rhyme label="c" type="f" corresp="#s2:l5">neer-te-vellen</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Met te geven Godt van <rhyme label="d" type="m" corresp="#s2:l6">al</rhyme>.</l>
        <l id="s2:l9" met="4" type="+-">Aen den Heer sy van dry <rhyme label="e" type="m">jaer</rhyme></l>
        <l id="s2:l10" met="4" type="+-">Offert in den Tempel <rhyme label="e" type="m" corresp="#s2:l9">haer</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Om de wereldts jeught te <rhyme label="f" type="f">leeren</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Dat Godt is ons beste <rhyme label="e" type="m" corresp="#s2:l9">paer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Sy is weerdigh Godt te <rhyme label="a" type="f">draegen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Moeder, ende Maghet <rhyme label="b" type="m">mé</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Op haer is Godts oogh <rhyme label="a" type="f" corresp="#s3:l1">geslaegen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Om te brengen ons den <rhyme label="b" type="m" corresp="#s3:l2">Vré</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Gabriel komt <rhyme label="c" type="f">neder-daelen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Leght dit wonder werck haer <rhyme label="d" type="m">uyt</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Nu sy brandt door <rhyme label="c" type="f" corresp="#s3:l5">Liefde-straelen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Alle vreese buyten <rhyme label="d" type="m" corresp="#s3:l6">sluyt</rhyme>.</l>
        <l id="s3:l9" met="4" type="+-">Sy gaet groeten haere <rhyme label="e" type="m">Nicht</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">En brenght mede s'Hemels <rhyme label="e" type="m" corresp="#s3:l9">licht</rhyme>:</l>
        <l id="s3:l11" met="4" type="+-">t'Kindt Joannes, die t'Lam <rhyme label="f" type="f">voor-loopt</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">Heylight sy door haer <rhyme label="e" type="m" corresp="#s3:l9">gesicht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Als den Heer nu was <rhyme label="a" type="f">geboren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Voor des menschens <rhyme label="b" type="m">Saeligheydt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Sy, tot Moeder Godts <rhyme label="a" type="f" corresp="#s4:l1">verkoren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Thoont een grootst' <rhyme label="b" type="m" corresp="#s4:l2">oodtmoedigheydt</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Daer sy suyver was <rhyme label="c" type="f">gebleven</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Suyver sy haer voor den <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Volght de Wet, gemeyn <rhyme label="c" type="f" corresp="#s4:l5">gegeven</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Wordt vernedert meer en <rhyme label="d" type="m" corresp="#s4:l6">meer</rhyme>:</l>
        <l id="s4:l9" met="4" type="+-">Naer een droeven levens <rhyme label="e" type="m">strydt</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">Sich nu met haer Soon <rhyme label="e" type="m" corresp="#s4:l9">verblydt</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Daer sy van al d'Hemels <rhyme label="f" type="f">Geesten</rhyme></l>
        <l id="s4:l12" met="4" type="+-">Eer, en Lof kryght voor <rhyme label="e" type="m" corresp="#s4:l9">altydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
