    <melody>Dem KÃ¶nig und Regenten dein</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DEn Coninck end' dijn <rhyme label="a" type="m">Overheydt</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Dien ghy te <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dijn Volck (HEEr) hebt <rhyme label="c" type="f">ghegeven</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dien geeft verstant met goet <rhyme label="a" type="m" corresp="#s1:l1">bescheyt</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Hert, Macht, en <rhyme label="b" type="m" corresp="#s1:l2">Moet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Des Conincx Soon <rhyme label="c" type="f" corresp="#s1:l3">verheven</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Op dat hy kennend' uwe <rhyme label="d" type="m">Recht</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Eenvoudt en <rhyme label="d" type="m" corresp="#s1:l7">slecht</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Dijn Volck altijdt <rhyme label="e" type="f">Regeere</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Met goet Recht <rhyme label="f" type="m">beyd'</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">En <rhyme label="f" type="m" corresp="#s1:l10">billickheyt</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Den Armen <rhyme label="g" type="m">dijn</rhyme>,</l>
        <l id="s1:l13" met="2" type="-+">Soo't hoort te <rhyme label="g" type="m" corresp="#s1:l12">zijn</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">Ghericht en Recht <rhyme label="e" type="f" corresp="#s1:l9">hanteere</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dan brengen Vreed' en <rhyme label="a" type="m">Rechticheyt</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Beyd' Bergh en <rhyme label="b" type="m">Dal</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">T'volck Vredelijck te <rhyme label="c" type="f">woonen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat sy gaen Wand'len sonder <rhyme label="a" type="m" corresp="#s2:l1">Leydt</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Vry over <rhyme label="b" type="m" corresp="#s2:l2">al</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Haer niemandt derff <rhyme label="c" type="f" corresp="#s2:l3">verhoonen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Want dijn Regent der Armen <rhyme label="d" type="m">Clach</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">(Die all' den <rhyme label="d" type="m" corresp="#s2:l7">Dach</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Int Volck verheft) <rhyme label="e" type="f">verhooret</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">D'elend' <rhyme label="f" type="m">bevrijdt</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Gheeft Spijse <rhyme label="f" type="m" corresp="#s2:l10">blijdt</rhyme>:</l>
        <l id="s2:l12" met="2" type="-+">Der Boosen <rhyme label="g" type="m">Rot</rhyme>,</l>
        <l id="s2:l13" met="2" type="-+">Hoochmoet en <rhyme label="g" type="m" corresp="#s2:l12">spot</rhyme>,</l>
        <l id="s2:l14" met="3" type="-+">Trots en Ghewelt <rhyme label="e" type="f" corresp="#s2:l9">verstooret</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Sy sullen hebben dy voor <rhyme label="a" type="m">Oogh'</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Beyd' Man en <rhyme label="b" type="m">Wijff</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">O Godt end' dy <rhyme label="c" type="f">vereeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">So langh' de Son in HEmel <rhyme label="a" type="m" corresp="#s3:l1">hooch</rhyme></l>
        <l id="s3:l5" met="2" type="-+">En Maen oock <rhyme label="b" type="m" corresp="#s3:l2">blijff</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dijn Loff altijdt <rhyme label="c" type="f" corresp="#s3:l3">vermeeren</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Des HEren coemst is vol <rhyme label="d" type="m">Ghenaed'</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">End' also <rhyme label="d" type="m" corresp="#s3:l7">gaet</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Als Gedeons Vel <rhyme label="e" type="f">ghespreyden</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">End' als den <rhyme label="f" type="m">Douw</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">T'landt Dorr' en <rhyme label="f" type="m" corresp="#s3:l10">Rouw</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+">Bedouw't dat <rhyme label="g" type="m">wel</rhyme>,</l>
        <l id="s3:l13" met="2" type="-+">T'gras spruytet <rhyme label="g" type="m" corresp="#s3:l12">snel</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Op Affghemaeyde <rhyme label="e" type="f" corresp="#s3:l9">Weyden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dan bloeyt de Vroom in lust en <rhyme label="a" type="m">Vreed'</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Tot syner <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Soo langh' de Maen sal <rhyme label="c" type="f">schijnen</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Syns Rijcks gebiedt end' <rhyme label="a" type="m" corresp="#s4:l1">Heerlickheyt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Streckt breet en <rhyme label="b" type="m" corresp="#s4:l2">wijt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Van Zee tot Zees <rhyme label="c" type="f" corresp="#s4:l3">termijnen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Die Mooren vallen hem te <rhyme label="d" type="m">voet</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">De Vyandt <rhyme label="d" type="m" corresp="#s4:l7">moet</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Stoff lecken end' hem <rhyme label="e" type="f">vreesen</rhyme>,</l>
        <l id="s4:l10" met="2" type="-+">Die <rhyme label="f" type="m">Coninghen</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Die sullen <rhyme label="f" type="m" corresp="#s4:l10">hem</rhyme></l>
        <l id="s4:l12" met="2" type="-+">Begiften <rhyme label="g" type="m">wel</rhyme>,</l>
        <l id="s4:l13" met="2" type="-+">Die Heyden <rhyme label="g" type="m" corresp="#s4:l12">veel</rhyme>,</l>
        <l id="s4:l14" met="3" type="-+">Hem alle dienstich <rhyme label="e" type="f" corresp="#s4:l9">wesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Van onrecht hy den Armen <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Verlossen <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als hy sal Hulploos <rhyme label="c" type="f">schreyen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Hem laten niet in <rhyme label="a" type="m" corresp="#s5:l1">Ongeval</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Noch int <rhyme label="b" type="m" corresp="#s5:l2">ghequel</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Maer wter Noot hem <rhyme label="c" type="f" corresp="#s5:l3">leyen</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Sal dien oock vriend'lijck syn <rhyme label="d" type="m">altijdt</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Die Armoed' <rhyme label="d" type="m" corresp="#s5:l7">lijdt</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Verlossense van <rhyme label="e" type="f">schaden</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">Van 's Woeckers <rhyme label="f" type="m">drang</rhyme>,</l>
        <l id="s5:l11" met="2" type="+-">En Wreeff'laers <rhyme label="f" type="m" corresp="#s5:l10">dwang</rhyme>,</l>
        <l id="s5:l12" met="2" type="-+">(Midts hy haer <rhyme label="g" type="m">bloet</rhyme></l>
        <l id="s5:l13" met="2" type="-+">Acht duyr en <rhyme label="g" type="m" corresp="#s5:l12">goet</rhyme>)</l>
        <l id="s5:l14" met="3" type="-+">Sal hyse wel <rhyme label="e" type="f" corresp="#s5:l9">Ontladen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Den Coninck veel gelucks <rhyme label="a" type="m">altijdt</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Arabisch <rhyme label="b" type="m">Gout</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Sal men hem willich <rhyme label="c" type="f">gheven</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">End' hem daer by aenbeden <rhyme label="a" type="m" corresp="#s6:l1">wijt</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Op hem <rhyme label="b" type="m" corresp="#s6:l2">betrout</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Want hy sal eeuwich <rhyme label="c" type="f" corresp="#s6:l3">leven</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Het Zaet ghesaeyt op Bergh' en <rhyme label="d" type="m">Plaen</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Sal dicke <rhyme label="d" type="m" corresp="#s6:l7">staen</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Veel vruchten salmen <rhyme label="e" type="f">maeyen</rhyme>,</l>
        <l id="s6:l10" met="2" type="-+">Ghelijck alst <rhyme label="f" type="m">Hout</rhyme>,</l>
        <l id="s6:l11" met="2" type="+-">Dat staet soo <rhyme label="f" type="m" corresp="#s6:l10">stout</rhyme>,</l>
        <l id="s6:l12" met="2" type="-+">In <rhyme label="g" type="m">Libano</rhyme></l>
        <l id="s6:l13" met="2" type="-+">Ghelijck <rhyme label="g" type="m" corresp="#s6:l12">also</rhyme>,</l>
        <l id="s6:l14" met="3" type="-+">Salt Coorn dan staen <rhyme label="e" type="f" corresp="#s6:l9">waeyen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Veel Volcks sal in den Steden <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Als gras end' <rhyme label="b" type="m">Loff</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Soo sullen sy <rhyme label="c" type="f">vermeeren</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Oock eewich sal die Name <rhyme label="a" type="m" corresp="#s7:l1">zijn</rhyme></l>
        <l id="s7:l5" met="2" type="-+">Van nu aen <rhyme label="b" type="m" corresp="#s7:l2">off</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Vercond't met grooter <rhyme label="c" type="f" corresp="#s7:l3">eeren</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">Gheseghent sullen oock <rhyme label="d" type="m">ghelijck</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Sijn in syn <rhyme label="d" type="m" corresp="#s7:l7">Rijck</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">Die Heyden, end' hem <rhyme label="e" type="f">prijsen</rhyme>,</l>
        <l id="s7:l10" met="2" type="-+">Ghelooft zij <rhyme label="f" type="m">Godt</rhyme></l>
        <l id="s7:l11" met="2" type="+-">De HEER <rhyme label="f" type="m" corresp="#s7:l10">Zeboth</rhyme>,</l>
        <l id="s7:l12" met="2" type="-+">Want hy <rhyme label="g" type="m">alleen</rhyme></l>
        <l id="s7:l13" met="2" type="-+">En anders <rhyme label="g" type="m" corresp="#s7:l12">gheen</rhyme></l>
        <l id="s7:l14" met="3" type="-+">Groot wonder can <rhyme label="e" type="f" corresp="#s7:l9">bewijsen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Syn Naem zij loff in <rhyme label="a" type="m">eeuwicheyt</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Syn <rhyme label="b" type="m">Majesteyt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Ghenoech can niemant <rhyme label="c" type="f">loven</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Syn Rijcx Cieraet, en <rhyme label="a" type="m" corresp="#s8:l1">Heerlickheyt</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Wert <rhyme label="b" type="m" corresp="#s8:l2">wtghebreyt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Hooch by den Vader <rhyme label="c" type="f" corresp="#s8:l3">boven</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Dat Aertrijck is vol syner <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">Hoe langhs hoe <rhyme label="d" type="m" corresp="#s8:l7">meer</rhyme>,</l>
        <l id="s8:l9" met="3" type="-+">Ghelooft wert synen <rhyme label="e" type="f">Name</rhyme>,</l>
        <l id="s8:l10" met="2" type="-+">Die vromen <rhyme label="f" type="m">al</rhyme>,</l>
        <l id="s8:l11" met="2" type="+-">Met <rhyme label="f" type="m" corresp="#s8:l10">welgheval</rhyme>,</l>
        <l id="s8:l12" met="2" type="-+">Die spreken <rhyme label="g" type="m">vry</rhyme></l>
        <l id="s8:l13" met="2" type="-+">Oprecht daer <rhyme label="g" type="m" corresp="#s8:l12">by</rhyme>,</l>
        <l id="s8:l14" met="3" type="-+">Van herten vrolijck <rhyme label="e" type="f" corresp="#s8:l9">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
