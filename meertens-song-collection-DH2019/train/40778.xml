<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>40778</id>
    <date>
      <year-start>1681</year-start>
      <year-end>1681</year-end>
    </date>
    <location>
      <orginal-name>leeuwarden</orginal-name>
      <standard-name>Leeuwarden, The Netherlands</standard-name>
      <coordinates>
        <lon>53.2005936</lon>
        <lat>5.7918548</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 136 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Gysbert Japicx</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">LOf-sjongh de Heer, uwt herts-gruwns <rhyme label="a" type="m">njoe</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Hy iz goe, ja 't heagste <rhyme label="a" type="m" corresp="#s1:l1">goe</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s1:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Lof-sjong God; dy, wa t'Goads <rhyme label="a" type="m">neamm'</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Preal-fiert, twang-stjoert, az ijnn' <rhyme label="a" type="m" corresp="#s2:l1">teamm'</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s2:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Lof-sjong d'Heer dy 't heer <rhyme label="a" type="m">oer-heer't</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Dat, aerne' Heer, heersck in <rhyme label="a" type="m" corresp="#s3:l1">rejear't</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s3:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dy allinne, boppa <rhyme label="a" type="m">wet</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Tjin Natuwre, wond're <rhyme label="a" type="m" corresp="#s4:l1">det</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s4:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Dy, dy de' Hijmm'len mey <rhyme label="a" type="m">forstân</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Macke', ô wirck fen socken <rhyme label="a" type="m" corresp="#s5:l1">hân</rhyme>!</l>
        <l id="s5:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s5:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dy de' yerd', swier fen wichte', oer 't <rhyme label="a" type="m">wiet</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Fest, oer 't onfest weage', <rhyme label="a" type="m" corresp="#s6:l1">uwt-spried't</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s6:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Dy de' haed-Ljeachten, glanz'-gleaun' <rhyme label="a" type="m">great</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Macke', al het eag bruwckt to <rhyme label="a" type="m" corresp="#s7:l1">steat'</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s7:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dy de Sinne blier in <rhyme label="a" type="m">bly</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Oere Dey joe de' <rhyme label="a" type="m" corresp="#s8:l1">heerschappy'</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s8:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Moanne' in Stiert' joe Hy de <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Wad in fadschippe' oere <rhyme label="a" type="m" corresp="#s9:l1">Nacht</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s9:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Dy Egiptens aet in <rhyme label="a" type="m">naet</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Heeg, leeg, minsch, djiers eer-bern' <rhyme label="a" type="m" corresp="#s10:l1">daet</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s10:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">In kritse' Isr'el, az in <rhyme label="a" type="m">buwt</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Spijt Nijls tosschen, dear midz <rhyme label="a" type="m" corresp="#s11:l1">uwt</rhyme>:</l>
        <l id="s11:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s11:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Mey' hanz kreft, dy 't mord-lee <rhyme label="a" type="m">wreck't</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Mey' hiet fall'gjende' earme', <rhyme label="a" type="m" corresp="#s12:l1">uwt-streck't</rhyme>:</l>
        <l id="s12:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s12:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Dy 't Rea Mar oon twa'en trog <rhyme label="a" type="m">smiet</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Midz trogh kleauwd', sney, schaet in <rhyme label="a" type="m" corresp="#s13:l1">spliet</rhyme>:</l>
        <l id="s13:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s13:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">In fierde' Isr'el trog dy <rhyme label="a" type="m">steeg'</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Trog Marz midz-hôl, wetter <rhyme label="a" type="m" corresp="#s14:l1">Ieeg</rhyme>:</l>
        <l id="s14:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s14:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Dear ijn Phar'o, fen Him <rhyme label="a" type="m">wirt</rhyme>,</l>
        <l id="s15:l2" met="4" type="+-">Mey sijn Krijs-ljue, gjealp, <rhyme label="a" type="m" corresp="#s15:l1">oer-stirt</rhyme>:</l>
        <l id="s15:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s15:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Dy sijn Folck laet', sijcker, <rhyme label="a" type="m">fry</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Trog dy wijlde <rhyme label="a" type="m" corresp="#s16:l1">wôsteny'</rhyme>:</l>
        <l id="s16:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s16:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Dy kreft-greate Keen'gen <rhyme label="a" type="m">sleyn</rhyme>,</l>
        <l id="s17:l2" met="4" type="+-">In uwt-dijllge het, schien <rhyme label="a" type="m" corresp="#s17:l1">t'eyn'</rhyme>:</l>
        <l id="s17:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s17:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Dy trog-loft'ge <rhyme label="a" type="m">Kening-staef</rhyme>,</l>
        <l id="s18:l2" met="4" type="+-">(Greatsch, ontsjoen', heag) hompe' yn 't <rhyme label="a" type="m" corresp="#s18:l1">graef</rhyme>:</l>
        <l id="s18:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s18:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s18:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Az Sihon, oerste' <rhyme label="a" type="m">Amoriet</rhyme>,</l>
        <l id="s19:l2" met="4" type="+-">Dy hy, az dol-tong're, <rhyme label="a" type="m" corresp="#s19:l1">smiet</rhyme>:</l>
        <l id="s19:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s19:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">In de' oer-greate' Og, Basans <rhyme label="a" type="m">haed</rhyme></l>
        <l id="s20:l2" met="4" type="+-">Blix'me hy, oon gruys to <rhyme label="a" type="m" corresp="#s20:l1">naet'</rhyme>:</l>
        <l id="s20:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s20:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s20:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">In hy joe, uwt mijlder <rhyme label="a" type="m">han</rhyme>,</l>
        <l id="s21:l2" met="4" type="+-">To erve'-eyndomm' herre <rhyme label="a" type="m" corresp="#s21:l1">Lan</rhyme>:</l>
        <l id="s21:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s21:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s21:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">To erve'-eyn', in plante 'er <rhyme label="a" type="m">del</rhyme>,</l>
        <l id="s22:l2" met="4" type="+-">Sijn ljeav' Tjienner, <rhyme label="a" type="m" corresp="#s22:l1">Israël</rhyme>:</l>
        <l id="s22:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s22:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s22:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="+-">Dy mey-wirddigh uwz <rhyme label="a" type="m">oon-seag</rhyme>,</l>
        <l id="s23:l2" met="4" type="+-">Yn uwz dol-druwx jamm're <rhyme label="a" type="m" corresp="#s23:l1">pleag</rhyme>:</l>
        <l id="s23:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s23:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s23:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="+-">Dy (uwz mijld) krefts-hânz schoerde <rhyme label="a" type="m">uwt</rhyme>,</l>
        <l id="s24:l2" met="4" type="+-">Uwt dea-fijnn' kluwr', tosck in <rhyme label="a" type="m" corresp="#s24:l1">tuwt</rhyme>:</l>
        <l id="s24:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s24:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s24:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="+-">Ja dy Heer, dy alle <rhyme label="a" type="m">flesch</rhyme>,</l>
        <l id="s25:l2" met="4" type="+-">Jout, mijld, fiedsel-brea, ney <rhyme label="a" type="m" corresp="#s25:l1">esch</rhyme>:</l>
        <l id="s25:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s25:l4" met="4" type="+-">Son on-eyn-brie, so <rhyme label="b" type="m" corresp="#s25:l3">goertier</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="+-">Lof-sjong, al het iz, mey <rhyme label="a" type="m">my</rhyme></l>
        <l id="s26:l2" met="4" type="+-">d'Hijmm'len God: ô lof-sjongh <rhyme label="a" type="m" corresp="#s26:l1">dy</rhyme>:</l>
        <l id="s26:l3" met="4" type="+-">Want sijn goeheyt giet so <rhyme label="b" type="m">fier</rhyme>,</l>
        <l id="s26:l4" met="4" type="+-">So on-eyn-brie, so <rhyme label="b" type="m" corresp="#s26:l3">goertier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
