<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>23876</id>
    <date>
      <year-start>1592</year-start>
      <year-end>1592</year-end>
    </date>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Psalm 100 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Ziele die verheft den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En mijnen Geest, verheuch hem <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Jn Godt mijnen Heere <rhyme label="b" type="m">valiant</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Mijn trooster, ende mijn <rhyme label="b" type="m" corresp="#s1:l3">heylant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want hy heeft, aengesen <rhyme label="a" type="m">vorwaert</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Sijner dienstmaecht, nedricheit <rhyme label="a" type="m" corresp="#s2:l1">claer</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Siet van nu aen, so sullen <rhyme label="b" type="m">wy</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Salich prysen, al volcken <rhyme label="b" type="m" corresp="#s2:l3">vry</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want hy heeft, groote, dingen <rhyme label="a" type="m">goedt</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Aen my gedaen, met <rhyme label="a" type="m" corresp="#s3:l1">ouervloedt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">De Heere, die almachtich <rhyme label="b" type="m">is</rhyme></l>
        <l id="s3:l4" met="4" type="-+">En synen naem heylich <rhyme label="b" type="m" corresp="#s3:l3">gewis</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sijn bermherticheit, Seer <rhyme label="a" type="m">troostrijck</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die duert altoos, en <rhyme label="a" type="m" corresp="#s4:l1">eeuwichlijck</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Uan geslacht, tot geslacht <rhyme label="b" type="m">certeyn</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Ouer haer die hem, vreesen <rhyme label="b" type="m" corresp="#s4:l3">reyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Door zijnen arm, machtich <rhyme label="a" type="m">sterck</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Oeffent hy gewelt, in dit <rhyme label="a" type="m" corresp="#s5:l1">perck</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hy heeft verstroyt, ende <rhyme label="b" type="m">versmaet</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">De houerdich van, herten <rhyme label="b" type="m" corresp="#s5:l3">quaet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die gheweldige, die heeft <rhyme label="a" type="m">hy</rhyme>:</l>
        <l id="s6:l2" met="4" type="-+">Uanden stoel, affgetrocken <rhyme label="a" type="m" corresp="#s6:l1">vry</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maer die nedrich is, ende <rhyme label="b" type="m">cleijn</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Heeft hy seer hooch, verheuen <rhyme label="b" type="m" corresp="#s6:l3">reijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die hongerighe, naden <rhyme label="a" type="m">Geest</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Heeft hy veruult, met goeden <rhyme label="a" type="m" corresp="#s7:l1">meest</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Maer die haer hielen, ryck en <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Heeft hy gelaten, naeckt en <rhyme label="b" type="m" corresp="#s7:l3">bloot</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Want hy gedenckt door syn <rhyme label="a" type="m">goedtheit</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Aen syn groote, <rhyme label="a" type="m" corresp="#s8:l1">bermherticheit</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Hy helpet op, met moede <rhyme label="b" type="m">snel</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Sijn knecht en dinaer <rhyme label="b" type="m" corresp="#s8:l3">Jsrael</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Gelijck hy heeft, belooft <rhyme label="a" type="m">bequaem</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Onse Uaderen <rhyme label="a" type="m" corresp="#s9:l1">altseaem</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Abraham, en zijn Zaet seer <rhyme label="b" type="m">rijck</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Altoos ende ock <rhyme label="b" type="m" corresp="#s9:l3">eeuwichlijck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
