        <l id="s1:l1" met="4" type="+-">Neen, neen gy hebt geen <rhyme label="a" type="f">vermoogen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Op myn hert alreeds <rhyme label="a" type="f" corresp="#s1:l1">bewoogen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Nu het <rhyme label="b" type="m">wedermint</rhyme>;</l>
        <l id="s1:l4" met="5" type="-+">De liefde maakt dat hy de herten <rhyme label="b" type="m" corresp="#s1:l3">wint</rhyme>.</l>
