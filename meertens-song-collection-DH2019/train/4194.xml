<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4194</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zie ik niet onder de bomen</melody>
  <author>Jacob Steendam</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GY die uwe <rhyme label="a" type="f">blonde-vlechten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Achter om u <rhyme label="b" type="m">jeugdig-hoofd</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Weet so aartig) op te <rhyme label="a" type="f" corresp="#s1:l1">hechten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dat het menig Nimph <rhyme label="b" type="m" corresp="#s1:l2">verdoofd</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Die met haar getoysel <rhyme label="c" type="m">pronkt</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">'t Geen den Minnaar vaak <rhyme label="c" type="m" corresp="#s1:l5">belonkt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Een verhaal moet ik <rhyme label="a" type="f">vertrekken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Op dat gy u <rhyme label="b" type="m">Maagden-staat</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Nimmermeer en laat <rhyme label="a" type="f" corresp="#s2:l1">bevlekken</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">'t Zy in dees', of gene <rhyme label="b" type="m" corresp="#s2:l2">graad</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Als de Wereld u <rhyme label="c" type="m">toe-lacht</rhyme>.</l>
        <l id="s2:l6" met="4" type="+-">En gy op geen onheyl <rhyme label="c" type="m" corresp="#s2:l5">acht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Eerst sal ik u <rhyme label="a" type="f">vergelijken</rhyme></l>
        <l id="s3:l2" met="4" type="+-">By een vars ontloken <rhyme label="b" type="m">Roos</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Die (al-lokkende) laaat <rhyme label="a" type="f" corresp="#s3:l1">kijken</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Een seer levendig <rhyme label="b" type="m" corresp="#s3:l2">gebloos</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Vyt haar knopje: wel <rhyme label="c" type="m">gesteld</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Daar den Dau sich me <rhyme label="c" type="m" corresp="#s3:l5">verseld</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">So lang als de gulde <rhyme label="a" type="f">stralen</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Van de Sonne, en den <rhyme label="b" type="m">Dau</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Op dit Roosje <rhyme label="a" type="f" corresp="#s4:l1">neder-dalen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Blijft het jeugdig, fris, en <rhyme label="b" type="m" corresp="#s4:l2">gau</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Maar so dra, als <rhyme label="c" type="m">Boreas</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Komt te naken haar <rhyme label="c" type="m" corresp="#s4:l5">gewas</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Is het wesen <rhyme label="a" type="f">lipper-geurig</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Niet dan schijn, en <rhyme label="b" type="m">enkel-waan</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Haar gedaante <rhyme label="a" type="f" corresp="#s5:l1">schoon-koleurig</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Kan in geen geval <rhyme label="b" type="m" corresp="#s5:l2">bestaan</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Word het elders <rhyme label="c" type="m">an-geraakt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">'t Is in yder deel <rhyme label="c" type="m" corresp="#s5:l5">mismaakt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Even so zijt gy ô! <rhyme label="a" type="f">Maagden</rhyme></l>
        <l id="s6:l2" met="4" type="+-">In het bloeyen van u <rhyme label="b" type="m">jeugd</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Als u treft, den <rhyme label="a" type="f" corresp="#s6:l1">onvertsaagden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Fel-bestrijder, van de <rhyme label="b" type="m" corresp="#s6:l2">deugd</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Die de Stralen van <rhyme label="c" type="m">Gods-woord</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En sijn Hemels-Dau, <rhyme label="c" type="m" corresp="#s6:l5">verstoord</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">In-dien dat de <rhyme label="a" type="f">vlugge-Vlerken</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Of den Grysard, 't Roosje <rhyme label="b" type="m">spaard</rhyme>:</l>
        <l id="s7:l3" met="4" type="+-">En een Plompaard komt te <rhyme label="a" type="f" corresp="#s7:l1">merken</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Haar gewaden, soet van <rhyme label="b" type="m" corresp="#s7:l2">aart</rhyme>:</l>
        <l id="s7:l5" met="4" type="+-">Die (ontijdig) buyten <rhyme label="c" type="m">reên</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Handeld met dit Roosje <rhyme label="c" type="m" corresp="#s7:l5">kleen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Al haar cieraad sal <rhyme label="a" type="f">vervallen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">En verwelken, yder <rhyme label="b" type="m">blad</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Maagden die te veylig <rhyme label="a" type="f" corresp="#s8:l1">mallen</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Derven wel haar <rhyme label="b" type="m" corresp="#s8:l2">eêlste-schat</rhyme>:</l>
        <l id="s8:l5" met="4" type="+-">Door hen, die haar 't heyl <rhyme label="c" type="m">ontrukt</rhyme>:</l>
        <l id="s8:l6" met="4" type="+-">En haar kuysheyts-Roosje <rhyme label="c" type="m" corresp="#s8:l5">plukt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Neemt den sin dan wat <rhyme label="a" type="f">sekreter</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Als het woord van my <rhyme label="b" type="m">geseyd</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Geen ding, voegd de Maagden <rhyme label="a" type="f" corresp="#s9:l1">beter</rhyme></l>
        <l id="s9:l4" met="4" type="+-">Als bepaalde <rhyme label="b" type="m" corresp="#s9:l2">zedigheyt</rhyme>:</l>
        <l id="s9:l5" met="4" type="+-">Dartelheyt en <rhyme label="c" type="m">hovaardy</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Helpt hier menig in de <rhyme label="c" type="m" corresp="#s9:l5">ly</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Soete-Maagden, laat u <rhyme label="a" type="f">leren</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Van u niet ge-achte <rhyme label="b" type="m">vriend</rhyme>:</l>
        <l id="s10:l3" met="4" type="+-">Somtijts in verschoven <rhyme label="a" type="f" corresp="#s10:l1">kleren</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Schuyld wel wat dat iemand <rhyme label="b" type="m" corresp="#s10:l2">diend</rhyme>:</l>
        <l id="s10:l5" met="4" type="+-">Vreest hem die u Maagdom <rhyme label="c" type="m">huld</rhyme>:</l>
        <l id="s10:l6" met="4" type="+-">Al mijn wenschen is <rhyme label="c" type="m" corresp="#s10:l5">vervuld</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
