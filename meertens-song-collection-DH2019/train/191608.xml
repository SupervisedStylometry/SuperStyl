  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">L. NV hebdy t'lest van des Propheten // <rhyme label="a" type="m">lied</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Vrec sangig t'zijn, dient ons verweten // <rhyme label="a" type="m" corresp="#s1:l1">niet</rhyme></l>
        <l id="s1:l3" met="5" type="+-">Laet ander nu met sanghe boeten // <rhyme label="b" type="f">lusten</rhyme>,</l>
        <l id="s1:l4" met="5" type="+-">Wy hebben doch ons wel ghequeten // <rhyme label="a" type="m" corresp="#s1:l1">siet</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Wy slachten oock de boge, en moeten // <rhyme label="b" type="f" corresp="#s1:l3">rusten</rhyme>.</l>
      </lg>
    </lg>
  </text>
