<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187544</id>
    <date>
      <year-start>1693</year-start>
      <year-end>1693</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Allemande amoureus</melody>
  <author>Frank A. Metaal</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wy die ter zee gaen <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O Heer wilt ons <rhyme label="a" type="f" corresp="#s1:l1">bewaren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Voor stormen ende <rhyme label="a" type="f" corresp="#s1:l1">baren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die ons soude doen <rhyme label="b" type="f">beven</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wilt noyt ons hert <rhyme label="a" type="f" corresp="#s1:l1">besware</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Maer in gesontheyt <rhyme label="a" type="f" corresp="#s1:l1">spare</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Neemt wech de droeve <rhyme label="a" type="f" corresp="#s1:l1">mare</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Door uwe macht <rhyme label="b" type="f" corresp="#s1:l4">verheven</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">O Heer ons doch <rhyme label="c" type="m">geleyt</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Want wy zijn nu <rhyme label="c" type="m" corresp="#s1:l9">bereyt</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Tot Visschen met <rhyme label="c" type="m" corresp="#s1:l9">bescheyt</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Op ’t water wijt en <rhyme label="c" type="m" corresp="#s1:l9">breyt</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Wilt ons doch niet <rhyme label="d" type="f">verlate</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">Maer komt ons Heer te <rhyme label="d" type="f" corresp="#s1:l13">bate</rhyme>,</l>
        <l id="s1:l15" met="3" type="-+">Want wy zijn swacke <rhyme label="d" type="f" corresp="#s1:l13">vate</rhyme>,</l>
        <l id="s1:l16" met="4" type="-+">Sonder u niet,ja brooser dan een <rhyme label="e" type="m">riet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Geleyt ons in u <rhyme label="a" type="f">wegen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Op dat wy recht <rhyme label="a" type="f" corresp="#s2:l1">terdegen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Het Visschen mogen <rhyme label="a" type="f" corresp="#s2:l1">plegen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Na u wil en <rhyme label="b" type="f">behage</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Want ’t is hier aen <rhyme label="a" type="f" corresp="#s2:l1">gelegen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Goe wercken te <rhyme label="a" type="f" corresp="#s2:l1">plegen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wert zegen door <rhyme label="a" type="f" corresp="#s2:l1">verkregen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Soo de Schrift doet <rhyme label="b" type="f" corresp="#s2:l4">gewagen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Geeft ons ô Vader <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Visschen in <rhyme label="c" type="m" corresp="#s2:l9">overvloet</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Op dat wy vol en <rhyme label="c" type="m" corresp="#s2:l9">soet</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Komen te Landen met <rhyme label="c" type="m" corresp="#s2:l9">spoet</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Alwaer wy uwen <rhyme label="d" type="f">Name</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Sullen loven <rhyme label="d" type="f" corresp="#s2:l13">bequame</rhyme>,</l>
        <l id="s2:l15" met="3" type="-+">Na het rechte <rhyme label="d" type="f" corresp="#s2:l13">betame</rhyme>,</l>
        <l id="s2:l16" met="4" type="-+">Al op Maes-sluys,Heer in u heyligh <rhyme label="e" type="m">huys</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Doet ons dan die <rhyme label="a" type="f">genade</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat wy wel neerstigh <rhyme label="a" type="f" corresp="#s3:l1">gade</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Slaen op u woort en <rhyme label="a" type="f" corresp="#s3:l1">dade</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die u dienaers <rhyme label="b" type="f">verkonde</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">U woort laet zijn als <rhyme label="a" type="f" corresp="#s3:l1">zade</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In ons hart vroegh en <rhyme label="a" type="f" corresp="#s3:l1">spade</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Doet opwassen als <rhyme label="a" type="f" corresp="#s3:l1">blade</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dat wy goet zijn <rhyme label="b" type="f" corresp="#s3:l4">bevonden</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Voor u heyligh <rhyme label="c" type="m">aenschijn</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Geeft gy ons <rhyme label="c" type="m" corresp="#s3:l9">Medicijn</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Bevrijt ons van de <rhyme label="c" type="m" corresp="#s3:l9">pijn</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Die eeuwighlijck sal <rhyme label="c" type="m" corresp="#s3:l9">zijn</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">Laet ons gaen niet <rhyme label="d" type="f">verloren</rhyme>,</l>
        <l id="s3:l14" met="3" type="-+">Dat wy in sonden <rhyme label="d" type="f" corresp="#s3:l13">smooren</rhyme>,</l>
        <l id="s3:l15" met="3" type="-+">Maer maeckt ons nieuw <rhyme label="d" type="f" corresp="#s3:l13">herboren</rhyme>,</l>
        <l id="s3:l16" met="4" type="-+">Met uwen Geest,geduerigh by ons <rhyme label="e" type="m">weest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
