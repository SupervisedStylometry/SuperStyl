    <melody>Ik had een gestadig minnetje</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GOds liefde is groot van <rhyme label="a" type="f">prijse</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die hy ons heeft <rhyme label="b" type="m">betoocht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">In deerste met <rhyme label="a" type="f" corresp="#s1:l1">aduijse</rhyme></l>
        <l id="s1:l4" met="3" type="-+">U ooren hier toe <rhyme label="b" type="m" corresp="#s1:l2">boocht</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Adam van duechden <rhyme label="b" type="m" corresp="#s1:l2">verdroocht</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Heuet Gods liefde <rhyme label="c" type="f">vernomen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Een hope is hem <rhyme label="c" type="f" corresp="#s1:l6">voorcomen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dus v al tot haer <rhyme label="b" type="m" corresp="#s1:l2">poocht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sy is oock claer <rhyme label="a" type="f">ghebleken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">In Abraham <rhyme label="b" type="m">voorwaer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">In die beloften <rhyme label="a" type="f" corresp="#s2:l1">onbezweken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Sachmen Gods liefde <rhyme label="b" type="m" corresp="#s2:l2">claer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In Isaac oock daer <rhyme label="b" type="m" corresp="#s2:l2">naer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Iacob heeftse oock <rhyme label="c" type="f">vercreghen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Sy en is ons niet <rhyme label="c" type="f" corresp="#s2:l6">versweghen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Maer gheworden <rhyme label="b" type="m" corresp="#s2:l2">openbaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy waren alle <rhyme label="a" type="f">gheweken</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Al van der <rhyme label="b" type="m">salicheyt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Maer ons en mocht niet <rhyme label="a" type="f" corresp="#s3:l1">ghebreken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Al inder <rhyme label="b" type="m" corresp="#s3:l2">eewicheyt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Gods liefde daer veel aen <rhyme label="b" type="m" corresp="#s3:l2">leyt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Doen wy noch vianden <rhyme label="c" type="f">waren</rhyme></l>
        <l id="s3:l7" met="3" type="-+">So is Christus sonder <rhyme label="c" type="f" corresp="#s3:l6">sparen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Uoor ons ter doot <rhyme label="b" type="m" corresp="#s3:l2">bereyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hier in prijst God zijn <rhyme label="a" type="f">liefde</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Teghen ons in deser <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dat hy totter doot toe <rhyme label="a" type="f" corresp="#s4:l1">gheriefde</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Sijnen sone <rhyme label="b" type="m" corresp="#s4:l2">ghebenedijt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Doen wy waren in Gods <rhyme label="b" type="m" corresp="#s4:l2">nijt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Is Christus voor ons <rhyme label="c" type="f">ghestoruen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">En heeft ons paeys <rhyme label="c" type="f" corresp="#s4:l6">verworuen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Uan sduyuels strick <rhyme label="b" type="m" corresp="#s4:l2">beurijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wilt malcanderen <rhyme label="a" type="f">bewijsen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">So Christus gheboden <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dat ghy die liefde laet <rhyme label="a" type="f" corresp="#s5:l1">rijsen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In als dat God <rhyme label="b" type="m" corresp="#s5:l2">aencleeft</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Maer voor die liefde <rhyme label="b" type="m" corresp="#s5:l2">beeft</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Die de werelt mach <rhyme label="c" type="f">behaghen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Want in haer loose <rhyme label="c" type="f" corresp="#s5:l6">laghen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Heefter so veel <rhyme label="b" type="m" corresp="#s5:l2">ghesneeft</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Prince wildy <rhyme label="a" type="f">veruullen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Gods wet goet en <rhyme label="b" type="m">oprecht</rhyme></l>
        <l id="s6:l3" met="3" type="-+">En laet v niet <rhyme label="a" type="f" corresp="#s6:l1">verdullen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Want sy is al <rhyme label="b" type="m" corresp="#s6:l2">ghehecht</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Inde liefde so ons <rhyme label="b" type="m" corresp="#s6:l2">secht</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Alle Schriftueren <rhyme label="c" type="f">oorconden</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Sy verwint in allen <rhyme label="c" type="f" corresp="#s6:l6">stonden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Hoe seer mense <rhyme label="b" type="m" corresp="#s6:l2">beuecht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
