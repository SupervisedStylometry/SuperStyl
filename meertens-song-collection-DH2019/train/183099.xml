<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183099</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Pots honderdduizend slapperment</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Komt Vrinden van <rhyme label="a" type="m">d'Oranje-Stam</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">Zingt na u stem regt <rhyme label="b" type="m">op</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">God dank hier binnen <rhyme label="a" type="m" corresp="#s1:l1">Amsteldam</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Zyn wy ten hoogsten <rhyme label="b" type="m" corresp="#s1:l2">top</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ons Pruis die heeft ons weer <rhyme label="c" type="m">hersteld</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met kragt van zyn <rhyme label="d" type="m">Armé</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Die wakker waaren in het <rhyme label="c" type="m" corresp="#s1:l5">Veld</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Met de Erfprins ook <rhyme label="d" type="m" corresp="#s1:l6">meê</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat ziet men nu hier in de <rhyme label="a" type="m">Stad</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het is vreugde <rhyme label="b" type="m">overal</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Voor my, het is een groote <rhyme label="a" type="m" corresp="#s2:l1">schat</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En duizend zonder <rhyme label="b" type="m" corresp="#s2:l2">tal</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Op dat Oranje word <rhyme label="c" type="m">bemind</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van ieder in de <rhyme label="d" type="m">Stad</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Een Cocarde of een <rhyme label="c" type="m" corresp="#s2:l5">Oranje-lint</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Draagt men met volle <rhyme label="d" type="m" corresp="#s2:l6">hart</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Zo gy nu wilt uit 't wandele <rhyme label="a" type="m">gaan</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dan blyft niet in u <rhyme label="b" type="m">huis</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wilt dan na de Joode Breedstraat <rhyme label="a" type="m" corresp="#s3:l1">gaan</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Kommende van de <rhyme label="b" type="m" corresp="#s3:l2">Sluis</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Als gy zit in een koets of <rhyme label="c" type="m">sleê</rhyme>;</l>
        <l id="s3:l6" met="3" type="-+">Dan zyt gy vast <rhyme label="d" type="m">bereid</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Zo u de Courant wel meede <rhyme label="c" type="m" corresp="#s3:l5">dee</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dat u niets tog <rhyme label="d" type="m" corresp="#s3:l6">spyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daar ziet men al wat schoonheid <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Van Kaarslicht en <rhyme label="b" type="m">Gebouw</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Verders is het ook niet <rhyme label="a" type="m" corresp="#s4:l1">mis</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Wel waard dat men 't <rhyme label="b" type="m" corresp="#s4:l2">beschouw</rhyme>;</l>
        <l id="s4:l5" met="4" type="-+">In alle Straaten hier in 't <rhyme label="c" type="m">rond</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Onnoemelyk in 't <rhyme label="d" type="m">getal</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Hoord men met eene blyde <rhyme label="c" type="m" corresp="#s4:l5">mond</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Oranje boven <rhyme label="d" type="m" corresp="#s4:l6">al</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Oranje is de waare <rhyme label="a" type="m">Stam</rhyme>;</l>
        <l id="s5:l2" met="3" type="-+">Die ons de Vryheid <rhyme label="b" type="m">gaf</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En ons uit slaafsche Keetens <rhyme label="a" type="m" corresp="#s5:l1">nam</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van Spanje's groote <rhyme label="b" type="m" corresp="#s5:l2">staf</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En woud gy dan zulk een <rhyme label="c" type="m">Vorst</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Niet kennen voor u <rhyme label="d" type="m">Heer</rhyme>;</l>
        <l id="s5:l7" met="4" type="-+">Ziet wat het u nu heeft <rhyme label="c" type="m" corresp="#s5:l5">gekost</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Een onvergankelyke <rhyme label="d" type="m" corresp="#s5:l6">eer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ik raad u menschen die gy <rhyme label="a" type="m">zyt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Weest zulks nooit van <rhyme label="b" type="m">sins</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Want het is verlooren <rhyme label="a" type="m" corresp="#s6:l1">arbeid</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Te vegten tegen een <rhyme label="b" type="m" corresp="#s6:l2">Prins</rhyme>:</l>
        <l id="s6:l5" met="4" type="-+">Ik raad u menschen wie gy <rhyme label="c" type="m">zyt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Weest zulks nooit van <rhyme label="d" type="m">sins</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Want het is verlooren <rhyme label="c" type="m" corresp="#s6:l5">arbeid</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Te vegten tegen een <rhyme label="d" type="m" corresp="#s6:l6">Prins</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
