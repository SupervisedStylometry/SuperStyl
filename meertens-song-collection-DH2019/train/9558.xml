<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9558</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="danklied" translation="gratitude song">danklied</tag>
    </tags>
    <melody>O welig Amsterdam</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrick Jansz. Prins</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GOdts handt die is vol <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Vol zenu ende <rhyme label="a" type="m" corresp="#s1:l1">kracht</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Wat dreyght ghy my <rhyme label="b" type="m">Doodt</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Mijn leven hy noch <rhyme label="c" type="m">spaert</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Op dat van my <rhyme label="c" type="m" corresp="#s1:l4">verklaert</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Werd' sijn daden <rhyme label="b" type="m" corresp="#s1:l3">groot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">De Heer heeft my, 't is <rhyme label="a" type="m">waer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met sieckt, benautheyt <rhyme label="a" type="m" corresp="#s2:l1">swaer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Harde <rhyme label="b" type="m">ghekastijt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maer gaf my in den <rhyme label="c" type="m">noodt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Niet over tot der <rhyme label="c" type="m" corresp="#s2:l4">doodt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">'t Waer een kleyne <rhyme label="b" type="m" corresp="#s2:l3">strijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Het duerde niet seer <rhyme label="a" type="m">langh</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Maer 't viel my dapper <rhyme label="a" type="m" corresp="#s3:l1">bangh</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met mijn naest <rhyme label="b" type="m">bloedt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Mijn goe bekende <rhyme label="c" type="m">trou</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die waren t'saem in <rhyme label="c" type="m" corresp="#s3:l4">rou</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Door mijn <rhyme label="b" type="m" corresp="#s3:l3">tegenspoedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Heer, ick had' meer <rhyme label="a" type="m">verdient</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Maer ben ick niet u <rhyme label="a" type="m" corresp="#s4:l1">Vrient</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Die ghy dier <rhyme label="b" type="m">gekocht</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hebt, met u heyligh <rhyme label="c" type="m">bloedt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat suyver ende <rhyme label="c" type="m" corresp="#s4:l4">goedt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">My verlossingh <rhyme label="b" type="m" corresp="#s4:l3">brocht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu bid' ick, lieve <rhyme label="a" type="m">Heer</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Alleen tot uwer <rhyme label="a" type="m" corresp="#s5:l1">eer</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En mijn <rhyme label="b" type="m">saligheydt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Geef my bequame <rhyme label="c" type="m">stof</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Op dat u eer, en <rhyme label="c" type="m" corresp="#s5:l4">lof</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Van my werdt <rhyme label="b" type="m" corresp="#s5:l3">verbreydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ey! laet doch open <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s6:l2" met="3" type="-+">De Poorten nu voor <rhyme label="a" type="m" corresp="#s6:l1">mijn</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Der <rhyme label="b" type="m">gherechtigheyt</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Dat ick in-gae <rhyme label="c" type="m">terstondt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En daer met hert, en <rhyme label="c" type="m" corresp="#s6:l4">mondt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Eer u <rhyme label="b" type="m" corresp="#s6:l3">Majesteyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hier door soo komt men <rhyme label="a" type="m">tot</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Den Heere onsen <rhyme label="a" type="m" corresp="#s7:l1">Godt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Wie daer door in <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En valt den Heer te <rhyme label="c" type="m">voet</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Na sijn behaghen <rhyme label="c" type="m" corresp="#s7:l4">doet</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Vast in deughden <rhyme label="b" type="m" corresp="#s7:l3">staet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Soo haest als ick u <rhyme label="a" type="m">badt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Leydt ghy my van het <rhyme label="a" type="m" corresp="#s8:l1">padt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Der kastijdingh <rhyme label="b" type="m">of</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Gesondheyt bracht ghy, <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Ick gheef u danck, en <rhyme label="c" type="m" corresp="#s8:l4">eer</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En alleen den <rhyme label="b" type="m" corresp="#s8:l3">lof</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Een weder-loon wil <rhyme label="a" type="m">ick</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Aen mijn Verlosser <rhyme label="a" type="m" corresp="#s9:l1">rijck</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Geven, nu met <rhyme label="b" type="m">vlijt</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ick sal den Kelck <rhyme label="c" type="m">heylsaem</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Op-nemen, en sijn <rhyme label="c" type="m" corresp="#s9:l4">Naem</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Breyden uyt <rhyme label="b" type="m" corresp="#s9:l3">altijt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">O Heer! om uwen <rhyme label="a" type="m">Naem</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Maeck my daer toe <rhyme label="a" type="m" corresp="#s10:l1">bequaem</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">Ey! mijn hert <rhyme label="b" type="m">bereydt</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dat ick u prijs en <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Mijn Bruydegom, mijn <rhyme label="c" type="m" corresp="#s10:l4">Heer</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">In der <rhyme label="b" type="m" corresp="#s10:l3">eeuwigheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
