<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24515</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
    </tags>
    <melody>De maan heeft zich verbleekt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Die Conynck van hier <rhyme label="a" type="f">bauen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">die siet ons altoes <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">mit synen claren <rhyme label="a" type="f" corresp="#s1:l1">ogen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">oec werwert dat wi <rhyme label="b" type="m" corresp="#s1:l2">gaen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">hi heeft gelecht syne <rhyme label="c" type="f">strycken</rhyme></l>
        <l id="s1:l6" met="3" type="-+">op dat hi ons mach <rhyme label="c" type="f" corresp="#s1:l5">trecken</rhyme></l>
        <l id="s1:l7" met="3" type="-+">al in syn hoechste <rhyme label="b" type="m" corresp="#s1:l2">sael</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hi spreket mit sueter <rhyme label="a" type="f">stemmen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">al totter zielen <rhyme label="b" type="m">myn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">woldy v seluen <rhyme label="a" type="f" corresp="#s2:l1">temmen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">ende myn geuangen <rhyme label="b" type="m" corresp="#s2:l2">syn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">ic wolde v staen in <rhyme label="c" type="f">staeden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">ende geuen v myne <rhyme label="c" type="f" corresp="#s2:l5">genaede</rhyme></l>
        <l id="s2:l7" met="3" type="-+">ende trecken v tot <rhyme label="b" type="m" corresp="#s2:l2">my</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O ziele ghi syt soe <rhyme label="a" type="f">edelen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">soe hogen <rhyme label="b" type="m">creatuer</rhyme></l>
        <l id="s3:l3" met="3" type="-+">geulaten wt mynen <rhyme label="a" type="f" corresp="#s3:l1">herten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">gy syt my gheworden <rhyme label="b" type="m" corresp="#s3:l2">suer</rhyme></l>
        <l id="s3:l5" met="3" type="-+">ick heb v <rhyme label="c" type="f">wtuercaren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">ic liet v noede <rhyme label="c" type="f" corresp="#s3:l5">verlaren</rhyme></l>
        <l id="s3:l7" met="3" type="-+">gy staet my alsoe <rhyme label="b" type="m" corresp="#s3:l2">duer</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O ziele slutet op uwes hertsen <rhyme label="a" type="f">doer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">ic sal daer comen <rhyme label="b" type="m">in</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ick en mach niet langer <rhyme label="a" type="f" corresp="#s4:l1">beyden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">ic wil v lere <rhyme label="b" type="m" corresp="#s4:l2">syn</rhyme></l>
        <l id="s4:l5" met="3" type="-+">gy moet v seluen <rhyme label="c" type="f">laten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">ende alles dynges   <rhyme label="c" type="f" corresp="#s4:l5">versaken</rhyme></l>
        <l id="s4:l7" met="3" type="-+">ende doen eens anders <rhyme label="b" type="m" corresp="#s4:l2">wil</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ende blyuet in eenre stylheit <rhyme label="a" type="f">staen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">van bynnen <rhyme label="b" type="m">opgericht</rhyme></l>
        <l id="s5:l3" met="3" type="-+">ende wees een ander doet of <rhyme label="a" type="f" corresp="#s5:l1">laet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">volget altoes mynen <rhyme label="b" type="m" corresp="#s5:l2">treck</rhyme></l>
        <l id="s5:l5" met="3" type="-+">soe moegdy comen hier <rhyme label="c" type="f">bauen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">daer mi die engelen <rhyme label="c" type="f" corresp="#s5:l5">lauen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">dat is die rechte <rhyme label="b" type="m" corresp="#s5:l2">wech</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O waerlicke myne <rhyme label="a" type="f">crachten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ick  moet my keren <rhyme label="b" type="m">in</rhyme></l>
        <l id="s6:l3" met="3" type="-+">ic hoer in mynen <rhyme label="a" type="f" corresp="#s6:l1">herten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">soe vrendelicken <rhyme label="b" type="m" corresp="#s6:l2">stemme</rhyme></l>
        <l id="s6:l5" met="3" type="-+">dat ic solde opwert <rhyme label="c" type="f">clymmen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">bauen alle dese eertsche <rhyme label="c" type="f" corresp="#s6:l5">dyngen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">ende staen in synen <rhyme label="b" type="m" corresp="#s6:l2">wyl</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hier en is geen langer <rhyme label="a" type="f">beyden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">wy moeten opwert <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s7:l3" met="3" type="-+">wille wy <rhyme label="a" type="f" corresp="#s7:l1">jubelieren</rhyme></l>
        <l id="s7:l4" met="3" type="-+">al inden hoegen <rhyme label="b" type="m" corresp="#s7:l2">throen</rhyme></l>
        <l id="s7:l5" met="3" type="-+">daer wy syn toe <rhyme label="c" type="f">gescapen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">die schepper die ons <rhyme label="c" type="f" corresp="#s7:l5">maecte</rhyme></l>
        <l id="s7:l7" met="3" type="-+">sal syn onse ewige <rhyme label="b" type="m" corresp="#s7:l2">loen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wy syn hier in <rhyme label="a" type="f">ellende</rhyme></l>
        <l id="s8:l2" met="3" type="-+">mit droefheit <rhyme label="b" type="m">ombeuaen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">het genaeket seer onsen <rhyme label="a" type="f" corresp="#s8:l1">eynde</rhyme></l>
        <l id="s8:l4" met="3" type="-+">die doet die volget ons <rhyme label="b" type="m" corresp="#s8:l2">nae</rhyme></l>
        <l id="s8:l5" met="3" type="-+">waer op moegen wy ons <rhyme label="c" type="f">verlaten</rhyme></l>
        <l id="s8:l6" met="3" type="-+">wy moeten ons ynwert <rhyme label="c" type="f" corresp="#s8:l5">saten</rhyme></l>
        <l id="s8:l7" met="3" type="-+">ende sien die ewicheit <rhyme label="b" type="m" corresp="#s8:l2">aen</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ick wil my <rhyme label="a" type="f">auergeuen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">ende tasten my seluen <rhyme label="b" type="m">aen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">al soldet my costen myn <rhyme label="a" type="f" corresp="#s9:l1">leuen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">ic willet nochtant <rhyme label="b" type="m" corresp="#s9:l2">doen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">ic moet my seluen <rhyme label="c" type="f">wagen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">ende alles dinge <rhyme label="c" type="f" corresp="#s9:l5">verdragen</rhyme></l>
        <l id="s9:l7" met="3" type="-+">sal ic die croen <rhyme label="b" type="m" corresp="#s9:l2">ontfaen</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Set wt uwer <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s10:l2" met="3" type="-+">der creatueren <rhyme label="b" type="m">mynne</rhyme></l>
        <l id="s10:l3" met="3" type="-+">ende stort in my vwe <rhyme label="a" type="f" corresp="#s10:l1">crachten</rhyme></l>
        <l id="s10:l4" met="3" type="-+">ic wil v hulper <rhyme label="b" type="m" corresp="#s10:l2">syn</rhyme></l>
        <l id="s10:l5" met="3" type="-+">soe moegdy ewelick <rhyme label="c" type="f">leuen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">ende staen mit hoge <rhyme label="c" type="f" corresp="#s10:l5">verheuen</rhyme></l>
        <l id="s10:l7" met="3" type="-+">ic wil v dienre <rhyme label="b" type="m" corresp="#s10:l2">syn</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
