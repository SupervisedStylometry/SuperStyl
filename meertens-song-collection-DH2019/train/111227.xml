    <melody>Van Paul Jonas en de Engelsman</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Zegt <rhyme label="a" type="m">Engeland</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Wat had gy in u <rhyme label="b" type="m">zin</rhyme>?</l>
        <l id="s1:l3" met="3" type="+-">Dat gy zo <rhyme label="a" type="m" corresp="#s1:l1">Astrand</rhyme>;</l>
        <l id="s1:l4" met="3" type="+-">Nam Sintestatius <rhyme label="b" type="m" corresp="#s1:l2">in</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Door u <rhyme label="c" type="m">Verradery</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Romtom aan alle <rhyme label="c" type="m" corresp="#s1:l5">zy</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Wy hebben <rhyme label="d" type="f">helden</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Die u niet kwyt en <rhyme label="d" type="f" corresp="#s1:l7">schelden</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Dat is 't woord van <rhyme label="c" type="m" corresp="#s1:l5">my</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Ag <rhyme label="a" type="m">Franryk</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Spreekt dog zo niet <rhyme label="b" type="m">Astrand</rhyme>.</l>
        <l id="s2:l3" met="3" type="+-">Ik weet dat ge <rhyme label="a" type="m" corresp="#s2:l1">publyk</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Vegt voor 't <rhyme label="b" type="m" corresp="#s2:l2">Nederland</rhyme>;</l>
        <l id="s2:l5" met="3" type="+-">'t Zal nu zo niet <rhyme label="c" type="m">gaan</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Als gy al hebt <rhyme label="c" type="m" corresp="#s2:l5">gedaan</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">U Hoogmoed kan <rhyme label="d" type="f">zinken</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Kanon zal beeter <rhyme label="d" type="f" corresp="#s2:l7">klinken</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Als 't 'er koomt op <rhyme label="c" type="m" corresp="#s2:l5">aan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Wel <rhyme label="a" type="m">Engeland</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Gy had het nooid <rhyme label="b" type="m">gedagt</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Al zyt gy zo <rhyme label="a" type="m" corresp="#s3:l1">Astrand</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Nu word gy uit <rhyme label="b" type="m" corresp="#s3:l2">gelagt</rhyme>;</l>
        <l id="s3:l5" met="3" type="+-">Gy meende na u <rhyme label="c" type="m">zin</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Alles te neemen <rhyme label="c" type="m" corresp="#s3:l5">in</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Den Fransen haan zal <rhyme label="d" type="f">kraayen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">De Oranje Vlag zal <rhyme label="d" type="f" corresp="#s3:l7">waayen</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Agt ons niet te <rhyme label="c" type="m" corresp="#s3:l5">min</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Ik weet gy heb <rhyme label="a" type="m">Geswooren</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Met Holland een <rhyme label="b" type="m">verbond</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Wy geevent niet <rhyme label="a" type="m" corresp="#s4:l1">verlooren</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Liever na de <rhyme label="b" type="m" corresp="#s4:l2">gront</rhyme>;</l>
        <l id="s4:l5" met="3" type="+-">Myn Volk vreeze <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Zy hebben geen <rhyme label="c" type="m" corresp="#s4:l5">verdriet</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Men zal de <rhyme label="d" type="f">Fransen</rhyme>;</l>
        <l id="s4:l8" met="3" type="+-">Nog op de Zee doen <rhyme label="d" type="f" corresp="#s4:l7">Dansen</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Blyft buiten ons <rhyme label="c" type="m" corresp="#s4:l5">gebiet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Met drie Fregatten <rhyme label="a" type="m">zoet</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Zylden wy <rhyme label="b" type="m">Geladen</rhyme>;</l>
        <l id="s5:l3" met="3" type="+-">'t Volk was vol <rhyme label="a" type="m" corresp="#s5:l1">moet</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Op Sintestatius <rhyme label="b" type="m" corresp="#s5:l2">aan</rhyme>;</l>
        <l id="s5:l5" met="3" type="+-">In 't midden van den <rhyme label="c" type="m">Nagt</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Daar niemant op en <rhyme label="c" type="m" corresp="#s5:l5">dagt</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Men naamse als <rhyme label="d" type="f">honden</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">En aan malkaar <rhyme label="d" type="f" corresp="#s5:l7">gebonden</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">In de Prison <rhyme label="c" type="m" corresp="#s5:l5">Gebragt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Ag Kooning <rhyme label="a" type="m">Loodewyk</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Wy benne niet <rhyme label="b" type="m">ontsteld</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Gy Vegt maar <rhyme label="a" type="m" corresp="#s6:l1">publyk</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Voor 't Hollandse <rhyme label="b" type="m" corresp="#s6:l2">Geld</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Doet al maar watje <rhyme label="c" type="m">kan</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Ik blyf den <rhyme label="c" type="m" corresp="#s6:l5">Engelsman</rhyme>,</l>
        <l id="s6:l7" met="2" type="+-">Niemant kan ons <rhyme label="d" type="f">deeren</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Myn Volk zal u <rhyme label="d" type="f" corresp="#s6:l7">keeren</rhyme></l>
        <l id="s6:l9" met="3" type="+-">'t Kruit is op de <rhyme label="c" type="m" corresp="#s6:l5">pan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">Wel <rhyme label="a" type="m">Engeland</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Gy Waar een held op <rhyme label="b" type="m">Zee</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Nu schryft Gy <rhyme label="a" type="m" corresp="#s7:l1">astrant</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">En spreekt nog van de <rhyme label="b" type="m" corresp="#s7:l2">VreÃª</rhyme>;</l>
        <l id="s7:l5" met="3" type="+-">Maar 't zal der zo niet <rhyme label="c" type="m">gaan</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Van Nieuws ter weer op <rhyme label="c" type="m" corresp="#s7:l5">aan</rhyme>,</l>
        <l id="s7:l7" met="2" type="+-">Ik heb 300 <rhyme label="d" type="f">scheepen</rhyme>.</l>
        <l id="s7:l8" met="3" type="+-">Die zulle u Nog <rhyme label="d" type="f" corresp="#s7:l7">sleepen</rhyme>,</l>
        <l id="s7:l9" met="3" type="+-">Als 't 'er koomt <rhyme label="c" type="m" corresp="#s7:l5">opaan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-">Zwyg valse <rhyme label="a" type="m">hond</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Voor myn in korten <rhyme label="b" type="m">stond</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Wy zullen u doen <rhyme label="a" type="m" corresp="#s8:l1">temmen</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Schieten in de <rhyme label="b" type="m" corresp="#s8:l2">Grond</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">den haan staat wel <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s8:l6" met="3" type="+-">En vreest nog geen <rhyme label="c" type="m" corresp="#s8:l5">gevaar</rhyme>,</l>
        <l id="s8:l7" met="2" type="+-">Men zal u nog wel <rhyme label="d" type="f">keeren</rhyme>,</l>
        <l id="s8:l8" met="3" type="+-">Op lekkerny <rhyme label="d" type="f" corresp="#s8:l7">Trakteeren</rhyme>,</l>
        <l id="s8:l9" met="3" type="+-">Net of 't kermis <rhyme label="c" type="m" corresp="#s8:l5">waar</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="+-">Ik ben voor geen <rhyme label="a" type="m">haan</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Of Holland nog <rhyme label="b" type="m">vervaart</rhyme>.</l>
        <l id="s9:l3" met="3" type="+-">Al schoon is <rhyme label="a" type="m" corresp="#s9:l1">Amerika</rhyme>;</l>
        <l id="s9:l4" met="3" type="+-">met u <rhyme label="b" type="m" corresp="#s9:l2">gepaard</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Gibralter is <rhyme label="c" type="m">astrant</rhyme>,</l>
        <l id="s9:l6" met="3" type="+-">Ze vreezen geen <rhyme label="c" type="m" corresp="#s9:l5">vyand</rhyme>,</l>
        <l id="s9:l7" met="2" type="+-">Portmahon <rhyme label="d" type="f">beneeven</rhyme>,</l>
        <l id="s9:l8" met="3" type="+-">daar ieder voor moet <rhyme label="d" type="f" corresp="#s9:l7">beeven</rhyme>,</l>
        <l id="s9:l9" met="3" type="+-">Zy vegten <rhyme label="c" type="m" corresp="#s9:l5">Florisant</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="+-">Ag <rhyme label="a" type="m">Engeland</rhyme>;</l>
        <l id="s10:l2" met="3" type="+-">Het zal wel anders <rhyme label="b" type="m">gaan</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Al benje zo <rhyme label="a" type="m" corresp="#s10:l1">Astrand</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">'t Gaat 'er weer op <rhyme label="b" type="m" corresp="#s10:l2">aan</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Koom wy met onze <rhyme label="c" type="m">vloot</rhyme>,</l>
        <l id="s10:l6" met="3" type="+-">Versien van kruyt en <rhyme label="c" type="m" corresp="#s10:l5">loot</rhyme></l>
        <l id="s10:l7" met="2" type="+-">doe sprak <rhyme label="d" type="f">Kinsbergen</rhyme>,</l>
        <l id="s10:l8" met="3" type="+-">Van Braam liet hem niet <rhyme label="d" type="f" corresp="#s10:l7">tergen</rhyme></l>
        <l id="s10:l9" met="3" type="+-">Holland heeft nog geen <rhyme label="c" type="m" corresp="#s10:l5">nood</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="+-">Loodewyk den <rhyme label="a" type="m">Kooning</rhyme>,</l>
        <l id="s11:l2" met="3" type="+-">Sprak dappere <rhyme label="b" type="m">Zoutman</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">Slaat hem uit zyn <rhyme label="a" type="m" corresp="#s11:l1">woning</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Toont maar watje <rhyme label="b" type="m" corresp="#s11:l2">kan</rhyme>,</l>
        <l id="s11:l5" met="3" type="+-">Dedel en <rhyme label="c" type="m">Braak</rhyme></l>
        <l id="s11:l6" met="3" type="+-">zy Vegten met <rhyme label="c" type="m" corresp="#s11:l5">vermaak</rhyme>,</l>
        <l id="s11:l7" met="2" type="+-">Onse Vloot met <rhyme label="d" type="f">scheepen</rhyme>,</l>
        <l id="s11:l8" met="3" type="+-">die zal den hond wel <rhyme label="d" type="f" corresp="#s11:l7">sleepen</rhyme>,</l>
        <l id="s11:l9" met="3" type="+-">En neemen weer de <rhyme label="c" type="m" corresp="#s11:l5">wraak</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="+-">Wel groot <rhyme label="a" type="m">Brittanje</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">Gy heeft ons vals <rhyme label="b" type="m">bedient</rhyme></l>
        <l id="s12:l3" met="3" type="+-">Wy meende dat <rhyme label="a" type="m" corresp="#s12:l1">Oranje</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">Was u beste <rhyme label="b" type="m" corresp="#s12:l2">Vriend</rhyme>,</l>
        <l id="s12:l5" met="3" type="+-">'t Komt wel anders <rhyme label="c" type="m">uit</rhyme>,</l>
        <l id="s12:l6" met="3" type="+-">Gy krygt wat op u <rhyme label="c" type="m" corresp="#s12:l5">huis</rhyme>,</l>
        <l id="s12:l7" met="2" type="+-">Wild tog maar <rhyme label="d" type="f">koomen</rhyme>,</l>
        <l id="s12:l8" met="3" type="+-">Op de zoute <rhyme label="d" type="f" corresp="#s12:l7">stroomen</rhyme>,</l>
        <l id="s12:l9" met="3" type="+-">Heist maar de Bloot Vlag <rhyme label="c" type="m" corresp="#s12:l5">uit</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="2" type="+-">Ik ben de <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">Genaamt <rhyme label="b" type="m">Lelyvorst</rhyme>,</l>
        <l id="s13:l3" met="3" type="+-">den schelm moet 'er <rhyme label="a" type="m" corresp="#s13:l1">an</rhyme>,</l>
        <l id="s13:l4" met="3" type="+-">Ik heb na het bloed <rhyme label="b" type="m" corresp="#s13:l2">gedorst</rhyme>;</l>
        <l id="s13:l5" met="3" type="+-">draag getrouw 't <rhyme label="c" type="m">zwaart</rhyme>,</l>
        <l id="s13:l6" met="3" type="+-">Holland weest niet <rhyme label="c" type="m" corresp="#s13:l5">vervaart</rhyme>,</l>
        <l id="s13:l7" met="2" type="+-">men zag 't aan <rhyme label="d" type="f">Parker</rhyme>;</l>
        <l id="s13:l8" met="3" type="+-">de vallen hond was <rhyme label="d" type="f" corresp="#s13:l7">sterker</rhyme>,</l>
        <l id="s13:l9" met="3" type="+-">Is ons geen Oortje <rhyme label="c" type="m" corresp="#s13:l5">waart</rhyme>,</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="2" type="+-">Ik wensch boven <rhyme label="a" type="m">maaten</rhyme>,</l>
        <l id="s14:l2" met="3" type="+-">Aan de Oranje <rhyme label="b" type="m">pland</rhyme>,</l>
        <l id="s14:l3" met="3" type="+-">den zegen aan de <rhyme label="a" type="m" corresp="#s14:l1">Staaten</rhyme>,</l>
        <l id="s14:l4" met="3" type="+-">Van ons <rhyme label="b" type="m" corresp="#s14:l2">Nederland</rhyme>,</l>
        <l id="s14:l5" met="3" type="+-">Als dat de Keysers <rhyme label="c" type="m">Kroon</rhyme>,</l>
        <l id="s14:l6" met="3" type="+-">mag blinken also <rhyme label="c" type="m" corresp="#s14:l5">schoon</rhyme>,</l>
        <l id="s14:l7" met="2" type="+-">Ook Frankryk <rhyme label="d" type="f">verheeven</rhyme>,</l>
        <l id="s14:l8" met="3" type="+-">Godt wil haar Zeegen <rhyme label="d" type="f" corresp="#s14:l7">Geeven</rhyme>,</l>
        <l id="s14:l9" met="3" type="+-">Uit zyn genade <rhyme label="c" type="m" corresp="#s14:l5">Troon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
