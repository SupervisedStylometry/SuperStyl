  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KOm Speelmeysje, wilw’ een <rhyme label="a" type="f">reysje</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Lipjes strelen, niet te <rhyme label="b" type="m">grof</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Kom mijn poes’le <rhyme label="a" type="f" corresp="#s1:l1">Minne-vleysje</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Hey! een kusje is maer <rhyme label="b" type="m" corresp="#s1:l2">stof</rhyme>,</l>
        <l id="s1:l5" met="5" type="+-">Kusjes, kusjes, soete lieve <rhyme label="c" type="f">kusjes</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Die ’t niet dient, die veeght het <rhyme label="b" type="m" corresp="#s1:l2">of</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Kom Speelmeysje, wilw’ een <rhyme label="a" type="f">reysje</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">T’Samen gaen in <rhyme label="b" type="m">Venus-hof</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Kom mijn poes’le <rhyme label="a" type="f" corresp="#s2:l1">Minne-vleysje</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Hey! een kusje is maer <rhyme label="b" type="m" corresp="#s2:l2">stof</rhyme>,</l>
        <l id="s2:l5" met="5" type="+-">Kusjes, kusjes, soete lieve <rhyme label="c" type="f">kusjes</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Die ’t niet dient, die veeght het <rhyme label="b" type="m" corresp="#s2:l2">of</rhyme>.</l>
      </lg>
    </lg>
