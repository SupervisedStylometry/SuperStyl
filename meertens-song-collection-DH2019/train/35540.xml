<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35540</id>
    <date>
      <year-start>1644</year-start>
      <year-end>1644</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Een kindje is ons geboren</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Joannes de Lixbona</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">TE Nazareth was <rhyme label="a" type="f">besloten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dees bloem in haeren <rhyme label="b" type="m">bodt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die nu is <rhyme label="a" type="f" corresp="#s1:l1">uytghesproten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Seer schoon verciert van <rhyme label="b" type="m" corresp="#s1:l2">Godt</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Te Bethlehem wordt <rhyme label="c" type="f">ghevonden</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Dit <rhyme label="d" type="m">Hemels-broodt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat ons gheneest van <rhyme label="c" type="f" corresp="#s1:l5">sonden</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En doodt de <rhyme label="d" type="m" corresp="#s1:l6">doodt</rhyme>.</l>
        <l id="s1:l9" met="5" type="-+">Een kindeken is ons gheboren, Godt lof en <rhyme label="e" type="m">danck</rhyme>,</l>
        <l id="s1:l10" met="5" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s1:l9">sanck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den schijn van 's Vader <rhyme label="a" type="f">glorie</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Sijns wesens <rhyme label="b" type="m">Even-beelt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wt sijn vruchtbaer <rhyme label="a" type="f" corresp="#s2:l1">memorie</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Eerst sonder vrouw <rhyme label="b" type="m" corresp="#s2:l2">gheteelt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Is van een maeght <rhyme label="c" type="f">gheboren</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Oock sonder <rhyme label="d" type="m">man</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Dit vat heeft hy <rhyme label="c" type="f" corresp="#s2:l5">verkoren</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Die wonder <rhyme label="d" type="m" corresp="#s2:l6">kan</rhyme>.</l>
        <l id="s2:l9" met="5" type="-+">Een kindeken is ons gheboren, Godt lof en <rhyme label="e" type="m">danck</rhyme>,</l>
        <l id="s2:l10" met="5" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s2:l9">sanck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maer siet, hoe dat onthaelt <rhyme label="a" type="f">wordt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Godts Soon, Godts Woordt, Godts <rhyme label="b" type="m">schijn</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Hoe quaelijck dat betaelt <rhyme label="a" type="f" corresp="#s3:l1">wordt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sijn min, sijn jonst, sijn <rhyme label="b" type="m" corresp="#s3:l2">pijn</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Hy gaet tot sijne <rhyme label="c" type="f">vrinden</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Soeckt over <rhyme label="d" type="m">al</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Gheen plaets en kan hy <rhyme label="c" type="f" corresp="#s3:l5">vinden</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Als in den <rhyme label="d" type="m" corresp="#s3:l6">stal</rhyme>.</l>
        <l id="s3:l9" met="5" type="-+">Een kindeken is ons gheboren, Godt lof en <rhyme label="e" type="m">danck</rhyme>,</l>
        <l id="s3:l10" met="5" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s3:l9">sanck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Comt alle <rhyme label="a" type="f">creatueren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dient die u 't wesen <rhyme label="b" type="m">gheeft</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Comt Enghels, sijn <rhyme label="a" type="f" corresp="#s4:l1">ghebueren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die door hem eeuwigh <rhyme label="b" type="m" corresp="#s4:l2">leeft</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Comt menschen tot u <rhyme label="c" type="f">broeder</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Soo kleyn, soo <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Die u met sijne <rhyme label="c" type="f" corresp="#s4:l5">Moeder</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Treckt uyt de <rhyme label="d" type="m" corresp="#s4:l6">doodt</rhyme>.</l>
        <l id="s4:l9" met="5" type="-+">Een kindeken is ons gheboren, Godt lof en <rhyme label="e" type="m">danck</rhyme>,</l>
        <l id="s4:l10" met="5" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s4:l9">sanck</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Comt Branders nu <rhyme label="a" type="f">beneden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Met al u <rhyme label="b" type="m">minne-vier</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Comt <rhyme label="a" type="f" corresp="#s5:l1">Volle-wetentheden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">V Meester die is <rhyme label="b" type="m" corresp="#s5:l2">hier</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Comt Throonen hem <rhyme label="c" type="f">ontfanghen</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Daer ghy in <rhyme label="d" type="m">rust</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Des werelts groot <rhyme label="c" type="f" corresp="#s5:l5">verlangen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Des Hemels <rhyme label="d" type="m" corresp="#s5:l6">lust</rhyme>.</l>
        <l id="s5:l9" met="5" type="-+">Een kindeken is ons gheboren, Godt lof en <rhyme label="e" type="m">danck</rhyme>,</l>
        <l id="s5:l10" met="5" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s5:l9">sanck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Comt hooghe <rhyme label="a" type="f">Heerschappijen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Comt alle Cracht en <rhyme label="b" type="m">Macht</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Comt Princen hem <rhyme label="a" type="f" corresp="#s6:l1">bevrijen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van 't haetich Ioods <rhyme label="b" type="m" corresp="#s6:l2">gheslacht</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Comt hier Arch-enghels <rhyme label="c" type="f">vlieghen</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Met soet <rhyme label="d" type="m">gheschal</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Comt Enghelkens hem <rhyme label="c" type="f" corresp="#s6:l5">wieghen</rhyme></l>
        <l id="s6:l8" met="2" type="-+">In desen <rhyme label="d" type="m" corresp="#s6:l6">stal</rhyme>.</l>
        <l id="s6:l9" met="5" type="-+">Een kindeken is ons gheboren, Godt lof en <rhyme label="e" type="m">danck</rhyme>,</l>
        <l id="s6:l10" met="5" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s6:l9">sanck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maer boven al, ghy <rhyme label="a" type="f">menschen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Comt dienen uwen <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Die u gheeft uwe <rhyme label="a" type="f" corresp="#s7:l1">wenschen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En noch belooft veel <rhyme label="b" type="m" corresp="#s7:l2">meer</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Comt hier van alle <rhyme label="c" type="f">kanten</rhyme>,</l>
        <l id="s7:l6" met="2" type="-+">O Adams <rhyme label="d" type="m">bloet</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Dit kindt comt in u <rhyme label="c" type="f" corresp="#s7:l5">planten</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Het eeuwigh <rhyme label="d" type="m" corresp="#s7:l6">goet</rhyme>.</l>
        <l id="s7:l9" met="5" type="-+">Een kindeken is ons gheboren, Godt lof en <rhyme label="e" type="m">danck</rhyme>,</l>
        <l id="s7:l10" met="5" type="-+">Verheught u, uytverkoren,, Met spel en <rhyme label="e" type="m" corresp="#s7:l9">sanck</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
