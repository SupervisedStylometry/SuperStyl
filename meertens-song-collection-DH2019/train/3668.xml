    <melody>Psalm 065 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MEN loeft v Heer met stemmen <rhyme label="a" type="f">reyne</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Tot Zion <rhyme label="b" type="m">openbaer</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Die beloefde gaeuen <rhyme label="a" type="f" corresp="#s1:l1">ghemeyne</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Betaelt men v oock <rhyme label="b" type="m" corresp="#s1:l2">daer</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">End dewyl dat ghy daer wilt <rhyme label="c" type="f">hooren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">T'ghebedt uwes volcks <rhyme label="d" type="m">goedt</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">So sullen daer v' <rhyme label="c" type="f" corresp="#s1:l5">wtuerkoren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">V vallen Heer te <rhyme label="d" type="m" corresp="#s1:l6">voet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al myn misdaeden end myn <rhyme label="a" type="f">sonden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Waeren seer groot end <rhyme label="b" type="m">swaer</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maer v' goedtheit niet om <rhyme label="a" type="f" corresp="#s2:l1">doergronden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Vergaf die al <rhyme label="b" type="m" corresp="#s2:l2">voorwaer</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Wel hem dien ghy hebt <rhyme label="c" type="f">wtghelesen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat hy by v sij <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Die oock altyt by v magh <rhyme label="c" type="f" corresp="#s2:l5">wesen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Sonder te scheyden <rhyme label="d" type="m" corresp="#s2:l6">meer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wy sullen met d'heerlickheit <rhyme label="a" type="f">schoone</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vwes huys versaedt <rhyme label="b" type="m">sijn</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">T'rijckdom Heer van t'huys uwer <rhyme label="a" type="f" corresp="#s3:l1">woone</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Sal ons wel voeden <rhyme label="b" type="m" corresp="#s3:l2">fyn</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Ghy die nae v' goedtheit <rhyme label="c" type="f">verheuen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ons beschermt end <rhyme label="d" type="m">bewaert</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Sult ons een goed' antwoorde <rhyme label="c" type="f" corresp="#s3:l5">gheuen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Nae v' kracht seer <rhyme label="d" type="m" corresp="#s3:l6">vermaert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">T'gheheel ęerdtryck aen v Heer <rhyme label="a" type="f">hanget</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Met een vertrauwen <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Al die dat groote Męer <rhyme label="a" type="f" corresp="#s4:l1">omvanghet</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Staen vast op v <rhyme label="b" type="m" corresp="#s4:l2">alleyn</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Ghy maeckt doer v' schricklicke <rhyme label="c" type="f">krachten</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Seer vast die berghen <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Van sterckheit end van groote <rhyme label="c" type="f" corresp="#s4:l5">machten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Sijt ghy O Heer niet <rhyme label="d" type="m" corresp="#s4:l6">bloot</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy kondt oock dat brullen haest <rhyme label="a" type="f">stillen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Des Męers als 't is <rhyme label="b" type="m">beweeght</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die menschen oock die woeden <rhyme label="a" type="f" corresp="#s5:l1">willen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ghy haest te stillen <rhyme label="b" type="m" corresp="#s5:l2">pleeght</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Als die volckeren wel <rhyme label="c" type="f">bemercken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die omhęr sijn O <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Dees groote wonderlicke <rhyme label="c" type="f" corresp="#s5:l5">wercken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Se sijn verwondert <rhyme label="d" type="m" corresp="#s5:l6">seer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Heer van den Oosten tot den <rhyme label="a" type="f">Westen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Nae v' goedtheit end <rhyme label="b" type="m">Woort</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Laett ghy voortkommen ons ten <rhyme label="a" type="f" corresp="#s6:l1">besten</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Allerley blydtschap <rhyme label="b" type="m" corresp="#s6:l2">voort</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">So haest als droogh' is ons <rhyme label="c" type="f">ęerdtrycke</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ghy besoeckt dat <rhyme label="d" type="m">voorwaer</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Met daw end reghen <rhyme label="c" type="f" corresp="#s6:l5">desghelycke</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Maeckt ghy't ryck' end <rhyme label="d" type="m" corresp="#s6:l6">vruchtbaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">T'water springend' wt die <rhyme label="a" type="f">fonteynen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Maeckt dat ęerdtrycke <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Tot taerw' end ander vruchten <rhyme label="a" type="f" corresp="#s7:l1">reyne</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">In grooten <rhyme label="b" type="m" corresp="#s7:l2">ouervloedt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Die gheploeghde voren <rhyme label="c" type="f">bequaeme</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Watert ghy t'sijner <rhyme label="d" type="m">tydt</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Ghy breeckt d' ęerdtryck, end sijn vrucht <rhyme label="c" type="f" corresp="#s7:l5">t'saeme</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Ghy seer <rhyme label="d" type="m" corresp="#s7:l6">ghebenedydt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy verciert dat jaer <rhyme label="a" type="f">ouervloedigh</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Met v' goederen <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">End laett t'vette reghenen <rhyme label="a" type="f" corresp="#s8:l1">goedigh</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Bouen wt uwen <rhyme label="b" type="m" corresp="#s8:l2">throon</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Die hutten selfs in de <rhyme label="c" type="f">woestyne</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Sijn vol vruchten met <rhyme label="d" type="m">een</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Die berghen seer vruchtbaer van <rhyme label="c" type="f" corresp="#s8:l5">wyne</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Staen lustigh groot end <rhyme label="d" type="m" corresp="#s8:l6">kleen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dan siet men in beemden end <rhyme label="a" type="f">weyden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">T'vee in groot ghetal <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">T'veldt (van den berghen <rhyme label="a" type="f" corresp="#s9:l1">afghescheyden</rhyme>)</l>
        <l id="s9:l4" met="3" type="-+">Siet met vol koorne <rhyme label="b" type="m" corresp="#s9:l2">staen</rhyme>.</l>
        <l id="s9:l5" met="4" type="-+">Also schynt dat ęerdtryck' <rhyme label="c" type="f">t'ontspringhen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">End te verblyden <rhyme label="d" type="m">seer</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Dies moeten wy juychen end <rhyme label="c" type="f" corresp="#s9:l5">singhen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">In v, van herten <rhyme label="d" type="m" corresp="#s9:l6">Heer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
