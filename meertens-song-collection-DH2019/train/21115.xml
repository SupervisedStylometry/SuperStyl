<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21115</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">SChoone Nimfjes, die hier <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Maeckt <rhyme label="a" type="m" corresp="#s1:l1">jolijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Laet u soete stemme <rhyme label="b" type="f">klincken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Komt en schickt u elck een <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Zy aen <rhyme label="c" type="m" corresp="#s1:l4">zy</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Om te eeten en om <rhyme label="b" type="f" corresp="#s1:l3">drincken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Sie ons Bruydt, na 's landts <rhyme label="a" type="m">manier</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Is seer <rhyme label="a" type="m" corresp="#s2:l1">fier</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Geciert, in haer staet <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">En de Bruygom nevens <rhyme label="c" type="m">haer</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Is <rhyme label="c" type="m" corresp="#s2:l4">een-paer</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Vriendelijck tot haer <rhyme label="b" type="f" corresp="#s2:l3">genegen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Waerom soudt niet elck een <rhyme label="a" type="m">vreught</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">In der <rhyme label="a" type="m" corresp="#s3:l1">deught</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Vryelijck mogen <rhyme label="b" type="f">hanteeren</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Heeft niet Christus mee ter <rhyme label="c" type="m">stee</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">In <rhyme label="c" type="m" corresp="#s3:l4">Galile</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Eenen Bruyloft gaen <rhyme label="b" type="f" corresp="#s3:l3">vereeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Daer hy van het Water <rhyme label="a" type="m">Wijn</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Maeckte <rhyme label="a" type="m" corresp="#s4:l1">fijn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Willende daer mede <rhyme label="b" type="f">leeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat hy selfs het huw'lijck <rhyme label="c" type="m">jent</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s4:l4">Exellent</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Stichtet, en haet het <rhyme label="b" type="f" corresp="#s4:l3">Hoereren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hy die Adam eerstmael <rhyme label="a" type="m">schiep</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Een slaep <rhyme label="a" type="m" corresp="#s5:l1">diep</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Op hem sant, na Schrifts <rhyme label="b" type="f">vermonden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Heeft u Bruydegom ten <rhyme label="c" type="m">loon</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Tot een <rhyme label="c" type="m" corresp="#s5:l4">kroon</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">V dees Eva <rhyme label="b" type="f" corresp="#s5:l3">toe-gesonden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Geluck Prince, jonge <rhyme label="a" type="m">paer</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Met <rhyme label="a" type="m" corresp="#s6:l1">elckaer</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Vreede wil u Godt <rhyme label="b" type="f">toe-senden</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Daer toe Olijf-spruyten <rhyme label="c" type="m">reen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Achter <rhyme label="c" type="m" corresp="#s6:l4">een</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Vreede moet u leven <rhyme label="b" type="f" corresp="#s6:l3">enden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
