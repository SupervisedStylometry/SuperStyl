  <author>Jacobus de Ruyter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">LIefste Rosalinde, waerom weende <rhyme label="a" type="m">gy</rhyme>?</l>
        <l id="s1:l2" met="6" type="+-">Seght my wat is d' oorsaeck van u droef <rhyme label="a" type="m" corresp="#s1:l1">gheschrey</rhyme></l>
        <l id="s1:l3" met="6" type="+-">Ick sal u misschien haest komen maeken <rhyme label="a" type="m" corresp="#s1:l1">bly</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Waerom sucht <rhyme label="a" type="m" corresp="#s1:l1">ghy</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">waerom ducht <rhyme label="a" type="m" corresp="#s1:l1">ghy</rhyme></l>
        <l id="s1:l6" met="2" type="+-">Segghet <rhyme label="a" type="m" corresp="#s1:l1">my</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">Ah! Leander mijnen nood is al te <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s2:l2" met="6" type="+-">Ick beween mijn Vader en mijn moeders <rhyme label="a" type="m" corresp="#s2:l1">doot</rhyme></l>
        <l id="s2:l3" met="6" type="+-">Ick sit hier alleen, ick ween, ick sucht, ick <rhyme label="a" type="m" corresp="#s2:l1">krijt</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Vader, <rhyme label="a" type="m" corresp="#s2:l1">Moeder</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Suster, <rhyme label="a" type="m" corresp="#s2:l1">Broeder</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">'k Bent al <rhyme label="a" type="m" corresp="#s2:l1">quyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Soete Rosalinde, 't doet my selve <rhyme label="a" type="m">pijn</rhyme>,</l>
        <l id="s3:l2" met="6" type="+-">'k Wenschte dat in droefheyt wesen kost de <rhyme label="a" type="m" corresp="#s3:l1">mijn</rhyme></l>
        <l id="s3:l3" met="6" type="+-">Oft dat uw gesucht wiert onder ons <rhyme label="a" type="m" corresp="#s3:l1">gemeen</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Ick soud' <rhyme label="a" type="m" corresp="#s3:l1">helpen</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">ende <rhyme label="a" type="m" corresp="#s3:l1">stelpen</rhyme></l>
        <l id="s3:l6" met="2" type="+-">Uw' <rhyme label="a" type="m" corresp="#s3:l1">gheween</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">Daer en sal noyt van mijn weenen zijn <rhyme label="a" type="m">com-af</rhyme></l>
        <l id="s4:l2" met="6" type="+-">Voor dat ick sal rusten neffens hun in 't <rhyme label="a" type="m" corresp="#s4:l1">graf</rhyme>,</l>
        <l id="s4:l3" met="6" type="+-">Ach! mijn lieve moeder waer ick toch by <rhyme label="a" type="m" corresp="#s4:l1">u</rhyme></l>
        <l id="s4:l4" met="2" type="+-">'k Ben mijn <rhyme label="a" type="m" corresp="#s4:l1">leven</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">dat ick <rhyme label="a" type="m" corresp="#s4:l1">leve</rhyme></l>
        <l id="s4:l6" met="2" type="+-">Nu al <rhyme label="a" type="m" corresp="#s4:l1">mu</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="+-">Sus, sus, Rosalinde, sus en krijt niet <rhyme label="a" type="m">meer</rhyme>,</l>
        <l id="s5:l2" met="6" type="+-">Troost u met den wille van den grooten <rhyme label="a" type="m" corresp="#s5:l1">Heer</rhyme>,</l>
        <l id="s5:l3" met="6" type="+-">Peyst dat yder mensch die Godt het leven <rhyme label="a" type="m" corresp="#s5:l1">gaf</rhyme></l>
        <l id="s5:l4" met="2" type="+-">Eens moet <rhyme label="a" type="m" corresp="#s5:l1">sterven</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">en <rhyme label="a" type="m" corresp="#s5:l1">bederven</rhyme></l>
        <l id="s5:l6" met="2" type="+-">In het <rhyme label="a" type="m" corresp="#s5:l1">graf</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="+-">'k Danck u seer Leander dat gy in mijn <rhyme label="a" type="m">smert</rhyme></l>
        <l id="s6:l2" met="6" type="+-">Soekt te zijn den trooster van mijn droevig <rhyme label="a" type="m" corresp="#s6:l1">hert</rhyme>:</l>
        <l id="s6:l3" met="6" type="+-">Gy troost my met woorden, maer 'k en krijgh <rhyme label="a" type="m" corresp="#s6:l1">daerom</rhyme></l>
        <l id="s6:l4" met="2" type="+-">Mijn <rhyme label="a" type="m" corresp="#s6:l1">beminde</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">doode <rhyme label="a" type="m" corresp="#s6:l1">vrienden</rhyme></l>
        <l id="s6:l6" met="2" type="+-">Niet <rhyme label="a" type="m" corresp="#s6:l1">wed'rom</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="+-">Is 't dat mijnen trouwen dienst aen u <rhyme label="a" type="m">behaeght</rhyme></l>
        <l id="s7:l2" met="6" type="+-">Ick sal uwen Vader wesen soete <rhyme label="a" type="m" corresp="#s7:l1">maeght</rhyme>,</l>
        <l id="s7:l3" met="6" type="+-">Den man daer men d' ouders om verlaete <rhyme label="a" type="m" corresp="#s7:l1">moet</rhyme></l>
        <l id="s7:l4" met="2" type="+-">Is u <rhyme label="a" type="m" corresp="#s7:l1">naerder</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">als u <rhyme label="a" type="m" corresp="#s7:l1">Vader</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">En uw' <rhyme label="a" type="m" corresp="#s7:l1">Bloedt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="6" type="+-">Soetjens wat Leander meynt gy dat een <rhyme label="a" type="m">maeght</rhyme></l>
        <l id="s8:l2" met="6" type="+-">Soo terstont tot yeder weder-liefde <rhyme label="a" type="m" corresp="#s8:l1">draeght</rhyme>,</l>
        <l id="s8:l3" met="6" type="+-">Van de goede jaghers wort het wildt <rhyme label="a" type="m" corresp="#s8:l1">verfoeyt</rhyme>,</l>
        <l id="s8:l4" met="2" type="+-">Dat sigh <rhyme label="a" type="m" corresp="#s8:l1">vanghen</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">laet, en <rhyme label="a" type="m" corresp="#s8:l1">langhen</rhyme></l>
        <l id="s8:l6" met="2" type="+-">Sonder <rhyme label="a" type="m" corresp="#s8:l1">moeyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="6" type="+-">Gy weet Rosalinde dat mijn hert u <rhyme label="a" type="m">mindt</rhyme>,</l>
        <l id="s9:l2" met="6" type="+-">Meer als eenen Vader mint sijn eygen <rhyme label="a" type="m" corresp="#s9:l1">kint</rhyme>,</l>
        <l id="s9:l3" met="6" type="+-">Ick heb' u verkoren voor mijn weerde <rhyme label="a" type="m" corresp="#s9:l1">vrouw</rhyme>,</l>
        <l id="s9:l4" met="2" type="+-"><rhyme label="a" type="m" corresp="#s9:l1">Rosalinde</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">mijn <rhyme label="a" type="m" corresp="#s9:l1">beminde</rhyme></l>
        <l id="s9:l6" met="2" type="+-">Gheeft m' uw <rhyme label="a" type="m" corresp="#s9:l1">Trouw</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="6" type="+-">Gy kont sterck bewegen mijn jongh hert en <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s10:l2" met="6" type="+-">Ick voel my genegen tot u <rhyme label="a" type="m" corresp="#s10:l1">weder-min</rhyme></l>
        <l id="s10:l3" met="6" type="+-">Uw' bevalligh spreken is mijn <rhyme label="a" type="m" corresp="#s10:l1">Hertjens-dief</rhyme></l>
        <l id="s10:l4" met="2" type="+-">'k Kies <rhyme label="a" type="m" corresp="#s10:l1">Leander</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">en geen <rhyme label="a" type="m" corresp="#s10:l1">ander</rhyme></l>
        <l id="s10:l6" met="2" type="+-">Voor mijn <rhyme label="a" type="m" corresp="#s10:l1">Lief</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="6" type="+-">Liefste als gy my uw' Trouw zijt <rhyme label="a" type="m">gevende</rhyme></l>
        <l id="s11:l2" met="6" type="+-">Soo vergeet de doode om de <rhyme label="a" type="m" corresp="#s11:l1">levende</rhyme></l>
        <l id="s11:l3" met="6" type="+-">Bidt Godt voor hun ziel; maer en weent niet <rhyme label="a" type="m" corresp="#s11:l1">meer</rhyme></l>
        <l id="s11:l4" met="2" type="+-">Als ghy meught <rhyme label="a" type="m" corresp="#s11:l1">nu</rhyme>;</l>
        <l id="s11:l5" met="2" type="+-">soo verheught <rhyme label="a" type="m" corresp="#s11:l1">u</rhyme></l>
        <l id="s11:l6" met="2" type="+-">In den <rhyme label="a" type="m" corresp="#s11:l1">Heer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="6" type="+-">Meer als eenigh kint sijn Ouders minnen <rhyme label="a" type="m">kan</rhyme></l>
        <l id="s12:l2" met="6" type="+-">Sal ick u beminnen mijnen lieven <rhyme label="a" type="m" corresp="#s12:l1">Man</rhyme>,</l>
        <l id="s12:l3" met="6" type="+-">Wy zijn Bruydt en Bruygom; wy zijn man en <rhyme label="a" type="m" corresp="#s12:l1">wijf</rhyme>;</l>
        <l id="s12:l4" met="2" type="+-">Wy-lie <rhyme label="a" type="m" corresp="#s12:l1">krielen</rhyme></l>
        <l id="s12:l5" met="2" type="+-">met twee <rhyme label="a" type="m" corresp="#s12:l1">zielen</rhyme>,</l>
        <l id="s12:l6" met="2" type="+-">In een <rhyme label="a" type="m" corresp="#s12:l1">lijf</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="6" type="+-">Eer de thiende maendt sal sien de <rhyme label="a" type="m">Sonne-schijn</rhyme></l>
        <l id="s13:l2" met="6" type="+-">Sult gy hoop ick moeder en ick vader <rhyme label="a" type="m" corresp="#s13:l1">zijn</rhyme>,</l>
        <l id="s13:l3" met="6" type="+-">En soo zult gy weder krijgen van den <rhyme label="a" type="m" corresp="#s13:l1">Heer</rhyme>,</l>
        <l id="s13:l4" met="2" type="+-">Vader, <rhyme label="a" type="m" corresp="#s13:l1">Moeder</rhyme>,</l>
        <l id="s13:l5" met="2" type="+-">Suster, <rhyme label="a" type="m" corresp="#s13:l1">Broeder</rhyme>,</l>
        <l id="s13:l6" met="2" type="+-">Wenscht ghy <rhyme label="a" type="m" corresp="#s13:l1">meer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="6" type="+-">Door de lieffelijcke woorden soo vol <rhyme label="a" type="m">geest</rhyme></l>
        <l id="s14:l2" met="6" type="+-">Keert mijn droevig sterf-huys in een <rhyme label="a" type="m" corresp="#s14:l1">bruyloffeest</rhyme></l>
        <l id="s14:l3" met="6" type="+-">Door den soeten troost die gy mijn hert <rhyme label="a" type="m" corresp="#s14:l1">aendoet</rhyme></l>
        <l id="s14:l4" met="2" type="+-">Keert mijn <rhyme label="a" type="m" corresp="#s14:l1">Lijden</rhyme>,</l>
        <l id="s14:l5" met="2" type="+-">in <rhyme label="a" type="m" corresp="#s14:l1">verblijden</rhyme></l>
        <l id="s14:l6" met="2" type="+-">'t Seur in't 't <rhyme label="a" type="m" corresp="#s14:l1">soet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
