  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WIe sou dencken <rhyme label="a" type="m">Amarant</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Dat u Oog myn koele <rhyme label="b" type="f">Zinnen</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Die noyt voelde vonck noch <rhyme label="a" type="m" corresp="#s1:l1">brant</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Soo sou brengen stracx aan't <rhyme label="b" type="f" corresp="#s1:l2">Minnen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ach Ach Nimph! Ick ben <rhyme label="c" type="m">gevat</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Laatme los, of troost me <rhyme label="c" type="m" corresp="#s1:l5">wat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Daar is nu myn Vryheyt <rhyme label="a" type="m">quyt</rhyme>!</l>
        <l id="s2:l2" met="4" type="+-">Daar is nu mijn Hert in <rhyme label="b" type="f">banden</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">En mits ghy de Bindster <rhyme label="a" type="m" corresp="#s2:l1">zijt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Raack ick licht wel noyt uyt <rhyme label="b" type="f" corresp="#s2:l2">konden</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Ach Ach Nimph! Ick ben <rhyme label="c" type="m">gevat</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Laatme los, of troost me <rhyme label="c" type="m" corresp="#s2:l5">wat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doch de banden zyn te <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Om weer los te willen <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Schoon myn Zinnen en <rhyme label="a" type="m" corresp="#s3:l1">Gemoet</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Alle Min wel eer <rhyme label="b" type="f" corresp="#s3:l2">mispresen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Soo dan nu ick ben <rhyme label="c" type="m">gevat</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Laat niet los maar troostme <rhyme label="c" type="m" corresp="#s3:l5">wat</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
