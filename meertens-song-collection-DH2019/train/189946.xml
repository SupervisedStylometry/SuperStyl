  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Lang moet u luyster <rhyme label="a" type="f">groeijen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Milde Baggus, bly <rhyme label="b" type="m">gemoed</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Lang moet uwe wijnstok <rhyme label="a" type="f" corresp="#s1:l1">bloeijen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Daar gy 't edel druyven <rhyme label="b" type="m" corresp="#s1:l2">bloed</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Soo welsmakend uit doet <rhyme label="a" type="f" corresp="#s1:l1">groeijen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Dat het yders wensch <rhyme label="b" type="m" corresp="#s1:l2">voldoet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Lang moet yder u <rhyme label="a" type="f">beminnen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Als verdryver van de <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als verheuger van de <rhyme label="a" type="f" corresp="#s2:l1">sinnen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Als verkwikker van het <rhyme label="b" type="m" corresp="#s2:l2">hert</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Altoos moet uw roem <rhyme label="a" type="f" corresp="#s2:l1">beginnen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Die noyt regt volpresen <rhyme label="b" type="m" corresp="#s2:l2">wert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want wie kan uw waarde <rhyme label="a" type="f">gaven</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Immer roemen soo 't <rhyme label="b" type="m">behoord</rhyme></l>
        <l id="s3:l3" met="4" type="+-">- Die soo vele borsten <rhyme label="a" type="f" corresp="#s3:l1">laven</rhyme> -</l>
        <l id="s3:l4" met="4" type="+-">Die uw schoonheyd meer <rhyme label="b" type="m" corresp="#s3:l2">bekoord</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Als de laffe minne <rhyme label="a" type="f" corresp="#s3:l1">slaven</rhyme></l>
        <l id="s3:l6" met="4" type="+-">'t Schoon dat haar hert <rhyme label="b" type="m" corresp="#s3:l2">doorboord</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Niets gaat uwe lof te <rhyme label="a" type="f">boven</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Nimmer oude <rhyme label="b" type="m">EvoÃ«</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Niets moet uwe luyster <rhyme label="a" type="f" corresp="#s4:l1">doven</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Ag, verhoord slegts dese <rhyme label="b" type="m" corresp="#s4:l2">bee</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Wild ons noit u stam <rhyme label="a" type="f" corresp="#s4:l1">ontroven</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Die ons vryd' van harte <rhyme label="b" type="m" corresp="#s4:l2">wee</rhyme>;</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Op dat wy u roem <rhyme label="a" type="f">verbreiden</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Dat het yder klinkt in 't <rhyme label="b" type="m">oor</rhyme></l>
        <l id="s5:l3" met="4" type="+-">En ons met uw stoet <rhyme label="a" type="f" corresp="#s5:l1">verblyden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ja, steeds volgen op uw <rhyme label="b" type="m" corresp="#s5:l2">spoor</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Wel, wy sullen ons <rhyme label="a" type="f" corresp="#s5:l1">bereiden</rhyme></l>
        <l id="s5:l6" met="4" type="+-">En gaan met dit glaasje <rhyme label="b" type="m" corresp="#s5:l2">voor</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
