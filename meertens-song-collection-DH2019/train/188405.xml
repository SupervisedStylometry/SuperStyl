    <tags/>
    <melody>Vrolijke uurtjes</melody>
  <author>Antoine Mahaut, Kornelis Elzevier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Vroly-ke <rhyme label="a" type="f">Uurtjes</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Die 'k zoo <rhyme label="a" type="f" corresp="#s1:l1">duurtjes</rhyme></l>
        <l id="s1:l3" met="4" type="+-">U, myn Engelin <rhyme label="b" type="m">betael</rhyme>!</l>
        <l id="s1:l4" met="4" type="+-">Ro-ze-mond, stookaende <rhyme label="a" type="f" corresp="#s1:l1">vuurtjes</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'k Bid volvoer myn <rhyme label="b" type="m" corresp="#s1:l3">Zegeprael</rhyme>:</l>
        <l id="s1:l6" met="2" type="+-">Voed myn <rhyme label="c" type="f">lusjes</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Door uw <rhyme label="c" type="f" corresp="#s1:l6">Kusjes</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Vrolyk meisje, 'k bid, houd <rhyme label="d" type="m">aen</rhyme>;</l>
        <l id="s1:l9" met="4" type="+-">Roze mond, wacht niet tot <rhyme label="c" type="f" corresp="#s1:l6">flusjes</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">'k c'isch terstond te zyn <rhyme label="d" type="m" corresp="#s1:l8">voldaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">'t Groen der <rhyme label="a" type="f">Boomen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Aen dees <rhyme label="a" type="f" corresp="#s2:l1">Stroomen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Maekt, dat niemand ons <rhyme label="b" type="m">bespiedt</rhyme>;</l>
        <l id="s2:l4" met="4" type="+-">Wil toch haestig herwaerd <rhyme label="a" type="f" corresp="#s2:l1">komen</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Nimf, weerhoud uw voetjes <rhyme label="b" type="m" corresp="#s2:l3">niet</rhyme>:</l>
        <l id="s2:l6" met="2" type="+-">'k Zal de <rhyme label="c" type="f">tipjes</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Van uw <rhyme label="c" type="f" corresp="#s2:l6">Lipjes</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Zachter drukken dan <rhyme label="d" type="m">voorheen</rhyme>;</l>
        <l id="s2:l9" met="4" type="+-">Rozemond, dat zyn de <rhyme label="c" type="f" corresp="#s2:l6">klipjes</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Waer ik Schipbreuk heb <rhyme label="d" type="m" corresp="#s2:l8">geleen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
