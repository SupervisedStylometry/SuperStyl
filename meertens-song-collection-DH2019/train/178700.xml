    <tags/>
    <melody>Psalm 096 Datheen</melody>
  <author>François Halma</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ZIngt Gode niewe <rhyme label="a" type="f">feestgezangen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Laat d'aarde en lucht uw toonen <rhyme label="a" type="f" corresp="#s1:l1">vangen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">O alle volken, zingt den <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Looft zynen naame en hoogheits <rhyme label="b" type="m" corresp="#s1:l3">eer</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Vermeldt zyn heil, dat wy <rhyme label="a" type="f" corresp="#s1:l1">erlangen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vertelt van dag tot dag zyn <rhyme label="a" type="f">eere</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">Meldt by de Heidenen zyn <rhyme label="a" type="f" corresp="#s2:l1">leere</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En wonderen, want onze <rhyme label="b" type="m">Godt</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Is groot, en zeer te pryzen, <rhyme label="b" type="m" corresp="#s2:l3">tot</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Het stargewelf, als <rhyme label="a" type="f" corresp="#s2:l1">Opperheere</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Heer is, boven alle <rhyme label="a" type="f">Goden</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Heel vreesselyk, een Godt in <rhyme label="a" type="f" corresp="#s3:l1">nooden</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Want Goden van het <rhyme label="b" type="m">Heidendom</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Zyn afgoôn, magteloos en <rhyme label="b" type="m" corresp="#s3:l3">stom</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Maar Godt heeft 's hemels stant <rhyme label="a" type="f" corresp="#s3:l1">geboden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zyn Majesteit, en hoog <rhyme label="a" type="f">gerichte</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En eer, zyn voor zyn <rhyme label="a" type="f" corresp="#s4:l1">aangezichte</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">In 't heiligdom sieraat en <rhyme label="b" type="m">kracht</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Godts eere en sterkte zy <rhyme label="b" type="m" corresp="#s4:l3">gedacht</rhyme></l>
        <l id="s4:l5" met="4" type="-+">By alle volken, naar 't <rhyme label="a" type="f" corresp="#s4:l1">gewigte</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Geeft Gode d'eer van zynen <rhyme label="a" type="f">naame</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Brengt offerhanden, als 't <rhyme label="a" type="f" corresp="#s5:l1">betaame</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En komt in zyne hoven, <rhyme label="b" type="m">daar</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Hy wordt gedient in 't <rhyme label="b" type="m" corresp="#s5:l3">openbaar</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Opdat het heidendom zich <rhyme label="a" type="f" corresp="#s5:l1">schaame</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Aanbidt den Heer der <rhyme label="a" type="f">heireschaaren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Daar hy zyn eer komt <rhyme label="a" type="f" corresp="#s6:l1">openbaaren</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En heerlykheit, in 't <rhyme label="b" type="m">heiligdom</rhyme>;</l>
        <l id="s6:l4" met="4" type="-+">Schrik voor zyn aangezicht <rhyme label="b" type="m" corresp="#s6:l3">rontom</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Gy aarde, en wil zyn' lof <rhyme label="a" type="f" corresp="#s6:l1">verklaaren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Zingt onder 't heidendom, Godts <rhyme label="a" type="f">ryke</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Bestaat, en heeft nooit zyns <rhyme label="a" type="f" corresp="#s7:l1">gelyke</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Ook wordt de wereldt <rhyme label="b" type="m">vastgestelt</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Zoodat ze heen noch weder <rhyme label="b" type="m" corresp="#s7:l3">helt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Opdat zyn recht den volken <rhyme label="a" type="f" corresp="#s7:l1">blyke</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Dat zich de hemelen <rhyme label="a" type="f">verblyden</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">En d'aarde zinge aan alle <rhyme label="a" type="f" corresp="#s8:l1">zyden</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">De zee bruische in haar volheit <rhyme label="b" type="m">op</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">'t Veldt hupple tot der bergen <rhyme label="b" type="m" corresp="#s8:l3">top</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">'t Geboomte juiche in deeze <rhyme label="a" type="f" corresp="#s8:l1">tyden</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Voor 't aangezicht van 's hemels <rhyme label="a" type="f">Heere</rhyme>:</l>
        <l id="s9:l2" met="4" type="-+">Want hy, bestuuwt met glans en <rhyme label="a" type="f" corresp="#s9:l1">eere</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Komt opdat hy de wereldt <rhyme label="b" type="m">richt</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">En hy de volken door het <rhyme label="b" type="m" corresp="#s9:l3">licht</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Van waarheit en zyn recht <rhyme label="a" type="f" corresp="#s9:l1">regeere</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
