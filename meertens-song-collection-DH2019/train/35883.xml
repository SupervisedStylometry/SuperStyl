  <author>Johanna de Gavre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ick ben een arm <rhyme label="a" type="m">creatuer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Sulcks wordt ick <rhyme label="b" type="f">bevonden</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Nu ick ben <rhyme label="b" type="f" corresp="#s1:l2">ghebonden</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Aem mijn Lichaem <rhyme label="a" type="m" corresp="#s1:l1">stuer</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Ick moet in d’aerde <rhyme label="c" type="f">woelen</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Teghen mijnen <rhyme label="d" type="m">danck</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">In vuyl modder <rhyme label="c" type="f" corresp="#s1:l5">poelen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Daer het is vol <rhyme label="d" type="m" corresp="#s1:l6">stanck</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Dat is mijn Lichaem <rhyme label="d" type="m" corresp="#s1:l6">kranck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick moet dat zijn heel <rhyme label="a" type="m">onderdaen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">K’moet daer voor sorghe <rhyme label="b" type="f">draghen</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Nacht ende <rhyme label="b" type="f" corresp="#s2:l2">daghen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">T’sijnen dienst al <rhyme label="a" type="m" corresp="#s2:l1">staen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Avont ende <rhyme label="c" type="f">morghen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Doen al sijn <rhyme label="d" type="m">gherief</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Oock den kost <rhyme label="c" type="f" corresp="#s2:l5">besorghen</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Oft het waer mijn <rhyme label="d" type="m" corresp="#s2:l6">Lief</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">En k’achtet voor een <rhyme label="d" type="m" corresp="#s2:l6">dief</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">T’is dickwils noch <rhyme label="a" type="m">heel</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">T’doet dickwils niet dan <rhyme label="b" type="f">kreunen</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Krochghen ende <rhyme label="b" type="f" corresp="#s3:l2">steunen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Teghen my op <rhyme label="a" type="m" corresp="#s3:l1">staen</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Wie sou dan willen <rhyme label="c" type="f">wenschen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Naer dit aerden <rhyme label="d" type="m">vat</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">O verblinde <rhyme label="c" type="f" corresp="#s3:l5">Menschen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Dat ghy’t niet en <rhyme label="d" type="m" corresp="#s3:l6">vat</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Waer dat is inden <rhyme label="d" type="m" corresp="#s3:l6">schat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat ick op heel de aerde <rhyme label="a" type="m">sien</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">T’is teghen mijn <rhyme label="b" type="f">ghenoeghen</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">K’en kan my niet <rhyme label="b" type="f" corresp="#s4:l2">voeghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Om dat jonst te <rhyme label="a" type="m" corresp="#s4:l1">bien</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Het staet my al <rhyme label="c" type="f">teghen</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Wat ick oyt <rhyme label="d" type="m">aensagh</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Oock s’wereldts rijcken <rhyme label="c" type="f" corresp="#s4:l5">seghen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Doen ick mijn <rhyme label="d" type="m" corresp="#s4:l6">beklagh</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Roep wat ick roepen <rhyme label="d" type="m" corresp="#s4:l6">magh</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wie sal my gheven <rhyme label="a" type="m">goedertier</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De Vleughelen der <rhyme label="b" type="f">Duyven</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Om daer me te <rhyme label="b" type="f" corresp="#s5:l2">schuyven</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Tot mijn groot <rhyme label="a" type="m" corresp="#s5:l1">plaisier</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Naer s’Hemels <rhyme label="c" type="f">contreyen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Daer men noydt en <rhyme label="d" type="m">sucht</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Maer altijdt sal <rhyme label="c" type="f" corresp="#s5:l5">verblijen</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Met een soet <rhyme label="d" type="m" corresp="#s5:l6">ghenucht</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Daer om neem ick de <rhyme label="d" type="m" corresp="#s5:l6">vlucht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">In de vijf wondekens der <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Daer t’altijt staet in’t <rhyme label="b" type="f">bloyen</rhyme>,</l>
        <l id="s6:l3" met="2" type="+-">Van het honingh <rhyme label="b" type="f" corresp="#s6:l2">vloyen</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">K’wil daer woonen <rhyme label="a" type="m" corresp="#s6:l1">in</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">K’wil my daer in <rhyme label="c" type="f">versaeden</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Naer mijn vollen <rhyme label="d" type="m">rust</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Slaen ghedurigh <rhyme label="c" type="f" corresp="#s6:l5">gaden</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">Waer mijn Ziel naer <rhyme label="d" type="m" corresp="#s6:l6">lust</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Tot dat sy is <rhyme label="d" type="m" corresp="#s6:l6">gherust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
