      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
      <tag standard-name="vermaanlied" translation="admonitory song">vermaanlied</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Jacobz. Soetenboom</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GHy die desen staet <rhyme label="a" type="m">aenveert</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">En <rhyme label="a" type="m" corresp="#s1:l1">begeert</rhyme></l>
        <l id="s1:l3" met="4" type="+-">'t Aertsche leven ons te <rhyme label="b" type="f">enden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wilt in't bitter ofte <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Wat ghy <rhyme label="c" type="m" corresp="#s1:l4">doet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Tot den Heer u altijdt <rhyme label="b" type="f" corresp="#s1:l3">wenden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Alles wat u dan <rhyme label="a" type="m">geschiet</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">'t Is al <rhyme label="a" type="m" corresp="#s2:l1">niet</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Alles kont ghy <rhyme label="b" type="f">overwinnen</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Ja in 't midden van den <rhyme label="c" type="m">druck</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Sal 't <rhyme label="c" type="m" corresp="#s2:l4">geluck</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Eerst ten rechten eens <rhyme label="b" type="f" corresp="#s2:l3">beginnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Alles wat ghy met <rhyme label="a" type="m">verstandt</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Neemt ter <rhyme label="a" type="m" corresp="#s3:l1">handt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">'t Sal u altijdt wel <rhyme label="b" type="f">gedyen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Godt sal door zijn wijsen <rhyme label="c" type="m">raedt</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Van het <rhyme label="c" type="m" corresp="#s3:l4">quaet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Vwe leven wel <rhyme label="b" type="f" corresp="#s3:l3">bevryen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hy sal sorge dragen <rhyme label="a" type="m">voort</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Wat 'er <rhyme label="a" type="m" corresp="#s4:l1">hoort</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Godt sal uwe stam <rhyme label="b" type="f">vermeeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En verleenen kracht en <rhyme label="c" type="m">deught</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Dat ghy <rhyme label="c" type="m" corresp="#s4:l4">meught</rhyme></l>
        <l id="s4:l6" met="4" type="+-">V bequamelijck <rhyme label="b" type="f" corresp="#s4:l3">geneeren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Vwe kind'ren boven <rhyme label="a" type="m">dien</rhyme></l>
        <l id="s5:l2" met="2" type="+-">Ghy sult <rhyme label="a" type="m" corresp="#s5:l1">sien</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Vwe wesen <rhyme label="b" type="f">in-gegraven</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En vereeren seer <rhyme label="c" type="m">bequaem</rhyme></l>
        <l id="s5:l5" met="2" type="+-">Vwen <rhyme label="c" type="m" corresp="#s5:l4">naem</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Met veel prijsselijcke <rhyme label="b" type="f" corresp="#s5:l3">gaven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">En wanneer ghy desen <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s6:l2" met="2" type="+-">Al <rhyme label="a" type="m" corresp="#s6:l1">verlaet</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Als ghy 't hier al gaet <rhyme label="b" type="f">begeven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Sult ghy noch tot uwer <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Meer en <rhyme label="c" type="m" corresp="#s6:l4">meer</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">In u kind'ren altijdt <rhyme label="b" type="f" corresp="#s6:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Wel dan Bruydegom en <rhyme label="a" type="m">Bruyt</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Tot <rhyme label="a" type="m" corresp="#s7:l1">besluyt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Wil den Hemel u <rhyme label="b" type="f">verleenen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dat ghy nu te samen <rhyme label="c" type="m">meught</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">In der <rhyme label="c" type="m" corresp="#s7:l4">deught</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Tot u saligheydt <rhyme label="b" type="f" corresp="#s7:l3">vereenen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
