<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35338</id>
    <date>
      <year-start>1712</year-start>
      <year-end>1712</year-end>
    </date>
    <location>
      <orginal-name>duinkerken</orginal-name>
      <standard-name>Duinkerken, France</standard-name>
      <coordinates>
        <lon>51.1907405</lon>
        <lat>2.4978867</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hoor Kees mijn vrijer</melody>
  <author>Jacobus de Ruyter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">MYn <rhyme label="a" type="f">uyt-verkooren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Mijn Engelinne <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Voor my <rhyme label="a" type="f" corresp="#s1:l1">gebooren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Weest eens van my <rhyme label="b" type="m" corresp="#s1:l2">gegroet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ã´ schoon en silver <rhyme label="c" type="m">maen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Hey laet ons trouwen <rhyme label="c" type="m" corresp="#s1:l5">gaen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ick ben tot u <rhyme label="d" type="f">genegen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">'K zal u altydt <rhyme label="c" type="m" corresp="#s1:l5">by-staen</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">In al uw' <rhyme label="d" type="f" corresp="#s1:l7">wegen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wie doet sijn <rhyme label="a" type="f">klachten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Al weenen voor myn <rhyme label="b" type="m">deur</rhyme>?</l>
        <l id="s2:l3" met="2" type="-+">'K wil die <rhyme label="a" type="f" corresp="#s2:l1">verachten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Daerom vertreckt <rhyme label="b" type="m" corresp="#s2:l2">Monsieur</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">'K en wil my tot de <rhyme label="c" type="m">Trouw</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Noch niet besteden <rhyme label="c" type="m" corresp="#s2:l5">nouw</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Daerom laet my met <rhyme label="d" type="f">vreden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Tracht naer een ander <rhyme label="c" type="m" corresp="#s2:l5">vrouw</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Door uw' <rhyme label="d" type="f" corresp="#s2:l7">gebeden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Myn <rhyme label="a" type="f">Engelinne</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Myn troost en <rhyme label="b" type="m">toeverlaet</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Die ick <rhyme label="a" type="f" corresp="#s3:l1">beminne</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Myn reden niet <rhyme label="b" type="m" corresp="#s3:l2">versmaedt</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Opent uw' deur' <rhyme label="c" type="m">terstondt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Laet m' aen uw' rooden <rhyme label="c" type="m" corresp="#s3:l5">mondt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Den kus van vrede <rhyme label="d" type="f">geven</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Maeckt met my een <rhyme label="c" type="m" corresp="#s3:l5">verbondt</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Om t' saem te <rhyme label="d" type="f" corresp="#s3:l7">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">'T is maer <rhyme label="a" type="f">flatatie</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Minnaer uw' woorden <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">G'en wint geen <rhyme label="a" type="f" corresp="#s4:l1">gratie</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Vertreckt van hier met <rhyme label="b" type="m" corresp="#s4:l2">spoedt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Laet my heden met <rhyme label="c" type="m">rust</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Uw' minne-voncken <rhyme label="c" type="m" corresp="#s4:l5">blust</rhyme></l>
        <l id="s4:l7" met="3" type="-+">By een der <rhyme label="d" type="f">Venus-dieren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die vol onkuyssche <rhyme label="c" type="m" corresp="#s4:l5">lust</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">G'heel nachten <rhyme label="d" type="f" corresp="#s4:l7">swieren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Door uw' straf <rhyme label="a" type="f">spreken</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Lief gy door-wondt mijn <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Wilt u niet <rhyme label="a" type="f" corresp="#s5:l1">wreken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maer helpt my uyt de <rhyme label="b" type="m" corresp="#s5:l2">smert</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Uw' borstiens <rhyme label="c" type="m">appel-rondt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daer by uw' rooden <rhyme label="c" type="m" corresp="#s5:l5">mondt</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Uw' wit yvoire <rhyme label="d" type="f">tanden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Die doen myn hert' <rhyme label="c" type="m" corresp="#s5:l5">terstondt</rhyme></l>
        <l id="s5:l9" met="2" type="-+">Door liefde <rhyme label="d" type="f" corresp="#s5:l7">branden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Houdt op van <rhyme label="a" type="f">klaegen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">'T doet my te groote <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">'K wil u <rhyme label="a" type="f" corresp="#s6:l1">op-draegen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Het hert' en trouw' van <rhyme label="b" type="m" corresp="#s6:l2">myn</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Door uw' <rhyme label="c" type="m">bevalligheydt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">'K ben tot uw' dienst <rhyme label="c" type="m" corresp="#s6:l5">bereydt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Ick wil my gaen <rhyme label="d" type="f">begeven</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Om tot de doodt ons <rhyme label="c" type="m" corresp="#s6:l5">scheydt</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">Met u te <rhyme label="d" type="f" corresp="#s6:l7">leven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wat groote <rhyme label="a" type="f">vreughden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Geniet ick door de <rhyme label="b" type="m">Trouw'</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Mijn Lief vol <rhyme label="a" type="f" corresp="#s7:l1">deughden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die my helpt uyt den <rhyme label="b" type="m" corresp="#s7:l2">rouw'</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Komt hier myn Engel <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Ontfanght een kus met <rhyme label="c" type="m" corresp="#s7:l5">spoedt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Op beyd' uw' roode <rhyme label="d" type="f">wangen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Wilt desen rinck voor <rhyme label="c" type="m" corresp="#s7:l5">goedt</rhyme>,</l>
        <l id="s7:l9" met="2" type="-+">Van my <rhyme label="d" type="f" corresp="#s7:l7">ontfangen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Laet Bassen <rhyme label="a" type="f">roncken</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">en speelt op veel en <rhyme label="b" type="m">fluyt</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Eens braef <rhyme label="a" type="f" corresp="#s8:l1">gedroncken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">'T soete kindt is de <rhyme label="b" type="m" corresp="#s8:l2">Bruydt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Parnassus Maeghden <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Komt hier in groot <rhyme label="c" type="m" corresp="#s8:l5">getal</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Danst t' saemen in een <rhyme label="d" type="f">ronde</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Dit jonge Paer dat <rhyme label="c" type="m" corresp="#s8:l5">zal</rhyme></l>
        <l id="s8:l9" met="2" type="-+">Oock vreught <rhyme label="d" type="f" corresp="#s8:l7">verkonden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
