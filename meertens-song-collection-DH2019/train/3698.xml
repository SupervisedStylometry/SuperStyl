    <melody>Psalm 024 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Pieter Datheen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">KOMT laet ons blyd' sijn in den <rhyme label="a" type="m">Heer</rhyme>:</l>
        <l id="s1:l2" met="4" type="-+">End met sangh' verbreyden sijn <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hy is ons troost end heyl <rhyme label="b" type="f">alleyne</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Laet ons met dancksegginghe <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Voor sijn aengesicht, ende voort <rhyme label="c" type="m" corresp="#s1:l4">aen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Hem singhen met vrueght psalmen <rhyme label="b" type="f" corresp="#s1:l3">reyne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want hy een groot Godt is <rhyme label="a" type="m">gheacht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Een Koningh van veel grooter <rhyme label="a" type="m" corresp="#s2:l1">macht</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dan die Goden sijn: want <rhyme label="b" type="f">t'ęerdtrycke</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Heeft hy in sijn handt, welcks vrucht <rhyme label="c" type="m">al</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Hem toebehoort, die bergh end <rhyme label="c" type="m" corresp="#s2:l4">dal</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Steedts voortbringhen beyde <rhyme label="b" type="f" corresp="#s2:l3">ghelycke</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die Zee hem alleyn <rhyme label="a" type="m">toebehoort</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hy heeft die ghemaeckt doer sijn <rhyme label="a" type="m" corresp="#s3:l1">Woort</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End schiep oock t'ęerdtrycke <rhyme label="b" type="f">bequaeme</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Komt laet ons al t'saem met <rhyme label="c" type="m">demoet</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Vallen voor onsen Godt te <rhyme label="c" type="m" corresp="#s3:l4">voet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Hy heeft ons gheschaepen al <rhyme label="b" type="f" corresp="#s3:l3">t'saeme</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy is een Godt die ons <rhyme label="a" type="m">behoedt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Wy sijn t'volck end die schaepen <rhyme label="a" type="m" corresp="#s4:l1">goet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">die van hem wel gheweydet <rhyme label="b" type="f">werden</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Hoort ghy hedent sijn stemme <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Wilt doch uwe herten so <rhyme label="c" type="m" corresp="#s4:l4">gaer</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Niet verstocken noch oock <rhyme label="b" type="f" corresp="#s4:l3">verherden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">So tot Massa end <rhyme label="a" type="m">Meriba</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Vwe Vaders voor ende <rhyme label="a" type="m" corresp="#s5:l1">na</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">In de woestyne te doen <rhyme label="b" type="f">plaeghen</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Se terghden my met herten <rhyme label="c" type="m">quaet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ende beproefden vroegh end <rhyme label="c" type="m" corresp="#s5:l4">spaedt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">All' myn wercken die sy daer <rhyme label="b" type="f" corresp="#s5:l3">saeghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Daer ick veertigh jaeren <rhyme label="a" type="m">eenpaer</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Met hen veel aerbeydts hadd' <rhyme label="a" type="m" corresp="#s6:l1">voorwaer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dies klaeghd' ick aldus <rhyme label="b" type="f">onuersweghen</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Dit volck is gantsch sonder <rhyme label="c" type="m">verstandt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Van herten gaer dwaelende, <rhyme label="c" type="m" corresp="#s6:l4">want</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Se willen niet leeren myn <rhyme label="b" type="f" corresp="#s6:l3">weghen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dies als ick van hen wierdt <rhyme label="a" type="m">versmaet</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Ick was toornigh doer sulck een <rhyme label="a" type="m" corresp="#s7:l1">daet</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">End hebb' in myn gramschap <rhyme label="b" type="f">ghesworen</rhyme>:</l>
        <l id="s7:l4" met="4" type="-+">Dat dit boos end verkeert <rhyme label="c" type="m">gheslacht'</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Nimmermeer soude sijn <rhyme label="c" type="m" corresp="#s7:l4">ghebracht</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Ter ruste myner <rhyme label="b" type="f" corresp="#s7:l3">wtuerkoren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
