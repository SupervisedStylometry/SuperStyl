    <melody>Al deze dingen waardig</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Willem de Swaen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Eelste Vrouw der <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Maria, weest <rhyme label="b" type="m">gegroet</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Door U wordt <rhyme label="a" type="f" corresp="#s1:l1">ne'ergehouwen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Het nijdigh Helsch' <rhyme label="b" type="m" corresp="#s1:l2">gebroed</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Ghy graty hebt <rhyme label="c" type="f">ge-von-den</rhyme></l>
        <l id="s1:l6" met="3" type="-+">By God, die on-se <rhyme label="c" type="f" corresp="#s1:l5">sonden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Neemt wegh, en all' ons' <rhyme label="d" type="m">schuld</rhyme>;</l>
        <l id="s1:l8" met="4" type="+-">God U heeft met deughd <rhyme label="d" type="m" corresp="#s1:l7">ver-vult</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den Opper-Heer der <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">,, Is met U, reyne <rhyme label="b" type="m">Maeght</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Den Heer wil U <rhyme label="a" type="f" corresp="#s2:l1">vereeren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy wil dat Ghy Hem <rhyme label="b" type="m" corresp="#s2:l2">draeght</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Gebenedijdt sult Gh' <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Meer als oyt Vrouw voor <rhyme label="c" type="f" corresp="#s2:l5">desen</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Ghy zijt <rhyme label="d" type="m">gebenedijdt</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Moeder Goods, ten allen <rhyme label="d" type="m" corresp="#s2:l7">tijd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Uw' Vrucht, die alle <rhyme label="a" type="f">vruchten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Brenght voort op dese <rhyme label="b" type="m">aerd'</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die doet de Duyvels <rhyme label="a" type="f" corresp="#s3:l1">vluchten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Hy alle eer is <rhyme label="b" type="m" corresp="#s3:l2">waerd</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Uw' Vrucht is <rhyme label="c" type="f">uytgelesen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En noyt ghenoegh <rhyme label="c" type="f" corresp="#s3:l5">volpresen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Hem wordt in 's Hemels <rhyme label="d" type="m">Hof</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Staeghs gegeven groote <rhyme label="d" type="m" corresp="#s3:l7">lof</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Maria! Godes <rhyme label="a" type="f">Moeder</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">Gestadigh voor ons <rhyme label="b" type="m">bidt</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Ey! bidt doch ons' <rhyme label="a" type="f" corresp="#s4:l1">Behoeder</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Beneffen wien Ghy <rhyme label="b" type="m" corresp="#s4:l2">sit</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Bidt dat Hy gunt <rhyme label="c" type="f">genaden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Aen ons met sond' <rhyme label="c" type="f" corresp="#s4:l5">beladen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ons by staet in ons' <rhyme label="d" type="m">dood</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Als wy sullen zijn in <rhyme label="d" type="m" corresp="#s4:l7">noodt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
