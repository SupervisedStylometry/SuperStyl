    </categories>
  <author>Johanna de Gavre</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Vrouw vol van <rhyme label="a" type="f">glorie</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Vrouw vol van <rhyme label="a" type="f" corresp="#s1:l1">victorie</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Eeuwighe <rhyme label="a" type="f" corresp="#s1:l1">memorie</rhyme></l>
        <l id="s1:l4" met="3" type="+-">van <rhyme label="b" type="m">Ierusalem</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Stadt Godts <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Arcke des <rhyme label="d" type="m">verbondts</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Wilt ons toch <rhyme label="c" type="f" corresp="#s1:l5">gheven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Vwe Moeders <rhyme label="d" type="m" corresp="#s1:l6">jonst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">O noydt <rhyme label="a" type="f">volpresen</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Schoon <rhyme label="a" type="f" corresp="#s2:l1">uytghelesen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Door u soet <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Zijn wy heel <rhyme label="b" type="m">verblijdt</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ons <rhyme label="c" type="f">voorspraeckersse</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wilt zijn voor <rhyme label="d" type="m">altijdt</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Siet u <rhyme label="c" type="f" corresp="#s2:l5">dienersse</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Loven u met <rhyme label="d" type="m" corresp="#s2:l6">vlijdt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Wy u <rhyme label="a" type="f">begroeten</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Die met u <rhyme label="a" type="f" corresp="#s3:l1">voeten</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Hebt heel <rhyme label="a" type="f" corresp="#s3:l1">vertreden</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">T’hooft van t’hels <rhyme label="b" type="m">Serpent</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">T’broodt van den <rhyme label="c" type="f">Hemel</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Vol van <rhyme label="d" type="m">soetigheydt</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Sonder <rhyme label="c" type="f" corresp="#s3:l5">gheschemel</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">draeght ghy u <rhyme label="d" type="m" corresp="#s3:l6">altijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">O Son vol <rhyme label="a" type="f">stralen</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Wegh sonder <rhyme label="a" type="f" corresp="#s4:l1">dwalen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Laet u <rhyme label="a" type="f" corresp="#s4:l1">achterhalen</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">In s’Hemels <rhyme label="b" type="m">Sion</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Schoon morghen <rhyme label="c" type="f">Sterre</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Vyt u schoon <rhyme label="d" type="m">tonneel</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Licht ons van <rhyme label="c" type="f" corresp="#s4:l5">verre</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Want ghy zijt ons <rhyme label="d" type="m" corresp="#s4:l6">deel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">O Schoon <rhyme label="a" type="f">Sibori</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Schoon <rhyme label="a" type="f" corresp="#s5:l1">Consistori</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Van <rhyme label="a" type="f" corresp="#s5:l1">d’alderheylighste</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-"><rhyme label="b" type="m">Dryvuldigheydt</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Hof vol <rhyme label="c" type="f">wellusten</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Maeght <rhyme label="d" type="m">onbevleckt</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">In wien wilt <rhyme label="c" type="f" corresp="#s5:l5">rusten</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">T’aenschijn Godts <rhyme label="d" type="m" corresp="#s5:l6">bedeckt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Wijngaerdt <rhyme label="a" type="f">vruchtbarigh</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Maer minder <rhyme label="a" type="f" corresp="#s6:l1">jaerigh</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">En zijt niet <rhyme label="a" type="f" corresp="#s6:l1">spaerigh</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Ons te schencken <rhyme label="b" type="m">dien</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">O suyver <rhyme label="c" type="f">Roose</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die noydt wordt <rhyme label="d" type="m">gheschent</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Maer altijt <rhyme label="c" type="f" corresp="#s6:l5">bloosen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Sal sonder <rhyme label="d" type="m" corresp="#s6:l6">endt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">O Leli <rhyme label="a" type="f">pure</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Schoon <rhyme label="a" type="f" corresp="#s7:l1">creature</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Tot alder <rhyme label="a" type="f" corresp="#s7:l1">ure</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Godt in u <rhyme label="b" type="m">vermaeckt</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Ghy kont wel <rhyme label="c" type="f">speuren</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Vyt Godts klaer <rhyme label="d" type="m">aenschijn</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Hoe ick <rhyme label="c" type="f" corresp="#s7:l5">betreure</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">T’ballinghschap van <rhyme label="d" type="m" corresp="#s7:l6">my</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">O Maeght vol <rhyme label="a" type="f">gratie</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Vol <rhyme label="a" type="f" corresp="#s8:l1">jubilatie</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Troost <rhyme label="a" type="f" corresp="#s8:l1">consolatie</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">In al onsen <rhyme label="b" type="m">noodt</rhyme>,</l>
        <l id="s8:l5" met="2" type="-+">Ghy weet te <rhyme label="c" type="f">streelen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Godts <rhyme label="d" type="m">rechtveerdigheydt</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Dat sy <rhyme label="c" type="f" corresp="#s8:l5">gheheel</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Moet zijn neer <rhyme label="d" type="m" corresp="#s8:l6">gheleydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
