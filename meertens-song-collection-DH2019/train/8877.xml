<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8877</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ich dank dir lieber Herr</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EEn Nieu liedt vrolijck <rhyme label="a" type="f">singhet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Den HEEr, die wonder <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Victory ons <rhyme label="a" type="f" corresp="#s1:l1">bedinghet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Syn Heylig' arm so <rhyme label="b" type="m" corresp="#s1:l2">goet</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">De HEERE laet <rhyme label="c" type="f">verconden</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Den volcken <rhyme label="d" type="m">Openbaer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Gherechticheyt van <rhyme label="c" type="f" corresp="#s1:l5">sonden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Door syn Ghenaed' <rhyme label="d" type="m" corresp="#s1:l6">All'gaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy denckt aen syn <rhyme label="a" type="f">ghenade</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Den Huyse <rhyme label="b" type="m">Israel</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Neemt syn Waerheyt te <rhyme label="a" type="f" corresp="#s2:l1">Raede</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dies mach nu weten <rhyme label="b" type="m" corresp="#s2:l2">wel</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Des gantschen Werelts <rhyme label="c" type="f">wijde</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Die Rechte <rhyme label="d" type="m">Salicheyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ons GOdes Heyl so <rhyme label="c" type="f" corresp="#s2:l5">blijde</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Hoe wel ist ons <rhyme label="d" type="m" corresp="#s2:l6">Bereydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Den HEERE Juychet <rhyme label="a" type="f">alle</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Singt, Roemet, Lovet <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Den HEER loofft met <rhyme label="a" type="f" corresp="#s3:l1">gheschalle</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met Psalm end' Harpen <rhyme label="b" type="m" corresp="#s3:l2">spel</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Basuynen laet <rhyme label="c" type="f">Hovieren</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Den Coninck onsen <rhyme label="d" type="m">HEER</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die Bommen Vrolijck <rhyme label="c" type="f" corresp="#s3:l5">Tieren</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Juychet tot syner <rhyme label="d" type="m" corresp="#s3:l6">Eer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die Waterstroome <rhyme label="a" type="f">Bruysschen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die Berghe Vrolijck <rhyme label="b" type="m">al</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Ter Eer des HEEREN <rhyme label="a" type="f" corresp="#s4:l1">Ruysschen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Want hy dit Aertsche <rhyme label="b" type="m" corresp="#s4:l2">Dal</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Coemt over al te <rhyme label="c" type="f">Richten</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Met syn <rhyme label="d" type="m">Gherechticheyt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Die Volcken te <rhyme label="c" type="f" corresp="#s4:l5">verlichten</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Nae syns Woordes <rhyme label="d" type="m" corresp="#s4:l6">Bescheydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
