      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Sei tanto graziosa</melody>
    <categories>
      <category translation="occasions">gelegenheden</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>G.A. Bredero</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ghewapende <rhyme label="a" type="f">Goddinne</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Die uyt het brein uws vaders sijt <rhyme label="b" type="f">geboren</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ghy laet uyt ware <rhyme label="a" type="f" corresp="#s1:l1">minne</rhyme></l>
        <l id="s1:l4" met="5" type="-+">En liefd' tot ons de heugelijcke <rhyme label="b" type="f" corresp="#s1:l2">Choren</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">U <rhyme label="c" type="m">borghery</rhyme></l>
        <l id="s1:l6" met="2" type="-+">De Hem'len <rhyme label="c" type="m" corresp="#s1:l5">bly</rhyme></l>
        <l id="s1:l7" met="3" type="-+">d' Onuytdrucklijcke <rhyme label="d" type="f">zalen</rhyme>;</l>
        <l id="s1:l8" met="2" type="-+">En ghy <rhyme label="e" type="f">alwaarde</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Die komt hier opter <rhyme label="e" type="f" corresp="#s1:l8">Aarden</rhyme></l>
        <l id="s1:l10" met="2" type="+-">By ons <rhyme label="d" type="f" corresp="#s1:l7">dalen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dies kruyderen en <rhyme label="a" type="f">bloemen</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">De heylicheen, bekent en noch <rhyme label="b" type="f">verholen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Haer wel gheluckich <rhyme label="a" type="f" corresp="#s2:l1">noemen</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Door 't voelen van de platten uwer <rhyme label="b" type="f" corresp="#s2:l2">zolen</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Waer ghy die <rhyme label="c" type="m">stelt</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Het gras-rijck <rhyme label="c" type="m" corresp="#s2:l5">velt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">De bergen, beemden, <rhyme label="d" type="f">bladen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Vieren de <rhyme label="e" type="f">treden</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Van u groot <rhyme label="e" type="f" corresp="#s2:l8">achtbaerheden</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">Vol <rhyme label="d" type="f" corresp="#s2:l7">ghenaden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De sterffelijcke <rhyme label="a" type="f">lieden</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Als sy 't gheruys van u vermaertheyt <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Sy wijcken met <rhyme label="a" type="f" corresp="#s3:l1">eerbieden</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">En neyghen deft met nedrich hart en <rhyme label="b" type="f" corresp="#s3:l2">ooren</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Voor u te <rhyme label="c" type="m">voet</rhyme></l>
        <l id="s3:l6" met="2" type="-+">In groot <rhyme label="c" type="m" corresp="#s3:l5">ootmoet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dijn majesteyt ter <rhyme label="d" type="f">eeren</rhyme>:</l>
        <l id="s3:l8" met="2" type="-+">Dus by u <rhyme label="e" type="f">krielen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">De sinnen en de <rhyme label="e" type="f" corresp="#s3:l8">sielen</rhyme></l>
        <l id="s3:l10" met="2" type="+-">Om te <rhyme label="d" type="f" corresp="#s3:l7">leeren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Vroe-moeder van de <rhyme label="a" type="f">Reden</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Ghy dochter vanden grooten God der <rhyme label="b" type="f">Goden</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ghy Stichsters vande <rhyme label="a" type="f" corresp="#s4:l1">Steden</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Ghy Koningin van wetten en <rhyme label="b" type="f" corresp="#s4:l2">gheboden</rhyme>.</l>
        <l id="s4:l5" met="2" type="-+">Voochdesse, <rhyme label="c" type="m">die</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Dees driemael <rhyme label="c" type="m" corresp="#s4:l5">drie</rhyme></l>
        <l id="s4:l7" met="3" type="-+">In u dienst hebt <rhyme label="d" type="f">ghenomen</rhyme>:</l>
        <l id="s4:l8" met="2" type="-+">Daalt af in <rhyme label="e" type="f">vreden</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En weest ons hier <rhyme label="e" type="f" corresp="#s4:l8">beneden</rhyme></l>
        <l id="s4:l10" met="2" type="+-"><rhyme label="d" type="f" corresp="#s4:l7">Welghekomen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
