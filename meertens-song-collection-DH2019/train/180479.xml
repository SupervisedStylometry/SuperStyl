    <tags/>
    <melody>Boerinnetje</melody>
  <author>Philippus van Sorgen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ach mijn <rhyme label="a" type="f">Vrindinne</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Segt my uw <rhyme label="a" type="f" corresp="#s1:l1">sinne</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Mijn herte gloeit als vuur van <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">En ’t mijne is soo kout als ys van <rhyme label="b" type="f" corresp="#s1:l3">binnen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Keert u tot mijnen <rhyme label="c" type="m">baet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ick weet van selfs wel <rhyme label="c" type="m" corresp="#s1:l5">raet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">O desolaten <rhyme label="c" type="m" corresp="#s1:l5">staet</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Sult gy ’t dan <rhyme label="b" type="f" corresp="#s1:l3">winnen</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ia ick sal ’t <rhyme label="a" type="f">vinden</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Waer mijn <rhyme label="a" type="f" corresp="#s2:l1">beminden</rhyme>?</l>
        <l id="s2:l3" met="4" type="-+">Daer ick mijn sin kan doen of <rhyme label="b" type="f">laeten</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Dit is de rechte wech om my te <rhyme label="b" type="f" corresp="#s2:l3">haten</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ick neem ’t nochtans soo <rhyme label="c" type="m">voor</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gy dwaelt van ’t rechte <rhyme label="c" type="m" corresp="#s2:l5">spoor</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ick geef u geen <rhyme label="c" type="m" corresp="#s2:l5">gehoor</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Gy moocht wat <rhyme label="b" type="f" corresp="#s2:l3">praeten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Wilt gy dan <rhyme label="a" type="f">vlieden</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Mijn sinnen ‘t <rhyme label="a" type="f" corresp="#s3:l1">rieden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">U eygen sin kan wel <rhyme label="b" type="f">dwaelen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Ick sal die wel door mijn kracht <rhyme label="b" type="f" corresp="#s3:l3">bepalen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die macht is by u <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">By wie is dan ’t <rhyme label="c" type="m" corresp="#s3:l5">gebiet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">By my die van u <rhyme label="c" type="m" corresp="#s3:l5">vliet</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Kunt gy die <rhyme label="b" type="f" corresp="#s3:l3">halen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
