        <l id="s1:l3" met="3" type="-+">Ryk en in aanzien te <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Zal 'k niet <rhyme label="b" type="f" corresp="#s1:l3">weÃªstreeven</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">'k Geef deez' <rhyme label="a" type="m" corresp="#s1:l1">getuigenis</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Maar 't grootst geluk kan <rhyme label="c" type="f">hieten</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Wanneer wy, vry van <rhyme label="d" type="m">schyn</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Dat geluk, dat wy <rhyme label="c" type="f" corresp="#s1:l6">genieten</rhyme>,</l>
