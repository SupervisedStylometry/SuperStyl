<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159833</id>
    <date>
      <year-start>1669</year-start>
      <year-end>1669</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La cardinale</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">LAast als Venus wufte blinde <rhyme label="a" type="m">Zoon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zijn koker hadt <rhyme label="b" type="f">geladen</rhyme>;</l>
        <l id="s1:l3" met="5" type="+-">Daalden hy uit 's Hemels hooge <rhyme label="a" type="m" corresp="#s1:l1">troon</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">By Tirus <rhyme label="b" type="f" corresp="#s1:l2">waterbaden</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Onder schaauw van <rhyme label="b" type="f" corresp="#s1:l2">bladen</rhyme>,</l>
        <l id="s1:l6" met="5" type="+-">Daar Apol, de broeder van <rhyme label="a" type="m" corresp="#s1:l1">Latoon</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ter sluik zijn min <rhyme label="b" type="f" corresp="#s1:l2">verzaden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Hy hadt zijn Leukotoé <rhyme label="a" type="m">gevat</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Omhelst met bey zijn <rhyme label="b" type="f">armen</rhyme>;</l>
        <l id="s2:l3" met="5" type="+-">Zuchtende, och mijn lief, mijn lieve <rhyme label="a" type="m" corresp="#s2:l1">schat</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ach wilt u doch <rhyme label="b" type="f" corresp="#s2:l2">ontfarmen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Over my <rhyme label="b" type="f" corresp="#s2:l2">erbarmen</rhyme>;</l>
        <l id="s2:l6" met="5" type="+-">Zie, ik ben de Godt van't <rhyme label="a" type="m" corresp="#s2:l1">Zonnerat</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die 't aartrijk komt <rhyme label="b" type="f" corresp="#s2:l2">verwarmen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Pallas is mijn zuster en <rhyme label="a" type="m">Diaan</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Uit Jupyns bloet <rhyme label="b" type="f">gesproten</rhyme>,</l>
        <l id="s3:l3" met="5" type="+-">Hy heeft in gedaante van een <rhyme label="a" type="m" corresp="#s3:l1">Zwaan</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wel Ledaas min <rhyme label="b" type="f" corresp="#s3:l2">genooten</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Zult gy my <rhyme label="b" type="f" corresp="#s3:l2">verstooten</rhyme>?</l>
        <l id="s3:l6" met="5" type="+-">Dan zal ik het aartrijk van de <rhyme label="a" type="m" corresp="#s3:l1">Maan</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En van mijn licht <rhyme label="b" type="f" corresp="#s3:l2">ontblooten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="+-">Maar de Maegt bleef even stuurs en <rhyme label="a" type="m">koel</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gelijk de Beer in't <rhyme label="b" type="f">Noorden</rhyme>;</l>
        <l id="s4:l3" met="5" type="+-">Als 't kristal van een bevrooze <rhyme label="a" type="m" corresp="#s4:l1">poel</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Wanneer de groene <rhyme label="b" type="f" corresp="#s4:l2">boorden</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Die het oogh <rhyme label="b" type="f" corresp="#s4:l2">bekoorden</rhyme>,</l>
        <l id="s4:l6" met="5" type="+-">Zijn verdort van't Levendigh <rhyme label="a" type="m" corresp="#s4:l1">gevoel</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En door de kouw <rhyme label="b" type="f" corresp="#s4:l2">versmoorden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="+-">Maar de Min trok doen een pijl heel <rhyme label="a" type="m">spits</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Uit zijn vergulde <rhyme label="b" type="f">kooker</rhyme>,</l>
        <l id="s5:l3" met="5" type="+-">En, door't groen, schoot hy die vlugge <rhyme label="a" type="m" corresp="#s5:l1">flits</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Een rechte <rhyme label="b" type="f" corresp="#s5:l2">vlammestoker</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Die gelijk een <rhyme label="b" type="f" corresp="#s5:l2">mooker</rhyme>,</l>
        <l id="s5:l6" met="5" type="+-">'t Harde hart versmeden, eerst zoo <rhyme label="a" type="m" corresp="#s5:l1">bits</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En al haar fierheit <rhyme label="b" type="f" corresp="#s5:l2">dook'er</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="+-">Zy zeeg neúr op Febus <rhyme label="a" type="m">Zonneborst</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">En zuchten; wreede <rhyme label="b" type="f">Goden</rhyme>!</l>
        <l id="s6:l3" met="5" type="+-">Hebt gy my verwonnen door de <rhyme label="a" type="m" corresp="#s6:l1">vorst</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Der liefden, al te <rhyme label="b" type="f" corresp="#s6:l2">snooden</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Kost gy my niet <rhyme label="b" type="f" corresp="#s6:l2">dooden</rhyme>?</l>
        <l id="s6:l6" met="5" type="+-">Want mijn hart, nu heet, na liefde <rhyme label="a" type="m" corresp="#s6:l1">dorst</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Doen heeftz'er mont <rhyme label="b" type="f" corresp="#s6:l2">gebooden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="+-">Maar de Godt die's Hemels blauwe <rhyme label="a" type="m">trans</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Verheerlykt met zijn <rhyme label="b" type="f">stralen</rhyme>,</l>
        <l id="s7:l3" met="5" type="+-">Ley van't hooft zijn gulde <rhyme label="a" type="m" corresp="#s7:l1">Zonnekrans</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Sprak schoone, laat nu <rhyme label="b" type="f" corresp="#s7:l2">dalen</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Hoogmoet, 'k zal <rhyme label="b" type="f" corresp="#s7:l2">verhalen</rhyme>,</l>
        <l id="s7:l6" met="5" type="+-">Hoe Euroop gewaagt heeft deze <rhyme label="a" type="m" corresp="#s7:l1">dans</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Op Tetis blaauwe <rhyme label="b" type="f" corresp="#s7:l2">Zalen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
