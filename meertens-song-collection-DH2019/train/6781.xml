<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6781</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>emmerick</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
    </tags>
    <melody>Het viel een hemelse dauw</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ewich God <rhyme label="a" type="f">almachtich</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Tot u so roepen <rhyme label="b" type="m">wy</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wilt ons eens sijn <rhyme label="a" type="f" corresp="#s1:l1">gedachtich</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Maeckt Israel eens <rhyme label="b" type="m" corresp="#s1:l2">bly</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Wilt haest Messias <rhyme label="c" type="f">senden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die ons verlossen <rhyme label="d" type="m">sal</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Van alle ons <rhyme label="c" type="f" corresp="#s1:l5">elenden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Geschiet door Adams <rhyme label="d" type="m" corresp="#s1:l6">val</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Wilt haest Messias <rhyme label="c" type="f" corresp="#s1:l5">senden</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Die ons verlossen <rhyme label="d" type="m" corresp="#s1:l6">sal</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Van alle ons <rhyme label="c" type="f" corresp="#s1:l5">elenden</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Geschiet door Adams <rhyme label="d" type="m" corresp="#s1:l6">val</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Och mocht ick eens <rhyme label="a" type="f">aenschouwen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die schone jonghe <rhyme label="b" type="m">maecht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die edel vrou der <rhyme label="a" type="f" corresp="#s2:l1">vrouwen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die Godt so heeft <rhyme label="b" type="m" corresp="#s2:l2">behaecht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat sy eens sal <rhyme label="c" type="f">ontfangen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Den Salich-maker <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Naer hem staet mijn <rhyme label="c" type="f" corresp="#s2:l5">verlangen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Hy is dat eewich <rhyme label="d" type="m" corresp="#s2:l6">goet</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Dat sy eens sal <rhyme label="c" type="f" corresp="#s2:l5">ontfangen</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Den Salich-maker <rhyme label="d" type="m" corresp="#s2:l6">soet</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Naer hem staet mijn <rhyme label="c" type="f" corresp="#s2:l5">verlangen</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Hy is dat eewich <rhyme label="d" type="m" corresp="#s2:l6">goet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ick wensch met hertzen <rhyme label="a" type="f">seere</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat ick haer dienen <rhyme label="b" type="m">mocht</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die tot so grooter <rhyme label="a" type="f" corresp="#s3:l1">eere</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Van Godt al sijn <rhyme label="b" type="m" corresp="#s3:l2">besocht</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Want sy is uyt <rhyme label="c" type="f">gelesen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van inder <rhyme label="d" type="m">eewicheyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Om eens moeder te <rhyme label="c" type="f" corresp="#s3:l5">wesen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van sijnder <rhyme label="d" type="m" corresp="#s3:l6">Majesteyt</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">Want sy is uyt <rhyme label="c" type="f" corresp="#s3:l5">gelesen</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Van inder <rhyme label="d" type="m" corresp="#s3:l6">eewicheyt</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Om eens moeder te <rhyme label="c" type="f" corresp="#s3:l5">wesen</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Van sijnder <rhyme label="d" type="m" corresp="#s3:l6">Majesteyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick groet u Maget <rhyme label="a" type="f">schoone</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Gebenedijt sijt <rhyme label="b" type="m">ghy</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Den Vader uyt den <rhyme label="a" type="f" corresp="#s4:l1">throone</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die is u altijdt <rhyme label="b" type="m" corresp="#s4:l2">by</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Hy heeft u <rhyme label="c" type="f">uytverkoren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Want ghy vol grati <rhyme label="d" type="m">sijt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Van u sal sijn <rhyme label="c" type="f" corresp="#s4:l5">geboren</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Christus <rhyme label="d" type="m" corresp="#s4:l6">gebenedijt</rhyme>.</l>
        <l id="s4:l9" met="3" type="-+">Hy heeft u <rhyme label="c" type="f" corresp="#s4:l5">uytverkoren</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Want ghy vol grati <rhyme label="d" type="m" corresp="#s4:l6">sijt</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Van u sal sijn <rhyme label="c" type="f" corresp="#s4:l5">geboren</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Christus <rhyme label="d" type="m" corresp="#s4:l6">gebenedijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wat sijn dit voor <rhyme label="a" type="f">nieu-maren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Seght my tâ€™verstant hier <rhyme label="b" type="m">van</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hoe ist dat ick sou <rhyme label="a" type="f" corresp="#s5:l1">baeren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daer ick noyt kende <rhyme label="b" type="m" corresp="#s5:l2">man</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Ick heb Godt <rhyme label="c" type="f">opgedragen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Van jonx mijn <rhyme label="d" type="m">suyverheyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Om hem soo te <rhyme label="c" type="f" corresp="#s5:l5">behaeghen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Tot mijnder <rhyme label="d" type="m" corresp="#s5:l6">salicheydt</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">Ick heb Godt <rhyme label="c" type="f" corresp="#s5:l5">opgedragen</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Van jonx mijn <rhyme label="d" type="m" corresp="#s5:l6">suyverheyt</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Om hem soo te <rhyme label="c" type="f" corresp="#s5:l5">behaeghen</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Tot mijnder <rhyme label="d" type="m" corresp="#s5:l6">salicheydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Schoon maecht wilt u <rhyme label="a" type="f">verblijen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En sijt toch niet <rhyme label="b" type="m">bevreest</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die Heer sal u <rhyme label="a" type="f" corresp="#s6:l1">bevrijen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">V senden sijnen <rhyme label="b" type="m" corresp="#s6:l2">geest</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Sijn kracht sal u <rhyme label="c" type="f">omschijnen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van hem sult sijn <rhyme label="d" type="m">vervult</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">En baeren sonder <rhyme label="c" type="f" corresp="#s6:l5">pijnen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Moeder Godts wesen <rhyme label="d" type="m" corresp="#s6:l6">sult</rhyme>.</l>
        <l id="s6:l9" met="3" type="-+">Sijn kracht sal u <rhyme label="c" type="f" corresp="#s6:l5">omschijnen</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Van hem sult sijn <rhyme label="d" type="m" corresp="#s6:l6">vervult</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">En baeren sonder <rhyme label="c" type="f" corresp="#s6:l5">pijnen</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Moeder Godts wesen <rhyme label="d" type="m" corresp="#s6:l6">sult</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn hert is seer <rhyme label="a" type="f">ontsteken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Als ick u heb <rhyme label="b" type="m">gehoort</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Met blijschap wil ick <rhyme label="a" type="f" corresp="#s7:l1">spreken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">My geschie naer u <rhyme label="b" type="m" corresp="#s7:l2">woort</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Mijn siel maeckt gtroot den <rhyme label="c" type="f">Heere</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Ick wil hem roepen <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Want hy sijn dienst-maecht <rhyme label="c" type="f" corresp="#s7:l5">teere</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Veel grati heeft <rhyme label="d" type="m" corresp="#s7:l6">gedaen</rhyme>.</l>
        <l id="s7:l9" met="3" type="-+">Mijn siel maeckt gtroot den <rhyme label="c" type="f" corresp="#s7:l5">Heere</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Ick wil hem roepen <rhyme label="d" type="m" corresp="#s7:l6">aen</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Want hy sijn dienst-maecht <rhyme label="c" type="f" corresp="#s7:l5">teere</rhyme></l>
        <l id="s7:l12" met="3" type="-+">Veel grati heeft <rhyme label="d" type="m" corresp="#s7:l6">gedaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ter eeren laet ons <rhyme label="a" type="f">singen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Van die schoon Maget <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Op dat sy ons wilt <rhyme label="a" type="f" corresp="#s8:l1">brengen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">In dat schoon hemels <rhyme label="b" type="m" corresp="#s8:l2">pleyn</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Daer vreucht is boven <rhyme label="c" type="f">maten</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Voor die tot Godt hem <rhyme label="d" type="m">keert</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Gheen herte en kanse <rhyme label="c" type="f" corresp="#s8:l5">vaten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">So die Schriftuer <rhyme label="d" type="m" corresp="#s8:l6">leert</rhyme>.</l>
        <l id="s8:l9" met="3" type="-+">Daer vreucht is boven <rhyme label="c" type="f" corresp="#s8:l5">maten</rhyme></l>
        <l id="s8:l10" met="3" type="-+">Voor die tot Godt hem <rhyme label="d" type="m" corresp="#s8:l6">keert</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">Gheen herte en kanse <rhyme label="c" type="f" corresp="#s8:l5">vaten</rhyme>,</l>
        <l id="s8:l12" met="3" type="-+">So die Schriftuer <rhyme label="d" type="m" corresp="#s8:l6">leert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
