<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>100098</id>
    <date>
      <year-start>1652</year-start>
      <year-end>1652</year-end>
    </date>
    <location>
      <orginal-name>delft</orginal-name>
      <standard-name>Delft, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0114017</lon>
        <lat>4.35839</lat>
      </coordinates>
    </location>
    <tags/>
    <melody/>
  <author>Arnold Bon</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">PHilander most laest over <rhyme label="a" type="m">Zee</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En vlieden van zijn <rhyme label="b" type="f">Herderinne</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Doch door het <rhyme label="b" type="f" corresp="#s1:l2">Minne</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Van zijn <rhyme label="a" type="m" corresp="#s1:l1">Galathe</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En VVonde van een diepen <rhyme label="c" type="m">Schicht</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Uyten Pijl-koocker van het kleyne <rhyme label="c" type="m" corresp="#s1:l5">wicht</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Geschooten in zijn <rhyme label="d" type="m">Hert</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Most keeren door de <rhyme label="d" type="m" corresp="#s1:l7">smert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Weer tot zijn eygen Herts <rhyme label="a" type="m">geneucht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Om 't koutste van zijn Ziel 't <rhyme label="b" type="f">ontvoncken</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Want door het <rhyme label="b" type="f" corresp="#s2:l2">loncken</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Van haer lieve <rhyme label="a" type="m" corresp="#s2:l1">Jeucht</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Hy strack zijn brandent Hert <rhyme label="c" type="m">verkoelt</rhyme>,</l>
        <l id="s2:l6" met="5" type="-+">Maer weer weer een nieuw geprickel in hem <rhyme label="c" type="m" corresp="#s2:l5">voelt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Want Venus metter <rhyme label="d" type="m">yl</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Schoot weer een nieuwe <rhyme label="d" type="m" corresp="#s2:l7">Pijl</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En den Springh-ader vande <rhyme label="a" type="m">lust</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ontstack strack door het hittich <rhyme label="b" type="f">branden</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Dies hy de <rhyme label="b" type="f" corresp="#s3:l2">stranden</rhyme>:</l>
        <l id="s3:l4" met="3" type="+-">Vande soete <rhyme label="a" type="m" corresp="#s3:l1">Rust</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Sijns Nimphies altijt volgen <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Hoe wel hy smaeckte noch het bitter <rhyme label="c" type="m" corresp="#s3:l5">soet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Van hare woorden <rhyme label="d" type="m">straf</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Die sy tot noch toe <rhyme label="d" type="m" corresp="#s3:l7">gaf</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O Galathea waerste <rhyme label="a" type="m">pant</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Waerom doet ghy hem dese <rhyme label="b" type="f">smerte</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Denckt dat zijn <rhyme label="b" type="f" corresp="#s4:l2">Herte</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Door de Min <rhyme label="a" type="m" corresp="#s4:l1">verbrant</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Want doen hy laestmael was <rhyme label="c" type="m">ontbloot</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">En dat hy derven most u eerbaer <rhyme label="c" type="m" corresp="#s4:l5">Root</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Soo was het dat zijn <rhyme label="d" type="m">Jeucht</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Vergingh in <rhyme label="d" type="m" corresp="#s4:l7">ongeneucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
