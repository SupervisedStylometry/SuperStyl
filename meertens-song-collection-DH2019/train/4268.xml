<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4268</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Prompt en volmaakt</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Pieter Lenaerts van der Goes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">PRompt, en <rhyme label="a" type="m">volmaect</rhyme> //</l>
        <l id="s1:l2" met="3" type="+-">Is mijn <rhyme label="b" type="f">wtvercoren</rhyme> //</l>
        <l id="s1:l3" met="2" type="-+">Soo wel <rhyme label="a" type="m" corresp="#s1:l1">geraect</rhyme> //</l>
        <l id="s1:l4" met="3" type="-+">En was noyt Helt <rhyme label="b" type="f" corresp="#s1:l2">geboren</rhyme> //</l>
        <l id="s1:l5" met="2" type="+-">Alvoren // <rhyme label="c" type="m">can</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Hy wech, en regel // <rhyme label="d" type="f">schrijven</rhyme> //</l>
        <l id="s1:l7" met="2" type="-+">Op t'herte sal <rhyme label="c" type="m" corresp="#s1:l5">dan</rhyme> //</l>
        <l id="s1:l8" met="2" type="-+">Dees jongen <rhyme label="c" type="m" corresp="#s1:l5">man</rhyme> //</l>
        <l id="s1:l9" met="3" type="+-">Voortan // als segel // <rhyme label="d" type="f" corresp="#s1:l6">blijven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">In een <rhyme label="a" type="m">loyal</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Hert, heb ick <rhyme label="b" type="f">besloten</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">Nu dat den <rhyme label="a" type="m" corresp="#s2:l1">strael</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Der minnen is <rhyme label="b" type="f" corresp="#s2:l2">geschoten</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Genoten // <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Moet met hem vreugd' // en // <rhyme label="d" type="f">weenen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Ken achte <rhyme label="c" type="m" corresp="#s2:l5">gepijn</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Ick sal <rhyme label="c" type="m" corresp="#s2:l5">t'sanguijn</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Aenschijn // in deugden // <rhyme label="d" type="f" corresp="#s2:l6">nemen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Eenpaerlick <rhyme label="a" type="m">staet</rhyme></l>
        <l id="s3:l2" met="3" type="+-">T'concept mijner <rhyme label="b" type="f">sinnen</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Dat ick (naer <rhyme label="a" type="m" corresp="#s3:l1">raet</rhyme>)</l>
        <l id="s3:l4" met="3" type="-+">Hem niet en sal <rhyme label="b" type="f" corresp="#s3:l2">beginnen</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Te minnen // <rhyme label="c" type="m">small</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Maer als mijn heere // <rhyme label="d" type="f">croonen</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">In spijte <rhyme label="c" type="m" corresp="#s3:l5">t'getal</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Der tongen <rhyme label="c" type="m" corresp="#s3:l5">al</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Ick sal // hem eere // <rhyme label="d" type="f" corresp="#s3:l6">thoonen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Troostbaerder // <rhyme label="a" type="m">geen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Inder Goden <rhyme label="b" type="f">troone</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Al waerder // <rhyme label="a" type="m" corresp="#s4:l1">een</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Adonis in <rhyme label="b" type="f" corresp="#s4:l2">persoone</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">Zijn schoone // <rhyme label="c" type="m">glans</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Moet moet noch menich // <rhyme label="d" type="f">buygen</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">Want, hy draecht den <rhyme label="c" type="m" corresp="#s4:l5">crans</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Der ionger <rhyme label="c" type="m" corresp="#s4:l5">mans</rhyme>,</l>
        <l id="s4:l9" met="3" type="+-">Dits gans // mijn eenich // <rhyme label="d" type="f" corresp="#s4:l6">tuygen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Een <rhyme label="a" type="m">Paragoen</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Is hy, vroom <rhyme label="b" type="f">gesegent</rhyme>:</l>
        <l id="s5:l3" met="2" type="-+">Van dien <rhyme label="a" type="m" corresp="#s5:l1">Baroen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">My alle troost <rhyme label="b" type="f" corresp="#s5:l2">beiegent</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Genegent- // <rhyme label="c" type="m">heyt</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Theimwaerts, can geuen // <rhyme label="d" type="f">smerte</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Maer laet met <rhyme label="c" type="m" corresp="#s5:l5">bescheyt</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">V zijn <rhyme label="c" type="m" corresp="#s5:l5">geseyt</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Hy leyt, my neuen // <rhyme label="d" type="f" corresp="#s5:l6">t'herte</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Rustich, en <rhyme label="a" type="m">fray</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Moet mijn Prince <rhyme label="b" type="f">varen</rhyme>:</l>
        <l id="s6:l3" met="2" type="-+">Den Rijnschen <rhyme label="a" type="m" corresp="#s6:l1">bay</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Wensch ick hem sonder <rhyme label="b" type="f" corresp="#s6:l2">sparen</rhyme>:</l>
        <l id="s6:l5" met="2" type="+-">Geen swaren // <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Wil ick ten fijne // <rhyme label="d" type="f">maken</rhyme>:</l>
        <l id="s6:l7" met="2" type="-+">Dit gracelick <rhyme label="c" type="m" corresp="#s6:l5">bloet</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">My altijt <rhyme label="c" type="m" corresp="#s6:l5">doet</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Meer soet // dan pijne // <rhyme label="d" type="f" corresp="#s6:l6">smaken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
