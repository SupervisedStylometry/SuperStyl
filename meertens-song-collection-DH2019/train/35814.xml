<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>35814</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>O liefste lief geprezen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Albertus Buitendyck</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">TOt Godes lof wilt <rhyme label="a" type="f">hooren</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wat wonder dat <rhyme label="b" type="m">geschiedt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Als Christus wort <rhyme label="a" type="f" corresp="#s1:l1">gebooren</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">Sijn Moeder, en heeft <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Haer zuyverheyt <rhyme label="a" type="f" corresp="#s1:l1">verlooren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Blijft Maeght als van te <rhyme label="a" type="f" corresp="#s1:l1">vooren</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Hier Moyses Braem-bosch <rhyme label="c" type="m">brant</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">'t Blijft ongeschent en <rhyme label="c" type="m" corresp="#s1:l7">kant</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ziet Elizaeus <rhyme label="a" type="f">buygen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Op Adams geslacht <rhyme label="b" type="m">doodt</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Ziet nu den Schepper <rhyme label="a" type="f" corresp="#s2:l1">zuygen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Den Borst, en lijde <rhyme label="b" type="m" corresp="#s2:l2">noodt</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Als een Beest <rhyme label="a" type="f" corresp="#s2:l1">d'aldermeeste</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Leyt in een krib' der <rhyme label="a" type="f" corresp="#s2:l1">Beesten</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Een Maghet jong en <rhyme label="c" type="m">teer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Bint d'aldersterckste <rhyme label="c" type="m" corresp="#s2:l7">Heer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die al de'es Werelts <rhyme label="a" type="f">hoecken</rhyme>.</l>
        <l id="s3:l2" met="3" type="-+">En Hemelen <rhyme label="b" type="m">vervult</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Besloten leyt in <rhyme label="a" type="f" corresp="#s3:l1">doecken</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">d' Rechter om onse <rhyme label="b" type="m" corresp="#s3:l2">schult</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En Misdaden moet <rhyme label="a" type="f" corresp="#s3:l1">lijden</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Ziet hoe in dese <rhyme label="a" type="f" corresp="#s3:l1">tijden</rhyme></l>
        <l id="s3:l7" met="3" type="-+">d' Een-hooren, en Zee <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Besluyt een Maeghde <rhyme label="c" type="m" corresp="#s3:l7">schoot</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Koninck leyt <rhyme label="a" type="f">verschoven</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">Godt is een Menschen <rhyme label="b" type="m">Kindt</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Die d' Engelen al <rhyme label="a" type="f" corresp="#s4:l1">loven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ons meer als hem <rhyme label="b" type="m" corresp="#s4:l2">bemint</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Een Zondaer schijnt te <rhyme label="a" type="f" corresp="#s4:l1">wesen</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Ons Zonden komt <rhyme label="a" type="f" corresp="#s4:l1">genesen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">'t Lam is een Harder <rhyme label="c" type="m">goedt</rhyme>.</l>
        <l id="s4:l8" met="3" type="-+">'t Vyer stroy geen schade <rhyme label="c" type="m" corresp="#s4:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Augustus, Heer van <rhyme label="a" type="f">Romen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van Apollo <rhyme label="b" type="m">verstaet</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Dat Christus is <rhyme label="a" type="f" corresp="#s5:l1">gekomen</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">De'es Keyser zijnen <rhyme label="b" type="m" corresp="#s5:l2">raet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Seyt: Met my d' Heer der <rhyme label="a" type="f" corresp="#s5:l1">Heeren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Moet ghy prijsen en <rhyme label="a" type="f" corresp="#s5:l1">eeren</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Hy geeft Godt d'hooghste <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Leyt ne'er sijn naem van <rhyme label="c" type="m" corresp="#s5:l7">Heer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Janus oorloghs kercx <rhyme label="a" type="f">poorten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Laet sluyten driemael <rhyme label="b" type="m">al</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Want in Christus <rhyme label="a" type="f" corresp="#s6:l1">geboorte</rhyme></l>
        <l id="s6:l4" met="3" type="-+">'t Was peys op 't aertsche <rhyme label="b" type="m" corresp="#s6:l2">dal</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">d' Afgode neder <rhyme label="a" type="f" corresp="#s6:l1">villen</rhyme>.</l>
        <l id="s6:l6" met="3" type="-+">Propheten en <rhyme label="a" type="f" corresp="#s6:l1">Sybillen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Vervult zijn inde <rhyme label="c" type="m">tijdt</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Hy weent, en ons <rhyme label="c" type="m" corresp="#s6:l7">verblijdt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Tot Romen mede <rhyme label="a" type="f">vloeyde</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Oly uyt een <rhyme label="b" type="m">fonteyn</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">t' Engaddi draeght en <rhyme label="a" type="f" corresp="#s7:l1">bloeyde</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Wijngaers en balsem <rhyme label="b" type="m" corresp="#s7:l2">reyn</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Drie Sonnen <rhyme label="a" type="f" corresp="#s7:l1">openbaerde</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Seer klaer by de <rhyme label="a" type="f" corresp="#s7:l1">Spanjaerde</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dese drie worden <rhyme label="c" type="m">een</rhyme>.</l>
        <l id="s7:l8" met="3" type="-+">By d' Harders een licht <rhyme label="c" type="m" corresp="#s7:l7">scheen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Een Starr' de Wijse <rhyme label="a" type="f">node</rhyme>.</l>
        <l id="s8:l2" met="3" type="-+">d' Engelen zongen <rhyme label="b" type="m">seer</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">In d' alderhooghste <rhyme label="a" type="f" corresp="#s8:l1">Gode</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Sy glory, lof en <rhyme label="b" type="m" corresp="#s8:l2">eer</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Den mensch van goede <rhyme label="a" type="f" corresp="#s8:l1">wille</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Zy vree sonder <rhyme label="a" type="f" corresp="#s8:l1">verschille</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">In dese blijdste <rhyme label="c" type="m">nacht</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Alles voelt Godes <rhyme label="c" type="m" corresp="#s8:l7">kracht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Geeft ons nu Heer, de <rhyme label="a" type="f">zegen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Dat ghy de spongy <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Om ons quaet uyt te <rhyme label="a" type="f" corresp="#s9:l1">vegen</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Als een Cyther <rhyme label="b" type="m" corresp="#s9:l2">verblijdt</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Alleen ons haert en <rhyme label="a" type="f" corresp="#s9:l1">sinnen</rhyme>.</l>
        <l id="s9:l6" met="3" type="-+">En wilt ons soo <rhyme label="a" type="f" corresp="#s9:l1">beminnen</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Dat ghy Ã´ Hemels <rhyme label="c" type="m">broodt</rhyme>!</l>
        <l id="s9:l8" met="3" type="-+">Ons spijst in onse <rhyme label="c" type="m" corresp="#s9:l7">noodt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
