<song>
  <meta>
    <id>186713</id>
    <date>
      <year-start>1674</year-start>
      <year-end>1674</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Psalm 038 Datheen</melody>
  <author>Joannes Six van Chandelier</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Heer, bestrafme niet door <rhyme label="a" type="f">slaagen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Noch met <rhyme label="a" type="f" corresp="#s1:l1">plaagen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wyl uw hart van toorne <rhyme label="b" type="m">siedt</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Noch kastyme, wyl uw <rhyme label="c" type="f">handen</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Vierig <rhyme label="c" type="f" corresp="#s1:l4">branden</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">In uw grimmigheeden <rhyme label="b" type="m" corresp="#s1:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Want de pylen van uw <rhyme label="a" type="f">boogen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s2:l1">Neergevloogen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Kleeven in myn lyf, en <rhyme label="b" type="m">ziel</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Uwe hand quam zoo, met <rhyme label="c" type="f">quaalen</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Op my <rhyme label="c" type="f" corresp="#s2:l4">daalen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dat ik jammerlyk <rhyme label="b" type="m" corresp="#s2:l3">verviel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">In myn vleesch is spier, noch <rhyme label="a" type="f">aader</rhyme></l>
        <l id="s3:l2" met="2" type="+-">Heel te <rhyme label="a" type="f" corresp="#s3:l1">gaader</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Door uw gramschaps streng <rhyme label="b" type="m">geweld</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">Myn gebeent heeft rust, noch <rhyme label="c" type="f">vreede</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Door de <rhyme label="c" type="f" corresp="#s3:l4">sneede</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Van myn sonde, die het <rhyme label="b" type="m" corresp="#s3:l3">quelt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Want myn onrecht <rhyme label="a" type="f">aangewassen</rhyme></l>
        <l id="s4:l2" met="2" type="+-">Gaat, met <rhyme label="a" type="f" corresp="#s4:l1">plassen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Langs myn hoofd, als baar, aan <rhyme label="b" type="m">baar</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">'t Is voor my, om haare <rhyme label="c" type="f">plaagen</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Meer te <rhyme label="c" type="f" corresp="#s4:l4">draagen</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Als een swaare last, te <rhyme label="b" type="m" corresp="#s4:l3">swaar</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Myn geswoore blaauwe <rhyme label="a" type="f">builen</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Aan 't <rhyme label="a" type="f" corresp="#s5:l1">vervuilen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">S[...]ken ettrig, en <rhyme label="b" type="m">verrot</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Van den dwaasen lichten <rhyme label="c" type="f">handel</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">In myn <rhyme label="c" type="f" corresp="#s5:l4">wandel</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Teegen mynen goeden <rhyme label="b" type="m" corresp="#s5:l3">God</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ik ben krom van <rhyme label="a" type="f">onvermoogen</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Seer <rhyme label="a" type="f" corresp="#s6:l1">geboogen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Door de trekking van myn <rhyme label="b" type="m">smart</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Als die, om gesturve <rhyme label="c" type="f">maagen</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Rouwe <rhyme label="c" type="f" corresp="#s6:l4">draagen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Gaa ik, al den dag, in 't <rhyme label="b" type="m" corresp="#s6:l3">swart</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Want myn darmen zyn vol <rhyme label="a" type="f">seeren</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s7:l1">versweeren</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Door een vuilgeachten <rhyme label="b" type="m">brand</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Niets is heel, 't is al <rhyme label="c" type="f">versturven</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Of <rhyme label="c" type="f" corresp="#s7:l4">verdurven</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">In myn vleesch, en <rhyme label="b" type="m" corresp="#s7:l3">ingewand</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Gansch verswakt, en <rhyme label="a" type="f">afgereeden</rhyme></l>
        <l id="s8:l2" met="2" type="+-">Zyn myn <rhyme label="a" type="f" corresp="#s8:l1">leeden</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Myn gebeent is seer <rhyme label="b" type="m">verbryst</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">Van het ruisschen mynes <rhyme label="c" type="f">harten</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">Om syn <rhyme label="c" type="f" corresp="#s8:l4">smarten</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Brul ik, dat men schrikt, en <rhyme label="b" type="m" corresp="#s8:l3">yst</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Al myn wensch, dien ik <rhyme label="a" type="f">begeere</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Leit, o <rhyme label="a" type="f" corresp="#s9:l1">Heere</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Voor uw oogen <rhyme label="b" type="m">uitgestrekt</rhyme>.</l>
        <l id="s9:l4" met="4" type="+-">Myn gesucht is voor uw <rhyme label="c" type="f">ooren</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Die steeds <rhyme label="c" type="f" corresp="#s9:l4">hooren</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Niet verborgen, noch <rhyme label="b" type="m" corresp="#s9:l3">bedekt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Laas, myn hart swaait gins, en <rhyme label="a" type="f">weeder</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-">Op, en <rhyme label="a" type="f" corresp="#s10:l1">needer</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Myne kracht verlietme <rhyme label="b" type="m">gants</rhyme>.</l>
        <l id="s10:l4" met="4" type="+-">'t Helle licht van ber myn <rhyme label="c" type="f">oogen</rhyme></l>
        <l id="s10:l5" met="2" type="+-">Is <rhyme label="c" type="f" corresp="#s10:l4">vervloogen</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Jaa ik mis hun siende <rhyme label="b" type="m" corresp="#s10:l3">glants</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Die my korts, uit liefde, <rhyme label="a" type="f">dienden</rhyme>,</l>
        <l id="s11:l2" met="2" type="+-">En myn <rhyme label="a" type="f" corresp="#s11:l1">vrienden</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Staan ter sy van my van <rhyme label="b" type="m">daan</rhyme>.</l>
        <l id="s11:l4" met="4" type="+-">Self myn naabestaande <rhyme label="c" type="f">maagen</rhyme></l>
        <l id="s11:l5" met="2" type="+-">Sien myn <rhyme label="c" type="f" corresp="#s11:l4">plaagen</rhyme></l>
        <l id="s11:l6" met="4" type="+-">Slechs van ver, en blyven <rhyme label="b" type="m" corresp="#s11:l3">staan</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">En die naa myn ziele <rhyme label="a" type="f">mikken</rhyme></l>
        <l id="s12:l2" met="2" type="+-">Leggen <rhyme label="a" type="f" corresp="#s12:l1">strikken</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Ieder soeker van myn <rhyme label="b" type="m">quaad</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Spreekt iet schalks, om my te <rhyme label="c" type="f">krenken</rhyme>,</l>
        <l id="s12:l5" met="2" type="+-">Sy <rhyme label="c" type="f" corresp="#s12:l4">bedenken</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Al den dag, een argen <rhyme label="b" type="m" corresp="#s12:l3">raad</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Maar ik hoor quantsuis geen <rhyme label="a" type="f">stukken</rhyme></l>
        <l id="s13:l2" met="2" type="+-">Van hun <rhyme label="a" type="f" corresp="#s13:l1">nukken</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Ben als doof, en <rhyme label="b" type="m">welgemoedt</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Als een stomme, die <rhyme label="c" type="f">beloogen</rhyme>,</l>
        <l id="s13:l5" met="2" type="+-"><rhyme label="c" type="f" corresp="#s13:l4">Onbewoogen</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Synen mond niet oopen <rhyme label="b" type="m" corresp="#s13:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Jaa, ik schyne, met twee <rhyme label="a" type="f">ooren</rhyme>,</l>
        <l id="s14:l2" met="2" type="+-">Niets te <rhyme label="a" type="f" corresp="#s14:l1">hooren</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Als een doof gebooren <rhyme label="b" type="m">man</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Welkers mond, hoewel <rhyme label="c" type="f">bestreeden</rhyme>,</l>
        <l id="s14:l5" met="2" type="+-">Niet een <rhyme label="c" type="f" corresp="#s14:l4">reeden</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Noch goed weerwoord uitten <rhyme label="b" type="m" corresp="#s14:l3">kan</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Want ik hoop op u, o <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s15:l2" met="2" type="+-">Stoor, noch <rhyme label="a" type="f" corresp="#s15:l1">keere</rhyme></l>
        <l id="s15:l3" met="4" type="+-">My aan laster, smaad, noch <rhyme label="b" type="m">spot</rhyme>.</l>
        <l id="s15:l4" met="4" type="+-">Gy zult, Heere, voor myn <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s15:l5" met="2" type="+-">Antwoord <rhyme label="c" type="f" corresp="#s15:l4">geeven</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">Als myn voorspraak, borg, en <rhyme label="b" type="m" corresp="#s15:l3">God</rhyme>,</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Want ik sei, dat myn <rhyme label="a" type="f">benyder</rhyme>,</l>
        <l id="s16:l2" met="2" type="+-">En <rhyme label="a" type="f" corresp="#s16:l1">bestryder</rhyme></l>
        <l id="s16:l3" met="4" type="+-">Sich niet, om myn val, <rhyme label="b" type="m">verbly</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Zoo myn voet aan 't wanklen <rhyme label="c" type="f">raakte</rhyme>,</l>
        <l id="s16:l5" met="2" type="+-">Sie, hy <rhyme label="c" type="f" corresp="#s16:l4">maakte</rhyme></l>
        <l id="s16:l6" met="4" type="+-">Sich te grooter teegen <rhyme label="b" type="m" corresp="#s16:l3">my</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="+-">Want ik ben gereed tot <rhyme label="a" type="f">hinken</rhyme>,</l>
        <l id="s17:l2" met="2" type="+-">Jaa tot <rhyme label="a" type="f" corresp="#s17:l1">sinken</rhyme>,</l>
        <l id="s17:l3" met="4" type="+-">Van het pak dat my <rhyme label="b" type="m">beswaart</rhyme>,</l>
        <l id="s17:l4" met="4" type="+-">Aangesien de schuld der <rhyme label="c" type="f">smarten</rhyme></l>
        <l id="s17:l5" met="2" type="+-">Mynes <rhyme label="c" type="f" corresp="#s17:l4">harten</rhyme></l>
        <l id="s17:l6" met="4" type="+-">Steeds ook voor myn oogen <rhyme label="b" type="m" corresp="#s17:l3">waart</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="+-">Want myn ongerechtig <rhyme label="a" type="f">heeden</rhyme>,</l>
        <l id="s18:l2" met="2" type="+-">U <rhyme label="a" type="f" corresp="#s18:l1">beleeden</rhyme>,</l>
        <l id="s18:l3" met="4" type="+-">Wys ik my geduurig <rhyme label="b" type="m">aan</rhyme>.</l>
        <l id="s18:l4" met="4" type="+-">Door myn sonden heel <rhyme label="c" type="f">beslommert</rhyme>,</l>
        <l id="s18:l5" met="2" type="+-">En <rhyme label="c" type="f" corresp="#s18:l4">bekommert</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">Ben ik seer daar in <rhyme label="b" type="m" corresp="#s18:l3">begaan</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="+-">Maar myn vyands schaaren <rhyme label="a" type="f">streeven</rhyme>,</l>
        <l id="s19:l2" met="2" type="+-">Vol van <rhyme label="a" type="f" corresp="#s19:l1">leeven</rhyme>,</l>
        <l id="s19:l3" met="4" type="+-">Magtiger, en nimmer <rhyme label="b" type="m">moe</rhyme>,</l>
        <l id="s19:l4" met="4" type="+-">Die, uit haat, om valsch <rhyme label="c" type="f">verwyten</rhyme>,</l>
        <l id="s19:l5" met="2" type="+-">My <rhyme label="c" type="f" corresp="#s19:l4">verbyten</rhyme>,</l>
        <l id="s19:l6" met="4" type="+-">Werden groot, en neemen <rhyme label="b" type="m" corresp="#s19:l3">toe</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="+-">Die my quaad, voor goed, <rhyme label="a" type="f">vergelden</rhyme>,</l>
        <l id="s20:l2" met="2" type="+-">En bits <rhyme label="a" type="f" corresp="#s20:l1">schelden</rhyme>,</l>
        <l id="s20:l3" met="4" type="+-">Doen alsins my <rhyme label="b" type="m">weederstand</rhyme>.</l>
        <l id="s20:l4" met="4" type="+-">Wyl de 't goede, met <rhyme label="c" type="f">behaagen</rhyme>,</l>
        <l id="s20:l5" met="2" type="+-">Naa blyf <rhyme label="c" type="f" corresp="#s20:l4">jaagen</rhyme>,</l>
        <l id="s20:l6" met="4" type="+-">Zynse teegen my <rhyme label="b" type="m" corresp="#s20:l3">gekant</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="+-">Heer, wil gy my nimmer <rhyme label="a" type="f">haaten</rhyme>,</l>
        <l id="s21:l2" met="2" type="+-">Noch <rhyme label="a" type="f" corresp="#s21:l1">verlaaten</rhyme>,</l>
        <l id="s21:l3" met="4" type="+-">Schoon een ieder my <rhyme label="b" type="m">begaf</rhyme>.</l>
        <l id="s21:l4" met="4" type="+-">O myn God, dewyl myn <rhyme label="c" type="f">klagten</rhyme></l>
        <l id="s21:l5" met="2" type="+-">U ve <rhyme label="c" type="f" corresp="#s21:l4">wachten</rhyme>,</l>
        <l id="s21:l6" met="4" type="+-">Zy niet verre van my <rhyme label="b" type="m" corresp="#s21:l3">af</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="+-">Kom my van dit kranke <rhyme label="a" type="f">weesen</rhyme></l>
        <l id="s22:l2" met="2" type="+-">Haast <rhyme label="a" type="f" corresp="#s22:l1">geneesen</rhyme>,</l>
        <l id="s22:l3" met="4" type="+-">Help myn ziel uit alle <rhyme label="b" type="m">stryd</rhyme>,</l>
        <l id="s22:l4" met="4" type="+-">Gy kunt, naa dit droevig <rhyme label="c" type="f">lyden</rhyme>,</l>
        <l id="s22:l5" met="2" type="+-">My <rhyme label="c" type="f" corresp="#s22:l4">verblyden</rhyme>,</l>
        <l id="s22:l6" met="4" type="+-">Wyl gy, Heer, myn heiland <rhyme label="b" type="m" corresp="#s22:l3">zyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
