<song>
  <meta>
    <id>185906</id>
    <date>
      <year-start>1790</year-start>
      <year-end>1790</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Kristallijnen beek</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ô LEnte <rhyme label="a" type="m">Tyd</rhyme>!</l>
        <l id="s1:l2" met="2" type="-+">Uw <rhyme label="a" type="m" corresp="#s1:l1">lieflykheid</rhyme></l>
        <l id="s1:l3" met="5" type="+-">Maekt den Mensch als <rhyme label="b" type="f">opgetogen</rhyme>;</l>
        <l id="s1:l4" met="3" type="+-">Gy vernieuwd elks <rhyme label="c" type="f">Leeven</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Gy kunt vreugde <rhyme label="c" type="f" corresp="#s1:l4">geven</rhyme>!</l>
        <l id="s1:l6" met="3" type="+-">Doet de Winter <rhyme label="c" type="f" corresp="#s1:l4">sneven</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Zonder <rhyme label="c" type="f" corresp="#s1:l4">tegenstreven</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Uw glans <rhyme label="d" type="m">alleen</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Steld ons te <rhyme label="d" type="m" corresp="#s1:l8">vreen</rhyme>;</l>
        <l id="s1:l10" met="5" type="+-">Wie erkend niet steeds uw zoet <rhyme label="b" type="f" corresp="#s1:l3">vermogen</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Uw Lustig <rhyme label="a" type="m">Groen</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Kan Lusten <rhyme label="a" type="m" corresp="#s2:l1">voen</rhyme>;</l>
        <l id="s2:l3" met="5" type="+-">En de Ziel een waer vernoegen <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">'k Mag de bloei der <rhyme label="c" type="f">boomen</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Vrolyk op zien <rhyme label="c" type="f" corresp="#s2:l4">komen</rhyme>;</l>
        <l id="s2:l6" met="3" type="+-">'k Zie de zilvre <rhyme label="c" type="f" corresp="#s2:l4">Stromen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Vlieten langs de <rhyme label="c" type="f" corresp="#s2:l4">Zomen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Uw lieflyk <rhyme label="d" type="m">zoet</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">Dat vreugde <rhyme label="d" type="m" corresp="#s2:l8">voed</rhyme></l>
        <l id="s2:l10" met="5" type="+-">Doet en Mensch en Vee gelyk <rhyme label="b" type="f" corresp="#s2:l3">herleven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ach! mogt myn <rhyme label="a" type="m">Oog</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">In deze <rhyme label="a" type="m" corresp="#s3:l1">boog</rhyme></l>
        <l id="s3:l3" met="5" type="+-">Ook myn Zielsvoogdesse eens <rhyme label="b" type="f">gewinnen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">'K zou dan adem <rhyme label="c" type="f">halen</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Door haer <rhyme label="c" type="f" corresp="#s3:l4">Flonkerstralen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Nimmer te <rhyme label="c" type="f" corresp="#s3:l4">bepalen</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Nimmer af te <rhyme label="c" type="f" corresp="#s3:l4">malen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">ô Lieve <rhyme label="d" type="m">Maegd</rhyme>;</l>
        <l id="s3:l9" met="2" type="-+">Die my <rhyme label="d" type="m" corresp="#s3:l8">behaegd</rhyme>:</l>
        <l id="s3:l10" met="5" type="+-">'K zweer, ik zal uw Schoonheid eeuwig <rhyme label="b" type="f" corresp="#s3:l3">minnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
