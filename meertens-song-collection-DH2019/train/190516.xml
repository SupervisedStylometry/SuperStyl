<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190516</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Waak op waak op Jeruzalem</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">De Patriotten zyn in <rhyme label="a" type="m">nood</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zy treuren en zy <rhyme label="b" type="f">klaagen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">'t Defensiewezen is reeds <rhyme label="a" type="m" corresp="#s1:l1">dood</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hun Volkstem legt <rhyme label="b" type="f" corresp="#s1:l2">verslagen</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Hun Collonelen <rhyme label="c" type="m">afgezet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Garnaalen Krygsraad legt <rhyme label="c" type="m" corresp="#s1:l5">verplet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">'t Verlies is niet te <rhyme label="d" type="f">dragen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Tien heeft de Klok <rhyme label="d" type="f" corresp="#s1:l7">geslagen</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Weg Amstels dappere <rhyme label="a" type="m">Societeit</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Weg gy Genootschaps <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Wy zyn de Landsverdervers <rhyme label="a" type="m" corresp="#s2:l1">kwyt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wy hebben hun <rhyme label="b" type="f" corresp="#s2:l2">Geweeren</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Nu kan het Patriots <rhyme label="c" type="m">gebroet</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Die Dieven van der braven <rhyme label="c" type="m" corresp="#s2:l5">goed</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Den Burger niet meer <rhyme label="d" type="f">plagen</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Elf heeft de Klok <rhyme label="d" type="f" corresp="#s2:l7">geslagen</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Weg Ruiters van het <rhyme label="a" type="m">Koningsplein</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Weg gy brutaale <rhyme label="b" type="f">snaaken</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Gy mist den Wafelkraam, ô <rhyme label="a" type="m" corresp="#s3:l1">pyn</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wie zal die nu <rhyme label="b" type="f" corresp="#s3:l2">bewaaken</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Gy kreeg u Paspoort voor <rhyme label="c" type="m">altyd</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Gy zyt u Paard en Glans nu <rhyme label="c" type="m" corresp="#s3:l5">kwyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Gaat Vader Hooft nu <rhyme label="d" type="f">klaagen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Twaalf heeft de Klok <rhyme label="d" type="f" corresp="#s3:l7">geslaagen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vivat Oranje is <rhyme label="a" type="m">herstelt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Met Neêrlands brave <rhyme label="b" type="f">Heeren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">De Kezen zyn ter neer <rhyme label="a" type="m" corresp="#s4:l1">gevelt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">De Deugd zal <rhyme label="b" type="f" corresp="#s4:l2">triumpheeren</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Wy brave blyven steeds <rhyme label="c" type="m">getrouw</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Aan de Overheid en aan <rhyme label="c" type="m" corresp="#s4:l5">Nassauw</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Waar voor we ons leven <rhyme label="d" type="f">wagen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Een heeft de Klok <rhyme label="d" type="f" corresp="#s4:l7">geslagen</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Vivat wy leeven nu in <rhyme label="a" type="m">vreugt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">'t Regentendom ter <rhyme label="b" type="f">eeren</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Oranjes heil ons hert <rhyme label="a" type="m" corresp="#s5:l1">verheugt</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Des wy vry <rhyme label="b" type="f" corresp="#s5:l2">Illumineeren</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Oranje draagen w' op den <rhyme label="c" type="m">hoed</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Hoe 't Keezendom ook raast en <rhyme label="c" type="m" corresp="#s5:l5">woed</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Zy moeten 't zelfs <rhyme label="d" type="f">draagen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Twee heeft de Klok <rhyme label="d" type="f" corresp="#s5:l7">geslaagen</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Vivat Amstels <rhyme label="a" type="m">Magistraat</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Wier trouw met haar moet <rhyme label="b" type="f">stryden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Den braven Burger <rhyme label="a" type="m" corresp="#s6:l1">toeverlaat</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wier heil ons hart <rhyme label="b" type="f" corresp="#s6:l2">verblyden</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Vivat wy offeren hen ons <rhyme label="c" type="m">bloed</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">God heeft hem voor 't gewelt <rhyme label="c" type="m" corresp="#s6:l5">behoed</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die rekke hun levens <rhyme label="d" type="f">daagen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Drie heeft de Klok <rhyme label="d" type="f" corresp="#s6:l7">geslaagen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dus juigen door gantsch <rhyme label="a" type="m">Leeuwendaal</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">d'Oranjes ware <rhyme label="b" type="f">Vrienden</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Dus triumpheeren <rhyme label="a" type="m" corresp="#s7:l1">overal</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wie regt en deugd <rhyme label="b" type="f" corresp="#s7:l2">beminden</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Dus juicht den braven <rhyme label="c" type="m">Burgery</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Daar zy tot heil der <rhyme label="c" type="m" corresp="#s7:l5">Maatschappy</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">d'Oranje Zon zien <rhyme label="d" type="f">daagen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Vier heeft de Klok <rhyme label="d" type="f" corresp="#s7:l7">geslaagen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
