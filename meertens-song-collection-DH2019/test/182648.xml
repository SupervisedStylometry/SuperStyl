  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">OCh mijn lieve <rhyme label="a" type="m">Coridon</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m">derida</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Trouw je my niet gy moet weeten <rhyme label="a" type="m" corresp="#s1:l1">waarom</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s1:l2">derida</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Mind gy een ander liever als <rhyme label="c" type="m">mijn</rhyme>?</l>
        <l id="s1:l6" met="3" type="+-">Ia, ja, liever als <rhyme label="c" type="m" corresp="#s1:l5">mijn</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Zegt Coridon wat reden mogen het <rhyme label="c" type="m" corresp="#s1:l5">zijn</rhyme>?</l>
        <l id="s1:l8" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s1:l2">derida</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Heeft Cupidoos zoete <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m">derida</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Uw zo geschoten ten Boezem <rhyme label="a" type="m" corresp="#s2:l1">in</rhyme>?</l>
        <l id="s2:l4" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s2:l2">derida</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Dat gy uwe trouw' zelfs <rhyme label="c" type="m">prezenteerd</rhyme>;</l>
        <l id="s2:l6" met="3" type="+-">Ia, ja, zelfs <rhyme label="c" type="m" corresp="#s2:l5">geprezenteerd</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Zulke Meysjes worden niet <rhyme label="c" type="m" corresp="#s2:l5">begeerd</rhyme>:</l>
        <l id="s2:l8" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s2:l2">deria</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Zijn dat de belofte? Ã´ <rhyme label="a" type="m">Coridon</rhyme>!</l>
        <l id="s3:l2" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m">derida</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Als gy plukte mijn <rhyme label="a" type="m" corresp="#s3:l1">Maagde-blom</rhyme>;</l>
        <l id="s3:l4" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s3:l2">derida</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Onder de zeden al van de <rhyme label="c" type="m">Trouw</rhyme>;</l>
        <l id="s3:l6" met="3" type="+-">Ia, ja, al van de <rhyme label="c" type="m" corresp="#s3:l5">Trouw</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Zult gy my nu laten in den <rhyme label="c" type="m" corresp="#s3:l5">rouw</rhyme>,</l>
        <l id="s3:l8" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s3:l2">derida</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Weg onnozele <rhyme label="a" type="m">Venus-kind</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m">derida</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Heeft u de Liefde het Hert <rhyme label="a" type="m" corresp="#s4:l1">verblind</rhyme>?</l>
        <l id="s4:l4" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s4:l2">derida</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Ik heb u gebreyd dat is wel <rhyme label="c" type="m">waar</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Ia, ja, dat is wel <rhyme label="c" type="m" corresp="#s4:l5">waar</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">Trouwe zoete Meyd, dat 's ver van <rhyme label="c" type="m" corresp="#s4:l5">daar</rhyme>:</l>
        <l id="s4:l8" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s4:l2">derida</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ach Coridon! Kund gy de <rhyme label="a" type="m">Min</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m">derida</rhyme></l>
        <l id="s5:l3" met="4" type="+-">Zo ligt stellen uyt uw' <rhyme label="a" type="m" corresp="#s5:l1">zin</rhyme>?</l>
        <l id="s5:l4" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s5:l2">derida</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Laat gy der my dan in den <rhyme label="c" type="m">rouw</rhyme>?</l>
        <l id="s5:l6" met="3" type="+-">Ia, ja, al in den <rhyme label="c" type="m" corresp="#s5:l5">rouw</rhyme>;</l>
        <l id="s5:l7" met="4" type="+-">Zo ben 'k geen Vrijster en ook geen <rhyme label="c" type="m" corresp="#s5:l5">Vrouw</rhyme>,</l>
        <l id="s5:l8" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s5:l2">derida</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zoete Susje maakt geen <rhyme label="a" type="m">verdriet</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m">derida</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Gy zijt de eerste of de laatste <rhyme label="a" type="m" corresp="#s6:l1">niet</rhyme>;</l>
        <l id="s6:l4" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s6:l2">derida</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Veele Meysjes al in ons <rhyme label="c" type="m">Land</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Ia, ja, al in ons <rhyme label="c" type="m" corresp="#s6:l5">land</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Die door de belofte blijven in <rhyme label="c" type="m" corresp="#s6:l5">schand</rhyme>;</l>
        <l id="s6:l8" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s6:l2">derida</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Coridon door u loos <rhyme label="a" type="m">bedrog</rhyme></l>
        <l id="s7:l2" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m">derida</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Hebt gy my in 't verdriet <rhyme label="a" type="m" corresp="#s7:l1">gebrogt</rhyme>;</l>
        <l id="s7:l4" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s7:l2">derida</rhyme>;</l>
        <l id="s7:l5" met="4" type="+-">Nu dat 'er mijn Eertje is van <rhyme label="c" type="m">kant</rhyme>,</l>
        <l id="s7:l6" met="3" type="+-">Ia, ja, nu is van <rhyme label="c" type="m" corresp="#s7:l5">Kant</rhyme>;</l>
        <l id="s7:l7" met="4" type="+-">Laat gy Bedrieger myin <rhyme label="c" type="m" corresp="#s7:l5">schand</rhyme>:</l>
        <l id="s7:l8" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s7:l2">derida</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Oorlof Dogters wie gy <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m">derida</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Als u eenen Minnaar <rhyme label="a" type="m" corresp="#s8:l1">vrijd</rhyme></l>
        <l id="s8:l4" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s8:l2">derida</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Doet dog dan niet als deze <rhyme label="c" type="m">Maagd</rhyme>:</l>
        <l id="s8:l6" met="3" type="+-">Ia, ja, als deze <rhyme label="c" type="m" corresp="#s8:l5">Maagd</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Eer gy het u te laat <rhyme label="c" type="m" corresp="#s8:l5">beklaagt</rhyme>:</l>
        <l id="s8:l8" met="5" type="-+">Fidon, fidon van falla <rhyme label="b" type="m" corresp="#s8:l2">derida</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
