  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">MYn Hart dat is my heel <rhyme label="a" type="m">ontstelt</rhyme>,</l>
        <l id="s1:l2" met="12" type="+-">Tiere la la la tiere la la la tiere la tiere liere liere <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vermits dat my de min steets <rhyme label="a" type="m" corresp="#s1:l1">quelt</rhyme>,</l>
        <l id="s1:l4" met="6" type="+-">Tiere la tiere liere liere <rhyme label="b" type="m" corresp="#s1:l2">la</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">O min wat baert gh' in <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Een biter soete <rhyme label="c" type="m" corresp="#s1:l5">pijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De min die baert oock wel wat <rhyme label="a" type="m">lust</rhyme>,</l>
        <l id="s2:l2" met="12" type="+-">Tiere la la la tiere la la la tiere la tiere liere liere <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maer noyt en laet sy my in <rhyme label="a" type="m" corresp="#s2:l1">rust</rhyme>,</l>
        <l id="s2:l4" met="6" type="+-">Tiere la tiere liere liere <rhyme label="b" type="m" corresp="#s2:l2">la</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">De min die pijnt mijn <rhyme label="c" type="m">hart</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Met onverdulde <rhyme label="c" type="m" corresp="#s2:l5">smart</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Des Avonts als ick rusten <rhyme label="a" type="m">wil</rhyme>,</l>
        <l id="s3:l2" met="12" type="+-">Tiere la la la tiere la la la tiere la tiere liere liere <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En laet sy my niet slapen <rhyme label="a" type="m" corresp="#s3:l1">stil</rhyme>,</l>
        <l id="s3:l4" met="6" type="+-">Tiere la tiere liere liere <rhyme label="b" type="m" corresp="#s3:l2">la</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maer sy comt quellen <rhyme label="c" type="m">dan</rhyme></l>
        <l id="s3:l6" met="3" type="-+">My dat ick niet slapen <rhyme label="c" type="m" corresp="#s3:l5">can</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Als ick nu al int bedde <rhyme label="a" type="m">legh</rhyme>,</l>
        <l id="s4:l2" met="12" type="+-">Tiere la la la tiere la la la tiere la tiere liere liere <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dan gaet de min niet van my <rhyme label="a" type="m" corresp="#s4:l1">wegh</rhyme>,</l>
        <l id="s4:l4" met="6" type="+-">Tiere la tiere liere liere <rhyme label="b" type="m" corresp="#s4:l2">la</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Maer dan comt sitten <rhyme label="c" type="m">in</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Mijn Hart de gayle <rhyme label="c" type="m" corresp="#s4:l5">Min</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Wanneer als ick al slaep met <rhyme label="a" type="m">schroom</rhyme>,</l>
        <l id="s5:l2" met="12" type="+-">Tiere la la la tiere la la la tiere la tiere liere liere <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">My dunckt ick sie haer in mijn <rhyme label="a" type="m" corresp="#s5:l1">droom</rhyme>,</l>
        <l id="s5:l4" met="6" type="+-">TIere la tiere liere liere <rhyme label="b" type="m" corresp="#s5:l2">la</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Waer door dat ick <rhyme label="c" type="m">ontwaeck</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En uyt mijn slaep <rhyme label="c" type="m" corresp="#s5:l5">gheraeck</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als de langhe nacht is <rhyme label="a" type="m">verby</rhyme>,</l>
        <l id="s6:l2" met="12" type="+-">Tiere la la la tiere la la la tiere la tiere liere liere <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">So laet sy niet met vrede <rhyme label="a" type="m" corresp="#s6:l1">my</rhyme></l>
        <l id="s6:l4" met="6" type="+-">Tiere la tiere liere liere <rhyme label="b" type="m" corresp="#s6:l2">la</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">En my pijnt sy weer <rhyme label="c" type="m">an</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Meer als ick lyden <rhyme label="c" type="m" corresp="#s6:l5">can</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ick heb my nau te deegh <rhyme label="a" type="m">gecleet</rhyme>,</l>
        <l id="s7:l2" met="12" type="+-">Tiere la la la tiere la la la tiere la tiere liere liere <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Of ick ghevoel het branden <rhyme label="a" type="m" corresp="#s7:l1">heet</rhyme></l>
        <l id="s7:l4" met="6" type="+-">Tiere la tiere liere liere <rhyme label="b" type="m" corresp="#s7:l2">la</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Der min die mijn hart <rhyme label="c" type="m">brant</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Te niet en heel tot <rhyme label="c" type="m" corresp="#s7:l5">schant</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Prince yemant mocht maghe <rhyme label="a" type="m">wat</rhyme></l>
        <l id="s8:l2" met="12" type="+-">Tiere la la la tiere la la la tiere la tiere liere liere <rhyme label="b" type="m">la</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">De min is segghend' wat is <rhyme label="a" type="m" corresp="#s8:l1">dat</rhyme>,</l>
        <l id="s8:l4" met="6" type="+-">Tiere la tiere liere liere <rhyme label="b" type="m" corresp="#s8:l2">la</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Soo gheeft hem voor <rhyme label="c" type="m">bescheyt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">'tIs liefdens <rhyme label="c" type="m" corresp="#s8:l5">vriendelijckheyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
