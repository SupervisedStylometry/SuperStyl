<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22280</id>
    <date>
      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Meine Seele erhebt den Herrn mein</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WT gheheelder herten danck ick <rhyme label="a" type="m">u</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En wil u Heer <rhyme label="b" type="f">loffsinghen</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Voor den Goden: want ghi my <rhyme label="a" type="m" corresp="#s1:l1">nu</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Bystaet in alle <rhyme label="b" type="f" corresp="#s1:l2">dinghen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Daerom wil ick <rhyme label="c" type="m">nu</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Aenbidden <rhyme label="c" type="m" corresp="#s1:l5">u</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">In uwen heylighen <rhyme label="d" type="f">Tempel</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En dancken <rhyme label="e" type="m">seer</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Uwen naem <rhyme label="e" type="m" corresp="#s1:l8">Heer</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Voor u trouwe, en u goetheyt <rhyme label="d" type="f" corresp="#s1:l7">wel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Want ghy hebt uwen name <rhyme label="a" type="m">fyn</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ghemaeckt seer groot en <rhyme label="b" type="f">heerlijck</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ouer alle dinck, door de woorden <rhyme label="a" type="m" corresp="#s2:l1">dijn</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dwelck bespotten de menschen <rhyme label="b" type="f" corresp="#s2:l2">alglijck</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Maer so <rhyme label="c" type="m">wanneer</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Ick aenroep u <rhyme label="c" type="m" corresp="#s2:l5">Heer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">So wilt doch my <rhyme label="d" type="f">verhooren</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En wilt doch <rhyme label="e" type="m">snel</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Mijn Siel laten <rhyme label="e" type="m" corresp="#s2:l8">wel</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Door u groote cracht <rhyme label="d" type="f" corresp="#s2:l7">oorbooren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Alle Coningen op aerden die dancken u <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En den hoochtsen prijs sy u <rhyme label="b" type="f">gheuen</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Om dat sy ws mondts woort, hebben <rhyme label="a" type="m" corresp="#s3:l1">gehoort</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En singhen op alle de <rhyme label="b" type="f" corresp="#s3:l2">weghen</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Vanden Heere <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Op dat dEere <rhyme label="c" type="m" corresp="#s3:l5">zijn</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Groot sy, en <rhyme label="d" type="f">onwtspreecklijck</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Al is Godt <rhyme label="e" type="m">hooch</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Noch siet hy <rhyme label="e" type="m" corresp="#s3:l8">doch</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Op die needrighe van het <rhyme label="d" type="f" corresp="#s3:l7">eerdtrijck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En hy kendt oock die stouten <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Van vers, en oock van <rhyme label="b" type="f">wijde</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Al wandelden ick schoon in droeffheyt <rhyme label="a" type="m" corresp="#s4:l1">al</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Onder tquaet volck aen alle <rhyme label="b" type="f" corresp="#s4:l2">zijden</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Soo sult ghy my <rhyme label="c" type="m">doch</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Onder dat <rhyme label="c" type="m" corresp="#s4:l5">Joch</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wel leuende <rhyme label="d" type="f">behouwen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">En met u <rhyme label="e" type="m">Handt</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Mijnder <rhyme label="e" type="m" corresp="#s4:l8">Vyandt</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Gramschap, ontstucken <rhyme label="d" type="f" corresp="#s4:l7">houwen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">My sal wel helpen u Rechte <rhyme label="a" type="m">handt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En eynden mijne <rhyme label="b" type="f">saken</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Want Heer ghy condt het doen <rhyme label="a" type="m" corresp="#s5:l1">vaeliandt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ghy sullet voor my wel <rhyme label="b" type="f" corresp="#s5:l2">maken</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Heer u <rhyme label="c" type="m">goetheyt</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Duert in <rhyme label="c" type="m" corresp="#s5:l5">eewicheyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Daerom laet niet om uwen <rhyme label="d" type="f">name</rhyme>:</l>
        <l id="s5:l8" met="2" type="-+">Dwerck van u <rhyme label="e" type="m">handt</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Comen in <rhyme label="e" type="m" corresp="#s5:l8">schandt</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Door Jhesum Christum, <rhyme label="d" type="f" corresp="#s5:l7">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
