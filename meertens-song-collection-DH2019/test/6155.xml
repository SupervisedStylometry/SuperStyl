        <l id="s1:l1" met="3" type="-+">IN 'Shemels hooghste <rhyme label="a" type="m">hoff</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Zy God eer ende <rhyme label="a" type="m" corresp="#s1:l1">loff</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Gheluck zy op der <rhyme label="b" type="f">aerde</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Want God heeft goed <rhyme label="c" type="m">ghedacht</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Het menschelick <rhyme label="c" type="m" corresp="#s1:l4">gheslacht</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Tot zijne gunst <rhyme label="b" type="f" corresp="#s1:l3">t'aenvaerden</rhyme>.</l>
