  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ach myn alderliefste <rhyme label="a" type="m">zoet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Staet op en doet eens <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ach myn alderliefste <rhyme label="a" type="m" corresp="#s1:l1">zoet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Staet op met goed <rhyme label="a" type="m" corresp="#s1:l1">gemoed</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Ik zal u geeven myn hand op <rhyme label="c" type="m">trouw</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Om te leven als man en <rhyme label="c" type="m" corresp="#s1:l5">vrouw</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Ik kom hier uit liefde <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Tog mynen wil <rhyme label="d" type="m" corresp="#s1:l7">voldoet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wie hoor ik hier voor myn <rhyme label="a" type="m">deur</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die my heeft op <rhyme label="b" type="f">geroepen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">My dunkt 't is myn <rhyme label="a" type="m" corresp="#s2:l1">zerviteur</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met bloemen zoet van <rhyme label="a" type="m" corresp="#s2:l1">geur</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Hy zal die met de zoeten <rhyme label="c" type="m">tyd</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Door liefde en <rhyme label="c" type="m" corresp="#s2:l5">minsaemheid</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">In myn hofken planten <rhyme label="d" type="m">gaen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat staet my zeer wel <rhyme label="d" type="m" corresp="#s2:l7">aan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Lief doet open want <rhyme label="a" type="m">verstaet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ik ben een huys moop <rhyme label="b" type="f">keerel</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Met wie het niet kwalyk <rhyme label="a" type="m" corresp="#s3:l1">gaet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Maer ik ben zeer wel in <rhyme label="a" type="m" corresp="#s3:l1">staet</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Ik ben eenen <rhyme label="c" type="m">hovenier</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Die kan zaeijen met <rhyme label="c" type="m" corresp="#s3:l5">plaisier</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Ik zal u hofken voor <rhyme label="d" type="m">gewis</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Beplanten wonder <rhyme label="d" type="m" corresp="#s3:l7">fris</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Hovernier ik hoor u <rhyme label="a" type="m">spraek</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En hebt verstaen u <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Hovenier ik hoor u <rhyme label="a" type="m" corresp="#s4:l1">spraek</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Die myn baert groot <rhyme label="a" type="m" corresp="#s4:l1">vermaek</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dees u grote liefde <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Die verheugt geheel myn <rhyme label="c" type="m" corresp="#s4:l5">gemoet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Daerom naer u wil <rhyme label="d" type="m">beplant</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Myn hof ten alle <rhyme label="d" type="m" corresp="#s4:l7">kant</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Ach hoe zoet is uwen <rhyme label="a" type="m">Hof</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ik zal het wel <rhyme label="b" type="f">beplanten</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En vercieren tot u <rhyme label="a" type="m" corresp="#s5:l1">lof</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met bloemen-meel van <rhyme label="a" type="m" corresp="#s5:l1">stof</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Ach myn liefste <rhyme label="c" type="m">Engelin</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">U hofken is naer mynen <rhyme label="c" type="m" corresp="#s5:l5">zin</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">'k Wou dat myn zoetste <rhyme label="d" type="m">spruit</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Daer nooit moest komen <rhyme label="d" type="m" corresp="#s5:l7">uit</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Alderliefste <rhyme label="a" type="m">hovenier</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Den hof is voor u <rhyme label="b" type="f">open</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Alderliefste <rhyme label="a" type="m" corresp="#s6:l1">hovenier</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Trekt daer in met <rhyme label="a" type="m" corresp="#s6:l1">plaisier</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Wy zullen zo het <rhyme label="c" type="m">behoord</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Maken zamen nu <rhyme label="c" type="m" corresp="#s6:l5">acoord</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Om te leven in den <rhyme label="d" type="m">trouw</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Gelijk als man en <rhyme label="d" type="m" corresp="#s6:l7">Vrouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
