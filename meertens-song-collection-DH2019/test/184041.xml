  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">De nos Loges <rhyme label="a" type="f">chéries</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Nous chassons le <rhyme label="b" type="m">souci</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Les sombres <rhyme label="a" type="f" corresp="#s1:l1">révéries</rhyme></l>
        <l id="s1:l4" met="3" type="-+">N'habitent point <rhyme label="b" type="m" corresp="#s1:l2">ici</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Exempts de vains <rhyme label="c" type="m">désirs</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Sans remords ni <rhyme label="c" type="m" corresp="#s1:l5">soupirs</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Noús passons nos <rhyme label="c" type="m" corresp="#s1:l5">loisirs</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Dans d'innocens <rhyme label="c" type="m" corresp="#s1:l5">plaisirs</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Tandis que Mars <rhyme label="a" type="f">arbore</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ses funestes <rhyme label="b" type="m">drapeaux</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Nous jouissons <rhyme label="a" type="f" corresp="#s2:l1">encore</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Des douceurs du <rhyme label="b" type="m" corresp="#s2:l2">repos</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">D'un bien si <rhyme label="c" type="m">précieux</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Rendons graces aux <rhyme label="c" type="m" corresp="#s2:l5">cieux</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Et que nos <rhyme label="c" type="m" corresp="#s2:l5">envieux</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Nous reconnoissent <rhyme label="c" type="m" corresp="#s2:l5">mieux</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Déjà ce sexe <rhyme label="a" type="f">aimable</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Regrette son <rhyme label="b" type="m">erreur</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Le Maçon <rhyme label="a" type="f" corresp="#s3:l1">raisonnable</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Est un ami de <rhyme label="b" type="m" corresp="#s3:l2">coeur</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">A plaire toujours <rhyme label="c" type="m">prêt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Il vole comme un <rhyme label="c" type="m" corresp="#s3:l5">traît</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Tendre autant que <rhyme label="c" type="m" corresp="#s3:l5">discret</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Lui seul sait son <rhyme label="c" type="m" corresp="#s3:l5">secret</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">L'amitié, la <rhyme label="a" type="f">sagesse</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Font notre plus grand <rhyme label="b" type="m">bien</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Resserrons- en sans <rhyme label="a" type="f" corresp="#s4:l1">cesse</rhyme></l>
        <l id="s4:l4" met="3" type="-+">L'agréable <rhyme label="b" type="m" corresp="#s4:l2">lien</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">C'est par de si heaux <rhyme label="c" type="m">noeuds</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Que les coeurs <rhyme label="c" type="m" corresp="#s4:l5">vertueux</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Formant les mêmes <rhyme label="c" type="m" corresp="#s4:l5">voeux</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Peuvent se rendre <rhyme label="c" type="m" corresp="#s4:l5">heureux</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
