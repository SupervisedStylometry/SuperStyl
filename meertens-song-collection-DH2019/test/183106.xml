    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Van de nachtegaal</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Den Hertog moet gaan <rhyme label="a" type="f">marcheren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Verwagt de orders van de <rhyme label="a" type="f" corresp="#s1:l1">Heeren</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Om naar Amsterdam te <rhyme label="b" type="m">gaan</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">'t Is met de Patriotten <rhyme label="b" type="m" corresp="#s1:l3">gedaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Speeld op trom, fiool en <rhyme label="a" type="f">snaaren</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Wilt het Oranje-huis <rhyme label="a" type="f" corresp="#s2:l1">bewaaren</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En bevryden voor <rhyme label="b" type="m">tegenspoÃªn</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Voor Menschen die het geweld aan <rhyme label="b" type="m" corresp="#s2:l3">doen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Wilt dan maar lustig <rhyme label="a" type="f">zingen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Voor Oranje dat wy <rhyme label="a" type="f" corresp="#s3:l1">beminnen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Zingt tot zyne eer en <rhyme label="b" type="m">lof</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Tot vreugde van het <rhyme label="b" type="m" corresp="#s3:l3">Oranje-hof</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'t Is Oranje daar wy voor <rhyme label="a" type="f">spreeken</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Oranje daar wy het voor <rhyme label="a" type="f" corresp="#s4:l1">wreeken</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Oranje is de eerste <rhyme label="b" type="m">Stam</rhyme></l>
        <l id="s4:l4" met="4" type="-+">waar door 't Land in Vryheid <rhyme label="b" type="m" corresp="#s4:l3">kwam</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Oranje mag men nu weer <rhyme label="a" type="f">noemen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">De Oranje-boom die houd zyn <rhyme label="a" type="f" corresp="#s5:l1">bloemen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Oranje dat wierd wel <rhyme label="b" type="m">gehaat</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Men wou afneemen zynen <rhyme label="b" type="m" corresp="#s5:l3">staat</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Moest ik dan Oranje <rhyme label="a" type="f">laaten</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Voor veele Menschen die 't <rhyme label="a" type="f" corresp="#s6:l1">haaten</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Nu mag tog myn Vrouw of <rhyme label="b" type="m">Kind</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Draagen een <rhyme label="b" type="m" corresp="#s6:l3">Oranje-lint</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Maar nu wil ik Oranje <rhyme label="a" type="f">schryven</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En in ons herte laaten <rhyme label="a" type="f" corresp="#s7:l1">blyven</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Oranje moet tog blyven <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Nooit zal de Oranje-kleur <rhyme label="b" type="m" corresp="#s7:l3">vergaan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Oranje is weerom <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Mogt nu de twist zyn <rhyme label="a" type="f" corresp="#s8:l1">weggenomen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dan roept men met een zoet <rhyme label="b" type="m">geluid</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Lang leeve de Oranje <rhyme label="b" type="m" corresp="#s8:l3">spruit</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Al die dit liedje heeft gaan <rhyme label="a" type="f">planten</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Dat waaren ook <rhyme label="a" type="f" corresp="#s9:l1">Oranje-klanten</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">En Onderwater kloek van <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Stryd voor het lieve <rhyme label="b" type="m" corresp="#s9:l3">Oranje-bloed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
