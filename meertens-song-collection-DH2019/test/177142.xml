  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Cloris uwe <rhyme label="a" type="m">deugdzaamheên</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Zyn beroemd by <rhyme label="a" type="m" corresp="#s1:l1">iedereen</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Om uw trouw en goede <rhyme label="b" type="f">zeden</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Geest en <rhyme label="b" type="f" corresp="#s1:l3">reden</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Hebben wy wel <rhyme label="c" type="m">eêr</rhyme></l>
        <l id="s1:l6" met="4" type="+-">In het klaverryke <rhyme label="d" type="m">veld</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Uwen lof <rhyme label="d" type="m" corresp="#s1:l6">gemeld</rhyme>.</l>
      </lg>
    </lg>
