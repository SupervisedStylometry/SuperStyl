<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>19497</id>
    <date>
      <year-start>1677</year-start>
      <year-end>1677</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="spotlied" translation="mocking song">spotlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="amusement">vermaak</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WIe komt hier zoo <rhyme label="a" type="f">raazen</rhyme>!</l>
        <l id="s1:l2" met="2" type="-+">Soo snoeven, zoo <rhyme label="a" type="f" corresp="#s1:l1">blaazen</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Soo juychen, zoo <rhyme label="a" type="f">toeten</rhyme>.</l>
        <l id="s2:l2" met="2" type="-+">Voor d'eer van de <rhyme label="a" type="f" corresp="#s2:l1">Knoeten</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Een stiel van de <rhyme label="a" type="f">Karcke</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Een bulderend' <rhyme label="a" type="f" corresp="#s3:l1">Varke</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wiens toom loosen <rhyme label="a" type="f">yver</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Stijgt langer jo <rhyme label="a" type="f" corresp="#s4:l1">stijver</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Waardig (zoo't voort <rhyme label="a" type="f">gaat</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">En zijn yver 't door <rhyme label="a" type="f" corresp="#s5:l1">staat</rhyme>)</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Gekroont van 't <rhyme label="a" type="f">kanalje</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Voor HOOFT van 't <rhyme label="a" type="f" corresp="#s6:l1">rapalje</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Om met vlugge <rhyme label="a" type="f">schreden</rhyme></l>
        <l id="s7:l2" met="2" type="-+">Ter Kansel te <rhyme label="a" type="f" corresp="#s7:l1">treden</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Soo slechts maer de <rhyme label="a" type="f">muuren</rhyme></l>
        <l id="s8:l2" met="2" type="-+">Het kosten <rhyme label="a" type="f" corresp="#s8:l1">verduuren</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">U tieren. Spijt <rhyme label="a" type="f">HOPPE</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Verstomde de <rhyme label="a" type="f" corresp="#s9:l1">koppen</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">Verbreekte de <rhyme label="a" type="f">wetten</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Diens tegens u <rhyme label="a" type="f" corresp="#s10:l1">setten</rhyme>:</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">Maer wan zulcke <rhyme label="a" type="f">pylers</rhyme>,</l>
        <l id="s11:l2" met="2" type="-+">En zandvaste <rhyme label="a" type="f" corresp="#s11:l1">stylers</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="2" type="-+">Beschraagde 't <rhyme label="a" type="f">getimmer</rhyme>,</l>
        <l id="s12:l2" met="2" type="-+">Dan schroomde men <rhyme label="a" type="f" corresp="#s12:l1">nimmer</rhyme>,</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="2" type="-+">U had men te <rhyme label="a" type="f">danken</rhyme></l>
        <l id="s13:l2" met="2" type="-+">Voor 't sparen van <rhyme label="a" type="f" corresp="#s13:l1">blancken</rhyme>:</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="2" type="-+">Uw steun aen 't <rhyme label="a" type="f">gemetsel</rhyme></l>
        <l id="s14:l2" met="2" type="-+">Gaf voordeel. Geen <rhyme label="a" type="f" corresp="#s14:l1">letsel</rhyme>,</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="2" type="-+">Gy Hanssen, Gy <rhyme label="a" type="f">Lysen</rhyme>,</l>
        <l id="s15:l2" met="2" type="-+">Gy moest'et dan <rhyme label="a" type="f" corresp="#s15:l1">prijsen</rhyme>,</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="2" type="-+">Want 't was na de <rhyme label="a" type="f">mode</rhyme>,</l>
        <l id="s16:l2" met="2" type="-+">En na u <rhyme label="a" type="f" corresp="#s16:l1">metode</rhyme>;</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="2" type="-+">Soo moest gy hem <rhyme label="a" type="f">houwen</rhyme></l>
        <l id="s17:l2" met="2" type="-+">(Of't mocht u wel <rhyme label="a" type="f" corresp="#s17:l1">rouwen</rhyme>?</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="2" type="-+">Tot loon van zijn <rhyme label="a" type="f">sotheydt</rhyme>,</l>
        <l id="s18:l2" met="2" type="-+">Een beestige <rhyme label="a" type="f" corresp="#s18:l1">botheydt</rhyme>,</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="2" type="-+">Een beelt als een <rhyme label="a" type="f">tooren</rhyme>,</l>
        <l id="s19:l2" met="2" type="-+">Dat yder kon <rhyme label="a" type="f" corresp="#s19:l1">hooren</rhyme>,</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="2" type="-+">Dat elck was <rhyme label="a" type="f">besweeken</rhyme></l>
        <l id="s20:l2" met="2" type="-+">Als hy quam te <rhyme label="a" type="f" corresp="#s20:l1">spreeken</rhyme>,</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="2" type="-+">Raakt mijn niet de <rhyme label="a" type="f">reek'ning</rhyme>,</l>
        <l id="s21:l2" met="2" type="-+">Als boven de <rhyme label="a" type="f" corresp="#s21:l1">teek'ning</rhyme>,</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="2" type="-+">Gy 't goet van je <rhyme label="a" type="f">Leeden</rhyme></l>
        <l id="s22:l2" met="2" type="-+">Onnut gingt <rhyme label="a" type="f" corresp="#s22:l1">besteeden</rhyme>;</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="2" type="-+">Was 't dan niet, <rhyme label="a" type="f">ocharmen</rhyme>?</l>
        <l id="s23:l2" met="2" type="-+">Wilt onser <rhyme label="a" type="f" corresp="#s23:l1">erbarmen</rhyme>;</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="2" type="-+">Helpt Geusen, helpt <rhyme label="a" type="f">Papen</rhyme>,</l>
        <l id="s24:l2" met="2" type="-+">Laat gunst by je <rhyme label="a" type="f" corresp="#s24:l1">rapen</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="2" type="-+">Besnoert dan je <rhyme label="a" type="f">bakhuys</rhyme>,</l>
        <l id="s25:l2" met="2" type="-+">En loopt na je <rhyme label="a" type="f" corresp="#s25:l1">kakhuys</rhyme>,</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="2" type="-+">Dat voor die zoo <rhyme label="a" type="f">snapte</rhyme>,</l>
        <l id="s26:l2" met="2" type="-+">En breydeloos <rhyme label="a" type="f" corresp="#s26:l1">klapte</rhyme>,</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="2" type="-+">Geeft beter <rhyme label="a" type="f">geschencken</rhyme>,</l>
        <l id="s27:l2" met="2" type="-+">Hebt anders te <rhyme label="a" type="f" corresp="#s27:l1">dencken</rhyme>,</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="2" type="-+">Tot rechte <rhyme label="a" type="f">belooning</rhyme>,</l>
        <l id="s28:l2" met="2" type="-+">Een heerlijcke <rhyme label="a" type="f" corresp="#s28:l1">krooning</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
