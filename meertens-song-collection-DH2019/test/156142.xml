  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Philis, mijne <rhyme label="a" type="f">uytverkore</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wild eens na uw Titer <rhyme label="a" type="f" corresp="#s1:l1">horen</rhyme>.</l>
        <l id="s1:l3" met="2" type="-+">Ag, laat uw <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Sig buygen voor mijn <rhyme label="b" type="m" corresp="#s1:l3">smert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wie noemd gy uw <rhyme label="a" type="f">uytverkoren</rhyme>?</l>
        <l id="s2:l2" met="4" type="+-">Titer 'k wil die taal niet <rhyme label="a" type="f" corresp="#s2:l1">horen</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Veel min mijn <rhyme label="b" type="m">hert</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ooit buygen voor uw <rhyme label="b" type="m" corresp="#s2:l3">smert</rhyme>.</l>
      </lg>
    </lg>
