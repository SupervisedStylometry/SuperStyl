  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">'t IS nu speel-dagh en naer <rhyme label="a" type="m">noen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Ick en heb niet veel te <rhyme label="a" type="m" corresp="#s1:l1">doen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Het is sonde leegh te <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Laet ons nu wat spelen <rhyme label="b" type="m" corresp="#s1:l3">gaen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Sa dan myn Lief <rhyme label="c" type="f">coragie</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wy zyn nu just <rhyme label="d" type="m">ghepaert</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Laet ons eens <rhyme label="c" type="f" corresp="#s1:l5">Mariagie</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Gaen spelen met de <rhyme label="d" type="m" corresp="#s1:l6">Kaert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dat gy soeckt dat soeck ick <rhyme label="a" type="m">med'</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Lief ick consenteer uw' <rhyme label="a" type="m" corresp="#s2:l1">bed'</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Laet ons nu in <rhyme label="b" type="m">vrolyckheydt</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Gaen passeren onsen <rhyme label="b" type="m" corresp="#s2:l3">tydt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Want het is <rhyme label="c" type="f">Mariagie</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Die ons tot blydtschap <rhyme label="d" type="m">stryckt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dat spel gheeft ons <rhyme label="c" type="f" corresp="#s2:l5">coragie</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">'t Is just dat ons <rhyme label="d" type="m" corresp="#s2:l6">ghelyckt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Liefste speelt ghy <rhyme label="a" type="m">Herte-Vrouw</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Schenckt aen my uw Hert' en <rhyme label="a" type="m" corresp="#s3:l1">Trouw'</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Soo speel ick u dan <rhyme label="b" type="m">Herten-heer</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ick trouw u in deught en <rhyme label="b" type="m" corresp="#s3:l3">eer'</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Weet dat ick naer die <rhyme label="c" type="f">gagie</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gheen hoogher Troef en <rhyme label="d" type="m">hoef</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Oft schoonder <rhyme label="c" type="f" corresp="#s3:l5">Mariagie</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Want Hertenen is <rhyme label="d" type="m" corresp="#s3:l6">Troef</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Lustigh Vryer speelt dan <rhyme label="a" type="m">gouw</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Want ick speel u <rhyme label="a" type="m" corresp="#s4:l1">Herte-Vrouw</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Speelt gy daer-op <rhyme label="b" type="m">Herten-Heer</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Want het is nu uwen <rhyme label="b" type="m" corresp="#s4:l3">keer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dit is een aerdigh <rhyme label="c" type="f">Kaert-spel</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dit spel is vol <rhyme label="d" type="m">vermaeck</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Het aerdt naer mynen aerdt <rhyme label="c" type="f" corresp="#s4:l5">wel</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Het is een lieve <rhyme label="d" type="m" corresp="#s4:l6">saeck'</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'t Spel begint eerst lieve <rhyme label="a" type="m">Bruydt</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">'t Spel en is nu noch niet <rhyme label="a" type="m" corresp="#s5:l1">uyt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Als dat spel komt op het <rhyme label="b" type="m">lest</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Is dat spel het <rhyme label="b" type="m" corresp="#s5:l3">alder-best</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Wy moeten samen <rhyme label="c" type="f">trouwen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">En Mariagie <rhyme label="d" type="m">doen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En dan oock Bruyloft <rhyme label="c" type="f" corresp="#s5:l5">houwen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Op eenen langhen <rhyme label="d" type="m" corresp="#s5:l6">noen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Myn Pastey is <rhyme label="a" type="m">Herten-aes</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Die ick aen u schenck, Ã´ <rhyme label="a" type="m" corresp="#s6:l1">Baes</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Schenckt soo oock aen uw' <rhyme label="b" type="m">Basin</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En backt Herten aes <rhyme label="b" type="m" corresp="#s6:l3">daer-in</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Beminnen wy <rhyme label="c" type="f">malkander'</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Uyt heel ons hert' en <rhyme label="d" type="m">sin</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Gheeft d'een hert voor het <rhyme label="c" type="f" corresp="#s6:l5">ander</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">D'een min voor d'ander <rhyme label="d" type="m" corresp="#s6:l6">min</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Speelt ons altydt <rhyme label="a" type="m">Hertenen</rhyme>;</l>
        <l id="s7:l2" met="4" type="+-">Ende noyt gheen <rhyme label="a" type="m" corresp="#s7:l1">Schuppenen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Want het spel waer al-te <rhyme label="b" type="m">droef</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Waer 't dat Schuppenen viel <rhyme label="b" type="m" corresp="#s7:l3">Troef</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Wirt Liefdes-boogh <rhyme label="c" type="f">ontspannen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Het sou een gheessel <rhyme label="d" type="m">zyn</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dit zy van ons <rhyme label="c" type="f" corresp="#s7:l5">ghebannen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Godt hoed' ons voor die <rhyme label="d" type="m" corresp="#s7:l6">pyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Schuppen-Troef doet al te <rhyme label="a" type="m">seer</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">'k Hou van Herten-troef al <rhyme label="a" type="m" corresp="#s8:l1">meer</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Vivat Herten-heer en <rhyme label="b" type="m">Vrouw</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Herten-troef en <rhyme label="b" type="m" corresp="#s8:l3">Herte-trouw</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Leeft langh ghepaerde <rhyme label="c" type="f">Herten</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Leeft langh ver-eende <rhyme label="d" type="m">twee</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Leeft vrolyck sonder <rhyme label="c" type="f" corresp="#s8:l5">smerten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Leeft sonder druck oft <rhyme label="d" type="m" corresp="#s8:l6">wee</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
