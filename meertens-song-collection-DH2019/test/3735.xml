    <melody>Psalm 132 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHEDENCKT doch des Dauids O <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">End oock aen al sijn lyden <rhyme label="b" type="m">swaer</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Die v eedt ded' in't <rhyme label="b" type="m" corresp="#s1:l2">openbaer</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">End v' Godt Iacobs vol van <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Beloften altydt ded' <rhyme label="b" type="m" corresp="#s1:l2">eerbaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Heer die beloefd' ick v (sprack <rhyme label="a" type="m">hy</rhyme>)</l>
        <l id="s2:l2" met="4" type="-+">Ick en wil in myn huys niet <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Noch op myn bedde slaepen <rhyme label="b" type="m" corresp="#s2:l2">gaen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Iae oock niet eenmael sluymen <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ick sal myn ooghen niet <rhyme label="b" type="m" corresp="#s2:l2">toeslaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Tot dat ick Godt een plaetse <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Vinde die voor hem sij <rhyme label="b" type="m">bequaem</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Daer die Heer onse Godt <rhyme label="b" type="m" corresp="#s3:l2">eersaem</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Wonen end blyuen magh <rhyme label="a" type="m" corresp="#s3:l1">alleyn</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">End des stercken Godts hooghe <rhyme label="b" type="m" corresp="#s3:l2">naem</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wy verstaen end ons wert <rhyme label="a" type="m">vertelt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Dat hem Ephrata t'schoone <rhyme label="b" type="m">dal</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Behaeght bouen die plaetsen <rhyme label="b" type="m" corresp="#s4:l2">al</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Wy hebben gheuonden in't <rhyme label="a" type="m" corresp="#s4:l1">veldt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Een oort t'welck voor v wesen <rhyme label="b" type="m" corresp="#s4:l2">sal</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daer sullen wy sijnde <rhyme label="a" type="m">verblydt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Hem besoecken, end t'aller <rhyme label="b" type="m">stondt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Hem bidden wt des herten <rhyme label="b" type="m" corresp="#s5:l2">grondt</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Dies maeckt v op Heer nu ter <rhyme label="a" type="m" corresp="#s5:l1">tyt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Ghy end d'Arcke van v' <rhyme label="b" type="m" corresp="#s5:l2">verbondt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Laett die priesters daer bekleydt <rhyme label="a" type="m">sijn</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Met die waere <rhyme label="b" type="m">gherechticheit</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Vwe heylghen met <rhyme label="b" type="m" corresp="#s6:l2">vroylickheit</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Vervult, end bewaert t'rycke <rhyme label="a" type="m" corresp="#s6:l1">fyn</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Vws ghesalfden voor <rhyme label="b" type="m" corresp="#s6:l2">teghenheit</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Godt heeft Dauid een eedt <rhyme label="a" type="m">ghedaen</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Die blyft vast end <rhyme label="b" type="m">onwackelbaer</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">End sprack: Op uwen stoel seer <rhyme label="b" type="m" corresp="#s7:l2">klaer</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Sal een uwer kindren voort <rhyme label="a" type="m" corresp="#s7:l1">aen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Regteren met voorspoet <rhyme label="b" type="m" corresp="#s7:l2">eenpaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Is't dat v' kinderen myn <rhyme label="a" type="m">Woort</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">End myn leere bewaeren <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Die sy verstaen: so sullen <rhyme label="b" type="m" corresp="#s8:l2">sy</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Met haeren kinderen nu <rhyme label="a" type="m" corresp="#s8:l1">voort</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Eewigh regieren <rhyme label="b" type="m" corresp="#s8:l2">onghestoort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Godt heeft verkoren Zion <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Hy heeft lust te wonen <rhyme label="b" type="m">aldaer</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Dit's myn ruste spreeckt hy <rhyme label="b" type="m" corresp="#s9:l2">voorwaer</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Myne lust, end myns herten <rhyme label="a" type="m" corresp="#s9:l1">kroon</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Daer wil ick wonen voor end <rhyme label="b" type="m" corresp="#s9:l2">naer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Haer spys' ick steedts seghenen <rhyme label="a" type="m">sal</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">End den aermen gheuen haer <rhyme label="b" type="m">broot</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Die priesters wil ick in den <rhyme label="b" type="m" corresp="#s10:l2">noot</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Met heyl kleyden, end ouer <rhyme label="a" type="m" corresp="#s10:l1">al</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Vervullen met blydtschap seer <rhyme label="b" type="m" corresp="#s10:l2">groot</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Dauid sal daer bloeyen met <rhyme label="a" type="m">spoet</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Verheuen wert sijn hoorn met <rhyme label="b" type="m">kracht</rhyme>:</l>
        <l id="s11:l3" met="4" type="-+">Ick hebb' hem te weghe <rhyme label="b" type="m" corresp="#s11:l2">ghebracht</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Een lampe die steedts lichten <rhyme label="a" type="m" corresp="#s11:l1">moet</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Tot allen tyden dagh end <rhyme label="b" type="m" corresp="#s11:l2">nacht</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ick wil met schand' end <rhyme label="a" type="m">smaedicheit</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Syn haeters bekleyden <rhyme label="b" type="m">ghemeyn</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Hen tot schand' end' oneer' met <rhyme label="b" type="m" corresp="#s12:l2">eyn</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Sal ick doen dat in <rhyme label="a" type="m" corresp="#s12:l1">heerlickheit</rhyme></l>
        <l id="s12:l5" met="4" type="-+">Dauids kroone bloeyen sal <rhyme label="b" type="m" corresp="#s12:l2">reyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
