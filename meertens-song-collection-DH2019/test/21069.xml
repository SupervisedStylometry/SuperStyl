<song>
  <meta>
    <id>21069</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Schoonste nimf van het woud</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VRolijck weest vereenight <rhyme label="a" type="m">Paer</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Dat te <rhyme label="a" type="m" corresp="#s1:l1">gaer</rhyme></l>
        <l id="s1:l3" met="4" type="+-">V gaet in den Echt <rhyme label="b" type="f">verbinden</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Ghy Genoode <rhyme label="c" type="m">alder-meest</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Op dees <rhyme label="c" type="m" corresp="#s1:l4">Feest</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Laet u doch vreughde <rhyme label="b" type="f" corresp="#s1:l3">vinden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hier is droefheydt niet <rhyme label="a" type="m">gesien</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Noch de <rhyme label="a" type="m" corresp="#s2:l1">Lien</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Die haer niet in deught <rhyme label="b" type="f">verblyen</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Hier is nu de rechte <rhyme label="c" type="m">vreught</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Vol <rhyme label="c" type="m" corresp="#s2:l4">geneught</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En het loon van 't soete <rhyme label="b" type="f" corresp="#s2:l3">vryen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hier wordt drift tot geyle <rhyme label="a" type="m">brandt</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Noch de <rhyme label="a" type="m" corresp="#s3:l1">schant</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Noch de valssche min <rhyme label="b" type="f">geleden</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Maer de liefde, daer de <rhyme label="c" type="m">deught</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">In <rhyme label="c" type="m" corresp="#s3:l4">verheught</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Heeft de geyle min <rhyme label="b" type="f" corresp="#s3:l3">vertreden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Zegen dael op u <rhyme label="a" type="m">geslacht</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Dagh en <rhyme label="a" type="m" corresp="#s4:l1">nacht</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Volght des Heeren heyl'ge <rhyme label="b" type="f">wetten</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Dan soo voeght hem God daer <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">By 't <rhyme label="c" type="m" corresp="#s4:l4">gesin</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Die op sijne wegen <rhyme label="b" type="f" corresp="#s4:l3">letten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Wensch de Bruygom, over 't <rhyme label="a" type="m">Jaer</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Werd' <rhyme label="a" type="m" corresp="#s5:l1">gewaer</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">'t Loon van sijn verdrietigh <rhyme label="b" type="f">slaven</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Dat hy singen mach <rhyme label="c" type="m">aldus</rhyme>:</l>
        <l id="s5:l5" met="2" type="+-">Suye, <rhyme label="c" type="m" corresp="#s5:l4">sus</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Komt ons soete schaepje <rhyme label="b" type="f" corresp="#s5:l3">laven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Drinckt daer op eens vrolijck <rhyme label="a" type="m">uyt</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Voor de <rhyme label="a" type="m" corresp="#s6:l1">Bruydt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Kust eens soet aen beyde <rhyme label="b" type="f">zijden</rhyme>.</l>
        <l id="s6:l4" met="4" type="+-">'t Zijn nu tijden van de <rhyme label="c" type="m">Feest</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Dat de <rhyme label="c" type="m" corresp="#s6:l4">Geest</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Wel een weynigh mach <rhyme label="b" type="f" corresp="#s6:l3">verblijden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
