  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Wat hovaerdy en <rhyme label="a" type="m">pragt</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Ziet men onder ‘t vrouw <rhyme label="a" type="m" corresp="#s1:l1">geslagt</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Wat nieuwe mode en <rhyme label="b" type="m">zwier</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Van zo menig <rhyme label="b" type="m" corresp="#s1:l3">venus-dier</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Sy gaen over <rhyme label="c" type="m">straet</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Als een Iuffrouw van <rhyme label="c" type="m" corresp="#s1:l5">staet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Heel netjes <rhyme label="d" type="m">gepalleert</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En het haertje <rhyme label="d" type="m" corresp="#s1:l7">gefriseert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Meenig lykt wel een <rhyme label="a" type="m">grim</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Schoon zy is een <rhyme label="a" type="m" corresp="#s2:l1">moffin</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Musjes schoon van <rhyme label="b" type="m">glans</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Op zijn Engels of <rhyme label="b" type="m" corresp="#s2:l3">frans</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Neusdoeken <rhyme label="c" type="m">geborduert</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">En het aenschyn <rhyme label="c" type="m" corresp="#s2:l5">gekouleert</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En montjes <rhyme label="d" type="m">geblanket</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Met moesjes <rhyme label="d" type="m" corresp="#s2:l7">opgeset</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">En door de <rhyme label="a" type="m">morserij</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Brengen zy zomtyds in <rhyme label="a" type="m" corresp="#s3:l1">ly</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Meenig onnosel <rhyme label="b" type="m">bloed</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Die ‘t gelag betalen <rhyme label="b" type="m" corresp="#s3:l3">moet</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Een Goude ketting om den <rhyme label="c" type="m">hals</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Maer de drommel dat is <rhyme label="c" type="m" corresp="#s3:l5">vals</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Fyn Oorlietten in het <rhyme label="d" type="m">Oor</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Draegt meenig slegte <rhyme label="d" type="m" corresp="#s3:l7">Sloor</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Ringen van <rhyme label="a" type="m">Diamant</rhyme>;</l>
        <l id="s4:l2" met="3" type="+-">Dragen zy aen haer <rhyme label="a" type="m" corresp="#s4:l1">hant</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Maer als men daer op <rhyme label="b" type="m">let</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Zy zijn slegts maer <rhyme label="b" type="m" corresp="#s4:l3">ingeset</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Van sijn gesleepen <rhyme label="c" type="m">glas</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">‘t Is bedrog al den <rhyme label="c" type="m" corresp="#s4:l5">bras</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Een Citsen Jak en <rhyme label="d" type="m">Rok</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die zo breed staet als een <rhyme label="d" type="m" corresp="#s4:l7">klok</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Waer in zy draeyen hun <rhyme label="a" type="m">gat</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Als zy maer gaen door de <rhyme label="a" type="m" corresp="#s5:l1">stad</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Komt een Ionkman hun te <rhyme label="b" type="m">gemoet</rhyme>:</l>
        <l id="s5:l4" met="3" type="+-">Die hun dan eens <rhyme label="b" type="m" corresp="#s5:l3">groet</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Sy neygen dan <rhyme label="c" type="m">gelyk</rhyme>;</l>
        <l id="s5:l6" met="3" type="+-">Dat ‘t hembt sleept door de <rhyme label="c" type="m" corresp="#s5:l5">slyk</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">De oogen van zo een <rhyme label="d" type="m">Venus-dier</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Branden door <rhyme label="d" type="m" corresp="#s5:l7">minnevier</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
