    <melody>Psalm 005 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HOor, Heer, mijn woort, merck op mijn <rhyme label="a" type="f">reden</rhyme>;</l>
        <l id="s1:l2" met="4" type="-+">Geeff op mijn droevich schreyen <rhyme label="b" type="m">acht</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En sinck het in u <rhyme label="b" type="m" corresp="#s1:l2">grondt-gedacht</rhyme>:</l>
        <l id="s1:l4" met="4" type="-+">Want overvloedt van <rhyme label="a" type="f" corresp="#s1:l1">bangicheden</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Port my tot <rhyme label="a" type="f" corresp="#s1:l1">beden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Eer noch de nacht heel is ten <rhyme label="a" type="f">enden</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Zijt ghy’t mijn Godt en <rhyme label="b" type="m">Over-Heer</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer ick alleen mijn re’en toe <rhyme label="b" type="m" corresp="#s2:l2">keer</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">Ick haest met be’e. Haest ghy in’t <rhyme label="a" type="f" corresp="#s2:l1">wenden</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Van mijn <rhyme label="a" type="f" corresp="#s2:l1">ellenden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ghy zijt geen Godt gelijck de <rhyme label="a" type="f">Goden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Gesmeedt in’t Goddeloose <rhyme label="b" type="m">breyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Van menschen, die, in’t hert niet <rhyme label="b" type="m" corresp="#s3:l2">reyn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Door vuylen Godtsdienst en <rhyme label="a" type="f" corresp="#s3:l1">geboden</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Ter ondeuchdt <rhyme label="a" type="f" corresp="#s3:l1">nooden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Voor u en kan geen Godloos <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hoe over-konstigh <rhyme label="b" type="m">geblancket</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hoe heyligh-schijnigh t’saem <rhyme label="b" type="m" corresp="#s4:l2">geset</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Hoe aengebeden en <rhyme label="a" type="f" corresp="#s4:l1">gepresen</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Behaeghlijck <rhyme label="a" type="f" corresp="#s4:l1">wesen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Geen Goddeloose kander <rhyme label="a" type="f">duren</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Voor uwe heylighst’ <rhyme label="b" type="m">Heylicheydt</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Voor u bestaet geen ziel, <rhyme label="b" type="m" corresp="#s5:l2">bereydt</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Haer even-mensch, door boose <rhyme label="a" type="f" corresp="#s5:l1">kuren</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Ramp toe te <rhyme label="a" type="f" corresp="#s5:l1">sturen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Geen hoofden die sich trots <rhyme label="a" type="f">verhoogen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Geen handen stout tot <rhyme label="b" type="m">tyranny</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Geen monden vol van <rhyme label="b" type="m" corresp="#s6:l2">snorckery</rhyme></l>
        <l id="s6:l4" met="4" type="-+">En konnen uwe Godlijck’ <rhyme label="a" type="f" corresp="#s6:l1">oogen</rhyme></l>
        <l id="s6:l5" met="2" type="-+">Voor sich <rhyme label="a" type="f" corresp="#s6:l1">gedoogen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ghy zijt een vyandt aller <rhyme label="a" type="f">quaden</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En yvert over haer ter <rhyme label="b" type="m">straff</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">De leugen-tongen snijdt ghy <rhyme label="b" type="m" corresp="#s7:l2">aff</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Die vaerdigh leggen ten <rhyme label="a" type="f" corresp="#s7:l1">verrade</rhyme></l>
        <l id="s7:l5" met="2" type="-+">En ’snaesten <rhyme label="a" type="f" corresp="#s7:l1">schade</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy hebt een grouwel van de <rhyme label="a" type="f">keelen</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Die dorstigh zijn na menschen <rhyme label="b" type="m">bloedt</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">V hert verfoeyt het valsch <rhyme label="b" type="m" corresp="#s8:l2">gebroedt</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Dat door bedrieghlijck <rhyme label="a" type="f" corresp="#s8:l1">tonge-spelen</rhyme></l>
        <l id="s8:l5" met="2" type="-+">Elcks gunst kan <rhyme label="a" type="f" corresp="#s8:l1">stelen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Maer ick, bedauwt in mijn’ <rhyme label="a" type="f">gedachten</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Met voor-smaeck uwer <rhyme label="b" type="m">goediche’en</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Wil ten gewijden drempel <rhyme label="b" type="m" corresp="#s9:l2">tre’en</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">En, in ontsach, mijn beed’ en <rhyme label="a" type="f" corresp="#s9:l1">klachten</rhyme></l>
        <l id="s9:l5" met="2" type="-+">Troost laten <rhyme label="a" type="f" corresp="#s9:l1">wachten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Weest my een voor-licht op de <rhyme label="a" type="f">wege</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Die leyden ter <rhyme label="b" type="m">gerechticheydt</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Op dat ick in het net, <rhyme label="b" type="m" corresp="#s10:l2">gespreydt</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Van haer, wiens hert my hard is <rhyme label="a" type="f" corresp="#s10:l1">tegen</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Niet werd <rhyme label="a" type="f" corresp="#s10:l1">gekregen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Wat zijn haer’ monden? <rhyme label="a" type="f">Wayffel-scholen</rhyme>.</l>
        <l id="s11:l2" met="4" type="-+">Haer herten? Nesten van veel <rhyme label="b" type="m">pijn</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Haer tong? Een woonst van valschen <rhyme label="b" type="m" corresp="#s11:l2">schijn</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Haer keelen? Open <rhyme label="a" type="f" corresp="#s11:l1">stanck-ryolen</rhyme></l>
        <l id="s11:l5" met="2" type="-+">En <rhyme label="a" type="f" corresp="#s11:l1">sonden-holen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Versmijtse, Godt; verbreeck haer <rhyme label="a" type="f">poogen</rhyme>.</l>
        <l id="s12:l2" met="4" type="-+">Verydel hares herten <rhyme label="b" type="m">raedt</rhyme>:</l>
        <l id="s12:l3" met="4" type="-+">Op datse d’ingebeelde <rhyme label="b" type="m" corresp="#s12:l2">daedt</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Daer’t hert soo seer heeft na <rhyme label="a" type="f" corresp="#s12:l1">getogen</rhyme>,</l>
        <l id="s12:l5" met="2" type="-+">Noyt en <rhyme label="a" type="f" corresp="#s12:l1">vermogen</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Verstootse; roeytse uyt, ten <rhyme label="a" type="f">gronde</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Die uws gebodts geen acht en <rhyme label="b" type="m">slaen</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Maer na haer eygen lusten <rhyme label="b" type="m" corresp="#s13:l2">gaen</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">En rennen, stout en <rhyme label="a" type="f" corresp="#s13:l1">ongebonden</rhyme>,</l>
        <l id="s13:l5" met="2" type="-+">Langhs ’tpadt der <rhyme label="a" type="f" corresp="#s13:l1">sonden</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Maer heugelijck sal sich <rhyme label="a" type="f">verblijde</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Al wie, terwijl hy streeft na’t <rhyme label="b" type="m">best</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Op u sijn gantsche hope <rhyme label="b" type="m" corresp="#s14:l2">vest</rhyme>.</l>
        <l id="s14:l4" met="4" type="-+">Den vromen sal geen lengt’ van <rhyme label="a" type="f" corresp="#s14:l1">tijden</rhyme></l>
        <l id="s14:l5" met="2" type="-+">Haer roem <rhyme label="a" type="f" corresp="#s14:l1">affsnijden</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Geen wonder. want uw’ kracht <rhyme label="a" type="f">beschermtse</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Vw’ sorg’ is om haer als een <rhyme label="b" type="m">schildt</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Vw’ zegen kroont en loontse <rhyme label="b" type="m" corresp="#s15:l2">mildt</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Vw’ lieffde koestert en <rhyme label="a" type="f" corresp="#s15:l1">verwermtse</rhyme></l>
        <l id="s15:l5" met="2" type="-+">Vw’ gunst <rhyme label="a" type="f" corresp="#s15:l1">om-ermtse</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
