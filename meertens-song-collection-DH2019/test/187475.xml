<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187475</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Malbrough s'en va-t-en guerre</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MAlbroek gaat ten oorlog <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ach! zou hy <rhyme label="a" type="f" corresp="#s1:l1">wederkeeren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Of blyven in het <rhyme label="c" type="m">Veld</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Te Paasschen zal hy <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Te Paaschen zal hy <rhyme label="a" type="f" corresp="#s2:l1">komen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Of op een ander <rhyme label="c" type="m">tyd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die daagen zyn <rhyme label="a" type="f">verdweenen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Die daagen zyn <rhyme label="a" type="f" corresp="#s3:l1">verdweenen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Malbrouk die komt niet <rhyme label="c" type="m">weer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Mevrouw klimt op haar <rhyme label="a" type="f">toren</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Mevrouw klimt op haar <rhyme label="a" type="f" corresp="#s4:l1">toren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Soo hoog als zy maar <rhyme label="c" type="m">kon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Sy ziet haar Pagie <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Sy ziet haar Pagie <rhyme label="a" type="f" corresp="#s5:l1">komen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hy was in ’t swart <rhyme label="c" type="m">gekleed</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Myn edele kind laat <rhyme label="a" type="f">hooren</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Myn edele kind laat <rhyme label="a" type="f" corresp="#s6:l1">hooren</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wat tyding brengt gy <rhyme label="c" type="m">mee</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Myn boodschap zegt den <rhyme label="a" type="f">Jonker</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Myn boodschap zegt den <rhyme label="a" type="f" corresp="#s7:l1">Jonker</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Is vry niet <rhyme label="c" type="m">aangenaam</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Gy gaat in traanen <rhyme label="a" type="f">smelten</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Gy gaat in traanen <rhyme label="a" type="f" corresp="#s8:l1">smelten</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Als gy myn reeden <rhyme label="c" type="m">hoort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Legt af uw mooije <rhyme label="a" type="f">kleeren</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Legt af uw mooje <rhyme label="a" type="f" corresp="#s9:l1">kleeren</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Van glinsterend <rhyme label="c" type="m">zatyn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">’t Is tyd mevrouw van <rhyme label="a" type="f">weenen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s10:l3" met="3" type="-+">’t Is tyd mevrouw van <rhyme label="a" type="f" corresp="#s10:l1">weenen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Heer Malbrouk is een <rhyme label="c" type="m">Lyk</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Den Man is reeds <rhyme label="a" type="f">begraven</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Den man is reeds <rhyme label="a" type="f" corresp="#s11:l1">begraven</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Hy eet of drinkt niet <rhyme label="c" type="m">meer</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">’k Heb hem in ’t Graf zien <rhyme label="a" type="f">dragen</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s12:l3" met="3" type="-+">’k Heb hem in ’t Graf zien <rhyme label="a" type="f" corresp="#s12:l1">dragen</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">met rouw en droef <rhyme label="c" type="m">geklag</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ja door vier <rhyme label="a" type="f">Officieren</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Ja door vier <rhyme label="a" type="f" corresp="#s13:l1">Officieren</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Met floers al op de <rhyme label="c" type="m">mouw</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Den eene droeg zyn <rhyme label="a" type="f">Sabel</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Den eene droeg zyn <rhyme label="a" type="f" corresp="#s14:l1">Sabel</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Den ander zyn Borst <rhyme label="c" type="m">Harnas</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Syn Schilt wiert ook <rhyme label="a" type="f">gedragen</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Syn Schilt wiert ook <rhyme label="a" type="f" corresp="#s15:l1">gedragen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">En verder droeg men <rhyme label="c" type="m">niets</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Het Graf was op de <rhyme label="a" type="f">hoeken</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Het Graf was op de <rhyme label="a" type="f" corresp="#s16:l1">hoeken</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Met Roosen ook <rhyme label="c" type="m">versierd</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Wyl nu de Oorlogs <rhyme label="a" type="f">mannen</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Wyl nu de Oorlogs <rhyme label="a" type="f" corresp="#s17:l1">mannen</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Daar weende <rhyme label="c" type="m">overluid</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Men hoorde ’t <rhyme label="a" type="f">Nagtegaaltje</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Men hoorde ’t <rhyme label="a" type="f" corresp="#s18:l1">Nagtegaaltje</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Daer zingen <rhyme label="c" type="m">overluid</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">De plegtigheid <rhyme label="a" type="f">voltrokken</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s19:l3" met="3" type="-+">De plegtigheid <rhyme label="a" type="f" corresp="#s19:l1">voltrokken</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">Kroop ieder na zyn <rhyme label="c" type="m">bedt</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Den eene met zyn <rhyme label="a" type="f">Vrouwe</rhyme>,</l>
        <l id="s20:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Den eene met zyn <rhyme label="a" type="f" corresp="#s20:l1">Vrouwe</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">Een ander met zyn <rhyme label="c" type="m">liefje</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Elk kon een byslaap <rhyme label="a" type="f">krygen</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s21:l3" met="3" type="-+">Elk kon een byslaap <rhyme label="a" type="f" corresp="#s21:l1">krygen</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Dit spel ging gansch <rhyme label="c" type="m">gemeen</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Ik ken wel duizend <rhyme label="a" type="f">meisjes</rhyme>,</l>
        <l id="s22:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s22:l3" met="3" type="-+">Ik ken wel duizend <rhyme label="a" type="f" corresp="#s22:l1">meisjes</rhyme>,</l>
        <l id="s22:l4" met="3" type="-+">Ja, duizend en nog <rhyme label="c" type="m">meer</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">En die wel zouden <rhyme label="a" type="f">lyden</rhyme>,</l>
        <l id="s23:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s23:l3" met="3" type="-+">En die wel zoude <rhyme label="a" type="f" corresp="#s23:l1">lyden</rhyme>,</l>
        <l id="s23:l4" met="3" type="-+">Een Vogel in haar <rhyme label="c" type="m">kooy</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Dit lyden ook de <rhyme label="a" type="f">zwarte</rhyme>,</l>
        <l id="s24:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s24:l3" met="3" type="-+">Dit lyden ook de <rhyme label="a" type="f" corresp="#s24:l1">zwarte</rhyme>,</l>
        <l id="s24:l4" met="3" type="-+">So wel als blond <rhyme label="c" type="m">geslagt</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">Een rotze zou niet <rhyme label="a" type="f">walgen</rhyme>,</l>
        <l id="s25:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s25:l3" met="3" type="-+">Een rotze zou niet <rhyme label="a" type="f" corresp="#s25:l1">walgen</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">Van zo een lekker <rhyme label="c" type="m">ding</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">Het schynt dat deeze <rhyme label="a" type="f">sleutel</rhyme>,</l>
        <l id="s26:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s26:l3" met="3" type="-+">Het schynt dat deeze <rhyme label="a" type="f" corresp="#s26:l1">sleutel</rhyme>,</l>
        <l id="s26:l4" met="3" type="-+">Op alle slooten <rhyme label="c" type="m">past</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="3" type="-+">Mevrouw op deze <rhyme label="a" type="f">woorden</rhyme>,</l>
        <l id="s27:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s27:l3" met="3" type="-+">Mevrouw op deze <rhyme label="a" type="f" corresp="#s27:l1">woorden</rhyme>,</l>
        <l id="s27:l4" met="3" type="-+">Gaf een bedroefde <rhyme label="c" type="m">zugt</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="3" type="-+">Want Malbrouk had voor <rhyme label="a" type="f">deeze</rhyme>,</l>
        <l id="s28:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s28:l3" met="3" type="-+">Want Malbrouk had voor <rhyme label="a" type="f" corresp="#s28:l1">deeze</rhyme>,</l>
        <l id="s28:l4" met="3" type="-+">Haar meer als eens <rhyme label="c" type="m">getoond</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="3" type="-+">Dat hy van Venus <rhyme label="a" type="f">vrienden</rhyme>,</l>
        <l id="s29:l2" met="4" type="-+">Mironton ton ton <rhyme label="b" type="f">mirontaine</rhyme></l>
        <l id="s29:l3" met="3" type="-+">Dat hy van Venus <rhyme label="a" type="f" corresp="#s29:l1">vrienden</rhyme>,</l>
        <l id="s29:l4" met="3" type="-+">De minste niet en <rhyme label="c" type="m">was</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
