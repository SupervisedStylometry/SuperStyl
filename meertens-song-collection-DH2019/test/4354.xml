    <melody>O kerstnacht schoner dan de dagen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Haar poezel lichaam, net <rhyme label="a" type="f">besneden</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Munt uit in alle <rhyme label="a" type="f" corresp="#s1:l1">geestigheden</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Beleit, en schranderheit, zijn <rhyme label="b" type="m">beid'</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Ten koor van haar gedagt <rhyme label="c" type="f">gestegen</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Die alles, na de maat, eerst <rhyme label="c" type="f" corresp="#s1:l4">wegen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">'t Geen uit haar lieve lippen <rhyme label="b" type="m" corresp="#s1:l3">scheit</rhyme>.</l>
      </lg>
    </lg>
  </text>
