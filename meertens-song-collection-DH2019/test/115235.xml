  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs Pijnardus quam van <rhyme label="a" type="f">Jage</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In den soeten <rhyme label="b" type="m">dageraet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Van ’t hy niet na sijn <rhyme label="a" type="f" corresp="#s1:l1">behage</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dan een Nimphje <rhyme label="b" type="m" corresp="#s1:l2">delicaet</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Die by haer vee een herders deuntje <rhyme label="c" type="f">queelde</rhyme></l>
        <l id="s1:l6" met="5" type="+-">Daer de Nagtegael in ’t bosje tegen <rhyme label="c" type="f" corresp="#s1:l5">speelde</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Dus hy haer seer vaerdigh, <rhyme label="d" type="f">Waerdigh</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Heeft gegroet, Uyt Liefde <rhyme label="e" type="m">soet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wel wie komt mijn hier <rhyme label="a" type="f">bespringe</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Daer ick dacht alleenigh <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Vermakende my met <rhyme label="a" type="f" corresp="#s2:l1">singe</rhyme></l>
        <l id="s2:l4" met="4" type="+-">By mijn Schaepjes in het <rhyme label="b" type="m" corresp="#s2:l2">Gras</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Mijn geest ontroert, in ben in dusent <rhyme label="c" type="f">sorgen</rhyme></l>
        <l id="s2:l6" met="5" type="+-">Wat my hier gemoet so vroeg inde <rhyme label="c" type="f" corresp="#s2:l5">morgen</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Sal zijn mijn verblijde, <rhyme label="d" type="f">Lijde</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Laet dan zijn, Een korte <rhyme label="e" type="m">pijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Waer door soud’ u Hart <rhyme label="a" type="f">versage</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Hier is geen ghevaer <rhyme label="b" type="m">altoos</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Ick Pijnardus kom van <rhyme label="a" type="f" corresp="#s3:l1">Jage</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Doch mijn Jacht was <rhyme label="b" type="m" corresp="#s3:l2">vruchteloos</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Ik reed uyt bos u stem soet klonk my <rhyme label="c" type="f">tegen</rhyme></l>
        <l id="s3:l6" met="5" type="+-">Hopende hier heb ik ’t regte wilt <rhyme label="c" type="f" corresp="#s3:l5">verkregen</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Flucx mijn Ingewande, <rhyme label="d" type="f">Brande</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Door de min, O <rhyme label="e" type="m">Harderin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Kan de Min soo vierigh <rhyme label="a" type="f">jage</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Daer ick u ben <rhyme label="b" type="m">onbekent</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dat sijn Loose <rhyme label="a" type="f" corresp="#s4:l1">minne-vlage</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Veynsery is meest <rhyme label="b" type="m" corresp="#s4:l2">present</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">Ghy hebt u moet ter Jagt niet konne <rhyme label="c" type="f">koele</rhyme></l>
        <l id="s4:l6" met="5" type="+-">Nu laet ghy ’t oogh in Venus luste <rhyme label="c" type="f" corresp="#s4:l5">woele</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Dese minne streecke: <rhyme label="d" type="f">Bleecke</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Al te veel, Bij Rijck en <rhyme label="e" type="m">Eel</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Cupido heeft mijn <rhyme label="a" type="f">verwonne</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Venus stoockt mijn <rhyme label="b" type="m">minne-brant</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">’t Pijl der liefde doet mijn <rhyme label="a" type="f" corresp="#s5:l1">jonne</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Dat ick stadigh blijf <rhyme label="b" type="m" corresp="#s5:l2">konstant</rhyme>,</l>
        <l id="s5:l5" met="5" type="-+">Op u verlieft mijn waerde <rhyme label="c" type="f">Harderinne</rhyme></l>
        <l id="s5:l6" met="5" type="+-">Die gedurig speelt in mijn gedagt en <rhyme label="c" type="f" corresp="#s5:l5">sinne</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Laet ick eens u kaeke, <rhyme label="d" type="f">Smaecke</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Tot verlicht: Van mijn <rhyme label="e" type="m">gesicht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Wilt u liefde toch niet <rhyme label="a" type="f">setten</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Op een boerse <rhyme label="b" type="m">Harderin</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dit sou u Geslacht <rhyme label="a" type="f" corresp="#s6:l1">besmette</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Door een achteloose <rhyme label="b" type="m" corresp="#s6:l2">min</rhyme>,</l>
        <l id="s6:l5" met="5" type="-+">Want ick bespeur gy zijt van groote <rhyme label="c" type="f">Huyse</rhyme></l>
        <l id="s6:l6" met="5" type="+-">En ick woon op Velt in slegte boere <rhyme label="c" type="f" corresp="#s6:l5">kluyse</rhyme></l>
        <l id="s6:l7" met="4" type="+-">En mijn Schaepjes leyde: <rhyme label="d" type="f">Weyde</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Na ’t bericht, Van Ouders <rhyme label="e" type="m">plicht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Mijn liefde sal niet <rhyme label="a" type="f">beswijcke</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Van u schoone Velt <rhyme label="b" type="m">Goddin</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">En dit Gout Juweel sal <rhyme label="a" type="f" corresp="#s7:l1">blijcke</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dat ghy vast staet in mijn <rhyme label="b" type="m" corresp="#s7:l2">sin</rhyme>,</l>
        <l id="s7:l5" met="5" type="-+">Ick wil veel eer mijn Vaders huys <rhyme label="c" type="f">verlate</rhyme></l>
        <l id="s7:l6" met="5" type="+-">En verwerpen alle praght en Hoofse <rhyme label="c" type="f" corresp="#s7:l5">state</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Eer ick u sou derve, <rhyme label="d" type="f">Swerve</rhyme>,</l>
        <l id="s7:l8" met="4" type="-+">Al mijn tijt, Met <rhyme label="e" type="m">Arrebeyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Wel hoe soud’ ghy u <rhyme label="a" type="f">begeve</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Tot de boerse <rhyme label="b" type="m">slaverny</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Stellen u in sober <rhyme label="a" type="f" corresp="#s8:l1">leve</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Voor u Pracht een Boere <rhyme label="b" type="m" corresp="#s8:l2">Py</rhyme>,</l>
        <l id="s8:l5" met="5" type="-+">Dat loof ick niet al swoert ghy by de <rhyme label="c" type="f">Gode</rhyme></l>
        <l id="s8:l6" met="5" type="+-">Nu sijn een Heer en werde een <rhyme label="c" type="f" corresp="#s8:l5">dienst-bode</rhyme></l>
        <l id="s8:l7" met="4" type="+-">Wilt by u wel-luste: <rhyme label="d" type="f">Ruste</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Dit betracht, Het meest <rhyme label="e" type="m">geacht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Dit is by propoost van <rhyme label="a" type="f">rede</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Dat de Liefde veel <rhyme label="b" type="m">vermagh</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Maer ick sal u Lief op <rhyme label="a" type="f" corresp="#s9:l1">hede</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Nog verheffen desen <rhyme label="b" type="m" corresp="#s9:l2">dagh</rhyme>,</l>
        <l id="s9:l5" met="5" type="-+">Ick sal van u een Hoofse Juffer <rhyme label="c" type="f">maken</rhyme>,</l>
        <l id="s9:l6" met="5" type="+-">Kleding na de mood’ van gout en zijde <rhyme label="c" type="f" corresp="#s9:l5">lake</rhyme></l>
        <l id="s9:l7" met="4" type="+-">En veel schoon Juweele, <rhyme label="d" type="f">deele</rhyme>,</l>
        <l id="s9:l8" met="4" type="-+">Voor het Boers: Met <rhyme label="e" type="m">Parel-snoers</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Ach Pijnardus wilt <rhyme label="a" type="f">bedare</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">U van dit voornemen <rhyme label="b" type="m">went</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Met een Harderin te <rhyme label="a" type="f" corresp="#s10:l1">pare</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Die dees pracht is <rhyme label="b" type="m" corresp="#s10:l2">onbekent</rhyme>,</l>
        <l id="s10:l5" met="5" type="-+">Ik weet van ’t hof nog van de hofse <rhyme label="c" type="f">trante</rhyme></l>
        <l id="s10:l6" met="5" type="+-">Mijn past geen zijd’ geen Parels <rhyme label="c" type="f" corresp="#s10:l5">diamante</rhyme></l>
        <l id="s10:l7" met="4" type="+-">Ick niet uyt mijn weyde: <rhyme label="d" type="f">scheyde</rhyme>,</l>
        <l id="s10:l8" met="4" type="-+">‘k ben vernoeght, Die mijn best <rhyme label="e" type="m">voegt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Staeck dees last wilt <rhyme label="a" type="f">resolveren</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Lief laet u sijn <rhyme label="b" type="m">onderreght</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Ghy sult al dees stacy <rhyme label="a" type="f" corresp="#s11:l1">leeren</rhyme></l>
        <l id="s11:l4" met="4" type="+-">Van mijn Kameniers en <rhyme label="b" type="m" corresp="#s11:l2">Kneghts</rhyme>,</l>
        <l id="s11:l5" met="5" type="-+">En sien veel vreugt van spel en <rhyme label="c" type="f">musikante</rhyme>,</l>
        <l id="s11:l6" met="5" type="+-">’t Op verheven zael veel pronken <rhyme label="c" type="f" corresp="#s11:l5">ledikante</rhyme></l>
        <l id="s11:l7" met="4" type="+-">Daer wy dan met kusjes: <rhyme label="d" type="f">Lusjes</rhyme>,</l>
        <l id="s11:l8" met="4" type="-+">Rusten fier, Na ons <rhyme label="e" type="m">pleysier</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Zoude wy soo los te <rhyme label="a" type="f">gader</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Voegen ons tot desen <rhyme label="b" type="m">Staet</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">Zonder wil van u Heer <rhyme label="a" type="f" corresp="#s12:l1">Vader</rhyme>,</l>
        <l id="s12:l4" met="4" type="+-">Neen gaet eerst met hem te <rhyme label="b" type="m" corresp="#s12:l2">raet</rhyme>,</l>
        <l id="s12:l5" met="5" type="-+">‘k Loof eer dat hy toe stemde dese <rhyme label="c" type="f">saecke</rhyme>,</l>
        <l id="s12:l6" met="5" type="+-">Hy van al sijn Schat u bastert soude <rhyme label="c" type="f" corresp="#s12:l5">make</rhyme></l>
        <l id="s12:l7" met="4" type="+-">Wil na uws gelijcke, <rhyme label="d" type="f">Kijcke</rhyme>,</l>
        <l id="s12:l8" met="4" type="-+">Dat ghy niet: Komt in ’t <rhyme label="e" type="m">verdriet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Wilt liefd’ maer tot mijn <rhyme label="a" type="f">neyge</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Stelt dees sorge uyt u <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Ick sal lof en eer <rhyme label="a" type="f" corresp="#s13:l1">verkrijgen</rhyme>,</l>
        <l id="s13:l4" met="4" type="+-">Als sy sien u schoon <rhyme label="b" type="m" corresp="#s13:l2">Goddin</rhyme>,</l>
        <l id="s13:l5" met="5" type="-+">Ik die wel eer staets Juffers had <rhyme label="c" type="f">gepresen</rhyme></l>
        <l id="s13:l6" met="5" type="+-">maer voor schat en goet sijn liefde moeter <rhyme label="c" type="f" corresp="#s13:l5">wesen</rhyme></l>
        <l id="s13:l7" met="4" type="+-">Dus beveelt u schaepjes: <rhyme label="d" type="f">knaepjes</rhyme>,</l>
        <l id="s13:l8" met="4" type="-+">By de waght, Van u <rhyme label="e" type="m">Geslacht</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Ach mijn Heer door liefs <rhyme label="a" type="f">vermogen</rhyme></l>
        <l id="s14:l2" met="4" type="+-">Val ick dan in u <rhyme label="b" type="m">genae</rhyme>:</l>
        <l id="s14:l3" met="4" type="+-">Want ghy hebt mijn Hart <rhyme label="a" type="f" corresp="#s14:l1">bewogen</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Om te werden u <rhyme label="b" type="m" corresp="#s14:l2">Ega</rhyme>,</l>
        <l id="s14:l5" met="5" type="-+">Ey laet ick eerst mijn Roose-hoet <rhyme label="c" type="f">versieren</rhyme></l>
        <l id="s14:l6" met="5" type="+-">Om mijn velt ghewaet met ranckjes van <rhyme label="c" type="f" corresp="#s14:l5">laurieren</rhyme></l>
        <l id="s14:l7" met="4" type="+-">Tot lof prijs der mage, <rhyme label="d" type="f">drage</rhyme>,</l>
        <l id="s14:l8" met="4" type="-+">En tot eer: Van u mijn <rhyme label="e" type="m">Heer</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Mijn Princes laet ons gaen <rhyme label="a" type="f">rijde</rhyme>,</l>
        <l id="s15:l2" met="4" type="+-">Vaerdigh na mijns vaders <rhyme label="b" type="m">Hof</rhyme>,</l>
        <l id="s15:l3" met="4" type="+-">Sit met vreught nu aen mijn <rhyme label="a" type="f" corresp="#s15:l1">sijde</rhyme>,</l>
        <l id="s15:l4" met="4" type="+-">’t Bly gemoet sal Singen <rhyme label="b" type="m" corresp="#s15:l2">lof</rhyme>,</l>
        <l id="s15:l5" met="5" type="-+">Juygt bos en dal met duysent zegen <rhyme label="c" type="f">zange</rhyme></l>
        <l id="s15:l6" met="5" type="+-">Ick en had in ’t Wout noyt eelder wilt <rhyme label="c" type="f" corresp="#s15:l5">gevange</rhyme>,</l>
        <l id="s15:l7" met="4" type="+-">Die ick sal met kroone: <rhyme label="d" type="f">Loone</rhyme>,</l>
        <l id="s15:l8" met="4" type="-+">Tot mijn bruyt, In kort <rhyme label="e" type="m">besluyt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="+-">Hadieu Klaver-velt en <rhyme label="a" type="f">boompjes</rhyme>,</l>
        <l id="s16:l2" met="4" type="+-">Hadieu lieve Schaepjes <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s16:l3" met="4" type="+-">Hadieu bron Fonteyne <rhyme label="a" type="f" corresp="#s16:l1">stroompjes</rhyme>,</l>
        <l id="s16:l4" met="4" type="+-">Hadieu maeghtschap en mijn <rhyme label="b" type="m" corresp="#s16:l2">stal</rhyme>,</l>
        <l id="s16:l5" met="5" type="-+">U Harderin, En sal u nooyt <rhyme label="c" type="f">vergeten</rhyme>,</l>
        <l id="s16:l6" met="5" type="+-">Schoon ick met mijn Lief in weelde waer <rhyme label="c" type="f" corresp="#s16:l5">geseten</rhyme></l>
        <l id="s16:l7" met="4" type="+-">Ach ’t Fortuyn is wonder: <rhyme label="d" type="f">onder</rhyme></l>
        <l id="s16:l8" met="4" type="-+">In ’t bestuer, Van <rhyme label="e" type="m">Avontuer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
