  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vliedt alderliefsten Jesus <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Seer neerstelick en <rhyme label="b" type="f">stranghe</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Ick ben nu te <rhyme label="b" type="f" corresp="#s1:l2">ganghe</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Wacker op den <rhyme label="a" type="m" corresp="#s1:l1">voet</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">K'en ben niet da'ck <rhyme label="c" type="f">pleghe</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Ick en blijve <rhyme label="d" type="m">niet</rhyme></l>
        <l id="s1:l7" met="3" type="+-">Ligghen onder <rhyme label="c" type="f" corresp="#s1:l5">weghe</rhyme></l>
        <l id="s1:l8" met="3" type="+-">So ghy self wel <rhyme label="d" type="m" corresp="#s1:l6">siet</rhyme>.</l>
        <l id="s1:l9" met="3" type="+-">So ghy self wel <rhyme label="d" type="m" corresp="#s1:l6">siet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het snoer van uwe liefde <rhyme label="a" type="m">schoon</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Dat ghy my hebt <rhyme label="b" type="f">ghegheven</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Heeft my op <rhyme label="b" type="f" corresp="#s2:l2">gheheven</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En ghemaeckt <rhyme label="a" type="m" corresp="#s2:l1">ydoon</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Om van,  deghs te <rhyme label="c" type="f">gaene</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Met mijn lichaem <rhyme label="d" type="m">teer</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">In der deughden <rhyme label="c" type="f" corresp="#s2:l5">baene</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Deuchdigh lancx om <rhyme label="d" type="m" corresp="#s2:l6">meer</rhyme>.</l>
        <l id="s2:l9" met="3" type="+-">Deuchdigh lancx om <rhyme label="d" type="m" corresp="#s2:l6">meer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hebt ghy tot deught my leeren <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Wilt graci my aen <rhyme label="b" type="f">knoopen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Om te leeren <rhyme label="b" type="f" corresp="#s3:l2">loopen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Tot de deught seer <rhyme label="a" type="m" corresp="#s3:l1">faen</rhyme>;</l>
        <l id="s3:l5" met="3" type="+-">In't gaen heb ick <rhyme label="c" type="f">smaeken</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Maer in't loopen <rhyme label="d" type="m">sal</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Ick my meer <rhyme label="c" type="f" corresp="#s3:l5">vermaeken</rhyme></l>
        <l id="s3:l8" met="3" type="+-">In deught over <rhyme label="d" type="m" corresp="#s3:l6">al</rhyme>.</l>
        <l id="s3:l9" met="3" type="+-">In deught over <rhyme label="d" type="m" corresp="#s3:l6">al</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vliet als een herte jonck voor <rhyme label="a" type="m">wt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Op de gheberchten <rhyme label="b" type="f">blye</rhyme></l>
        <l id="s4:l3" met="3" type="+-">Vol van <rhyme label="b" type="f" corresp="#s4:l2">specerije</rhyme></l>
        <l id="s4:l4" met="3" type="+-">En van alle <rhyme label="a" type="m" corresp="#s4:l1">cruyt</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">Maer eer ghy u gaet <rhyme label="c" type="f">roeren</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Ghy moet my hier <rhyme label="d" type="m">bi'en</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Uwer liefden <rhyme label="c" type="f" corresp="#s4:l5">snoeren</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Anders ghy mocht <rhyme label="d" type="m" corresp="#s4:l6">vlien</rhyme>.</l>
        <l id="s4:l9" met="3" type="+-">Anders ghy mocht <rhyme label="d" type="m" corresp="#s4:l6">vlien</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">K'en mach vryheyt aen u niet <rhyme label="a" type="m">ge'en</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Ghy syt my al te <rhyme label="b" type="f">gauwe</rhyme></l>
        <l id="s5:l3" met="3" type="+-">T'is noodt dat ick my <rhyme label="b" type="f" corresp="#s5:l2">houwe</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Aen u hier <rhyme label="a" type="m" corresp="#s5:l1">bene'en</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">Laet het u <rhyme label="c" type="f">believen</rhyme></l>
        <l id="s5:l6" met="3" type="+-">En maeckt gheen <rhyme label="d" type="m">gherucht</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Wilt my hier in <rhyme label="c" type="f" corresp="#s5:l5">gherieven</rhyme></l>
        <l id="s5:l8" met="3" type="+-">Doet met my de <rhyme label="d" type="m" corresp="#s5:l6">vlucht</rhyme>.</l>
        <l id="s5:l9" met="3" type="+-">Doet met my de <rhyme label="d" type="m" corresp="#s5:l6">vlucht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Lof die op voet my hebt <rhyme label="a" type="m">gheset</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Laet my houden u <rhyme label="b" type="f">stringhen</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Tot dat ick can <rhyme label="b" type="f" corresp="#s6:l2">springhen</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Over s'deught <rhyme label="a" type="m" corresp="#s6:l1">belet</rhyme>:</l>
        <l id="s6:l5" met="3" type="+-">Dan soo sal ick <rhyme label="c" type="f">trachten</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Tot den bergh des <rhyme label="d" type="m">deught</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Daer sal ick <rhyme label="c" type="f" corresp="#s6:l5">verwachten</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Uw schoone <rhyme label="d" type="m" corresp="#s6:l6">vreught</rhyme>.</l>
        <l id="s6:l9" met="3" type="+-">Uw schoone <rhyme label="d" type="m" corresp="#s6:l6">vreught</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
