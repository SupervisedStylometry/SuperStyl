  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAt hovaerdy en <rhyme label="a" type="m">pragt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Ziet men onder het Vrouw <rhyme label="a" type="m" corresp="#s1:l1">geslagt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wat nieuwe mode en <rhyme label="b" type="m">zwier</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Van zoo menig venus <rhyme label="b" type="m" corresp="#s1:l3">dier</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zy gaen over <rhyme label="c" type="m">straet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Als een Juffrouw van <rhyme label="c" type="m" corresp="#s1:l5">staet</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Heel netjes <rhyme label="d" type="m">gepalleert</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">En het haertje <rhyme label="d" type="m" corresp="#s1:l7">gevriseert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Menig lykt wel een <rhyme label="a" type="m">grim</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Schoon zy is een <rhyme label="a" type="m" corresp="#s2:l1">Moffin</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Musjes schoon van <rhyme label="b" type="m">glans</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Op zijn Engels of <rhyme label="b" type="m" corresp="#s2:l3">Frans</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Neusdoeken <rhyme label="c" type="m">geborduurt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En het aenschijn <rhyme label="c" type="m" corresp="#s2:l5">gebouluurt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">En met montjes <rhyme label="d" type="m">geblanket</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Met moesjes <rhyme label="d" type="m" corresp="#s2:l7">opgezet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En door de <rhyme label="a" type="m">morsery</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Brengen zy haer in de <rhyme label="a" type="m" corresp="#s3:l1">ly</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Menig onnosel <rhyme label="b" type="m">bloed</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die 't gelag betalen <rhyme label="b" type="m" corresp="#s3:l3">moet</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Een goude ketting om den <rhyme label="c" type="m">hals</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">Maer de drommel die is <rhyme label="c" type="m" corresp="#s3:l5">vals</rhyme>;</l>
        <l id="s3:l7" met="4" type="-+">Fyn Oorlietten in het <rhyme label="d" type="m">Oor</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Draegt menig slegte <rhyme label="d" type="m" corresp="#s3:l7">sloor</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ringen van <rhyme label="a" type="m">Diamant</rhyme>;</l>
        <l id="s4:l2" met="4" type="-+">Dragen zy aen haer <rhyme label="a" type="m" corresp="#s4:l1">hant</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Maer als men daer op <rhyme label="b" type="m">let</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Zy zijn slegts maer <rhyme label="b" type="m" corresp="#s4:l3">ingezet</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Van fyn geslepen <rhyme label="c" type="m">glas</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">'t Is bedrog al den <rhyme label="c" type="m" corresp="#s4:l5">bras</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Een Citzen Jak en <rhyme label="d" type="m">Rok</rhyme>:</l>
        <l id="s4:l8" met="4" type="-+">Die zoo breed staet als een <rhyme label="d" type="m" corresp="#s4:l7">klok</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Waer in zy draeyen hun <rhyme label="a" type="m">gat</rhyme>:</l>
        <l id="s5:l2" met="4" type="-+">Als zy maer gaen door de <rhyme label="a" type="m" corresp="#s5:l1">Stad</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Komt een Jonkman hun te <rhyme label="b" type="m">gemoet</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Die hun dan eens <rhyme label="b" type="m" corresp="#s5:l3">groet</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Zy neigen dan <rhyme label="c" type="m">gelijk</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dat 't hembt sleept door den <rhyme label="c" type="m" corresp="#s5:l5">slyk</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">De Oogen van zoo een Venus <rhyme label="d" type="m">dier</rhyme>:</l>
        <l id="s5:l8" met="4" type="-+">Branden door <rhyme label="d" type="m" corresp="#s5:l7">minnevier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Kousjes rood of <rhyme label="a" type="m">wit</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Dat byzonder netjes <rhyme label="a" type="m" corresp="#s6:l1">zit</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Muyltjes van gedrukt <rhyme label="b" type="m">Leer</rhyme>;</l>
        <l id="s6:l4" met="4" type="-+">Met witte hakken met <rhyme label="b" type="m" corresp="#s6:l3">begeer</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Het is van buiten <rhyme label="c" type="m">bont</rhyme>:</l>
        <l id="s6:l6" met="3" type="-+">Maer van ondere <rhyme label="c" type="m" corresp="#s6:l5">Stront</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Menig pronkt door de <rhyme label="d" type="m">Stad</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Zy hebbe zomtijds geen hembt aen 't <rhyme label="d" type="m" corresp="#s6:l7">gat</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Menig Meyd doet wat zy <rhyme label="a" type="m">kan</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">Om te krijgen een <rhyme label="a" type="m" corresp="#s7:l1">Man</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Maer zoo zy zijn <rhyme label="b" type="m">getrouwt</rhyme>:</l>
        <l id="s7:l4" met="4" type="-+">Het hun zomtijds dan <rhyme label="b" type="m" corresp="#s7:l3">berout</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Want dese Juffrouw <rhyme label="c" type="m">mooy</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">Slaept op een Bed van <rhyme label="c" type="m" corresp="#s7:l5">strooy</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Wat dunkt u van zoo'n arm <rhyme label="d" type="m">dier</rhyme>?</l>
        <l id="s7:l8" met="4" type="-+">Is dog echter na de <rhyme label="d" type="m" corresp="#s7:l7">zwier</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Luisen gelooft <rhyme label="a" type="m">mijn</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Daer zomtijds meer <rhyme label="a" type="m" corresp="#s8:l1">zijn</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">Als duiten in de <rhyme label="b" type="m">Kas</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Weg met zulken kalen <rhyme label="b" type="m" corresp="#s8:l3">Tas</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">'t Is beter een frisse <rhyme label="c" type="m">Boerin</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Daer is nog voordeel <rhyme label="c" type="m" corresp="#s8:l5">in</rhyme>;</l>
        <l id="s8:l7" met="4" type="-+">Maer zoo een kale <rhyme label="d" type="m">neet</rhyme>,</l>
        <l id="s8:l8" met="4" type="-+">Is verdriet ook niet een <rhyme label="d" type="m" corresp="#s8:l7">beet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
