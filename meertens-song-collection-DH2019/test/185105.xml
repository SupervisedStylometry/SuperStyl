  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Lief vriendin die ik <rhyme label="a" type="f">beminnen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Lief vriendinne hoord my <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Gy staat in myn hert en <rhyme label="a" type="f" corresp="#s1:l1">zinnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">'k Bid u laat my troost <rhyme label="b" type="m" corresp="#s1:l2">ontfaan</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Tot u alleen is myn <rhyme label="c" type="f">verlangen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Door de liefde en rechte <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Lief Rosalinde, myn <rhyme label="e" type="f">welbeminde</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Aanhoort myn rede hoe 'k u <rhyme label="d" type="m" corresp="#s1:l6">bemin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Niemand op de g'heeel <rhyme label="a" type="f">Aarde</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Die myn hert behagen <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Als gy maget vol van <rhyme label="a" type="f" corresp="#s2:l1">waarde</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Ach aanhoort myn bede <rhyme label="b" type="m" corresp="#s2:l2">dan</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Laat my nu troost van u <rhyme label="c" type="f">verwerven</rhyme></l>
        <l id="s2:l6" met="4" type="-+">En geef gehoor aen 't geen ik <rhyme label="d" type="m">vraag</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Lief Rosalinde, myn <rhyme label="e" type="f">welbeminde</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">'t Is al agtien maanden dat ik liefde <rhyme label="d" type="m" corresp="#s2:l6">draag</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Toen durfd' ik u niet <rhyme label="a" type="f">spreeken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Om te klaagen myne <rhyme label="b" type="m">smert</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Ik liet myne traane <rhyme label="a" type="f" corresp="#s3:l1">leken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Door de liefde aan myn <rhyme label="b" type="m" corresp="#s3:l2">hert</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Als ik aanzag u minzaam <rhyme label="c" type="f">weezen</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Of dat ik hoorde uwe <rhyme label="d" type="m">taal</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Lief Rosalinde myn <rhyme label="e" type="f">welbeminde</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">'k Bid u maak een einde aan myn droeve <rhyme label="d" type="m" corresp="#s3:l6">kwaal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Nu ik neem de <rhyme label="a" type="f">stoutigheden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Om te vragen uwe <rhyme label="b" type="m">trouw</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Lieve Lief steld my in <rhyme label="a" type="f" corresp="#s4:l1">vrede</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Help een minnaar uit den <rhyme label="b" type="m" corresp="#s4:l2">rouw</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Want gy staat in myn hart <rhyme label="c" type="f">geschreeven</rhyme></l>
        <l id="s4:l6" met="4" type="-+">En niemand anders wil ik <rhyme label="d" type="m">nooit</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Lief Rosalinde, myn <rhyme label="e" type="f">welbeminde</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Gy zyt die 'k minne zal tot aan de <rhyme label="d" type="m" corresp="#s4:l6">dood</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Minnaar uwe zoete <rhyme label="a" type="f">woorde</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Dwingen ook myn teer <rhyme label="b" type="m">gemoed</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Als dat ik op alle <rhyme label="a" type="f" corresp="#s5:l1">zoorten</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Tot u liefde draagen <rhyme label="b" type="m" corresp="#s5:l2">moet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Zyt dan standvastig in u <rhyme label="c" type="f">liefde</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Ik zal ook weezen uw <rhyme label="d" type="m">Huisvrouw</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Houd Rosalinde, voor u <rhyme label="e" type="f">beminde</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Laat ons zamen treden in de egten <rhyme label="d" type="m" corresp="#s5:l6">trouw</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Nu ziet men in vreugde <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Rosalinde met haar <rhyme label="b" type="m">vriend</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Wie kan meerder liefde <rhyme label="a" type="f" corresp="#s6:l1">geeven</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Als m'in deez' twee herten <rhyme label="b" type="m" corresp="#s6:l2">vind</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Zy zyn door liefd' malkaar <rhyme label="c" type="f">geneegen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Geheel minzaam ende liefde <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Lief Rosalinde, met u <rhyme label="e" type="f">beminde</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Niemand als den schepper die u scheiden <rhyme label="d" type="m" corresp="#s6:l6">zal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
