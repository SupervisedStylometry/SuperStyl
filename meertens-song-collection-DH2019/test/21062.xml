<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21062</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Een amoureus fier gelaat</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GEluck vrede in 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Bruydegom en vrou <rhyme label="b" type="m">Bruydt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wil u de Heere <rhyme label="a" type="f" corresp="#s1:l1">geven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">De reyne lifde <rhyme label="b" type="m" corresp="#s1:l2">spruyt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Vyt 't hert tusschen u <rhyme label="c" type="f">beyden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Wilt in Gods vrees <rhyme label="d" type="m">bequaem</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Altijt u wegen <rhyme label="c" type="f" corresp="#s1:l5">leyden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">'t Sal u niet qualijck <rhyme label="d" type="m" corresp="#s1:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want God belooft sijn <rhyme label="a" type="f">zegen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die hem vreesen <rhyme label="b" type="m">een-paer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die gaen op sijne <rhyme label="a" type="f" corresp="#s2:l1">wegen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Verwachten oock hier <rhyme label="b" type="m" corresp="#s2:l2">naer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">De kroone, soo wy <rhyme label="c" type="f">lesen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En de eeuwige <rhyme label="d" type="m">vreucht</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die is met al u <rhyme label="c" type="f" corresp="#s2:l5">wesen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Voeght u tot eer en <rhyme label="d" type="m" corresp="#s2:l6">deucht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hoort uyt het woordt des <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hoe heelijck sy dees <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Als wy Genesis <rhyme label="a" type="f" corresp="#s3:l1">lesen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Na dat God door sijn <rhyme label="b" type="m" corresp="#s3:l2">raedt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Hemel en aerd' <rhyme label="c" type="f">geschapen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Hadde, soo sprack hy <rhyme label="d" type="m">vry</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Laet ons den mensche <rhyme label="c" type="f" corresp="#s3:l5">maecken</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">'t Beelt dat ons gelijck <rhyme label="d" type="m" corresp="#s3:l6">zy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al uyt een klomp der <rhyme label="a" type="f">aerden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Schiep God den mensche <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Een Heer liet hy hem <rhyme label="a" type="f" corresp="#s4:l1">werden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Over de Aerd' met <rhyme label="b" type="m" corresp="#s4:l2">spoet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Over vogels en <rhyme label="c" type="f">dieren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Over Visschen <rhyme label="d" type="m">gemeen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Had Adam al 't <rhyme label="c" type="f" corresp="#s4:l5">bestieren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Maer hy was doch <rhyme label="d" type="m" corresp="#s4:l6">alleen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dies heeft de Heer <rhyme label="a" type="f">genomen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Een rib uyt Adams <rhyme label="b" type="m">lijf</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Waer af dat is <rhyme label="a" type="f" corresp="#s5:l1">gekomen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Eva, 't welck was sijn <rhyme label="b" type="m" corresp="#s5:l2">wijf</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die God oock hem <rhyme label="c" type="f">toe-voeghde</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Tot eenen hulpe <rhyme label="d" type="m">reen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Waer in Adam <rhyme label="c" type="f" corresp="#s5:l5">genoeghde</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Want 't was sijn vleesch en <rhyme label="d" type="m" corresp="#s5:l6">been</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Geluckigh mach hy <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dien  God den Heere <rhyme label="b" type="m">geeft</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Een deughtsaem Vrouw <rhyme label="a" type="f" corresp="#s6:l1">gepresen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Die hem door liefde <rhyme label="b" type="m" corresp="#s6:l2">aenkleeft</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">In voorspoet of <rhyme label="c" type="f">gevare</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Wat hun de Heer <rhyme label="d" type="m">toevoeght</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Haer Man oock niet <rhyme label="c" type="f" corresp="#s6:l5">besware</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Met eenigh <rhyme label="d" type="m" corresp="#s6:l6">ongenoeght</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Man die heeft <rhyme label="a" type="f">verkregen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Een eerbaer Vrouwe <rhyme label="b" type="m">vroet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sal tot haer zijn <rhyme label="a" type="f" corresp="#s7:l1">genegen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Als tot sijn vleesch en <rhyme label="b" type="m" corresp="#s7:l2">bloedt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">De Vrou sal oock <rhyme label="c" type="f">beminnen</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Haer Man met ganscher <rhyme label="d" type="m">vlijdt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Soo sult ghy vreughde <rhyme label="c" type="f" corresp="#s7:l5">winnen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">En altijt zijn <rhyme label="d" type="m" corresp="#s7:l6">verblijdt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Gods zegen sal u <rhyme label="a" type="f">naecken</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">V Huys tot aller <rhyme label="b" type="m">tijdt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">V Huys-Vrou sal hy <rhyme label="a" type="f" corresp="#s8:l1">maecken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Als een Wijn-stock <rhyme label="b" type="m" corresp="#s8:l2">verblijdt</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Rontsom u wooningh <rhyme label="c" type="f">schoone</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Die vruchten draeght <rhyme label="d" type="m">bequaem</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Aen den disch in een <rhyme label="c" type="f" corresp="#s8:l5">kroone</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Sullen u kind'ren <rhyme label="d" type="m" corresp="#s8:l6">staen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ghy sult oock noch <rhyme label="a" type="f">aenschouwen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Veel kindts-kinderen <rhyme label="b" type="m">mee</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Tot dat ghy uyt 't <rhyme label="a" type="f" corresp="#s9:l1">benouwen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Komt in d'eeuwige <rhyme label="b" type="m" corresp="#s9:l2">stee</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Waer vreught is boven <rhyme label="c" type="f">maten</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Die geen oogen <rhyme label="d" type="m">gesien</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Noch gehoor konde <rhyme label="c" type="f" corresp="#s9:l5">vaten</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Daer ghy henen sult <rhyme label="d" type="m" corresp="#s9:l6">vlien</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Prins Bruydegom <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Leeft vredigh met u <rhyme label="b" type="m">Bruydt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Wilt haer swackheydt <rhyme label="a" type="f" corresp="#s10:l1">toe-geven</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Bid ick u in 't <rhyme label="b" type="m" corresp="#s10:l2">besluyt</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Vrou-Bruydt geeft u Man <rhyme label="c" type="f">eere</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Soo 't den vroomen <rhyme label="d" type="m">betaemt</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Siet hoe Sara haer <rhyme label="c" type="f" corresp="#s10:l5">Heere</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Abraham heeft <rhyme label="d" type="m" corresp="#s10:l6">genaemt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
