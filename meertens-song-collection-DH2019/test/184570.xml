<song>
  <meta>
    <id>184570</id>
    <date>
      <year-start>1803</year-start>
      <year-end>1803</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het droevige nonnetje</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">De Boer en Landman moet ik altyd <rhyme label="a" type="f">eere</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">En pryzen zyne eerelyke <rhyme label="b" type="m">staat</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Zyn zy al arm, somtyds in slegte <rhyme label="a" type="f" corresp="#s1:l1">kleeren</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Nogtans met regt hy alles te boven <rhyme label="b" type="m" corresp="#s1:l2">gaat</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Pausen, <rhyme label="c" type="f">Presaaten</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">En <rhyme label="c" type="f" corresp="#s1:l5">Potentaaten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Edel van hun <rhyme label="d" type="m">geslagt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Den Landman niet <rhyme label="d" type="m" corresp="#s1:l7">veragt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Als Adam werkte en Eva <rhyme label="a" type="f">artebeiden</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Zegt my eens waar was toen de <rhyme label="b" type="m">Edelman</rhyme>?</l>
        <l id="s2:l3" met="5" type="-+">De Boer alleen het menschenzaad <rhyme label="a" type="f" corresp="#s2:l1">verspreidde</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Daarom prys ik de Landman waar ik <rhyme label="b" type="m" corresp="#s2:l2">kan</rhyme>.</l>
        <l id="s2:l5" met="2" type="-+">Hy moet 't al <rhyme label="c" type="f">geeven</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Sou yder <rhyme label="c" type="f" corresp="#s2:l5">leeven</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Want alle eerbaar <rhyme label="d" type="m">goed</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Komt van de Landman <rhyme label="d" type="m" corresp="#s2:l7">zoet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">De Boer moet dag en nagt spitten en <rhyme label="a" type="f">zaaijen</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">En wieden 't land, maaken de velde <rhyme label="b" type="m">klaar</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Om in den oogst zyn kooren af te <rhyme label="a" type="f" corresp="#s3:l1">maaijen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Menschen en beesten spyst hy <rhyme label="b" type="m" corresp="#s3:l2">allegaar</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">Prinsen en <rhyme label="c" type="f">Graaven</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Moet hy ook <rhyme label="c" type="f" corresp="#s3:l5">laaven</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die pryzen hem met <rhyme label="d" type="m">spoed</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Vivat de Landman <rhyme label="d" type="m" corresp="#s3:l7">goed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Paus, Cardinaalen, Bisschoppen, <rhyme label="a" type="f">Prelaten</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Abten, Proosten, Pastoors, <rhyme label="b" type="m">Monniken</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Capellaanen, Kosters, immers alle <rhyme label="a" type="f" corresp="#s4:l1">staaten</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Doctooren en ook <rhyme label="b" type="m" corresp="#s4:l2">Canonnieken</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Moet den Boer <rhyme label="c" type="f">pryzen</rhyme></l>
        <l id="s4:l6" met="2" type="-+">En eer <rhyme label="c" type="f" corresp="#s4:l5">bewyzen</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Daarom zo pryst hem <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Vivat de Landman <rhyme label="d" type="m" corresp="#s4:l7">goed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Hoe zou de Koning oorlog kunnen <rhyme label="a" type="f">voeren</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Tegen zyn vyanden te stryde <rhyme label="b" type="m">gaan</rhyme>.</l>
        <l id="s5:l3" met="5" type="-+">Kwam al het geld niet van de <rhyme label="a" type="f" corresp="#s5:l1">Boeren</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Alle fabrieken zoude stille <rhyme label="b" type="m" corresp="#s5:l2">staan</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Al de <rhyme label="c" type="f">Soldaaten</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Duytsers en <rhyme label="c" type="f" corresp="#s5:l5">Croaten</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En ook de Landman <rhyme label="d" type="m">goed</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Die ook al spyzen <rhyme label="d" type="m" corresp="#s5:l7">moet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Hoe zoude Prinsen en edele <rhyme label="a" type="f">Heeren</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Krygen zoo meenige delikaate <rhyme label="b" type="m">kost</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Met de Madammen lustig <rhyme label="a" type="f" corresp="#s6:l1">domineeren</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Oranje Appeltjes de beste <rhyme label="b" type="m" corresp="#s6:l2">kost</rhyme>.</l>
        <l id="s6:l5" met="2" type="-+">Krieken en <rhyme label="c" type="f">Capoenen</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Lekkere <rhyme label="c" type="f" corresp="#s6:l5">Limoenen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Hy dat al geeven <rhyme label="d" type="m">moet</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Vivat de Landman <rhyme label="d" type="m" corresp="#s6:l7">goed</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Wat zouden al de Kooplieden gaan <rhyme label="a" type="f">maaken</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">De Herbergier, Brouwer en <rhyme label="b" type="m">Winkelier</rhyme>,</l>
        <l id="s7:l3" met="5" type="-+">Zonder den Boer en konde niets <rhyme label="a" type="f" corresp="#s7:l1">geraaken</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">Hoe zoude zy verkoopen waaen <rhyme label="b" type="m" corresp="#s7:l2">dier</rhyme>.</l>
        <l id="s7:l5" met="2" type="-+">Zonder de <rhyme label="c" type="f">Boeren</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Niemand kan <rhyme label="c" type="f" corresp="#s7:l5">roeren</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">En door de Landman <rhyme label="d" type="m">goed</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Die ons zaam spyzen <rhyme label="d" type="m" corresp="#s7:l7">doet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">De Mulder met zyn wyf en meid en <rhyme label="a" type="f">knegten</rhyme>,</l>
        <l id="s8:l2" met="5" type="-+">Zoude van honger sterven en <rhyme label="b" type="m">vergaan</rhyme>,</l>
        <l id="s8:l3" met="5" type="-+">Niet een Soldaat zoude kunnen <rhyme label="a" type="f" corresp="#s8:l1">vegten</rhyme>,</l>
        <l id="s8:l4" met="5" type="-+">Waar by drie of vier maanden zonder <rhyme label="b" type="m" corresp="#s8:l2">graan</rhyme>.</l>
        <l id="s8:l5" met="2" type="-+">De Boer moet <rhyme label="c" type="f">haalen</rhyme></l>
        <l id="s8:l6" met="2" type="-+">Zoude zy <rhyme label="c" type="f" corresp="#s8:l5">maalen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Valt hem daar voor te <rhyme label="d" type="m">voet</rhyme>.</l>
        <l id="s8:l8" met="3" type="-+">Vivat de Landman <rhyme label="d" type="m" corresp="#s8:l7">goed</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">Wat zou de Bakker en de Brouwer <rhyme label="a" type="f">maaken</rhyme>,</l>
        <l id="s9:l2" met="5" type="-+">Kost hy niet koopen Koorn, Gerst en <rhyme label="b" type="m">Mout</rhyme>,</l>
        <l id="s9:l3" met="5" type="-+">Zonder de Landman niemant kan <rhyme label="a" type="f" corresp="#s9:l1">geraaken</rhyme>,</l>
        <l id="s9:l4" met="5" type="-+">Want niemant en kan hakken zonder <rhyme label="b" type="m" corresp="#s9:l2">hout</rhyme>.</l>
        <l id="s9:l5" met="2" type="-+">Brouwer en <rhyme label="c" type="f">Bakker</rhyme></l>
        <l id="s9:l6" met="2" type="-+">Leeft van de <rhyme label="c" type="f" corresp="#s9:l5">akker</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Kust hem om den <rhyme label="d" type="m">voet</rhyme>.</l>
        <l id="s9:l8" met="3" type="-+">Vivat den Landman <rhyme label="d" type="m" corresp="#s9:l7">goed</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="5" type="-+">Oorlof voor 't lest gy Boeren en <rhyme label="a" type="f">Boerinnen</rhyme>,</l>
        <l id="s10:l2" met="5" type="-+">'k Heb u gepreezen in het <rhyme label="b" type="m">generaal</rhyme>,</l>
        <l id="s10:l3" met="5" type="-+">'k Hoop dat gy den Hemel zult <rhyme label="a" type="f" corresp="#s10:l1">winnen</rhyme>,</l>
        <l id="s10:l4" met="5" type="-+">Sorgt dan wel voor het <rhyme label="b" type="m" corresp="#s10:l2">principaal</rhyme>,</l>
        <l id="s10:l5" met="2" type="-+">Al <rhyme label="c" type="f">pryzenswaardig</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Syt niet <rhyme label="c" type="f" corresp="#s10:l5">hovaardig</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Leeft te zamen in <rhyme label="d" type="m">ootmoed</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Looft altyd Jezus <rhyme label="d" type="m" corresp="#s10:l7">zoet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
