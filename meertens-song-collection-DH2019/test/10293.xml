        <l id="s1:l1" met="3" type="-+">Bedroghen <rhyme label="a" type="m">nimmermeer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dat sweer ick by mijn <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>,</l>
