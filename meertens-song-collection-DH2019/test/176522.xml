      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ARnold en ghy <rhyme label="a" type="f">Hillegonde</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Dat ik nu wat spelen <rhyme label="b" type="m">konde</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Toreen lust vermaak en <rhyme label="a" type="f" corresp="#s1:l1">los</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Om u Feeste te <rhyme label="b" type="m" corresp="#s1:l2">vereeren</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">'k Sou my als een Switser <rhyme label="c" type="f">weeren</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Maar mijn Rietjen is te <rhyme label="c" type="f" corresp="#s1:l5">grof</rhyme>.</l>
      </lg>
