    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 021 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DIE Koningh sal seer sijn <rhyme label="a" type="m">verheught</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat hy doer v' handt <rhyme label="b" type="f">krachtigh</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Verlost is Heer <rhyme label="b" type="f" corresp="#s1:l2">almachtigh</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Hoe vol sal hy wesen der <rhyme label="a" type="m" corresp="#s1:l1">vreughdt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Siende dat hy <rhyme label="c" type="m">gewis</rhyme>:</l>
        <l id="s1:l6" met="3" type="-+">Doer v' kracht bevrydt <rhyme label="c" type="m" corresp="#s1:l5">is</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Synen lust end beghęeren <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">So hy dat <rhyme label="b" type="f">ouerlegghet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Heer ghy hem niet <rhyme label="b" type="f" corresp="#s2:l2">ontsegghet</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">End al dat hy v bidden <rhyme label="a" type="m" corresp="#s2:l1">sal</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Daer van hy doet <rhyme label="c" type="m">vermaen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Sal hy van v <rhyme label="c" type="m" corresp="#s2:l5">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want eer hy bidt sijnde <rhyme label="a" type="m">benauwt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Maeckt ghy hem (Heere <rhyme label="b" type="f">goedigh</rhyme>)</l>
        <l id="s3:l3" met="3" type="-+">Met ryckdom <rhyme label="b" type="f" corresp="#s3:l2">ouervloedigh</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Een kroone van gheloutert <rhyme label="a" type="m" corresp="#s3:l1">gaudt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Maeckt ghy hem, die ghy <rhyme label="c" type="m">stelt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Op't hooft midden in't <rhyme label="c" type="m" corresp="#s3:l5">veldt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy beghęerde doch anders <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dan dat ghy hem woudt <rhyme label="b" type="f">gheuen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Slechts eenen tydt om <rhyme label="b" type="f" corresp="#s4:l2">leuen</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">Ghy hebt hier bouen (so men <rhyme label="a" type="m" corresp="#s4:l1">siet</rhyme>)</l>
        <l id="s4:l5" met="3" type="-+">Hem vergont <rhyme label="c" type="m">ouervloedt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Iae t'eewigh leuen <rhyme label="c" type="m" corresp="#s4:l5">soet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Doer v' goetheit maeckt ghy <rhyme label="a" type="m">altydt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat sijnen goeden <rhyme label="b" type="f">naeme</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wydt verbreydt werdt <rhyme label="b" type="f" corresp="#s5:l2">bequaeme</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Want ghy Heer die goedertier <rhyme label="a" type="m" corresp="#s5:l1">sijt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Hem prys end eere <rhyme label="c" type="m">wilt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Altydt gheuen seer <rhyme label="c" type="m" corresp="#s5:l5">milt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy verciert hem, dat hy hier <rhyme label="a" type="m">naer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Eenen spieghel sal <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Vwer goetheit <rhyme label="b" type="f" corresp="#s6:l2">ghepresen</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Ghy hebt hem verheughet <rhyme label="a" type="m" corresp="#s6:l1">voorwaer</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">End sijn herte <rhyme label="c" type="m">verlicht</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Doer v' lieflick <rhyme label="c" type="m" corresp="#s6:l5">ghesicht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Op dat die Koningh sij <rhyme label="a" type="m">bevrydt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">In sijn leedt end <rhyme label="b" type="f">benauwen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Op Godt staet sijn <rhyme label="b" type="f" corresp="#s7:l2">vertrauwen</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Van Godt wachtt hy hulp' in den <rhyme label="a" type="m" corresp="#s7:l1">strydt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Doer welcken hy <rhyme label="c" type="m">eenpaer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Vast blyft sonder <rhyme label="c" type="m" corresp="#s7:l5">gheuaer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">V' handt is sterck ghenoegh om <rhyme label="a" type="m">slaen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Iae om neder te <rhyme label="b" type="f">vellen</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Die hen teghen v <rhyme label="b" type="f" corresp="#s8:l2">stellen</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Sy werden oock te niet <rhyme label="a" type="m" corresp="#s8:l1">ghedaen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Die uwen prys end <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Hebben verachtet <rhyme label="c" type="m" corresp="#s8:l5">Heer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">V' gramschap hen verslinden <rhyme label="a" type="m">sal</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Als eenen heeten <rhyme label="b" type="f">ouen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Onder vol vyers end <rhyme label="b" type="f" corresp="#s9:l2">bouen</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Sy werden oock verdoruen <rhyme label="a" type="m" corresp="#s9:l1">al</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Van v daer men op <rhyme label="c" type="m">siet</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Werden sy bracht tot <rhyme label="c" type="m" corresp="#s9:l5">niet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Wtgheroeyt werden sy met <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Van der ęerden met <rhyme label="b" type="f">machte</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Sy end al haer <rhyme label="b" type="f" corresp="#s10:l2">gheslachte</rhyme>.</l>
        <l id="s10:l4" met="4" type="-+">Daer en werdt gedachtenis <rhyme label="a" type="m" corresp="#s10:l1">gheen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Noch gewagh vroegh of <rhyme label="c" type="m">spaedt</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Van haeren stamme <rhyme label="c" type="m" corresp="#s10:l5">quaedt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Om dat se hebben <rhyme label="a" type="m">onderstaen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Den Koningh te <rhyme label="b" type="f">beschaeden</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Met allerley <rhyme label="b" type="f" corresp="#s11:l2">boosdaeden</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">Teghen v sy met list <rhyme label="a" type="m" corresp="#s11:l1">raedslaen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Maer daer werdt doer haer <rhyme label="c" type="m">kracht</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Niet besonders <rhyme label="c" type="m" corresp="#s11:l5">volbracht</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Als waer't dat die boos' <rhyme label="a" type="m">opgericht</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Te saem teghen v <rhyme label="b" type="f">quaemen</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Ghy soudt se haest <rhyme label="b" type="f" corresp="#s12:l2">beschaemen</rhyme>.</l>
        <l id="s12:l4" met="4" type="-+">Ghy sult se schieten in't <rhyme label="a" type="m" corresp="#s12:l1">aensicht</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Daer op hebt ghy <rhyme label="c" type="m">ghemickt</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">End' uwen bogh' <rhyme label="c" type="m" corresp="#s12:l5">gheschickt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Daer om maeckt v doch op O <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Wilt met ernst nu <rhyme label="b" type="f">bewysen</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">V kracht niet om <rhyme label="b" type="f" corresp="#s13:l2">volprysen</rhyme>.</l>
        <l id="s13:l4" met="4" type="-+">Op dat wy altyt t'uwer <rhyme label="a" type="m" corresp="#s13:l1">eer</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Prysen ons leuen <rhyme label="c" type="m">langh</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Vwe macht met <rhyme label="c" type="m" corresp="#s13:l5">lofsangh'</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
