    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Koninginne des <rhyme label="a" type="m">Hemels</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Verblijt u <rhyme label="a" type="m" corresp="#s1:l1">Maria</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Den ghy hebt <rhyme label="b" type="f">ontfangen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die ist van den dooden <rhyme label="b" type="f" corresp="#s1:l3">operstanden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Bidt Godt voor ons. <rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maria Maget <rhyme label="a" type="m">wtverkooren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Wend van ons ws kints <rhyme label="a" type="m" corresp="#s2:l1">toorn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat hy ons wil <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die vreucht in dat eewigh <rhyme label="b" type="f" corresp="#s2:l3">leven</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Bidt Godt voor ons. <rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Koninginne der <rhyme label="a" type="m">eeren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Versoent ons doch den <rhyme label="a" type="m" corresp="#s3:l1">Heeren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Als wy sullen <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Helpt ons by Godt genaed <rhyme label="b" type="f" corresp="#s3:l3">verwerven</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Bidt Godt voor ons. <rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Maria Moeder reyne <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Den last der sonden ons <rhyme label="a" type="m" corresp="#s4:l1">mishaeght</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Daerom willen wy ons <rhyme label="b" type="f">bekeeren</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Versoenen ons met Godt den <rhyme label="b" type="f" corresp="#s4:l3">Heeren</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Bidt Godt voor ons. <rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Bidt voor ons Hemels <rhyme label="a" type="m">Koningin</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat wy vrolijck vaeren daer <rhyme label="a" type="m" corresp="#s5:l1">hin</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wt die bedroefde <rhyme label="b" type="f">jammerdal</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Behoedt ons voor eewiger <rhyme label="b" type="f" corresp="#s5:l3">qual</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Bidt Godt voor ons. <rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Vreucht u lieve <rhyme label="a" type="m">Christenheyt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Looft Godt met <rhyme label="a" type="m" corresp="#s6:l1">bestendigheyt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die aent cruys heeft <rhyme label="b" type="f">gehangen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Die is van den dooden <rhyme label="b" type="f" corresp="#s6:l3">operstanden</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Bidt Godt voor ons. <rhyme label="c" type="m">Alleluia</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
