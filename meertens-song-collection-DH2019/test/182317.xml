<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182317</id>
    <date>
      <year-start>1786</year-start>
      <year-end>1786</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Daar waren twee wijfjes die klopten malkaar</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vrinden ik weet al weer een nieuw <rhyme label="a" type="m">liet</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wat in zekere straat al is <rhyme label="a" type="m" corresp="#s1:l1">geschied</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Lourensie, <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Wat in zekere straat al is <rhyme label="a" type="m" corresp="#s1:l1">geschied</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al van een meisje <rhyme label="a" type="m">pertinent</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Haar naam die is ook wel <rhyme label="a" type="m" corresp="#s2:l1">bekent</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Lourensie, <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Haar naam die is ook wel <rhyme label="a" type="m" corresp="#s2:l1">bekent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'t Was op een avond haar moeder was niet <rhyme label="a" type="m">tuys</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Toen liet zy daar twee Jonge knaapen in <rhyme label="a" type="m" corresp="#s3:l1">huys</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Lourensie, <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Toen liet zy daar twee jonge knaapen in <rhyme label="a" type="m" corresp="#s3:l1">huys</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Zy knoeyde en stoeyde en voelde ze <rhyme label="a" type="m">lang</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Eer dat zy raakte al aan de <rhyme label="a" type="m" corresp="#s4:l1">gang</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Lourensie, <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Eer dat zy raakte al aan de <rhyme label="a" type="m" corresp="#s4:l1">gang</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maar toe zy met haar zaken raakten <rhyme label="a" type="m">klaar</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Toen wierde het daar de Buuren <rhyme label="a" type="m" corresp="#s5:l1">gewaar</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Lourensie, <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Toen wierde het daar de Buuren <rhyme label="a" type="m" corresp="#s5:l1">gewaar</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Zy schopte en trapte toen op de <rhyme label="a" type="m">deur</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Zy spraken de Timmerman moet komen <rhyme label="a" type="m" corresp="#s6:l1">veur</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Lourensie, <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Zy spraken de Timmerman moet komen <rhyme label="a" type="m" corresp="#s6:l1">veur</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Maar toen zy hoorden dat groot <rhyme label="a" type="m">rumoer</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Sprongen zy uit bed al op de <rhyme label="a" type="m" corresp="#s7:l1">vloer</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Lourensie, <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Sprongen zy uit bed al op de <rhyme label="a" type="m" corresp="#s7:l1">vloer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De Knaapies die Vlugten de deur toen <rhyme label="a" type="m">uit</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Toen raakten het heele spel <rhyme label="a" type="m" corresp="#s8:l1">verbruyt</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Lourensie, <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Toen raakten het heele spel <rhyme label="a" type="m" corresp="#s8:l1">verbruyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Oorlof meisjes tot <rhyme label="a" type="m">besluyt</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Neemt u slag zoo niet waar, al is u moeder <rhyme label="a" type="m" corresp="#s9:l1">uit</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Lourensie: <rhyme label="b" type="f">kardoesie</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Neemt u slag zoo niet waar, al is u moeder <rhyme label="a" type="m" corresp="#s9:l1">uit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
