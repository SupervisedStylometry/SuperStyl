  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O God van <rhyme label="a" type="f">Hemelrijcke</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghy sijt al onsen <rhyme label="b" type="m">schilt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Bewaert ons <rhyme label="a" type="f" corresp="#s1:l1">algelijcke</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Door u ghenade <rhyme label="b" type="m" corresp="#s1:l2">milt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">En wilt ons wijsheyt <rhyme label="c" type="f">geven</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">en sterck ons met accoort // int <rhyme label="d" type="m">woordt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Op dat wy mogen <rhyme label="c" type="f" corresp="#s1:l5">leven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Als Christenen <rhyme label="d" type="m" corresp="#s1:l6">behoort</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vredich te leven <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Laet ons tot alder <rhyme label="b" type="m">tijdt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ons na den vrede <rhyme label="a" type="f" corresp="#s2:l1">keeren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die na volgen met <rhyme label="b" type="m" corresp="#s2:l2">vlijt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dat wy wandelen <rhyme label="c" type="f">peure</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Soo onsen roep ghenaemt // <rhyme label="d" type="m">betaemt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Recht reyn na de <rhyme label="c" type="f" corresp="#s2:l5">Schriftuere</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Vredich by een <rhyme label="d" type="m" corresp="#s2:l6">versaemt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy sijt een Godt des <rhyme label="a" type="f">vreden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Niet een God der <rhyme label="b" type="m">tweedracht</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Soo moeten al de <rhyme label="a" type="f" corresp="#s3:l1">leden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Thooft na volgen met <rhyme label="b" type="m" corresp="#s3:l2">kracht</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Vlieden twist en <rhyme label="c" type="f">discoorden</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Al voor een slange quaet // <rhyme label="d" type="m">verstaet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Recht na des Heeren <rhyme label="c" type="f" corresp="#s3:l5">woorden</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Bewijsen met der <rhyme label="d" type="m" corresp="#s3:l6">daet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wilt met der daet <rhyme label="a" type="f">bewijsen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Laet blijcken vruchten <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dat de Werelt mach <rhyme label="a" type="f" corresp="#s4:l1">prijsen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">U Hemels Vader <rhyme label="b" type="m" corresp="#s4:l2">soet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Laet lichten seer <rhyme label="c" type="f">manierich</rhyme></l>
        <l id="s4:l6" met="4" type="-+">U licht int openbaer // seer <rhyme label="d" type="m">klaer</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Als Maeghden wijs <rhyme label="c" type="f" corresp="#s4:l5">vercierich</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Volcht u Bruyd'gom <rhyme label="d" type="m" corresp="#s4:l6">eenpaer</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laet ons vredich <rhyme label="a" type="f">nagangen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat hooft Christi <rhyme label="b" type="m">voorseyt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Tverstant geven <rhyme label="a" type="f" corresp="#s5:l1">gevangen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In zijn <rhyme label="b" type="m" corresp="#s5:l2">gehoorsaemheyt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die Gods geboden <rhyme label="c" type="f">houwen</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Volght niet u eygen hooft // <rhyme label="d" type="m">verdooft</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Segt ons Syrach <rhyme label="c" type="f" corresp="#s5:l5">voltrouwen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Dat vry van hem <rhyme label="d" type="m" corresp="#s5:l6">ghelooft</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dat sout sijn cracht <rhyme label="a" type="f">verlooren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Moet uyt soo schrift <rhyme label="b" type="m">gewaeght</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dat suere deech al <rhyme label="a" type="f" corresp="#s6:l1">vooren</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Moet worden <rhyme label="b" type="m" corresp="#s6:l2">uytgevaecht</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Op dat wy moghen <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Sonder Schalcheyt en list // vol <rhyme label="d" type="m">twist</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Ontrecken boven <rhyme label="c" type="f" corresp="#s6:l5">desen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Van die de waerheyt <rhyme label="d" type="m" corresp="#s6:l6">mist</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die d' argenisse <rhyme label="a" type="f">aenrechten</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Die stroyen quaet <rhyme label="b" type="m">fenijn</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Daer deur des Heeren <rhyme label="a" type="f" corresp="#s7:l1">knechten</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Dickmael vererget <rhyme label="b" type="m" corresp="#s7:l2">sijn</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Twaer na schrifts <rhyme label="c" type="f">vermonden</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Beter een Molen steen // niet <rhyme label="d" type="m">cleen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Aen sijn hals <rhyme label="c" type="f" corresp="#s7:l5">gebonden</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Geworpen inde <rhyme label="d" type="m" corresp="#s7:l6">Zeen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Sulck argelicke <rhyme label="a" type="f">leden</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Moeten soo ick <rhyme label="b" type="m">verstae</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Van 't lichaem sijn <rhyme label="a" type="f" corresp="#s8:l1">gesneden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Want sy doen groote <rhyme label="b" type="m" corresp="#s8:l2">schae</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Sy sijn als die op <rhyme label="c" type="f">breken</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">De water dammen sterck // haer <rhyme label="d" type="m">werc</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Wort daer by <rhyme label="c" type="f" corresp="#s8:l5">vergeleecken</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Na Salomons <rhyme label="d" type="m" corresp="#s8:l6">opmerck</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wat mocht doch Corach <rhyme label="a" type="f">baten</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dathan, Abyron <rhyme label="b" type="m">me</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Datse Israel boven <rhyme label="a" type="f" corresp="#s9:l1">maten</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Brochten in groote <rhyme label="b" type="m" corresp="#s9:l2">onvre</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Sy maeckten groot <rhyme label="c" type="f">oproeren</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Tegens sHeeren-verbont // <rhyme label="d" type="m">terstont</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Maer levendich zy <rhyme label="c" type="f" corresp="#s9:l5">voeren</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Al in der Hellen <rhyme label="d" type="m" corresp="#s9:l6">gront</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Seba vol van <rhyme label="a" type="f">practijcke</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Die droech op David <rhyme label="b" type="m">nijt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Heeft Israel <rhyme label="a" type="f" corresp="#s10:l1">autentijcke</rhyme></l>
        <l id="s10:l4" met="3" type="-+">verweckt tot eenen <rhyme label="b" type="m" corresp="#s10:l2">strijt</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Maecktese seer <rhyme label="c" type="f">afwendich</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Al van den Coninck goet // den <rhyme label="d" type="m">bloedt</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Quam oock noch seer <rhyme label="c" type="f" corresp="#s10:l5">ellendich</rhyme></l>
        <l id="s10:l8" met="3" type="-+">In de doodt met der <rhyme label="d" type="m" corresp="#s10:l6">spoet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Die heymelicke <rhyme label="a" type="f">steken</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Wonden haer selven <rhyme label="b" type="m">seer</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Gelyck dat is <rhyme label="a" type="f" corresp="#s11:l1">gebleecken</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Aen Haman en noch <rhyme label="b" type="m" corresp="#s11:l2">meer</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Die Israel <rhyme label="c" type="f">benoude</rhyme></l>
        <l id="s11:l6" met="4" type="-+">Maer loon hy van sulc dinc // <rhyme label="d" type="m">ontfinc</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Den boom hy om haer <rhyme label="c" type="f" corresp="#s11:l5">boude</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Daer hy noch selve aen <rhyme label="d" type="m" corresp="#s11:l6">hinck</rhyme>:</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Balam schalck sonder <rhyme label="a" type="f">misse</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Gaf heymelijck <rhyme label="b" type="m">raet</rhyme></l>
        <l id="s12:l3" met="3" type="-+">En stelde een <rhyme label="a" type="f" corresp="#s12:l1">argernisse</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Onder Israels <rhyme label="b" type="m" corresp="#s12:l2">saet</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Want Balack die sant <rhyme label="c" type="f">Vrouwen</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Int leger op gerust seer <rhyme label="d" type="m">juyst</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Daer door quamen in <rhyme label="c" type="f" corresp="#s12:l5">rouwen</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Wel drie en twintich <rhyme label="d" type="m" corresp="#s12:l6">duyst</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Prince den twist wilt <rhyme label="a" type="f">schouwen</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Hebt vrede uwen <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Sijt meer gesint om <rhyme label="a" type="f" corresp="#s13:l1">spouwen</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Dan om blasen daer <rhyme label="b" type="m" corresp="#s13:l2">in</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Soo zult ghy niet <rhyme label="c" type="f">verbranden</rhyme></l>
        <l id="s13:l6" met="4" type="-+">komt ghy hem niet te naer // <rhyme label="d" type="m">voorwaer</rhyme></l>
        <l id="s13:l7" met="3" type="-+">In Steden en in <rhyme label="c" type="f" corresp="#s13:l5">Landen</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Maket vyer groot <rhyme label="d" type="m" corresp="#s13:l6">misbaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
