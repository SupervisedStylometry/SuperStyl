    <melody>Het viel een hemelse dauw</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GY sult al saligh <rhyme label="a" type="f">weesen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Indien g' uw' Heer uw' <rhyme label="b" type="m">Godt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Uyt liefd' wilt leere <rhyme label="a" type="f" corresp="#s1:l1">vreesen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En houden sijn <rhyme label="b" type="m" corresp="#s1:l2">Gebodt</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">De droeve <rhyme label="c" type="m">tribulati</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Sal hy <rhyme label="d" type="f">veranderen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Door Goddelijke <rhyme label="c" type="m" corresp="#s1:l5">grati</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En blijde <rhyme label="d" type="f" corresp="#s1:l6">troostingen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den arbeydt uwer <rhyme label="a" type="f">handen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sult gy dan eeten <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Een Wijngaert aen de <rhyme label="a" type="f" corresp="#s2:l1">Wanden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Uw' Huysvrouw weesen <rhyme label="b" type="m" corresp="#s2:l2">sel</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Vervult met schoone <rhyme label="c" type="m">vrughten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Tot troostingh van den <rhyme label="d" type="f">Man</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Die duysenden <rhyme label="c" type="m" corresp="#s2:l5">genughten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Van haer ontfangen <rhyme label="d" type="f" corresp="#s2:l6">kan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als jonge groen' <rhyme label="a" type="f">Olyven</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sullen uw Kinders <rhyme label="b" type="m">vroet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Rontom uw tafel <rhyme label="a" type="f" corresp="#s3:l1">blijven</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En u vermaken <rhyme label="b" type="m" corresp="#s3:l2">soet</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Jerusalemsche <rhyme label="c" type="m">goeden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Tot troost van uwen <rhyme label="d" type="f">geest</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Uyt Sion sullen <rhyme label="c" type="m" corresp="#s3:l5">vloeden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Soo gy den Heere <rhyme label="d" type="f" corresp="#s3:l6">vreest</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Siet dusdanigen <rhyme label="a" type="f">seegen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Wil Godt u geeven <rhyme label="b" type="m">milt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Indien gy maer sijn <rhyme label="a" type="f" corresp="#s4:l1">weegen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Vroom'lijk intreeden <rhyme label="b" type="m" corresp="#s4:l2">wilt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dus soek Godt te <rhyme label="c" type="m">behagen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In uwen <rhyme label="d" type="f">Eghten-staet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Op dat g' in lange <rhyme label="c" type="m" corresp="#s4:l5">dagen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Mooght sien uw' Kinders <rhyme label="d" type="f" corresp="#s4:l6">saet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Goodts Kerke sal <rhyme label="a" type="f">vermeeren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Door uw' Godtvrughtigh <rhyme label="b" type="m">saet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Dat gy met deught en <rhyme label="a" type="f" corresp="#s5:l1">eeren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Voortteelt in <rhyme label="b" type="m" corresp="#s5:l2">d'Eghten-staet</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Wel aen dan, o <rhyme label="c" type="m">Getrouden</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Gaet voort in alle <rhyme label="d" type="f">deught</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Godt sal sijn woordt wel <rhyme label="c" type="m" corresp="#s5:l5">houden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En gunnen 's Hemels <rhyme label="d" type="f" corresp="#s5:l6">vreught</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
