  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE HEER is Coninck over <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dies hem verheucht dat aertsche <rhyme label="a" type="m" corresp="#s1:l1">dal</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Veel Eylanden zijn <rhyme label="b" type="f">blijde</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Met Wolcken is hy wel <rhyme label="c" type="m">becleedt</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Gherechte end <rhyme label="c" type="m" corresp="#s1:l4">Rechtveerdicheet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Stercken' syn Stoel <rhyme label="b" type="f" corresp="#s1:l3">altijde</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Een Vuyr daer gaet voor hem al <rhyme label="d" type="m">hen</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Ontsteeckt al syne <rhyme label="d" type="m" corresp="#s1:l7">Vyanden</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Syn Blixem Licht op <rhyme label="e" type="f">Aerden</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">De Aerd' verschrickt daer aff al <rhyme label="f" type="m">seer</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Als Was versmelten voor den <rhyme label="f" type="m" corresp="#s1:l10">HEER</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Die Berghen groot van <rhyme label="e" type="f" corresp="#s1:l9">weerden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het segghen syn <rhyme label="a" type="m">Gherechticheyt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die HEmelen seer wel <rhyme label="a" type="m" corresp="#s2:l1">bereyt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die Volcken sien syn <rhyme label="b" type="f">eere</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Het moeten schamen haer all' <rhyme label="c" type="m">gaer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Der Beelden Dienaers, En die <rhyme label="c" type="m" corresp="#s2:l4">haer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Des Affgoods Roemen <rhyme label="b" type="f" corresp="#s2:l3">seere</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">Aenbidt hem alle Goden <rhyme label="d" type="m">ghy</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Dit hoorde Zyon end' is <rhyme label="d" type="m" corresp="#s2:l7">bly</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Vol vreucht syn al die <rhyme label="e" type="f">steden</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">T' verheugen haer die Docht'ren <rhyme label="f" type="m">Jent</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Van Juda, (HEEr) dijns <rhyme label="f" type="m" corresp="#s2:l10">Regiment</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Om dijn <rhyme label="e" type="f" corresp="#s2:l9">Gherechticheden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Heer ghy zijt in den Hoochsten <rhyme label="a" type="m">Throon</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Op Aerden boven allen <rhyme label="a" type="m" corresp="#s3:l1">schoon</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Nu ghy die Vreest den <rhyme label="b" type="f">HEERE</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Het Boose haet met Hert en <rhyme label="c" type="m">Moet</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Der Heyl'gen Sielen Godt <rhyme label="c" type="m" corresp="#s3:l4">Behoedt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Van s'Boosen Handt end' <rhyme label="b" type="f" corresp="#s3:l3">Weere</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">De Vroom' een licht van hem <rhyme label="d" type="m">Ontfaen</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">End' Vreucht die altijdt Oprecht <rhyme label="d" type="m" corresp="#s3:l7">gaen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Ghy Vromen wilt syn <rhyme label="e" type="f">Blijde</rhyme></l>
        <l id="s3:l10" met="4" type="-+">In Godt den Heer, en Dancket <rhyme label="f" type="m">hem</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Syn Heylicheyt met Luyder <rhyme label="f" type="m" corresp="#s3:l10">stem</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Prijst nu en t'allen <rhyme label="e" type="f" corresp="#s3:l9">tijde</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
