  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">DEn Heere van <rhyme label="a" type="m">al</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Light in eenen <rhyme label="a" type="m" corresp="#s1:l1">stal</rhyme>:</l>
        <l id="s1:l3" met="2" type="-+">Laet klincken syn <rhyme label="b" type="f">glori'</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Doet syn <rhyme label="b" type="f" corresp="#s1:l3">victori'</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Singen t'allen <rhyme label="c" type="m">kant</rhyme>:</l>
        <l id="s1:l6" met="2" type="-+">Dat hy <rhyme label="d" type="f">neer-komt-daelen</rhyme>.</l>
        <l id="s1:l7" met="2" type="-+">t'Is om ons te <rhyme label="d" type="f" corresp="#s1:l6">haelen</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Uyt ons vyandts <rhyme label="c" type="m" corresp="#s1:l5">bandt</rhyme>:</l>
        <l id="s1:l9" met="2" type="-+">Hy eyndight de <rhyme label="e" type="m">pyn</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Van een droevigh <rhyme label="f" type="f">leven</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Voor vraeck, komt troost <rhyme label="f" type="f" corresp="#s1:l10">geven</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">En ons toe-vlucht <rhyme label="e" type="m" corresp="#s1:l9">syn</rhyme>.</l>
        <l id="s1:l13" met="2" type="-+">Verdriet, ende <rhyme label="g" type="m">smert</rhyme>,</l>
        <l id="s1:l14" met="2" type="-+">Vlucht verr' uyt ons <rhyme label="g" type="m" corresp="#s1:l13">hert</rhyme>;</l>
        <l id="s1:l15" met="2" type="-+">Siet daelen <rhyme label="h" type="f">benede</rhyme></l>
        <l id="s1:l16" met="2" type="-+">Den eeuwigen <rhyme label="h" type="f" corresp="#s1:l15">vrede</rhyme>:</l>
        <l id="s1:l17" met="2" type="-+">O prys van ons <rhyme label="i" type="m">klacht</rhyme>,</l>
        <l id="s1:l18" met="2" type="+-">Uur', lanck <rhyme label="i" type="m" corresp="#s1:l17">verwacht</rhyme>!</l>
        <l id="s1:l19" met="2" type="-+">Voor al <rhyme label="h" type="f" corresp="#s1:l15">eeuwigheden</rhyme></l>
        <l id="s1:l20" met="2" type="-+">Is ons wensch <rhyme label="i" type="m" corresp="#s1:l17">volbracht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Serpent, wy syn <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Van uw' <rhyme label="a" type="m" corresp="#s2:l1">slaeverny</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">Verr' syn onse <rhyme label="b" type="f">suchten</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Ons brenght <rhyme label="b" type="f" corresp="#s2:l3">genuchten</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Desen blyden <rhyme label="c" type="m">tydt</rhyme>:</l>
        <l id="s2:l6" met="2" type="-+">Uw' hoop' is <rhyme label="d" type="f">verloren</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Om ons, <rhyme label="d" type="f" corresp="#s2:l6">uyt-verkoren</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Groeyt uw meerder <rhyme label="c" type="m" corresp="#s2:l5">spyt</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">Godt in <rhyme label="e" type="m">Bethlehem</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Verlicht ons te <rhyme label="f" type="f">gader</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Hy komt syn ons <rhyme label="f" type="f" corresp="#s2:l10">Vader</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">En wy volgen <rhyme label="e" type="m" corresp="#s2:l9">hem</rhyme>.</l>
        <l id="s2:l13" met="2" type="-+">Hy doet <rhyme label="g" type="m">vergaen</rhyme></l>
        <l id="s2:l14" met="2" type="-+">Uw leet, ons <rhyme label="g" type="m" corresp="#s2:l13">gedaen</rhyme>:</l>
        <l id="s2:l15" met="2" type="-+">Godt komt u <rhyme label="h" type="f">bestryden</rhyme>,</l>
        <l id="s2:l16" met="2" type="-+">Om ons te <rhyme label="h" type="f" corresp="#s2:l15">bevryden</rhyme>:</l>
        <l id="s2:l17" met="2" type="-+">Hy sal u <rhyme label="i" type="m">verslaen</rhyme>.</l>
        <l id="s2:l18" met="2" type="+-">Ons' <rhyme label="i" type="m" corresp="#s2:l17">Saeligheydt</rhyme></l>
        <l id="s2:l19" met="2" type="-+">Verlost ons van <rhyme label="h" type="f" corresp="#s2:l15">lyden</rhyme></l>
        <l id="s2:l20" met="2" type="-+">Voor al' <rhyme label="i" type="m" corresp="#s2:l17">eeuwigheydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
