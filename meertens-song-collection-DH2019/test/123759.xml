    <melody>De wereld is in rep en roer</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Van al de Landen hier op <rhyme label="a" type="m">aard</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Is Holland mij het meeste <rhyme label="a" type="m" corresp="#s1:l1">waard</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Het bloeit door 's Hemels <rhyme label="b" type="f">zegen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Geen goud bedekt het in zijn <rhyme label="c" type="m">schoot</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Maar werkzaamheid verdient er 't <rhyme label="c" type="m" corresp="#s1:l4">brood</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In ruimte aller <rhyme label="b" type="f" corresp="#s1:l3">wegen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Van al de Talen hier op <rhyme label="a" type="m">aard</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Is Hollands taal mij 't meeste <rhyme label="a" type="m" corresp="#s2:l1">waard</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Z'is zuiver van <rhyme label="b" type="f">gebreken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Op maagdenlippen klinkt zij <rhyme label="c" type="m">zacht</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Een man kan nooit met forscher <rhyme label="c" type="m" corresp="#s2:l4">kracht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dan in het Hollandsch <rhyme label="b" type="f" corresp="#s2:l3">spreken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Van al de Zeden hier op <rhyme label="a" type="m">aard</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Zijn die van Holland 't meest mij <rhyme label="a" type="m" corresp="#s3:l1">waard</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Waar zij niet zijn <rhyme label="b" type="f">bedorven</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Gaan vreugde en Godsdienst hand aan <rhyme label="c" type="m">hand</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Door moed en zeden heeft ons <rhyme label="c" type="m" corresp="#s3:l4">land</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zijn grootste roem <rhyme label="b" type="f" corresp="#s3:l3">verworven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Van al de Meisjes hier op <rhyme label="a" type="m">aard</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Zijn die van Holland 't meest mij <rhyme label="a" type="m" corresp="#s4:l1">waard</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'k Wist ze nergens op te <rhyme label="b" type="f">sporen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Die zoo beminnelijk zijn en <rhyme label="c" type="m">goed</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Dan die bij ons zijn <rhyme label="c" type="m" corresp="#s4:l4">opgevoed</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In Holland zijn <rhyme label="b" type="f" corresp="#s4:l3">geboren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Van al de Vrouwen hier op <rhyme label="a" type="m">aard</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Zijn die van Holland 't meest mij <rhyme label="a" type="m" corresp="#s5:l1">waard</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Zij doen ons gaan op <rhyme label="b" type="f">bloemen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Waar kan men meer op <rhyme label="c" type="m">huwelijksmin</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Op trouw zorg in 't <rhyme label="c" type="m" corresp="#s5:l4">huisgezin</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Dan hier in Holland <rhyme label="b" type="f" corresp="#s5:l3">roemen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Van al de Dichters hier op <rhyme label="a" type="m">aard</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Zijn die van Holland 't meest mij <rhyme label="a" type="m" corresp="#s6:l1">waard</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hoe voelen we ons <rhyme label="b" type="f">ontgloeijen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Wanneer er van hun gouden <rhyme label="c" type="m">lier</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Met zachtheid, kracht en edelen <rhyme label="c" type="m" corresp="#s6:l4">zwier</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">De schoonste zangen <rhyme label="b" type="f" corresp="#s6:l3">vloeijen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Van al de Vorsten hier op <rhyme label="a" type="m">aard</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Is NeÃªrlands Koning 't meest mij <rhyme label="a" type="m" corresp="#s7:l1">waard</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zijn welvaart is de <rhyme label="b" type="f">mijnen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Geboren op dezelfde <rhyme label="c" type="m">grond</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Stond mijne wieg waar zijne <rhyme label="c" type="m" corresp="#s7:l4">stond</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Mijn graf zal staan bij 't <rhyme label="b" type="f" corresp="#s7:l3">zijnen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Van alles wat men vind op <rhyme label="a" type="m">aard</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Is Holland mij het meeste <rhyme label="a" type="m" corresp="#s8:l1">waard</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Want waar we ons ook <rhyme label="b" type="f">begeven</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Naar Noord of Zuid naar Oost of <rhyme label="c" type="m">West</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">In Holland vind men alles <rhyme label="c" type="m" corresp="#s8:l4">best</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Lang! lang! moet Holland <rhyme label="b" type="f" corresp="#s8:l3">leven</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
