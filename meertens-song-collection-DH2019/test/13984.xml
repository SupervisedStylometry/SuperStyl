<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13984</id>
    <date>
      <year-start>1670</year-start>
      <year-end>1670</year-end>
    </date>
    <location>
      <orginal-name>middelburg</orginal-name>
      <standard-name>Middelburg, The Netherlands</standard-name>
      <coordinates>
        <lon>51.4997302</lon>
        <lat>3.6136962</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>J'aimerai en ce village</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">WAt zijnd 'er al <rhyme label="a" type="f">ploegen</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Op de Aerde <rhyme label="b" type="m">wijdt</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Om d' Aerde te <rhyme label="a" type="f" corresp="#s1:l1">voegen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Tot den <rhyme label="b" type="m" corresp="#s1:l2">Somer-tijdt</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Om vruchten te <rhyme label="c" type="f">geven</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Na dat men 't zaet daer in <rhyme label="d" type="m">werpt</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">(Vruchten die als <rhyme label="c" type="f" corresp="#s1:l5">leven</rhyme>)</l>
        <l id="s1:l8" met="4" type="+-">En ons herte mer <rhyme label="d" type="m" corresp="#s1:l6">versterckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ick quam eens <rhyme label="a" type="f">getreden</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Van Armuyden <rhyme label="b" type="m">af</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Naer Camp-Veer de <rhyme label="a" type="f" corresp="#s2:l1">stede</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Daer ick mijn <rhyme label="b" type="m" corresp="#s2:l2">begaf</rhyme></l>
        <l id="s2:l5" met="3" type="+-">In mijn <rhyme label="c" type="f">eenigheden</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Overleyde doen <rhyme label="d" type="m">terstont</rhyme>;</l>
        <l id="s2:l7" met="3" type="+-">Hoe dat liefd' en <rhyme label="c" type="f" corresp="#s2:l5">vrede</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Altijt most zijn in ons <rhyme label="d" type="m" corresp="#s2:l6">mondt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Want Gods goede <rhyme label="a" type="f">wercken</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Die zijn <rhyme label="b" type="m">wonderbaer</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Men mach 't wel <rhyme label="a" type="f" corresp="#s3:l1">bemercken</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">'t Is over al <rhyme label="b" type="m" corresp="#s3:l2">klaer</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Dat hy Wijn en <rhyme label="c" type="f">Oly</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Kooren geeft tot ons <rhyme label="d" type="m">behoet</rhyme>:</l>
        <l id="s3:l7" met="3" type="+-">Hy spijst Vee en <rhyme label="c" type="f" corresp="#s3:l5">Menschen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Want hy is doch eeuwigh <rhyme label="d" type="m" corresp="#s3:l6">goet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Die des Aerdtrijcx <rhyme label="a" type="f">gronden</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Heeft soo vast <rhyme label="b" type="m">gestelt</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">En de Zee <rhyme label="a" type="f" corresp="#s4:l1">gebonden</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Die niet hoger <rhyme label="b" type="m" corresp="#s4:l2">zwelt</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Als ghy in uw' <rhyme label="c" type="f">palen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Haer doet rennen, aen haer <rhyme label="d" type="m">oort</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Datse snel moet <rhyme label="c" type="f" corresp="#s4:l5">dalen</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Op het wencken van u <rhyme label="d" type="m" corresp="#s4:l6">woort</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Boomen in der <rhyme label="a" type="f">Aerden</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Die staen schoon <rhyme label="b" type="m">geciert</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Met vruchten van <rhyme label="a" type="f" corresp="#s5:l1">waerden</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Van die 't Al <rhyme label="b" type="m" corresp="#s5:l2">regiert</rhyme>:</l>
        <l id="s5:l5" met="3" type="+-">Smaecken <rhyme label="c" type="f">veelerhande</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En van verscheyden <rhyme label="d" type="m">coleur</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Doet God <rhyme label="c" type="f" corresp="#s5:l5">offerande</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Danckt hem eeuwelijc daer <rhyme label="d" type="m" corresp="#s5:l6">veur</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">'k Hief doen op mijn <rhyme label="a" type="f">oogen</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Recht ter zee-waert <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Ick sag Menschen <rhyme label="a" type="f" corresp="#s6:l1">poogen</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Mede tot <rhyme label="b" type="m" corresp="#s6:l2">gewin</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">En met schepen <rhyme label="c" type="f">varen</rhyme>;</l>
        <l id="s6:l6" met="4" type="+-">Tot gerief van den <rhyme label="d" type="m">Koopman</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">En brengen de <rhyme label="c" type="f" corresp="#s6:l5">Waeren</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">En oock te voeren daer <rhyme label="d" type="m" corresp="#s6:l6">van</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Uyt Neptunis <rhyme label="a" type="f">vloeden</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Quamen oock te <rhyme label="b" type="m">landt</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Visschen, om te <rhyme label="a" type="f" corresp="#s7:l1">voeden</rhyme></l>
        <l id="s7:l4" met="3" type="+-">De natuere, <rhyme label="b" type="m" corresp="#s7:l2">want</rhyme></l>
        <l id="s7:l5" met="3" type="+-">God heeft in 't <rhyme label="c" type="f">Beginne</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Den Mensch als Heere <rhyme label="d" type="m">gestelt</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Dat hy nu kan <rhyme label="c" type="f" corresp="#s7:l5">winnen</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Op d' Aerde en Zee sijn <rhyme label="d" type="m" corresp="#s7:l6">gelt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Doen ging ick <rhyme label="a" type="f">bedincken</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Heer Almachtigh <rhyme label="b" type="m">God</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Waer aen soude 't <rhyme label="a" type="f" corresp="#s8:l1">mincken</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Als wy u <rhyme label="b" type="m" corresp="#s8:l2">gebodt</rhyme></l>
        <l id="s8:l5" met="3" type="+-">Van herten <rhyme label="c" type="f">betrachten</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Om te wesen <rhyme label="d" type="m">gehoorsaem</rhyme></l>
        <l id="s8:l7" met="3" type="+-">By daegh en by <rhyme label="c" type="f" corresp="#s8:l5">nachte</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Wy waren u <rhyme label="d" type="m" corresp="#s8:l6">aengenaem</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Die als <rhyme label="a" type="f">wandelaren</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Of, om sijn <rhyme label="b" type="m">handt-werck</rhyme></l>
        <l id="s9:l3" met="3" type="+-">Reysen ofte <rhyme label="a" type="f" corresp="#s9:l1">varen</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">(Neemt doch goet <rhyme label="b" type="m" corresp="#s9:l2">opmerck</rhyme>)</l>
        <l id="s9:l5" met="3" type="+-">Om te <rhyme label="c" type="f">speculeren</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Wat u tot leeringe <rhyme label="d" type="m">dient</rhyme>,</l>
        <l id="s9:l7" met="3" type="+-">Soo kondt ghy de <rhyme label="c" type="f" corresp="#s9:l5">Heere</rhyme></l>
        <l id="s9:l8" met="4" type="+-">Hebben als me-gaende <rhyme label="d" type="m" corresp="#s9:l6">Vriendt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
