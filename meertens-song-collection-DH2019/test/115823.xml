      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Fillis by 't ruischen der <rhyme label="a" type="f">blad'ren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Daar een klein beekje <rhyme label="b" type="m">vloeid</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Dat heene schiet door <rhyme label="a" type="f" corresp="#s1:l1">d'ad'ren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En 't Land alom <rhyme label="b" type="m" corresp="#s1:l2">besproeit</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ach! ach! laat ons daar <rhyme label="a" type="f" corresp="#s1:l1">vergad'ren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Daar 't grasje weelig <rhyme label="b" type="m" corresp="#s1:l2">groeit</rhyme>.</l>
      </lg>
