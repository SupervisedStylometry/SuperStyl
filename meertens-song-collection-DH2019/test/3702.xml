      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 099 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">ONSE Godt die <rhyme label="a" type="m">is</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Een Koningh <rhyme label="a" type="m" corresp="#s1:l1">ghewis</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Daerom ooc nu <rhyme label="b" type="m">raest</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Dat volck gantsch <rhyme label="b" type="m" corresp="#s1:l3">verdwaest</rhyme>.</l>
        <l id="s1:l5" met="3" type="+-">Op Cherubin <rhyme label="c" type="m">voort</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Vaert god <rhyme label="c" type="m" corresp="#s1:l5">ongestoort</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dies moet dat gantsch <rhyme label="d" type="m">ęerdryck</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Hem vreesen al <rhyme label="d" type="m" corresp="#s1:l7">gelyck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Seer groot is de <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Verheuen in <rhyme label="a" type="m" corresp="#s2:l1">eer</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">In Zion met <rhyme label="b" type="m">kracht</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Dies yder <rhyme label="b" type="m" corresp="#s2:l3">gheslacht</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Moet den naeme <rhyme label="c" type="m">sijn</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Met sangh prysen <rhyme label="c" type="m" corresp="#s2:l5">fyn</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Want hy is <rhyme label="d" type="m">wonderbaer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">End seer heyligh <rhyme label="d" type="m" corresp="#s2:l7">voorwaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Die Koningh niet <rhyme label="a" type="m">slecht</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Heeft seer lief dat <rhyme label="a" type="m" corresp="#s3:l1">recht</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">End <rhyme label="b" type="m">rechtuęerdicheit</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Met die <rhyme label="b" type="m" corresp="#s3:l3">billicheit</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Daer leydt die <rhyme label="c" type="m">Heere</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Iacob met <rhyme label="c" type="m" corresp="#s3:l5">eere</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Nae sijn Woort met <rhyme label="d" type="m">bescheydt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In der <rhyme label="d" type="m" corresp="#s3:l7">gherechticheit</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Louet nu dan <rhyme label="a" type="m">vry</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Onsen Godt seer <rhyme label="a" type="m" corresp="#s4:l1">bly</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">End valt met <rhyme label="b" type="m">ootmoet</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Voor hem nu te <rhyme label="b" type="m" corresp="#s4:l3">voet</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Sijn naem is <rhyme label="c" type="m">hilligh</rhyme>.</l>
        <l id="s4:l6" met="3" type="+-">Moses <rhyme label="c" type="m" corresp="#s4:l5">goetwilligh</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">End Aaron die <rhyme label="d" type="m">t'saem</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Godes dienaers sijn <rhyme label="d" type="m" corresp="#s4:l7">bequaem</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Die end <rhyme label="a" type="m">Samuel</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Sijn nae Godts <rhyme label="a" type="m" corresp="#s5:l1">beuel</rhyme></l>
        <l id="s5:l3" met="3" type="+-">Biddende, <rhyme label="b" type="m">verhoort</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Nae sijn Godtlick <rhyme label="b" type="m" corresp="#s5:l3">Woort</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">V' volck <rhyme label="c" type="m">beladen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Als sy v <rhyme label="c" type="m" corresp="#s5:l5">baden</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Waeren van v <rhyme label="d" type="m">ontfaen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">End verhoort van stond' <rhyme label="d" type="m" corresp="#s5:l7">aen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Wt den wolcken <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Sprack hy met hen <rhyme label="a" type="m" corresp="#s6:l1">daer</rhyme>.</l>
        <l id="s6:l3" met="3" type="+-">End toonde hen <rhyme label="b" type="m">bloot</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Die colomne <rhyme label="b" type="m" corresp="#s6:l3">root</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Sy <rhyme label="c" type="m">onbeswaeret</rhyme>,</l>
        <l id="s6:l6" met="3" type="+-">Hebben <rhyme label="c" type="m" corresp="#s6:l5">bewaeret</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Sijn Woort ende <rhyme label="d" type="m">verbondt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">T'welck quam wt sijnen <rhyme label="d" type="m" corresp="#s6:l7">mondt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Ghy die ons Godt <rhyme label="a" type="m">sijt</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Hebt verhoort <rhyme label="a" type="m" corresp="#s7:l1">altijdt</rhyme></l>
        <l id="s7:l3" met="3" type="+-">V' volck: die <rhyme label="b" type="m">misdaedt</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Hebt ghy vroegh end <rhyme label="b" type="m" corresp="#s7:l3">spaedt</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Hen gantsch <rhyme label="c" type="m">vergheuen</rhyme>:</l>
        <l id="s7:l6" met="3" type="+-">Doch ghy <rhyme label="c" type="m" corresp="#s7:l5">daerneuen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Hebt sonder <rhyme label="d" type="m">onderlaet</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Ghestraft haer sonden <rhyme label="d" type="m" corresp="#s7:l7">quaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Den Heere nu <rhyme label="a" type="m">pryst</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Hem eere <rhyme label="a" type="m" corresp="#s8:l1">bewijst</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Wilt hem bidden <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Op sijnen bergh <rhyme label="b" type="m" corresp="#s8:l3">saen</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Nae sijn <rhyme label="c" type="m">ghevallen</rhyme>.</l>
        <l id="s8:l6" met="3" type="+-">Want bouen <rhyme label="c" type="m" corresp="#s8:l5">allen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Is hy heyligh end <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Dies elck hem louen <rhyme label="d" type="m" corresp="#s8:l7">moet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
