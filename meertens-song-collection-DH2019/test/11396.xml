  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VRiendin van my <rhyme label="a" type="f">gekooren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Cieraad van onze <rhyme label="b" type="m">Jeugt</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">O Amstels - kind! <rhyme label="a" type="f" corresp="#s1:l1">gebooren</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Van Ouderen in <rhyme label="b" type="m" corresp="#s1:l2">deugt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">O edel Blom der Vrouwen,, <rhyme label="c" type="m">ach</rhyme>!</l>
        <l id="s1:l6" met="4" type="+-">Wanneer ik u aanschouwen,, <rhyme label="c" type="m" corresp="#s1:l5">mach</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Mijn hart dat is <rhyme label="b" type="m" corresp="#s1:l2">verheugd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mits u veel schoone <rhyme label="a" type="f">gaaven</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van God zijn <rhyme label="b" type="m">toegeleid</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Die gy tracht hand te <rhyme label="a" type="f" corresp="#s2:l1">haven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In alle <rhyme label="b" type="m" corresp="#s2:l2">eerbaarheid</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Datmen, in al de Werelt,, <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Een Maagd daar meê beperelt,, <rhyme label="c" type="m" corresp="#s2:l5">ziet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als u, o braave <rhyme label="b" type="m" corresp="#s2:l2">Meid</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Uw' schoonheid blijk ten <rhyme label="a" type="f">eersten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Aan 't cierlijk blonde <rhyme label="b" type="m">Haar</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wiens lof men poogt, op 't <rhyme label="a" type="f" corresp="#s3:l1">zeersten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Te maaken <rhyme label="b" type="m" corresp="#s3:l2">openbaar</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Mits yder hair een krulzel,, <rhyme label="c" type="m">heeft</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Waar door het u een hulzel,, <rhyme label="c" type="m" corresp="#s3:l5">geeft</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Gestrengelt door <rhyme label="b" type="m" corresp="#s3:l2">malkaar</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Uw' Voorhooft zonder <rhyme label="a" type="f">rimpel</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Rondom u jeugdig <rhyme label="b" type="m">Breyn</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Niet redenloos noch <rhyme label="a" type="f" corresp="#s4:l1">zimpel</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Noch in gedaanten <rhyme label="b" type="m" corresp="#s4:l2">kleyn</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Vertoont u rijpe zinnen,, <rhyme label="c" type="m">wijs</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dies ik u, mijn Vriendinne,, <rhyme label="c" type="m" corresp="#s4:l5">prijs</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Voorzichtig, kuys en <rhyme label="b" type="m" corresp="#s4:l2">reyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Uw' blauw' lazure <rhyme label="a" type="f">Oogjes</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Glinsteren als <rhyme label="b" type="m">Robijn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Recht in twee ronde <rhyme label="a" type="f" corresp="#s5:l1">boogjes</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Aan u vercierde <rhyme label="b" type="m" corresp="#s5:l2">Kruyn</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Wiens Kristalijne luyster,, <rhyme label="c" type="m">klaar</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">De Minnaars werpt in 't duister,, <rhyme label="c" type="m" corresp="#s5:l5">naar</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Bezingelt in dees <rhyme label="b" type="m" corresp="#s5:l2">thuyn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Als zy de minne - <rhyme label="a" type="f">vonkjes</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Zich vesten in 't <rhyme label="b" type="m">gewricht</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Door d'aangenaame <rhyme label="a" type="f" corresp="#s6:l1">lonkjes</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van 't lodderlijk <rhyme label="b" type="m" corresp="#s6:l2">gezicht</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Zoo dat Apollos gulde,, <rhyme label="c" type="m">glans</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Het glinst'ren nau kan dulden,, <rhyme label="c" type="m" corresp="#s6:l5">thans</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Van 't held're luchtig <rhyme label="b" type="m" corresp="#s6:l2">licht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wiens flikkerende <rhyme label="a" type="f">straalen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Ontslaan de nacht - <rhyme label="b" type="m">Gordijn</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En doen twee Fakkels <rhyme label="a" type="f" corresp="#s7:l1">daalen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die Lievers lichters <rhyme label="b" type="m" corresp="#s7:l2">zijn</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">In Bloempjes als Vrou Flora,, <rhyme label="c" type="m">sticht</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Wiens glans niet voor Aurora,, <rhyme label="c" type="m" corresp="#s7:l5">swicht</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Noch Delius in <rhyme label="b" type="m" corresp="#s7:l2">schijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Uw' neuze, als een <rhyme label="a" type="f">gevel</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Van u verheven <rhyme label="b" type="m">hoofd</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Werd, door verachtings <rhyme label="a" type="f" corresp="#s8:l1">nevel</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">In geenen deel <rhyme label="b" type="m" corresp="#s8:l2">verdooft</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Getrokken door den vinger,, <rhyme label="c" type="m">Godts</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Dat zy der Helden dwinger,, <rhyme label="c" type="m" corresp="#s8:l5">trots</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Van krachten heeft <rhyme label="b" type="m" corresp="#s8:l2">berooft</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Uw' lieve bolle <rhyme label="a" type="f">wangen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Die, met een eerbaar <rhyme label="b" type="m">schaamt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Geduurig zijn <rhyme label="a" type="f" corresp="#s9:l1">bevangen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">'t Geen wel een maagt <rhyme label="b" type="m" corresp="#s9:l2">betaamt</rhyme>;</l>
        <l id="s9:l5" met="4" type="+-">Dewijl daar op twee roosjes,, <rhyme label="c" type="m">staan</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Die steeds met roode bloosjes,, <rhyme label="c" type="m" corresp="#s9:l5">gaan</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Zijn 't schoonste Schoon <rhyme label="b" type="m" corresp="#s9:l2">genaamt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Uw' aangenaame <rhyme label="a" type="f">lipjes</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Als duynen om u <rhyme label="b" type="m">mondt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Gelijk twee harde <rhyme label="a" type="f" corresp="#s10:l1">klipjes</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Voor d'Oceaanze <rhyme label="b" type="m" corresp="#s10:l2">grond</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Weêrstaan de brakke stroomen,, <rhyme label="c" type="m">straf</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Dat zy niet varder komen,, <rhyme label="c" type="m" corresp="#s10:l5">af</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Als daar Neptunus <rhyme label="b" type="m" corresp="#s10:l2">stond</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Uw' wit yvoore <rhyme label="a" type="f">Tanden</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Staan effen, wel en <rhyme label="b" type="m">vast</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Omzingelt met twee <rhyme label="a" type="f" corresp="#s11:l1">randen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Wiens schoonheid immer <rhyme label="b" type="m" corresp="#s11:l2">wast</rhyme>;</l>
        <l id="s11:l5" met="4" type="+-">Waar door den zoeten Nectar,, <rhyme label="c" type="m">glijt</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Daar meê gy steeds een trekster,, <rhyme label="c" type="m" corresp="#s11:l5">zijt</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Die ons alzoo <rhyme label="b" type="m" corresp="#s11:l2">verrast</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Uw' Tonge vloeyt in 't <rhyme label="a" type="f">praten</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Wiens reden ik <rhyme label="b" type="m">ontfong</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Wanneer die, boven <rhyme label="a" type="f" corresp="#s12:l1">maten</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Een geestig deuntje <rhyme label="b" type="m" corresp="#s12:l2">zong</rhyme>;</l>
        <l id="s12:l5" met="4" type="+-">Als gy een Godd'lijk deuntjen,, <rhyme label="c" type="m">zingt</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Mijn hertje als een Aapjen,, <rhyme label="c" type="m" corresp="#s12:l5">springt</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Zoo jeugdig, en zoo <rhyme label="b" type="m" corresp="#s12:l2">jong</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Want ziet, uw' held're <rhyme label="a" type="f">keeltje</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Voortreft het Snaaren - <rhyme label="b" type="m">spel</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">Veel zoeter als een <rhyme label="a" type="f" corresp="#s13:l1">Veeltje</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Die wel is op zijn <rhyme label="b" type="m" corresp="#s13:l2">stel</rhyme>;</l>
        <l id="s13:l5" met="4" type="+-">Ja die gelijk een Cyter,, <rhyme label="c" type="m">klinkt</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Waar by den schraale Krijter,, <rhyme label="c" type="m" corresp="#s13:l5">stinkt</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Met zijn uitheems <rhyme label="b" type="m" corresp="#s13:l2">gerel</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">O puik van alle <rhyme label="a" type="f">Vrouwen</rhyme>!</l>
        <l id="s14:l2" met="3" type="-+">O zuiver schoone <rhyme label="b" type="m">Beelt</rhyme>!</l>
        <l id="s14:l3" met="3" type="-+">Als w' u mogen <rhyme label="a" type="f" corresp="#s14:l1">aenschouwen</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Ons hert dat is <rhyme label="b" type="m" corresp="#s14:l2">verheugt</rhyme>;</l>
        <l id="s14:l5" met="4" type="+-">Door 't kouten van het radde,, <rhyme label="c" type="m">lidt</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Beslooten tusschen 't gladde,, <rhyme label="c" type="m" corresp="#s14:l5">wit</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Gy menig wondt en <rhyme label="b" type="m" corresp="#s14:l2">heelt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Uw' kinn', doch met geen <rhyme label="a" type="f">kloofjen</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Die is niet plomp noch <rhyme label="b" type="m">grof</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Die u zeer jeugdig <rhyme label="a" type="f" corresp="#s15:l1">hoofjen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Verciert als 't eelste <rhyme label="b" type="m" corresp="#s15:l2">stof</rhyme>;</l>
        <l id="s15:l5" met="4" type="+-">Diens wezen is zoo geestig,, <rhyme label="c" type="m">wis</rhyme>,</l>
        <l id="s15:l6" met="4" type="+-">Dat mijn vernuft te beestig,, <rhyme label="c" type="m" corresp="#s15:l5">is</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Om t' uyten half uw' <rhyme label="b" type="m" corresp="#s15:l2">lof</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Uw' blank' Albaste <rhyme label="a" type="f">halsje</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Voert gy, gelijk een <rhyme label="b" type="m">Zwaan</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Zoo rustig op, en <rhyme label="a" type="f" corresp="#s16:l1">alsje</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Uw' Rabat hebt <rhyme label="b" type="m" corresp="#s16:l2">afgedaan</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Zoo ziet men, door het witte,, <rhyme label="c" type="m">vel</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">De blaauwe Ad'ren zitten,, <rhyme label="c" type="m" corresp="#s16:l5">wel</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Als Juno by <rhyme label="b" type="m" corresp="#s16:l2">Diaan</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Uw' armen en uw' <rhyme label="a" type="f">handen</rhyme>,</l>
        <l id="s17:l2" met="3" type="-+">In alles wel <rhyme label="b" type="m">gemaakt</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Doen my inwendig <rhyme label="a" type="f" corresp="#s17:l1">branden</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Als die 't gezicht <rhyme label="b" type="m" corresp="#s17:l2">genaakt</rhyme>,</l>
        <l id="s17:l5" met="4" type="+-">Gevoegt in zulke schaakels,, <rhyme label="c" type="m">net</rhyme>,</l>
        <l id="s17:l6" met="4" type="+-">Wie zich aan deze taakels,, <rhyme label="c" type="m" corresp="#s17:l5">zet</rhyme>,</l>
        <l id="s17:l7" met="3" type="-+">Zijn vryheid is <rhyme label="b" type="m" corresp="#s17:l2">geraakt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Wat Lof zal ik u <rhyme label="a" type="f">geven</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Dat u gelijken <rhyme label="b" type="m">zal</rhyme>?</l>
        <l id="s18:l3" met="3" type="-+">Geen Schilder geeft het <rhyme label="a" type="f" corresp="#s18:l1">leven</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">d' Algever geeft het <rhyme label="b" type="m" corresp="#s18:l2">al</rhyme>,</l>
        <l id="s18:l5" met="4" type="+-">Dat nimmer Brein besluiten,, <rhyme label="c" type="m">kan</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">Om 't Lof daar recht te uiten,, <rhyme label="c" type="m" corresp="#s18:l5">van</rhyme>.</l>
        <l id="s18:l7" met="3" type="-+">In wijsheid is men <rhyme label="b" type="m" corresp="#s18:l2">mal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
