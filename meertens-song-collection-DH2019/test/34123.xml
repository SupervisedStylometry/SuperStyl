    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Fortune que tu m'importunes</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+"><rhyme label="a" type="f">Clarinde</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Myn lief, myn <rhyme label="a" type="f" corresp="#s1:l1">beminde</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Laet u wreetheyt <rhyme label="b" type="m">af</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En blyft my niet soo stuer en <rhyme label="b" type="m" corresp="#s1:l3">straf</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Maer wilt <rhyme label="c" type="f">ghenesen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Hem die voor <rhyme label="c" type="f" corresp="#s1:l5">desen</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Voor u soet <rhyme label="c" type="f" corresp="#s1:l5">wesen</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Hem ten offer <rhyme label="b" type="m" corresp="#s1:l3">gaf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="1" type="-+">'kMoet <rhyme label="a" type="f">leyden</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Als ick u eerst <rhyme label="a" type="f" corresp="#s2:l1">vreyden</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Scheen ick uwen <rhyme label="b" type="m">vrint</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Maer nu scheynt ghy anders <rhyme label="b" type="m" corresp="#s2:l3">ghesint</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Die u te <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Had' <rhyme label="c" type="f" corresp="#s2:l5">uyt-verkoren</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Heb gunst <rhyme label="c" type="f" corresp="#s2:l5">verloren</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">En sulckx noyt <rhyme label="b" type="m" corresp="#s2:l3">verdint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="1" type="-+">Maer <rhyme label="a" type="f">heden</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Noch u <rhyme label="a" type="f" corresp="#s3:l1">schoonigheden</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">My houden <rhyme label="b" type="m">verblint</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Waer-om ghy blyft van my <rhyme label="b" type="m" corresp="#s3:l3">bemint</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Thoont dan <rhyme label="c" type="f">ghenaeden</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Wilt niet <rhyme label="c" type="f" corresp="#s3:l5">versmaeden</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Daer ghy gheen <rhyme label="c" type="f" corresp="#s3:l5">daeden</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Van on-trouw in <rhyme label="b" type="m" corresp="#s3:l3">vint</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="1" type="-+">Wilt <rhyme label="a" type="f">thoonen</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Dat ghy kont <rhyme label="a" type="f" corresp="#s4:l1">beloonen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Myn <rhyme label="b" type="m">ghetrouwigheyt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Dat ick u gunst niet en blyf <rhyme label="b" type="m" corresp="#s4:l3">quyt</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Maer laet <rhyme label="c" type="f">verwinnen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">U stuere <rhyme label="c" type="f" corresp="#s4:l5">sinnen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">'kSal my <rhyme label="c" type="f" corresp="#s4:l5">bekinnen</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">U slaef <rhyme label="b" type="m" corresp="#s4:l3">t'alder-tyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
