<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178363</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Hoe zoet is het daar de vriendschap woont</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Vreugd! de Vyfde Willem <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Uit het Oranje <rhyme label="b" type="m">Bloed</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Wien Nassauws trouw door d'adren <rhyme label="a" type="m" corresp="#s1:l1">zweevt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Die Land en Kerk <rhyme label="b" type="m" corresp="#s1:l2">behoed</rhyme>;</l>
        <l id="s1:l5" met="4" type="-+">Hy leevt voor 't vrye <rhyme label="c" type="m">Vaderland</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Hoe zeer de boosheid <rhyme label="c" type="m" corresp="#s1:l5">zaamenspant</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Hy leevt, Hy leevt, Hy <rhyme label="a" type="m" corresp="#s1:l1">leevt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">God schenkt ons zyn Verjaardag <rhyme label="a" type="m">weêr</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Op deezen Achtsten <rhyme label="b" type="m">Maart</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die ons, tot 's Hoogstens lov en <rhyme label="a" type="m" corresp="#s2:l1">eer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Weer stof tot juichen <rhyme label="b" type="m" corresp="#s2:l2">baart</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Hoe zeer het Loevesteinsch <rhyme label="c" type="m">gebroed</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Hem Haat, en Smaad, en valsch <rhyme label="c" type="m" corresp="#s2:l5">begroet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Hy leevt, Hy leevt, Hy <rhyme label="a" type="m" corresp="#s2:l1">leevt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hoe zeer dat woedend <rhyme label="a" type="m">Moorders-rot</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Te Waapen, Waapen <rhyme label="b" type="m">snelt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En zwaarden scherpt voor 's Vorsten <rhyme label="a" type="m" corresp="#s3:l1">strot</rhyme></l>
        <l id="s3:l4" met="3" type="-+">'t Word van God perk <rhyme label="b" type="m" corresp="#s3:l2">gesteld</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dies geeft hem eer en zingt hem <rhyme label="c" type="m">lof</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Uw toon klinkt boven 't Starren <rhyme label="c" type="m" corresp="#s3:l5">Hof</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">De dierbre Willem <rhyme label="a" type="m" corresp="#s3:l1">leeft</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gedenkt de daaden, die zyn <rhyme label="a" type="m">Huis</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Aan Neêrland heeft <rhyme label="b" type="m">gedaan</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hoe 't U heeft van het Spaansch <rhyme label="a" type="m" corresp="#s4:l1">gespuis</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En van Duc d'Alb' <rhyme label="b" type="m" corresp="#s4:l2">ontslaên</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En hoe een wreede Moorders <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Den eersten Willem aan 't <rhyme label="c" type="m" corresp="#s4:l5">Vaderland</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ontroof, ontroof, on <rhyme label="a" type="m" corresp="#s4:l1">roof</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">'t Is nu twee Eeuwen juist <rhyme label="a" type="m">geleên</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat Vader Willem <rhyme label="b" type="m">viel</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Wie denkt daar aan niet met <rhyme label="a" type="m" corresp="#s5:l1">geween</rhyme>?</l>
        <l id="s5:l4" met="3" type="-+">Wien gruwelt niet de <rhyme label="b" type="m" corresp="#s5:l2">Ziel</rhyme>?</l>
        <l id="s5:l5" met="4" type="-+">Als hy de snoode boosheid <rhyme label="c" type="m">ziet</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Die zoortgelyken schendaad <rhyme label="c" type="m" corresp="#s5:l5">riet</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Door laster <rhyme label="a" type="m" corresp="#s5:l1">aangehitst</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Gedenkt wat Maurits heeft <rhyme label="a" type="m">gewrocht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En hoe hy, tot Gods <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Te Dordrecht heeft by een <rhyme label="a" type="m" corresp="#s6:l1">gebrocht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Handhaavers van de <rhyme label="b" type="m" corresp="#s6:l2">Leer</rhyme>;</l>
        <l id="s6:l5" met="4" type="-+">Die Leer, die Gods genade <rhyme label="c" type="m">roemt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">En vryen wil en krachten <rhyme label="c" type="m" corresp="#s6:l5">doemt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Hersteld, hersteld, <rhyme label="a" type="m" corresp="#s6:l1">hersteld</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Gedenkt hoe Fredrik Hendrik <rhyme label="a" type="m">streed</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ee hoe hy 't <rhyme label="b" type="m">Staatsgebouw</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Met een vast dak heeft <rhyme label="a" type="m" corresp="#s7:l1">overkleed</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ten einde 't nimmer <rhyme label="b" type="m" corresp="#s7:l2">zou</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Zyn blood gesteld aan ramp of <rhyme label="c" type="m">druk</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Maar, onder God, voor <rhyme label="c" type="m" corresp="#s7:l5">ongeluk</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Beveiligd, blyven <rhyme label="a" type="m" corresp="#s7:l1">staan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Gedenkt hoe tweede Willem, 't <rhyme label="a" type="m">Land</rhyme></l>
        <l id="s8:l2" met="3" type="-+">De Vree met Spanjen <rhyme label="b" type="m">schonk</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">En 't vryde van den Oorlogs <rhyme label="a" type="m" corresp="#s8:l1">brand</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Die tachtig jaaren <rhyme label="b" type="m" corresp="#s8:l2">blonk</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">En maakte dat 't <rhyme label="c" type="m">Staatendom</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Tot allerhoogsten luister <rhyme label="c" type="m" corresp="#s8:l5">klom</rhyme></l>
        <l id="s8:l7" met="3" type="-+">In aanzien, macht en <rhyme label="a" type="m" corresp="#s8:l1">eer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Gedenkt wat derde Willem <rhyme label="a" type="m">deed</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En hoe hy voor u <rhyme label="b" type="m">vogt</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Toen 't heevig Vyfspan u <rhyme label="a" type="m" corresp="#s9:l1">bestreed</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En haast ten onder <rhyme label="b" type="m" corresp="#s9:l2">brogt</rhyme>:</l>
        <l id="s9:l5" met="4" type="-+">Hoe hy uit ramp druk en <rhyme label="c" type="m">geweld</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">U wederom in Vryheid <rhyme label="c" type="m" corresp="#s9:l5">steld'</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Met eer met eer, met <rhyme label="a" type="m" corresp="#s9:l1">eer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ja Willem Friso schaft ook <rhyme label="a" type="m">stof</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Tot zyn <rhyme label="b" type="m">gedachtenis</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Die, daar hy streedt met eer en <rhyme label="a" type="m" corresp="#s10:l1">lef</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Helaas! verdronken <rhyme label="b" type="m" corresp="#s10:l2">is</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">En liet een zwanger' Ega <rhyme label="c" type="m">na</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Die ons nochtans, door Gods <rhyme label="c" type="m" corresp="#s10:l5">genaê</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Weêr baard' een jongen <rhyme label="a" type="m" corresp="#s10:l1">Prins</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Gedenkt aan Vierde Willems <rhyme label="a" type="m">daên</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Die u wan Vrankryks <rhyme label="b" type="m">Macht</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Welk u gewislyk kon <rhyme label="a" type="m" corresp="#s11:l1">verslaan</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Tot ruimt' en Vreede <rhyme label="b" type="m" corresp="#s11:l2">bracht</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Waarom de Staat den <rhyme label="c" type="m">Luistertrap</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Van 't erffelyk <rhyme label="c" type="m" corresp="#s11:l5">Stadhouderschap</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Voor eeuwig aan hem <rhyme label="a" type="m" corresp="#s11:l1">schonk</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Hoe had het Engeland <rhyme label="a" type="m">vergaan</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Was Vyfde Willems <rhyme label="b" type="m">Macht</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Door kiels gebrek en 't <rhyme label="a" type="m" corresp="#s12:l1">wederstaan</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Zyns raads, niet als <rhyme label="b" type="m" corresp="#s12:l2">verkracht</rhyme>?</l>
        <l id="s12:l5" met="4" type="-+">Tuig, Doggersbank! tuig vijfde <rhyme label="c" type="m">dag</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Van Oogstmaand: van dien dapp'ren <rhyme label="c" type="m" corresp="#s12:l5">slag</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Die toen geleeverd <rhyme label="a" type="m" corresp="#s12:l1">is</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Nochtans heeft deeze Vorst ons <rhyme label="a" type="m">Land</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Door zijn zeer wijs <rhyme label="b" type="m">beleid</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Voor inval, en voor Scheepen <rhyme label="a" type="m" corresp="#s13:l1">brand</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Des Engelsman <rhyme label="b" type="m" corresp="#s13:l2">bevreid</rhyme>:</l>
        <l id="s13:l5" met="4" type="-+">Geen ramp heeft onze Kust <rhyme label="c" type="m">gedeert</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Neen: Willem heeft die <rhyme label="c" type="m" corresp="#s13:l5">afgeweert</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Door Gods <rhyme label="a" type="m" corresp="#s13:l1">Goedgunstigheid</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Dus ziet g'ô Volk! hoe op den <rhyme label="a" type="m">duur</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Oranje voor U <rhyme label="b" type="m">streed</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">En hoe het Vorstelijk <rhyme label="a" type="m" corresp="#s14:l1">Heldenvuur</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Zijn gloed, voor u <rhyme label="b" type="m" corresp="#s14:l2">besteed</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Heeft, en zijn Bloed voor U <rhyme label="c" type="m">gestort</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">'t Welk thans helaas vergeeten <rhyme label="c" type="m" corresp="#s14:l5">wordt</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Van een ondankbren <rhyme label="a" type="m" corresp="#s14:l1">hoop</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Keert ô misleiden! keert tog <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">En toont een dankbaar <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Eer uw gedrag van God den <rhyme label="a" type="m" corresp="#s15:l1">Heer</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Door wraak beteugelt <rhyme label="b" type="m" corresp="#s15:l2">wert</rhyme>:</l>
        <l id="s15:l5" met="4" type="-+">Geeft aan Oranjen hert en <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">En stijft alzo den <rhyme label="c" type="m" corresp="#s15:l5">Eendrachtsband</rhyme>,</l>
        <l id="s15:l7" met="3" type="-+">Die U heeft groot <rhyme label="a" type="m" corresp="#s15:l1">gemaakt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Laat U tot voorbeeld zijn den <rhyme label="a" type="m">Briel</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Die 's Prinsen deugd <rhyme label="b" type="m">erkent</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Die Stad, die d'Alba 't eerst <rhyme label="a" type="m" corresp="#s16:l1">ontviel</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">En nu ook 't steven <rhyme label="b" type="m" corresp="#s16:l2">wendt</rhyme>,</l>
        <l id="s16:l5" met="4" type="-+">Om recht te Zeilen zo als 't <rhyme label="c" type="m">hoort</rhyme>,</l>
        <l id="s16:l6" met="4" type="-+">Ai maakt met haar een zoet <rhyme label="c" type="m" corresp="#s16:l5">accoord</rhyme></l>
        <l id="s16:l7" met="3" type="-+">Tot heil van 't <rhyme label="a" type="m" corresp="#s16:l1">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Zoo moet die Stad, die d'Eerkroon <rhyme label="a" type="m">draagt</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Des heils van 't <rhyme label="b" type="m">Vaderland</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Geroemd zyn, dat zy d'eendracht <rhyme label="a" type="m" corresp="#s17:l1">schraagt</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Door deugd en door <rhyme label="b" type="m" corresp="#s17:l2">verstand</rhyme>!</l>
        <l id="s17:l5" met="4" type="-+">God schenk' haar Zegen zonder <rhyme label="c" type="m">maat</rhyme>,</l>
        <l id="s17:l6" met="4" type="-+">Als de verlosser van den <rhyme label="c" type="m" corresp="#s17:l5">Staat</rhyme>,</l>
        <l id="s17:l7" met="3" type="-+">Van twist en van <rhyme label="a" type="m" corresp="#s17:l1">geweld</rhyme>!</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Zoo praal' Oranj' in glans en <rhyme label="a" type="m">gloor</rhyme></l>
        <l id="s18:l2" met="3" type="-+">En moet vol luister <rhyme label="b" type="m">staan</rhyme></l>
        <l id="s18:l3" met="4" type="-+">Aan Neerlands Hoofd, all' eeuwen <rhyme label="a" type="m" corresp="#s18:l1">door</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Ja zo lang Zon en <rhyme label="b" type="m" corresp="#s18:l2">Maan</rhyme>,</l>
        <l id="s18:l5" met="4" type="-+">Verspreiden Dag en Nacht heur <rhyme label="c" type="m">licht</rhyme>,</l>
        <l id="s18:l6" met="4" type="-+">Tot dat de tyd voor 't eeuwig <rhyme label="c" type="m" corresp="#s18:l5">zwicht</rhyme>,</l>
        <l id="s18:l7" met="3" type="-+">En laster wordt <rhyme label="a" type="m" corresp="#s18:l1">gedoemt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Wie heeft een Ziel, die 't recht <rhyme label="a" type="m">bemint</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">Die deugd na waarde <rhyme label="b" type="m">schat</rhyme>,</l>
        <l id="s19:l3" met="4" type="-+">Die hier in geen genoegen <rhyme label="a" type="m" corresp="#s19:l1">vindt</rhyme>?</l>
        <l id="s19:l4" met="3" type="-+">Die niet betreedt dit <rhyme label="b" type="m" corresp="#s19:l2">pad</rhyme>?</l>
        <l id="s19:l5" met="4" type="-+">Wel aan dan, waarde <rhyme label="c" type="m">Burgerschaer</rhyme>!</l>
        <l id="s19:l6" met="4" type="-+">Voor welk' ik tokkelde de <rhyme label="c" type="m" corresp="#s19:l5">snaar</rhyme>,</l>
        <l id="s19:l7" met="3" type="-+">Volgt gy met Amen <rhyme label="a" type="m" corresp="#s19:l1">na</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
