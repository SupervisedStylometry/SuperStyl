<song>
  <meta>
    <id>180476</id>
    <date>
      <year-start>1681</year-start>
      <year-end>1681</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Tien geboden</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Seer Heylich Heerlijck wijs <rhyme label="a" type="f">allvaerdigh</rhyme></l>
        <l id="s1:l2" met="4" type="-+">En Algenoechsaem Groot en <rhyme label="b" type="m">Goet</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Almachtich, Trouw, en oock <rhyme label="a" type="f" corresp="#s1:l1">Rechtvaerdigh</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Waerachtigh leven minlijck <rhyme label="b" type="m" corresp="#s1:l2">soet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Van ende voor u selfs en <rhyme label="a" type="f">allen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die u recht kennen zijt gy <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Ia al wat in het hert kan <rhyme label="a" type="f" corresp="#s2:l1">vallen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Zijt Ghy en noch oneyndigh <rhyme label="b" type="m" corresp="#s2:l2">meer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Soo sijn oock alle uw <rhyme label="a" type="f">besluyten</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Uw daeden en uw gansch <rhyme label="b" type="m">beleyt</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">De Aerd’ en Hemel hoort men <rhyme label="a" type="f" corresp="#s3:l1">uyten</rhyme>:</l>
        <l id="s3:l4" met="4" type="-+">Als s’Heeren doen is <rhyme label="b" type="m" corresp="#s3:l2">Heerlijckheit</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Al was ‘t dan dat gy mijn wout <rhyme label="a" type="f">dooden</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Voor Eeuwigh, Gy blijft even <rhyme label="b" type="m">goet</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">De wil omhelst maer ’t vleesch seer <rhyme label="a" type="f" corresp="#s4:l1">noode</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Al wat U wijsheit met mijn <rhyme label="b" type="m" corresp="#s4:l2">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Komt dan o Heer Ghy zijt de <rhyme label="a" type="f">Hooge</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Doet alles met my dat u <rhyme label="b" type="m">lust</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">’t Sy met of tegen mijn <rhyme label="a" type="f" corresp="#s5:l1">vermogen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">U vry beleyt dat is mijn <rhyme label="b" type="m" corresp="#s5:l2">rust</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hebt Ghy meer eer in anders <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Siet Heer hier ben ick, doot mijn <rhyme label="b" type="m">vry</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Laet Uwe Eer maer sijn <rhyme label="a" type="f" corresp="#s6:l1">verheven</rhyme>;</l>
        <l id="s6:l4" met="4" type="-+">Want dat is goet of ick schoon <rhyme label="b" type="m" corresp="#s6:l2">ly</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Doch dit weet ick o Heer <rhyme label="a" type="f">verkoren</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Dat eens van wil met u te <rhyme label="b" type="m">sijn</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Geen teecken is van een <rhyme label="a" type="f" corresp="#s7:l1">verloren</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Maer van een die sal salich <rhyme label="b" type="m" corresp="#s7:l2">sijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Al wat op Aerd mijn dan kan <rhyme label="a" type="f">beuren</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Van Uwe handt ’t is mijn al <rhyme label="b" type="m">goet</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">‘K wil over uw beleyt niet <rhyme label="a" type="f" corresp="#s8:l1">treuren</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Heer al uw bitter is mijn <rhyme label="b" type="m" corresp="#s8:l2">soet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wilt Ghy mijn laaten onder <rhyme label="a" type="f">sonden</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">U wil bemin ick ’t is mijn <rhyme label="b" type="m">wel</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">Wilt Ghy mijn slaen jae diepe <rhyme label="a" type="f" corresp="#s9:l1">wonden</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Of leyden steedes door de <rhyme label="b" type="m" corresp="#s9:l2">Hel</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Wilt ghy door duyster mijn doen <rhyme label="a" type="f">treden</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Of brengen in een dorr’ <rhyme label="b" type="m">woestijn</rhyme>;</l>
        <l id="s10:l3" met="4" type="-+">Ick ben met Uwe wil te <rhyme label="a" type="f" corresp="#s10:l1">vreden</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">U doen sal mijn genoegen <rhyme label="b" type="m" corresp="#s10:l2">zijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Hoewel ick sond’ en boose <rhyme label="a" type="f">daden</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Als vleesches werck geduyrich <rhyme label="b" type="m">haet</rhyme>;</l>
        <l id="s11:l3" met="4" type="-+">Oock lijden, dorheyt en veel <rhyme label="a" type="f" corresp="#s11:l1">quaden</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Mijn om haer selfs niet aen en <rhyme label="b" type="m" corresp="#s11:l2">staet</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Maer wensch van al te sijn <rhyme label="a" type="f">ontslaegen</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Soo verr’ het quaet en sondigh <rhyme label="b" type="m">is</rhyme>;</l>
        <l id="s12:l3" met="4" type="-+">Soo ben ick oock willigh om te <rhyme label="a" type="f" corresp="#s12:l1">dragen</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Als het mijn ’s Heeren wil soo <rhyme label="b" type="m" corresp="#s12:l2">is</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">De tijt, de maet, en oock de <rhyme label="a" type="f">wijse</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Laet ick vrywillich aen uw <rhyme label="b" type="m">Heer</rhyme>;</l>
        <l id="s13:l3" met="4" type="-+">Gy blijft, Gy komt, ick wil uw <rhyme label="a" type="f" corresp="#s13:l1">prijsen</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">In al uw wegen even <rhyme label="b" type="m" corresp="#s13:l2">seer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Al wat uw handt mijn toe komt <rhyme label="a" type="f">senden</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Dat, en niet anders ik en <rhyme label="b" type="m">wil</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Ghy zijt ’t begin en oock mijn <rhyme label="a" type="f" corresp="#s14:l1">Ende</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Ick wacht op u en sitte <rhyme label="b" type="m" corresp="#s14:l2">stil</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">‘K wil u in alles Heyligh <rhyme label="a" type="f">roemen</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">U lieven, loven, prijsen <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">U Goet in all’ uw daeden <rhyme label="a" type="f" corresp="#s15:l1">noemen</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">In alle staeten t’aller <rhyme label="b" type="m" corresp="#s15:l2">ty</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Dit is mijn wensch door Gods <rhyme label="a" type="f">genaede</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">In mijn gewrocht door s’Geestes <rhyme label="b" type="m">kracht</rhyme>;</l>
        <l id="s16:l3" met="4" type="-+">Ick hangh van Iesus af <rhyme label="a" type="f" corresp="#s16:l1">gestaede</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Van en door wien, ick alles <rhyme label="b" type="m" corresp="#s16:l2">waght</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Oock dat, om dit te blijven <rhyme label="a" type="f">wenschen</rhyme>;</l>
        <l id="s17:l2" met="4" type="-+">In wensch en wil steets voort te <rhyme label="b" type="m">spoen</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Tot dat ick kom daer sael’ge <rhyme label="a" type="f" corresp="#s17:l1">menschen</rhyme></l>
        <l id="s17:l4" met="4" type="-+">Haer wensch verwisselen met <rhyme label="b" type="m" corresp="#s17:l2">doen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
