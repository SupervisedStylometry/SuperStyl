<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9014</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="morgenlied" translation="morning song">morgenlied</tag>
    </tags>
    <melody>Ich dank dir lieber Herr</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk Danck dy lieve <rhyme label="a" type="f">HEERE</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Dat ghy my hebt <rhyme label="b" type="m">bewaert</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In desen Nacht so <rhyme label="a" type="f" corresp="#s1:l1">seere</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Daer in ick lach <rhyme label="b" type="m" corresp="#s1:l2">beswaert</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Met Duysternis <rhyme label="c" type="f">Omvanghen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Daer toe in groote <rhyme label="d" type="m">Noot</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Daer wt ben ick <rhyme label="c" type="f" corresp="#s1:l5">Ontgangen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Holpt ghy my HEEr mijn <rhyme label="d" type="m" corresp="#s1:l6">Godt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Met danck wil ick dy <rhyme label="a" type="f">loven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ghy mijn Godt ende <rhyme label="b" type="m">HEER</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">In Hemel hooch daer <rhyme label="a" type="f" corresp="#s2:l1">boven</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Den dach my oock toe <rhyme label="b" type="m" corresp="#s2:l2">keer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Waerom ick gae <rhyme label="c" type="f">Aenbeden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">End dijnen Will' mach <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Leyd' my in dijnen <rhyme label="c" type="f" corresp="#s2:l5">Zeden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">End breeck den Wille <rhyme label="d" type="m" corresp="#s2:l6">mijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dat ick (HEER) niet <rhyme label="a" type="f">Affwijcke</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Van dijnen rechten <rhyme label="b" type="m">Baen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die Vyandt niet <rhyme label="a" type="f" corresp="#s3:l1">beslijcke</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">End my doe Dwalen <rhyme label="b" type="m" corresp="#s3:l2">gaen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Behoudt my door dijn <rhyme label="c" type="f">Goede</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat bidd' ick vlijtich <rhyme label="d" type="m">dy</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Voor s'Duyvels List end <rhyme label="c" type="f" corresp="#s3:l5">Woede</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Als hy Aenvechtet <rhyme label="d" type="m" corresp="#s3:l6">my</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Help my t'Gelooff wel <rhyme label="a" type="f">vaeten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Aen dijn Soon JEsum <rhyme label="b" type="m">Christ</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wilt all' mijn Sond' <rhyme label="a" type="f" corresp="#s4:l1">verlaten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Want ghy Ghenadich <rhyme label="b" type="m" corresp="#s4:l2">bist</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy sult my niet <rhyme label="c" type="f">Affslaghen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Als ghy Belovet <rhyme label="d" type="m">bly</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat hy mijn Sond' heeft <rhyme label="c" type="f" corresp="#s4:l5">draghen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">End my ghemaecket <rhyme label="d" type="m" corresp="#s4:l6">vry</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die Hoping' my oock <rhyme label="a" type="f">geve</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die niet verderven <rhyme label="b" type="m">laet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Daer toe een Christlijck <rhyme label="a" type="f" corresp="#s5:l1">Lieve</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Tot dien, die my doet <rhyme label="b" type="m" corresp="#s5:l2">quaet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Dat ick hem noch <rhyme label="c" type="f">beminne</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Niet soeck' daer in het <rhyme label="d" type="m">mijn</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Met Lieffde goet van <rhyme label="c" type="f" corresp="#s5:l5">sinne</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Al nae den Wille <rhyme label="d" type="m" corresp="#s5:l6">dijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dijn Woort laet my <rhyme label="a" type="f">belijden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Voor s'Werelts <rhyme label="b" type="m">Overmoet</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dy dienen t'allen <rhyme label="a" type="f" corresp="#s6:l1">tijden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Niet achten Gelt noch <rhyme label="b" type="m" corresp="#s6:l2">Goet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Dat my haest mocht Aff <rhyme label="c" type="f">leyden</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van dijner Waerheyt <rhyme label="d" type="m">Claer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Wilt my (HEER) niet <rhyme label="c" type="f" corresp="#s6:l5">Affscheyden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Van dijn Christlijcke <rhyme label="d" type="m" corresp="#s6:l6">Schaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Laet my den Dach <rhyme label="a" type="f">voleynden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Tot Loff des Naemes <rhyme label="b" type="m">dijn</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dat ick van dy niet <rhyme label="a" type="f" corresp="#s7:l1">weynde</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Aent Eynd' bestandich <rhyme label="b" type="m" corresp="#s7:l2">zijn</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Behoed' my Lijff end <rhyme label="c" type="f">Leven</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daer toe die Vrucht int <rhyme label="d" type="m">Landt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Wat ghy my hebt <rhyme label="c" type="f" corresp="#s7:l5">ghegeven</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Staet all' in dijner <rhyme label="d" type="m" corresp="#s7:l6">Handt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
