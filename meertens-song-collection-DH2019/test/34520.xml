    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Lelia, zo blank als <rhyme label="a" type="m">sneew</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Hoorde Roemerts montje <rhyme label="b" type="f">spreeken</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Dit 's de blankste van onz' <rhyme label="a" type="m" corresp="#s1:l1">eew</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Wie zou niet om kusjens <rhyme label="b" type="f" corresp="#s1:l2">smeeken</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Haar sijn gantsche leven <rhyme label="c" type="m">lang</rhyme>?</l>
        <l id="s1:l6" met="4" type="+-">Hy, vrypostig, kuste haar <rhyme label="d" type="f">kaaken</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Lelia, tot haar <rhyme label="d" type="f" corresp="#s1:l6">volmaken</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Kreeg een Roos op elke <rhyme label="c" type="m" corresp="#s1:l5">wang</rhyme>.</l>
      </lg>
    </lg>
