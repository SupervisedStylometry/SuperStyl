    <melody>Psalm 106 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DANCKT Godt want hy is <rhyme label="a" type="m">vriendelick</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">End sijn goetheit duert <rhyme label="a" type="m" corresp="#s1:l1">eewighlick</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Waer is hy die doch kan <rhyme label="b" type="f">wtspreken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Des Heeren wonderwercken <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">End sijn schoon daeden klaer <rhyme label="b" type="f" corresp="#s1:l3">ghebleken</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ghenoeghsaem konnen prysen <rhyme label="c" type="m" corresp="#s1:l4">sal</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wel dien die houdt v' ghebodt <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">End in allen dinghen recht <rhyme label="a" type="m" corresp="#s2:l1">doet</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Ghedenckt myns Heer' nae v' <rhyme label="b" type="f">ghenaede</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">End nae vwe <rhyme label="c" type="m">vriendelickheit</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Laet my toekommen vroegh end <rhyme label="b" type="f" corresp="#s2:l3">spaede</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Vwen bystandt end <rhyme label="c" type="m" corresp="#s2:l4">goedicheit</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dat wy sien end hooren <rhyme label="a" type="m">ghemeyn</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">T'heyl uwer wtverkoren <rhyme label="a" type="m" corresp="#s3:l1">reyn</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">End ons daer in moghen <rhyme label="b" type="f">verblyden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Dat het uwen volcke wel <rhyme label="c" type="m">gaet</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Dat ick magh roemen t'allen <rhyme label="b" type="f" corresp="#s3:l3">tyden</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Met v' erfdeel in desen <rhyme label="c" type="m" corresp="#s3:l4">staet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wy endt onse vaders met <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hebben v vergramt groot end <rhyme label="a" type="m" corresp="#s4:l1">kleen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Gantsch verkeert sijn Heer onse <rhyme label="b" type="f">weghen</rhyme>:</l>
        <l id="s4:l4" met="4" type="-+">Wy sijn trauwloos aen v gaer <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ons vaders in AEgypten <rhyme label="b" type="f" corresp="#s4:l3">pleghen</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">V' wercken te vergheten <rhyme label="c" type="m" corresp="#s4:l4">Heer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sy en hebben niet recht <rhyme label="a" type="m">bedacht</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Vwer goetheit seer groote <rhyme label="a" type="m" corresp="#s5:l1">kracht</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Maer and die Zee sijn sy al <rhyme label="b" type="f">t'saeme</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Den Heere gheweest seer <rhyme label="c" type="m">rebel</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Doch hy hielp se (doer sijnen <rhyme label="b" type="f" corresp="#s5:l3">naeme</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">End doer sijn macht) end niemant <rhyme label="c" type="m" corresp="#s5:l4">el</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy heeft ghestraft die Zee seer <rhyme label="a" type="m">wydt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Sy wierdt droogh' end des waters <rhyme label="a" type="m" corresp="#s6:l1">quyt</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Hy voerde se doer dat Męer <rhyme label="b" type="f">krachtigh</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Als doer een woestijne seer <rhyme label="c" type="m">breedt</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">End hielp se doer sijn handt <rhyme label="b" type="f" corresp="#s6:l3">almachtigh</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Van all' haer vianden seer <rhyme label="c" type="m" corresp="#s6:l4">wreedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy heeft se los end vry <rhyme label="a" type="m">ghestelt</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Van haerer vianden <rhyme label="a" type="m" corresp="#s7:l1">ghewelt</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">In't Męer sijn die alle <rhyme label="b" type="f">versopen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Die sijn volck haeten, t'welck heeft <rhyme label="c" type="m">fyn</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Godt vertrauwt, end met grooten <rhyme label="b" type="f" corresp="#s7:l3">hoopen</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Ghelooft end gheroemt den Godt <rhyme label="c" type="m" corresp="#s7:l4">sijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maer se hebben vergheten <rhyme label="a" type="m">saen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Die wercken die Godt hadd' <rhyme label="a" type="m" corresp="#s8:l1">ghedaen</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Sijnen raet sy niet en <rhyme label="b" type="f">verwachten</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Maer wierden in de woestyn' <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Belust, end hebben met <rhyme label="b" type="f" corresp="#s8:l3">verachten</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Godt gheterght end ghelastert <rhyme label="c" type="m" corresp="#s8:l4">swaer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Hy ghaf hen haerer herten <rhyme label="a" type="m">lust</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Dat haer beghęerte wierdt <rhyme label="a" type="m" corresp="#s9:l1">gheblust</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">Maer hebben haest die walghde <rhyme label="b" type="f">kreghen</rhyme>.</l>
        <l id="s9:l4" met="4" type="-+">Se hebben Mosi <rhyme label="c" type="m">wederstaen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">End oock Aaron aller <rhyme label="b" type="f" corresp="#s9:l3">weghen</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Die de heylghe kleyders hadd' <rhyme label="c" type="m" corresp="#s9:l4">aen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Onder Dathan ghingh t'ęerdryck <rhyme label="a" type="m">op</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">End viel Abiram op den <rhyme label="a" type="m" corresp="#s10:l1">kop</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">T'vyer wierdt haest' onder hen <rhyme label="b" type="f">ontsteken</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">T'welck die boose heeft verbrandt <rhyme label="c" type="m">gaer</rhyme>.</l>
        <l id="s10:l5" met="4" type="-+">End tot Oreb gantsch <rhyme label="b" type="f" corresp="#s10:l3">afgheweken</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Maeckten end dienden t kalf daer <rhyme label="c" type="m" corresp="#s10:l4">naer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">So waeren se verdwaelt gantsch <rhyme label="a" type="m">seer</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">End hebben Godt, welck was haer <rhyme label="a" type="m" corresp="#s11:l1">eer'</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">By een weydende kalf <rhyme label="b" type="f">gheleken</rhyme>.</l>
        <l id="s11:l4" met="4" type="-+">End die wercken vergheten <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Die Godt hadd' met kracht <rhyme label="b" type="f" corresp="#s11:l3">onbesweken</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">In Aegypten ghedaen seer <rhyme label="c" type="m" corresp="#s11:l4">wel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Sijn daeden verghaeten sy <rhyme label="a" type="m">haest</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Die doch t'landt Cham maeckten <rhyme label="a" type="m" corresp="#s12:l1">verbaest</rhyme>:</l>
        <l id="s12:l3" met="4" type="-+">End oock alle sijn <rhyme label="b" type="f">wonderwercken</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Die by t'roode Męer sijn <rhyme label="c" type="m">gheschiedt</rhyme>:</l>
        <l id="s12:l5" met="4" type="-+">Daerom liet Godt sijn gramschap <rhyme label="b" type="f" corresp="#s12:l3">mercken</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">End wilde t'volck bringhen tot <rhyme label="c" type="m" corresp="#s12:l4">niet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Maer Moses Godts verkoren <rhyme label="a" type="m">knecht</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Heeft hem tusschen beyden <rhyme label="a" type="m" corresp="#s13:l1">ghelecht</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">End Godts toornicheit <rhyme label="b" type="f">afghewendet</rhyme>:</l>
        <l id="s13:l4" met="4" type="-+">So dat hy sijn straffe <rhyme label="c" type="m">naeliet</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Dat sijn volck niet gantsch wierdt <rhyme label="b" type="f" corresp="#s13:l3">gheschendet</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Met so veel plaeghen end <rhyme label="c" type="m" corresp="#s13:l4">verdriet</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Se verachtden t'beloefde <rhyme label="a" type="m">landt</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Vol goederen aen elcken <rhyme label="a" type="m" corresp="#s14:l1">kant</rhyme>.</l>
        <l id="s14:l3" met="4" type="-+">Sijnen woorden sy niet <rhyme label="b" type="f">vertrauwden</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Maer murmureerden voor end <rhyme label="c" type="m">naer</rhyme>,</l>
        <l id="s14:l5" met="4" type="-+">In den hutten die sy hen <rhyme label="b" type="f" corresp="#s14:l3">bauwden</rhyme>,</l>
        <l id="s14:l6" met="4" type="-+">End verachtden Godts stemme <rhyme label="c" type="m" corresp="#s14:l4">klaer</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Daerom hief de Heer' op sijn <rhyme label="a" type="m">handt</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Teghen hen, end heeft se met <rhyme label="a" type="m" corresp="#s15:l1">schandt</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Met hoopen te neder <rhyme label="b" type="f">gheslaeghen</rhyme>.</l>
        <l id="s15:l4" met="4" type="-+">Onder den volckeren <rhyme label="c" type="m">aldaer</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">Sijn sy met haer saet doer veel <rhyme label="b" type="f" corresp="#s15:l3">plaeghen</rhyme>,</l>
        <l id="s15:l6" met="4" type="-+">Verstroyt in Godts toorne seer <rhyme label="c" type="m" corresp="#s15:l4">swaer</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Baal Peor hinghen se <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">End hebben te eten <rhyme label="a" type="m" corresp="#s16:l1">bestaen</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Der dooden afgoden <rhyme label="b" type="f">offranden</rhyme>:</l>
        <l id="s16:l4" met="4" type="-+">Se maeckten Godt met haer doen <rhyme label="c" type="m">gram</rhyme>,</l>
        <l id="s16:l5" met="4" type="-+">Dies nam se de Heer' wt den <rhyme label="b" type="f" corresp="#s16:l3">landen</rhyme>,</l>
        <l id="s16:l6" met="4" type="-+">In den toorn die ouer hen <rhyme label="c" type="m" corresp="#s16:l4">quam</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Doe quam Pinehas met der <rhyme label="a" type="m">daet</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">End straft' een sulck schandelick <rhyme label="a" type="m" corresp="#s17:l1">quaet</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">Daermet wierden ghestilt die <rhyme label="b" type="f">plaeghen</rhyme>.</l>
        <l id="s17:l4" met="4" type="-+">T'welcke werck van Godt was <rhyme label="c" type="m">gheacht</rhyme>,</l>
        <l id="s17:l5" met="4" type="-+">Voor een werck nae sijn wel <rhyme label="b" type="f" corresp="#s17:l3">behaeghen</rhyme>,</l>
        <l id="s17:l6" met="4" type="-+">In der gherechticheit <rhyme label="c" type="m" corresp="#s17:l4">volbracht</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Sy vergramden oock Godt <rhyme label="a" type="m">altydt</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Aen't twistwater me haeren <rhyme label="a" type="m" corresp="#s18:l1">strydt</rhyme>:</l>
        <l id="s18:l3" met="4" type="-+">Mosen se oock jammerlick <rhyme label="b" type="f">plaeghden</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">Bedroefden hem so seer sijn <rhyme label="c" type="m">hert'</rhyme>,</l>
        <l id="s18:l5" met="4" type="-+">Dat hy wat sprack dat Godt <rhyme label="b" type="f" corresp="#s18:l3">mishaeghde</rhyme>,</l>
        <l id="s18:l6" met="4" type="-+">Doer ongheduldicheit end <rhyme label="c" type="m" corresp="#s18:l4">smert'</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">De heydenen so Godt <rhyme label="a" type="m">beual</rhyme>,</l>
        <l id="s19:l2" met="4" type="-+">Braghten se niet om <rhyme label="a" type="m" corresp="#s19:l1">oueral</rhyme>.</l>
        <l id="s19:l3" met="4" type="-+">Maer spaerden die t'werck was <rhyme label="b" type="f">verboden</rhyme>:</l>
        <l id="s19:l4" met="4" type="-+">End leerden doen haer boosheit <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s19:l5" met="4" type="-+">Se dienden haer vervloeckt' <rhyme label="b" type="f" corresp="#s19:l3">afgoden</rhyme>,</l>
        <l id="s19:l6" met="4" type="-+">Die hen waeren eenen <rhyme label="c" type="m" corresp="#s19:l4">aenstoot</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Den veldtduyuelen seer <rhyme label="a" type="m">onreyn</rhyme>,</l>
        <l id="s20:l2" met="4" type="-+">Hebben sy gheoffert <rhyme label="a" type="m" corresp="#s20:l1">ghemeyn</rhyme>,</l>
        <l id="s20:l3" met="4" type="-+">Haer sonen end dochteren <rhyme label="b" type="f">t'saemen</rhyme>:</l>
        <l id="s20:l4" met="4" type="-+">Se verghoten t'onschuldigh <rhyme label="c" type="m">bloet</rhyme>,</l>
        <l id="s20:l5" met="4" type="-+">Haerer kinderen, in den <rhyme label="b" type="f" corresp="#s20:l3">naemen</rhyme></l>
        <l id="s20:l6" met="4" type="-+">Der afgoden in <rhyme label="c" type="m" corresp="#s20:l4">ouervloet</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Se hebben t'landt met bloet <rhyme label="a" type="m">bedeckt</rhyme>,</l>
        <l id="s21:l2" met="4" type="-+">Der onschuldighen, end <rhyme label="a" type="m" corresp="#s21:l1">bevleckt</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">Met haer onreynheit niet om <rhyme label="b" type="f">lyden</rhyme>.</l>
        <l id="s21:l4" met="4" type="-+">Se hebben schendtlick <rhyme label="c" type="m">gheboeleert</rhyme>,</l>
        <l id="s21:l5" met="4" type="-+">End daeghelicks aen allen <rhyme label="b" type="f" corresp="#s21:l3">zyden</rhyme>,</l>
        <l id="s21:l6" met="4" type="-+">Met den afgoden <rhyme label="c" type="m" corresp="#s21:l4">ghehoereert</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Godt wierdt ouer t'volck seer <rhyme label="a" type="m">beroert</rhyme>,</l>
        <l id="s22:l2" met="4" type="-+">Dat hy so wydt hadde <rhyme label="a" type="m" corresp="#s22:l1">gheuoert</rhyme>,</l>
        <l id="s22:l3" met="4" type="-+">End kreegh een walghingh' sijner <rhyme label="b" type="f">eruen</rhyme>.</l>
        <l id="s22:l4" met="4" type="-+">Dies ghaf hy sijn volck in der <rhyme label="c" type="m">nacht</rhyme>,</l>
        <l id="s22:l5" met="4" type="-+">Der heydenen, die haer <rhyme label="b" type="f" corresp="#s22:l3">verderuen</rhyme></l>
        <l id="s22:l6" met="4" type="-+">End verdruckingh' sochten met <rhyme label="c" type="m" corresp="#s22:l4">kracht</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Haer haeters hebbens' onder <rhyme label="a" type="m">voet</rhyme></l>
        <l id="s23:l2" met="4" type="-+">Ghehaet, end ghebracht tot <rhyme label="a" type="m" corresp="#s23:l1">ootmoet</rhyme>:</l>
        <l id="s23:l3" met="4" type="-+">Godt maeckte se vry <rhyme label="b" type="f">onbelaeden</rhyme>.</l>
        <l id="s23:l4" met="4" type="-+">Maer se dreuen <rhyme label="c" type="m">moetwillicheit</rhyme>,</l>
        <l id="s23:l5" met="4" type="-+">Niet ter węerelt konde hen <rhyme label="b" type="f" corresp="#s23:l3">schaeden</rhyme>,</l>
        <l id="s23:l6" met="4" type="-+">Dan alleyn haer eyghen <rhyme label="c" type="m" corresp="#s23:l4">boosheit</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Doch aensagh de Heer in den <rhyme label="a" type="m">noot</rhyme>,</l>
        <l id="s24:l2" met="4" type="-+">Sijn volck, end wt ghenaede <rhyme label="a" type="m" corresp="#s24:l1">bloot</rhyme>,</l>
        <l id="s24:l3" met="4" type="-+">Hoorde hy haer bidden end <rhyme label="b" type="f">klaeghen</rhyme>:</l>
        <l id="s24:l4" met="4" type="-+">Aen sijn verbondt heeft hy <rhyme label="c" type="m">ghedacht</rhyme>,</l>
        <l id="s24:l5" met="4" type="-+">End sijns toorns heeft hy leet <rhyme label="b" type="f" corresp="#s24:l3">ghedraeghen</rhyme>,</l>
        <l id="s24:l6" met="4" type="-+">Nae sijn goetheit seer hoogh' <rhyme label="c" type="m" corresp="#s24:l4">gheacht</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">By hen daer sijn volck was <rhyme label="a" type="m">gheuaen</rhyme>,</l>
        <l id="s25:l2" met="4" type="-+">Vonden se gunst van stonden <rhyme label="a" type="m" corresp="#s25:l1">aen</rhyme>.</l>
        <l id="s25:l3" met="4" type="-+">Wilt ons Heer goedighlick <rhyme label="b" type="f">bevryden</rhyme>,</l>
        <l id="s25:l4" met="4" type="-+">End verlossen doer uwen <rhyme label="c" type="m">naem</rhyme>,</l>
        <l id="s25:l5" met="4" type="-+">Van den heydenen t'allen <rhyme label="b" type="f" corresp="#s25:l3">zyden</rhyme>,</l>
        <l id="s25:l6" met="4" type="-+">So sullen wy v prysen <rhyme label="c" type="m" corresp="#s25:l4">t'saem</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Gheloeft sij Godt van <rhyme label="a" type="m">Israel</rhyme>,</l>
        <l id="s26:l2" met="4" type="-+">In eewicheit end niemant <rhyme label="a" type="m" corresp="#s26:l1">el</rhyme>:</l>
        <l id="s26:l3" met="4" type="-+">Hy sij altyt heerlick <rhyme label="b" type="f">ghepresen</rhyme>.</l>
        <l id="s26:l4" met="4" type="-+">Dies moet het volck spreken <rhyme label="c" type="m">verhueght</rhyme>,</l>
        <l id="s26:l5" met="4" type="-+">Het moet also eewighlick <rhyme label="b" type="f" corresp="#s26:l3">wesen</rhyme>,</l>
        <l id="s26:l6" met="4" type="-+">Godt sij alsins gheloeft met <rhyme label="c" type="m" corresp="#s26:l4">vrueghdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
