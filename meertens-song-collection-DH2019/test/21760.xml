        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Psalm 026 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">RIcht my, Heer, na mijn <rhyme label="a" type="m">recht</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">Want ick mijn  wegen <rhyme label="a" type="m" corresp="#s1:l1">slecht</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Bewandel met een vroom <rhyme label="b" type="m">gemoet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Op Godt staet mijn <rhyme label="c" type="f">vertrouwen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hy sal my so vast <rhyme label="c" type="f" corresp="#s1:l4">houwen</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Dat my niet struyck'len sal de <rhyme label="b" type="m" corresp="#s1:l3">voet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">* O Heer versoecke <rhyme label="a" type="m">my</rhyme></l>
        <l id="s2:l2" met="3" type="-+">End' ondertaste <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Off ick met valsheyt <rhyme label="b" type="m">ommegae</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Bekoor mijn hert' end' <rhyme label="c" type="f">nieren</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In allerley <rhyme label="c" type="f" corresp="#s2:l4">manieren</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">End' goed' acht op mijn wandel <rhyme label="b" type="m" corresp="#s2:l3">slae</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">* Want dijn gunst end' <rhyme label="a" type="m">weldaet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">My eewelijcken <rhyme label="a" type="m" corresp="#s3:l1">staet</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Voor d'ooghen claer end' int <rhyme label="b" type="m">ghesicht</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Ick hebbe mynen <rhyme label="c" type="f">handel</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">End' allen mynen <rhyme label="c" type="f" corresp="#s3:l4">wandel</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Nae dyne waerheyt steed's <rhyme label="b" type="m" corresp="#s3:l3">ghericht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">* Ick hebbe noyt <rhyme label="a" type="m">gheleeft</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Met t'volck dat hem <rhyme label="a" type="m" corresp="#s4:l1">begheeft</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Tot loghentael end' valschen <rhyme label="b" type="m">schijn</rhyme>.</l>
        <l id="s4:l4" met="3" type="-+">Ick wil met dien niet <rhyme label="c" type="f">treden</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die onder hare <rhyme label="c" type="f" corresp="#s4:l4">leden</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Bedeckt end' dobbelhertich <rhyme label="b" type="m" corresp="#s4:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">* Ick schroom' end' hebb' een <rhyme label="a" type="m">haet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van al t'gheselschap <rhyme label="a" type="m" corresp="#s5:l1">quaet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Dat sick tot boosheyt heeft <rhyme label="b" type="m">ghewendt</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Dies will' ick niet <rhyme label="c" type="f">verkeeren</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Met lie'en die Godt niet <rhyme label="c" type="f" corresp="#s5:l4">eeren</rhyme></l>
        <l id="s5:l6" met="4" type="-+">End' dien zijn naem is <rhyme label="b" type="m" corresp="#s5:l3">onbekent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">* Ick wasch' myn' handen <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s6:l2" met="3" type="-+">So ick't van herten <rhyme label="a" type="m" corresp="#s6:l1">meyn'</rhyme></l>
        <l id="s6:l3" met="4" type="-+">In onschult suyuer ende <rhyme label="b" type="m">claer</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dies sal ick comen <rhyme label="c" type="f">treden</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Met offers end' <rhyme label="c" type="f" corresp="#s6:l4">ghebeden</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Voor dynen heylighsten <rhyme label="b" type="m" corresp="#s6:l3">autaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">* Op dat mijn hert' end' <rhyme label="a" type="m">mont</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dijn lofsang make <rhyme label="a" type="m" corresp="#s7:l1">kondt</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Met heller stemmen end' <rhyme label="b" type="m">gheklanck</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">End' dyne <rhyme label="c" type="f">wonderwercken</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Die sick vry laten <rhyme label="c" type="f" corresp="#s7:l4">mercken</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Vertelle dy tot lof end' <rhyme label="b" type="m" corresp="#s7:l3">danck</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">* Ick muur met herten <rhyme label="a" type="m">kuys</rhyme></l>
        <l id="s8:l2" met="3" type="-+">De stede van dijn <rhyme label="a" type="m" corresp="#s8:l1">huys</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Daer sick dijn goetheyt <rhyme label="b" type="m">openbaert</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Ick houd' in grooter <rhyme label="c" type="f">weerden</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Die woonstadt hier op <rhyme label="c" type="f" corresp="#s8:l4">eerden</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Daer sick dijn heerlijckheyt <rhyme label="b" type="m" corresp="#s8:l3">verclaert</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">* Laet mijn siel inden <rhyme label="a" type="m">loop</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Met der sondaren <rhyme label="a" type="m" corresp="#s9:l1">hoop</rhyme></l>
        <l id="s9:l3" met="4" type="-+">O Heer niet werden <rhyme label="b" type="m">opgheraept</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Noch mijn onschuldig <rhyme label="c" type="f">leuen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Met't volck die haer <rhyme label="c" type="f" corresp="#s9:l4">begheuen</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Tot bloet vergieten, zijn <rhyme label="b" type="m" corresp="#s9:l3">betraept</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">* Want in haer wreede <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Steeckt boosheydt <rhyme label="a" type="m" corresp="#s10:l1">allerhandt</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Die hun de gheest altijts <rhyme label="b" type="m">bedenckt</rhyme>.</l>
        <l id="s10:l4" met="3" type="-+">De vuyst wert hun <rhyme label="c" type="f">bestreken</rhyme></l>
        <l id="s10:l5" met="3" type="-+">End' d'ooghen gants <rhyme label="c" type="f" corresp="#s10:l4">besteken</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Met giften diemen steed's hun <rhyme label="b" type="m" corresp="#s10:l3">schenckt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">* Maer ick sal my <rhyme label="a" type="m">altoos</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Afkeeren van het <rhyme label="a" type="m" corresp="#s11:l1">boos'</rhyme></l>
        <l id="s11:l3" met="4" type="-+">End' steed's met vroomheyt <rhyme label="b" type="m">ommegaen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">O Heer, mits dijn <rhyme label="c" type="f">ghenaden</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Verlost my vanden <rhyme label="c" type="f" corresp="#s11:l4">quaden</rhyme>.</l>
        <l id="s11:l6" met="4" type="-+">Ontferm dy mijns: end' neemt my <rhyme label="b" type="m" corresp="#s11:l3">aen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">* Die wyl' mijn voeten <rhyme label="a" type="m">staen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Nu op de rechte <rhyme label="a" type="m" corresp="#s12:l1">baen</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Wel vast end' vry van allen <rhyme label="b" type="m">noot</rhyme>.</l>
        <l id="s12:l4" met="3" type="-+">So wil ick God den <rhyme label="c" type="f">Heere</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Oock gheuen prijs end' <rhyme label="c" type="f" corresp="#s12:l4">eere</rhyme></l>
        <l id="s12:l6" met="4" type="-+">Voor menichten van volcken <rhyme label="b" type="m" corresp="#s12:l3">groot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
