<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>23874</id>
    <date>
      <year-start>1592</year-start>
      <year-end>1592</year-end>
    </date>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="danklied" translation="gratitude song">danklied</tag>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
      <tag standard-name="bijbels historielied" translation="biblical history song">bijbels historielied</tag>
    </tags>
    <melody>Nun freut euch liebe Christen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">LAet ons den Heere dancken <rhyme label="a" type="m">reijn</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Loff ende prijs, <rhyme label="b" type="f">toesingen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Dus is gheboren, een Kindeken <rhyme label="a" type="m" corresp="#s1:l1">cleijn</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Noyt wonderlijcker <rhyme label="b" type="f" corresp="#s1:l2">dingen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Van eender Maget <rhyme label="c" type="m">suyuerlijck</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Godts Soone vanden <rhyme label="c" type="m" corresp="#s1:l5">Hemelrijck</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Tsal ons daer met <rhyme label="b" type="f" corresp="#s1:l2">gelingen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wy lagen al in sonden <rhyme label="a" type="m">doot</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Versoncken in godts <rhyme label="b" type="f">tooren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maer Godt, door syne Lieffde <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Wold ons niet laten <rhyme label="b" type="f" corresp="#s2:l2">verlooren</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Hy heeft, aller volcken <rhyme label="c" type="m">heylant</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">Wt den hemel neder <rhyme label="c" type="m" corresp="#s2:l5">ghesant</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Des laet ons vruecht, <rhyme label="b" type="f" corresp="#s2:l2">oorbooren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een  kindt is ons, gebooren <rhyme label="a" type="m">reen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een Soon is des ons <rhyme label="b" type="f">ghegeuen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Om ons te helpen, wt <rhyme label="a" type="m" corresp="#s3:l1">gheween</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Wt de doot, in het <rhyme label="b" type="f" corresp="#s3:l2">leuen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dt is de stercke, <rhyme label="c" type="m">Emanuel</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Coninck en Heer, in <rhyme label="c" type="m" corresp="#s3:l5">Israel</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">In Jsrael <rhyme label="b" type="f" corresp="#s3:l2">verheuen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wonderlijck is, de naeme <rhyme label="a" type="m">sijn</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Raedt, Cracht, Heldt, ewich <rhyme label="b" type="f">Uader</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Hy is een vreedtsaem Prince <rhyme label="a" type="m" corresp="#s4:l1">fijn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Onser sonden <rhyme label="b" type="f" corresp="#s4:l2">ontlader</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Noch is sijnen name <rhyme label="c" type="m">gewis</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Godt de onse gerechticheit <rhyme label="c" type="m" corresp="#s4:l5">is</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Des looft hem alle <rhyme label="b" type="f" corresp="#s4:l2">gader</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Onse vyanden <rhyme label="a" type="m">altesaem</rhyme>:</l>
        <l id="s5:l2" met="3" type="-+">Sonde, Doot, Duyuel, <rhyme label="b" type="f">helle</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Heeft hy verwonnen, zeer <rhyme label="a" type="m" corresp="#s5:l1">bequaem</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En helpt ons wt <rhyme label="b" type="f" corresp="#s5:l2">gequelle</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Des laet ons, uan herten <rhyme label="c" type="m">verblijt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">De Name Gods, <rhyme label="c" type="m" corresp="#s5:l5">gebenedijt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Louen, en prijsen, <rhyme label="b" type="f" corresp="#s5:l2">snelle</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">In Betlehem, de cleijne <rhyme label="a" type="m">Stadt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Is ons zeer <rhyme label="b" type="f">wtvercooren</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">So Michias, vercondicht <rhyme label="a" type="m" corresp="#s6:l1">hadt</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Desen Heylandt, <rhyme label="b" type="f" corresp="#s6:l2">geboren</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Gebooren is, in <rhyme label="c" type="m">Bethlehem</rhyme>:</l>
        <l id="s6:l6" met="4" type="-+">Godts Sone, Coninck der <rhyme label="c" type="m" corresp="#s6:l5">Joden</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die ons vry maeckt, van <rhyme label="b" type="f" corresp="#s6:l2">tooren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Op den velde, de herddren <rhyme label="a" type="m">daer</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Verscheen Godts Engel <rhyme label="b" type="f">claere</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En sprack met woorden, soet tot <rhyme label="a" type="m" corresp="#s7:l1">haer</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Hoort een so blyde <rhyme label="b" type="f" corresp="#s7:l2">maere</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Heden is v geboren <rhyme label="c" type="m">goedt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Christus de Heer, en Heylandt <rhyme label="c" type="m" corresp="#s7:l5">soet</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">In Dauidts Stadt <rhyme label="b" type="f" corresp="#s7:l2">voorwaere</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die Hemelsche, scharen <rhyme label="a" type="m">ghemeen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Hebben Godt hooch <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Sy looffden Godt, met stemmen <rhyme label="a" type="m" corresp="#s8:l1">reen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Seggende so wy <rhyme label="b" type="f" corresp="#s8:l2">leesen</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">Loff sy Godt, Jnden hoochsten <rhyme label="c" type="m">Troon</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Op der aerden vrede <rhyme label="c" type="m" corresp="#s8:l5">Idoon</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">En een goedtwillich <rhyme label="b" type="f" corresp="#s8:l2">wesen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wt Oosten, tot <rhyme label="a" type="m">Jerusalem</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Quamen de wijse <rhyme label="b" type="f">vragen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Waer is de Coninck, der <rhyme label="a" type="m" corresp="#s9:l1">Joden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Ghebooren in desse <rhyme label="b" type="f" corresp="#s9:l2">dagen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Sy hadden ghesien, de Sterre <rhyme label="c" type="m">syn</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">En quamen om hem, taenbidden <rhyme label="c" type="m" corresp="#s9:l5">fijn</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">En te offeren <rhyme label="b" type="f" corresp="#s9:l2">gauen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Laet ons ock alle, cleijn en <rhyme label="a" type="m">groot</rhyme>:</l>
        <l id="s10:l2" met="3" type="-+">Soecken dit Kindeken <rhyme label="b" type="f">reene</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Het helpet ons, wt aller <rhyme label="a" type="m" corresp="#s10:l1">noot</rhyme></l>
        <l id="s10:l4" met="3" type="-+">En brengt ons wt <rhyme label="b" type="f" corresp="#s10:l2">ghemeene</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Offert hem siel, Lichaem, en <rhyme label="c" type="m">Geest</rhyme></l>
        <l id="s10:l6" met="4" type="-+">Looft, ende prijst, hem minst, en <rhyme label="c" type="m" corresp="#s10:l5">meest</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Ja looft hem int <rhyme label="b" type="f" corresp="#s10:l2">ghemeene</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Wy louen dy, O Godes <rhyme label="a" type="m">Soon</rhyme></l>
        <l id="s11:l2" met="3" type="-+">U dancken wy van <rhyme label="b" type="f">herten</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Dat ghy quaemt, wt den Hemels <rhyme label="a" type="m" corresp="#s11:l1">troon</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Ons te helpen wt <rhyme label="b" type="f" corresp="#s11:l2">smerten</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Wy prijsen dy, Heer Jesu <rhyme label="c" type="m">Christ</rhyme>:</l>
        <l id="s11:l6" met="4" type="-+">Dat ghy een mensch, geworden <rhyme label="c" type="m" corresp="#s11:l5">bist</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">En sijt ons hulper <rhyme label="b" type="f" corresp="#s11:l2">werden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
