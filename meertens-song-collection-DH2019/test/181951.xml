  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hollanders vol van <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ontziet geen vlees of <rhyme label="a" type="m" corresp="#s1:l1">bloed</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sy weten van geen <rhyme label="b" type="f">treuren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Soo als men ziet <rhyme label="b" type="f" corresp="#s1:l3">gebeuren</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s1:l5">vlugt</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Uw streelen en <rhyme label="a" type="m">gevly</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat brengt uw steeds in <rhyme label="a" type="m" corresp="#s2:l1">ly</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Soo als je ziet <rhyme label="b" type="f">gebeuren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Holland weet van geen <rhyme label="b" type="f" corresp="#s2:l3">treuren</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">O Holland <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s2:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Gy ziet dit stuk wel <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Toe wy u gingen <rhyme label="a" type="m" corresp="#s3:l1">slaan</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dat gy het niet kost <rhyme label="b" type="f">houwen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gy raakte in groot <rhyme label="b" type="f" corresp="#s3:l3">benouwen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s3:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Uw streelen en <rhyme label="a" type="m">gevly</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat bragt uw in de <rhyme label="a" type="m" corresp="#s4:l1">ly</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gy kunt ons niet <rhyme label="b" type="f">vermaaken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met al uw valsche <rhyme label="b" type="f" corresp="#s4:l3">zaaken</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s4:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gy dit stuk wel aan <rhyme label="a" type="m">zag</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wy waaren aan de <rhyme label="a" type="m" corresp="#s5:l1">slag</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hoe dat wy ons konde <rhyme label="b" type="f">roeren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Gy kost ons niet <rhyme label="b" type="f" corresp="#s5:l3">beloeren</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">want hoord eens wat een <rhyme label="c" type="m">klugt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s5:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat dogt gy van die <rhyme label="a" type="m">klugt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Doe gy moest op de <rhyme label="a" type="m" corresp="#s6:l1">vlugt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Gy kunt ons niet meer <rhyme label="b" type="f">foppen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">want wy u duyvels <rhyme label="b" type="f" corresp="#s6:l3">kloppen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s6:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Engelsche Dog hoord <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat kan zoo niet meer <rhyme label="a" type="m" corresp="#s7:l1">gaan</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Met al uw valsche <rhyme label="b" type="f">zaaken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die kunnen ons niet <rhyme label="b" type="f" corresp="#s7:l3">vermaaken</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s7:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Onze Schippers kloek van <rhyme label="a" type="m">moed</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ontzien geen vlees of <rhyme label="a" type="m" corresp="#s8:l1">bloed</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Captyn Staaring <rhyme label="b" type="f">verheeven</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Verdyt ‘t om op te <rhyme label="b" type="f" corresp="#s8:l3">geven</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s8:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Engelsman dit <rhyme label="a" type="m">aanhoord</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En wild u maaken <rhyme label="a" type="m" corresp="#s9:l1">voort</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">wild uw niet meer <rhyme label="b" type="f">betrouwen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Op onze Leeuw zyn <rhyme label="b" type="f" corresp="#s9:l3">klouwen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Siet viva <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s9:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Weg Engelsman met <rhyme label="a" type="m">voer</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">De Leeuw legt op zyn <rhyme label="a" type="m" corresp="#s10:l1">loer</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hy laat hem niet meer <rhyme label="b" type="f">wiegen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Veel minder meer <rhyme label="b" type="f" corresp="#s10:l3">bedriegen</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s10:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Komen wy weer aan de <rhyme label="a" type="m">Slag</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Dan gy wel vreezen <rhyme label="a" type="m" corresp="#s11:l1">mag</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Al voor de Leeuw zyn <rhyme label="b" type="f">klouwen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">wild u maar niet <rhyme label="b" type="f" corresp="#s11:l3">benouwen</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s11:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Matroozen en <rhyme label="a" type="m">Officiers</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Zoldaaten en <rhyme label="a" type="m" corresp="#s12:l1">Granediers</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Die leeve uit een <rhyme label="b" type="f">treuren</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Soo als men ziet <rhyme label="b" type="f" corresp="#s12:l3">gebeuren</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s12:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Viva Matrooze <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Leert met een bly <rhyme label="a" type="m" corresp="#s13:l1">geschal</rhyme>,</l>
        <l id="s13:l3" met="3" type="-+">En wild maar lustig <rhyme label="b" type="f">drinken</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">En laat de Glaasies <rhyme label="b" type="f" corresp="#s13:l3">klinken</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s13:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Dit Liedje is <rhyme label="a" type="m">gedigt</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">En hier gebragt aan ‘t <rhyme label="a" type="m" corresp="#s14:l1">licht</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">De Ruyter wild het <rhyme label="b" type="f">hooren</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">By Zoutman <rhyme label="b" type="f" corresp="#s14:l3">uitverkooren</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Viva zeer <rhyme label="c" type="m">onbedugt</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">weg Engelsman op de <rhyme label="c" type="m" corresp="#s14:l5">vlugt</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Voor ‘t lest nu <rhyme label="a" type="m">Engelsman</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Doet nu maar wat gy <rhyme label="a" type="m" corresp="#s15:l1">kan</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Ons vlagge laaten <rhyme label="b" type="f">waaijen</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Ons Scheepen leggen <rhyme label="b" type="f" corresp="#s15:l3">draaijen</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Hyst de vlagge <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Tot boven in den <rhyme label="c" type="m" corresp="#s15:l5">top</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Die dit Lied heeft <rhyme label="a" type="m">gedigt</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Matroos zyn hert was <rhyme label="a" type="m" corresp="#s16:l1">ligt</rhyme>.</l>
        <l id="s16:l3" met="3" type="-+">Syn Naam mag je wel <rhyme label="b" type="f">weeten</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">‘t Is P--- D----- <rhyme label="b" type="f" corresp="#s16:l3">geheeten</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Dit is het laast <rhyme label="c" type="m">besluyt</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">Hy lust zyn Glaasie wel <rhyme label="c" type="m" corresp="#s16:l5">uit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
