    <melody>Het viel een hemelse dauw</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="narratives">verhalen</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Het viel een hemels <rhyme label="a" type="f">douwe</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In een kleyn <rhyme label="b" type="m">maechdeken</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ten was noyt beter <rhyme label="a" type="f" corresp="#s1:l1">vrouwe</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat ded’ een <rhyme label="b" type="m" corresp="#s1:l2">kindeken</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat van haer was <rhyme label="c" type="f">geboren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En sy bleef maghet <rhyme label="d" type="m">fijn</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">O maghet <rhyme label="c" type="f" corresp="#s1:l5">uytverkoren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Lof moet u altoos <rhyme label="d" type="m" corresp="#s1:l6">sijn</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Dat van haer was <rhyme label="c" type="f" corresp="#s1:l5">geboren</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">En sy bleef maghet <rhyme label="d" type="m" corresp="#s1:l6">fijn</rhyme>:</l>
        <l id="s1:l11" met="3" type="+-">O maghet <rhyme label="c" type="f" corresp="#s1:l5">uytverkoren</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Lof moet u altoos <rhyme label="d" type="m" corresp="#s1:l6">sijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die maghet ginck met <rhyme label="a" type="f">kinde</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gheen swaerheyt ginck haer <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Als Ioseph dat <rhyme label="a" type="f" corresp="#s2:l1">versinde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die goede weerde <rhyme label="b" type="m" corresp="#s2:l2">man</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Hy docht’ ick wilse <rhyme label="c" type="f">laten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Den Vader ben ick <rhyme label="d" type="m">niet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En trecken mijnder <rhyme label="c" type="f" corresp="#s2:l5">straten</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Eer my schande <rhyme label="d" type="m" corresp="#s2:l6">geschiet</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Hy docht’ ick wilse <rhyme label="c" type="f" corresp="#s2:l5">laten</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Den Vader ben ick <rhyme label="d" type="m" corresp="#s2:l6">niet</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">En trecken mijnder <rhyme label="c" type="f" corresp="#s2:l5">straten</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Eer my schande <rhyme label="d" type="m" corresp="#s2:l6">geschiet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Al van des hemels <rhyme label="a" type="f">throne</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sprack hem den Enghel <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">O Ioseph Davids <rhyme label="a" type="f" corresp="#s3:l1">Soone</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">O uytverkoren <rhyme label="b" type="m" corresp="#s3:l2">man</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Blijft toch beyde te <rhyme label="c" type="f">gader</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">‘Tis boven menschen <rhyme label="d" type="m">kracht</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat Godt almachtich <rhyme label="c" type="f" corresp="#s3:l5">Vader</rhyme></l>
        <l id="s3:l8" met="3" type="-+">In haer dus heeft <rhyme label="d" type="m" corresp="#s3:l6">gewracht</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">Blijft toch beyde te <rhyme label="c" type="f" corresp="#s3:l5">gader</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">‘Tis boven menschen <rhyme label="d" type="m" corresp="#s3:l6">kracht</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Dat Godt almachtich <rhyme label="c" type="f" corresp="#s3:l5">Vader</rhyme></l>
        <l id="s3:l12" met="3" type="-+">In haer dus heeft <rhyme label="d" type="m" corresp="#s3:l6">gewracht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Den Keyser groot van <rhyme label="a" type="f">machte</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Heeft een gebodt <rhyme label="b" type="m">gedaen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat volghend’ het <rhyme label="a" type="f" corresp="#s4:l1">geslachte</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Elck naer die stadt moest <rhyme label="b" type="m" corresp="#s4:l2">gaen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Van daer hy was <rhyme label="c" type="f">gebooren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En brengen sijn <rhyme label="d" type="m">tribuyt</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Dat dedemen daer <rhyme label="c" type="f" corresp="#s4:l5">hooren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En roepen <rhyme label="d" type="m" corresp="#s4:l6">overluyt</rhyme>.</l>
        <l id="s4:l9" met="3" type="-+">Van daer hy was <rhyme label="c" type="f" corresp="#s4:l5">gebooren</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">En brengen sijn <rhyme label="d" type="m" corresp="#s4:l6">tribuyt</rhyme>:</l>
        <l id="s4:l11" met="3" type="+-">Dat dedemen daer <rhyme label="c" type="f" corresp="#s4:l5">hooren</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">En roepen <rhyme label="d" type="m" corresp="#s4:l6">overluyt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maria Ioseph <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Quamen te Bethleem <rhyme label="b" type="m">waert</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Want daer was Iosephs <rhyme label="a" type="f" corresp="#s5:l1">stede</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Soo Schrifture <rhyme label="b" type="m" corresp="#s5:l2">verclaert</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Maer sy mocht niewers <rhyme label="c" type="f">inne</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Men wees haer altoos <rhyme label="d" type="m">voort</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Der Engelen <rhyme label="c" type="f" corresp="#s5:l5">Koninginne</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En wiert daer niet <rhyme label="d" type="m" corresp="#s5:l6">gehoort</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">Maer sy mocht niewers <rhyme label="c" type="f" corresp="#s5:l5">inne</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Men wees haer altoos <rhyme label="d" type="m" corresp="#s5:l6">voort</rhyme>:</l>
        <l id="s5:l11" met="3" type="+-">Der Engelen <rhyme label="c" type="f" corresp="#s5:l5">Koninginne</rhyme></l>
        <l id="s5:l12" met="3" type="-+">En wiert daer niet <rhyme label="d" type="m" corresp="#s5:l6">gehoort</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Daer naer hebben sy <rhyme label="a" type="f">vonden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Een huys seer dun <rhyme label="b" type="m">gedaeckt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Binnen soo kiorten <rhyme label="a" type="f" corresp="#s6:l1">stonden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hebben logijs <rhyme label="b" type="m" corresp="#s6:l2">gemaeckt</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Daer wert die maghet <rhyme label="c" type="f">moeder</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Al sonder wee oft <rhyme label="d" type="m">pijn</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Van ‘smenschen Soon en <rhyme label="c" type="f" corresp="#s6:l5">Broeder</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Mocht hy ons naerder <rhyme label="d" type="m" corresp="#s6:l6">sijn</rhyme>?</l>
        <l id="s6:l9" met="3" type="-+">Daer wert die maghet <rhyme label="c" type="f" corresp="#s6:l5">moeder</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Al sonder wee oft <rhyme label="d" type="m" corresp="#s6:l6">pijn</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">Van ‘smenschen Soon en <rhyme label="c" type="f" corresp="#s6:l5">Broeder</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Mocht hy ons naerder <rhyme label="d" type="m" corresp="#s6:l6">sijn</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Hoe bly was sy van <rhyme label="a" type="f">binnen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Aensiend’ haer kint <rhyme label="b" type="m">minsoot</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sy was vervult met <rhyme label="a" type="f" corresp="#s7:l1">minnen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En leyt hem op den <rhyme label="b" type="m" corresp="#s7:l2">schoot</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Sy kust’ hem aen sijn <rhyme label="c" type="f">wanghen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Sy kust; hem <rhyme label="d" type="m">menichfout</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dat hy quam sijn <rhyme label="c" type="f" corresp="#s7:l5">gevangen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Verlossen jonck en <rhyme label="d" type="m" corresp="#s7:l6">out</rhyme>.</l>
        <l id="s7:l9" met="3" type="-+">Sy kust’ hem aen sijn <rhyme label="c" type="f" corresp="#s7:l5">wanghen</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Sy kust; hem <rhyme label="d" type="m" corresp="#s7:l6">menichfout</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Dat hy quam sijn <rhyme label="c" type="f" corresp="#s7:l5">gevangen</rhyme>,</l>
        <l id="s7:l12" met="3" type="-+">Verlossen jonck en <rhyme label="d" type="m" corresp="#s7:l6">out</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Maria schoon <rhyme label="a" type="f">fonteyne</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Daer Godt sijn rust in <rhyme label="b" type="m">nam</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Bidt voor ons al <rhyme label="a" type="f" corresp="#s8:l1">gemeyne</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Iesus dat soete <rhyme label="b" type="m" corresp="#s8:l2">lam</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Dat hy ons in wil <rhyme label="c" type="f">laten</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Int hemelsche <rhyme label="d" type="m">palleys</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Daer vreucht is boven <rhyme label="c" type="f" corresp="#s8:l5">maten</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">En liefd’ en soeten <rhyme label="d" type="m" corresp="#s8:l6">peys</rhyme>.</l>
        <l id="s8:l9" met="3" type="-+">Dat hy ons in wil <rhyme label="c" type="f" corresp="#s8:l5">laten</rhyme></l>
        <l id="s8:l10" met="3" type="-+">Int hemelsche <rhyme label="d" type="m" corresp="#s8:l6">palleys</rhyme>,</l>
        <l id="s8:l11" met="3" type="+-">Daer vreucht is boven <rhyme label="c" type="f" corresp="#s8:l5">maten</rhyme>,</l>
        <l id="s8:l12" met="3" type="-+">En liefd’ en soeten <rhyme label="d" type="m" corresp="#s8:l6">peys</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
