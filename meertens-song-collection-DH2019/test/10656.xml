    <melody>Ik maak geklaag met reden op deze tijd</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ICk maeck <rhyme label="a" type="m">geclach</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met reden op dese <rhyme label="b" type="m">tijt</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Want ick en <rhyme label="a" type="m" corresp="#s1:l1">mach</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Niet eens worden <rhyme label="b" type="m" corresp="#s1:l2">verblijt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Van een deuchtsamige <rhyme label="c" type="f">vrouwe</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Die ick op eer en <rhyme label="c" type="f" corresp="#s1:l5">trouwe</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">So lang hebbe <rhyme label="b" type="m" corresp="#s1:l2">gevrijt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Sy is oock <rhyme label="a" type="m">waert</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dat ick haer <rhyme label="b" type="m">bemin</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Gheen schoonder <rhyme label="a" type="m" corresp="#s2:l1">expaert</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ic hier op Aerden <rhyme label="b" type="m" corresp="#s2:l2">vin</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Haer eerbaer <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Mach so niet zijn <rhyme label="c" type="f" corresp="#s2:l5">ghepresen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">T'is meer in mijnen <rhyme label="b" type="m" corresp="#s2:l2">sin</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Helena en <rhyme label="a" type="m">was</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">in schoonheyt noyt so <rhyme label="b" type="m">rijc</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Noch op dit <rhyme label="a" type="m" corresp="#s3:l1">pas</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">is niemant haers <rhyme label="b" type="m" corresp="#s3:l2">ghelijck</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van verstandigher <rhyme label="c" type="f">manieren</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Gaet sy haer <rhyme label="c" type="f" corresp="#s3:l5">vercieren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Haer deucht is <rhyme label="b" type="m" corresp="#s3:l2">autentijck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Princesse <rhyme label="a" type="m">siet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Al waert dat my den <rhyme label="b" type="m">doot</rhyme></l>
        <l id="s4:l3" met="2" type="-+">In dit <rhyme label="a" type="m" corresp="#s4:l1">verdriet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met haer pijlen <rhyme label="b" type="m" corresp="#s4:l2">doorschiet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Soo sou ic v <rhyme label="c" type="f">lauderen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">dus doet my die cleyn <rhyme label="c" type="f" corresp="#s4:l5">eeren</rhyme></l>
        <l id="s4:l7" met="3" type="-+">V dienaer in lijden <rhyme label="b" type="m" corresp="#s4:l2">groot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
