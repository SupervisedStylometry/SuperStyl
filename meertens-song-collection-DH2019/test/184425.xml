      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">G'naade bidden wy van <rhyme label="a" type="m">U</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">G'naade is der Zielen <rhyme label="b" type="f">anker</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">En een <rhyme label="b" type="f" corresp="#s1:l2">Kranke</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Vindt in uw' genaade, <rhyme label="c" type="m">zagt'</rhyme></l>
        <l id="s1:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s1:l4">Heelings-kracht</rhyme>.</l>
        <l id="s1:l6" met="4" type="+-">Laat Genaade onzen <rhyme label="d" type="f">herten</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Steeds verligten alle <rhyme label="d" type="f" corresp="#s1:l6">smerten</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Van des Kruisses <rhyme label="e" type="m">Ridderschap</rhyme>.</l>
