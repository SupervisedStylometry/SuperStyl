    <melody>Een aardig vrouwtje heeft mij bedrogen</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VUee v ghy ypocrijten, snoo <rhyme label="a" type="f">guyten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghy die Gods woort <rhyme label="b" type="m">diuijn</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Ia wilt tot ons verwijten, toe <rhyme label="a" type="f" corresp="#s1:l1">sluyten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Uoor alle menschen <rhyme label="b" type="m" corresp="#s1:l2">fijn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">En wilt daer buyten <rhyme label="b" type="m" corresp="#s1:l2">zijn</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ghy die der weduwen <rhyme label="c" type="f">huysen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Dooreet, vreest v van <rhyme label="d" type="m">dien</rhyme></l>
        <l id="s1:l8" met="5" type="-+">Met v ghebeden en schijn lanck van <rhyme label="c" type="f" corresp="#s1:l6">abuysen</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Hoe meyndy doordeel <rhyme label="d" type="m" corresp="#s1:l7">tontulien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wee v ghy blinde leyders <rhyme label="a" type="f">mispresen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die den hoochmoet <rhyme label="b" type="m">begheert</rhyme></l>
        <l id="s2:l3" met="4" type="-+">En zijt Gods woorts afscheyders ten <rhyme label="a" type="f" corresp="#s2:l1">desen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Door laster, druck en <rhyme label="b" type="m" corresp="#s2:l2">sweert</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wee v die Christum <rhyme label="b" type="m" corresp="#s2:l2">deert</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ghy die als witte <rhyme label="c" type="f">grauen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Wat schijnt hier int <rhyme label="d" type="m">aensien</rhyme></l>
        <l id="s2:l8" met="5" type="-+">En binnen zijt ghy als stinckende <rhyme label="c" type="f" corresp="#s2:l6">slauen</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Hoe meyndy doordeel <rhyme label="d" type="m" corresp="#s2:l7">tontulien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wce v ghy Schriftgheleerde <rhyme label="a" type="f">moordadich</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghy guychelachtich <rhyme label="b" type="m">saet</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Ghy die als de verkeerde, <rhyme label="a" type="f" corresp="#s3:l1">onghestadich</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Met uwen valschen <rhyme label="b" type="m" corresp="#s3:l2">raet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Het volck zijt laende <rhyme label="b" type="m" corresp="#s3:l2">quaet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Door inghestelde <rhyme label="c" type="f">lasten</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Seer swaerlick int <rhyme label="d" type="m">bespien</rhyme></l>
        <l id="s3:l8" met="5" type="-+">Daer ghy den vingher niet en wilt aen <rhyme label="c" type="f" corresp="#s3:l6">tasten</rhyme></l>
        <l id="s3:l9" met="3" type="+-">Hoe meyndy doordeel <rhyme label="d" type="m" corresp="#s3:l7">tontulien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wee v die onueruleten, <rhyme label="a" type="f">beneuen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Uerheft, verciert, en <rhyme label="b" type="m">boudt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">De grauen der Propheten <rhyme label="a" type="f" corresp="#s4:l1">verheuen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Die v voorvaders <rhyme label="b" type="m" corresp="#s4:l2">stoudt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hebben verdruct <rhyme label="b" type="m" corresp="#s4:l2">benoudt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En seght met woorden <rhyme label="c" type="f">crachtich</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Hadden wy dat <rhyme label="d" type="m">ghesien</rhyme></l>
        <l id="s4:l8" met="5" type="-+">Wy en souden int bloet niet zijn <rhyme label="c" type="f" corresp="#s4:l6">deelachtich</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Hoe meyndy doordeel <rhyme label="d" type="m" corresp="#s4:l7">tontulien</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dies spreect dit moechdy weten al <rhyme label="a" type="f">vooren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die wijsheyt Gods <rhyme label="b" type="m">voorwaer</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Ick sal van mijn Propheten <rhyme label="a" type="f" corresp="#s5:l1">verkooren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Tot v lieden senden <rhyme label="b" type="m" corresp="#s5:l2">claer</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Dien suldy doon <rhyme label="b" type="m" corresp="#s5:l2">eenpaer</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Op dat het bloet sou <rhyme label="c" type="f">strijden</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Teghen v boos <rhyme label="d" type="m">gheschien</rhyme></l>
        <l id="s5:l8" met="5" type="-+">Dat ghestort is sichtent Abels <rhyme label="c" type="f" corresp="#s5:l6">tijden</rhyme></l>
        <l id="s5:l9" met="3" type="+-">Hoe meendy doordeel <rhyme label="d" type="m" corresp="#s5:l7">tontulien</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Prince veruult de maten seer <rhyme label="a" type="f">vierich</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ws vaders ganghen <rhyme label="b" type="m">snoot</rhyme></l>
        <l id="s6:l3" met="4" type="-+">In steden en in straten <rhyme label="a" type="f" corresp="#s6:l1">bloetghierich</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Tot dat tghetal <rhyme label="b" type="m" corresp="#s6:l2">mijnioot</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ueruult sal wesen <rhyme label="b" type="m" corresp="#s6:l2">bloot</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Uerdruct in al v <rhyme label="c" type="f">ganghen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Gods wtuercoren <rhyme label="d" type="m">lien</rhyme></l>
        <l id="s6:l8" met="5" type="-+">Ey valschen aert, en ghebroetsel der <rhyme label="c" type="f" corresp="#s6:l6">slanghen</rhyme></l>
        <l id="s6:l9" met="3" type="+-">Hoe meyndy doordeel <rhyme label="d" type="m" corresp="#s6:l7">tontulien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
