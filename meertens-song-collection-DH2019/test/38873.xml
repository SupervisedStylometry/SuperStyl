    <melody>Herders hij is geboren</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="narratives">verhalen</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEer hoe wonder syn u <rhyme label="a" type="f">wercken</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe wonder is u <rhyme label="b" type="m">macht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Lest-mal quaem ick te <rhyme label="a" type="f" corresp="#s1:l1">Kercken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Het was op een <rhyme label="b" type="m" corresp="#s1:l2">Kers-nacht</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daer stont een <rhyme label="c" type="m">Cavalier</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Al voor een Joffrauws <rhyme label="c" type="m" corresp="#s1:l5">deur</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En dede syn <rhyme label="d" type="m">beklagh</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ontrendt den <rhyme label="d" type="m" corresp="#s1:l7">middernacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Het was een groote <rhyme label="a" type="f">koude</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Het sneeuwde al soo <rhyme label="b" type="m">seer</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">De liefd' most ick <rhyme label="a" type="f" corresp="#s2:l1">aenschauwen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Van desen grooten <rhyme label="b" type="m" corresp="#s2:l2">Heer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ick leyde my achter de <rhyme label="c" type="m">banck</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Om te hooren synen <rhyme label="c" type="m" corresp="#s2:l5">sanck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Om te hooren het <rhyme label="d" type="m">vermaeck</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">De uytkomst van de <rhyme label="d" type="m" corresp="#s2:l7">saeck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy liet een Albade <rhyme label="a" type="f">spelen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Al voor syn schoon <rhyme label="b" type="m">Goddin</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Had ick Cuypidos <rhyme label="a" type="f" corresp="#s3:l1">vleugels</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ick vloogh ter vensteren <rhyme label="b" type="m" corresp="#s3:l2">in</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat seyde hy <rhyme label="c" type="m">menighmal</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al met eene soete <rhyme label="c" type="m" corresp="#s3:l5">tael</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Al met een soet <rhyme label="d" type="m">ghesmeck</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Het schynd dat hy <rhyme label="d" type="m" corresp="#s3:l7">besweeck</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wilt door u venster <rhyme label="a" type="f">kycken</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ick bidde u doet my de <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Help my myn vonnisse <rhyme label="a" type="f" corresp="#s4:l1">stricken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ick en verheysch van u niet <rhyme label="b" type="m" corresp="#s4:l2">meer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hy heeft het soo wel <rhyme label="c" type="m">ghemaeckt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Haer jonck herte is <rhyme label="c" type="m" corresp="#s4:l5">ontwaeckt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Doen hoorde hy een <rhyme label="d" type="m">stem</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Die weder riep tot <rhyme label="d" type="m" corresp="#s4:l7">hem</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Myn Heer ghy moet <rhyme label="a" type="f">vertrecken</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wat staet ghy hier en <rhyme label="b" type="m">maeckt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Papa die sal <rhyme label="a" type="f" corresp="#s5:l1">ontwecken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Mama niet vast en <rhyme label="b" type="m" corresp="#s5:l2">slaept</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En myn <rhyme label="c" type="m">Camenier</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En slapt niet ver van <rhyme label="c" type="m" corresp="#s5:l5">hier</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ick bid u <rhyme label="d" type="m">retireert</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Niet meer en <rhyme label="d" type="m" corresp="#s5:l7">lammenteert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O mynen soeten <rhyme label="a" type="f">Engel</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Soo ghy myn leven <rhyme label="b" type="m">ent</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Soo sal ick gaen <rhyme label="a" type="f" corresp="#s6:l1">volbrenghen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Het s'weirels <rhyme label="b" type="m" corresp="#s6:l2">commandement</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">O doodt o felle <rhyme label="c" type="m">doodt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Helpt my uyt den <rhyme label="c" type="m" corresp="#s6:l5">noodt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Helpt my uyt de <rhyme label="d" type="m">pyn</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dat ick eens verlost magh <rhyme label="d" type="m" corresp="#s6:l7">zyn</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O doodt gy beydt soo <rhyme label="a" type="f">lange</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Ghy laet my in <rhyme label="b" type="m">ghetreur</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ick sal my gaen <rhyme label="a" type="f" corresp="#s7:l1">verhanghen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Al voor myn Matressens <rhyme label="b" type="m" corresp="#s7:l2">deur</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Myn lief mon <rhyme label="c" type="m">Ceur</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Worpt uwen snutdoeck <rhyme label="c" type="m" corresp="#s7:l5">deur</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Dat het magh zyn een <rhyme label="d" type="m">strop</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Om my te hanghen <rhyme label="d" type="m" corresp="#s7:l7">op</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Doen sy dees woorden <rhyme label="a" type="f">hoorde</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die haer minnaer <rhyme label="b" type="m">sprack</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Sy liet haer met een <rhyme label="a" type="f" corresp="#s8:l1">koorde</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Al soetjens rysen <rhyme label="b" type="m" corresp="#s8:l2">af</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Het was in klaer <rhyme label="c" type="m">maeneschyn</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Sy was in wit <rhyme label="c" type="m" corresp="#s8:l5">Satijn</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Schoonder als den <rhyme label="d" type="m">daghraet</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Die smorgens vroegh <rhyme label="d" type="m" corresp="#s8:l7">opstaet</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Myn heer viel neer ter <rhyme label="a" type="f">eerden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En kuste haer schoon <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Een Kotse met ses <rhyme label="a" type="f" corresp="#s9:l1">Peerden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Stont daer al in <rhyme label="b" type="m" corresp="#s9:l2">gespant</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Hy en heeft niet lanck <rhyme label="c" type="m">ghebeyt</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Hy heeftse daer in <rhyme label="c" type="m" corresp="#s9:l5">gheleyt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Hy voerdense <rhyme label="d" type="m">playsant</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Al naer syns Vaders <rhyme label="d" type="m" corresp="#s9:l7">landt</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
