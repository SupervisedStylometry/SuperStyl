<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24534</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
    </tags>
    <melody>Mijn hart dat is in lijden</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Myn hert dat is in <rhyme label="a" type="f">liden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">alst dencket opt <rhyme label="b" type="m">hemelryke</rhyme></l>
        <l id="s1:l3" met="3" type="-+">daer sy hem alle ver  <rhyme label="a" type="f" corresp="#s1:l1">blyden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">mit Jhesus <rhyme label="b" type="m" corresp="#s1:l2">ewelicke</rhyme></l>
        <l id="s1:l5" met="3" type="-+">ende syn van sorgen <rhyme label="c" type="m">vry</rhyme></l>
        <l id="s1:l6" met="3" type="-+">och here hoe wee is <rhyme label="c" type="m" corresp="#s1:l5">my</rhyme></l>
        <l id="s1:l7" met="3" type="-+">als my comt inden <rhyme label="d" type="m">syn</rhyme></l>
        <l id="s1:l8" met="3" type="-+">dat ic daer niet en <rhyme label="d" type="m" corresp="#s1:l7">byn</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Och ic byn hier <rhyme label="a" type="f">beneden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">hier bauen die ic <rhyme label="b" type="m">mynne</rhyme></l>
        <l id="s2:l3" met="3" type="-+">tys lange tyt <rhyme label="a" type="f" corresp="#s2:l1">geleden</rhyme></l>
        <l id="s2:l4" met="3" type="-+">ic en hoerden nie van <rhyme label="b" type="m" corresp="#s2:l2">hem</rhyme></l>
        <l id="s2:l5" met="3" type="-+">hoe dattet mit hem <rhyme label="c" type="m">staet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">O Jhesus lief <rhyme label="c" type="m" corresp="#s2:l5">ontfaet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">myn ziele die aldus <rhyme label="d" type="m">seer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">om v quelt lieue <rhyme label="d" type="m" corresp="#s2:l7">here</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wie sal myn bosscop <rhyme label="a" type="f">dragen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">aen Jhesus myn suete <rhyme label="b" type="m">lief</rhyme></l>
        <l id="s3:l3" met="3" type="-+">och mocht ic hem <rhyme label="a" type="f" corresp="#s3:l1">behagen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">dat waer al myn <rhyme label="b" type="m" corresp="#s3:l2">beryf</rhyme></l>
        <l id="s3:l5" met="3" type="-+">soe wolde ic vrolick <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">myn herte lidet dickewyle <rhyme label="c" type="m" corresp="#s3:l5">pyn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">als ic my yet <rhyme label="d" type="m">ontgae</rhyme></l>
        <l id="s3:l8" met="3" type="-+">ende myn lief niet vast en <rhyme label="d" type="m" corresp="#s3:l7">stae</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick en weet geen beter <rhyme label="a" type="f">bade</rhyme></l>
        <l id="s4:l2" met="3" type="-+">te senden aen my <rhyme label="b" type="m">lief</rhyme></l>
        <l id="s4:l3" met="3" type="-+">dan ic my geue toe <rhyme label="a" type="f" corresp="#s4:l1">gade</rhyme></l>
        <l id="s4:l4" met="3" type="-+">myn herte sal syn myn  <rhyme label="b" type="m" corresp="#s4:l2">brief</rhyme></l>
        <l id="s4:l5" met="3" type="-+">hi salt al wal <rhyme label="c" type="m">verstaen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">hi ist diet herte siet <rhyme label="c" type="m" corresp="#s4:l5">aen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">ende hi daer nae <rhyme label="d" type="m">verleent</rhyme></l>
        <l id="s4:l8" met="3" type="-+">syne gracie die hem <rhyme label="d" type="m" corresp="#s4:l7">dient</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O Jhesus here der <rhyme label="a" type="f">heren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">gades wtuercaren <rhyme label="b" type="m">soen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">aensiet doch myne <rhyme label="a" type="f" corresp="#s5:l1">begeeren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">gelieuet v wilt <rhyme label="b" type="m" corresp="#s5:l2">doen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">het is seer myne <rhyme label="c" type="m">begeerte</rhyme></l>
        <l id="s5:l6" met="3" type="-+">te syn <rhyme label="c" type="m" corresp="#s5:l5">gevisentiert</rhyme></l>
        <l id="s5:l7" met="3" type="-+">van v o lief te <rhyme label="d" type="m">hant</rhyme></l>
        <l id="s5:l8" met="3" type="-+">my seer nae v <rhyme label="d" type="m" corresp="#s5:l7">verlanget</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">V suete <rhyme label="a" type="f">conselacie</rhyme></l>
        <l id="s6:l2" met="3" type="-+">O Jhesus lieue <rhyme label="b" type="m">here</rhyme></l>
        <l id="s6:l3" met="3" type="-+">is my een grote <rhyme label="a" type="f" corresp="#s6:l1">gracie</rhyme></l>
        <l id="s6:l4" met="3" type="-+">ic bid v daer om <rhyme label="b" type="m" corresp="#s6:l2">seer</rhyme></l>
        <l id="s6:l5" met="3" type="-+">och voelde ic die in <rhyme label="c" type="m">my</rhyme></l>
        <l id="s6:l6" met="3" type="-+">van sorgen waer ic <rhyme label="c" type="m" corresp="#s6:l5">vry</rhyme></l>
        <l id="s6:l7" met="3" type="-+">want hoet hert is <rhyme label="d" type="m">ghewont</rhyme></l>
        <l id="s6:l8" met="3" type="-+">sy maket al <rhyme label="d" type="m" corresp="#s6:l7">gesont</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Waer blyfdy <rhyme label="a" type="f">wtuercaeren</rhyme></l>
        <l id="s7:l2" met="3" type="-+">ic en weerde v niet <rhyme label="b" type="m">ontwaer</rhyme></l>
        <l id="s7:l3" met="3" type="-+">ic bid v wilt doch <rhyme label="a" type="f" corresp="#s7:l1">hoeren</rhyme></l>
        <l id="s7:l4" met="3" type="-+">myn  hert dat is my <rhyme label="b" type="m" corresp="#s7:l2">swaer</rhyme></l>
        <l id="s7:l5" met="3" type="-+">te maele seer <rhyme label="c" type="m">ongestelt</rhyme></l>
        <l id="s7:l6" met="3" type="-+">het is om v dat <rhyme label="c" type="m" corresp="#s7:l5">quelt</rhyme></l>
        <l id="s7:l7" met="3" type="-+">van rouwen salt <rhyme label="d" type="m">vergaen</rhyme></l>
        <l id="s7:l8" met="3" type="-+">macht uwen troest niet <rhyme label="d" type="m" corresp="#s7:l7">ontfaen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ick had v alsoe <rhyme label="a" type="f">geerne</rhyme></l>
        <l id="s8:l2" met="3" type="-+">O Jhesu lief in <rhyme label="b" type="m">my</rhyme></l>
        <l id="s8:l3" met="3" type="-+">gy syt myns hertsen <rhyme label="a" type="f" corresp="#s8:l1">verclaren</rhyme></l>
        <l id="s8:l4" met="3" type="-+">mit v soe byn ick <rhyme label="b" type="m" corresp="#s8:l2">blyde</rhyme></l>
        <l id="s8:l5" met="3" type="-+">tys anders groet <rhyme label="c" type="m">verdriet</rhyme></l>
        <l id="s8:l6" met="3" type="-+">al dat in my <rhyme label="c" type="m" corresp="#s8:l5">geschiet</rhyme></l>
        <l id="s8:l7" met="3" type="-+">woe schoen woe suet et mach <rhyme label="d" type="m">syn</rhyme></l>
        <l id="s8:l8" met="3" type="-+">et wort al int leste <rhyme label="d" type="m" corresp="#s8:l7">pyn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">En wilt my niet <rhyme label="a" type="f">begeuen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">myn alre liefste <rhyme label="b" type="m">here</rhyme></l>
        <l id="s9:l3" met="3" type="-+">want sonder v te <rhyme label="a" type="f" corresp="#s9:l1">leuen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">en wonsche ic nvmmer <rhyme label="b" type="m" corresp="#s9:l2">meer</rhyme></l>
        <l id="s9:l5" met="3" type="-+">gy syt myn hert myn <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s9:l6" met="3" type="-+">ic  bid v spreket my <rhyme label="c" type="m" corresp="#s9:l5">in</rhyme></l>
        <l id="s9:l7" met="3" type="-+">laet my van v <rhyme label="d" type="m">verstaen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">hoe gy wilt syn <rhyme label="d" type="m" corresp="#s9:l7">ontfaen</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Och here voel <rhyme label="a" type="f">trybelacie</rhyme></l>
        <l id="s10:l2" met="3" type="-+">is my aldus <rhyme label="b" type="m">bereyt</rhyme></l>
        <l id="s10:l3" met="3" type="-+">heb icket verdient och <rhyme label="a" type="f" corresp="#s10:l1">lacy</rhyme></l>
        <l id="s10:l4" met="3" type="-+">dat is my waerlic <rhyme label="b" type="m" corresp="#s10:l2">leyt</rhyme></l>
        <l id="s10:l5" met="3" type="-+">my doncket ghi slaet my <rhyme label="c" type="m">leech</rhyme></l>
        <l id="s10:l6" met="3" type="-+">ten is ommer niet alst <rhyme label="c" type="m" corresp="#s10:l5">peech</rhyme></l>
        <l id="s10:l7" met="3" type="-+">ick clage v myn <rhyme label="d" type="m">verdriet</rhyme></l>
        <l id="s10:l8" met="3" type="-+">ende gien hoert my <rhyme label="d" type="m" corresp="#s10:l7">niet</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Och mensche gy moeget wal <rhyme label="a" type="f">clagen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">dat gy hebt groet <rhyme label="b" type="m">verdriet</rhyme></l>
        <l id="s11:l3" met="3" type="-+">hoe soldy my <rhyme label="a" type="f" corresp="#s11:l1">behagen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">en haddys lidens <rhyme label="b" type="m" corresp="#s11:l2">niet</rhyme></l>
        <l id="s11:l5" met="3" type="-+">ic heb geleden <rhyme label="c" type="m">meer</rhyme></l>
        <l id="s11:l6" met="3" type="-+">al claegdi noch soe <rhyme label="c" type="m" corresp="#s11:l5">seer</rhyme></l>
        <l id="s11:l7" met="3" type="-+">och haddy my seer <rhyme label="d" type="m">weert</rhyme></l>
        <l id="s11:l8" met="3" type="-+">liden waer al dine <rhyme label="d" type="m" corresp="#s11:l7">begeerte</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Och woldi <rhyme label="a" type="f">ouerdencken</rhyme></l>
        <l id="s12:l2" met="3" type="-+">wat ic v geerne wil <rhyme label="b" type="m">doen</rhyme></l>
        <l id="s12:l3" met="3" type="-+">ic wil v hier voer <rhyme label="a" type="f" corresp="#s12:l1">schencken</rhyme></l>
        <l id="s12:l4" met="3" type="-+">een sunderlynge <rhyme label="b" type="m" corresp="#s12:l2">croen</rhyme></l>
        <l id="s12:l5" met="3" type="-+">voer ellic <rhyme label="c" type="m">wederstaen</rhyme></l>
        <l id="s12:l6" met="3" type="-+">och wat suldy <rhyme label="c" type="m" corresp="#s12:l5">ontfaen</rhyme></l>
        <l id="s12:l7" met="3" type="-+">comt v dat inden <rhyme label="d" type="m">syn</rhyme></l>
        <l id="s12:l8" met="3" type="-+">wort liden groet <rhyme label="d" type="m" corresp="#s12:l7">gewynne</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Ick wil v mit my <rhyme label="a" type="f">croenen</rhyme></l>
        <l id="s13:l2" met="3" type="-+">voert liden dat gi <rhyme label="b" type="m">lyt</rhyme></l>
        <l id="s13:l3" met="3" type="-+">ende sult my daer toe <rhyme label="a" type="f" corresp="#s13:l1">loene</rhyme></l>
        <l id="s13:l4" met="3" type="-+">voer hebben tot alre <rhyme label="b" type="m" corresp="#s13:l2">tyt</rhyme></l>
        <l id="s13:l5" met="3" type="-+">daer gi by my sult <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s13:l6" met="3" type="-+">ende v loen sult <rhyme label="c" type="m" corresp="#s13:l5">ontfaen</rhyme></l>
        <l id="s13:l7" met="3" type="-+">mit mynen vrenden <rhyme label="d" type="m">groet</rhyme></l>
        <l id="s13:l8" met="3" type="-+">dan sydy wtter <rhyme label="d" type="m" corresp="#s13:l7">noet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
