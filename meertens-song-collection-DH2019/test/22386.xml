    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Mijn Siele prijst den Heere <rhyme label="a" type="m">fier</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En mijnen gheest op <rhyme label="b" type="f">springhet</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">In Godt mijn Salichmaker <rhyme label="a" type="m" corresp="#s1:l1">hier</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maria selue <rhyme label="b" type="f" corresp="#s1:l2">singhet</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Sijn goetheyt <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Heeft die <rhyme label="c" type="m" corresp="#s1:l5">ootmoet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Sijns Dienstmaechts niet <rhyme label="d" type="f">versteken</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En heeft <rhyme label="e" type="m">volbracht</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Sijn Goddelijcke <rhyme label="e" type="m" corresp="#s1:l8">macht</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Dies sy my al lof <rhyme label="d" type="f" corresp="#s1:l7">spreeken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sijnen name is <rhyme label="a" type="m">ghebenedijt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En gheeft den menschen <rhyme label="b" type="f">vrede</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die hier op zijn goetheyt <rhyme label="a" type="m" corresp="#s2:l1">altijt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoepen en vreesen <rhyme label="b" type="f" corresp="#s2:l2">mede</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">Want in syn <rhyme label="c" type="m">werck</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Is hy seer <rhyme label="c" type="m" corresp="#s2:l5">sterck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Met synen armen <rhyme label="d" type="f">fijn</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Verstroeyt <rhyme label="e" type="m">terstont</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">tot inden <rhyme label="e" type="m" corresp="#s2:l8">gront</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Die opgheblasen van herten <rhyme label="d" type="f" corresp="#s2:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die Rijcke hy vanden stoele <rhyme label="a" type="m">stelt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die nedere doet hy <rhyme label="b" type="f">rijsen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En die van hongher waren <rhyme label="a" type="m" corresp="#s3:l1">ghequelt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Heeft hy veruult met <rhyme label="b" type="f" corresp="#s3:l2">spijsen</rhyme>:</l>
        <l id="s3:l5" met="2" type="-+">Die Rijcke hy <rhyme label="c" type="m">laet</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">gaen <rhyme label="c" type="m" corresp="#s3:l5">onuerzaet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En oock met ijdel <rhyme label="d" type="f">handen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Maer in dit <rhyme label="e" type="m">dal</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Die armen <rhyme label="e" type="m" corresp="#s3:l8">al</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Helpt hy wt droefheyts <rhyme label="d" type="f" corresp="#s3:l7">banden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hy heeft Israhel zijnen <rhyme label="a" type="m">Knecht</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Ghenadich <rhyme label="b" type="f">aenghenomen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Syn goetheyt hy bedacht te <rhyme label="a" type="m" corresp="#s4:l1">recht</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Daer met hy hem wou <rhyme label="b" type="f" corresp="#s4:l2">vromen</rhyme>:</l>
        <l id="s4:l5" met="2" type="-+">So hy <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">den Vaderen <rhyme label="c" type="m" corresp="#s4:l5">claer</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Hadde voorlanck <rhyme label="d" type="f">ghesworen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Abraham <rhyme label="e" type="m">bekent</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">En zijn Zaet <rhyme label="e" type="m" corresp="#s4:l8">gent</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">In eewicheyt <rhyme label="d" type="f" corresp="#s4:l7">vercoren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
