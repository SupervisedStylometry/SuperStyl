  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">’t BRuidtje zulle wy strick <rhyme label="a" type="f">strackjes</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Gaan geleide naar het <rhyme label="b" type="m">bed</rhyme>’;</l>
        <l id="s1:l3" met="4" type="+-">Vertelt gy elkaar wat <rhyme label="a" type="f" corresp="#s1:l1">quackjes</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wy voldoen de <rhyme label="b" type="m" corresp="#s1:l2">Maagde-wet</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">En geven aan de <rhyme label="c" type="m">Bruit</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Een kus, maar tot <rhyme label="c" type="m" corresp="#s1:l5">besluit</rhyme></l>
        <l id="s1:l7" met="2" type="-+">En schreie <rhyme label="d" type="m">om</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Haar <rhyme label="d" type="m" corresp="#s1:l7">Maagedom</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Schier onze ogen <rhyme label="c" type="m" corresp="#s1:l5">uit</rhyme>.</l>
      </lg>
    </lg>
  </text>
