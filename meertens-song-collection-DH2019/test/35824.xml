  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LAet ons eendrachtigh <rhyme label="a" type="f">heden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met een bereyde <rhyme label="b" type="m">geest</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Vieren met ruste <rhyme label="a" type="f" corresp="#s1:l1">leden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dit tegenwoordigh <rhyme label="b" type="m" corresp="#s1:l2">feest</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">'t Feest van onse <rhyme label="c" type="f">Patroone</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Sint N..... is sijn <rhyme label="d" type="m">naem</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Wiens vreucht in 's Hemels <rhyme label="c" type="f" corresp="#s1:l5">throone</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Is groot, en hier sijn <rhyme label="d" type="m" corresp="#s1:l6">faem</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Hoe aengenaem en <rhyme label="e" type="f">krachtigh</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Dat zijn by Godt <rhyme label="e" type="f" corresp="#s1:l9">almachtigh</rhyme></l>
        <l id="s1:l11" met="3" type="+-">De heylige <rhyme label="f" type="f">Patroonen</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Die zijn in 's Hemels <rhyme label="g" type="m">Hof</rhyme>.</l>
        <l id="s1:l13" met="3" type="-+">Ick sal daer yet van <rhyme label="f" type="f" corresp="#s1:l11">toonen</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Tot Godes eer en <rhyme label="g" type="m" corresp="#s1:l12">lof</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Nicanor, in de <rhyme label="a" type="f">tijde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van Iudas <rhyme label="b" type="m">Machabe</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Goods eer en faem <rhyme label="a" type="f" corresp="#s2:l1">bestrijde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Goods volck ende Goods <rhyme label="b" type="m" corresp="#s2:l2">Stee</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">d' Hooghste Priester <rhyme label="c" type="f">Onias</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Aen Judas <rhyme label="d" type="m">openbaert</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Als mede <rhyme label="c" type="f" corresp="#s2:l5">Jeremias</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Begraven langh in d' <rhyme label="d" type="m" corresp="#s2:l6">aerdt</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Dese trouwe <rhyme label="e" type="f">voorbidder</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">En Propheet geeft de <rhyme label="e" type="f" corresp="#s2:l9">Ridder</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Iudas, een goude <rhyme label="f" type="f">degen</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Goods gift genoemt in d' <rhyme label="g" type="m">hant</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Victory heeft <rhyme label="f" type="f" corresp="#s2:l11">gekregen</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Tegen sijnen <rhyme label="g" type="m" corresp="#s2:l12">vyandt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Spanjen wordt heel <rhyme label="a" type="f">verlooren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En geruineert <rhyme label="b" type="m">schier</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Door de Heydens en <rhyme label="a" type="f" corresp="#s3:l1">Mooren</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">In 't Iaer acht hondert <rhyme label="b" type="m" corresp="#s3:l2">vier</rhyme></l>
        <l id="s3:l5" met="3" type="-+">En dertich; als de <rhyme label="c" type="f">Koningh</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Van Spanjen, Ramier, <rhyme label="d" type="m">bidt</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Tot hem uyt 's Hemels <rhyme label="c" type="f" corresp="#s3:l5">woningh</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Komt Sint Jacob in 't <rhyme label="d" type="m" corresp="#s3:l6">wit</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">De Meerder op der <rhyme label="e" type="f">aerde</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Beschermer der <rhyme label="e" type="f" corresp="#s3:l9">Spanjaerde</rhyme>:</l>
        <l id="s3:l11" met="3" type="+-">Dese Patroon <rhyme label="f" type="f">begonne</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Heeft als een Heldt de <rhyme label="g" type="m">strijdt</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">Hebben de Moor <rhyme label="f" type="f" corresp="#s3:l11">verwonnen</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Door hem, en 't landt <rhyme label="g" type="m" corresp="#s3:l12">bevrijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die van <rhyme label="a" type="f">Antiochi√©n</rhyme>.</l>
        <l id="s4:l2" met="3" type="-+">In 't quaedt, soo jongh als <rhyme label="b" type="m">oudt</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Waren verhart als <rhyme label="a" type="f" corresp="#s4:l1">steenen</rhyme>;</l>
        <l id="s4:l4" met="3" type="-+">In deughden heel <rhyme label="b" type="m" corresp="#s4:l2">verkout</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Na zonden volge <rhyme label="c" type="f">plagen</rhyme>:</l>
        <l id="s4:l6" met="3" type="-+">De Turcken by haer <rhyme label="d" type="m">haest</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Als Mieren krielen <rhyme label="c" type="f" corresp="#s4:l5">sagen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Tot sterven zijn <rhyme label="d" type="m" corresp="#s4:l6">verbaest</rhyme>.</l>
        <l id="s4:l9" met="3" type="-+">Als Ninive sy <rhyme label="e" type="f">weenen</rhyme>:</l>
        <l id="s4:l10" met="3" type="-+">Sint Lucas is <rhyme label="e" type="f" corresp="#s4:l9">verschenen</rhyme>.</l>
        <l id="s4:l11" met="3" type="+-">Patroon, eertijdts haer <rhyme label="f" type="f">Borger</rhyme>:</l>
        <l id="s4:l12" met="3" type="-+">Hy verslaet als een <rhyme label="g" type="m">Heldt</rhyme>,</l>
        <l id="s4:l13" met="3" type="-+">Beschermer en <rhyme label="f" type="f" corresp="#s4:l11">besorger</rhyme></l>
        <l id="s4:l14" met="3" type="-+">De Turcken met <rhyme label="g" type="m" corresp="#s4:l12">gewelt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ambrosius <rhyme label="a" type="f">Milanen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met roeyen, na sijn <rhyme label="b" type="m">doodt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Van noodt, vyant en <rhyme label="a" type="f" corresp="#s5:l1">tranen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Bevrijdt uyt liefde <rhyme label="b" type="m" corresp="#s5:l2">groot</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Sint Felix komt <rhyme label="c" type="f">neer-dalen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Als Nola was <rhyme label="d" type="m">beset</rhyme></l>
        <l id="s5:l7" met="3" type="-+">(Soo de Schrijvers <rhyme label="c" type="f" corresp="#s5:l5">vehalen</rhyme>)</l>
        <l id="s5:l8" met="3" type="-+">Al d'Heydens macht <rhyme label="d" type="m" corresp="#s5:l6">belet</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">Als Haerlem <rhyme label="e" type="f">uytermaten</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Beplant was met <rhyme label="e" type="f" corresp="#s5:l9">Soldaten</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Soo dra Sint Baef <rhyme label="f" type="f">gekomen</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Is met 't swaert in de <rhyme label="g" type="m">lucht</rhyme>;</l>
        <l id="s5:l13" met="3" type="-+">Sy vreesen al en <rhyme label="f" type="f" corresp="#s5:l11">schromen</rhyme>,</l>
        <l id="s5:l14" met="3" type="-+">Sy vliegen op de <rhyme label="g" type="m" corresp="#s5:l12">vlucht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O heyligh <rhyme label="a" type="f">uytverkoren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Patroon, en heyligh <rhyme label="b" type="m">vat</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Krijght dat wy uwe <rhyme label="a" type="f" corresp="#s6:l1">sporen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Volgen in 's Hemels <rhyme label="b" type="m" corresp="#s6:l2">padt</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Voor ons Als <rhyme label="c" type="f">Campioenen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Tegens den vyandt <rhyme label="d" type="m">strijdt</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Goods gramschap wilt <rhyme label="c" type="f" corresp="#s6:l5">versoenen</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Toont dat ghy voor ons <rhyme label="d" type="m" corresp="#s6:l6">zijt</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Beschermer, en <rhyme label="e" type="f">behoeder</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Een schilt, Vader en <rhyme label="e" type="f" corresp="#s6:l9">Moeder</rhyme>.</l>
        <l id="s6:l11" met="3" type="+-">Bemint ons als sijn <rhyme label="f" type="f">jongen</rhyme></l>
        <l id="s6:l12" met="3" type="-+">De Hen, of <rhyme label="g" type="m">Pellicaen</rhyme>.</l>
        <l id="s6:l13" met="3" type="-+">Maeckt dat met blijde <rhyme label="f" type="f" corresp="#s6:l11">tongen</rhyme></l>
        <l id="s6:l14" met="3" type="-+">Wy 's Hemels poort in <rhyme label="g" type="m" corresp="#s6:l12">gaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
