    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach Phyllis lief mijn hartedief</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+"><rhyme label="a" type="m">Zinstreelstertje</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+"><rhyme label="a" type="m" corresp="#s1:l1">Lustteelstertje</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Vol toverende <rhyme label="b" type="f">lonken</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Weêrhouw uw <rhyme label="c" type="m">gloed</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Wil myn <rhyme label="c" type="m" corresp="#s1:l4">gemoed</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Niet meer in min <rhyme label="b" type="f" corresp="#s1:l3">ontvonken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Min my niet <rhyme label="a" type="m">meer</rhyme>;</l>
        <l id="s2:l2" met="2" type="-+">Maar wil veel <rhyme label="a" type="m" corresp="#s2:l1">eêr</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Aan my uw afkeer <rhyme label="b" type="f">tonen</rhyme>;</l>
        <l id="s2:l4" met="2" type="-+">Omdat myn <rhyme label="c" type="m">hart</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">ô Wrede <rhyme label="c" type="m" corresp="#s2:l4">smart</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Uw liefde niet mag <rhyme label="b" type="f" corresp="#s2:l3">kronen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Het Noodlot <rhyme label="a" type="m">eist</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Dat gy <rhyme label="a" type="m" corresp="#s3:l1">verreist</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En ik u moet <rhyme label="b" type="f">verlaten</rhyme>;</l>
        <l id="s3:l4" met="2" type="-+">Doch waar gy <rhyme label="c" type="m">gaat</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Maak hier op <rhyme label="c" type="m" corresp="#s3:l4">staat</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat ik u nooit zal <rhyme label="b" type="f" corresp="#s3:l3">haten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Ik bid dat <rhyme label="a" type="m">gy</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Vaak denkt om <rhyme label="a" type="m" corresp="#s4:l1">my</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'k Zal dikwyls om u <rhyme label="b" type="f">denken</rhyme>;</l>
        <l id="s4:l4" met="2" type="-+">Dit is, <rhyme label="c" type="m">vrindin</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">All' 't geen de <rhyme label="c" type="m" corresp="#s4:l4">Min</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Aan ons tot troost wil <rhyme label="b" type="f" corresp="#s4:l3">schenken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Leef, vry van <rhyme label="a" type="m">druk</rhyme>;</l>
        <l id="s5:l2" met="2" type="-+">Woon by 't <rhyme label="a" type="m" corresp="#s5:l1">Geluk</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Vernoegen wy ons <rhyme label="b" type="f">beiden</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Dat ik u <rhyme label="c" type="m">zal</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Hier uit dit <rhyme label="c" type="m" corresp="#s5:l4">dal</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">In grootscher plaats <rhyme label="b" type="f" corresp="#s5:l3">geleiden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
