    <tags/>
    <melody>Sei tanto graziosa</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh Lief! ick moet u <rhyme label="a" type="f">klagen</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Mijn smert die ick om uwen 't wil moet <rhyme label="b" type="f">lijden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Door veel bedroefde <rhyme label="a" type="f" corresp="#s1:l1">vlagen</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Die my aen alle kant het hert <rhyme label="b" type="f" corresp="#s1:l2">door-snijden</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Ick sucht en <rhyme label="c" type="m">smeeck</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Ick vley en <rhyme label="c" type="m" corresp="#s1:l5">spreeck</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Soo lieflijck en soo <rhyme label="d" type="f">aerdigh</rhyme>:</l>
        <l id="s1:l8" met="2" type="-+">Maer ghy <rhyme label="e" type="f">Godinne</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Acht mijn getrouwe <rhyme label="e" type="f" corresp="#s1:l8">minne</rhyme></l>
        <l id="s1:l10" met="2" type="+-">Gants <rhyme label="d" type="f" corresp="#s1:l7">onwaerdigh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick sucht veel bitt're <rhyme label="a" type="f">suchten</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Maer ach, ghy wilt het niet van my <rhyme label="b" type="f">bemercken</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Tot u doen ickse <rhyme label="a" type="f" corresp="#s2:l1">vluchten</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Ghy siet het wel aen mijn verwonnen <rhyme label="b" type="f" corresp="#s2:l2">wercken</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Nochtans ghy <rhyme label="c" type="m">zijt</rhyme></l>
        <l id="s2:l6" met="2" type="-+">En blijft <rhyme label="c" type="m" corresp="#s2:l5">altijt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Seer stuers en straf van <rhyme label="d" type="f">sinnen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Ick kan 't niet <rhyme label="e" type="f">mijen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Al proef ick seer veel <rhyme label="e" type="f" corresp="#s2:l8">lijen</rhyme></l>
        <l id="s2:l10" met="2" type="+-">In het <rhyme label="d" type="f" corresp="#s2:l7">minnen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">In eerbaerheyt te <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Dat is Godin mijn lust en hooghste <rhyme label="b" type="f">vreughde</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ghy kunt haest <rhyme label="a" type="f" corresp="#s3:l1">overwinnen</rhyme></l>
        <l id="s3:l4" met="5" type="-+">U tweede Ziel, door u roemwaerde <rhyme label="b" type="f" corresp="#s3:l2">deughden</rhyme>.</l>
        <l id="s3:l5" met="2" type="-+">O Lief <rhyme label="c" type="m">eerbaer</rhyme>!</l>
        <l id="s3:l6" met="2" type="-+">Siet u <rhyme label="c" type="m" corresp="#s3:l5">Dienaer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met knien <rhyme label="d" type="f">neer-gebogen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Wiens <rhyme label="e" type="f">minne-tranen</rhyme></l>
        <l id="s3:l9" met="3" type="-+">U soecken te <rhyme label="e" type="f" corresp="#s3:l8">vermanen</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">Tot <rhyme label="d" type="f" corresp="#s3:l7">medoogen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Princesse wilt <rhyme label="a" type="f">gedencken</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Wat Liefde doet en wat sy kan <rhyme label="b" type="f">uytwercken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wilt my een bloosje <rhyme label="a" type="f" corresp="#s4:l1">schencken</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Waer aen ick Lief u gunste kan <rhyme label="b" type="f" corresp="#s4:l2">bemercken</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Op dat mijn <rhyme label="c" type="m">hert</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Van al sijn <rhyme label="c" type="m" corresp="#s4:l5">smert</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Gerustigh mach <rhyme label="d" type="f">bedaren</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">En wy met <rhyme label="e" type="f">vreughden</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Door Liefd, in ware <rhyme label="e" type="f" corresp="#s4:l8">deughden</rhyme>,</l>
        <l id="s4:l10" met="2" type="+-">Mogen <rhyme label="d" type="f" corresp="#s4:l7">paren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
