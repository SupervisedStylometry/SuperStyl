    <melody>Psalm 100 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ALl’ die hier levens adem <rhyme label="a" type="m">schept</rhyme></l>
        <l id="s1:l2" met="4" type="-+">En op der aerden wooning <rhyme label="a" type="m" corresp="#s1:l1">hebt</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Doet allent-halven <rhyme label="b" type="m">vreuchd-bewijs</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Den Heere, onsen Godt, ten <rhyme label="b" type="m" corresp="#s1:l3">prijs</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Treedt in, en laet <rhyme label="a" type="m">triumph-geneucht</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">uyt overvloedt van <rhyme label="a" type="m" corresp="#s2:l1">herten-vreuchdt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Voor sijns groot-waerdicheyts <rhyme label="b" type="m">aenschijn</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Vw’ Godsdienst een geselschap <rhyme label="b" type="m" corresp="#s2:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dit zy u een onwanckle <rhyme label="a" type="m">leer</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Een wisse waerheydt: Dat de <rhyme label="a" type="m" corresp="#s3:l1">Heer</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Ontwijsselijcke Godheydt <rhyme label="b" type="m">heeft</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En selver allen alles <rhyme label="b" type="m" corresp="#s3:l3">geeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">All’ eygen eere zy <rhyme label="a" type="m">versaeckt</rhyme>.</l>
        <l id="s4:l2" met="4" type="-+">Niet wy, maer hy heeft ons <rhyme label="a" type="m" corresp="#s4:l1">gemaeckt</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Hy, onsen herder en <rhyme label="b" type="m">geley</rhyme>;</l>
        <l id="s4:l4" met="4" type="-+">Wy, ’tvee en schapen sijner <rhyme label="b" type="m" corresp="#s4:l3">wey</rhyme>’.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Treedt tot sijn heyl’ge poorten <rhyme label="a" type="m">in</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Met eerdanck-volle ziel en <rhyme label="a" type="m" corresp="#s5:l1">sin</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Brengt lippen rijck van <rhyme label="b" type="m">eeren-tael</rhyme></l>
        <l id="s5:l4" met="4" type="-+">In ’sTempels eerst’ en tweede <rhyme label="b" type="m" corresp="#s5:l3">zael</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doet sijnes Godtheyts grootheydt <rhyme label="a" type="m">kondt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Sijn naem heb zegen in u <rhyme label="a" type="m" corresp="#s6:l1">mondt</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Sijn plicht verricht hy die dit <rhyme label="b" type="m">doet</rhyme>:</l>
        <l id="s6:l4" met="4" type="-+">Want hy (de Heer) is <rhyme label="b" type="m" corresp="#s6:l3">over-goedt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">’tGeduren sijner <rhyme label="a" type="m">goedichhe’en</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Streckt sich door alle eeuwen <rhyme label="a" type="m" corresp="#s7:l1">heen</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Sijn waerheydt staet en blijft in <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Van ’teen tot ’tandere <rhyme label="b" type="m" corresp="#s7:l3">geslacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
