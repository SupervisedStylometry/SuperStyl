      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>C'est trop courir les eaux</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">God'lijck klaer-stralend' <rhyme label="a" type="m">Licht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ach Blixems uyt 't <rhyme label="a" type="m" corresp="#s1:l1">ghesicht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Van Sylvia, wiens <rhyme label="b" type="m">glans</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Verdooft de gloor en <rhyme label="c" type="m">strael</rhyme></l>
        <l id="s1:l5" met="3" type="-+">En luystert <rhyme label="c" type="m" corresp="#s1:l4">t'eenemael</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van Pheby gulde <rhyme label="b" type="m" corresp="#s1:l3">Krans</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ha hem, wiens soet <rhyme label="a" type="m">gheklanghm</rhyme></l>
        <l id="s2:l2" met="3" type="-+">'t Zy in gespreeck of <rhyme label="a" type="m" corresp="#s2:l1">sangh</rhyme></l>
        <l id="s2:l3" met="3" type="-+">De Muse <rhyme label="b" type="m">overtreft</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Een Pierus docht'ren <rhyme label="c" type="m">doet</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wijcken, die met <rhyme label="c" type="m" corresp="#s2:l4">hooghmoet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Haer sangh hadden <rhyme label="b" type="m" corresp="#s2:l3">verheft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Violet, Roos, <rhyme label="a" type="m">Ang'lier</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">V reuck is niet een <rhyme label="a" type="m" corresp="#s3:l1">sier</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Te achten by de <rhyme label="b" type="m">geur</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En aessems soete <rhyme label="c" type="m">douw</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van mijn Zielwaerde <rhyme label="c" type="m" corresp="#s3:l4">vrouw</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Maer moet wijcken daer <rhyme label="b" type="m" corresp="#s3:l3">veur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wech Nectar en <rhyme label="a" type="m">Ambroos</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Want als ick maer 't <rhyme label="a" type="m" corresp="#s4:l1">gebloos</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Eens druck van 't sach <rhyme label="b" type="m">Robijn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Der lippen <rhyme label="c" type="m">oversoet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Mijns Nymphs, die Nectar <rhyme label="c" type="m" corresp="#s4:l4">goet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Kan meer vermaecken <rhyme label="b" type="m" corresp="#s4:l3">mijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Als ick gevoel het <rhyme label="a" type="m">teer</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En poesel lijf: ter <rhyme label="a" type="m" corresp="#s5:l1">neer</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Mijn vryheyt henen <rhyme label="b" type="m">valt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En door 't gevoel soo <rhyme label="c" type="m">wert</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ontsteecken mijn jongh <rhyme label="c" type="m" corresp="#s5:l4">hert</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Door u, diens roem dat <rhyme label="b" type="m" corresp="#s5:l3">bralt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
