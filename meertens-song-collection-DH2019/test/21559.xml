        <lon>53.3670442</lon>
        <lat>7.2058004</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="vermaanlied" translation="admonitory song">vermaanlied</tag>
    </tags>
    <melody>Och lig dij nu en slaapt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VUaer lighdy nv en <rhyme label="a" type="m">slaept</rhyme></l>
        <l id="s1:l2" met="3" type="-+">So vast in schijn van <rhyme label="b" type="f">droomen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Eer v die doot <rhyme label="a" type="m" corresp="#s1:l1">betraept</rhyme></l>
        <l id="s1:l4" met="3" type="-+">So vliet der sonden <rhyme label="b" type="f" corresp="#s1:l2">toomen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Uerlicht ws leuens <rhyme label="c" type="m">staet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Uernieut in gheest en <rhyme label="d" type="f">leuen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Coemt tot den <rhyme label="c" type="m" corresp="#s1:l5">Aduocaet</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Die ons God heeft <rhyme label="d" type="f" corresp="#s1:l6">ghegheuen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy is selue <rhyme label="a" type="m">voorwaer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die versoeninghe onser <rhyme label="b" type="f">sonden</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Den hoecksteen of <rhyme label="a" type="m" corresp="#s2:l1">pilaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die vast blijft <rhyme label="b" type="f" corresp="#s2:l2">onuerslonden</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En voor hem en staet gheen <rhyme label="c" type="m">macht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Tghewelt moet voor hem <rhyme label="d" type="f">wijcken</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Hy verwinnet door zijn <rhyme label="c" type="m" corresp="#s2:l5">cracht</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Des duyuels boos <rhyme label="d" type="f" corresp="#s2:l6">practijcken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy sal v wel <rhyme label="a" type="m">ontfaen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Wildy al tot hem <rhyme label="b" type="f">comen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Uan allen last <rhyme label="a" type="m" corresp="#s3:l1">ontlaen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hy heeft op hem <rhyme label="b" type="f" corresp="#s3:l2">ghenomen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Onser schulden groot <rhyme label="c" type="m">ghetal</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Naer Esaias <rhyme label="d" type="f">schrijuen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hy heeft ons groot en <rhyme label="c" type="m" corresp="#s3:l5">smal</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Ghemaect met siele en <rhyme label="d" type="f" corresp="#s3:l6">lijuen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die nv den mensche <rhyme label="a" type="m">maect</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sal hem oock voetsel <rhyme label="b" type="f">gheuen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dus alle sorghe <rhyme label="a" type="m" corresp="#s4:l1">slaect</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En zijt doch eerst <rhyme label="b" type="f" corresp="#s4:l2">ghedreuen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Na Gods <rhyme label="c" type="m">gherechticheyt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sijn rijcke soect tallen <rhyme label="d" type="f">stonden</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wat v dient ter <rhyme label="c" type="m" corresp="#s4:l5">behoefticheyt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Sal v toe zijn <rhyme label="d" type="f" corresp="#s4:l6">ghesonden</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Bidt God om broot, hy <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s5:l2" met="3" type="-+">U dat broot vrywillich <rhyme label="b" type="f">gheuen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dat ons hier in dit <rhyme label="a" type="m" corresp="#s5:l1">dal</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Gheeft dat eewich <rhyme label="b" type="f" corresp="#s5:l2">leuen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Dat en is dat Manna <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Datse inder woestynen <rhyme label="d" type="f">aten</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Want sy storuen oock <rhyme label="c" type="m" corresp="#s5:l5">siet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En moesten de doot <rhyme label="d" type="f" corresp="#s5:l6">smaken</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Want die dit broot wel <rhyme label="a" type="m">smaect</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat oock Christus sal <rhyme label="b" type="f">gheuen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Is vander doot <rhyme label="a" type="m" corresp="#s6:l1">ontwaect</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Want hy sal eewich <rhyme label="b" type="f" corresp="#s6:l2">leuen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Bidt God om v <rhyme label="c" type="m">behoet</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die sal de siele <rhyme label="d" type="f">spijsen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Al met zijn woorden <rhyme label="c" type="m" corresp="#s6:l5">soet</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Daer in wilt <rhyme label="d" type="f" corresp="#s6:l6">veriolijsen</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Der valscher leeraers <rhyme label="a" type="m">broot</rhyme></l>
        <l id="s7:l2" met="3" type="-+">En mach ons niet <rhyme label="b" type="f">versaden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Want tis wt door boosheyt <rhyme label="a" type="m" corresp="#s7:l1">groot</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Als caf vol <rhyme label="b" type="f" corresp="#s7:l2">onghenaden</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Dat woort Gods is <rhyme label="c" type="m">voorwaer</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Een vier dat sal <rhyme label="d" type="f">bederuen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat caf der leeringhen <rhyme label="c" type="m" corresp="#s7:l5">swaer</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Daer die siele door moet <rhyme label="d" type="f" corresp="#s7:l6">steruen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Want hy en wilt die <rhyme label="a" type="m">doot</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Des sondaers niet, noch <rhyme label="b" type="f">tsneuen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Maer dat hy bekeere <rhyme label="a" type="m" corresp="#s8:l1">bloot</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Tot God, en hael daer <rhyme label="b" type="f" corresp="#s8:l2">tleuen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Want hy is neder <rhyme label="c" type="m">ghedaelt</rhyme></l>
        <l id="s8:l6" met="3" type="-+">En ghebacken tonser <rhyme label="d" type="f">vramen</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Met tvier der liefden <rhyme label="c" type="m" corresp="#s8:l5">doorstraelt</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Aent cruyce voor ons al <rhyme label="d" type="f" corresp="#s8:l6">tsamen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O Prince Goddelick <rhyme label="a" type="m">keest</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Wilt ons wt liefde <rhyme label="b" type="f">schencken</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Uwen heylighen <rhyme label="a" type="m" corresp="#s9:l1">gheest</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Wilt ons vianden <rhyme label="b" type="f" corresp="#s9:l2">crencken</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Dat niemant ons en <rhyme label="c" type="m">mach</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Letten in gheender <rhyme label="d" type="f">zijden</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Want v hoocheyt <rhyme label="c" type="m" corresp="#s9:l5">vermach</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Ons te <rhyme label="d" type="f" corresp="#s9:l6">ghebenedijden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
