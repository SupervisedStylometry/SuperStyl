    <melody>Zoet meisje aardige prij</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Zoete Meisjes lekkere <rhyme label="a" type="m">Wyn</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dat is mijn leven en mijn <rhyme label="a" type="m" corresp="#s1:l1">Medicyn</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Want Wyn baart vreugd en zoet <rhyme label="b" type="m">geneugt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Versterkt het minziek <rhyme label="b" type="m" corresp="#s1:l3">Hert</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Dat door de vreugt gekluystert <rhyme label="c" type="m">wert</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Versterkt het minne zieke <rhyme label="c" type="m" corresp="#s1:l5">Hert</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">O zoete tongen gy Meisjes <rhyme label="d" type="m">lief</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Doet mijn verheuge ô gy Harte <rhyme label="d" type="m" corresp="#s1:l7">dief</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Welk een vreugde is ‘t minne <rhyme label="a" type="m">zoet</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Dat ons verheugde ja door Wyn <rhyme label="a" type="m" corresp="#s2:l1">gevoet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want mysies zoet Wyn doet ons <rhyme label="b" type="m">goed</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">De Wyn ons kragte geven <rhyme label="b" type="m" corresp="#s2:l3">moet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Waar door de vreugt en <rhyme label="c" type="m">min</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Dat is ons Herte lust en ziel en <rhyme label="c" type="m" corresp="#s2:l5">zin</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Lieve Meisjes lekkere <rhyme label="d" type="m">Wyn</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Dat is myn Leeven en mijn <rhyme label="d" type="m" corresp="#s2:l7">Medicyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ionkheyt te zamen besteed uw <rhyme label="a" type="m">tyd</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Uw Ieugt bekwaem tot die vreugt <rhyme label="a" type="m" corresp="#s3:l1">bereit</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Want Wyn en min Iaagt vreugde <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Geen kwelling voel ik in mijn <rhyme label="b" type="m" corresp="#s3:l3">zin</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Maar ons Hart van vuur <rhyme label="c" type="m">krioelt</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Dat doet de Meisjes maken <rhyme label="c" type="m" corresp="#s3:l5">goet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Want als den ouden dag komt <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Dan is ‘t Minne en die vreugt <rhyme label="d" type="m" corresp="#s3:l7">gedaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
