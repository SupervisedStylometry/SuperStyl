    <tags/>
    <melody>Psalm 148 Datheen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">LOoft den Heere uit 's hemels <rhyme label="a" type="m">boogen</rhyme>:</l>
        <l id="s1:l2" met="4" type="-+">Looft den Heerscher in den <rhyme label="a" type="m" corresp="#s1:l1">hoogen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Laat den lof des Heeren <rhyme label="b" type="m">hooren</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Looft hem gy zyn <rhyme label="b" type="m" corresp="#s1:l3">Englenkooren</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Zyn lof klink' by de <rhyme label="c" type="f">heireschaaren</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Dat Zon en Maan zyn' lof <rhyme label="c" type="f" corresp="#s1:l5">verklaaren</rhyme>.</l>
        <l id="s1:l7" met="4" type="-+">Looft hem al gy <rhyme label="d" type="m">starrelichten</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Flonkerende <rhyme label="d" type="m" corresp="#s1:l7">nachtgezichten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Looft den Heer gy <rhyme label="a" type="m">hemeltranssen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Zuiverder dan <rhyme label="a" type="m" corresp="#s2:l1">spiegelglanssen</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Looft den Heer gy <rhyme label="b" type="m">opperwatren</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Daar de donderen uit <rhyme label="b" type="m" corresp="#s2:l3">klatren</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Dat Godt toch door hen zy <rhyme label="c" type="f">geprezen</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Als oirsprong van hun kracht en <rhyme label="c" type="f" corresp="#s2:l5">wezen</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Daar zy eeuwig in <rhyme label="d" type="m">beklyven</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">En altoos bevestigt <rhyme label="d" type="m" corresp="#s2:l7">blyven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Hy gafze ook een' loop en <rhyme label="a" type="m">orden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Die niet zal verbroken <rhyme label="a" type="m" corresp="#s3:l1">worden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">En die zy niet <rhyme label="b" type="m">overtreeden</rhyme>.</l>
        <l id="s3:l4" met="4" type="-+">Looft den Heer, van hier <rhyme label="b" type="m" corresp="#s3:l3">beneden</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Gy wallevisschen die <rhyme label="c" type="f">gevonden</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Wordt in de zee: gy diepste <rhyme label="c" type="f" corresp="#s3:l5">gronden</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">Vier en hagel, sneeuw en <rhyme label="d" type="m">dampen</rhyme>;</l>
        <l id="s3:l8" met="4" type="-+">Stormwindt, wekker van veel <rhyme label="d" type="m" corresp="#s3:l7">rampen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Looft den Heer gy hooge <rhyme label="a" type="m">kruinen</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Van de bergen, heuvlen, <rhyme label="a" type="m" corresp="#s4:l1">duinen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Vruchtgeboomte en <rhyme label="b" type="m">cederboomen</rhyme>.</l>
        <l id="s4:l4" met="4" type="-+">'t Wildt, en 't vee, dat aan de <rhyme label="b" type="m" corresp="#s4:l3">stroomen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Het gras afscheert; gekorve <rhyme label="c" type="f">dieren</rhyme>;</l>
        <l id="s4:l6" met="4" type="-+">De vogels, die de lucht <rhyme label="c" type="f" corresp="#s4:l5">doorzwieren</rhyme>.</l>
        <l id="s4:l7" met="4" type="-+">Gy ook Koningen der <rhyme label="d" type="m">aarde</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Vorsten, Rechters, hoog in <rhyme label="d" type="m" corresp="#s4:l7">waarde</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maagden, en ook <rhyme label="a" type="m">jongelingen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Ouden, jongen, wilt hem <rhyme label="a" type="m" corresp="#s5:l1">zingen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Datze 's Heeren naame <rhyme label="b" type="m">looven</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">In zyn ruime <rhyme label="b" type="m" corresp="#s5:l3">tempelhoven</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Dewyl zyn naam is <rhyme label="c" type="f">hoogverheven</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Erkent als Godt van heil en <rhyme label="c" type="f" corresp="#s5:l5">leven</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Zyne Majesteit en <rhyme label="d" type="m">straalen</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Schittren uit zyn <rhyme label="d" type="m" corresp="#s5:l7">hemelzaalen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dus komt hy zyn' glans <rhyme label="a" type="m">verspreiden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">En zyn heerlykheit <rhyme label="a" type="m" corresp="#s6:l1">verbreiden</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Over d'aarde aan alle <rhyme label="b" type="m">kanten</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Daar hy komt zyn waarheit <rhyme label="b" type="m" corresp="#s6:l3">planten</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Men ziet den hoorn zyns volks <rhyme label="c" type="f">verhoogen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">En zynen roem tot 's hemels <rhyme label="c" type="f" corresp="#s6:l5">boogen</rhyme>;</l>
        <l id="s6:l7" met="4" type="-+">Van zyn ware <rhyme label="d" type="m">gunstgenooten</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">In zyn heilverbondt <rhyme label="d" type="m" corresp="#s6:l7">gesloten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
