    <melody>Waar is de dochter van Sion</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AVe Maria reyn Maghet <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Ootmoedich en <rhyme label="b" type="f">simpel</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Bracht ghy uwen lieuen <rhyme label="a" type="m" corresp="#s1:l1">Soon</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Als nv inden <rhyme label="b" type="f" corresp="#s1:l2">Tempel</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die daghen der <rhyme label="a" type="m">suyuering</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Hebt ghy <rhyme label="b" type="f">onderhouwen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Ghehoorsaemheyt <rhyme label="a" type="m" corresp="#s2:l1">sonderling</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Leert ghy alle <rhyme label="b" type="f" corresp="#s2:l2">Vrouwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Gheheel suyuer <rhyme label="a" type="m">onbeuleckt</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Sijt ghy Maecht <rhyme label="b" type="f">ghebleuen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Nochtans volbracht ghy <rhyme label="a" type="m" corresp="#s3:l1">perfeckt</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Die wet Godts <rhyme label="b" type="f" corresp="#s3:l2">verheuen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Simeon had seer <rhyme label="a" type="m">verlanckt</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Na v kint met <rhyme label="b" type="f">kermen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dat hy als heden <rhyme label="a" type="m" corresp="#s4:l1">ontfanckt</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Met vreucht in zijn <rhyme label="b" type="f" corresp="#s4:l2">armen</rhyme>-</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Met herten gheheel <rhyme label="a" type="m">verblijt</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Heeft Symeon <rhyme label="b" type="f">schoone</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">V Moeder <rhyme label="a" type="m" corresp="#s5:l1">ghebenedijt</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Met v lieue <rhyme label="b" type="f" corresp="#s5:l2">Sone</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Tot v heeft hy oock <rhyme label="a" type="m">gheseyt</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Niet tot v <rhyme label="b" type="f">verblijden</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat het sweert van <rhyme label="a" type="m" corresp="#s6:l1">droeuicheyt</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">V siel sou <rhyme label="b" type="f" corresp="#s6:l2">doorsnijden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ghy offerden uwen <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Met wijsen <rhyme label="b" type="f">verstande</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Godt den Vader inden <rhyme label="a" type="m" corresp="#s7:l1">troon</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Tot een <rhyme label="b" type="f" corresp="#s7:l2">offerande</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Met twee Tortelduyfkens <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Hebt ghy Maghet <rhyme label="b" type="f">teere</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Weer ghelost v Sone <rhyme label="a" type="m" corresp="#s8:l1">goet</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Jesum onsen <rhyme label="b" type="f" corresp="#s8:l2">Heere</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Alle dinck nae Moyses <rhyme label="a" type="m">wet</rhyme>,,</l>
        <l id="s9:l2" met="3" type="+-">Volbracht ghy <rhyme label="b" type="f">bequame</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Na v stadt van <rhyme label="a" type="m" corresp="#s9:l1">Nazaret</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Keerden ghy <rhyme label="b" type="f" corresp="#s9:l2">eersame</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Weerdighe princersse <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Moeder Godts <rhyme label="b" type="f">idoone</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Bidt voor ons alle <rhyme label="a" type="m" corresp="#s10:l1">ghemeyn</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Jesum uwen <rhyme label="b" type="f" corresp="#s10:l2">Sone</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Die ghy hebt <rhyme label="a" type="m">ghepresenteert</rhyme>,</l>
        <l id="s11:l2" met="3" type="+-">In des Tempels <rhyme label="b" type="f">Steden</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Den Vader <rhyme label="a" type="m" corresp="#s11:l1">gheoffereert</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Op den dach van <rhyme label="b" type="f" corresp="#s11:l2">heden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
