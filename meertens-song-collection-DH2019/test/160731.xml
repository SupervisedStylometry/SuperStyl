<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160731</id>
    <date>
      <year-start>1770</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Kwel mij niet meer mijn gedachten</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Kweld my niet meer myn <rhyme label="a" type="f">gedogten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Wyl het tyd van Slaepen <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Laet u zorg myn maer <rhyme label="a" type="f" corresp="#s1:l1">wagten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Wyl myn hert vermorsseld <rhyme label="b" type="m" corresp="#s1:l2">is</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Door het heen en <rhyme label="c" type="f">wederdenken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Het kan onmogelyk anders <rhyme label="d" type="m">zyn</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Sal myn niemand ruste <rhyme label="c" type="f" corresp="#s1:l5">schenken</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Soo moet ik vergaen van <rhyme label="d" type="m" corresp="#s1:l6">pyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Alle vreugd heb ik <rhyme label="a" type="f">verlooren</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ag geen lust vind ik by <rhyme label="b" type="m">myn</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Waer toe ben ik dan <rhyme label="a" type="f" corresp="#s2:l1">gebooren</rhyme>?</l>
        <l id="s2:l4" met="4" type="+-">Hemel staet myn dag maer <rhyme label="b" type="m" corresp="#s2:l2">by</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Doet myn anders liever <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En verlost myn van de <rhyme label="d" type="m">pyn</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Ik wil veel liever alles <rhyme label="c" type="f" corresp="#s2:l5">derven</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Dan altoos gekweld te <rhyme label="d" type="m" corresp="#s2:l6">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ag ik zie den Dag <rhyme label="a" type="f">aenbreeken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En het Morgen Rood <rhyme label="b" type="m">verschynt</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Door myn Bidden en myn <rhyme label="a" type="f" corresp="#s3:l1">Smeeken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Voor die hier ook leid in <rhyme label="b" type="m" corresp="#s3:l2">pyn</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Komt de Nagt zoo komt myn <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Komt den Dag zo is <rhyme label="d" type="m">gewis</rhyme>:</l>
        <l id="s3:l7" met="4" type="+-">Geen van beyde kan ik <rhyme label="c" type="f" corresp="#s3:l5">myden</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Het geduld het beste <rhyme label="d" type="m" corresp="#s3:l6">is</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Moet ik dan van droefheid <rhyme label="a" type="f">smooren</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Siet geen Sterveling myn <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Sal my imand kunnen <rhyme label="a" type="f" corresp="#s4:l1">hooren</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Groote Goon zo is ‘t <rhyme label="b" type="m" corresp="#s4:l2">gedaen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Doet de schaemten myn nog <rhyme label="c" type="f">zwygen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Ia gewis hoe klopt myn <rhyme label="d" type="m">hert</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Waer zal nu ‘t Eynde <rhyme label="c" type="f" corresp="#s4:l5">blyven</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Van myn rampen en myn <rhyme label="d" type="m" corresp="#s4:l6">smert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">‘t Graf alleen kan myn <rhyme label="a" type="f">bevryden</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Maer wie schrikt niet voor de <rhyme label="b" type="m">dood</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Een die hier gestaeg moet <rhyme label="a" type="f" corresp="#s5:l1">lyden</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">En van alles zyn <rhyme label="b" type="m" corresp="#s5:l2">ontblood</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Weg dan wereld, weg dan <rhyme label="c" type="f">Sorgen</rhyme>:</l>
        <l id="s5:l6" met="4" type="+-">Gy baert veel <rhyme label="d" type="m">bekommernis</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Ik zie al weer den dag van <rhyme label="c" type="f" corresp="#s5:l5">morgen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">het geduld het beste <rhyme label="d" type="m" corresp="#s5:l6">is</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
