    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Bruidegom met u is de strijd begonnen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="+-">O Maria, noyt genoech <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s1:l2" met="5" type="+-">Vrolijck moet gy met u kindt <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme>:</l>
        <l id="s1:l3" met="5" type="+-">Vrolijck zijt, o Maget, vol <rhyme label="b" type="f">genaden</rhyme>,</l>
        <l id="s1:l4" met="5" type="+-">Godts Moeder, soeter dan <rhyme label="b" type="f" corresp="#s1:l3">sukaden</rhyme>:</l>
        <l id="s1:l5" met="3" type="+-">Klaer zy // En vruchtbaer <rhyme label="c" type="m">gy</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">'t Wonder van den maeghden <rhyme label="d" type="m">staet</rhyme>:</l>
        <l id="s1:l7" met="4" type="+-">Maeght gy baerde // En <rhyme label="e" type="f">bewaerde</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Al uw suyverheyts <rhyme label="d" type="m" corresp="#s1:l6">cieraet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="+-">Uyt u heeft getrocken ons <rhyme label="a" type="f">natuere</rhyme>,</l>
        <l id="s2:l2" met="5" type="+-">'tWoort des Vaders, schijnsel en <rhyme label="a" type="f" corresp="#s2:l1">figuere</rhyme></l>
        <l id="s2:l3" met="5" type="+-">Door wiens doot ons is gekomen 't <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s2:l4" met="5" type="+-">Door wiens leven nu de dood moet <rhyme label="b" type="f" corresp="#s2:l3">sne</rhyme>-</l>
        <l id="s2:l5" met="3" type="+-">ven // Chooren // Soet om <rhyme label="c" type="m">hooren</rhyme>;</l>
        <l id="s2:l6" met="4" type="+-">Die de Engelen <rhyme label="d" type="m">verheught</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Die zijn vriende // Hem <rhyme label="e" type="f">siende</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Geeft hier boven volle <rhyme label="d" type="m" corresp="#s2:l6">vreught</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="+-">Gy dan, die hem hebt <rhyme label="a" type="f">ontfangen</rhyme>,</l>
        <l id="s3:l2" met="5" type="+-">En daerom blinckt met de son <rhyme label="a" type="f" corresp="#s3:l1">omhangen</rhyme></l>
        <l id="s3:l3" met="5" type="+-">Die de Maen hebt onder uw' <rhyme label="b" type="f">voeten</rhyme>,</l>
        <l id="s3:l4" met="5" type="+-">Die de sterren Kroonen, en <rhyme label="b" type="f" corresp="#s3:l3">begroeten</rhyme>:</l>
        <l id="s3:l5" met="3" type="+-">Maget // Bidt en <rhyme label="c" type="m">vraget</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Dien uwen lieven <rhyme label="d" type="m">Soon</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Dat na d'ende // der <rhyme label="e" type="f">ellende</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Hy toch zijn wilt onsen <rhyme label="d" type="m" corresp="#s3:l6">loon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
