    <melody>Psalm 143 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Heer wilt mijn ghebedt <rhyme label="a" type="f">verhooren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Laet comen toch tot uwen <rhyme label="a" type="f" corresp="#s1:l1">ooren</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Mijn truerich bidden, dat ick <rhyme label="b" type="m">doe</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Nae uwe trouwe sonder <rhyme label="a" type="f" corresp="#s1:l1">stooren</rhyme></l>
        <l id="s1:l5" met="4" type="-+">En v gherechtigheyt my <rhyme label="b" type="m" corresp="#s1:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Met uwen dienaer wilt niet <rhyme label="a" type="f">strijden</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Van v oordeel wilt hem <rhyme label="a" type="f" corresp="#s2:l1">beurijden</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Dat hi niet comme in grooter <rhyme label="b" type="m">pijn</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Want gheen Mensch geboren van <rhyme label="a" type="f" corresp="#s2:l1">wijue</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Al voor v can gherechtich <rhyme label="b" type="m" corresp="#s2:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Want mijn vyant heeft my <rhyme label="a" type="f">bestreden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Ter neder ghebracht heeft hi mijn <rhyme label="a" type="f" corresp="#s3:l1">leuen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Noch is hem aldat niet <rhyme label="b" type="m">ghenoech</rhyme></l>
        <l id="s3:l4" met="4" type="-+">In eenen duysteren pudt <rhyme label="a" type="f" corresp="#s3:l1">beneden</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Sluyt hi my, dwelck den doden <rhyme label="b" type="m" corresp="#s3:l3">voecht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vuaer door mijn siel lijdt grote <rhyme label="a" type="f">schaden</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Mijn hert met weemoet is <rhyme label="a" type="f" corresp="#s4:l1">beladen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">In dien Pudt heb ick <rhyme label="b" type="m">ouerdacht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Te mijnen trooste, alle de <rhyme label="a" type="f" corresp="#s4:l1">daden</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Vwer handen constich <rhyme label="b" type="m" corresp="#s4:l3">ghewracht</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Daerom ist dat ick claechlick <rhyme label="a" type="f">roepe</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Tot v mijn handen ick <rhyme label="a" type="f" corresp="#s5:l1">opheffe</rhyme></l>
        <l id="s5:l3" met="4" type="-+">En mijn siel, in haer roepen <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Nae v begeert sonder <rhyme label="a" type="f" corresp="#s5:l1">wanhopen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Als dorre Lant, na dWater <rhyme label="b" type="m" corresp="#s5:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Haest v wilt mi behulpigh <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Den Gheest bestrijt tmi nv <rhyme label="a" type="f" corresp="#s6:l1">mitsdesen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Verbercht v aenschijn van mi <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Anders so sal ick ghelijck <rhyme label="a" type="f" corresp="#s6:l1">wesen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Den gheven diemen int graf <rhyme label="b" type="m" corresp="#s6:l3">schiet</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Laet nv vroech hooren v <rhyme label="a" type="f">goethede</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Want op v mijn hoop ick <rhyme label="a" type="f" corresp="#s7:l1">bestede</rhyme></l>
        <l id="s7:l3" met="4" type="-+">En vanden wech die ick houden <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Wilt my rechte kennenschap <rhyme label="a" type="f" corresp="#s7:l1">gheuen</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Want ick tot v heffe hert en <rhyme label="b" type="m" corresp="#s7:l3">moet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Verlost my Heer door v <rhyme label="a" type="f">ghenade</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Myner vyanden, diemy <rhyme label="a" type="f" corresp="#s8:l1">schaden</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Want ick betrouw v gansch en <rhyme label="b" type="m">gaer</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Ghi zijt mijn hope vroech en <rhyme label="a" type="f" corresp="#s8:l1">spade</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Ghy troost in al mijn lijden <rhyme label="b" type="m" corresp="#s8:l3">swaer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Door uwe leere wilt my <rhyme label="a" type="f">dringhen</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Om uwen wille te <rhyme label="a" type="f" corresp="#s9:l1">volbringhen</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Want ghi alleen mijn recht Godt <rhyme label="b" type="m">sijt</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Maect doch dat uwen Gheest mi <rhyme label="a" type="f" corresp="#s9:l1">brenghe</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Inden rechten wech met <rhyme label="b" type="m" corresp="#s9:l3">iolijt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ontslaet my van dit wesen <rhyme label="a" type="f">Heere</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Om dijns weerden naems wille ende <rhyme label="a" type="f" corresp="#s10:l1">eere</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Verlost mijn heel wt last en <rhyme label="b" type="m">noot</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Door v gherechticheyt <rhyme label="a" type="f" corresp="#s10:l1">alleene</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Op dat ick v lof make <rhyme label="b" type="m" corresp="#s10:l3">groot</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Mijn vianden die my <rhyme label="a" type="f">bespringhen</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Door v goetheyt te niet wilt <rhyme label="a" type="f" corresp="#s11:l1">bringhen</rhyme></l>
        <l id="s11:l3" met="4" type="-+">En die stracx nae mijn leuen <rhyme label="b" type="m">staen</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Wiltse alle verdoen en <rhyme label="a" type="f" corresp="#s11:l1">verslinden</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Want ick ben v Knecht <rhyme label="b" type="m" corresp="#s11:l3">onderdaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
