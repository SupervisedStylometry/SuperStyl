<song>
  <meta>
    <id>14937</id>
    <date>
      <year-start>1622</year-start>
      <year-end>1622</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La Dauphine</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk weet een Dochter tjats en <rhyme label="a" type="m">broets</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En sy heeft een hoope <rhyme label="a" type="m" corresp="#s1:l1">goets</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Vry veule // vry <rhyme label="b" type="f">veule</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Maer ick sech het jou goet <rhyme label="a" type="m" corresp="#s1:l1">moets</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Heur zinnen loope <rhyme label="b" type="f" corresp="#s1:l3">speule</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Aengaende wat heur goet <rhyme label="a" type="m">belangt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Meen ick jou hier met myn <rhyme label="a" type="m" corresp="#s2:l1">hanght</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Te schrijven // te <rhyme label="b" type="f">schrijven</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Maer het is soo quaden <rhyme label="a" type="m" corresp="#s2:l1">panght</rhyme></l>
        <l id="s2:l5" met="3" type="-+">K’wilt liever laten <rhyme label="b" type="f" corresp="#s2:l3">blyven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">En laet het niet seyd’ myt <rhyme label="a" type="m">gedacht</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Neemt doch op haer leven <rhyme label="a" type="m" corresp="#s3:l1">acht</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">En weesen // en <rhyme label="b" type="f">weesen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Denckt hoe lieflyck datse <rhyme label="a" type="m" corresp="#s3:l1">laght</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Dat schier een Mensch mach <rhyme label="b" type="f" corresp="#s3:l3">vreesen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">S’heeft een Kop ghelijck een <rhyme label="a" type="m">Beer</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Met een prooper Neusje <rhyme label="a" type="m" corresp="#s4:l1">teer</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Vol puysten // vol <rhyme label="b" type="f">puysten</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En haer Ooghen leepen <rhyme label="a" type="m" corresp="#s4:l1">zeer</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Soo groot (omtrent) als <rhyme label="b" type="f" corresp="#s4:l3">vuysten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Haer Wanghen schoon as Indie <rhyme label="a" type="m">blaeuw</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Lippen kout en nimmer <rhyme label="a" type="m" corresp="#s5:l1">laeuw</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Vol cleuren // vol <rhyme label="b" type="f">cleuren</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Kin en Keel, blont grijs en <rhyme label="a" type="m" corresp="#s5:l1">graeuw</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Wat dunckt u synt oock <rhyme label="b" type="f" corresp="#s5:l3">leuren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Noch hetse propre hanghden <rhyme label="a" type="m">cleen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Ick mien ghelijck een Cake <rhyme label="a" type="m" corresp="#s6:l1">Been</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">En  voeten // en <rhyme label="b" type="f">voeten</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Want sy moet noch in het <rhyme label="a" type="m" corresp="#s6:l1">Veen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">De Turve trappen <rhyme label="b" type="f" corresp="#s6:l3">moeten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Somma t’Lijf is net te <rhyme label="a" type="m">pas</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Oft uyt een Boom ehouwen <rhyme label="a" type="m" corresp="#s7:l1">was</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Vol quasten // vol <rhyme label="b" type="f">quasten</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Al laach ghy t’saem in seer langh <rhyme label="a" type="m" corresp="#s7:l1">Gras</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Ic sout niet eens mis <rhyme label="b" type="f" corresp="#s7:l3">tasten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Maer k’heb gheen sin in sulcken <rhyme label="a" type="m">stick</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Se ismen al te plom, te <rhyme label="a" type="m" corresp="#s8:l1">dick</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">En laelijck // en <rhyme label="b" type="f">laelijck</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">En daer by //is sy, segh <rhyme label="a" type="m" corresp="#s8:l1">ick</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Een kleyn, kleyn beetje <rhyme label="b" type="f" corresp="#s8:l3">smaelijck</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Houtje aen een moye <rhyme label="a" type="m">Mayt</rhyme></l>
        <l id="s9:l2" met="4" type="-+">As onse Claertje Creles <rhyme label="a" type="m" corresp="#s9:l1">sayt</rhyme>,</l>
        <l id="s9:l3" met="2" type="-+">Ghy Princen,, ghy <rhyme label="b" type="f">Princen</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Want je wilt doch <rhyme label="a" type="m" corresp="#s9:l1">vrolijckheyt</rhyme></l>
        <l id="s9:l5" met="3" type="-+">En zonghd geselschap <rhyme label="b" type="f" corresp="#s9:l3">winssen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
