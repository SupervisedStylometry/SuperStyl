    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Sei tanto graziosa</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEer Godt barmhartig <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Hoe veel, hoe groot zijn die <rhyme label="b" type="f">barmhartigheden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Daer mee u voll' <rhyme label="a" type="f" corresp="#s1:l1">springh-ader</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Besproeyt ons arme menschen hier <rhyme label="b" type="f" corresp="#s1:l2">beneden</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">V lof, u eer, kan <rhyme label="c" type="m">nimmermeer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van ons vol zijn <rhyme label="d" type="f">gesongen</rhyme>:</l>
        <l id="s1:l7" met="2" type="-+">Laet u <rhyme label="e" type="f">behagen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Den lof die u <rhyme label="e" type="f" corresp="#s1:l7">op-dragen</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Onse <rhyme label="d" type="f" corresp="#s1:l6">tongen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Eer souden wy de <rhyme label="a" type="f">Starren</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Daer mee het Firmament schoon is <rhyme label="b" type="f">beperelt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Net tellen hier van <rhyme label="a" type="f" corresp="#s2:l1">varren</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">En al dat stuyvens Zant der wijde <rhyme label="b" type="f" corresp="#s2:l2">wereldt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Och al veel eer, als wy, o <rhyme label="c" type="m">Heer</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Al uw groote <rhyme label="d" type="f">weldaden</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Souden voor <rhyme label="e" type="f">stellen</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Hoe soud' een mensch <rhyme label="e" type="f" corresp="#s2:l7">op-tellen</rhyme></l>
        <l id="s2:l9" met="2" type="-+">V <rhyme label="d" type="f" corresp="#s2:l6">genaden</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Om een van al te <rhyme label="a" type="f">noemen</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">'t Is een genaed' en goetheydt noyt <rhyme label="b" type="f">vol-presen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die altijdt staet te <rhyme label="a" type="f" corresp="#s3:l1">roemen</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Die ghy den eersten mensche hebt <rhyme label="b" type="f" corresp="#s3:l2">bewesen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Hy was alleen, ghy saeght hem <rhyme label="c" type="m">treen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Eensaem door u <rhyme label="d" type="f">Lust-hoven</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Ghy saeght <rhyme label="e" type="f">t'ontbreken</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Dies vondt ghy goet te <rhyme label="e" type="f" corresp="#s3:l7">spreken</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Dus van <rhyme label="d" type="f" corresp="#s3:l6">boven</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dees edel <rhyme label="a" type="f">creature</rhyme></l>
        <l id="s4:l2" met="5" type="-+">En mach op aerden soo alleen niet <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ick wil hem een <rhyme label="a" type="f" corresp="#s4:l1">parture</rhyme></l>
        <l id="s4:l4" met="5" type="-+">En goet geselschap, een trou hulpe <rhyme label="b" type="f" corresp="#s4:l2">geven</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Op dat den mensch, heb al sijn <rhyme label="c" type="m">wensch</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ghy hebt hem in sijn <rhyme label="d" type="f">slapen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Een rib <rhyme label="e" type="f">ontnomen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Daer uyt de Vrouw most <rhyme label="e" type="f" corresp="#s4:l7">komen</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Schoon <rhyme label="d" type="f" corresp="#s4:l6">geschapen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Eva alsoo <rhyme label="a" type="f">bereydet</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Hebt ghy tot Adam die noch eensaem <rhyme label="b" type="f">woonde</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Als met de handt <rhyme label="a" type="f" corresp="#s5:l1">geleydet</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Die Eva liefd', en u Heer danck <rhyme label="b" type="f" corresp="#s5:l2">betoonde</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Hy sprack seer bly, dees is van <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Een been van mijnen <rhyme label="d" type="f">beene</rhyme>:</l>
        <l id="s5:l7" met="2" type="-+">'k Sie, en <rhyme label="e" type="f">vereyssche</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Dat 's is van mijnen <rhyme label="e" type="f" corresp="#s5:l7">vleyssche</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Wy sijn <rhyme label="d" type="f" corresp="#s5:l6">eene</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">'t Geen ghy voor soo veel <rhyme label="a" type="f">jaren</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Aen Adam deed', doet ghy nu aen sijn <rhyme label="b" type="f">kind'ren</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ghy doetse t'samen <rhyme label="a" type="f" corresp="#s6:l1">paren</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Op dat dit eel geslaght niet sou <rhyme label="b" type="f" corresp="#s6:l2">vermind'ren</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Alleen u handt, den <rhyme label="c" type="m">Houw'lijcx-bandt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Vast leggen kan en <rhyme label="d" type="f">binden</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">'t Welck nu op <rhyme label="e" type="f">huyden</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Dees nieuw-getrouwe <rhyme label="e" type="f" corresp="#s6:l7">luyden</rhyme></l>
        <l id="s6:l9" met="2" type="-+"><rhyme label="d" type="f" corresp="#s6:l6">Onder-vinden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy hebt alleen, o <rhyme label="a" type="f">Heere</rhyme>!</l>
        <l id="s7:l2" met="5" type="-+">Dit lieve paer in liefd' gevoeght te <rhyme label="b" type="f">same</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">V komt alleen de <rhyme label="a" type="f" corresp="#s7:l1">eere</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">Dies sy en wy lof-zingen uwen <rhyme label="b" type="f" corresp="#s7:l2">name</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Ghy hebt 't gevoeght, dat hier <rhyme label="c" type="m">genoeght</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Wilt doch nu voort Heer <rhyme label="d" type="f">geven</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Dat's altijt <rhyme label="e" type="f">stille</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">In liefd' naer uwen <rhyme label="e" type="f" corresp="#s7:l7">wille</rhyme></l>
        <l id="s7:l9" met="2" type="-+">Mogen <rhyme label="d" type="f" corresp="#s7:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Laet nimmermeer <rhyme label="a" type="f">verkouden</rhyme></l>
        <l id="s8:l2" met="5" type="-+">d' Oprechte min in liefde tusschen <rhyme label="b" type="f">beyden</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Laetse altijdt vrede <rhyme label="a" type="f" corresp="#s8:l1">houden</rhyme>,</l>
        <l id="s8:l4" met="5" type="-+">Tot dat de bleecke doodt haer komt te <rhyme label="b" type="f" corresp="#s8:l2">scheyden</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Verblijt dit paer, doch menigh <rhyme label="c" type="m">jaer</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">O Heer! met u <rhyme label="d" type="f">genade</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Stort als een <rhyme label="e" type="f">regen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Op haer u rijcken <rhyme label="e" type="f" corresp="#s8:l7">zegen</rhyme></l>
        <l id="s8:l9" met="2" type="-+">Vroegh en <rhyme label="d" type="f" corresp="#s8:l6">spade</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
