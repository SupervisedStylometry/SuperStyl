<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21093</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>zaandam</orginal-name>
      <standard-name>Zaandam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.4424926</lon>
        <lat>4.8298607</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Het haantje van de wijnen</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">KOmt laet ons 't Houlijck <rhyme label="a" type="f">prijsen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">'T is ja wel prijsen <rhyme label="b" type="m">waert</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Den Echten-staet eere <rhyme label="a" type="f" corresp="#s1:l1">bewijsen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Wy die hier zijn <rhyme label="b" type="m" corresp="#s1:l2">vergaert</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Wy willen dees lof <rhyme label="c" type="f">vermeeren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met sangh die lieff'lijck <rhyme label="d" type="m">luyt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ons jonge paer volcx ter <rhyme label="c" type="f" corresp="#s1:l5">eeren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">G'luck Bruyd'gom en vrou <rhyme label="d" type="m" corresp="#s1:l6">Bruydt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Na dat ons God <rhyme label="a" type="f">almachtigh</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hemel en aerde <rhyme label="b" type="m">schiep</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Het vee, de Engelen <rhyme label="a" type="f" corresp="#s2:l1">krachtigh</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">De visch in de zee <rhyme label="b" type="m" corresp="#s2:l2">diep</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Schiep hy na sijnen <rhyme label="c" type="f">beelde</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Adam van aerden <rhyme label="d" type="m">kluyt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En settede hem in <rhyme label="c" type="f" corresp="#s2:l5">weelde</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">G'luck Bruyd'gom en vrou <rhyme label="d" type="m" corresp="#s2:l6">Bruydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Adam sijnde <rhyme label="a" type="f">geschapen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gestelt in 't <rhyme label="b" type="m">Paradijs</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">God liet hem diepe <rhyme label="a" type="f" corresp="#s3:l1">slapen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En door sijn raedt seer <rhyme label="b" type="m" corresp="#s3:l2">wijs</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Hy maeckte van Adams <rhyme label="c" type="f">zijde</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Eva, daer sy quam <rhyme label="d" type="m">uyt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Een hulp tot Adams <rhyme label="c" type="f" corresp="#s3:l5">verblijden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">G'luck Bruyd'gom en vrou <rhyme label="d" type="m" corresp="#s3:l6">Bruydt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Christus, na Schrifts <rhyme label="a" type="f">vermonden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Genoot ter Bruyloft <rhyme label="b" type="m">mee</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Heeft hem aldaer <rhyme label="a" type="f" corresp="#s4:l1">gevonden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Tot Cana in <rhyme label="b" type="m" corresp="#s4:l2">Galile</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Daer hy Water deed' <rhyme label="c" type="f">verkeeren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In Wijn, na zijn <rhyme label="d" type="m">besluyt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Den Echten-staet ter <rhyme label="c" type="f" corresp="#s4:l5">eeren</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">G'luck Bruygom en Vrou <rhyme label="d" type="m" corresp="#s4:l6">Bruydt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wy mogen den Wijn wel <rhyme label="a" type="f">drincken</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die 't hart verheught in <rhyme label="b" type="m">deught</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hear Bruyd'gom wilt <rhyme label="a" type="f" corresp="#s5:l1">gedencken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">De kroone die u <rhyme label="b" type="m" corresp="#s5:l2">verheught</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Wy gunnen u, tot Godts <rhyme label="c" type="f">eeren</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Menigh jonge <rhyme label="d" type="m">Wijngaerd-spruyt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Tot uwes geslachts <rhyme label="c" type="f" corresp="#s5:l5">vermeeren</rhyme>;</l>
        <l id="s5:l8" met="3" type="-+">G'luck Bruyd'gom en Vrou <rhyme label="d" type="m" corresp="#s5:l6">Bruydt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Prins, is de Koninck <rhyme label="a" type="f">stercke</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En stercker noch de <rhyme label="b" type="m">Wijn</rhyme>?</l>
        <l id="s6:l3" met="3" type="-+">Men leest, die 't soo <rhyme label="a" type="f" corresp="#s6:l1">bemercke</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dat Vroutjens noch starcker <rhyme label="b" type="m" corresp="#s6:l2">zijn</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Maer starcker noch de <rhyme label="c" type="f">Waerheydt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die uwe Echt t'samen <rhyme label="d" type="m">sluyt</rhyme>;</l>
        <l id="s6:l7" met="3" type="-+">Wy wenschen u liefdens <rhyme label="c" type="f" corresp="#s6:l5">klaerheydt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">G'luck Bruydegom en Vrou <rhyme label="d" type="m" corresp="#s6:l6">Bruydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
