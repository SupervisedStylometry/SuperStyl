      </coordinates>
    </location>
    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Psalm 023 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Wilt doch als <rhyme label="a" type="m">nou</rhyme></l>
        <l id="s1:l2" met="3" type="-+">O onsen Godt <rhyme label="b" type="f">ghepresen</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Nae u woordt <rhyme label="a" type="m" corresp="#s1:l1">trou</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">In ‘t midden by ons <rhyme label="b" type="f" corresp="#s1:l2">wesen</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Met uwe <rhyme label="c" type="m">kracht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van boven in ons <rhyme label="d" type="f">wercken</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Dat wy met <rhyme label="c" type="m" corresp="#s1:l5">macht</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Uwes naems eer <rhyme label="d" type="f" corresp="#s1:l6">verstercken</rhyme>:</l>
        <l id="s1:l9" met="2" type="-+">Want sonder <rhyme label="e" type="m">u</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Konnen wy niet <rhyme label="f" type="f">bedrijven</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Daerom wilt <rhyme label="e" type="m" corresp="#s1:l9">nu</rhyme></l>
        <l id="s1:l12" met="3" type="-+">In ‘t midden by ons <rhyme label="f" type="f" corresp="#s1:l10">blijven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Opent den <rhyme label="a" type="m">mondt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van uwen knecht <rhyme label="b" type="f">ootmoedich</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Tot deser <rhyme label="a" type="m" corresp="#s2:l1">stondt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat hy u woordt <rhyme label="b" type="f" corresp="#s2:l2">voorspoedigh</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Ons recht <rhyme label="c" type="m">voor-draecht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Nae u goed <rhyme label="d" type="f">wel-behaghen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Heel <rhyme label="c" type="m" corresp="#s2:l5">onvertsaeght</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Den rechten sin <rhyme label="d" type="f" corresp="#s2:l6">voor-draghen</rhyme>:</l>
        <l id="s2:l9" met="2" type="-+">Dat wy ‘t oock <rhyme label="e" type="m">bly</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Ghetrouwelijck <rhyme label="f" type="f">bewaren</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Daerom dat <rhyme label="e" type="m" corresp="#s2:l9">wy</rhyme></l>
        <l id="s2:l12" met="3" type="-+">By een zijn, en <rhyme label="f" type="f" corresp="#s2:l10">vergaren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Lof eer en <rhyme label="a" type="m">danck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sy u Vader <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Ons leven <rhyme label="a" type="m" corresp="#s3:l1">lanck</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Door uwen Soon <rhyme label="b" type="f" corresp="#s3:l2">ghegheven</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Ende oock <rhyme label="c" type="m">wijs</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Den Heylighen Gheest seer <rhyme label="d" type="f">goedich</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Sy lof en <rhyme label="c" type="m" corresp="#s3:l5">prijs</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Van ons ‘t samen <rhyme label="d" type="f" corresp="#s3:l6">eenmoedich</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">U Heyl’ge <rhyme label="e" type="m">naem</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Loven wy al te <rhyme label="f" type="f">samen</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Vyerich <rhyme label="e" type="m" corresp="#s3:l9">bequaem</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Tot in eeuwigheyt, <rhyme label="f" type="f" corresp="#s3:l10">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
