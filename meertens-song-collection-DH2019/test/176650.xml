    <melody>Psalm 130 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Godt is een Godt der <rhyme label="a" type="f">Goden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoort menschen kleyn en <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hy ghebiet sijn <rhyme label="a" type="f" corresp="#s1:l1">Gheboden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Te houden tot den <rhyme label="b" type="m" corresp="#s1:l2">doodt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hy is een Prins des <rhyme label="c" type="f">levens</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Vorst over Zion <rhyme label="d" type="m">teer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Looft dien Koninck <rhyme label="c" type="f" corresp="#s1:l5">verheven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En gheeft hem prijs en <rhyme label="d" type="m" corresp="#s1:l6">eer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den Heere wil ick <rhyme label="a" type="f">vresen,k</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Uyt liefden gheen <rhyme label="b" type="m">bedwanck</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En hem ghehoorsaem <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dienen mijn leven <rhyme label="b" type="m" corresp="#s2:l2">lanck</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Daerom Heere neycht u <rhyme label="c" type="f">ooghen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Op u kranck <rhyme label="d" type="m">instrument</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En wilt u tot my <rhyme label="c" type="f" corresp="#s2:l5">poghen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Uw Gheest doch in my <rhyme label="d" type="m" corresp="#s2:l6">sent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Lijdt u mijn <rhyme label="a" type="f">uytverkooren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Al wordt ghy hier <rhyme label="b" type="m">benout</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ick sal u wel <rhyme label="a" type="f" corresp="#s3:l1">verhooren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Soo ghy mijn woordt <rhyme label="b" type="m" corresp="#s3:l2">behoudt</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Ick sal u niet <rhyme label="c" type="f">begheven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al verliet een Moeder ‘t <rhyme label="d" type="m">Kindt</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Waer voor so wilt ghy <rhyme label="c" type="f" corresp="#s3:l5">beven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Als ghy ghenade <rhyme label="d" type="m" corresp="#s3:l6">vindt</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hier op so wil ick <rhyme label="a" type="f">waghen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Mijn lijf en al mijn <rhyme label="b" type="m">goet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat Kruysse wil ick <rhyme label="a" type="f" corresp="#s4:l1">draghen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dwinghen mijn vleesch tot <rhyme label="b" type="m" corresp="#s4:l2">boet</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Abel is doot <rhyme label="c" type="f">ghebleven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Door Aajans nijdigh <rhyme label="d" type="m">slagh</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">‘t Vervolch heeft hem <rhyme label="c" type="f" corresp="#s4:l5">verheven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Tot in den laetsten <rhyme label="d" type="m" corresp="#s4:l6">dach</rhyme>/</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn schilt zijt ghy, o <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Mijn wapen <rhyme label="b" type="m">onvervaert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wie sal my moghen <rhyme label="a" type="f" corresp="#s5:l1">deeren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Als ghy my Heer <rhyme label="b" type="m" corresp="#s5:l2">bewaert</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">Dat mijn vyanden <rhyme label="c" type="f">saghen</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Als Leeuwen t’elcken <rhyme label="d" type="m">kant</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">So mach ick niet <rhyme label="c" type="f" corresp="#s5:l5">vertraghen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">O Godt doet my <rhyme label="d" type="m" corresp="#s5:l6">bystant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Truyrt niet ghy Christen <rhyme label="a" type="f">Schapen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Al wort ghy hier <rhyme label="b" type="m">benijdt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ghy Herders wilt niet <rhyme label="a" type="f" corresp="#s6:l1">slapen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">My dunckt het is nu <rhyme label="b" type="m" corresp="#s6:l2">tijdt</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Weest vroomlijck op den <rhyme label="c" type="f">wachte</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Bewaert u Schaepkens <rhyme label="d" type="m">wel</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Al voor dit arch <rhyme label="c" type="f" corresp="#s6:l5">gheslachte</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Want sy zijn seer <rhyme label="d" type="m" corresp="#s6:l6">rebel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Simei David <rhyme label="a" type="f">vloeckte</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En wierp hem met een <rhyme label="b" type="m">steen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Maer David hem <rhyme label="a" type="f" corresp="#s7:l1">verkloeckte</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ende loofde Godt <rhyme label="b" type="m" corresp="#s7:l2">alleen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Abisai wilt dit <rhyme label="c" type="f">wreecken</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">Maer David sprack, <rhyme label="d" type="m">misschien</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Godt hevet hem <rhyme label="c" type="f" corresp="#s7:l5">gheheeten</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Sal mijn ellent aen <rhyme label="d" type="m" corresp="#s7:l6">sien</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Men sach veel Ioden <rhyme label="a" type="f">trueren</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat dede Hamen <rhyme label="b" type="m">quaet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Hy meyndse te <rhyme label="a" type="f" corresp="#s8:l1">verscheuren</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Door sijnen valschen <rhyme label="b" type="m" corresp="#s8:l2">raet</rhyme>:</l>
        <l id="s8:l5" met="3" type="-+">Ghenade sy <rhyme label="c" type="f">ontfanghen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">De Tyran kreegh sijn <rhyme label="d" type="m">loon</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">De Koninck deedt hem <rhyme label="c" type="f" corresp="#s8:l5">hanghen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Aen een soo hooghen <rhyme label="d" type="m" corresp="#s8:l6">Boom</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Ioseph door groot <rhyme label="a" type="f">benijden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Wert van sijn Broers <rhyme label="b" type="m">verkocht</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">‘t Gheschiede nae dien <rhyme label="a" type="f" corresp="#s9:l1">tijden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Sy hebben hem <rhyme label="b" type="m" corresp="#s9:l2">besocht</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Het ghebrack haer aen <rhyme label="c" type="f">Kooren</rhyme>:</l>
        <l id="s9:l6" met="3" type="-+">‘t Was in <rhyme label="d" type="m">Egypten-Landt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Ioseph wist wel te <rhyme label="c" type="f" corresp="#s9:l5">vooren</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Spijsde noch sijn <rhyme label="d" type="m" corresp="#s9:l6">vyandt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Neemt een exempel <rhyme label="a" type="f">mede</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Aen Christum onsen <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Die hem versmaetheyt <rhyme label="a" type="f" corresp="#s10:l1">dede</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Die Dreyghde hy niet <rhyme label="b" type="m" corresp="#s10:l2">weer</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Maer heeft daer voor <rhyme label="c" type="f">ghebeden</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En sprack tot haer <rhyme label="d" type="m">versoen</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Vergheeft haer <rhyme label="c" type="f" corresp="#s10:l5">overtreden</rhyme>.</l>
        <l id="s10:l8" met="3" type="-+">Vader wat sy my <rhyme label="d" type="m" corresp="#s10:l6">doen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
