  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MAeghden, uwe MAEGHDEN <rhyme label="a" type="m">LOF</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">'t Beste <rhyme label="a" type="m" corresp="#s1:l1">stof</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Bevindt ghy hier <rhyme label="b" type="f">gepresen</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">U hoogste lof, in dit <rhyme label="c" type="m">Liedt</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Is al <rhyme label="c" type="m" corresp="#s1:l4">niet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">By dat, de daet sal <rhyme label="b" type="f" corresp="#s1:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als Messias voor ons <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">In een <rhyme label="a" type="m" corresp="#s2:l1">stal</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Mensch wilde zijn <rhyme label="b" type="f">gebooren</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">Heeft een Maghet <rhyme label="c" type="m">onbesmet</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Reyn en <rhyme label="c" type="m" corresp="#s2:l4">net</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Voor hemselve <rhyme label="b" type="f" corresp="#s2:l3">verkooren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Een Maeght te zijn 't is een <rhyme label="a" type="m">gaef</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Ia, soo <rhyme label="a" type="m" corresp="#s3:l1">braef</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die Godt alleen kan <rhyme label="b" type="f">geven</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Om dit aldergrootste <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Maeght, ghy <rhyme label="c" type="m" corresp="#s3:l4">moet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Godt loven al u <rhyme label="b" type="f" corresp="#s3:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Gout, Parels leght op een <rhyme label="a" type="m">Schael</rhyme></l>
        <l id="s4:l2" met="2" type="+-">
          <rhyme label="a" type="m" corresp="#s4:l1">Altemael</rhyme>
        </l>
        <l id="s4:l3" met="3" type="-+">Des Werelts hooghste <rhyme label="b" type="f">saken</rhyme>:</l>
        <l id="s4:l4" met="4" type="+-">Soo op de ander Schael <rhyme label="c" type="m">leyt</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-"><rhyme label="c" type="m" corresp="#s4:l4">Suyverheyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">'t Eerst geen gewicht kan <rhyme label="b" type="f" corresp="#s4:l3">maken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hoe schoon is niet, 't kuys <rhyme label="a" type="m">geslacht</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Klaer van <rhyme label="a" type="m" corresp="#s5:l1">kracht</rhyme>?</l>
        <l id="s5:l3" met="3" type="-+">U memory sal <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Altijdt: by Godt, ende <rhyme label="c" type="m">Mensch</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Meer als <rhyme label="c" type="m" corresp="#s5:l4">wensch</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Sult ghy werden <rhyme label="b" type="f" corresp="#s5:l3">verheven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Door uw' Maeghdom, zijt ghy <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Dat uw' <rhyme label="a" type="m" corresp="#s6:l1">haeght</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">d' Heyligen Geest sijn <rhyme label="b" type="f">Tempel</rhyme></l>
        <l id="s6:l4" met="4" type="+-">En Godes waerste <rhyme label="c" type="m">vrindin</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Na sijn <rhyme label="c" type="m" corresp="#s6:l4">sin</rhyme>.</l>
        <l id="s6:l6" met="3" type="-+">Lucia geeft 't <rhyme label="b" type="f" corresp="#s6:l3">exempel</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Een Maeght is oock <rhyme label="a" type="m">Martelaer</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Vroom en <rhyme label="a" type="m" corresp="#s7:l1">klaer</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Al doodts' haer vleysch, moet <rhyme label="b" type="f">strijden</rhyme>.</l>
        <l id="s7:l4" met="4" type="+-">Sult om 't vleys 't meeste <rhyme label="c" type="m">torment</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Na uw' <rhyme label="c" type="m" corresp="#s7:l4">endt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Als Martelaer <rhyme label="b" type="f" corresp="#s7:l3">verblijden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Niet strijdt d' Engel tegen 't <rhyme label="a" type="m">vleysch</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Vleysch, noch <rhyme label="a" type="m" corresp="#s8:l1">eysch</rhyme></l>
        <l id="s8:l3" met="3" type="-+">In d' Engelen op <rhyme label="b" type="f">rijsen</rhyme>.</l>
        <l id="s8:l4" met="4" type="+-">d' Oudt-vaders u Maeghden <rhyme label="c" type="m">reyn</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">
          <rhyme label="c" type="m" corresp="#s8:l4">Algemeyn</rhyme>
        </l>
        <l id="s8:l6" met="3" type="-+">Boven d' Engelen <rhyme label="b" type="f" corresp="#s8:l3">prijsen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Leer noch, als ghy uwe <rhyme label="a" type="m">le'en</rhyme></l>
        <l id="s9:l2" met="2" type="+-">Hout <rhyme label="a" type="m" corresp="#s9:l1">besne'en</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ghy zijt Godts <rhyme label="b" type="f">Koninginne</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">En Godts Bruydt. Moet hem <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">Anders <rhyme label="c" type="m" corresp="#s9:l4">geen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Voor Bruydegom <rhyme label="b" type="f" corresp="#s9:l3">beminnen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Op uw', Godt sijn ooghen <rhyme label="a" type="m">slaet</rhyme>,</l>
        <l id="s10:l2" met="2" type="+-">En by <rhyme label="a" type="m" corresp="#s10:l1">staet</rhyme>;</l>
        <l id="s10:l3" met="3" type="-+">Vreught, soetigheyt, laet <rhyme label="b" type="f">dalen</rhyme>.</l>
        <l id="s10:l4" met="4" type="+-">Maeghden niemant uwe <rhyme label="c" type="m">staet</rhyme></l>
        <l id="s10:l5" met="2" type="+-">Die 't al <rhyme label="c" type="m" corresp="#s10:l4">gaet</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Te boven, kan <rhyme label="b" type="f" corresp="#s10:l3">verhalen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Fijne Maeght, in d'uur des <rhyme label="a" type="m">doodts</rhyme>,</l>
        <l id="s11:l2" met="2" type="+-">Van 't Lam <rhyme label="a" type="m" corresp="#s11:l1">Goodts</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Sult 't Hemels Hof <rhyme label="b" type="f">ontfangen</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">En met uwe Maeghden <rhyme label="c" type="m">kroon</rhyme>.</l>
        <l id="s11:l5" met="2" type="+-">Altijdt <rhyme label="c" type="m" corresp="#s11:l4">schoon</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Sult 't Lam volgen, met <rhyme label="b" type="f" corresp="#s11:l3">sangen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
