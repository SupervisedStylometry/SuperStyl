      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Venus gij en uw kind</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe lustich en hoe <rhyme label="a" type="m">groen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ist in dit Meys <rhyme label="a" type="m" corresp="#s1:l1">faysoen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Hoe lieflijck ist nu <rhyme label="b" type="f">buyten</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Nu al de jonge <rhyme label="b" type="f" corresp="#s1:l3">spruyten</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van gras, van kruyt, van <rhyme label="c" type="f">bloemen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Om strijt den Heere <rhyme label="c" type="f" corresp="#s1:l5">roemen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al 't Velt is nu ter <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Als een schoon groen <rhyme label="a" type="m" corresp="#s2:l1">tapijt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat onlangns stont als <rhyme label="b" type="f">Heyde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Is nu grasrijcke <rhyme label="b" type="f" corresp="#s2:l3">weyde</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En d'Ackers met haer <rhyme label="c" type="f">voren</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Staen groen van 't vruchtbaer <rhyme label="c" type="f" corresp="#s2:l5">Coren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De boomen hooch en <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die onlangs schenen <rhyme label="a" type="m" corresp="#s3:l1">doodt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Als dor en gantsch <rhyme label="b" type="f">bevroren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Staen als van nieus <rhyme label="b" type="f" corresp="#s3:l3">geboren</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Met bladeren en <rhyme label="c" type="f">knoppen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Verciert tot in de <rhyme label="c" type="f" corresp="#s3:l5">toppen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Vogelkens <rhyme label="a" type="m">rontsom</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die onlangs scheenen <rhyme label="a" type="m" corresp="#s4:l1">stom</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Nu weer met helder <rhyme label="b" type="f">kelen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den lof haers Scheppers <rhyme label="b" type="f" corresp="#s4:l3">quelen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Den Leeuwrick climt na <rhyme label="c" type="f">boven</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Om daer den Heer te <rhyme label="c" type="f" corresp="#s4:l5">loven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Elck schepsel na zijn <rhyme label="a" type="m">aert</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Sijns Scheppers lof <rhyme label="a" type="m" corresp="#s5:l1">verclaert</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Comt edel <rhyme label="b" type="f">Creature</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Comt leert van de <rhyme label="b" type="f" corresp="#s5:l3">Nature</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Comt mensch comt leert slaen <rhyme label="c" type="f">gade</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Den Meytijdt der <rhyme label="c" type="f" corresp="#s5:l5">ghenade</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De Son aent <rhyme label="a" type="m">Firmament</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Haer cracht nu neder <rhyme label="a" type="m" corresp="#s6:l1">sent</rhyme>Â :</l>
        <l id="s6:l3" met="3" type="-+">Nu sy is hooch <rhyme label="b" type="f">geresen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Stracx ziet men aen haer <rhyme label="b" type="f" corresp="#s6:l3">wesen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Aen boomen, kruyden, <rhyme label="c" type="f">dieren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Selfs aen de kleyne <rhyme label="c" type="f" corresp="#s6:l5">mieren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Het crachtich <rhyme label="a" type="m">Sonne-licht</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Van Gods vriendlijck <rhyme label="a" type="m" corresp="#s7:l1">gesicht</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">'t Welck breeckt de herde <rhyme label="b" type="f">steenen</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Heeft lang u hert <rhyme label="b" type="f" corresp="#s7:l3">bescheenen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Noch staet ghy als te <rhyme label="c" type="f">vooren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Verkout en gantsch <rhyme label="c" type="f" corresp="#s7:l5">bevroren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ghy blijft van vrucht soo <rhyme label="a" type="m">bloot</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Als waert ghy dor en <rhyme label="a" type="m" corresp="#s8:l1">doot</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Soo eenich sap en <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Is in u stam <rhyme label="b" type="f" corresp="#s8:l3">gebleven</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Begint nu vrucht te <rhyme label="c" type="f">dragen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">In dees soo vruchtbaer <rhyme label="c" type="f" corresp="#s8:l5">daghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Rijst uyt der sonden <rhyme label="a" type="m">stoff</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Wast tot uws Scheppers <rhyme label="a" type="m" corresp="#s9:l1">loff</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Als wel geplante <rhyme label="b" type="f">boomen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Nu zijn genaden <rhyme label="b" type="f" corresp="#s9:l3">stroomen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">U wortel soo <rhyme label="c" type="f">besproeyen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Poocht nu in deught te <rhyme label="c" type="f" corresp="#s9:l5">groeyen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Laet Heer u Hemelsch <rhyme label="a" type="m">vocht</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Afdruypen door de <rhyme label="a" type="m" corresp="#s10:l1">Locht</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Laet op ons herten <rhyme label="b" type="f">dalen</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Uws aenschijns Sonne <rhyme label="b" type="f" corresp="#s10:l3">stralen</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Soo sullen wy <rhyme label="c" type="f">ontluycken</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Als roosen op hear <rhyme label="c" type="f" corresp="#s10:l5">struycken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
