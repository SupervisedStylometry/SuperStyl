<song>
  <meta>
    <id>9542</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Waar schuilt mijn nimfje in het groen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WEl dan, m'en Liefste <rhyme label="a" type="m">verheucht</rhyme>:</l>
        <l id="s1:l2" met="4" type="+-">Siet de schoone <rhyme label="b" type="m">lieflijckheyt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van langh verwachtede <rhyme label="a" type="m" corresp="#s1:l1">vreught</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Heeft u eeuwigh troost <rhyme label="b" type="m" corresp="#s1:l2">bereyt</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Want het werck Van de <rhyme label="c" type="m">Kerck</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Sal soo sterck gaen boven alle <rhyme label="c" type="m" corresp="#s1:l5">merck</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dat self de beste <rhyme label="a" type="m">Wijngaerdt</rhyme></l>
        <l id="s2:l2" met="4" type="+-">In de Baal-hamon op 't <rhyme label="b" type="m">velt</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Van goede vruchtbaren <rhyme label="a" type="m" corresp="#s2:l1">aerdt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Door den Salomon <rhyme label="b" type="m" corresp="#s2:l2">gestelt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">En ghericht: noch al <rhyme label="c" type="m">swicht</rhyme></l>
        <l id="s2:l6" met="5" type="-+">In gewicht voor mijne <rhyme label="c" type="m" corresp="#s2:l5">Kerck-gesticht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want dien de Hoeders hy <rhyme label="a" type="m">gaf</rhyme>;</l>
        <l id="s3:l2" met="4" type="+-">Om de vruchten, hy <rhyme label="b" type="m">ontfingh</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Daer duysent sickelen <rhyme label="a" type="m" corresp="#s3:l1">af</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Sy twee hondert, na <rhyme label="b" type="m" corresp="#s3:l2">bedingh</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Van 't geheel, Sonder <rhyme label="c" type="m">scheel</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">'t Hoedermeel, was 't vijfde part en <rhyme label="c" type="m" corresp="#s3:l5">deel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer mijn, ick selve <rhyme label="a" type="m">bewaer</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Sy is voor m'en <rhyme label="b" type="m">aengesicht</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Ick vangh de vruchte van <rhyme label="a" type="m" corresp="#s4:l1">haer</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'t Voordeel is tot my <rhyme label="b" type="m" corresp="#s4:l2">gericht</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Wel dan Schoon, Stae ten <rhyme label="c" type="m">toon</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Met u loon, by 'swerelts smaet en <rhyme label="c" type="m" corresp="#s4:l5">hoon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">O ghy! die d'Hove <rhyme label="a" type="m">bewoont</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Daer m'en Kerck is <rhyme label="b" type="m">ingeplant</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">Siet uw' geselle <rhyme label="a" type="m" corresp="#s5:l1">ghekroont</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Mercken op u eender <rhyme label="b" type="m" corresp="#s5:l2">hant</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Op dan voort, Laet het <rhyme label="c" type="m">woort</rhyme></l>
        <l id="s5:l6" met="5" type="-+">Ongestoort, Van alle zijn <rhyme label="c" type="m" corresp="#s5:l5">gehoort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
