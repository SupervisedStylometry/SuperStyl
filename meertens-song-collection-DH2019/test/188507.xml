  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Kom Fillis laeten we ons <rhyme label="a" type="f">verblyden</rhyme>.</l>
        <l id="s1:l2" met="4" type="-+">Gebruiken we onze prille <rhyme label="b" type="m">jeugd</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Wy lachen om die ons <rhyme label="a" type="f" corresp="#s1:l1">benyden</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Kom baden wy in weelde en <rhyme label="b" type="m" corresp="#s1:l2">vreugd</rhyme>.</l>
        <l id="s1:l5" met="2" type="-+">Vereenigde <rhyme label="c" type="f">harten</rhyme>.</l>
        <l id="s1:l6" met="2" type="-+">Trotseren en <rhyme label="c" type="f" corresp="#s1:l5">tarten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">De plagen die andren <rhyme label="d" type="m">ontzien</rhyme>.</l>
        <l id="s1:l8" met="2" type="-+">Ik ken geene <rhyme label="c" type="f" corresp="#s1:l5">smarten</rhyme>.</l>
        <l id="s1:l9" met="4" type="-+">Zoo lang gy meuw liefde zult <rhyme label="d" type="m" corresp="#s1:l7">bien</rhyme>.</l>
      </lg>
    </lg>
