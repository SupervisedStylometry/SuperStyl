<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>191708</id>
    <date>
      <year-start>1653</year-start>
      <year-end>1653</year-end>
    </date>
    <location>
      <orginal-name>burghoorn</orginal-name>
      <standard-name>Burghoorn</standard-name>
    </location>
    <tags/>
    <melody/>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Segh eens, aller-schoonste der <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Waer is u Lief <rhyme label="b" type="m">gegaen</rhyme>?</l>
        <l id="s1:l3" met="3" type="-+">Wiist ons de plaetsen <rhyme label="b" type="m" corresp="#s1:l2">aen</rhyme>.</l>
        <l id="s1:l4" met="5" type="-+">Wy gaen soecken waer hy siine kudde magh <rhyme label="a" type="f" corresp="#s1:l1">houwen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">En waer dat siine Tenten <rhyme label="b" type="m" corresp="#s1:l2">staen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Miin Lief is ten Hove <rhyme label="a" type="f">genadert</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Onder het <rhyme label="b" type="m">reuck-gewas</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat miine ziel <rhyme label="b" type="m" corresp="#s2:l2">genas</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Daer hy't Lammeken weyd, en de kudde <rhyme label="a" type="f" corresp="#s2:l1">vergadert</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Die miin schat, hoop, en sorge <rhyme label="b" type="m" corresp="#s2:l2">was</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Miin Lief is de miin, ik de <rhyme label="a" type="f">siine</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ik ben de siin', hy <rhyme label="b" type="m">miin</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hy sal miin Trooster <rhyme label="b" type="m" corresp="#s3:l2">ziin</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Ik gevoel door siin min al miin kracht <rhyme label="a" type="f" corresp="#s3:l1">onder-miinen</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En ik swiim om siin <rhyme label="b" type="m" corresp="#s3:l2">mondeliin</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vriendin, ghy werd schoonder <rhyme label="a" type="f">beleden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Als Tirsa Konings <rhyme label="b" type="m">stee</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Iae lieffelijcker <rhyme label="b" type="m" corresp="#s4:l2">mee</rhyme></l>
        <l id="s4:l4" met="5" type="-+">Als Jerusalem self, en veel sterker van <rhyme label="a" type="f" corresp="#s4:l1">leden</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Als een Veld-heyr ten strijde <rhyme label="b" type="m" corresp="#s4:l2">ree</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ah! Lief, soet gewelt van miin <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Keert u gesicht van't <rhyme label="b" type="m">miin</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">U lonkjes doen my <rhyme label="b" type="m" corresp="#s5:l2">piin</rhyme>.</l>
        <l id="s5:l4" met="5" type="-+">Ik gevoel dat u Min my vermeeftert van <rhyme label="a" type="f" corresp="#s5:l1">binnen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">En ik kan sonder u niet <rhyme label="b" type="m" corresp="#s5:l2">ziin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">'kEn kan miin gesicht niet <rhyme label="a" type="f">versaden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En tot u schoonheyds <rhyme label="b" type="m">lof</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Vind' ik steeds nieuwe <rhyme label="b" type="m" corresp="#s6:l2">stof</rhyme>.</l>
        <l id="s6:l4" met="5" type="-+">'kSou miin handen verlieft in vlechten wel <rhyme label="a" type="f" corresp="#s6:l1">baden</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Als ik u vond' in miinen <rhyme label="b" type="m" corresp="#s6:l2">hof</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">'Tis al by miin Liefken <rhyme label="a" type="f">verschoven</rhyme></l>
        <l id="s7:l2" met="3" type="-+">De Koninginnen <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">En Maegden sonder <rhyme label="b" type="m" corresp="#s7:l2">tal</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">Ia de By-wijven ook kan haer luyster <rhyme label="a" type="f" corresp="#s7:l1">verdooven</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Dits de Duyf die miin finnen <rhyme label="b" type="m" corresp="#s7:l2">stal</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Vol-maeckt, sonder vlekke <rhyme label="a" type="f">geboren</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Haer Moeders eenigh <rhyme label="b" type="m">kind</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Is't hertjen dat my <rhyme label="b" type="m" corresp="#s8:l2">mind</rhyme>.</l>
        <l id="s8:l4" met="5" type="-+">Daerom gaet van haer wey noyt een schaepken <rhyme label="a" type="f" corresp="#s8:l1">verloren</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Want ik heb haer te wel <rhyme label="b" type="m" corresp="#s8:l2">gesind</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Wanneer haer de Maegdekens <rhyme label="a" type="f">naken</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">De Dochters van <rhyme label="b" type="m">Sion</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die men de schoonste <rhyme label="b" type="m" corresp="#s9:l2">von</rhyme>,</l>
        <l id="s9:l4" met="5" type="-+">Iae de By-wiiven self die ontsluyten haer <rhyme label="a" type="f" corresp="#s9:l1">kaken</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Tot den lof van miin schoone <rhyme label="b" type="m" corresp="#s9:l2">Son</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Daer gaet haer Jerusalem <rhyme label="a" type="f">prijsen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Siet eens den <rhyme label="b" type="m">dageraet</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Die aen haer wangen <rhyme label="b" type="m" corresp="#s10:l2">staet</rhyme>.</l>
        <l id="s10:l4" met="5" type="-+">'tSchiint haer montje bedaut wil haer Bruydegom <rhyme label="a" type="f" corresp="#s10:l1">wiis√©</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Wat daer uyt een soet reuckje <rhyme label="b" type="m" corresp="#s10:l2">gat</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">De Maen kan haer schoonheyd niet <rhyme label="a" type="f">halen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Noch suyverheyd de <rhyme label="b" type="m">Son</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Hoe klaer die immer <rhyme label="b" type="m" corresp="#s11:l2">ston</rhyme>:</l>
        <l id="s11:l4" met="5" type="-+">En sy kan ook een vrees uyt haer oogen doen <rhyme label="a" type="f" corresp="#s11:l1">stralen</rhyme></l>
        <l id="s11:l5" met="4" type="-+">Die de grootst' wel verschrikken <rhyme label="b" type="m" corresp="#s11:l2">kon</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ik gong nae de groene <rhyme label="a" type="f">valeyen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">En nae den <rhyme label="b" type="m">noten-hof</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Om daer te plukken <rhyme label="b" type="m" corresp="#s12:l2">of</rhyme></l>
        <l id="s12:l4" met="5" type="-+">Den granaet, en de druyf die miin Lief placht te <rhyme label="a" type="f" corresp="#s12:l1">leyen</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">En siin min staeg te singen <rhyme label="b" type="m" corresp="#s12:l2">lof</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Met een, eer dat ik't nauwelijx <rhyme label="a" type="f">wiste</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Ontvliegt my miine <rhyme label="b" type="m">ziel</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Als't snel-vervoerde <rhyme label="b" type="m" corresp="#s13:l2">wiel</rhyme></l>
        <l id="s13:l4" met="5" type="-+">Van Aminadabs Kar, na den Vriend die ik <rhyme label="a" type="f" corresp="#s13:l1">miste</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Die miin vyer in siin boesem <rhyme label="b" type="m" corresp="#s13:l2">hiel</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Keer weer, keer u vluchtige <rhyme label="a" type="f">schreden</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Keer weder, <rhyme label="b" type="m">Sulamiet</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Gy die ons ziel <rhyme label="b" type="m" corresp="#s14:l2">gebied</rhyme>.</l>
        <l id="s14:l4" met="5" type="-+">Wat haekt gy om u Vriend sterk te moed te gaen <rhyme label="a" type="f" corresp="#s14:l1">treden</rhyme></l>
        <l id="s14:l5" met="4" type="-+">Als een rey met een juychend <rhyme label="b" type="m" corresp="#s14:l2">Lied</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
