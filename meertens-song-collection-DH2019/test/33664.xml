        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="marialied" translation="Maria song">Marialied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Waar is de dochter van Sion</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WAer is te vinden dese <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die soo sterk is van <rhyme label="b" type="f">maghte</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Die't helsch Serpent verpletten <rhyme label="a" type="m" corresp="#s1:l1">souw</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En breeken al sijn <rhyme label="b" type="f" corresp="#s1:l2">kraghte</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ist Judith die den houwer <rhyme label="a" type="m">nam</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En Holofern' <rhyme label="b" type="f">onthoofde</rhyme>?</l>
        <l id="s2:l3" met="4" type="-+">Of ist Jahel, die <rhyme label="a" type="m" corresp="#s2:l1">Sisaram</rhyme></l>
        <l id="s2:l4" met="3" type="-+">'t Hooft met een spijker <rhyme label="b" type="f" corresp="#s2:l2">kloofde</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Het is Maria eel en <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die suyver is <rhyme label="b" type="f">ontfangen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Tot wie den eenigen Goodts <rhyme label="a" type="m" corresp="#s3:l1">Soon</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Met liefde is <rhyme label="b" type="f" corresp="#s3:l2">bevangen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat hy haer van der <rhyme label="a" type="m">eeuwigheydt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Tot moeder heeft <rhyme label="b" type="f">verkooren</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Daerom heeft hy haer wel <rhyme label="a" type="m" corresp="#s4:l1">bereydt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om reyn te sijn <rhyme label="b" type="f" corresp="#s4:l2">gebooren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hier van soo staet een schoon <rhyme label="a" type="m">figuer</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Waer meed' men dit kan <rhyme label="b" type="f">toone</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Beschreeven in de ouw' <rhyme label="a" type="m" corresp="#s5:l1">Schriftuer</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Van dien vermaerden <rhyme label="b" type="f" corresp="#s5:l2">Troone</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Die Salomon heeft <rhyme label="a" type="m">opgebout</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van heel spier wit <rhyme label="b" type="f">Yvoore</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">En die bedekt met 't fijnste <rhyme label="a" type="m" corresp="#s6:l1">gout</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Het welk hy kost <rhyme label="b" type="f" corresp="#s6:l2">bespoore</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Merkt, Salomon bediedt Goodts <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Die na sijn goet <rhyme label="b" type="f">begeeren</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Mariam stelt tot sijnen <rhyme label="a" type="m" corresp="#s7:l1">Troon</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Om daer in te <rhyme label="b" type="f" corresp="#s7:l2">regneeren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Het fijne gout dat blinkt in 't <rhyme label="a" type="m">oogh</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met glinsterende <rhyme label="b" type="f">straelen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Haer liefde straelt tot Godt om <rhyme label="a" type="m" corresp="#s8:l1">hoogh</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Dat Godt komt op haer <rhyme label="b" type="f" corresp="#s8:l2">daelen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">'t Yvoor is wit tot in de <rhyme label="a" type="m">grondt</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">'t Is waerdt om op te <rhyme label="b" type="f">letten</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Beduyt haer reynheydt sonder <rhyme label="a" type="m" corresp="#s9:l1">sondt</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En sonder eenigh <rhyme label="b" type="f" corresp="#s9:l2">smetten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">De hardigheydt van het <rhyme label="a" type="m">Yvoor</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Bequaem om langh te <rhyme label="b" type="f">dueren</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Verduldigheydt verstaet hier <rhyme label="a" type="m" corresp="#s10:l1">door</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">In lijden t'aller <rhyme label="b" type="f" corresp="#s10:l2">ueren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Als Davids Tooren staet sy <rhyme label="a" type="m">vast</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Verciert met duysent <rhyme label="b" type="f">schilden</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Dit Krijghs-cieraet haer seer wel <rhyme label="a" type="m" corresp="#s11:l1">past</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Want sy dan vyandt <rhyme label="b" type="f" corresp="#s11:l2">stilden</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Den vyandt die braght met <rhyme label="a" type="m">gewelt</rhyme></l>
        <l id="s12:l2" met="3" type="-+">De Weerelt tot veel <rhyme label="b" type="f">sonden</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Die werdt nu heel ter <rhyme label="a" type="m" corresp="#s12:l1">neer-gevelt</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En van een Vrouw <rhyme label="b" type="f" corresp="#s12:l2">gebonden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Weest bly Maria, suyver <rhyme label="a" type="m">Maeght</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">O sterke Maeght en <rhyme label="b" type="f">Moeder</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Bidt Godt dat hem altijdt <rhyme label="a" type="m" corresp="#s13:l1">behaeght</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Te weesen ons <rhyme label="b" type="f" corresp="#s13:l2">Behoeder</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Lof Godt van sijne milde <rhyme label="a" type="m">daet</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Godt moet sijn hoogh <rhyme label="b" type="f">gepreesen</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Dat hy u heeft gebruykt om 't <rhyme label="a" type="m" corresp="#s14:l1">quaet</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Des Weerelts te <rhyme label="b" type="f" corresp="#s14:l2">geneesen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
