<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182597</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Vrienden luister naar mijn rede</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Vriende luister na myn <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Ik zal zingen een aardig <rhyme label="b" type="m">lied</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ik zal uw wat nieuws <rhyme label="a" type="f" corresp="#s1:l1">ontleden</rhyme>:</l>
        <l id="s1:l4" met="4" type="+-">Wat t' Amsterdam is <rhyme label="b" type="m" corresp="#s1:l2">geschied</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Hoe een Jonker steld zyn <rhyme label="c" type="m">sm</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Op een overschoon <rhyme label="c" type="m" corresp="#s1:l5">Godin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Deze Jonker lag in 't <rhyme label="a" type="f">Venster</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Uit te Kyken: en hy <rhyme label="b" type="m">zag</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Een Boerin: die daar voorby <rhyme label="a" type="f" corresp="#s2:l1">kwam</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Ja voorby kwam op dien <rhyme label="b" type="m" corresp="#s2:l2">dag</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Die met Vogels was <rhyme label="c" type="m">belaan</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Die zy zou verkopen <rhyme label="c" type="m" corresp="#s2:l5">gaan</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">De Heer sprak tegen 't boere <rhyme label="a" type="f">meisje</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Waar gaa jy zo haastig <rhyme label="b" type="m">heen</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">De Boerin zei na de markt voor een <rhyme label="a" type="f" corresp="#s3:l1">rysje</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ik koom met vinken van <rhyme label="b" type="m" corresp="#s3:l2">Amstelveen</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">De heer zag het Meisje <rhyme label="c" type="m">aan</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Zulke staan my heel wel <rhyme label="c" type="m" corresp="#s3:l5">aan</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Schoone Vog'len; zy ze <rhyme label="a" type="f">Vog'len</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Schoonder Vog'len heb ik <rhyme label="b" type="m">nouw</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">De heer sgrak waar voor laatje u <rhyme label="a" type="f" corresp="#s4:l1">Vog'len</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">'t Meisje sprak ter goede <rhyme label="b" type="m" corresp="#s4:l2">trouw</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Voor negen stuivers myn <rhyme label="c" type="m">Heer</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Zomtyds kryg ik nog wel <rhyme label="c" type="m" corresp="#s4:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Negen stuivers zal ik je <rhyme label="a" type="f">geeven</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Tree maar binnen ontsangt u <rhyme label="b" type="m">geld</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ik nooit mooijer klugt <rhyme label="a" type="f" corresp="#s5:l1">bedreeven</rhyme>.</l>
        <l id="s5:l4" met="4" type="+-">Sprak de Jonker als een <rhyme label="b" type="m" corresp="#s5:l2">held</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Met zo sprak 't Meisje <rhyme label="c" type="m">ras</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En stak 't geldje in haar <rhyme label="c" type="m" corresp="#s5:l5">tas</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">En hy gaf haar wyn met <rhyme label="a" type="f">zuiker</rhyme>:</l>
        <l id="s6:l2" met="4" type="+-">Ag! ag! zeidze: zulke <rhyme label="b" type="m">Drank</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">O die smaakt my als de <rhyme label="a" type="f" corresp="#s6:l1">duiker</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Ag hoe lekker smaakt my <rhyme label="b" type="m" corresp="#s6:l2">dat</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Doe hy zag dat zy het <rhyme label="c" type="m">mogt</rhyme>;</l>
        <l id="s6:l6" met="4" type="+-">Heeft hy haar nog eens <rhyme label="c" type="m" corresp="#s6:l5">toegebrogt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hy greep 't Meisje by haar <rhyme label="a" type="f">armen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">En hy wierp haar op 't <rhyme label="b" type="m">Bed</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">'t Meisje dat begon te <rhyme label="a" type="f" corresp="#s7:l1">kermen</rhyme>;</l>
        <l id="s7:l4" met="4" type="+-">En zy ze√Ø wel wat is <rhyme label="b" type="m" corresp="#s7:l2">dat</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Vroeg ik 't uw van te veren <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Waar voor gy u vog'len <rhyme label="c" type="m" corresp="#s7:l5">liet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Neen zy zei 'k meende myn <rhyme label="a" type="f">Vinken</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">De Jonker zei ik meende <rhyme label="b" type="m">jou</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Daarom gaf ik u wyn te <rhyme label="a" type="f" corresp="#s8:l1">drinken</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Om dat ik u eerst zoenen <rhyme label="b" type="m" corresp="#s8:l2">wou</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Met zo zweeg het Boerinnetje <rhyme label="c" type="m">ftil</rhyme>:</l>
        <l id="s8:l6" met="4" type="+-">En de Jonker dee zyn <rhyme label="c" type="m" corresp="#s8:l5">wil</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">En de Jonker was vol <rhyme label="a" type="f">vreugde</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">By het vangen van een <rhyme label="b" type="m">Vink</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">En zyn hert dat hem <rhyme label="a" type="f" corresp="#s9:l1">verheugde</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Door het van te vooren <rhyme label="b" type="m" corresp="#s9:l2">drink'</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Brandewyn met Zuyker <rhyme label="c" type="m">zoet</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Deed hem en 't Boerinnetje <rhyme label="c" type="m" corresp="#s9:l5">goed</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Oorlof schoone <rhyme label="a" type="f">Boerinnen</rhyme>:</l>
        <l id="s10:l2" met="4" type="+-">Die met Vogels gaat te <rhyme label="b" type="m">mark</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Treed dog in geen Herberg <rhyme label="a" type="f" corresp="#s10:l1">binnen</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Geat maar regt uit na de <rhyme label="b" type="m" corresp="#s10:l2">mark</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Maar weest getrouw die u <rhyme label="c" type="m">bemind</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Op dat gy wederliefde <rhyme label="c" type="m" corresp="#s10:l5">vind</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
