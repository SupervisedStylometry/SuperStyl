    <melody>Que la danse m'est contraire</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">STeenveldt, zijt ghy niet <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Tot een alderhoochste <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Die 'nen Bruygom hebt <rhyme label="a" type="f" corresp="#s1:l1">ghegheven</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Aen de Moeder van den <rhyme label="b" type="m" corresp="#s1:l2">Heer</rhyme>?</l>
        <l id="s1:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s1:l5">boven</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s1:l6">behaeght</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die in u soo heeft <rhyme label="a" type="f">ghebloncken</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Uytghemunt in alle <rhyme label="b" type="m">deught</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Voor ootmoedicheyt <rhyme label="a" type="f" corresp="#s2:l1">ghesoncken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Heeft den Hemel seer <rhyme label="b" type="m" corresp="#s2:l2">verheucht</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s2:l5">boven</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s2:l6">behaeght</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Vry van alle 'swerelts <rhyme label="a" type="f">perten</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Stack hy uyt in <rhyme label="b" type="m">suyverheydt</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">In onnooselheydt van <rhyme label="a" type="f" corresp="#s3:l1">herten</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">In Godts dienst, in <rhyme label="b" type="m" corresp="#s3:l2">viericheydt</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s3:l5">boven</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s3:l6">behaeght</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Die Mariam, seer <rhyme label="a" type="f">ontsteken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Heeft ghedient met alle <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Maer sijn liefd' oock <rhyme label="a" type="f" corresp="#s4:l1">vergheleken</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Heeft de Maeght met <rhyme label="b" type="m" corresp="#s4:l2">wedermin</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s4:l5">boven</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s4:l6">behaeght</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Met twee gheesten sy <rhyme label="a" type="f">ghecomen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Van sijn liefde soo <rhyme label="b" type="m">verheert</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Heeft hem metter handt <rhyme label="a" type="f" corresp="#s5:l1">ghenomen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Met haer echte trouw <rhyme label="b" type="m" corresp="#s5:l2">vereert</rhyme>.</l>
        <l id="s5:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s5:l5">boven</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s5:l6">behaeght</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Die hem oock hierom doen <rhyme label="a" type="f">draghen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Heeft met Ioseph haer lief <rhyme label="b" type="m">kindt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat van hem met groot <rhyme label="a" type="f" corresp="#s6:l1">behaghen</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Wert in dese jonst <rhyme label="b" type="m" corresp="#s6:l2">ghedient</rhyme>.</l>
        <l id="s6:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s6:l5">boven</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s6:l6">behaeght</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hierom heeft s' oock hem <rhyme label="a" type="f">ghegheven</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Haren liefsten Bruygoms <rhyme label="b" type="m">naem</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die met hem soo was <rhyme label="a" type="f" corresp="#s7:l1">verheven</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">En ghemaect daer toe <rhyme label="b" type="m" corresp="#s7:l2">bequaem</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s7:l5">boven</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s7:l6">behaeght</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">In des hemels wijde <rhyme label="a" type="f">salen</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Wort ghehouden nu de <rhyme label="b" type="m">feest</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Waer met Ioseph siet nu <rhyme label="a" type="f" corresp="#s8:l1">stralen</rhyme></l>
        <l id="s8:l4" met="4" type="+-">Ioseph alle salich <rhyme label="b" type="m" corresp="#s8:l2">gheest</rhyme>.</l>
        <l id="s8:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s8:l5">boven</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s8:l6">behaeght</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Wilt dit bruyloft-liedt dan <rhyme label="a" type="f">singhen</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Looft dit wel gheluckich <rhyme label="b" type="m">paer</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">In jolijt 't hert op laet <rhyme label="a" type="f" corresp="#s9:l1">springhen</rhyme></l>
        <l id="s9:l4" met="4" type="+-">In de Bruyt verheught te <rhyme label="b" type="m" corresp="#s9:l2">gaer</rhyme>.</l>
        <l id="s9:l5" met="4" type="+-">Wilt den nieuwen Ioseph <rhyme label="c" type="f">loven</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Dient den Bruygom van de <rhyme label="d" type="m">Maeght</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Nu ghestelt by haer daer <rhyme label="c" type="f" corresp="#s9:l5">boven</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Die haer soo hier heeft <rhyme label="d" type="m" corresp="#s9:l6">behaeght</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
