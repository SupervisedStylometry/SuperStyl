<song>
  <meta>
    <id>51992</id>
    <date>
      <year-start>1840</year-start>
      <year-end>1840</year-end>
    </date>
    <location>
      <orginal-name>amsterdam (n. leliestraat mm 195)</orginal-name>
      <standard-name>Nieuwe Leliestraat, Amsterdam, The Netherlands</standard-name>
