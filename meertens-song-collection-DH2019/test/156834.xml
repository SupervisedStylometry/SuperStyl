<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>156834</id>
    <date>
      <year-start>1670</year-start>
      <year-end>1670</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Krelis Joostens malle vrijage</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Doen Orangie sigh met <rhyme label="a" type="f">Spangie</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Had vereenight door de <rhyme label="b" type="m">Vree</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Trock het moedighlijck <rhyme label="a" type="f" corresp="#s1:l1">Brittangie</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">'t Oorloghs-Lemmer uyt de <rhyme label="b" type="m" corresp="#s1:l2">Schee</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Hy dreyght den dapp'ren <rhyme label="c" type="m">Leeuw</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En den bequame <rhyme label="c" type="m" corresp="#s1:l5">Zeeuw</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Hy stroopt <rhyme label="d" type="m">verwoet</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Het Coopmans <rhyme label="d" type="m" corresp="#s1:l7">Goet</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">En bloet van Wees of <rhyme label="c" type="m" corresp="#s1:l5">Weeuw</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">En haer Hooft verslint de <rhyme label="a" type="f">bladen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Door de inlantsche Krijgh en <rhyme label="b" type="m">twist</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Die haer in Conincks bloedt dorst <rhyme label="a" type="f" corresp="#s2:l1">baeden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Gaf zich een koleurde <rhyme label="b" type="m" corresp="#s2:l2">mist</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">De Schepen zijlen weer en <rhyme label="c" type="m">werf</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van 't leven en <rhyme label="c" type="m" corresp="#s2:l5">verderf</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Het Nederlants <rhyme label="d" type="m">bloet</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Geeft Lijf en <rhyme label="d" type="m" corresp="#s2:l7">Goedt</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">En Vissers in haer <rhyme label="c" type="m" corresp="#s2:l5">Erf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">En hier toe voert hy op zijn <rhyme label="a" type="f">Kuste</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Macht van Schepen by de <rhyme label="b" type="m">Zee</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dreyght den Leeuw sijn soete <rhyme label="a" type="f" corresp="#s3:l1">lusten</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En verdoemt ons goede <rhyme label="b" type="m" corresp="#s3:l2">Vre'e</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Nu leestmen in de <rhyme label="c" type="m">Courant</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hoe dat in <rhyme label="c" type="m" corresp="#s3:l5">Amsterlant</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Een Vloot leyt <rhyme label="d" type="m">ree</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Om gaen ter <rhyme label="d" type="m" corresp="#s3:l7">Zee</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Na Spaens of Franse <rhyme label="c" type="m" corresp="#s3:l5">strant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">En Onse Vloot in Zee <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Vint hem nouwelijcks buyten <rhyme label="b" type="m">gaets</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Heeft een Zeeuws Fregat <rhyme label="a" type="f" corresp="#s4:l1">genomen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat ons preyde van veel <rhyme label="b" type="m" corresp="#s4:l2">quaets</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dus sent ont <rhyme label="c" type="m">Commandeur</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Als onse Schepen <rhyme label="c" type="m" corresp="#s4:l5">veur</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Op de Zeeuse <rhyme label="d" type="m">Kust</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Hoe dat men <rhyme label="d" type="m" corresp="#s4:l7">wist</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">De staten hare <rhyme label="c" type="m" corresp="#s4:l5">keur</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">d'Onse en Zeeuw met dertigh <rhyme label="a" type="f">Kielen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Voor de Hoofden kruysten <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Vol gepropt met <rhyme label="a" type="f" corresp="#s5:l1">Oorloghs-Zielen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Kruyt en Loot sonder <rhyme label="b" type="m" corresp="#s5:l2">rantsoen</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Ons Admirael is op ly <rhyme label="c" type="m">gedraeyt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Hy schiet een <rhyme label="c" type="m" corresp="#s5:l5">schoot</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Zet uyt de <rhyme label="d" type="m">boot</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">De bloet-vlagh <rhyme label="d" type="m" corresp="#s5:l7">waeyt</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">En nae 't Pitsjaerden draeft de <rhyme label="c" type="m" corresp="#s5:l5">Ruyter</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Met sijn houte Paert in <rhyme label="a" type="f">Zee</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En terstont daer op <rhyme label="b" type="m">besluyter</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Waer hy goede dienst mee <rhyme label="a" type="f" corresp="#s6:l1">dee</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Maer even soo 't <rhyme label="b" type="m" corresp="#s6:l2">geviel</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Te Pleymuyden in de <rhyme label="c" type="m">Kniel</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Sien wy daer <rhyme label="c" type="m" corresp="#s6:l5">maet</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Een <rhyme label="d" type="m">Kammeraet</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Die op ons dragent <rhyme label="d" type="m" corresp="#s6:l7">hiel</rhyme>.</l>
        <l id="s6:l9" met="3" type="-+">My belust met haer te <rhyme label="c" type="m" corresp="#s6:l5">kooten</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Stou de Kist en Koop om <rhyme label="a" type="f">leegh</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Vingen Reef en <rhyme label="b" type="m">Mars-zeyls-schooten</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Smeeten Schenckels uyt de <rhyme label="a" type="f" corresp="#s7:l1">weegh</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Wy ruckt'ons Schut te <rhyme label="b" type="m" corresp="#s7:l2">boort</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Terwijl men roepen <rhyme label="c" type="m">hoort</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Gebedt, <rhyme label="c" type="m" corresp="#s7:l5">gebedt</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Een yeder <rhyme label="d" type="m">set</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Zich neder op dat <rhyme label="d" type="m" corresp="#s7:l7">woort</rhyme>.</l>
        <l id="s7:l9" met="3" type="-+">En ons Capiteyn <rhyme label="c" type="m" corresp="#s7:l5">verheven</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Nae 't gebedt, ons vragen <rhyme label="a" type="f">doet</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Of een yeder is <rhyme label="b" type="m">genegen</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Om te wagen Lijf en <rhyme label="a" type="f" corresp="#s8:l1">Goet</rhyme>:</l>
        <l id="s8:l4" met="4" type="+-">Men antwoort ja, des <rhyme label="b" type="m" corresp="#s8:l2">doet</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Ons Luytenant vol <rhyme label="c" type="m">moet</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Op alles <rhyme label="c" type="m" corresp="#s8:l5">acht</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">De Stuer-man <rhyme label="d" type="m">lacht</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">Matroos loopt als <rhyme label="d" type="m" corresp="#s8:l7">verwoet</rhyme>.</l>
        <l id="s8:l9" met="3" type="-+">Boots en Schieman roept sa <rhyme label="c" type="m" corresp="#s8:l5">Jongen</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Brenght ons bouts en Reebants <rhyme label="a" type="f">op</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Daer wort Gentelman <rhyme label="b" type="m">gesongen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Heyst u Mars-zeyls in den <rhyme label="a" type="f" corresp="#s9:l1">top</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Ons Constapel <rhyme label="b" type="m" corresp="#s9:l2">terstont</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Brenght ons Kruyt-hoorens en <rhyme label="c" type="m">Lont</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Ons <rhyme label="c" type="m" corresp="#s9:l5">Corperael</rhyme>,</l>
        <l id="s9:l7" met="2" type="-+">Brengt Loot en <rhyme label="d" type="m">Stael</rhyme>,</l>
        <l id="s9:l8" met="2" type="-+">En Bijlen uyt het <rhyme label="d" type="m" corresp="#s9:l7">ront</rhyme>.</l>
        <l id="s9:l9" met="3" type="-+">En ons Admirael vol <rhyme label="c" type="m" corresp="#s9:l5">tooren</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Op sijn Schip vol Schut, <rhyme label="a" type="f">Tergoes</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Groet sijn Vyant na 't <rhyme label="b" type="m">behooren</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Brenght het op zijn Duyts <rhyme label="a" type="f" corresp="#s10:l1">avoes</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Was uyt geen Glas noch <rhyme label="b" type="m" corresp="#s10:l2">Schael</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Haer Donder en <rhyme label="c" type="m">Metael</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Daer een <rhyme label="c" type="m" corresp="#s10:l5">Banket</rhyme>,</l>
        <l id="s10:l7" met="2" type="-+">Wiert <rhyme label="d" type="m">voor-geset</rhyme></l>
        <l id="s10:l8" met="2" type="-+">Van Pulver, Vier en <rhyme label="d" type="m" corresp="#s10:l7">Stael</rhyme>.</l>
        <l id="s10:l9" met="3" type="-+">Hier op wackert sulcks een <rhyme label="c" type="m" corresp="#s10:l5">koeltje</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">By en wy die wenden <rhyme label="a" type="f">net</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Dus men haer op <rhyme label="b" type="m">Hollants-Stoeltje</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Noch eenmael te kacken <rhyme label="a" type="f" corresp="#s11:l1">set</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Men gaf soo dapper <rhyme label="b" type="m" corresp="#s11:l2">vonck</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Een van haer Schepen <rhyme label="c" type="m">sonck</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">De tweede raeckt in <rhyme label="c" type="m" corresp="#s11:l5">brandt</rhyme>,</l>
        <l id="s11:l7" met="2" type="-+">En blaeckt en <rhyme label="d" type="m">sprongh</rhyme>,</l>
        <l id="s11:l8" met="2" type="-+">Met oudt en <rhyme label="d" type="m" corresp="#s11:l7">jongh</rhyme>,</l>
        <l id="s11:l9" met="3" type="-+">Gins stoot een tegen 't <rhyme label="c" type="m" corresp="#s11:l5">Strandt</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">De Avondt Son verhuyst na 't <rhyme label="a" type="f">Noorden</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">Verlicht schaduwe en <rhyme label="b" type="m">schim</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">De Nacht verduystert ons de <rhyme label="a" type="f" corresp="#s12:l1">boorden</rhyme></l>
        <l id="s12:l4" met="4" type="+-">Van de zwoele <rhyme label="b" type="m" corresp="#s12:l2">Zuyder-kim</rhyme>:</l>
        <l id="s12:l5" met="3" type="-+">Dus hindert ons de <rhyme label="c" type="m">Nacht</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">En daerom wiert de <rhyme label="c" type="m" corresp="#s12:l5">Wacht</rhyme></l>
        <l id="s12:l7" met="2" type="-+">Weer <rhyme label="d" type="m">uyt-geset</rhyme>,</l>
        <l id="s12:l8" met="2" type="-+">En smorgens <rhyme label="d" type="m" corresp="#s12:l7">met</rhyme>,</l>
        <l id="s12:l9" met="3" type="-+">Den dagh haer komst <rhyme label="c" type="m" corresp="#s12:l5">verwacht</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Maer den Suffer nijpt sijn <rhyme label="a" type="f">ooren</rhyme></l>
        <l id="s13:l2" met="4" type="+-">Dicht by 't hooft, en druypt-staert <rhyme label="b" type="m">heen</rhyme>:</l>
        <l id="s13:l3" met="4" type="+-">Danckt den Schepper van hier <rhyme label="a" type="f" corresp="#s13:l1">boven</rhyme></l>
        <l id="s13:l4" met="4" type="+-">Samen voor gesonde <rhyme label="b" type="m" corresp="#s13:l2">Leen</rhyme>:</l>
        <l id="s13:l5" met="3" type="-+">Treedt aen in u <rhyme label="c" type="m">gemoedt</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Valt uwen Godt te <rhyme label="c" type="m" corresp="#s13:l5">voet</rhyme>,</l>
        <l id="s13:l7" met="2" type="-+">Zinght sijn <rhyme label="d" type="m">lof</rhyme></l>
        <l id="s13:l8" met="2" type="-+">En roemt daer <rhyme label="d" type="m" corresp="#s13:l7">of</rhyme></l>
        <l id="s13:l9" met="3" type="-+">Gelijck als David <rhyme label="c" type="m" corresp="#s13:l5">doet</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Wel op dan fiere <rhyme label="a" type="f">Nederlanders</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Maeckt vry Wapen-tuygh en <rhyme label="b" type="m">Wandt</rhyme>,</l>
        <l id="s14:l3" met="4" type="+-">Taeckelt toe Schepen en <rhyme label="a" type="f" corresp="#s14:l1">Branders</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">En beschermpt u <rhyme label="b" type="m" corresp="#s14:l2">Vaderlandt</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">Avoes daer op dees <rhyme label="c" type="m">kleur</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Wy drincken daer eens <rhyme label="c" type="m" corresp="#s14:l5">veur</rhyme></l>
        <l id="s14:l7" met="2" type="-+">Tot een <rhyme label="d" type="m">besluyt</rhyme>,</l>
        <l id="s14:l8" met="2" type="-+">Een Trooytje <rhyme label="d" type="m" corresp="#s14:l7">uyt</rhyme>,</l>
        <l id="s14:l9" met="3" type="-+">Ey spoelt de roock eens <rhyme label="c" type="m" corresp="#s14:l5">deur</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
