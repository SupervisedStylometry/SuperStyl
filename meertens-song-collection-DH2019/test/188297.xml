  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">DUld my dat ik u mag <rhyme label="a" type="f">kussen</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-"><rhyme label="a" type="f" corresp="#s1:l1">Ondertussen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat gy my een Krans <rhyme label="b" type="m">schakeert</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Mag ik wel Ã´ smaaklyk <rhyme label="c" type="f">Blondje</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-"><rhyme label="c" type="f" corresp="#s1:l4">Rozemondje</rhyme>!</l>
        <l id="s1:l6" met="4" type="+-">Die myn vrees in vreugd <rhyme label="b" type="m" corresp="#s1:l3">verkeert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Lachebekje, vol <rhyme label="a" type="f">vermoogen</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Zie myn <rhyme label="a" type="f" corresp="#s2:l1">oogen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Hoe zy branden <rhyme label="b" type="m">Aardsgodin</rhyme>!</l>
        <l id="s2:l4" met="4" type="+-">Gy verlekkerd al myn <rhyme label="c" type="f">zinnen</rhyme>.</l>
        <l id="s2:l5" met="2" type="+-">'k Moet <rhyme label="c" type="f" corresp="#s2:l4">beginnen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Of ik sterf van suivre <rhyme label="b" type="m" corresp="#s2:l3">min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ik moet duizend Roosjes <rhyme label="a" type="f">plukken</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Eer 't sal <rhyme label="a" type="f" corresp="#s3:l1">lukken</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat gy sult uw wensch <rhyme label="b" type="m">voldoen</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">En hun blaadjes netjes <rhyme label="c" type="f">voegen</rhyme></l>
        <l id="s3:l5" met="2" type="+-">Vol <rhyme label="c" type="f" corresp="#s3:l4">genoegen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Tot een Kroon uw min ten <rhyme label="b" type="m" corresp="#s3:l3">zoen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Rozemond myn schat myn <rhyme label="a" type="f">Liefje</rhyme>!</l>
        <l id="s4:l2" met="2" type="+-">Harte <rhyme label="a" type="f" corresp="#s4:l1">diefje</rhyme>!</l>
        <l id="s4:l3" met="4" type="+-">Ik zal wagten zoo g' u <rhyme label="b" type="m">spoed</rhyme>.</l>
        <l id="s4:l4" met="4" type="+-">Liefde zal de Roosjes <rhyme label="c" type="f">plukken</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">My <rhyme label="c" type="f" corresp="#s4:l4">verrukken</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Als gy blust myn <rhyme label="b" type="m" corresp="#s4:l3">minnegloed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
