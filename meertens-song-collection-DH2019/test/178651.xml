    <tags/>
    <melody>Psalm 047 Datheen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">VOlken, groot van <rhyme label="a" type="m">tal</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Juigt met bly <rhyme label="a" type="m" corresp="#s1:l1">geschal</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Klapt ook in de <rhyme label="b" type="m">handt</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Door het gansche <rhyme label="b" type="m" corresp="#s1:l3">landt</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Want de hoogste <rhyme label="c" type="m">Heer</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Is bekleedt met <rhyme label="c" type="m" corresp="#s1:l5">eer</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Hy is <rhyme label="d" type="m">vreesselyk</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">En een Vorst wiens <rhyme label="d" type="m" corresp="#s1:l7">ryk</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Over d'aarde <rhyme label="e" type="m">gaat</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">En alom <rhyme label="e" type="m" corresp="#s1:l9">bestaat</rhyme>;</l>
        <l id="s1:l11" met="3" type="+-">Ja, een Koning, <rhyme label="f" type="m">dien</rhyme></l>
        <l id="s1:l12" met="3" type="+-">Alles moet <rhyme label="f" type="m" corresp="#s1:l11">ontzien</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">D'Opperheerscher <rhyme label="a" type="m">stelt</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Onder ons <rhyme label="a" type="m" corresp="#s2:l1">gewelt</rhyme></l>
        <l id="s2:l3" met="3" type="+-">Volken, fier in <rhyme label="b" type="m">moedt</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Dien wy zelfs den <rhyme label="b" type="m" corresp="#s2:l3">voet</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Zetten op den <rhyme label="c" type="m">nek</rhyme>.</l>
        <l id="s2:l6" met="3" type="+-">Godt, naar zyn <rhyme label="c" type="m" corresp="#s2:l5">besprek</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Kiest onze <rhyme label="d" type="m">erfenis</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Voor ons; als <rhyme label="d" type="m" corresp="#s2:l7">gewis</rhyme></l>
        <l id="s2:l9" met="3" type="+-">Jakobs <rhyme label="e" type="m">heerlykheit</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Hem eerst <rhyme label="e" type="m" corresp="#s2:l9">toegezeit</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Van hem, als zyn <rhyme label="f" type="m">kindt</rhyme>,</l>
        <l id="s2:l12" met="3" type="+-">Tederlyk <rhyme label="f" type="m" corresp="#s2:l11">bemint</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Godt vaart op <rhyme label="a" type="m">omhoog</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Naar des hemels <rhyme label="a" type="m" corresp="#s3:l1">boog</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Met gejuich en <rhyme label="b" type="m">vreugdt</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Alles is <rhyme label="b" type="m" corresp="#s3:l3">verheugt</rhyme></l>
        <l id="s3:l5" met="3" type="+-">Door 't <rhyme label="c" type="m">bazuingeschal</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Klinkende over <rhyme label="c" type="m" corresp="#s3:l5">al</rhyme>.</l>
        <l id="s3:l7" met="3" type="+-">Psalmzingt onzen <rhyme label="d" type="m">Godt</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Psalmzingt, naar 't <rhyme label="d" type="m" corresp="#s3:l7">gebodt</rhyme>,</l>
        <l id="s3:l9" met="3" type="+-">Onzen <rhyme label="e" type="m">Hemelheer</rhyme>;</l>
        <l id="s3:l10" met="3" type="+-">Psalmzingt t'zyner <rhyme label="e" type="m" corresp="#s3:l9">eer</rhyme>;</l>
        <l id="s3:l11" met="3" type="+-">Looft den Koning, <rhyme label="f" type="m">dien</rhyme></l>
        <l id="s3:l12" met="3" type="+-">Elk moet hulde <rhyme label="f" type="m" corresp="#s3:l11">biën</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Want Godts <rhyme label="a" type="m">koningkryk</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Sluit in zich <rhyme label="a" type="m" corresp="#s4:l1">gelyk</rhyme></l>
        <l id="s4:l3" met="3" type="+-">D'aarde en <rhyme label="b" type="m">oceaan</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Die 'er onder <rhyme label="b" type="m" corresp="#s4:l3">staan</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Zingt, met <rhyme label="c" type="m">onderwys</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Zynen naam ten <rhyme label="c" type="m" corresp="#s4:l5">prys</rhyme>;</l>
        <l id="s4:l7" met="3" type="+-">Want de Heer <rhyme label="d" type="m">regeert</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">En wordt hoog <rhyme label="d" type="m" corresp="#s4:l7">geërt</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Overal <rhyme label="e" type="m">rontom</rhyme></l>
        <l id="s4:l10" met="3" type="+-">By het <rhyme label="e" type="m" corresp="#s4:l9">heidendom</rhyme>;</l>
        <l id="s4:l11" met="3" type="+-">Daar zyn <rhyme label="f" type="m">heerschappy</rhyme></l>
        <l id="s4:l12" met="3" type="+-">Blinkt aan alle <rhyme label="f" type="m" corresp="#s4:l11">zy</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Godt zit op den <rhyme label="a" type="m">troon</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Overheerlyk <rhyme label="a" type="m" corresp="#s5:l1">schoon</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Van zyn <rhyme label="b" type="m">heiligheit</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Vol van <rhyme label="b" type="m" corresp="#s5:l3">Majesteit</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">'t Edele <rhyme label="c" type="m">geslacht</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Is byeen <rhyme label="c" type="m" corresp="#s5:l5">gebragt</rhyme></l>
        <l id="s5:l7" met="3" type="+-">By het volk van <rhyme label="d" type="m">Godt</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">'s Heeren erve en <rhyme label="d" type="m" corresp="#s5:l7">lot</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">'t Zaadt van <rhyme label="e" type="m">Abraham</rhyme>.</l>
        <l id="s5:l10" met="3" type="+-">Want der <rhyme label="e" type="m" corresp="#s5:l9">vorstenstam</rhyme></l>
        <l id="s5:l11" met="3" type="+-"><rhyme label="f" type="m">D'aardtryksschilden</rhyme>,</l>
        <l id="s5:l12" met="3" type="+-">heeft Godt, die eeuwig <rhyme label="f" type="m" corresp="#s5:l11">leeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
