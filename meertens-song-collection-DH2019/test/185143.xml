<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185143</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>O schepper fier</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Doet <rhyme label="a" type="m">neerstigheydt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">O vrienden die Godt <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Om te sijn van <rhyme label="b" type="f" corresp="#s1:l2">binnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Vereenight in den <rhyme label="c" type="m">Geest</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">De <rhyme label="a" type="m" corresp="#s1:l1">eenigheyt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Moet ghy met Godt <rhyme label="b" type="f" corresp="#s1:l2">beginnen</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Soo ghy wilt <rhyme label="b" type="f" corresp="#s1:l2">ghewinnen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Vrede, der zielen <rhyme label="c" type="m" corresp="#s1:l4">feest</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Die den vrede soeckt van <rhyme label="d" type="f">buyten</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Quelt hem selven met <rhyme label="d" type="f" corresp="#s1:l9">dispuyten</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">In ‘t eenigh <rhyme label="e" type="m">een</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">In rust en anders <rhyme label="e" type="m" corresp="#s1:l11">geen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Die aen ‘t <rhyme label="a" type="m">verstant</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hem selven heeft <rhyme label="b" type="f">ghebonden</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Dat hy heeft <rhyme label="b" type="f" corresp="#s2:l2">ghevonden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En uyt de schrift <rhyme label="c" type="m">ghehaelt</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Stuck-werck ist, <rhyme label="a" type="m" corresp="#s2:l1">want</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Daer sijn weer ander <rhyme label="b" type="f" corresp="#s2:l2">gronden</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Die tot allen <rhyme label="b" type="f" corresp="#s2:l2">stonden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Bewijsen dat hy <rhyme label="c" type="m" corresp="#s2:l4">dwaelt</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Godts vrede hoogh te <rhyme label="d" type="f">loven</rhyme>,</l>
        <l id="s2:l10" met="4" type="+-">Gaet alle verstant te <rhyme label="d" type="f" corresp="#s2:l9">boven</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">In ‘t eenigh <rhyme label="e" type="m">een</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Is rust en anders <rhyme label="e" type="m" corresp="#s2:l11">geen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Eenigh is <rhyme label="a" type="m">Godt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Eenigh sijn al sijn <rhyme label="b" type="f">leden</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Vol liefden en <rhyme label="b" type="f" corresp="#s3:l2">vreden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In eenen Geest <rhyme label="c" type="m">ghebout</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Een is ‘t <rhyme label="a" type="m" corresp="#s3:l1">ghebodt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al sijnt verscheyden <rhyme label="b" type="f" corresp="#s3:l2">seden</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">Eenigh is de <rhyme label="b" type="f" corresp="#s3:l2">reden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Waer op ‘t gheloove <rhyme label="c" type="m" corresp="#s3:l4">hout</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Die ‘t gheloove deylt in <rhyme label="d" type="f">stucken</rhyme>,</l>
        <l id="s3:l10" met="4" type="+-">Dien en macht niet wel <rhyme label="d" type="f" corresp="#s3:l9">ghelucken</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">In ‘t eenigh <rhyme label="e" type="m">een</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Is rust en anders <rhyme label="e" type="m" corresp="#s3:l11">geen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Van <rhyme label="a" type="m">aenbegin</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sijn alle <rhyme label="b" type="f">Creaturen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Beelden en <rhyme label="b" type="f" corresp="#s4:l2">Figuren</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Van ‘t waerachtighe <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">aa Heeft eenen <rhyme label="a" type="m" corresp="#s4:l1">sin</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Na in-hout der <rhyme label="b" type="f" corresp="#s4:l2">schrifturen</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Om den mensch te <rhyme label="b" type="f" corresp="#s4:l2">sturen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">In ‘t Godtlijck wesen <rhyme label="c" type="m" corresp="#s4:l4">soet</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Neemt ghy ‘t beelt voor ‘t ware <rhyme label="d" type="f">wesen</rhyme>,</l>
        <l id="s4:l10" met="4" type="+-">‘t Sal u ziele niet <rhyme label="d" type="f" corresp="#s4:l9">ghenesen</rhyme>,</l>
        <l id="s4:l11" met="2" type="+-">In ‘t eenigh <rhyme label="e" type="m">een</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Is rust en anders <rhyme label="e" type="m" corresp="#s4:l11">geen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">En Godes <rhyme label="a" type="m">rijck</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Gheleken by de <rhyme label="b" type="f">zaden</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Dat wast op by <rhyme label="b" type="f" corresp="#s5:l2">graden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Inwendigh in den <rhyme label="c" type="m">mensch</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">‘t Comt niet <rhyme label="a" type="m" corresp="#s5:l1">ghelijck</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">’t Sijn gaven der <rhyme label="b" type="f" corresp="#s5:l2">ghenaden</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Godts heerlijcke <rhyme label="b" type="f" corresp="#s5:l2">daden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Sijn in der tijt na <rhyme label="c" type="m" corresp="#s5:l4">wensch</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Siet Godts liefde, siet Godts <rhyme label="d" type="f">minne</rhyme>,</l>
        <l id="s5:l10" met="4" type="+-">Siet Godts groote wijsheyt <rhyme label="d" type="f" corresp="#s5:l9">inne</rhyme>,</l>
        <l id="s5:l11" met="2" type="+-">In ‘t eenigh <rhyme label="e" type="m">een</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Is rust en anders <rhyme label="e" type="m" corresp="#s5:l11">geen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Ionck sijnde ‘t <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met melck moetmen ‘t <rhyme label="b" type="f">spysen</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Daer na <rhyme label="b" type="f" corresp="#s6:l2">onderwysen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Wat hoogher <rhyme label="c" type="m">altemet</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Allenghs <rhyme label="a" type="m" corresp="#s6:l1">begint</rhyme></l>
        <l id="s6:l6" met="3" type="-+">‘t Verstant in hem te <rhyme label="b" type="f" corresp="#s6:l2">rysen</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Endelijck de <rhyme label="b" type="f" corresp="#s6:l2">grysen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Wort stercke spijs <rhyme label="c" type="m" corresp="#s6:l4">gheset</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Wilt die spyse niet <rhyme label="d" type="f">begheeren</rhyme>,</l>
        <l id="s6:l10" met="4" type="+-">Die ghy niet en cont <rhyme label="d" type="f" corresp="#s6:l9">verteeren</rhyme>,</l>
        <l id="s6:l11" met="2" type="+-">In ‘t eenigh <rhyme label="e" type="m">een</rhyme>,</l>
        <l id="s6:l12" met="3" type="-+">Is rust en anders <rhyme label="e" type="m" corresp="#s6:l11">geen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Liefde ist <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Kennissen sijn de <rhyme label="b" type="f">deelen</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Die Godt als <rhyme label="b" type="f" corresp="#s7:l2">Iuweelen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Voor eerst den mensche <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s7:l5" met="2" type="-+">Waer liefd’ is <rhyme label="a" type="m" corresp="#s7:l1">heen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daer vallen veel <rhyme label="b" type="f" corresp="#s7:l2">krackeelen</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Maer liefde can <rhyme label="b" type="f" corresp="#s7:l2">heelen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Want sy de volheyt <rhyme label="c" type="m" corresp="#s7:l4">heeft</rhyme>,</l>
        <l id="s7:l9" met="4" type="+-">Christi liefde <rhyme label="d" type="f">onghemeten</rhyme>,</l>
        <l id="s7:l10" met="4" type="+-">Gaet te boven alle <rhyme label="d" type="f" corresp="#s7:l9">weten</rhyme>,</l>
        <l id="s7:l11" met="2" type="+-">In ‘t eenigh <rhyme label="e" type="m">een</rhyme>,</l>
        <l id="s7:l12" met="3" type="-+">Ist rust en anders <rhyme label="e" type="m" corresp="#s7:l11">geen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
