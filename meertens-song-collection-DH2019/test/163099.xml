    <melody>Vrolijke uurtjes</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">Vrolyk <rhyme label="a" type="f">uurtjes</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Dat zoo <rhyme label="a" type="f" corresp="#s1:l1">duurtjes</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">U myn Engelin <rhyme label="b" type="m">betael</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">U myn Engelin <rhyme label="b" type="m" corresp="#s1:l3">betael</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Rosemond stook aen de <rhyme label="a" type="f" corresp="#s1:l1">vuurtjes</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Ik bid volvoert myn <rhyme label="b" type="m" corresp="#s1:l3">zeegenprael</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Ik bid volvoert myn <rhyme label="b" type="m" corresp="#s1:l3">zeegenprael</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Doet myn <rhyme label="a" type="f">Lusjes</rhyme>:</l>
        <l id="s2:l2" met="2" type="+-">Door u <rhyme label="a" type="f" corresp="#s2:l1">kusjes</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Ag myn Engelin hoort <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Ag myn Engelin hoort <rhyme label="b" type="m" corresp="#s2:l3">aen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">‘k Bid ey wagt dog niet tot <rhyme label="a" type="f" corresp="#s2:l1">flusjes</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">‘k Eys terstond te zyn <rhyme label="b" type="m" corresp="#s2:l3">voldaen</rhyme>.</l>
        <l id="s2:l7" met="4" type="-+">‘k Eys terstond te zyn <rhyme label="b" type="m" corresp="#s2:l3">voldaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">‘t Groen der <rhyme label="a" type="f">boomen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Water <rhyme label="a" type="f" corresp="#s3:l1">stroomen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">‘k Sie dat niemand ons <rhyme label="b" type="m">verspiet</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">‘k Sie dat niemand ons <rhyme label="b" type="m" corresp="#s3:l3">verspiet</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Wilt dog haestig herwaerts <rhyme label="a" type="f" corresp="#s3:l1">komen</rhyme>.</l>
        <l id="s3:l6" met="4" type="-+">‘k Bid weerhoud myn voetjes <rhyme label="b" type="m" corresp="#s3:l3">niet</rhyme>.</l>
        <l id="s3:l7" met="4" type="-+">‘k Bid weerhoud myn voetjes <rhyme label="b" type="m" corresp="#s3:l3">niet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Laet ik de <rhyme label="a" type="f">tipjes</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Van <rhyme label="a" type="f" corresp="#s4:l1">Lipjes</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Zagjes drukken als <rhyme label="b" type="m">voorheen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Zagjes drukken als <rhyme label="b" type="m" corresp="#s4:l3">voorheen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Rozemond dat zyn de <rhyme label="a" type="f" corresp="#s4:l1">klipjes</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Daer ‘k Schipbreuk heb op <rhyme label="b" type="m" corresp="#s4:l3">geleen</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Daer ‘k Schipbreuk heb op <rhyme label="b" type="m" corresp="#s4:l3">geleen</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Voet myn <rhyme label="a" type="f">Luste</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Door het <rhyme label="a" type="f" corresp="#s5:l1">ruste</rhyme>.</l>
        <l id="s5:l3" met="4" type="+-">Ach myn lieve <rhyme label="b" type="m">Rozemond</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Ach myn lieve <rhyme label="b" type="m" corresp="#s5:l3">Rozemond</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Om myn minnebrand te <rhyme label="a" type="f" corresp="#s5:l1">blussen</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Gy maekt myn Ionkhert <rhyme label="b" type="m" corresp="#s5:l3">gezond</rhyme>.</l>
        <l id="s5:l7" met="4" type="-+">Gy maekt myn Ionkhert <rhyme label="b" type="m" corresp="#s5:l3">gezond</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Vrolyk <rhyme label="a" type="f">Meisje</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Poezel <rhyme label="a" type="f" corresp="#s6:l1">Vlysje</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">‘k Bid u hoord myn reden <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">‘k Bid u hoord myn reden <rhyme label="b" type="m" corresp="#s6:l3">aen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Laete wy zamen voor een <rhyme label="a" type="f" corresp="#s6:l1">reysie</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Met ons tweetjes wandelen <rhyme label="b" type="m" corresp="#s6:l3">gaen</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Met ons tweetjes wandelen <rhyme label="b" type="m" corresp="#s6:l3">gaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">Vrolyk <rhyme label="a" type="f">Meisje</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Ieugdig <rhyme label="a" type="f" corresp="#s7:l1">Vleisje</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Hoord myn smeeken en <rhyme label="b" type="m">gebeen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Hoord myn smeeken en <rhyme label="b" type="m" corresp="#s7:l3">gebeen</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Laet myn dog maer eens een <rhyme label="a" type="f" corresp="#s7:l1">reisje</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Ey waerom vlugt gy dog <rhyme label="b" type="m" corresp="#s7:l3">heen</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Ey waerom vlugt gy dog <rhyme label="b" type="m" corresp="#s7:l3">heen</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="+-"><rhyme label="a" type="f">Cupidootje</rhyme>,</l>
        <l id="s8:l2" met="2" type="+-">Doet een <rhyme label="a" type="f" corresp="#s8:l1">schootje</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Doorschiet myn schoone <rhyme label="b" type="m">Engelin</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Doorschiet myn schoone <rhyme label="b" type="m" corresp="#s8:l3">Engelin</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Treft haer eensjes met een <rhyme label="a" type="f" corresp="#s8:l1">lootje</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Dat het vliegt ten Boezem <rhyme label="b" type="m" corresp="#s8:l3">in</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Dat het vliegt ten Boezem <rhyme label="b" type="m" corresp="#s8:l3">in</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="+-">Kleurig <rhyme label="a" type="f">Roosje</rhyme>,</l>
        <l id="s9:l2" met="2" type="+-">Wat een <rhyme label="a" type="f" corresp="#s9:l1">bloosje</rhyme>:</l>
        <l id="s9:l3" met="4" type="+-">Daelt ‘er op u Aengezigt <rhyme label="b" type="m">neer</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Daelt ‘er op u Aengezigt <rhyme label="b" type="m" corresp="#s9:l3">neer</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Gun my dat ik maer een <rhyme label="a" type="f" corresp="#s9:l1">poosje</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">My mag buygen voor u <rhyme label="b" type="m" corresp="#s9:l3">neer</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">My mag buygen voor u <rhyme label="b" type="m" corresp="#s9:l3">neer</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
