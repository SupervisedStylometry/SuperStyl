  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Die <rhyme label="a" type="m">Amaril</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Beminnen <rhyme label="a" type="m" corresp="#s1:l1">wil</rhyme></l>
        <l id="s1:l3" met="4" type="-+">oet harenhoog moed teugel <rhyme label="b" type="f">vieren</rhyme>.</l>
        <l id="s1:l4" met="4" type="-+">Moet naer de konst zyn tong <rhyme label="b" type="f" corresp="#s1:l3">bestieren</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Of zwygen <rhyme label="a" type="m" corresp="#s1:l1">stil</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Die Nimf die yder kan <rhyme label="c" type="f">bekoren</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Is schoonmaeris ook trots <rhyme label="c" type="f" corresp="#s1:l6">geboren</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">Want haer <rhyme label="d" type="f">aenminnigheden</rhyme></l>
        <l id="s1:l9" met="4" type="-+">Waer door zy alles tot haer <rhyme label="e" type="m">wendt</rhyme></l>
        <l id="s1:l10" met="4" type="-+">(Doch geenzins haere fiere <rhyme label="d" type="f" corresp="#s1:l8">zeden</rhyme>)</l>
        <l id="s1:l11" met="4" type="+-">Zyn haer meerdan tewel <rhyme label="e" type="m" corresp="#s1:l9">bekend</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Zy voer' haer <rhyme label="a" type="m">schoon</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Alom ten <rhyme label="a" type="m" corresp="#s2:l1">toon</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Zy zal wel haest dien trots <rhyme label="b" type="f">beschreyen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Vergeefsch tracht zy ons te <rhyme label="b" type="f" corresp="#s2:l3">verleyen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Wy zyn 't <rhyme label="a" type="m" corresp="#s2:l1">gewoon</rhyme>.</l>
        <l id="s2:l6" met="4" type="-+">Wy willen wel een schoonheid <rhyme label="c" type="f">minnen</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">Wy laeten wel ons hart <rhyme label="c" type="f" corresp="#s2:l6">verwinnen</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Maer wachten ook <rhyme label="d" type="f">belooning</rhyme>.</l>
        <l id="s2:l9" met="4" type="-+">Vaer wel o fiere <rhyme label="e" type="m">herderin</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Uw trotsheid duldt geenzints <rhyme label="d" type="f" corresp="#s2:l8">verschooning</rhyme>;</l>
        <l id="s2:l11" met="4" type="+-">Die dooft, ja doot de trouwste <rhyme label="e" type="m" corresp="#s2:l9">min</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
