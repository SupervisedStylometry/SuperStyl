      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Rijck Moeder Godts <rhyme label="a" type="f">Marije</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Soeter dan honigh <rhyme label="b" type="m">raet</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Des Hemels poorte <rhyme label="a" type="f" corresp="#s1:l1">blije</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">die altijdts open <rhyme label="b" type="m" corresp="#s1:l2">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">RYck Moeder Godts <rhyme label="a" type="f">Marije</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Soeter dan honigh <rhyme label="b" type="m">raet</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Des Hemels poorte <rhyme label="a" type="f" corresp="#s2:l1">blije</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">die altijdts open <rhyme label="b" type="m" corresp="#s2:l2">staet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy zijt onse <rhyme label="a" type="f">Zee-sterre</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van dese wereldt <rhyme label="b" type="m">wijdt</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Laet ons van by, en <rhyme label="a" type="f" corresp="#s3:l1">verre</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Door u licht zijn <rhyme label="b" type="m" corresp="#s3:l2">bevrijdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
