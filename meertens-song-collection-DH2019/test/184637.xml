    <tags/>
    <melody>Arma bella</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">DOet <rhyme label="a" type="f">vergaeren</rhyme></l>
        <l id="s1:l2" met="2" type="+-">Blyde <rhyme label="a" type="f" corresp="#s1:l1">snaeren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Geeft met d'Engels soet <rhyme label="b" type="m">geklanck</rhyme>:</l>
        <l id="s1:l4" met="2" type="-+">Godt, mensch <rhyme label="c" type="f">geboren</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Voor ons, <rhyme label="c" type="f" corresp="#s1:l4">verloren</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Doet ons <rhyme label="d" type="f">suchten</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Verre <rhyme label="d" type="f" corresp="#s1:l6">vluchten</rhyme>;</l>
        <l id="s1:l8" met="4" type="+-">Geeft het kindtjen lof en <rhyme label="b" type="m" corresp="#s1:l3">danck</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Siet t'kindt in liefde <rhyme label="e" type="f">branden</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">DÃ¡t breeckt des wereldts <rhyme label="e" type="f" corresp="#s1:l9">banden</rhyme>:</l>
        <l id="s1:l11" met="4" type="+-">Heel de wereldt sy <rhyme label="f" type="m">verheught</rhyme>,</l>
        <l id="s1:l12" met="3" type="+-">Kloeck, Herders, thoont uw' <rhyme label="f" type="m" corresp="#s1:l11">vreught</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">t' Moet al <rhyme label="a" type="f">geven</rhyme></l>
        <l id="s2:l2" met="2" type="+-">Een nieuw <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Fellen winter stilt uw' <rhyme label="b" type="m">kouw</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Laet u niet <rhyme label="c" type="f">vinden</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Gy Noordtsche <rhyme label="c" type="f" corresp="#s2:l4">winden</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Wegh <rhyme label="d" type="f">haegel-vlaegen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Komt schoon <rhyme label="d" type="f" corresp="#s2:l6">daegen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">En bevrydt het kindt van <rhyme label="b" type="m" corresp="#s2:l3">rouw</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Fonteynen, ende <rhyme label="e" type="f">beken</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Kiest langhs het veldt uw' <rhyme label="e" type="f" corresp="#s2:l9">streken</rhyme>;</l>
        <l id="s2:l11" met="4" type="+-">Brenght-ons-voort den groenen <rhyme label="f" type="m">tydt</rhyme></l>
        <l id="s2:l12" met="3" type="+-">die d'aerde heel <rhyme label="f" type="m" corresp="#s2:l11">verblydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-"><rhyme label="a" type="f">Nachtergaelen</rhyme>,</l>
        <l id="s3:l2" met="2" type="+-">Komt hier <rhyme label="a" type="f" corresp="#s3:l1">daelen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Opent uwe soete <rhyme label="b" type="m">stem</rhyme>:</l>
        <l id="s3:l4" met="2" type="-+">Stelt t'kintjens <rhyme label="c" type="f">lyden</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Door sanck ter <rhyme label="c" type="f" corresp="#s3:l4">syden</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Voeght uw' <rhyme label="d" type="f">kelen</rhyme></l>
        <l id="s3:l7" met="2" type="-+">By ons <rhyme label="d" type="f" corresp="#s3:l6">spelen</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Rondt den Stal van <rhyme label="b" type="m" corresp="#s3:l3">Bethlehem</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">Terwylen dat wy <rhyme label="e" type="f">singen</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Gy Schaepkens moght nu <rhyme label="e" type="f" corresp="#s3:l9">springen</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Uwen Herder is den <rhyme label="f" type="m">Heer</rhyme>,</l>
        <l id="s3:l12" met="3" type="+-">En vreest den wolf niet <rhyme label="f" type="m" corresp="#s3:l11">meer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
