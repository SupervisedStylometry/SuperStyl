<song>
  <meta>
    <id>35770</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Ik ging nog hedenmorgen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LAet Schepper hede <rhyme label="a" type="f">dalen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Uyt uwe milden <rhyme label="b" type="m">handt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Gracy uyt 's Hemels <rhyme label="a" type="f" corresp="#s1:l1">zalen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Op mijn tongh en <rhyme label="b" type="m" corresp="#s1:l2">verstandt</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Dat ick tot u lof op dese <rhyme label="c" type="m">dagh</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Van Sint Michiel <rhyme label="a" type="f" corresp="#s1:l1">verhalen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Eenige daden <rhyme label="c" type="m" corresp="#s1:l5">mach</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sinte Michiel <rhyme label="a" type="f">verkoren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is, tot Prins, Vorst en <rhyme label="b" type="m">Hooft</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van al d' Engelen <rhyme label="a" type="f" corresp="#s2:l1">Chooren</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Na 't Landt, van Godt <rhyme label="b" type="m" corresp="#s2:l2">belooft</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Heeft 't Volck voor de roode zee <rhyme label="c" type="m">gebracht</rhyme>;</l>
        <l id="s2:l6" met="3" type="-+">In de zee laet <rhyme label="a" type="f" corresp="#s2:l1">versmooren</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Pharo met al sijn <rhyme label="c" type="m" corresp="#s2:l5">macht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Lucifer wilde <rhyme label="a" type="f">stelle</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sijn stoel aldernaest <rhyme label="b" type="m">Godt</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Sint Michiel stoot in <rhyme label="a" type="f" corresp="#s3:l1">d'Helle</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dese verwonne <rhyme label="b" type="m" corresp="#s3:l2">Sot</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Die hem van hooger plaets had <rhyme label="c" type="m">beroemt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Is met sijn <rhyme label="a" type="f" corresp="#s3:l1">me-geselle</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Om d' hoovaerdy <rhyme label="c" type="m" corresp="#s3:l5">verdoemt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy heeft in Moyses <rhyme label="a" type="f">handen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Godes beschreven <rhyme label="b" type="m">Wet</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Abrahams <rhyme label="a" type="f" corresp="#s4:l1">offerhanden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Van Isaac mee <rhyme label="b" type="m" corresp="#s4:l2">belet</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Als Abraham souw doode met 't <rhyme label="c" type="m">Swaerdt</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Sijn Soon, ende <rhyme label="a" type="f" corresp="#s4:l1">verbranden</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Van 't Swaert, en vyer hem <rhyme label="c" type="m" corresp="#s4:l5">spaert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Drie vroome <rhyme label="a" type="f">Iongelingen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In de Oven <rhyme label="b" type="m">gestelt</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Sijn hulp en troost <rhyme label="a" type="f" corresp="#s5:l1">ontfingen</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">In 't vyer gaen als op 't <rhyme label="b" type="m" corresp="#s5:l2">veldt</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Geen schade lijden, noch geen <rhyme label="c" type="m">torment</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">In 't vyer Goods Lof-zangh <rhyme label="a" type="f" corresp="#s5:l1">zingen</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Uyt 't vyer gaen <rhyme label="c" type="m" corresp="#s5:l5">ongeschent</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Balam siet, ziet de <rhyme label="a" type="f">degen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van Michiel blincke <rhyme label="b" type="m">klaer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Als hy passeert de <rhyme label="a" type="f" corresp="#s6:l1">wegen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Om d' Isra'lietsche <rhyme label="b" type="m" corresp="#s6:l2">schaer</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Heel te vervloecken; om Balacx <rhyme label="c" type="m">aes</rhyme></l>
        <l id="s6:l6" met="3" type="-+">'t Ontfangen; haer Goods <rhyme label="a" type="f" corresp="#s6:l1">zegen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Most geven in de <rhyme label="c" type="m" corresp="#s6:l5">plaets</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Josue in sijn <rhyme label="a" type="f">dagen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Door hem krijght groote <rhyme label="b" type="m">macht</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Van Senachrib <rhyme label="a" type="f" corresp="#s7:l1">verslagen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Heeft Michiel op een <rhyme label="b" type="m" corresp="#s7:l2">nacht</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Hondert en tachtentigh duyzent <rhyme label="c" type="m">stijf</rhyme>.</l>
        <l id="s7:l6" met="3" type="-+">Uyt 't Paradijs <rhyme label="a" type="f" corresp="#s7:l1">verjagen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Heeft Adam met sijn <rhyme label="c" type="m" corresp="#s7:l5">Wijf</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Geweest is de <rhyme label="a" type="f">Bewaerder</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Van de oude <rhyme label="b" type="m">Synagoogh</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Is nu Patroon en <rhyme label="a" type="f" corresp="#s8:l1">Vader</rhyme>;</l>
        <l id="s8:l4" met="3" type="-+">En houdt altijdt sijn <rhyme label="b" type="m" corresp="#s8:l2">oogh</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Op Christus Kerck (gebouwt op een <rhyme label="c" type="m">steen</rhyme>.)</l>
        <l id="s8:l6" met="3" type="-+">Gedachtigh <rhyme label="a" type="f" corresp="#s8:l1">allegader</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Is ons, door sijn <rhyme label="c" type="m" corresp="#s8:l5">gebeen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">SinteMichiel sal <rhyme label="a" type="f">noode</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">En blasen <rhyme label="b" type="m">over-luyt</rhyme>:</l>
        <l id="s9:l3" met="3" type="-+">Tot oordeel Goods komt <rhyme label="a" type="f" corresp="#s9:l1">dooden</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Sal Antichrist de <rhyme label="b" type="m" corresp="#s9:l2">guyt</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Verwinnen. Sal Christus bloedigh <rhyme label="c" type="m">kruys</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Aen goede; Ketters, <rhyme label="a" type="f" corresp="#s9:l1">Ioden</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Vertoone met <rhyme label="c" type="m" corresp="#s9:l5">gedruys</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Sinte Michiel sal <rhyme label="a" type="f">toonen</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Aen Godes throon de <rhyme label="b" type="m">Mensch</rhyme>.</l>
        <l id="s10:l3" met="3" type="-+">De Goede 's Hemels <rhyme label="a" type="f" corresp="#s10:l1">throone</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Sullen krijge na <rhyme label="b" type="m" corresp="#s10:l2">wensch</rhyme>;</l>
        <l id="s10:l5" met="4" type="-+">Quade sullen hoore: gaet van <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">(Sonder uw' te <rhyme label="a" type="f" corresp="#s10:l1">verschoone</rhyme>)</l>
        <l id="s10:l7" met="3" type="-+">Gaet, blijft in d'Helsche <rhyme label="c" type="m" corresp="#s10:l5">pijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Bewaerder, en <rhyme label="a" type="f">Patroone</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Aerds-Engel Sint <rhyme label="b" type="m">Michiel</rhyme>!</l>
        <l id="s11:l3" met="3" type="-+">Verkrijght van Godes <rhyme label="a" type="f" corresp="#s11:l1">Soone</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Dat onse waerde <rhyme label="b" type="m" corresp="#s11:l2">ziel</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">In d'ure des doodts, mach met <rhyme label="c" type="m">geneught</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Komen in 's Hemels <rhyme label="a" type="f" corresp="#s11:l1">throone</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Komen in s' Hemels <rhyme label="c" type="m" corresp="#s11:l5">vreught</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
