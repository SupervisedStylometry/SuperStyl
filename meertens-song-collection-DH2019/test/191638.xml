  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'t IS Kermis komt op 't <rhyme label="a" type="m">Feest</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">In Amstels grote <rhyme label="b" type="f">steden</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wien sou niet met <rhyme label="c" type="m">gejuig</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Zyn lof by ons <rhyme label="b" type="f" corresp="#s1:l2">besteden</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Daar Amstels soete <rhyme label="a" type="m">Jeugt</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Zig nu weer in <rhyme label="b" type="f">verblyd</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Komt maakt het Feest by <rhyme label="c" type="m">ons</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En werd met ons <rhyme label="b" type="f" corresp="#s2:l2">verblyd</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Zo sal 't Feest syn <rhyme label="a" type="m">vergroot</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Van Amstels groote <rhyme label="b" type="f">Stad</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Den Burger en den <rhyme label="c" type="m">Boer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Kreeg dan wel ligt zyn <rhyme label="b" type="f" corresp="#s3:l2">pard</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Daarom komt lustig <rhyme label="a" type="m">nadert</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Komt nadert onse <rhyme label="b" type="f">Stad</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Wy sullen met u zyn, te <rhyme label="c" type="m">samen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Uitmaken een groote <rhyme label="b" type="f" corresp="#s4:l2">schat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Met het Meysje van <rhyme label="a" type="m">playzier</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ik zoek myn Venus <rhyme label="b" type="f">dier</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En kom zo op het <rhyme label="c" type="m">strand</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Met het Zusje hand aan <rhyme label="b" type="f" corresp="#s5:l2">hand</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ik zoek myn Venus <rhyme label="a" type="m">dier</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Met haar maak ik myn <rhyme label="b" type="f">playzier</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Komt helpt my hier in by <rhyme label="c" type="m">tyds</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Eer ons den tyd <rhyme label="b" type="f" corresp="#s6:l2">ontglyd</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
