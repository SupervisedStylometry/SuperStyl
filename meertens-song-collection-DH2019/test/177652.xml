    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">HEER, onze Heer, grootmagtig <rhyme label="a" type="f">Opperwezen</rhyme>!</l>
        <l id="s1:l2" met="5" type="-+">Hoe wordt uw naam op aard alom <rhyme label="a" type="f" corresp="#s1:l1">gepreezen</rhyme>!</l>
        <l id="s1:l3" met="5" type="-+">Gij, die den glans van uwe <rhyme label="b" type="m">majesteit</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Hebt boven lucht en heemlen <rhyme label="b" type="m" corresp="#s1:l3">uitgebreid</rhyme>.</l>
      </lg>
    </lg>
