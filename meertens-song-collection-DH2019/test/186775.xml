    <melody>Psalm 100 Datheen</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Gy aarde, juicht alom den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Dient blydelyk den Heer met <rhyme label="a" type="m" corresp="#s1:l1">eer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Komt seedig voor syn <rhyme label="b" type="m">aangesigt</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En singt een vroolik <rhyme label="b" type="m" corresp="#s1:l3">dankgedicht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Heer is God, weet ook dat <rhyme label="a" type="m">hy</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Ons tot syn erfvolk schiep, niet <rhyme label="a" type="m" corresp="#s2:l1">wy</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Hy maakte self, uit <rhyme label="b" type="m">goedigheit</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ons tot syn schaapen, die hy <rhyme label="b" type="m" corresp="#s2:l3">weidt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gaat tot syn poorten in met <rhyme label="a" type="m">lof</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Met lofsang in syn tempels <rhyme label="a" type="m" corresp="#s3:l1">hof</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Belyd syn deugd, die gy <rhyme label="b" type="m">geniet</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En pryst syn naam, en hoog <rhyme label="b" type="m" corresp="#s3:l3">gebied</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Want God, de Heer, is volle <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Syn goedheit, die ons eeuwig <rhyme label="a" type="m" corresp="#s4:l1">hoedt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En syne waarheit houdt haar <rhyme label="b" type="m">kracht</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Van elk geslacht, tot elk <rhyme label="b" type="m" corresp="#s4:l3">geslacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
