<song>
  <meta>
    <id>8837</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody>Ich ruf zu dir Herr Jesu Christ</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">VErlost my doch (O Godt mijn <rhyme label="a" type="m">Heer</rhyme>)</l>
        <l id="s1:l2" met="3" type="-+">Van alle mijn <rhyme label="b" type="f">Vyanden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Want (siet) De Boose Loeren <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Om my te maecken <rhyme label="b" type="f" corresp="#s1:l2">t'schanden</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Die stercke teghen my soo <rhyme label="c" type="m">wreet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Vergaed'ren ende <rhyme label="d" type="f">Treden</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Sonder <rhyme label="d" type="f" corresp="#s1:l6">Reden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ten is mijn schult Jae <rhyme label="c" type="m" corresp="#s1:l5">niet</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Dat sy soo syn <rhyme label="d" type="f" corresp="#s1:l6">t'Onvreden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Staet op, help my wt dit <rhyme label="a" type="m">ghequel</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Godt Zebaoth end' <rhyme label="b" type="f">HEEre</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En soeckt te Huys (Godt <rhyme label="a" type="m" corresp="#s2:l1">Israel</rhyme>)</l>
        <l id="s2:l4" met="3" type="-+">Die Heyden wijt end' <rhyme label="b" type="f" corresp="#s2:l2">veere</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Wilt haerder gheen Ontfermen <rhyme label="c" type="m">(Heer</rhyme>)</l>
        <l id="s2:l6" met="3" type="-+">Die Willens t'quaet <rhyme label="d" type="f">aenvaten</rhyme>.</l>
        <l id="s2:l7" met="2" type="+-">Maer haer <rhyme label="d" type="f" corresp="#s2:l6">laten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Als Honden tsavonts <rhyme label="c" type="m" corresp="#s2:l5">weer</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Gaen huylen achter <rhyme label="d" type="f" corresp="#s2:l6">straten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Haer mont die overvallet <rhyme label="a" type="m">my</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Haer lippen zijn vol <rhyme label="b" type="f">Sweerden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Wie soudet hooren? segghen <rhyme label="a" type="m" corresp="#s3:l1">sy</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Gaen pralen hier op <rhyme label="b" type="f" corresp="#s3:l2">Eerden</rhyme>?</l>
        <l id="s3:l5" met="4" type="-+">Maer ghy begheckt die Heyden <rhyme label="c" type="m">wreet</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Myn Schuts, myn sterckte <rhyme label="d" type="f">crachtich</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Godt <rhyme label="d" type="f" corresp="#s3:l6">Almachtich</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Thoont my syn <rhyme label="c" type="m" corresp="#s3:l5">goedicheyt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Aen myn Vyanden <rhyme label="d" type="f" corresp="#s3:l6">prachtich</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">En Dootse niet, dat het niet <rhyme label="a" type="m">(Heer</rhyme>)</l>
        <l id="s4:l2" met="3" type="-+">Vergheet myn volck <rhyme label="b" type="f">(bysonder</rhyme>)</l>
        <l id="s4:l3" met="4" type="-+">Maer met dijn macht verstroytse <rhyme label="a" type="m" corresp="#s4:l1">seer</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">(Myn Schilt) verstootse <rhyme label="b" type="f" corresp="#s4:l2">t'onder</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Haer Leer al enckel sonde <rhyme label="c" type="m">is</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Vol Hooverdy sy <rhyme label="d" type="f">steken</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Ende <rhyme label="d" type="f" corresp="#s4:l6">spreken</rhyme></l>
        <l id="s4:l8" met="3" type="-+">All' vloeck end' Bans <rhyme label="c" type="m" corresp="#s4:l5">Advijs</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">End' niet dan <rhyme label="d" type="f" corresp="#s4:l6">wederspreken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Vernieltse sonder all' <rhyme label="a" type="m">ghenaed'</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En thoont dat God noch <rhyme label="b" type="f">crachtich</rhyme></l>
        <l id="s5:l3" met="4" type="-+">In Jacob heerschet metter <rhyme label="a" type="m" corresp="#s5:l1">daet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">End' over al <rhyme label="b" type="f" corresp="#s5:l2">waerachtich</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Laet door de Stadt sy loopen <rhyme label="c" type="m">(HEer</rhyme>)</l>
        <l id="s5:l6" met="3" type="-+">Om Spijs des Avonts <rhyme label="d" type="f">stonden</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Als die <rhyme label="d" type="f" corresp="#s5:l6">Honden</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Van Hongher huylen <rhyme label="c" type="m" corresp="#s5:l5">seer</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Als niet voor haer is <rhyme label="d" type="f" corresp="#s5:l6">vonden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ick wil van uwer Macht, O <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Steedts neerstich dy <rhyme label="b" type="f">Loffsinghen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dijn Goetheyt smorgens Roemen <rhyme label="a" type="m" corresp="#s6:l1">seer</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Mijn Godt dijn Loff voort <rhyme label="b" type="f" corresp="#s6:l2">bringhen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Mijn Schuts en Toevlucht, Heer, zijt <rhyme label="c" type="m">ghy</rhyme></l>
        <l id="s6:l6" met="3" type="-+">In mijnen noot en <rhyme label="d" type="f">lijden</rhyme></l>
        <l id="s6:l7" met="2" type="+-">T'allen <rhyme label="d" type="f" corresp="#s6:l6">Tijden</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">O Godt ghenadich <rhyme label="c" type="m" corresp="#s6:l5">my</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Mijn Schuts aen allen <rhyme label="d" type="f" corresp="#s6:l6">zijden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
