    <tags/>
    <melody>Acht gij mijn lijden dan niet</melody>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Ach gy myn lyden dan <rhyme label="a" type="m">niet</rhyme>?</l>
        <l id="s1:l2" met="4" type="+-">Ag al te wreede en straffe <rhyme label="b" type="m">Filida</rhyme>!</l>
        <l id="s1:l3" met="5" type="+-">Schept gy <rhyme label="b" type="m" corresp="#s1:l2">vermaak</rhyme>?</l>
        <l id="s1:l4" met="2" type="-+">Weet gy van geen <rhyme label="c" type="f">gena</rhyme>?</l>
        <l id="s1:l5" met="2" type="-+">kan ik uw <rhyme label="c" type="f" corresp="#s1:l4">zinnen</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">door stadig <rhyme label="c" type="f" corresp="#s1:l4">minnen</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Dan niet <rhyme label="d" type="f">verwinnen</rhyme>?</l>
        <l id="s1:l8" met="3" type="+-">Ag! ag! dat den kleynen <rhyme label="d" type="f" corresp="#s1:l7">Bengel</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Myn bevallige <rhyme label="d" type="f" corresp="#s1:l7">Engel</rhyme>,</l>
        <l id="s1:l10" met="5" type="+-">Eens al myn gehengel <rhyme label="e" type="m">zag</rhyme>;</l>
        <l id="s1:l11" met="4" type="+-">Hy nam zyn koker, pyl en <rhyme label="e" type="m" corresp="#s1:l10">boog</rhyme>,</l>
        <l id="s1:l12" met="3" type="+-">En daalden zagjes van om <rhyme label="f" type="m">hoog</rhyme>,</l>
        <l id="s1:l13" met="3" type="+-">En schoot u de Min straks ten Boezem <rhyme label="f" type="m" corresp="#s1:l12">in</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
