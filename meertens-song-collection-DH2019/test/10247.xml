    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Als de Son de bruyne <rhyme label="a" type="f">daaken</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Met sijn glans quam <rhyme label="a" type="f" corresp="#s1:l1">naaken</rhyme>,</l>
        <l id="s1:l3" met="7" type="+-">Dreef ik mijn sacht gewolde veetje langs het <rhyme label="b" type="m">Super-dal</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Waar ik mijn <rhyme label="c" type="m">Son</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Beschooten <rhyme label="c" type="m" corresp="#s1:l4">von</rhyme>:</l>
        <l id="s1:l6" met="2" type="-+">O mont! o <rhyme label="a" type="f" corresp="#s1:l1">kaaken</rhyme>!</l>
        <l id="s1:l7" met="2" type="-+">Wiens root <rhyme label="d" type="m">geswier</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Op <rhyme label="d" type="m" corresp="#s1:l7">glinster-spier</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Mijn eerste vryheyt <rhyme label="b" type="m" corresp="#s1:l3">stal</rhyme>.</l>
      </lg>
