    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">GHeen volmaeckte <rhyme label="a" type="m">vreught</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Is hier by de <rhyme label="a" type="m" corresp="#s1:l1">jeught</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Dan dat <rhyme label="b" type="m">in</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Den Echt vereenight en <rhyme label="c" type="m">vergaert</rhyme></l>
        <l id="s1:l5" met="2" type="+-">Hert en <rhyme label="b" type="m" corresp="#s1:l3">sin</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Als Lief met Lief in liefde <rhyme label="c" type="m" corresp="#s1:l4">paert</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Door de <rhyme label="b" type="m" corresp="#s1:l3">minn</rhyme>’</l>
        <l id="s1:l8" met="4" type="-+">Natuer gheen meerder vreught en <rhyme label="c" type="m" corresp="#s1:l4">baert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Nieuwen levens <rhyme label="a" type="m">lust</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Werckt dees soete <rhyme label="a" type="m" corresp="#s2:l1">rust</rhyme>:</l>
        <l id="s2:l3" met="2" type="+-">Want sy <rhyme label="b" type="m">kan</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Verbreyden het gheslacht, en <rhyme label="c" type="m">naem</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Ende <rhyme label="b" type="m" corresp="#s2:l3">dan</rhyme></l>
        <l id="s2:l6" met="4" type="-+">In suer en soet vernoeghen <rhyme label="c" type="m" corresp="#s2:l4">t’saem</rhyme></l>
        <l id="s2:l7" met="2" type="+-">Vrouw en <rhyme label="b" type="m" corresp="#s2:l3">man</rhyme>;</l>
        <l id="s2:l8" met="4" type="-+">Twee zielen zijn ’t in een <rhyme label="c" type="m" corresp="#s2:l4">lichaem</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Siet maer op ’t <rhyme label="a" type="m">ghepaert</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Tortel-duyfkens <rhyme label="a" type="m" corresp="#s3:l1">aert</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">Hoe dat <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Steedts korren en treck-becken <rhyme label="c" type="m">soet</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">En by <rhyme label="b" type="m" corresp="#s3:l3">ty</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Haer sachte pluymkens ruyven <rhyme label="c" type="m" corresp="#s3:l4">vroedt</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Waer in <rhyme label="b" type="m" corresp="#s3:l3">bly</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Haer jonghskens worden <rhyme label="c" type="m" corresp="#s3:l4">op-gevoedt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Rijckdom, <rhyme label="a" type="m">over-vloedt</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Rust, vreed’, en <rhyme label="a" type="m" corresp="#s4:l1">voor-spoedt</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Saligh <rhyme label="b" type="m">Paer</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Wensch’ ick u dan, en dat ghy <rhyme label="c" type="m">meught</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Alle <rhyme label="b" type="m" corresp="#s4:l3">jaer</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Voort-teelen uw’ gheslacht in <rhyme label="c" type="m" corresp="#s4:l4">deught</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">En hier <rhyme label="b" type="m" corresp="#s4:l3">naer</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Mooght hebben d’eeuwelijcke <rhyme label="c" type="m" corresp="#s4:l4">vreught</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Jonghe Maeghden, <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Oock verheught, <rhyme label="a" type="m" corresp="#s5:l1">verblijdt</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Oft schoon <rhyme label="b" type="m">al</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Vw’ herte door ’t langh-toeven <rhyme label="c" type="m">wroeght</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Het <rhyme label="b" type="m" corresp="#s5:l3">gheval</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Dat alle dinghen schickt en <rhyme label="c" type="m" corresp="#s5:l4">voeght</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">(Hoop’ ick) <rhyme label="b" type="m" corresp="#s5:l3">sal</rhyme></l>
        <l id="s5:l8" met="4" type="-+">V maecken oock eer langh <rhyme label="c" type="m" corresp="#s5:l4">vernoeght</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
