<song>
  <meta>
    <id>34548</id>
    <date>
      <year-start>1720</year-start>
      <year-end>1720</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Maastricht gij schone stad</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ANtwerpen k'moet u <rhyme label="a" type="f">prijsen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ghy overschoone <rhyme label="b" type="m">Stadt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">U lof en Eer' <rhyme label="a" type="f" corresp="#s1:l1">bewijsen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ende noch boven <rhyme label="b" type="m" corresp="#s1:l2">dat</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">U Schoonheyt gaen <rhyme label="c" type="f">Lauderen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En 't kostelijck <rhyme label="d" type="m">cieraet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">'t Welck altijdt sal <rhyme label="c" type="f" corresp="#s1:l5">floreeren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Soo langh' de werelt <rhyme label="d" type="m" corresp="#s1:l6">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghy zijt de schoonste <rhyme label="a" type="f">stede</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Van g'heel het <rhyme label="b" type="m">Nederlant</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">U Vestens die sijn <rhyme label="a" type="f" corresp="#s2:l1">mede</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met Boomen schoon <rhyme label="b" type="m" corresp="#s2:l2">beplant</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Vier reken, groote <rhyme label="c" type="f">Linden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Daer sijt ghy med' <rhyme label="d" type="m">verciert</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gheen schoonder wercken te <rhyme label="c" type="f" corresp="#s2:l5">vinden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Daer menigh Voghel <rhyme label="d" type="m" corresp="#s2:l6">swiert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Met scheld en steene <rhyme label="a" type="f">mueren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Is uwe Stadt <rhyme label="b" type="m">bewaert</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Niemant kan u doen <rhyme label="a" type="f" corresp="#s3:l1">treuren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De werelt door <rhyme label="b" type="m" corresp="#s3:l2">versmaert</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Een schoon Casteel daer <rhyme label="c" type="f">neven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Bewatert, en <rhyme label="d" type="m">bemant</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">'k Moet u de eere <rhyme label="c" type="f" corresp="#s3:l5">gheven</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">De peirel van <rhyme label="d" type="m" corresp="#s3:l6">Brabant</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">G'hebt twee-en-veertigh <rhyme label="a" type="f">Kercken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Waer in dat zijn te <rhyme label="b" type="m">sien</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Veel Costelijcke <rhyme label="a" type="f" corresp="#s4:l1">wercken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Beelden, en <rhyme label="b" type="m" corresp="#s4:l2">Reliquien</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Waer in begrepen <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Veel kloosters in 't <rhyme label="d" type="m">ghetal</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En ons Lief Vrouw van <rhyme label="c" type="f" corresp="#s4:l5">dese</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">De grootste van <rhyme label="d" type="m" corresp="#s4:l6">al</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">G'hebt twee-en-twintigh <rhyme label="a" type="f">Merten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Daer onder groot en <rhyme label="b" type="m">kleen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Tot dienst van de goed' <rhyme label="a" type="f" corresp="#s5:l1">Heeren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De Borghers in 't <rhyme label="b" type="m" corresp="#s5:l2">ghemeen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Een schoone Bors <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Veel Coop-lien kleyn, en <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die daer door weldigh <rhyme label="c" type="f" corresp="#s5:l5">leven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Een troost in haeren <rhyme label="d" type="m" corresp="#s5:l6">noot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">d'Hooft kerck is Ons L. <rhyme label="a" type="f">Vrouwe</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Seer Rijckelijck <rhyme label="b" type="m">verciert</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">s' Is weert om te <rhyme label="a" type="f" corresp="#s6:l1">aenschouwen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Daer m'haer Feest-daghen <rhyme label="b" type="m" corresp="#s6:l2">viert</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">s' Heeft eenen hooghen <rhyme label="c" type="f">Thoren</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Van wit Arduyn <rhyme label="d" type="m">ghesneen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Dry Orghels fraey om <rhyme label="c" type="f" corresp="#s6:l5">hooren</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Van boven tot <rhyme label="d" type="m" corresp="#s6:l6">beneen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ses hondert twintigh <rhyme label="a" type="f">voeten</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Is desen Thoren <rhyme label="b" type="m">hoogh</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Men klemt daer op met <rhyme label="a" type="f" corresp="#s7:l1">moeten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Noch wort de Kele <rhyme label="b" type="m" corresp="#s7:l2">droogh</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Twee Wijsers wonder <rhyme label="c" type="f">schoone</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Met handen die seer <rhyme label="d" type="m">fris</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Aen ider een <rhyme label="c" type="f" corresp="#s7:l5">bethoonen</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Wijsen hoe laet dat <rhyme label="d" type="m" corresp="#s7:l6">is</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">s' Heeft derthien duysent <rhyme label="a" type="f">huysen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En vijf hondert daer <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Sonder Kercken of <rhyme label="a" type="f" corresp="#s8:l1">Cluysen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">(k' Segh) dat gaet vast en <rhyme label="b" type="m" corresp="#s8:l2">vry'</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Vijf Parochien <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Verciert met <rhyme label="d" type="m">Schilderijen</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Alles ghestelt naer 't <rhyme label="c" type="f" corresp="#s8:l5">leven</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Seer aenghenaem om <rhyme label="d" type="m" corresp="#s8:l6">sien</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">De Gelt-Munt weert <rhyme label="a" type="f">ghepresen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Vlees-huys, en Visch Merckt <rhyme label="b" type="m">med'</rhyme></l>
        <l id="s9:l3" met="3" type="-+">'t Groot Oosters huys moet <rhyme label="a" type="f" corresp="#s9:l1">wesen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">'t Verciersel van de <rhyme label="b" type="m" corresp="#s9:l2">Sted'</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">T' is weert om te <rhyme label="c" type="f">aenschouwen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">'t Stadt-huys, een konstigh <rhyme label="d" type="m">werck</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Van Arduyn-steen <rhyme label="c" type="f" corresp="#s9:l5">ghehouwen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Ghebauwt seer groot en <rhyme label="d" type="m" corresp="#s9:l6">sterck</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">s'Heeft twee-en-dertigh <rhyme label="a" type="f">wijcken</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">'t Enghels en Neffens <rhyme label="b" type="m">huys</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">T'is weert om te <rhyme label="a" type="f" corresp="#s10:l1">bekijcken</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Heer Iesus aen het <rhyme label="b" type="m" corresp="#s10:l2">Cruys</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">'t Welck op de Meir naer 't <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Seer schoon ten thoone <rhyme label="d" type="m">staet</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">d'Antwerpsche Stadt <rhyme label="c" type="f" corresp="#s10:l5">verheven</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Het al te boven <rhyme label="d" type="m" corresp="#s10:l6">gaet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Noch prijs ick 't Huys van <rhyme label="a" type="f">Aken</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">En oock van <rhyme label="b" type="m">Portugael</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">'t Heeft veel ghekost van <rhyme label="a" type="f" corresp="#s11:l1">maecken</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En 't ander alte <rhyme label="b" type="m" corresp="#s11:l2">mael</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Antwerp heeft wijde <rhyme label="c" type="f">Straten</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Curieus om aen te <rhyme label="d" type="m">sien</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Huysen voor <rhyme label="c" type="f" corresp="#s11:l5">Potentaten</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">En Schrift gheleerde <rhyme label="d" type="m" corresp="#s11:l6">Lien</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Antwerpen doet <rhyme label="a" type="f">Floreeren</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">De Konst van het <rhyme label="b" type="m">Pinceel</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Veel Boeck-druckers van <rhyme label="a" type="f" corresp="#s12:l1">eeren</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Hebben sy voor hun <rhyme label="b" type="m" corresp="#s12:l2">deel</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">'t Beelt Snijders werck naer 't <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">Ambachten veel der <rhyme label="d" type="m">hant</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">k' Moet u de eeren <rhyme label="c" type="f" corresp="#s12:l5">gheven</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">De Peirel van <rhyme label="d" type="m" corresp="#s12:l6">Brabant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
