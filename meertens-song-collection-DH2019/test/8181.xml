  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">OVerschoone soete <rhyme label="a" type="m">beelt</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Uwe geur die ruykt als <rhyme label="b" type="f">Flora</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En u glans blinckt als <rhyme label="b" type="f" corresp="#s1:l2">Aurora</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Venus selfs in u <rhyme label="a" type="m" corresp="#s1:l1">speelt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Puykje, 't soetst van alle <rhyme label="c" type="f">maagden</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Glans en parel van de <rhyme label="d" type="m">deucht</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die soo meenigh borst <rhyme label="c" type="f" corresp="#s1:l5">behaaghden</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">En verweckt tot Venus <rhyme label="d" type="m" corresp="#s1:l6">vreucht</rhyme>.</l>
      </lg>
    </lg>
  </text>
