    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
    </tags>
    <melody>O mijn ziel dank nu de Heer</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author></author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Daer en is gheen wee soo <rhyme label="a" type="f">groot</rhyme>//</l>
        <l id="s1:l2" met="3" type="+-">als therten wee dit selden <rhyme label="b" type="f">mist</rhyme>// }</l>
        <l id="s1:l3" met="4" type="+-">Gheenen listigher <rhyme label="a" type="f" corresp="#s1:l1">aenstoet</rhyme>//</l>
        <l id="s1:l4" met="3" type="+-">dan hier en is der vrouwen <rhyme label="b" type="f" corresp="#s1:l2">list</rhyme>// }</l>
        <l id="s1:l5" met="4" type="+-">Meerder leeren noch <rhyme label="c" type="f">benijen</rhyme>//</l>
        <l id="s1:l6" met="3" type="-+">Dan tgheen dat wt afgunst <rhyme label="d" type="m">rijst</rhyme>//</l>
        <l id="s1:l7" met="3" type="-+">Oock gheen wraec tot gheenen <rhyme label="c" type="f" corresp="#s1:l5">tijden</rhyme>//</l>
        <l id="s1:l8" met="4" type="+-">Dan het seer felle <rhyme label="c" type="f" corresp="#s1:l5">bestrijden</rhyme>//</l>
        <l id="s1:l9" met="4" type="+-">als d'een viant d'ander <rhyme label="d" type="m" corresp="#s1:l6">spijst</rhyme>//</l>
        <l id="s1:l10" met="4" type="-+">met wrake die hy hem <rhyme label="d" type="m" corresp="#s1:l6">bewijst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Gheen hooft soo schalck <rhyme label="a" type="f">gheboren</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Als het hooft der <rhyme label="b" type="f">slanghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Daer en wert ghen booser <rhyme label="a" type="f" corresp="#s2:l1">toren</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Dan van vrouwen <rhyme label="b" type="f" corresp="#s2:l2">ontfanghen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">By den leeuwen oft den <rhyme label="c" type="f">draken</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Waer van my lieuer <rhyme label="d" type="m">ghewoont</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dan dat ick sou <rhyme label="c" type="f" corresp="#s2:l5">gheraken</rhyme></l>
        <l id="s2:l8" met="4" type="+-">By een wijf diemen siet <rhyme label="c" type="f" corresp="#s2:l5">blaken</rhyme></l>
        <l id="s2:l9" met="4" type="+-">In haer boosheyt die sy <rhyme label="d" type="m" corresp="#s2:l6">toont</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Waer in sy haer selfs meest <rhyme label="d" type="m" corresp="#s2:l6">hoont</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wanneer sy boos sal <rhyme label="a" type="f">werden</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Dan verandert haer <rhyme label="b" type="f">ghelaet</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Soo mismaect is haer <rhyme label="a" type="f" corresp="#s3:l1">volherden</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Als een sack die leelijck <rhyme label="b" type="f" corresp="#s3:l2">staet</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Huer man moet hem haerder <rhyme label="c" type="f">schamen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En wanneer ment hem <rhyme label="d" type="m">voorleyt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Soo doetmen dit <rhyme label="c" type="f" corresp="#s3:l5">altesamen</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Int hert wee: tis oock sijn <rhyme label="c" type="f" corresp="#s3:l5">blamen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Dit quaet wort van my <rhyme label="d" type="m" corresp="#s3:l6">beschreyt</rhyme>.</l>
        <l id="s3:l10" met="4" type="-+">En onder veel quaets groot <rhyme label="d" type="m" corresp="#s3:l6">gheseyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een clappeye sonder <rhyme label="a" type="f">schande</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Die rust, wilt <rhyme label="b" type="f">ontwroeten</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Is ghelijck een wech van <rhyme label="a" type="f" corresp="#s4:l1">sande</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Voor een outmans <rhyme label="b" type="f" corresp="#s4:l2">voeten</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Hoe wel ghy haer schoone <rhyme label="c" type="f">siet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">En laet v niet <rhyme label="d" type="m">bedrieghen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En begheertse daerom <rhyme label="c" type="f" corresp="#s4:l5">niet</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Is sy rijck, als dan <rhyme label="c" type="f" corresp="#s4:l5">gheschiet</rhyme></l>
        <l id="s4:l9" met="4" type="+-">Verachtingh, tsal niet <rhyme label="d" type="m" corresp="#s4:l6">lieghen</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Versmaetheyt sal wt haer <rhyme label="d" type="m" corresp="#s4:l6">vlieghen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die hier heeft een boose <rhyme label="a" type="f">vrouwe</rhyme></l>
        <l id="s5:l2" met="3" type="+-">Heeft een truerich <rhyme label="b" type="f">herte</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">T'welck beseten is met <rhyme label="a" type="f" corresp="#s5:l1">rouwe</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Leet druck pijn en <rhyme label="b" type="f" corresp="#s5:l2">smerte</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Een wijf daer de man tgheender <rhyme label="c" type="f">uren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">By en heeft van vruecht <rhyme label="d" type="m">ghewin</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dese maeckt door sulcke <rhyme label="c" type="f" corresp="#s5:l5">curen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Dat de man nerghers can <rhyme label="c" type="f" corresp="#s5:l5">duren</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Tot gheen dinghen gheeft hy <rhyme label="d" type="m" corresp="#s5:l6">sin</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Sulcx eynde volght na het <rhyme label="d" type="m" corresp="#s5:l6">beghin</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Van een vrouwe coemt de <rhyme label="a" type="f">sonde</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Om haer moeten wy <rhyme label="b" type="f">steruen</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Soomen twater diep van <rhyme label="a" type="f" corresp="#s6:l1">gronde</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Sijnen loop doet <rhyme label="b" type="f" corresp="#s6:l2">deruen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Soo en salmen het wijfs <rhyme label="c" type="f">wille</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Huer niet volghen <rhyme label="d" type="m">laten</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Laet sy v in vre niet <rhyme label="c" type="f" corresp="#s6:l5">stille</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Scheyt van haer en scuut <rhyme label="c" type="f" corresp="#s6:l5">gheschille</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">[Maer de Christen <rhyme label="d" type="m" corresp="#s6:l6">staten</rhyme></l>
        <l id="s6:l10" met="4" type="-+">Sullen sulcx niet <rhyme label="d" type="m" corresp="#s6:l6">aenvaten</rhyme>.]</l>
      </lg>
    </lg>
  </text>
</song>
